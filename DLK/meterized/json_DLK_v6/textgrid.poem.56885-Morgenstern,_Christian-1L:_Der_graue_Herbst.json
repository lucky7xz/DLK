{"textgrid.poem.56885": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der graue Herbst", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der graue Herbst", "tokens": ["Der", "grau\u00b7e", "Herbst"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "l\u00e4dt mich zu sich hinaus,", "tokens": ["l\u00e4dt", "mich", "zu", "sich", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PRF", "APZR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00fcbern grauen See,", "tokens": ["\u00fc\u00b7bern", "grau\u00b7en", "See", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "\u00fcbern grauen Wald,", "tokens": ["\u00fc\u00b7bern", "grau\u00b7en", "Wald", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "in die graue, graue Himmelsferne ...", "tokens": ["in", "die", "grau\u00b7e", ",", "grau\u00b7e", "Him\u00b7mels\u00b7fer\u00b7ne", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Bin ich der einzige Mensch der Erde? ...", "tokens": ["Bin", "ich", "der", "ein\u00b7zi\u00b7ge", "Mensch", "der", "Er\u00b7de", "?", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Tiefe Verlassenheit f\u00e4llt mich an.", "tokens": ["Tie\u00b7fe", "Ver\u00b7las\u00b7sen\u00b7heit", "f\u00e4llt", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}