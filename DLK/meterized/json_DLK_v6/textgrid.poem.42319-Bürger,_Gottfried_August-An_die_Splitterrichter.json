{"textgrid.poem.42319": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "An die Splitterrichter", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das freut mich doch, ihr Herren Falken,", "tokens": ["Das", "freut", "mich", "doch", ",", "ihr", "Her\u00b7ren", "Fal\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihr, Gott wei\u00df warum? erbost,", "tokens": ["Die", "ihr", ",", "Gott", "wei\u00df", "wa\u00b7rum", "?", "er\u00b7bost", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "$,", "NN", "VVFIN", "PWAV", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So gern auf meine Fehler sto\u00dft,", "tokens": ["So", "gern", "auf", "mei\u00b7ne", "Feh\u00b7ler", "sto\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ihr nichts mehr ersto\u00dft, ihr Falken,", "tokens": ["Da\u00df", "ihr", "nichts", "mehr", "er\u00b7sto\u00dft", ",", "ihr", "Fal\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADV", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als Splitter nur von euern Balken.", "tokens": ["Als", "Split\u00b7ter", "nur", "von", "eu\u00b7ern", "Bal\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}