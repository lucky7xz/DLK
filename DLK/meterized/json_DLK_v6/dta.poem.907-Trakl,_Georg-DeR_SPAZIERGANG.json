{"dta.poem.907": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "DeR SPAZIERGANG", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Musik summt im Geh\u00f6lz am Nachmittag.", "tokens": ["Mu\u00b7sik", "summt", "im", "Ge\u00b7h\u00f6lz", "am", "Nach\u00b7mit\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Im Korn sich ernste Vogelscheuchen drehn.", "tokens": ["Im", "Korn", "sich", "erns\u00b7te", "Vo\u00b7gel\u00b7scheu\u00b7chen", "drehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hollunderb\u00fcsche sacht am Weg verwehn;", "tokens": ["Hol\u00b7lun\u00b7der\u00b7b\u00fc\u00b7sche", "sacht", "am", "Weg", "ver\u00b7wehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Haus zerflimmert wunderlich und vag.", "tokens": ["Ein", "Haus", "zer\u00b7flim\u00b7mert", "wun\u00b7der\u00b7lich", "und", "vag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In Goldnem schwebt ein Duft von Thymian,", "tokens": ["In", "Gold\u00b7nem", "schwebt", "ein", "Duft", "von", "Thy\u00b7mi\u00b7an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf einem Stein steht eine heitere Zahl.", "tokens": ["Auf", "ei\u00b7nem", "Stein", "steht", "ei\u00b7ne", "hei\u00b7te\u00b7re", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Auf einer Wiese spielen Kinder Ball,", "tokens": ["Auf", "ei\u00b7ner", "Wie\u00b7se", "spie\u00b7len", "Kin\u00b7der", "Ball", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dann hebt ein Baum vor dir zu kreisen an.", "tokens": ["Dann", "hebt", "ein", "Baum", "vor", "dir", "zu", "krei\u00b7sen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du tr\u00e4umst: die Schwester k\u00e4mmt ihr blondes Haar,", "tokens": ["Du", "tr\u00e4umst", ":", "die", "Schwes\u00b7ter", "k\u00e4mmt", "ihr", "blon\u00b7des", "Haar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auch schreibt ein ferner Freund dir einen Brief.", "tokens": ["Auch", "schreibt", "ein", "fer\u00b7ner", "Freund", "dir", "ei\u00b7nen", "Brief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Schober flieht durchs Grau vergilbt und schief", "tokens": ["Ein", "Scho\u00b7ber", "flieht", "durchs", "Grau", "ver\u00b7gilbt", "und", "schief"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und manchmal schwebst du leicht und wunderbar.", "tokens": ["Und", "manch\u00b7mal", "schwebst", "du", "leicht", "und", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Zeit verrinnt. O s\u00fc\u00dfer Helios!", "tokens": ["Die", "Zeit", "ver\u00b7rinnt", ".", "O", "s\u00fc\u00b7\u00dfer", "He\u00b7li\u00b7os", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O Bild im Kr\u00f6tent\u00fcmpel s\u00fc\u00df und klar;", "tokens": ["O", "Bild", "im", "Kr\u00f6\u00b7ten\u00b7t\u00fcm\u00b7pel", "s\u00fc\u00df", "und", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Sand versinkt ein Eden wunderbar.", "tokens": ["Im", "Sand", "ver\u00b7sinkt", "ein", "E\u00b7den", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Goldammern wiegt ein Busch in seinem Scho\u00df.", "tokens": ["Gold\u00b7am\u00b7mern", "wiegt", "ein", "Busch", "in", "sei\u00b7nem", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein Bruder stirbt dir in verwunschnem Land", "tokens": ["Ein", "Bru\u00b7der", "stirbt", "dir", "in", "ver\u00b7wun\u00b7schnem", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und st\u00e4hlern schaun dich deine Augen an.", "tokens": ["Und", "st\u00e4h\u00b7lern", "schaun", "dich", "dei\u00b7ne", "Au\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Goldnem dort ein Duft von Thymian.", "tokens": ["In", "Gold\u00b7nem", "dort", "ein", "Duft", "von", "Thy\u00b7mi\u00b7an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Knabe legt am Weiler einen Brand.", "tokens": ["Ein", "Kna\u00b7be", "legt", "am", "Wei\u00b7ler", "ei\u00b7nen", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Die Liebenden in Faltern neu ergl\u00fchn", "tokens": ["Die", "Lie\u00b7ben\u00b7den", "in", "Fal\u00b7tern", "neu", "er\u00b7gl\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und schaukeln heiter hin um Stein und Zahl.", "tokens": ["Und", "schau\u00b7keln", "hei\u00b7ter", "hin", "um", "Stein", "und", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aufflattern Kr\u00e4hen um ein ekles Mahl", "tokens": ["Auf\u00b7flat\u00b7tern", "Kr\u00e4\u00b7hen", "um", "ein", "ek\u00b7les", "Mahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und deine Stirne tost durchs sanfte Gr\u00fcn.", "tokens": ["Und", "dei\u00b7ne", "Stir\u00b7ne", "tost", "durchs", "sanf\u00b7te", "Gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Im Dornenstrauch verendet weich ein Wild.", "tokens": ["Im", "Dor\u00b7nen\u00b7strauch", "ver\u00b7en\u00b7det", "weich", "ein", "Wild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nachgleitet dir ein heller Kindertag,", "tokens": ["Nach\u00b7glei\u00b7tet", "dir", "ein", "hel\u00b7ler", "Kin\u00b7der\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der graue Wind, der flatterhaft und vag", "tokens": ["Der", "grau\u00b7e", "Wind", ",", "der", "flat\u00b7ter\u00b7haft", "und", "vag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verfallne D\u00fcfte durch die D\u00e4mmerung sp\u00fclt.", "tokens": ["Ver\u00b7fall\u00b7ne", "D\u00fcf\u00b7te", "durch", "die", "D\u00e4m\u00b7me\u00b7rung", "sp\u00fclt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.8": {"line.1": {"text": "Ein altes Wiegenlied macht dich sehr bang.", "tokens": ["Ein", "al\u00b7tes", "Wie\u00b7gen\u00b7lied", "macht", "dich", "sehr", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am Wegrand fromm ein Weib ihr Kindlein stillt.", "tokens": ["Am", "Weg\u00b7rand", "fromm", "ein", "Weib", "ihr", "Kin\u00b7dlein", "stillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Traumwandelnd h\u00f6rst du wie ihr Bronnen quillt.", "tokens": ["Traum\u00b7wan\u00b7delnd", "h\u00f6rst", "du", "wie", "ihr", "Bron\u00b7nen", "quillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "KOKOM", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus Apfelzweigen f\u00e4llt ein Weiheklang.", "tokens": ["Aus", "Ap\u00b7fel\u00b7zwei\u00b7gen", "f\u00e4llt", "ein", "Wei\u00b7he\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Und Brot und Wein sind s\u00fc\u00df von harten M\u00fchn.", "tokens": ["Und", "Brot", "und", "Wein", "sind", "s\u00fc\u00df", "von", "har\u00b7ten", "M\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nach Fr\u00fcchten tastet silbern deine Hand.", "tokens": ["Nach", "Fr\u00fcch\u00b7ten", "tas\u00b7tet", "sil\u00b7bern", "dei\u00b7ne", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die tote Rahel geht durchs Ackerland.", "tokens": ["Die", "to\u00b7te", "Ra\u00b7hel", "geht", "durchs", "A\u00b7cker\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit friedlicher Geberde winkt das Gr\u00fcn.", "tokens": ["Mit", "fried\u00b7li\u00b7cher", "Ge\u00b7ber\u00b7de", "winkt", "das", "Gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-----+-+", "measure": "dactylic.init"}}, "stanza.10": {"line.1": {"text": "Gesegnet auch bl\u00fcht armer M\u00e4gde Scho\u00df,", "tokens": ["Ge\u00b7seg\u00b7net", "auch", "bl\u00fcht", "ar\u00b7mer", "M\u00e4g\u00b7de", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die tr\u00e4umend dort am alten Brunnen stehn.", "tokens": ["Die", "tr\u00e4u\u00b7mend", "dort", "am", "al\u00b7ten", "Brun\u00b7nen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Einsame froh auf stillen Pfaden gehn", "tokens": ["Ein\u00b7sa\u00b7me", "froh", "auf", "stil\u00b7len", "Pfa\u00b7den", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "ADJA", "NN", "VVINF"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Mit Gottes Kreaturen s\u00fcndelos.", "tokens": ["Mit", "Got\u00b7tes", "Kre\u00b7a\u00b7tu\u00b7ren", "s\u00fcn\u00b7de\u00b7los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}