{"dta.poem.8643": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Erinnerung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Erinn'rungsvoller Baum, du stehst in Trauer;", "tokens": ["Er\u00b7inn'\u00b7rungs\u00b7vol\u00b7ler", "Baum", ",", "du", "stehst", "in", "Trau\u00b7er", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Laub ist welk, mein Leben ist es auch.", "tokens": ["Dein", "Laub", "ist", "welk", ",", "mein", "Le\u00b7ben", "ist", "es", "auch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Herz durchziehen bange Wehmuthschauer,", "tokens": ["Mein", "Herz", "durch\u00b7zie\u00b7hen", "ban\u00b7ge", "Weh\u00b7muth\u00b7schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie dein Gezweig des Herbstes k\u00fchler Hauch.", "tokens": ["Wie", "dein", "Ge\u00b7zweig", "des", "Herbs\u00b7tes", "k\u00fch\u00b7ler", "Hauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hier sa\u00dfen wir in abendlicher Stille,", "tokens": ["Hier", "sa\u00b7\u00dfen", "wir", "in", "a\u00b7bend\u00b7li\u00b7cher", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sanft bebte \u00fcber uns dein fl\u00fcsternd Gr\u00fcn,", "tokens": ["Sanft", "beb\u00b7te", "\u00fc\u00b7ber", "uns", "dein", "fl\u00fcs\u00b7ternd", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PPER", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf jenen H\u00f6h'n, die nun in Nebelh\u00fclle,", "tokens": ["Auf", "je\u00b7nen", "H\u00f6h'n", ",", "die", "nun", "in", "Ne\u00b7bel\u00b7h\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verweilte noch der Sonne leztes Gl\u00fch'n.", "tokens": ["Ver\u00b7weil\u00b7te", "noch", "der", "Son\u00b7ne", "lez\u00b7tes", "Gl\u00fch'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie selig hielt das M\u00e4dchen ich umfangen,", "tokens": ["Wie", "se\u00b7lig", "hielt", "das", "M\u00e4d\u00b7chen", "ich", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und horchte ihrem leisen Liebesschwur;", "tokens": ["Und", "horch\u00b7te", "ih\u00b7rem", "lei\u00b7sen", "Lie\u00b7bes\u00b7schwur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und holder lachten uns die Bl\u00fcthenwangen", "tokens": ["Und", "hol\u00b7der", "lach\u00b7ten", "uns", "die", "Bl\u00fc\u00b7then\u00b7wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der auferwachten g\u00f6ttlichen Natur.", "tokens": ["Der", "au\u00b7fer\u00b7wach\u00b7ten", "g\u00f6tt\u00b7li\u00b7chen", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch hatte kaum der Lenz die sanfte Seele", "tokens": ["Doch", "hat\u00b7te", "kaum", "der", "Lenz", "die", "sanf\u00b7te", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verhaucht, und seine Bl\u00fcthen hingestreut,", "tokens": ["Ver\u00b7haucht", ",", "und", "sei\u00b7ne", "Bl\u00fc\u00b7then", "hin\u00b7ge\u00b7streut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kaum war verstummt im Hain die s\u00fc\u00dfe Kehle:", "tokens": ["Kaum", "war", "ver\u00b7stummt", "im", "Hain", "die", "s\u00fc\u00b7\u00dfe", "Keh\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "War auch dahin der Liebe Seligkeit.", "tokens": ["War", "auch", "da\u00b7hin", "der", "Lie\u00b7be", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PAV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O traure, Herz, vor\u00fcber sind ", "tokens": ["O", "trau\u00b7re", ",", "Herz", ",", "vor\u00b7\u00fc\u00b7ber", "sind"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "$,", "NN", "$,", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da liebend dir ein Herz entgegenschlug,", "tokens": ["Da", "lie\u00b7bend", "dir", "ein", "Herz", "ent\u00b7ge\u00b7gen\u00b7schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die andern schleichen hin in stiller Klage,", "tokens": ["Die", "an\u00b7dern", "schlei\u00b7chen", "hin", "in", "stil\u00b7ler", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der todten Liebe finstrer Leichenzug.", "tokens": ["Der", "tod\u00b7ten", "Lie\u00b7be", "finst\u00b7rer", "Lei\u00b7chen\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}