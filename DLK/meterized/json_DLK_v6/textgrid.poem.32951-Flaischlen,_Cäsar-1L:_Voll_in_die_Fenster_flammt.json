{"textgrid.poem.32951": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Voll in die Fenster flammt", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Voll in die Fenster flammt", "tokens": ["Voll", "in", "die", "Fens\u00b7ter", "flammt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "der Fr\u00fchm\u00e4rzsonnenschein", "tokens": ["der", "Fr\u00fch\u00b7m\u00e4rz\u00b7son\u00b7nen\u00b7schein"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und blendet irrlichtgolden", "tokens": ["und", "blen\u00b7det", "irr\u00b7licht\u00b7gol\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "durch meine Schreiberein ...", "tokens": ["durch", "mei\u00b7ne", "Schrei\u00b7be\u00b7re\u00b7in", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Und neckt und nickt und lockt", "tokens": ["Und", "neckt", "und", "nickt", "und", "lockt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "und l\u00e4\u00dft mir keine Ruh", "tokens": ["und", "l\u00e4\u00dft", "mir", "kei\u00b7ne", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und vom Gesims ein Sperling", "tokens": ["und", "vom", "Ge\u00b7sims", "ein", "Sper\u00b7ling"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "piepst kritisch-frech dazu:", "tokens": ["piepst", "kri\u00b7tischfrech", "da\u00b7zu", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PAV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "'s ist Fr\u00fchling, alter Mann!", "tokens": ["'s", "ist", "Fr\u00fch\u00b7ling", ",", "al\u00b7ter", "Mann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "geh in den Wald hinaus", "tokens": ["geh", "in", "den", "Wald", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "APZR"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "und l\u00fcfte dir die Seele", "tokens": ["und", "l\u00fcf\u00b7te", "dir", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "von Staub und Motten aus!", "tokens": ["von", "Staub", "und", "Mot\u00b7ten", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein einziger Sonnenblick", "tokens": ["Ein", "ein\u00b7zi\u00b7ger", "Son\u00b7nen\u00b7blick"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "schafft mehr an Lebenskraft,", "tokens": ["schafft", "mehr", "an", "Le\u00b7bens\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "als deiner kl\u00fcgsten B\u00fccher", "tokens": ["als", "dei\u00b7ner", "kl\u00fcgs\u00b7ten", "B\u00fc\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "weiseste Wissenschaft!", "tokens": ["wei\u00b7ses\u00b7te", "Wis\u00b7sen\u00b7schaft", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}