{"textgrid.poem.40102": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Trost", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist nicht g\u00fctig, in ein Auge schauen,", "tokens": ["Es", "ist", "nicht", "g\u00fc\u00b7tig", ",", "in", "ein", "Au\u00b7ge", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in dessen Schoo\u00df ein schweigend Weh sich windet:", "tokens": ["in", "des\u00b7sen", "Schoo\u00df", "ein", "schwei\u00b7gend", "Weh", "sich", "win\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "ADJD", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "das R\u00e4tsel lockt, die Scham des Mitleids schwindet,", "tokens": ["das", "R\u00e4t\u00b7sel", "lockt", ",", "die", "Scham", "des", "Mit\u00b7leids", "schwin\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "denn eine tiefe Wollust schl\u00e4ft im Grauen.", "tokens": ["denn", "ei\u00b7ne", "tie\u00b7fe", "Wol\u00b7lust", "schl\u00e4ft", "im", "Grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr Eitlen! wollt ihr Den mit Trost erbauen,", "tokens": ["Ihr", "Eit\u00b7len", "!", "wollt", "ihr", "Den", "mit", "Trost", "er\u00b7bau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VMFIN", "PPER", "ART", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der selbst kein Wort f\u00fcr seine Schwermut findet?", "tokens": ["der", "selbst", "kein", "Wort", "f\u00fcr", "sei\u00b7ne", "Schwer\u00b7mut", "fin\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Kr\u00e4nze, die der Schmerz um S\u00e4rge bindet:", "tokens": ["Die", "Kr\u00e4n\u00b7ze", ",", "die", "der", "Schmerz", "um", "S\u00e4r\u00b7ge", "bin\u00b7det", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die ", "tokens": ["die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Und meint ihr denn, wer Einsam sich befragte", "tokens": ["Und", "meint", "ihr", "denn", ",", "wer", "Ein\u00b7sam", "sich", "be\u00b7frag\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PWS", "ADJD", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "mit seinem Leid, er h\u00e4tt' es ", "tokens": ["mit", "sei\u00b7nem", "Leid", ",", "er", "h\u00e4tt'", "es"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer m\u00fchsam sich in dunkler Tiefe plagte,", "tokens": ["Wer", "m\u00fch\u00b7sam", "sich", "in", "dunk\u00b7ler", "Tie\u00b7fe", "plag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "der wei\u00df auch, wann zum Gipfel er gedrungen;", "tokens": ["der", "wei\u00df", "auch", ",", "wann", "zum", "Gip\u00b7fel", "er", "ge\u00b7drun\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ADV", "$,", "PWAV", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "doch wer an seinem Leben ", "tokens": ["doch", "wer", "an", "sei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "hat um des Lebens ", "tokens": ["hat", "um", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}