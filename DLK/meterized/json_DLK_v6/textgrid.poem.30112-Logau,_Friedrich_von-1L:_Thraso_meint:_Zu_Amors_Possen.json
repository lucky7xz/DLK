{"textgrid.poem.30112": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Thraso meint: Zu Amors Possen", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Thraso meint: Zu Amors Possen", "tokens": ["Thra\u00b7so", "meint", ":", "Zu", "A\u00b7mors", "Pos\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$.", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sey er viel zu viel verdrussen,", "tokens": ["Sey", "er", "viel", "zu", "viel", "ver\u00b7drus\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKA", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lade Lieb in ein Pistol,", "tokens": ["La\u00b7de", "Lieb", "in", "ein", "Pis\u00b7tol", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Schiesse sie ins weite Hol;", "tokens": ["Schies\u00b7se", "sie", "ins", "wei\u00b7te", "Hol", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann er dieses f\u00fcrgenummen,", "tokens": ["Wann", "er", "die\u00b7ses", "f\u00fcr\u00b7ge\u00b7num\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sey sie selten wieder kummen.", "tokens": ["Sey", "sie", "sel\u00b7ten", "wie\u00b7der", "kum\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}