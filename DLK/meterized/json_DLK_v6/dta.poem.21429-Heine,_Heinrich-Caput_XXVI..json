{"dta.poem.21429": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eich k\u00fcsse dich, und ich f\u00fchle wie mich", "tokens": ["\u201e", "ich", "k\u00fcs\u00b7se", "dich", ",", "und", "ich", "f\u00fch\u00b7le", "wie", "mich"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "KOKOM", "PPER"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dein Genius begeistert;", "tokens": ["Dein", "Ge\u00b7nius", "be\u00b7geis\u00b7tert", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Es hat ein wunderbarer Rausch", "tokens": ["Es", "hat", "ein", "wun\u00b7der\u00b7ba\u00b7rer", "Rausch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich meiner Seele bemeistert.", "tokens": ["Sich", "mei\u00b7ner", "See\u00b7le", "be\u00b7meis\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u201emir ist, als ob ich auf der Stra\u00df\u2019", "tokens": ["\u201e", "mir", "ist", ",", "als", "ob", "ich", "auf", "der", "Stra\u00df'"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "$,", "KOKOM", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Nachtw\u00e4chter singen h\u00f6rte \u2013", "tokens": ["Die", "Nacht\u00b7w\u00e4ch\u00b7ter", "sin\u00b7gen", "h\u00f6r\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$("], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Es sind Hymeneen, Hochzeitmusik,", "tokens": ["Es", "sind", "Hy\u00b7me\u00b7ne\u00b7en", ",", "Hoch\u00b7zeit\u00b7mu\u00b7sik", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mein s\u00fc\u00dfer Lustgef\u00e4hrte!", "tokens": ["Mein", "s\u00fc\u00b7\u00dfer", "Lust\u00b7ge\u00b7f\u00e4hr\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201ejetzt kommen die reitenden Diener auch,", "tokens": ["\u201e", "jetzt", "kom\u00b7men", "die", "rei\u00b7ten\u00b7den", "Die\u00b7ner", "auch", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mit \u00fcppig lodernden Fackeln,", "tokens": ["Mit", "\u00fcp\u00b7pig", "lo\u00b7dern\u00b7den", "Fa\u00b7ckeln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie tanzen ehrbar den Fackeltanz,", "tokens": ["Sie", "tan\u00b7zen", "ehr\u00b7bar", "den", "Fa\u00b7ckel\u00b7tanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie springen und h\u00fcpfen und wackeln.", "tokens": ["Sie", "sprin\u00b7gen", "und", "h\u00fcp\u00b7fen", "und", "wa\u00b7ckeln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "\u201ees kommt der hoch- und wohlweise Senat,", "tokens": ["\u201e", "es", "kommt", "der", "hoch", "und", "wohl\u00b7wei\u00b7se", "Se\u00b7nat", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+---", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es kommen die Oberalten;", "tokens": ["Es", "kom\u00b7men", "die", "O\u00b7be\u00b7ral\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der B\u00fcrgermeister r\u00e4uspert sich", "tokens": ["Der", "B\u00fcr\u00b7ger\u00b7meis\u00b7ter", "r\u00e4us\u00b7pert", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und will eine Rede halten.", "tokens": ["Und", "will", "ei\u00b7ne", "Re\u00b7de", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}