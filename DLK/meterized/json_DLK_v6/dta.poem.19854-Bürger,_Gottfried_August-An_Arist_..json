{"dta.poem.19854": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "An Arist .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn der gute Himmel mir             ", "tokens": ["Wenn", "der", "gu\u00b7te", "Him\u00b7mel", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewig, ewig doch verg\u00f6nte,", "tokens": ["E\u00b7wig", ",", "e\u00b7wig", "doch", "ver\u00b7g\u00f6n\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich, braver Man, mit dir", "tokens": ["Da\u00df", "ich", ",", "bra\u00b7ver", "Man", ",", "mit", "dir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "VVFIN", "PIS", "$,", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Tage leben k\u00f6nte!", "tokens": ["Mei\u00b7ne", "Ta\u00b7ge", "le\u00b7ben", "k\u00f6n\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimmer, nimmer wolt\u2019 ich dann,", "tokens": ["Nim\u00b7mer", ",", "nim\u00b7mer", "wolt'", "ich", "dann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VMFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Noch nach andern Freuden jagen.", "tokens": ["Noch", "nach", "an\u00b7dern", "Freu\u00b7den", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ja, f\u00fcrwahr! ich wolte dran", "tokens": ["Ja", ",", "f\u00fcr\u00b7wahr", "!", "ich", "wol\u00b7te", "dran"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "$.", "PPER", "VMFIN", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kein gemeines Opfer wagen.", "tokens": ["Kein", "ge\u00b7mei\u00b7nes", "Op\u00b7fer", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Lieb\u2019 und Wein wolt\u2019 ich entsagen,", "tokens": ["Lieb'", "und", "Wein", "wolt'", "ich", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Deren doch ein froher Man", "tokens": ["De\u00b7ren", "doch", "ein", "fro\u00b7her", "Man"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Nicht gar leicht entrathen kan.", "tokens": ["Nicht", "gar", "leicht", "ent\u00b7ra\u00b7then", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}