{"dta.poem.2589": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Wonneburg der Liebe/  \n  Auf Hn. C. W. u. J. H. C. V. Hochzeit/  \n den 3. Septembr. 1680.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es stieg die Venus gleich aus ihrem Schwanen-Bette/", "tokens": ["Es", "stieg", "die", "Ve\u00b7nus", "gleich", "aus", "ih\u00b7rem", "Schwa\u00b7nen\u00b7Bet\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und h\u00fcllt\u2019 in zarten Flor den Schnee der Glieder ein/", "tokens": ["Und", "h\u00fcllt'", "in", "zar\u00b7ten", "Flor", "den", "Schnee", "der", "Glie\u00b7der", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "ART", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In dem die Gratien umb sie gesch\u00e4fftig seyn/", "tokens": ["In", "dem", "die", "Gra\u00b7ti\u00b7en", "umb", "sie", "ge\u00b7sch\u00e4ff\u00b7tig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "APPR", "PPER", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihre treue Dienst erweisen in die Wette.", "tokens": ["Und", "ih\u00b7re", "treu\u00b7e", "Dienst", "er\u00b7wei\u00b7sen", "in", "die", "Wet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die flichtet Perl und Gold in die gerollten Haare/", "tokens": ["Die", "flich\u00b7tet", "Perl", "und", "Gold", "in", "die", "ge\u00b7roll\u00b7ten", "Haa\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein andre ziert die Brust mit der Eder Wahre.", "tokens": ["Ein", "and\u00b7re", "ziert", "die", "Brust", "mit", "der", "E\u00b7der", "Wah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Sie schien nun satt geschm\u00fcckt das Engel-gleiche Weib/", "tokens": ["Sie", "schien", "nun", "satt", "ge\u00b7schm\u00fcckt", "das", "En\u00b7gel\u00b7glei\u00b7che", "Weib", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "VVPP", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als/ wie der schnelle Blitz und wie der Parther Pfeile/", "tokens": ["Als", "/", "wie", "der", "schnel\u00b7le", "Blitz", "und", "wie", "der", "Par\u00b7ther", "Pfei\u00b7le", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "KOKOM", "ART", "ADJA", "NN", "KON", "PWAV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Cupido zu ihr rennt; Rufft/ Mutter/ auf und eile", "tokens": ["Cu\u00b7pi\u00b7do", "zu", "ihr", "rennt", ";", "Rufft", "/", "Mut\u00b7ter", "/", "auf", "und", "ei\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPR", "PPER", "VVFIN", "$.", "NN", "$(", "NN", "$(", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wo mich getragen hat dein Lilgen-reicher Leib/", "tokens": ["Wo", "mich", "ge\u00b7tra\u00b7gen", "hat", "dein", "Lil\u00b7gen\u00b7rei\u00b7cher", "Leib", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und wo du Mutter bist/ so komm\u2019 und la\u00df dich leiten/", "tokens": ["Und", "wo", "du", "Mut\u00b7ter", "bist", "/", "so", "komm'", "und", "la\u00df", "dich", "lei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "NN", "VAFIN", "$(", "ADV", "VVFIN", "KON", "VVIMP", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "In eine ", "tokens": ["In", "ei\u00b7ne"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Ich habe selbst darzu den Grundstein erst gelegt/", "tokens": ["Ich", "ha\u00b7be", "selbst", "dar\u00b7zu", "den", "Grund\u00b7stein", "erst", "ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PAV", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Me\u00dfstab/ den du sihst/ hat alles abgemessen/", "tokens": ["Der", "Me\u00df\u00b7stab", "/", "den", "du", "sihst", "/", "hat", "al\u00b7les", "ab\u00b7ge\u00b7mes\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PPER", "VVFIN", "$(", "VAFIN", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Jedweder Balcken ist gezimmert von Cypressen/", "tokens": ["Jed\u00b7we\u00b7der", "Bal\u00b7cken", "ist", "ge\u00b7zim\u00b7mert", "von", "Cyp\u00b7res\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "APPR", "NE", "$("], "meter": "++-+-+-+--+--", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Und thenrem Cedern-Holtz/ das Libanon sonst tr\u00e4gt/", "tokens": ["Und", "then\u00b7rem", "Ce\u00b7dern\u00b7Holtz", "/", "das", "Li\u00b7ba\u00b7non", "sonst", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$(", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das Pflaster ist gemacht von Jaspis und Achaten/", "tokens": ["Das", "Pflas\u00b7ter", "ist", "ge\u00b7macht", "von", "Jas\u00b7pis", "und", "A\u00b7cha\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "NE", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und Rosen drein gesetzt von brennenden Granaten.", "tokens": ["Und", "Ro\u00b7sen", "drein", "ge\u00b7setzt", "von", "bren\u00b7nen\u00b7den", "Gra\u00b7na\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Den Eingang ziert ein Thor von klarem Helffenbein/", "tokens": ["Den", "Ein\u00b7gang", "ziert", "ein", "Thor", "von", "kla\u00b7rem", "Helf\u00b7fen\u00b7bein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der Hof gr\u00fcnt von Smaragd und bl\u00fcht von Amethisten/", "tokens": ["Der", "Hof", "gr\u00fcnt", "von", "Sma\u00b7ragd", "und", "bl\u00fcht", "von", "A\u00b7met\u00b7his\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "VVFIN", "APPR", "NE", "$("], "meter": "-++-++-+-+-+-", "measure": "unknown.measure.septa"}, "line.21": {"text": "Es mu\u00df der Hyacinth sich an den Mauren br\u00fcsten.", "tokens": ["Es", "mu\u00df", "der", "Hya\u00b7cin\u00b7th", "sich", "an", "den", "Mau\u00b7ren", "br\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die W\u00e4nde putzt nicht nur ein seltner Marmelstein/", "tokens": ["Die", "W\u00e4n\u00b7de", "putzt", "nicht", "nur", "ein", "selt\u00b7ner", "Mar\u00b7mel\u00b7stein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Sardonich und Topa\u00df vermischen sich Ophiten/", "tokens": ["Sar\u00b7do\u00b7nich", "und", "To\u00b7pa\u00df", "ver\u00b7mi\u00b7schen", "sich", "O\u00b7phi\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "VVFIN", "PRF", "NN", "$("], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "Es blutet der Rubin bey gr\u00fcnen Chrysoliten.", "tokens": ["Es", "blu\u00b7tet", "der", "Ru\u00b7bin", "bey", "gr\u00fc\u00b7nen", "Chry\u00b7so\u00b7li\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.25": {"text": "Die Seulen sind Berill/ mit denen der Saffir/", "tokens": ["Die", "Seu\u00b7len", "sind", "Be\u00b7rill", "/", "mit", "de\u00b7nen", "der", "Saf\u00b7fir", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NE", "$(", "APPR", "PRELS", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Verm\u00e4hlet seine Pracht/ und die gewelbte Decke/", "tokens": ["Ver\u00b7m\u00e4h\u00b7let", "sei\u00b7ne", "Pracht", "/", "und", "die", "ge\u00b7welb\u00b7te", "De\u00b7cke", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "KON", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Besch\u00e4mt den Himmel fast; das Blut der Purpur-Schnecke/", "tokens": ["Be\u00b7sch\u00e4mt", "den", "Him\u00b7mel", "fast", ";", "das", "Blut", "der", "Pur\u00b7pur\u00b7Schne\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$.", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und was vor Schmeltzwerck mehr erhebt der Muscheln Zier/", "tokens": ["Und", "was", "vor", "Sch\u00b7melt\u00b7zwerck", "mehr", "er\u00b7hebt", "der", "Mu\u00b7scheln", "Zier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "ADV", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Ja Farben auf sie mahlt die \u00fcber Pinsel-Striche/", "tokens": ["Ja", "Far\u00b7ben", "auf", "sie", "mahlt", "die", "\u00fc\u00b7ber", "Pin\u00b7sel\u00b7Stri\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "APPR", "PPER", "VVFIN", "ART", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Sind w\u00fcrdig da\u00df man sie mit diesem Glantz vergliche.", "tokens": ["Sind", "w\u00fcr\u00b7dig", "da\u00df", "man", "sie", "mit", "die\u00b7sem", "Glantz", "ver\u00b7gli\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOUS", "PIS", "PPER", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Jedweder Zimmer prangt mit seinem edlen Stein/", "tokens": ["Jed\u00b7we\u00b7der", "Zim\u00b7mer", "prangt", "mit", "sei\u00b7nem", "ed\u00b7len", "Stein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die Fenster sind erleucht von Demant und Chrystallen/", "tokens": ["Die", "Fens\u00b7ter", "sind", "er\u00b7leucht", "von", "De\u00b7mant", "und", "Chrys\u00b7tal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+--+-++-", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "Es darff kein Sonnen-Strahl in ihre Spiegel fallen/", "tokens": ["Es", "darff", "kein", "Son\u00b7nen\u00b7Strahl", "in", "ih\u00b7re", "Spie\u00b7gel", "fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Sie haben an sich selbst ein ewig Licht und Schein.", "tokens": ["Sie", "ha\u00b7ben", "an", "sich", "selbst", "ein", "e\u00b7wig", "Licht", "und", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PRF", "ADV", "ART", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der Leib des Chalcedons/ das Fleisch der Carniolen/", "tokens": ["Der", "Leib", "des", "Chal\u00b7ce\u00b7dons", "/", "das", "Fleisch", "der", "Car\u00b7ni\u00b7o\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ist von der Rahmen Rand als schlechter Zeug zu holen.", "tokens": ["Ist", "von", "der", "Rah\u00b7men", "Rand", "als", "schlech\u00b7ter", "Zeug", "zu", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "KOUS", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die Tische sind klamm Gold/ darein des K\u00fcnstlers Hand", "tokens": ["Die", "Ti\u00b7sche", "sind", "klamm", "Gold", "/", "da\u00b7rein", "des", "K\u00fcnst\u00b7lers", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "NN", "$(", "PAV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Geschichte jener Zeit und der Verliebten Tr\u00e4ume", "tokens": ["Ge\u00b7schich\u00b7te", "je\u00b7ner", "Zeit", "und", "der", "Ver\u00b7lieb\u00b7ten", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PDAT", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Vortreflich hat gepr\u00e4gt; wie theils verkehrt in B\u00e4ume/", "tokens": ["Vor\u00b7tre\u00b7flich", "hat", "ge\u00b7pr\u00e4gt", ";", "wie", "theils", "ver\u00b7kehrt", "in", "B\u00e4u\u00b7me", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$.", "PWAV", "ADV", "VVPP", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Theils Blumen worden sind durch den so s\u00fcssen Brand.", "tokens": ["Theils", "Blu\u00b7men", "wor\u00b7den", "sind", "durch", "den", "so", "s\u00fcs\u00b7sen", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAPP", "VAFIN", "APPR", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "Narcissens Sch\u00f6nheit schmiltzt in einem klaren Bronnen/", "tokens": ["Nar\u00b7cis\u00b7sens", "Sch\u00f6n\u00b7heit", "schmiltzt", "in", "ei\u00b7nem", "kla\u00b7ren", "Bron\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und Clytie buhlt noch mit ihrem Mann/ der Sonnen.", "tokens": ["Und", "Cly\u00b7tie", "buhlt", "noch", "mit", "ih\u00b