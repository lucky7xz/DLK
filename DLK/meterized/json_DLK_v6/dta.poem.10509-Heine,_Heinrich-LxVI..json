{"dta.poem.10509": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Mir tr\u00e4umt': ich bin der liebe Gott,", "tokens": ["Mir", "tr\u00e4umt'", ":", "ich", "bin", "der", "lie\u00b7be", "Gott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sitz' im Himmel droben,", "tokens": ["Und", "sitz'", "im", "Him\u00b7mel", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Englein sitzen um mich her,", "tokens": ["Und", "En\u00b7glein", "sit\u00b7zen", "um", "mich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die meine Verse loben.", "tokens": ["Die", "mei\u00b7ne", "Ver\u00b7se", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und Kuchen ess' ich und Confekt", "tokens": ["Und", "Ku\u00b7chen", "ess'", "ich", "und", "Con\u00b7fekt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr manchen lieben Gulden,", "tokens": ["F\u00fcr", "man\u00b7chen", "lie\u00b7ben", "Gul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Kardinal trink' ich dabei,", "tokens": ["Und", "Kar\u00b7di\u00b7nal", "trink'", "ich", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und habe keine Schulden.", "tokens": ["Und", "ha\u00b7be", "kei\u00b7ne", "Schul\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch Langeweile plagt mich sehr,", "tokens": ["Doch", "Lan\u00b7ge\u00b7wei\u00b7le", "plagt", "mich", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wollt', ich w\u00e4r' auf Erden,", "tokens": ["Ich", "wollt'", ",", "ich", "w\u00e4r'", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und w\u00e4r' ich nicht der liebe Gott,", "tokens": ["Und", "w\u00e4r'", "ich", "nicht", "der", "lie\u00b7be", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich k\u00f6nnt' des Teufels werden.", "tokens": ["Ich", "k\u00f6nnt'", "des", "Teu\u00b7fels", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du langer Engel Gabriel,", "tokens": ["Du", "lan\u00b7ger", "En\u00b7gel", "Ga\u00b7bri\u00b7el", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geh', mach' dich auf die Sohlen,", "tokens": ["Geh'", ",", "mach'", "dich", "auf", "die", "Soh\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und meinen theuren Freund Eugen", "tokens": ["Und", "mei\u00b7nen", "theu\u00b7ren", "Freund", "Eu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "NE"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sollst du herauf mir holen.", "tokens": ["Sollst", "du", "her\u00b7auf", "mir", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Such' ihn nicht im Collegium,", "tokens": ["Such'", "ihn", "nicht", "im", "Col\u00b7le\u00b7gi\u00b7um", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Such' ihn beim Glas Tokaier;", "tokens": ["Such'", "ihn", "beim", "Glas", "To\u00b7kai\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Such' ihn nicht in der Hedwigskirch,", "tokens": ["Such'", "ihn", "nicht", "in", "der", "Hed\u00b7wigs\u00b7kirch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.4": {"text": "Such' ihn bei Mamsell Meyer.", "tokens": ["Such'", "ihn", "bei", "Mam\u00b7sell", "Me\u00b7yer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Da breitet aus sein Fl\u00fcgelpaar", "tokens": ["Da", "brei\u00b7tet", "aus", "sein", "Fl\u00fc\u00b7gel\u00b7paar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und fliegt herab der Engel,", "tokens": ["Und", "fliegt", "her\u00b7ab", "der", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und packt ihn auf, und bringt herauf", "tokens": ["Und", "packt", "ihn", "auf", ",", "und", "bringt", "her\u00b7auf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Freund, den lieben Bengel.", "tokens": ["Den", "Freund", ",", "den", "lie\u00b7ben", "Ben\u00b7gel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ja, Jung', ich bin der liebe Gott,", "tokens": ["Ja", ",", "Jung'", ",", "ich", "bin", "der", "lie\u00b7be", "Gott", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NE", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ich regier' die Erde!", "tokens": ["Und", "ich", "re\u00b7gier'", "die", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab's ja immer dir gesagt,", "tokens": ["Ich", "hab's", "ja", "im\u00b7mer", "dir", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich was Rechts noch werde.", "tokens": ["Da\u00df", "ich", "was", "Rechts", "noch", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und Wunder thu' ich alle Tag,", "tokens": ["Und", "Wun\u00b7der", "thu'", "ich", "al\u00b7le", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die sollen dich entz\u00fccken,", "tokens": ["Die", "sol\u00b7len", "dich", "ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und dir zum Spa\u00dfe will ich heut", "tokens": ["Und", "dir", "zum", "Spa\u00b7\u00dfe", "will", "ich", "heut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Stadt Ix-Ix begl\u00fccken.", "tokens": ["Die", "Stadt", "Ix\u00b7Ix", "be\u00b7gl\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die Pflastersteine auf der Stra\u00df',", "tokens": ["Die", "Pflas\u00b7ter\u00b7stei\u00b7ne", "auf", "der", "Stra\u00df'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die sollen jetzt sich spalten,", "tokens": ["Die", "sol\u00b7len", "jetzt", "sich", "spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und eine Auster, frisch und klar,", "tokens": ["Und", "ei\u00b7ne", "Aus\u00b7ter", ",", "frisch", "und", "klar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll jeder Stein enthalten.", "tokens": ["Soll", "je\u00b7der", "Stein", "ent\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ein Regen von Zitronensaft", "tokens": ["Ein", "Re\u00b7gen", "von", "Zit\u00b7ro\u00b7nen\u00b7saft"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Soll thauig sie begiessen,", "tokens": ["Soll", "thau\u00b7ig", "sie", "be\u00b7gies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und in den Stra\u00dfeng\u00f6ssen soll", "tokens": ["Und", "in", "den", "Stra\u00b7\u00dfen\u00b7g\u00f6s\u00b7sen", "soll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der beste Rheinwein flie\u00dfen.", "tokens": ["Der", "bes\u00b7te", "Rhein\u00b7wein", "flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Wie freuen die Ix-Ixer sich,", "tokens": ["Wie", "freu\u00b7en", "die", "Ix\u00b7I\u00b7xer", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PRF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie gehen schon an's Fressen;", "tokens": ["Sie", "ge\u00b7hen", "schon", "an's", "Fres\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Herren von dem Landgericht,", "tokens": ["Die", "Her\u00b7ren", "von", "dem", "Land\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die saufen aus den G\u00f6ssen.", "tokens": ["Die", "sau\u00b7fen", "aus", "den", "G\u00f6s\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Wie freuen die Poeten sich", "tokens": ["Wie", "freu\u00b7en", "die", "Po\u00b7et\u00b7en", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei solchem G\u00f6tterfra\u00dfe!", "tokens": ["Bei", "sol\u00b7chem", "G\u00f6t\u00b7ter\u00b7fra\u00b7\u00dfe", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Leutnants und die F\u00e4hnderichs,", "tokens": ["Die", "Leut\u00b7nants", "und", "die", "F\u00e4hn\u00b7de\u00b7richs", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die lecken ab die Stra\u00dfe.", "tokens": ["Die", "le\u00b7cken", "ab", "die", "Stra\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Die Leutnants und die F\u00e4hnderichs,", "tokens": ["Die", "Leut\u00b7nants", "und", "die", "F\u00e4hn\u00b7de\u00b7richs", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das sind die kl\u00fcgsten Leute,", "tokens": ["Das", "sind", "die", "kl\u00fcgs\u00b7ten", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie denken, alle Tag' geschieht", "tokens": ["Sie", "den\u00b7ken", ",", "al\u00b7le", "Tag'", "ge\u00b7schieht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Wunder so wie heute.", "tokens": ["Kein", "Wun\u00b7der", "so", "wie", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "KOKOM", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}