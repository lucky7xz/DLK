{"textgrid.poem.54991": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Selbstbetrug", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Vorhang schwebet hin und her", "tokens": ["Der", "Vor\u00b7hang", "schwe\u00b7bet", "hin", "und", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei meiner Nachbarin.", "tokens": ["Bei", "mei\u00b7ner", "Nach\u00b7ba\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gewi\u00df, sie lauschet \u00fcberquer,", "tokens": ["Ge\u00b7wi\u00df", ",", "sie", "lau\u00b7schet", "\u00fc\u00b7berq\u00b7uer", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob ich zu Hause bin", "tokens": ["Ob", "ich", "zu", "Hau\u00b7se", "bin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und ob der eifers\u00fccht'ge Groll,", "tokens": ["Und", "ob", "der", "ei\u00b7fer\u00b7s\u00fccht'\u00b7ge", "Groll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den ich am Tag gehegt,", "tokens": ["Den", "ich", "am", "Tag", "ge\u00b7hegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sich, wie er nun auf immer soll,", "tokens": ["Sich", ",", "wie", "er", "nun", "auf", "im\u00b7mer", "soll", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$,", "PWAV", "PPER", "ADV", "APPR", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im tiefen Herzen regt.", "tokens": ["Im", "tie\u00b7fen", "Her\u00b7zen", "reg\u00b7t."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["APPRART", "ADJA", "NN", "NE"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Doch leider hat das sch\u00f6ne Kind", "tokens": ["Doch", "lei\u00b7der", "hat", "das", "sch\u00f6\u00b7ne", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dergleichen nicht gef\u00fchlt.", "tokens": ["Derg\u00b7lei\u00b7chen", "nicht", "ge\u00b7f\u00fchlt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich seh, es ist der Abendwind,", "tokens": ["Ich", "seh", ",", "es", "ist", "der", "Ab\u00b7end\u00b7wind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der mit dem Vorhang spielt.", "tokens": ["Der", "mit", "dem", "Vor\u00b7hang", "spielt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}