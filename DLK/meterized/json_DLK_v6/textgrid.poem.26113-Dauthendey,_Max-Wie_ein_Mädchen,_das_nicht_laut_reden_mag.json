{"textgrid.poem.26113": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Wie ein M\u00e4dchen, das nicht laut reden mag", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Still wolkenverhangen der Maientag.", "tokens": ["Still", "wol\u00b7ken\u00b7ver\u00b7han\u00b7gen", "der", "Mai\u00b7en\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Gr\u00fcn d\u00e4mmerig kommt er durchs Gras gegangen,", "tokens": ["Gr\u00fcn", "d\u00e4m\u00b7me\u00b7rig", "kommt", "er", "durchs", "Gras", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Steht am Berg unter Bl\u00fctenb\u00e4umen befangen,", "tokens": ["Steht", "am", "Berg", "un\u00b7ter", "Bl\u00fc\u00b7ten\u00b7b\u00e4u\u00b7men", "be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wie ein M\u00e4dchen, das nicht laut reden mag,", "tokens": ["Wie", "ein", "M\u00e4d\u00b7chen", ",", "das", "nicht", "laut", "re\u00b7den", "mag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Seit sie heimlich zur Nacht bei dem Liebsten lag", "tokens": ["Seit", "sie", "heim\u00b7lich", "zur", "Nacht", "bei", "dem", "Liebs\u00b7ten", "lag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPRART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Und mu\u00df die zu roten Lippen jetzt h\u00fcten.", "tokens": ["Und", "mu\u00df", "die", "zu", "ro\u00b7ten", "Lip\u00b7pen", "jetzt", "h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "PTKZU", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Gedankenlos lacht jeder Vogel im Hag,", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7los", "lacht", "je\u00b7der", "Vo\u00b7gel", "im", "Hag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Aber M\u00e4dchen, wenn sie lieben, die halten sich sacht", "tokens": ["A\u00b7ber", "M\u00e4d\u00b7chen", ",", "wenn", "sie", "lie\u00b7ben", ",", "die", "hal\u00b7ten", "sich", "sacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "KOUS", "PPER", "VVINF", "$,", "PRELS", "VVFIN", "PRF", "VVFIN"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Und werden erst unbedacht wieder zur n\u00e4chsten Nacht.", "tokens": ["Und", "wer\u00b7den", "erst", "un\u00b7be\u00b7dacht", "wie\u00b7der", "zur", "n\u00e4chs\u00b7ten", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}