{"dta.poem.18162": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die Absicht GOttes  \n warum er die Blumen erschaffen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als GOtt die Welt erschuf hat seine weise", "tokens": ["Als", "Gott", "die", "Welt", "er\u00b7schuf", "hat", "sei\u00b7ne", "wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Macht,", "tokens": ["Macht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Wie woll die Einfalt gl\u00e4ubt; doch", "tokens": ["Wie", "woll", "die", "Ein\u00b7falt", "gl\u00e4ubt", ";", "doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$.", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "nichts herf\u00fcrgebracht", "tokens": ["nichts", "her\u00b7f\u00fcr\u00b7ge\u00b7bracht"], "token_info": ["word", "word"], "pos": ["PIS", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Dabei er nicht zugleich auf ihren", "tokens": ["Da\u00b7bei", "er", "nicht", "zu\u00b7gleich", "auf", "ih\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "PTKNEG", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zwek gesehen:", "tokens": ["Zwek", "ge\u00b7se\u00b7hen", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Wer dieses nur bedenkt, mu\u00df es", "tokens": ["Wer", "die\u00b7ses", "nur", "be\u00b7denkt", ",", "mu\u00df", "es"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PDAT", "ADV", "VVFIN", "$,", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "als wahr gestehen.", "tokens": ["als", "wahr", "ge\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Jm Kreaturen Heer, das keiner \u00fcberz\u00e4hlt", "tokens": ["Jm", "Kre\u00b7a\u00b7tu\u00b7ren", "Heer", ",", "das", "kei\u00b7ner", "\u00fc\u00b7berz\u00b7\u00e4hlt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "$,", "PRELS", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und was dazu geh\u00f6rt, hat er nichts auserw\u00e4hlt", "tokens": ["Und", "was", "da\u00b7zu", "ge\u00b7h\u00f6rt", ",", "hat", "er", "nichts", "au\u00b7ser\u00b7w\u00e4hlt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PAV", "VVFIN", "$,", "VAFIN", "PPER", "PIS", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dabei nicht sein Verstand, ein Allmachts weises Wollen,", "tokens": ["Da\u00b7bei", "nicht", "sein", "Ver\u00b7stand", ",", "ein", "All\u00b7machts", "wei\u00b7ses", "Wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "PPOSAT", "NN", "$,", "ART", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein jedes Ding zum Zwek, zum Nutz bestimmen sollen.", "tokens": ["Ein", "je\u00b7des", "Ding", "zum", "Zwek", ",", "zum", "Nutz", "be\u00b7stim\u00b7men", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wir leugnen dieses nicht, es scheint auch sonder Streit", "tokens": ["Wir", "leug\u00b7nen", "die\u00b7ses", "nicht", ",", "es", "scheint", "auch", "son\u00b7der", "Streit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "PTKNEG", "$,", "PPER", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da er die Welt gemacht, hat er die Herrlichkeit", "tokens": ["Da", "er", "die", "Welt", "ge\u00b7macht", ",", "hat", "er", "die", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die seine Hoheit ziert, dem Menschen vorgeleget,", "tokens": ["Die", "sei\u00b7ne", "Ho\u00b7heit", "ziert", ",", "dem", "Men\u00b7schen", "vor\u00b7ge\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die am Gesch\u00f6pfen strahlt und die Vernunft erweget.", "tokens": ["Die", "am", "Ge\u00b7sch\u00f6p\u00b7fen", "strahlt", "und", "die", "Ver\u00b7nunft", "er\u00b7we\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Doch wenn man sich zugleich im Reiche der Natur,", "tokens": ["Doch", "wenn", "man", "sich", "zu\u00b7gleich", "im", "Rei\u00b7che", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PRF", "ADV", "APPRART", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Bem\u00fcht den Zwek zu sehn an jeder Kreatur", "tokens": ["Be\u00b7m\u00fcht", "den", "Zwek", "zu", "sehn", "an", "je\u00b7der", "Kre\u00b7a\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKZU", "VVINF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wornach der Wiz stets forscht; so wird man allzeit", "tokens": ["Wor\u00b7nach", "der", "Wiz", "stets", "forscht", ";", "so", "wird", "man", "all\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "VVPP", "$.", "ADV", "VAFIN", "PIS", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.20": {"text": "finden,", "tokens": ["fin\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "Da\u00df sich der Menschen Lust und Nutz darin verbinden.", "tokens": ["Da\u00df", "sich", "der", "Men\u00b7schen", "Lust", "und", "Nutz", "da\u00b7rin", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "NN", "KON", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ein Beispiel leget uns der Blumen bunte Schaar,", "tokens": ["Ein", "Bei\u00b7spiel", "le\u00b7get", "uns", "der", "Blu\u00b7men", "bun\u00b7te", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die allenthalben prangt, vor unsern Augen dar.", "tokens": ["Die", "al\u00b7len\u00b7thal\u00b7ben", "prangt", ",", "vor", "un\u00b7sern", "Au\u00b7gen", "dar", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Man fr\u00e4gt warum die Macht, die \u00fcber alles gehet,", "tokens": ["Man", "fr\u00e4gt", "wa\u00b7rum", "die", "Macht", ",", "die", "\u00fc\u00b7ber", "al\u00b7les", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PWAV", "ART", "NN", "$,", "PRELS", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der Erden schwarzen Grund mit solchen Schmuk", "tokens": ["Der", "Er\u00b7den", "schwar\u00b7zen", "Grund", "mit", "sol\u00b7chen", "Schmuk"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "bes\u00e4et?", "tokens": ["be\u00b7s\u00e4et", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.27": {"text": "Wer an die Weisheit denkt, die nichts vergeblich thut,", "tokens": ["Wer", "an", "die", "Weis\u00b7heit", "denkt", ",", "die", "nichts", "ver\u00b7geb\u00b7lich", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "PRELS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wer dabei noch erwegt, da\u00df ", "tokens": ["Wer", "da\u00b7bei", "noch", "er\u00b7wegt", ",", "da\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "PAV", "ADV", "VVPP", "$,", "KOUS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Der wird mit der Vernunft gar bald begreiflich sp\u00fcren,", "tokens": ["Der", "wird", "mit", "der", "Ver\u00b7nunft", "gar", "bald", "be\u00b7greif\u00b7lich", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Es soll ihr bunter Schein durchs Aug das Herze", "tokens": ["Es", "soll", "ihr", "bun\u00b7ter", "Schein", "durchs", "Aug", "das", "Her\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "r\u00fchren;", "tokens": ["r\u00fch\u00b7ren", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.32": {"text": "Es soll der Farben Pracht, ihr Gold und Silber-", "tokens": ["Es", "soll", "der", "Far\u00b7ben", "Pracht", ",", "ihr", "Gold", "und", "Sil\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "$,", "PPOSAT", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "schein", "tokens": ["schein"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+", "measure": "single.up"}, "line.34": {"text": "Der Sinnen holde Lust, des Herzens Labsal sein;", "tokens": ["Der", "Sin\u00b7nen", "hol\u00b7de", "Lust", ",", "des", "Her\u00b7zens", "Lab\u00b7sal", "sein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Es soll die Lieblichkeit, die aus denselben quillet,", "tokens": ["Es", "soll", "die", "Lieb\u00b7lich\u00b7keit", ",", "die", "aus", "den\u00b7sel\u00b7ben", "quil\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "PRELS", "APPR", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ein Lebensbalsam sein, wenn er die Lung erf\u00fcllet;", "tokens": ["Ein", "Le\u00b7bens\u00b7bal\u00b7sam", "sein", ",", "wenn", "er", "die", "Lung", "er\u00b7f\u00fcl\u00b7let", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAINF", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Es soll derselben Saft, der in den Bl\u00e4ttern stekt,", "tokens": ["Es", "soll", "der\u00b7sel\u00b7ben", "Saft", ",", "der", "in", "den", "Bl\u00e4t\u00b7tern", "stekt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDAT", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wenn ihn die Kunst auspre\u00dft und ihn ein Kranker", "tokens": ["Wenn", "ihn", "die", "Kunst", "aus\u00b7pre\u00dft", "und", "ihn", "ein", "Kran\u00b7ker"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "KON", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "lekt", "tokens": ["lekt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.40": {"text": "F\u00fcr Angst und Mattigkeit, f\u00fcr mancherlei Beschwer-", "tokens": ["F\u00fcr", "Angst", "und", "Mat\u00b7tig\u00b7keit", ",", "f\u00fcr", "man\u00b7cher\u00b7lei", "Be\u00b7schwer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "PIAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "den", "tokens": ["den"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.42": {"text": "Die unsern K\u00f6rper qu\u00e4ln, ein heilsam Mittel wer-", "tokens": ["Die", "un\u00b7sern", "K\u00f6r\u00b7per", "qu\u00e4ln", ",", "ein", "heil\u00b7sam", "Mit\u00b7tel", "wer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$,", "ART", "ADJD", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "den.", "tokens": ["den", "."], "token_info": ["word", "punct"], "pos": ["ART", "$."], "meter": "-", "measure": "single.down"}, "line.44": {"text": "Da\u00df dies der Weisheit Zwek, des Sch\u00f6pfers Absicht", "tokens": ["Da\u00df", "dies", "der", "Weis\u00b7heit", "Zwek", ",", "des", "Sch\u00f6p\u00b7fers", "Ab\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PDS", "ART", "NN", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "sei,", "tokens": ["sei", ","], "token_info": ["word", "punct"], "pos": ["VAFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.46": {"text": "Dazu stimmt jederman mit seiner Meinung bei:", "tokens": ["Da\u00b7zu", "stimmt", "je\u00b7der\u00b7man", "mit", "sei\u00b7ner", "Mei\u00b7nung", "bei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Wie aber solte ", "tokens": ["Wie", "a\u00b7ber", "sol\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADV", "VMFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.48": {"text": "Wirft man dagegen ein, f\u00fcr all erschaffen haben?", "tokens": ["Wirft", "man", "da\u00b7ge\u00b7gen", "ein", ",", "f\u00fcr", "all", "er\u00b7schaf\u00b7fen", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PAV", "PTKVZ", "$,", "APPR", "PIAT", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Wer in der Niedrigkeit, im tiefen Staube sitzt", "tokens": ["Wer", "in", "der", "Nied\u00b7rig\u00b7keit", ",", "im", "tie\u00b7fen", "Stau\u00b7be", "sitzt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Und in dem Arbeits Joch bei armen Kummer schwitzt", "tokens": ["Und", "in", "dem", "Ar\u00b7beits", "Joch", "bei", "ar\u00b7men", "Kum\u00b7mer", "schwitzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NE", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Der mu\u00df das Gartenland, wo andre Blumen", "tokens": ["Der", "mu\u00df", "das", "Gar\u00b7ten\u00b7land", ",", "wo", "and\u00b7re", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "pun