{"dta.poem.10916": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die B\u00e4hren.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Beym Anblick dieser Bestien, wird auch der K\u00fchneste ver-\nstummen,", "tokens": ["Beym", "An\u00b7blick", "die\u00b7ser", "Be\u00b7sti\u00b7en", ",", "wird", "auch", "der", "K\u00fch\u00b7nes\u00b7te", "ver", "stum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "$,", "VAFIN", "ADV", "ART", "ADJA", "TRUNC", "VVINF", "$,"], "meter": "-+-+--+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Man stutzt, man h\u00e4lt sich in Gefahr, die schwere Bratzen re-", "tokens": ["Man", "stutzt", ",", "man", "h\u00e4lt", "sich", "in", "Ge\u00b7fahr", ",", "die", "schwe\u00b7re", "Brat\u00b7zen", "re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "PRF", "APPR", "NN", "$,", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Es drohen uns sechs offne Rachen; man sieht und h\u00f6rt recht", "tokens": ["Es", "dro\u00b7hen", "uns", "sechs", "off\u00b7ne", "Ra\u00b7chen", ";", "man", "sieht", "und", "h\u00f6rt", "recht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "CARD", "ADJA", "NN", "$.", "PIS", "VVFIN", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ein wildes heiseres Gebr\u00fcll, ein Mord - und Blutbegierges", "tokens": ["Ein", "wil\u00b7des", "hei\u00b7se\u00b7res", "Ge\u00b7br\u00fcll", ",", "ein", "Mord", "und", "Blut\u00b7be\u00b7gier\u00b7ges"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "ART", "NN", "$(", "KON", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Man sieht hier schwere Lasten Fleisch, von Grimm und", "tokens": ["Man", "sieht", "hier", "schwe\u00b7re", "Las\u00b7ten", "Fleisch", ",", "von", "Grimm", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ADJA", "NN", "NN", "$,", "APPR", "NE", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und ob den einen gleich der Schlummer in einen tiefen", "tokens": ["Und", "ob", "den", "ei\u00b7nen", "gleich", "der", "Schlum\u00b7mer", "in", "ei\u00b7nen", "tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ART", "ADV", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "So f\u00fcrchtet man doch sein Erwachen. Doch halt! sie sind ja", "tokens": ["So", "f\u00fcrch\u00b7tet", "man", "doch", "sein", "Er\u00b7wa\u00b7chen", ".", "Doch", "halt", "!", "sie", "sind", "ja"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PPOSAT", "NN", "$.", "KON", "VVFIN", "$.", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Wir k\u00f6nnen sicher n\u00e4her treten! komm la\u00dft uns Farb und", "tokens": ["Wir", "k\u00f6n\u00b7nen", "si\u00b7cher", "n\u00e4\u00b7her", "tre\u00b7ten", "!", "komm", "la\u00dft", "uns", "Farb", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "VVINF", "$.", "VVFIN", "VVIMP", "PPER", "NN", "KON"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Mu\u00df jeder, so im Bild, als Urbild der starken B\u00e4ren, nicht gestehn,", "tokens": ["Mu\u00df", "je\u00b7der", ",", "so", "im", "Bild", ",", "als", "Ur\u00b7bild", "der", "star\u00b7ken", "B\u00e4\u00b7ren", ",", "nicht", "ge\u00b7stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "$,", "ADV", "APPRART", "NN", "$,", "KOUS", "NN", "ART", "ADJA", "NN", "$,", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.10": {"text": "Da\u00df auch aus ihnen, wie aus allem, ein helles Licht der All-", "tokens": ["Da\u00df", "auch", "aus", "ih\u00b7nen", ",", "wie", "aus", "al\u00b7lem", ",", "ein", "hel\u00b7les", "Licht", "der", "All"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPER", "$,", "PWAV", "APPR", "PIS", "$,", "ART", "ADJA", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "Die Maa\u00df, auch in so plumpen Gliedern, ein Art von", "tokens": ["Die", "Maa\u00df", ",", "auch", "in", "so", "plum\u00b7pen", "Glie\u00b7dern", ",", "ein", "Art", "von"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "ADV", "ADJA", "NN", "$,", "ART", "NN", "APPR"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Da er den Menschen ungereizt, nicht leichtlich, schadet noch", "tokens": ["Da", "er", "den", "Men\u00b7schen", "un\u00b7ge\u00b7reizt", ",", "nicht", "leicht\u00b7lich", ",", "scha\u00b7det", "noch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "$,", "PTKNEG", "ADJD", "$,", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.13": {"text": "Wo Hungen ihn nicht w\u00fctig macht, noch mehr, die uns er-", "tokens": ["Wo", "Hun\u00b7gen", "ihn", "nicht", "w\u00fc\u00b7tig", "macht", ",", "noch", "mehr", ",", "die", "uns", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "PPER", "PTKNEG", "ADJD", "VVFIN", "$,", "ADV", "ADV", "$,", "PRELS", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Womit er uns im Winter n\u00fctzt, sind einer weisen Sch\u00f6pfung", "tokens": ["Wo\u00b7mit", "er", "uns", "im", "Win\u00b7ter", "n\u00fctzt", ",", "sind", "ei\u00b7ner", "wei\u00b7sen", "Sch\u00f6p\u00b7fung"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.15": {"text": "Es wird auch eine weise Vorsorg, in diesem Thier, f\u00fcr uns geschaut,", "tokens": ["Es", "wird", "auch", "ei\u00b7ne", "wei\u00b7se", "Vor\u00b7sorg", ",", "in", "die\u00b7sem", "Thier", ",", "f\u00fcr", "uns", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,", "APPR", "PDAT", "NN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.16": {"text": "Wer wollte denn, auch in dem B\u00e4hren, des Sch\u00f6pfers Lieb und", "tokens": ["Wer", "woll\u00b7te", "denn", ",", "auch", "in", "dem", "B\u00e4h\u00b7ren", ",", "des", "Sch\u00f6p\u00b7fers", "Lieb", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ADV", "$,", "ADV", "APPR", "ART", "NN", "$,", "ART", "NN", "NN", "KON"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}}}}}