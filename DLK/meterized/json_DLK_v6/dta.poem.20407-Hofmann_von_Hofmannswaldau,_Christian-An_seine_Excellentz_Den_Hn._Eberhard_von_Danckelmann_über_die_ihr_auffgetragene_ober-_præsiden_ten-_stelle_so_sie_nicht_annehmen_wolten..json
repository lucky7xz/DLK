{"dta.poem.20407": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An seine Excellentz  \n Den Hn. Eberhard von Danckelmann/  \n \u00fcber die ihr auffgetragene ober- pr\u00e6siden ten-  \n stelle/ so sie nicht annehmen wolten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein F\u00fcrst erkl\u00e4rt dich heut zum ober-pr\u00e4sidenten/", "tokens": ["Dein", "F\u00fcrst", "er\u00b7kl\u00e4rt", "dich", "heut", "zum", "o\u00b7ber\u00b7pr\u00e4\u00b7si\u00b7den\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und thut was er vorl\u00e4ngst bey sich beschlossen hat.", "tokens": ["Und", "thut", "was", "er", "vor\u00b7l\u00e4ngst", "bey", "sich", "be\u00b7schlos\u00b7sen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "PPER", "ADV", "APPR", "PRF", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die selbst erheben es/ die es beneiden k\u00f6nten/", "tokens": ["Die", "selbst", "er\u00b7he\u00b7ben", "es", "/", "die", "es", "be\u00b7nei\u00b7den", "k\u00f6n\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PPER", "$(", "PRELS", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Du aber widerstrebst und klagst ob dieser that.", "tokens": ["Du", "a\u00b7ber", "wi\u00b7der\u00b7strebst", "und", "klagst", "ob", "die\u00b7ser", "that", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "KON", "VVFIN", "KOUS", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Allein wie lange will sich dein gehorsam sch\u00fctzen?", "tokens": ["Al\u00b7lein", "wie", "lan\u00b7ge", "will", "sich", "dein", "ge\u00b7hor\u00b7sam", "sch\u00fct\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADV", "VMFIN", "PRF", "PPOSAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nachdem du schon so offt dich dessen hast erwehrt.", "tokens": ["Nach\u00b7dem", "du", "schon", "so", "offt", "dich", "des\u00b7sen", "hast", "er\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "PPER", "PDS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Beschweret dich denn mehr da\u00df oben an zusitzen/", "tokens": ["Be\u00b7schwe\u00b7ret", "dich", "denn", "mehr", "da\u00df", "o\u00b7ben", "an", "zu\u00b7sit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "KOUS", "ADV", "APZR", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als dich die arbeit nicht mit ihrer last beschwert?", "tokens": ["Als", "dich", "die", "ar\u00b7beit", "nicht", "mit", "ih\u00b7rer", "last", "be\u00b7schwert", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was neues gibt man dir? was hastu mehr erhalten?", "tokens": ["Was", "neu\u00b7es", "gibt", "man", "dir", "?", "was", "has\u00b7tu", "mehr", "er\u00b7hal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "VVFIN", "PIS", "PPER", "$.", "PWS", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du nimmst ja nur den platz desselben amtes ein/", "tokens": ["Du", "nimmst", "ja", "nur", "den", "platz", "des\u00b7sel\u00b7ben", "am\u00b7tes", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "PDAT", "ADJA", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das dich Europa sieht vor langer zeit verwalten/", "tokens": ["Das", "dich", "Eu\u00b7ro\u00b7pa", "sieht", "vor", "lan\u00b7ger", "zeit", "ver\u00b7wal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "NE", "VVFIN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und woran es dich l\u00e4ngst glaubt in besitz zu seyn.", "tokens": ["Und", "wo\u00b7ran", "es", "dich", "l\u00e4ngst", "glaubt", "in", "be\u00b7sitz", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "ADV", "VVFIN", "APPR", "NE", "PTKZU", "VAINF", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Was fremde dir geweyht bey deiner schweren b\u00fcrde/", "tokens": ["Was", "frem\u00b7de", "dir", "ge\u00b7weyht", "bey", "dei\u00b7ner", "schwe\u00b7ren", "b\u00fcr\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "PPER", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wilstu da\u00df Friedrich nun es dir entziehen soll?", "tokens": ["Wils\u00b7tu", "da\u00df", "Fried\u00b7rich", "nun", "es", "dir", "ent\u00b7zie\u00b7hen", "soll", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KOUS", "NE", "ADV", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Nein/ wer die b\u00fcrde tr\u00e4gt/ tr\u00e4gt billich auch die w\u00fcrde/", "tokens": ["Nein", "/", "wer", "die", "b\u00fcr\u00b7de", "tr\u00e4gt", "/", "tr\u00e4gt", "bil\u00b7lich", "auch", "die", "w\u00fcr\u00b7de", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PWS", "ART", "NN", "VVFIN", "$(", "VVFIN", "ADJD", "ADV", "ART", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Di\u00df ist sein weisser spruch/ und dabey bleibt es wohl.", "tokens": ["Di\u00df", "ist", "sein", "weis\u00b7ser", "spruch", "/", "und", "da\u00b7bey", "bleibt", "es", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$(", "KON", "PAV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die h\u00f6he so dich schreckt/ die muste dich erh\u00f6hen/", "tokens": ["Die", "h\u00f6\u00b7he", "so", "dich", "schreckt", "/", "die", "mus\u00b7te", "dich", "er\u00b7h\u00f6\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPER", "VVFIN", "$(", "ART", "ADJA", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und wird zu deinem amt mit nutzen zugethan;", "tokens": ["Und", "wird", "zu", "dei\u00b7nem", "amt", "mit", "nut\u00b7zen", "zu\u00b7ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Je h\u00f6her einer steht/ ie weiter kan er sehen/", "tokens": ["Je", "h\u00f6\u00b7her", "ei\u00b7ner", "steht", "/", "ie", "wei\u00b7ter", "kan", "er", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "VVFIN", "$(", "ADV", "ADV", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Je besser auch dein aug den staat bewachen kan.", "tokens": ["Je", "bes\u00b7ser", "auch", "dein", "aug", "den", "staat", "be\u00b7wa\u00b7chen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PPOSAT", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und wem vermeynest du hierinn zu widerstreben?", "tokens": ["Und", "wem", "ver\u00b7mey\u00b7nest", "du", "hie\u00b7rinn", "zu", "wi\u00b7der\u00b7stre\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Nicht dencke/ da\u00df es blo\u00df ein werck der menschen sey;", "tokens": ["Nicht", "den\u00b7cke", "/", "da\u00df", "es", "blo\u00df", "ein", "werck", "der", "men\u00b7schen", "sey", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$(", "KOUS", "PPER", "ADV", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der himmel/ wie du weist/ mu\u00df ehr und reichthum geben/", "tokens": ["Der", "him\u00b7mel", "/", "wie", "du", "weist", "/", "mu\u00df", "ehr", "und", "reicht\u00b7hum", "ge\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "PPER", "VVFIN", "$(", "VMFIN", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der legt auch dir dein gl\u00fcck durch Friedrichs g\u00fcte bey.", "tokens": ["Der", "legt", "auch", "dir", "dein", "gl\u00fcck", "durch", "Fried\u00b7richs", "g\u00fc\u00b7te", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPER", "PPOSAT", "NN", "APPR", "NE", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Gewi\u00df k\u00f6nt itzt Schwerin/ nach seiner grossen seelen/", "tokens": ["Ge\u00b7wi\u00df", "k\u00f6nt", "itzt", "Schwe\u00b7rin", "/", "nach", "sei\u00b7ner", "gros\u00b7sen", "see\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "$(", "APPR", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die dich erleuchtet hat/ aus seinem grabe gehn/", "tokens": ["Die", "dich", "er\u00b7leuch\u00b7tet", "hat", "/", "aus", "sei\u00b7nem", "gra\u00b7be", "gehn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$(", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Er w\u00fcrde dich uns selbst als seine zucht empfehlen/", "tokens": ["Er", "w\u00fcr\u00b7de", "dich", "uns", "selbst", "als", "sei\u00b7ne", "zucht", "emp\u00b7feh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPER", "ADV", "KOUS", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und sagen da\u00df dich GOtt an seiner statt ersehn.", "tokens": ["Und", "sa\u00b7gen", "da\u00df", "dich", "Gott", "an", "sei\u00b7ner", "statt", "er\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "PPER", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}