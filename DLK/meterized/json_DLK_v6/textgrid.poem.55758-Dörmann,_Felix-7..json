{"textgrid.poem.55758": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' in Deiner Seele", "tokens": ["Ich", "hab'", "in", "Dei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das schlafende Feuer entdeckt,", "tokens": ["Das", "schla\u00b7fen\u00b7de", "Feu\u00b7er", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und seine verheerenden Gluten", "tokens": ["Und", "sei\u00b7ne", "ver\u00b7hee\u00b7ren\u00b7den", "Glu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mit tollem Jauchzen geweckt.", "tokens": ["Mit", "tol\u00b7lem", "Jauch\u00b7zen", "ge\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Die Flammen lodern und steigen,", "tokens": ["Die", "Flam\u00b7men", "lo\u00b7dern", "und", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mein Leib versiecht und verf\u00e4llt,", "tokens": ["Mein", "Leib", "ver\u00b7siecht", "und", "ver\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "In Schande, Blut und Vernichtung", "tokens": ["In", "Schan\u00b7de", ",", "Blut", "und", "Ver\u00b7nich\u00b7tung"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dein schmetterndes Lachen gellt.", "tokens": ["Dein", "schmet\u00b7tern\u00b7des", "La\u00b7chen", "gellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die blutige, blasse Madonna,", "tokens": ["Die", "blu\u00b7ti\u00b7ge", ",", "blas\u00b7se", "Ma\u00b7don\u00b7na", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit Augen br\u00e4unlich umringt,", "tokens": ["Mit", "Au\u00b7gen", "br\u00e4un\u00b7lich", "um\u00b7ringt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die stachlichte Knute der Liebe", "tokens": ["Die", "stach\u00b7lich\u00b7te", "Knu\u00b7te", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ins Herz mir, ins zuckende schwingt.", "tokens": ["Ins", "Herz", "mir", ",", "ins", "zu\u00b7cken\u00b7de", "schwingt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "$,", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Die dunkelroten Tropfen,", "tokens": ["Die", "dun\u00b7kel\u00b7ro\u00b7ten", "Trop\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie perlen langsam zu Tal,", "tokens": ["Sie", "per\u00b7len", "lang\u00b7sam", "zu", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und Leib und Seel' durchsch\u00fcttert", "tokens": ["Und", "Leib", "und", "Seel'", "durch\u00b7sch\u00fct\u00b7tert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die t\u00f6dlichste Wonne der Qual.", "tokens": ["Die", "t\u00f6d\u00b7lichs\u00b7te", "Won\u00b7ne", "der", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}