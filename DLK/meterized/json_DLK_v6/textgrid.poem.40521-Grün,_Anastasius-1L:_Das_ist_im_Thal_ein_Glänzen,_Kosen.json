{"textgrid.poem.40521": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das ist im Thal ein Gl\u00e4nzen, Kosen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist im Thal ein Gl\u00e4nzen, Kosen", "tokens": ["Das", "ist", "im", "Thal", "ein", "Gl\u00e4n\u00b7zen", ",", "Ko\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "ART", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Blumen, B\u00e4umen, Sonnenlicht,", "tokens": ["Von", "Blu\u00b7men", ",", "B\u00e4u\u00b7men", ",", "Son\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch die sich, wie lebend'ge Rosen,", "tokens": ["Durch", "die", "sich", ",", "wie", "le\u00b7ben\u00b7d'\u00b7ge", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ein Kranz von bl\u00fch'nden Menschen flicht!", "tokens": ["Ein", "Kranz", "von", "bl\u00fch'n\u00b7den", "Men\u00b7schen", "flicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit kaltem strengen Angesichte", "tokens": ["Mit", "kal\u00b7tem", "stren\u00b7gen", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Blickt nur das Alpenhaupt darein;", "tokens": ["Blickt", "nur", "das", "Al\u00b7pen\u00b7haupt", "da\u00b7rein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist' s denn nicht auch ber\u00fchrt vom Lichte?", "tokens": ["Ist'", "s", "denn", "nicht", "auch", "be\u00b7r\u00fchrt", "vom", "Lich\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "VVPP", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Was mag sein d\u00fcstres Sinnen sein?", "tokens": ["Was", "mag", "sein", "d\u00fcst\u00b7res", "Sin\u00b7nen", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nacht ist's geworden allzuschnelle", "tokens": ["Nacht", "ist's", "ge\u00b7wor\u00b7den", "all\u00b7zu\u00b7schnel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "VAPP", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Dunkel h\u00fcllt des Thales Hag;", "tokens": ["Und", "Dun\u00b7kel", "h\u00fcllt", "des", "Tha\u00b7les", "Hag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht ahnt, wer's sah so froh und helle,", "tokens": ["Nicht", "ahnt", ",", "wer's", "sah", "so", "froh", "und", "hel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "PWS", "VVFIN", "ADV", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df es so finster, stumm sein mag!", "tokens": ["Da\u00df", "es", "so", "fins\u00b7ter", ",", "stumm", "sein", "mag", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$,", "ADJD", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf allen Wesen, graunbeklommen,", "tokens": ["Auf", "al\u00b7len", "We\u00b7sen", ",", "graun\u00b7be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Finsterni\u00df Vernichtung ruht!", "tokens": ["Der", "Fins\u00b7ter\u00b7ni\u00df", "Ver\u00b7nich\u00b7tung", "ruht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Einst, als die erste Nacht gekommen,", "tokens": ["Einst", ",", "als", "die", "ers\u00b7te", "Nacht", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie war es, Mensch, dir da zu Muth?", "tokens": ["Wie", "war", "es", ",", "Mensch", ",", "dir", "da", "zu", "Muth", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "NN", "$,", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Den B\u00e4umen bangt und graut im D\u00fcstern,", "tokens": ["Den", "B\u00e4u\u00b7men", "bangt", "und", "graut", "im", "D\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Zweige tasten scheu im Kreis;", "tokens": ["Die", "Zwei\u00b7ge", "tas\u00b7ten", "scheu", "im", "Kreis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Dasein noch sich zuzufl\u00fcstern", "tokens": ["Ihr", "Da\u00b7sein", "noch", "sich", "zu\u00b7zu\u00b7fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "PRF", "VVIZU"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beginnt's im Laub zu rauschen leis.", "tokens": ["Be\u00b7ginnt's", "im", "Laub", "zu", "rau\u00b7schen", "leis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PTKZU", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Rose Gluth kann jetzt nicht hellen!", "tokens": ["Der", "Ro\u00b7se", "Gluth", "kann", "jetzt", "nicht", "hel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sie der Mensch zertrete nicht,", "tokens": ["Da\u00df", "sie", "der", "Mensch", "zer\u00b7tre\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "L\u00e4\u00dft sie ihr Duften bange quellen,", "tokens": ["L\u00e4\u00dft", "sie", "ihr", "Duf\u00b7ten", "ban\u00b7ge", "quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Duft wird H\u00fclfeschrei und Licht!", "tokens": ["Ihr", "Duft", "wird", "H\u00fcl\u00b7fe\u00b7schrei", "und", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Lichterglanz, der wie mit Sehnen", "tokens": ["Der", "Lich\u00b7ter\u00b7glanz", ",", "der", "wie", "mit", "Seh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Thal aus Fensteraugen bricht,", "tokens": ["Im", "Thal", "aus", "Fens\u00b7ter\u00b7au\u00b7gen", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er quillt wie flammenhelle Thr\u00e4nen", "tokens": ["Er", "quillt", "wie", "flam\u00b7me\u00b7nhel\u00b7le", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um ein verlornes, gr\u00f6\u00dfres Licht.", "tokens": ["Um", "ein", "ver\u00b7lor\u00b7nes", ",", "gr\u00f6\u00df\u00b7res", "Licht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch sieh vom Flammenkranz umschlungen", "tokens": ["Doch", "sieh", "vom", "Flam\u00b7men\u00b7kranz", "um\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Haupt der Alpe, gluthumrollt,", "tokens": ["Das", "Haupt", "der", "Al\u00b7pe", ",", "glut\u00b7hum\u00b7rollt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ob zu sparen ihr gelungen", "tokens": ["Als", "ob", "zu", "spa\u00b7ren", "ihr", "ge\u00b7lun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PTKZU", "VVINF", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Theil von ihrem Tagesgold!", "tokens": ["Ein", "Theil", "von", "ih\u00b7rem", "Ta\u00b7ges\u00b7gold", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Als ob tag\u00fcber sie gefangen", "tokens": ["Als", "ob", "ta\u00b7g\u00fc\u00b7ber", "sie", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Kranz die Rosen all' im Thal;", "tokens": ["Im", "Kranz", "die", "Ro\u00b7sen", "all'", "im", "Thal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PIS", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ob bei Tag dir von den Wangen,", "tokens": ["Als", "ob", "bei", "Tag", "dir", "von", "den", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPR", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du Volk des Thals, das Roth sie stahl!", "tokens": ["Du", "Volk", "des", "Thals", ",", "das", "Roth", "sie", "stahl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wenn um der Witwe Leib sich senken", "tokens": ["Wenn", "um", "der", "Wit\u00b7we", "Leib", "sich", "sen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die schwarzen Trauerh\u00fcllen dicht,", "tokens": ["Die", "schwar\u00b7zen", "Trau\u00b7er\u00b7h\u00fcl\u00b7len", "dicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gl\u00fcht oft ein s\u00fc\u00dfes R\u00fcckgedenken", "tokens": ["Gl\u00fcht", "oft", "ein", "s\u00fc\u00b7\u00dfes", "R\u00fcck\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch fort auf ihrem Angesicht.", "tokens": ["Noch", "fort", "auf", "ih\u00b7rem", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Du aber, heitres Herz im Thale,", "tokens": ["Du", "a\u00b7ber", ",", "heit\u00b7res", "Herz", "im", "Tha\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun deine hellen Tage bl\u00fchn,", "tokens": ["Nun", "dei\u00b7ne", "hel\u00b7len", "Ta\u00b7ge", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bewahre sorgsam ihre Strahle,", "tokens": ["Be\u00b7wah\u00b7re", "sorg\u00b7sam", "ih\u00b7re", "Strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In deinen N\u00e4chten nachzugl\u00fchn.", "tokens": ["In", "dei\u00b7nen", "N\u00e4ch\u00b7ten", "nach\u00b7zu\u00b7gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}