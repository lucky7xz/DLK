{"textgrid.poem.35680": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "4. Die beiden Schwestern bei der Rose", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df sie stehn,", "tokens": ["La\u00df", "sie", "stehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Schwesterchen,", "tokens": ["Schwes\u00b7ter\u00b7chen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Diese junge Rose!", "tokens": ["Die\u00b7se", "jun\u00b7ge", "Ro\u00b7se", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Siehst du nicht,", "tokens": ["Siehst", "du", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Da\u00df sie sticht?", "tokens": ["Da\u00df", "sie", "sticht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "La\u00df sie, kleine Lose!", "tokens": ["La\u00df", "sie", ",", "klei\u00b7ne", "Lo\u00b7se", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Unbegl\u00fcckt", "tokens": ["Un\u00b7be\u00b7gl\u00fcckt"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Wer sie pfl\u00fcckt", "tokens": ["Wer", "sie", "pfl\u00fcckt"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Vom bedornten Stamme!", "tokens": ["Vom", "be\u00b7dorn\u00b7ten", "Stam\u00b7me", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Tief ins Herz", "tokens": ["Tief", "ins", "Herz"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPRART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Dringt der Schmerz", "tokens": ["Dringt", "der", "Schmerz"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Von Cytherens Flamme.", "tokens": ["Von", "Cy\u00b7the\u00b7rens", "Flam\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Als sie mir", "tokens": ["Als", "sie", "mir"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Damon hier", "tokens": ["Da\u00b7mon", "hier"], "token_info": ["word", "word"], "pos": ["PAV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Vor die Brust gestecket;", "tokens": ["Vor", "die", "Brust", "ge\u00b7ste\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "M\u00e4dchen, ah!", "tokens": ["M\u00e4d\u00b7chen", ",", "ah", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Was ward da", "tokens": ["Was", "ward", "da"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV"], "meter": "+--", "measure": "dactylic.init"}, "line.6": {"text": "Schnell in mir erwecket!", "tokens": ["Schnell", "in", "mir", "er\u00b7we\u00b7cket", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Voller Glut", "tokens": ["Vol\u00b7ler", "Glut"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "War mein Blut;", "tokens": ["War", "mein", "Blut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Zitternd alle Glieder!", "tokens": ["Zit\u00b7ternd", "al\u00b7le", "Glie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Nimmermehr", "tokens": ["Nim\u00b7mer\u00b7mehr"], "token_info": ["word"], "pos": ["ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Findet er", "tokens": ["Fin\u00b7det", "er"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+--", "measure": "dactylic.init"}, "line.6": {"text": "Mich so f\u00fchlend wieder.", "tokens": ["Mich", "so", "f\u00fch\u00b7lend", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Wei\u00dft du nicht", "tokens": ["Wei\u00dft", "du", "nicht"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Das Ger\u00fccht,", "tokens": ["Das", "Ge\u00b7r\u00fccht", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wie die Ros' entsprossen?", "tokens": ["Wie", "die", "Ros'", "ent\u00b7spros\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Aus der Qual,", "tokens": ["Aus", "der", "Qual", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "--+", "measure": "anapaest.init"}, "line.5": {"text": "Die einmal", "tokens": ["Die", "ein\u00b7mal"], "token_info": ["word", "word"], "pos": ["ART", "ADV"], "meter": "+--", "measure": "dactylic.init"}, "line.6": {"text": "Eos' Aug' entflossen.", "tokens": ["E\u00b7os'", "Aug'", "ent\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Morgens fr\u00fch", "tokens": ["Mor\u00b7gens", "fr\u00fch"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Eilte sie", "tokens": ["Eil\u00b7te", "sie"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Von dem tr\u00e4gen Gatten;", "tokens": ["Von", "dem", "tr\u00e4\u00b7gen", "Gat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Tr\u00f6pfelte", "tokens": ["Tr\u00f6p\u00b7fel\u00b7te"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Z\u00e4rtliche", "tokens": ["Z\u00e4rt\u00b7li\u00b7che"], "token_info": ["word"], "pos": ["NN"], "meter": "+--", "measure": "dactylic.init"}, "line.6": {"text": "Thr\u00e4nen auf die Matten.", "tokens": ["Thr\u00e4\u00b7nen", "auf", "die", "Mat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Wonniglich", "tokens": ["Won\u00b7nig\u00b7lich"], "token_info": ["word"], "pos": ["NE"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Zeigte sich", "tokens": ["Zeig\u00b7te", "sich"], "token_info": ["word", "word"], "pos": ["VVFIN", "PRF"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Da die Blume Florens;", "tokens": ["Da", "die", "Blu\u00b7me", "Flo\u00b7rens", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NE", "$."], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Purpurrot,", "tokens": ["Pur\u00b7pur\u00b7rot", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Wie das Rot", "tokens": ["Wie", "das", "Rot"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Auf der Wang' Aurorens.", "tokens": ["Auf", "der", "Wang'", "Au\u00b7ro\u00b7rens", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Wer sie bricht,", "tokens": ["Wer", "sie", "bricht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Der kann nicht", "tokens": ["Der", "kann", "nicht"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VMFIN", "PTKNEG"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Amors Pfeil' entfliehen.", "tokens": ["A\u00b7mors", "Pfeil'", "ent\u00b7flie\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Drum hat ihr,", "tokens": ["Drum", "hat", "ihr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "(warnung dir!)", "tokens": ["(", "war\u00b7nung", "dir", "!", ")"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "PPER", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Zeus den Dorn verliehen.", "tokens": ["Zeus", "den", "Dorn", "ver\u00b7lie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}