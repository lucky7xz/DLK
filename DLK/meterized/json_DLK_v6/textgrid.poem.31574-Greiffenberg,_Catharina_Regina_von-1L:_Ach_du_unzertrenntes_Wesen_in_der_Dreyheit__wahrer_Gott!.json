{"textgrid.poem.31574": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach du unzertrenntes Wesen in der Dreyheit / wahrer Gott!", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach du unzertrenntes Wesen in der Dreyheit / wahrer Gott!", "tokens": ["Ach", "du", "un\u00b7zer\u00b7trenn\u00b7tes", "We\u00b7sen", "in", "der", "Drey\u00b7heit", "/", "wah\u00b7rer", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "PPER", "ADJA", "NN", "APPR", "ART", "NN", "$(", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Vatter / der du hast die Welt / und in dieser mich erschaffen:", "tokens": ["Vat\u00b7ter", "/", "der", "du", "hast", "die", "Welt", "/", "und", "in", "die\u00b7ser", "mich", "er\u00b7schaf\u00b7fen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PRELS", "PPER", "VAFIN", "ART", "NN", "$(", "KON", "APPR", "PDAT", "PPER", "VVPP", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "mich Gefallnen auch erl\u00f6st / Sohn / mit Creutz und Leidens Waffen:", "tokens": ["mich", "Ge\u00b7fall\u00b7nen", "auch", "er\u00b7l\u00f6st", "/", "Sohn", "/", "mit", "Creutz", "und", "Lei\u00b7dens", "Waf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "VVPP", "$(", "NN", "$(", "APPR", "NN", "KON", "NN", "NN", "$."], "meter": "+-+----+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "und du Geist mein Herzens-Herrscher / tr\u00f6stest mich in aller Noth.", "tokens": ["und", "du", "Geist", "mein", "Her\u00b7zens\u00b7Herr\u00b7scher", "/", "tr\u00f6s\u00b7test", "mich", "in", "al\u00b7ler", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "PPOSAT", "NN", "$(", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Alle Drey sind eins / zu retten meine Seel vom S\u00fcnden-Tod.", "tokens": ["Al\u00b7le", "Drey", "sind", "eins", "/", "zu", "ret\u00b7ten", "mei\u00b7ne", "Seel", "vom", "S\u00fcn\u00b7den\u00b7Tod", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "CARD", "VAFIN", "PIS", "$(", "PTKZU", "VVINF", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Gott gab seinen liebsten Sohn: Dieser ist im Tod entschlaffen /", "tokens": ["Gott", "gab", "sei\u00b7nen", "liebs\u00b7ten", "Sohn", ":", "Die\u00b7ser", "ist", "im", "Tod", "ent\u00b7schlaf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$.", "PDS", "VAFIN", "APPRART", "NN", "VVINF", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "aller S\u00fcnden Qu\u00e4l-Stich Ihn / mir Ruh zu verschaffen traffen:", "tokens": ["al\u00b7ler", "S\u00fcn\u00b7den", "Qu\u00e4l\u00b7Stich", "Ihn", "/", "mir", "Ruh", "zu", "ver\u00b7schaf\u00b7fen", "traf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "PPER", "$(", "PPER", "NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "+-+--+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "und mein Tr\u00f6ster / tunkt im Glauben mich in Christi Wunden-roht.", "tokens": ["und", "mein", "Tr\u00f6s\u00b7ter", "/", "tunkt", "im", "Glau\u00b7ben", "mich", "in", "Chris\u00b7ti", "Wun\u00b7den\u00b7roht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "VVFIN", "APPRART", "NN", "PRF", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Vnterschiedlich in den Werken / gleich im Willen / Gnad' und G\u00fct /", "tokens": ["Vn\u00b7ter\u00b7schied\u00b7lich", "in", "den", "Wer\u00b7ken", "/", "gleich", "im", "Wil\u00b7len", "/", "Gnad'", "und", "G\u00fct", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$(", "ADV", "APPRART", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "kan man di\u00df gedreyte Eins und geeinte Drey erkennen /", "tokens": ["kan", "man", "di\u00df", "ge\u00b7drey\u00b7te", "Eins", "und", "ge\u00b7ein\u00b7te", "Drey", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PDS", "ADJA", "NN", "KON", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "aus der abgetheilten Ordnung. Ach begreif' es mein Gem\u00fct.", "tokens": ["aus", "der", "ab\u00b7ge\u00b7theil\u00b7ten", "Ord\u00b7nung", ".", "Ach", "be\u00b7greif'", "es", "mein", "Ge\u00b7m\u00fct", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "ITJ", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "jedem sind die Werk gemein / und auf keine Wei\u00df zu kennen:", "tokens": ["je\u00b7dem", "sind", "die", "Werk", "ge\u00b7mein", "/", "und", "auf", "kei\u00b7ne", "Wei\u00df", "zu", "ken\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "ADJD", "$(", "KON", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "doch Geb\u00e4hrungs Eigenschafft nur dem Vatter zu zuschreiben;", "tokens": ["doch", "Ge\u00b7b\u00e4h\u00b7rungs", "Ei\u00b7gen\u00b7schafft", "nur", "dem", "Vat\u00b7ter", "zu", "zu\u00b7schrei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "da\u00df er aus von beeden geht / nur dem Geist; dem Sohn / beleiben.", "tokens": ["da\u00df", "er", "aus", "von", "bee\u00b7den", "geht", "/", "nur", "dem", "Geist", ";", "dem", "Sohn", "/", "be\u00b7lei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "APPR", "NE", "VVFIN", "$(", "ADV", "ART", "NN", "$.", "ART", "NN", "$(", "VVPP", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}