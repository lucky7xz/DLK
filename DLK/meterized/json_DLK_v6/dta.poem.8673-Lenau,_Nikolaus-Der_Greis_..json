{"dta.poem.8673": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der Greis .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Durch Bl\u00fcthen winket der Abendstern,", "tokens": ["Durch", "Bl\u00fc\u00b7then", "win\u00b7ket", "der", "A\u00b7bends\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein L\u00fcftchen spielt im Gezweige;", "tokens": ["Ein", "L\u00fcft\u00b7chen", "spielt", "im", "Ge\u00b7zwei\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Greis genie\u00dft im Garten so gern", "tokens": ["Der", "Greis", "ge\u00b7nie\u00dft", "im", "Gar\u00b7ten", "so", "gern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ADV", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Des Tages s\u00fc\u00dfe Neige.", "tokens": ["Des", "Ta\u00b7ges", "s\u00fc\u00b7\u00dfe", "Nei\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dort seine Enkel, sie jagen frisch", "tokens": ["Dort", "sei\u00b7ne", "En\u00b7kel", ",", "sie", "ja\u00b7gen", "frisch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Grase hin und wieder;", "tokens": ["Im", "Gra\u00b7se", "hin", "und", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die V\u00f6glein singen im Geb\u00fcsch", "tokens": ["Die", "V\u00f6\u00b7glein", "sin\u00b7gen", "im", "Ge\u00b7b\u00fcsch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun ihre Schlummerlieder.", "tokens": ["Nun", "ih\u00b7re", "Schlum\u00b7mer\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es lieben Kinder und V\u00f6gelein,", "tokens": ["Es", "lie\u00b7ben", "Kin\u00b7der", "und", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u2014 Die Gl\u00fccklichsten auf Erden! \u2014", "tokens": ["Die", "Gl\u00fcck\u00b7lichs\u00b7ten", "auf", "Er\u00b7den", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "APPR", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bevor sie Abends schlafen ein,", "tokens": ["Be\u00b7vor", "sie", "A\u00b7bends", "schla\u00b7fen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch einmal laut zu werden.", "tokens": ["Noch", "ein\u00b7mal", "laut", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da schl\u00e4ngelt der schnelle Kinderkreis", "tokens": ["Da", "schl\u00e4n\u00b7gelt", "der", "schnel\u00b7le", "Kin\u00b7der\u00b7kreis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sich bl\u00fchend durch bl\u00fchende B\u00e4ume,", "tokens": ["Sich", "bl\u00fc\u00b7hend", "durch", "bl\u00fc\u00b7hen\u00b7de", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie gaukeln um den stillen Greis", "tokens": ["Sie", "gau\u00b7keln", "um", "den", "stil\u00b7len", "Greis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie selige Jugendtr\u00e4ume.", "tokens": ["Wie", "se\u00b7li\u00b7ge", "Ju\u00b7gend\u00b7tr\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sein Auge folgt am Wiesenplan", "tokens": ["Sein", "Au\u00b7ge", "folgt", "am", "Wie\u00b7sen\u00b7plan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Unschuld fr\u00f6hlichen Streichen;", "tokens": ["Der", "Un\u00b7schuld", "fr\u00f6h\u00b7li\u00b7chen", "Strei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da jauchzt ein Knabe zu ihm heran,", "tokens": ["Da", "jauchzt", "ein", "Kna\u00b7be", "zu", "ihm", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihm eine Blume zu reichen.", "tokens": ["Ihm", "ei\u00b7ne", "Blu\u00b7me", "zu", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Der Alte nimmt sie l\u00e4chelnd hin,", "tokens": ["Der", "Al\u00b7te", "nimmt", "sie", "l\u00e4\u00b7chelnd", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und streichelt den sch\u00f6nen Jungen,", "tokens": ["Und", "strei\u00b7chelt", "den", "sch\u00f6\u00b7nen", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und will liebkosend ihn n\u00e4her ziehn;", "tokens": ["Und", "will", "lieb\u00b7ko\u00b7send", "ihn", "n\u00e4\u00b7her", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der aber ist wieder entsprungen.", "tokens": ["Der", "a\u00b7ber", "ist", "wie\u00b7der", "ent\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Und wie der Greis nun die Blume h\u00e4lt,", "tokens": ["Und", "wie", "der", "Greis", "nun", "die", "Blu\u00b7me", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und ansieht immer genauer,", "tokens": ["Und", "an\u00b7sieht", "im\u00b7mer", "ge\u00b7nau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein ernstes Sinnen ihn \u00fcberf\u00e4llt,", "tokens": ["Ein", "erns\u00b7tes", "Sin\u00b7nen", "ihn", "\u00fc\u00b7berf\u00b7\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Halb Freud', und milde Trauer.", "tokens": ["Halb", "Freud'", ",", "und", "mil\u00b7de", "Trau\u00b7er", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Er h\u00e4lt die Blume so inniglich,", "tokens": ["Er", "h\u00e4lt", "die", "Blu\u00b7me", "so", "in\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die ihm das Kind erkoren,", "tokens": ["Die", "ihm", "das", "Kind", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als h\u00e4tte seine Seele sich", "tokens": ["Als", "h\u00e4t\u00b7te", "sei\u00b7ne", "See\u00b7le", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPOSAT", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ganz in die Blume verloren.", "tokens": ["Ganz", "in", "die", "Blu\u00b7me", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.9": {"line.1": {"text": "Als f\u00fchlt' er sich gar nah verwandt", "tokens": ["Als", "f\u00fchlt'", "er", "sich", "gar", "nah", "ver\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Blume, erdentsprossen,", "tokens": ["Der", "Blu\u00b7me", ",", "er\u00b7dents\u00b7pros\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als h\u00e4tte die Blum' ihn leise genannt", "tokens": ["Als", "h\u00e4t\u00b7te", "die", "Blum'", "ihn", "lei\u00b7se", "ge\u00b7nannt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "ART", "NN", "PPER", "ADJD", "VVPP"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihren lieben, trauten Genossen.", "tokens": ["Ih\u00b7ren", "lie\u00b7ben", ",", "trau\u00b7ten", "Ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Schon sp\u00fcrt er im Innern keimen wohl", "tokens": ["Schon", "sp\u00fcrt", "er", "im", "In\u00b7nern", "kei\u00b7men", "wohl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "VVFIN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das stille Pflanzenleben,", "tokens": ["Das", "stil\u00b7le", "Pflan\u00b7zen\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das bald aus seinem H\u00fcgel soll", "tokens": ["Das", "bald", "aus", "sei\u00b7nem", "H\u00fc\u00b7gel", "soll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "PPOSAT", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Blumen sich erheben.", "tokens": ["In", "Blu\u00b7men", "sich", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}