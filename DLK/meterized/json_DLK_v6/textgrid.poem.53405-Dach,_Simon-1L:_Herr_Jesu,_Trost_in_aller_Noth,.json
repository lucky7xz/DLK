{"textgrid.poem.53405": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr Jesu, Trost in aller Noth,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Jesu, Trost in aller Noth,", "tokens": ["Herr", "Je\u00b7su", ",", "Trost", "in", "al\u00b7ler", "Noth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du L\u00e4nge meiner Tage,", "tokens": ["Du", "L\u00e4n\u00b7ge", "mei\u00b7ner", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich weis da\u00df ich alzeit den Tod", "tokens": ["Ich", "weis", "da\u00df", "ich", "al\u00b7zeit", "den", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "KOUS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An meinem Halse trage,", "tokens": ["An", "mei\u00b7nem", "Hal\u00b7se", "tra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn du gebeutst so scheid ich ab,", "tokens": ["Wenn", "du", "ge\u00b7beutst", "so", "scheid", "ich", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd nehme mit mir in das Grab", "tokens": ["Vnd", "neh\u00b7me", "mit", "mir", "in", "das", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zeit, Welt und alle Plage.", "tokens": ["Zeit", ",", "Welt", "und", "al\u00b7le", "Pla\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die lieben Meinen, ihr Geschrey,", "tokens": ["Die", "lie\u00b7ben", "Mei\u00b7nen", ",", "ihr", "Ge\u00b7schrey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dieweil sie mich verlohren,", "tokens": ["Die\u00b7weil", "sie", "mich", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "K\u00f6mmt meinem Hertzen nimmer bey,", "tokens": ["K\u00f6mmt", "mei\u00b7nem", "Hert\u00b7zen", "nim\u00b7mer", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weg sind Gesicht und Ohren:", "tokens": ["Weg", "sind", "Ge\u00b7sicht", "und", "Oh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Ich bawe dort das finstre Reich", "tokens": ["Ich", "ba\u00b7we", "dort", "das", "finst\u00b7re", "Reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der stillen Schatten und bin gleich", "tokens": ["Der", "stil\u00b7len", "Schat\u00b7ten", "und", "bin", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als w\u00e4r' ich nie gebohren.", "tokens": ["Als", "w\u00e4r'", "ich", "nie", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dir aber bleib' ich unentwand,", "tokens": ["Dir", "a\u00b7ber", "bleib'", "ich", "un\u00b7ent\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird gleich der Leib begraben,", "tokens": ["Wird", "gleich", "der", "Leib", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du wirst auch dann in deine Hand", "tokens": ["Du", "wirst", "auch", "dann", "in", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich angeschrieben haben,", "tokens": ["Mich", "an\u00b7ge\u00b7schrie\u00b7ben", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und meine Seel erhebest Du,", "tokens": ["Und", "mei\u00b7ne", "Seel", "er\u00b7he\u00b7best", "Du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die wird im Himmel sich mit Rhu", "tokens": ["Die", "wird", "im", "Him\u00b7mel", "sich", "mit", "Rhu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "PRF", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ewign Frewden laben.", "tokens": ["Und", "e\u00b7wign", "Frew\u00b7den", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Vnd wirst du denn ein Feldgeschrey", "tokens": ["Vnd", "wirst", "du", "denn", "ein", "Feld\u00b7ge\u00b7schrey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hoch in die Wolcken machen,", "tokens": ["Hoch", "in", "die", "Wol\u00b7cken", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "(das Ende n\u00e4hert sich herbey)", "tokens": ["(", "das", "En\u00b7de", "n\u00e4\u00b7hert", "sich", "her\u00b7bey", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PRF", "PTKVZ", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Der Erden Baw wird krachen,", "tokens": ["Der", "Er\u00b7den", "Baw", "wird", "kra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Im Fewer alles untergehn,", "tokens": ["Im", "Fe\u00b7wer", "al\u00b7les", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Todten werden aufferstehn,", "tokens": ["Die", "Tod\u00b7ten", "wer\u00b7den", "auf\u00b7fer\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So werd auch ich erwachen.", "tokens": ["So", "werd", "auch", "ich", "er\u00b7wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nichts bleibt im Grabe nicht ein, Haar", "tokens": ["Nichts", "bleibt", "im", "Gra\u00b7be", "nicht", "ein", ",", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "PTKNEG", "PTKVZ", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vermag der Tod zu f\u00e4llen,", "tokens": ["Ver\u00b7mag", "der", "Tod", "zu", "f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du nimst auch meiner Aschen war,", "tokens": ["Du", "nimst", "auch", "mei\u00b7ner", "A\u00b7schen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Nagel wird mir fehlen,", "tokens": ["Kein", "Na\u00b7gel", "wird", "mir", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NE", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann wird die Seele hoch erfrewt,", "tokens": ["Dann", "wird", "die", "See\u00b7le", "hoch", "er\u00b7frewt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit ihrem Leib in Ewigheit", "tokens": ["Mit", "ih\u00b7rem", "Leib", "in", "E\u00b7wig\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sich wiederumb verm\u00e4hlen.", "tokens": ["Sich", "wie\u00b7de\u00b7rumb", "ver\u00b7m\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O Prei\u00df, Triumph, o Ehr' und Danck,", "tokens": ["O", "Prei\u00df", ",", "Tri\u00b7umph", ",", "o", "Ehr'", "und", "Danck", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "FM", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den ich dir werde bringen,", "tokens": ["Den", "ich", "dir", "wer\u00b7de", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Engel hoher Lobgesangk", "tokens": ["Der", "En\u00b7gel", "ho\u00b7her", "Lob\u00b7ge\u00b7sangk"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird in den meinen klingen.", "tokens": ["Wird", "in", "den", "mei\u00b7nen", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Herr la\u00df mich hie durch Lieb und Leid", "tokens": ["Herr", "la\u00df", "mich", "hie", "durch", "Lieb", "und", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVIMP", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach solcher grossen Seeligkeit", "tokens": ["Nach", "sol\u00b7cher", "gros\u00b7sen", "See\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit h\u00f6chsten Kr\u00e4fften ringen.", "tokens": ["Mit", "h\u00f6chs\u00b7ten", "Kr\u00e4ff\u00b7ten", "rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "La\u00df meinen Geist den eiteln Wahn", "tokens": ["La\u00df", "mei\u00b7nen", "Geist", "den", "ei\u00b7teln", "Wahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Erden nicht verderben,", "tokens": ["Der", "Er\u00b7den", "nicht", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Lehr mich der S\u00fcnden abgethan", "tokens": ["Lehr", "mich", "der", "S\u00fcn\u00b7den", "ab\u00b7ge\u00b7than"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Busse t\u00e4glich sterben,", "tokens": ["Durch", "Bus\u00b7se", "t\u00e4g\u00b7lich", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und k\u00f6mpt es an die letzte Noht,", "tokens": ["Und", "k\u00f6mpt", "es", "an", "die", "letz\u00b7te", "Noht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00df mich durch einen selign Tod", "tokens": ["La\u00df", "mich", "durch", "ei\u00b7nen", "se\u00b7lign", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das ewig' Heil ererben.", "tokens": ["Das", "e\u00b7wig'", "Heil", "er\u00b7er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}