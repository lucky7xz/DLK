{"textgrid.poem.32920": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hab Dank, du mein liebes Rad du!", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hab Dank, du mein liebes Rad du!", "tokens": ["Hab", "Dank", ",", "du", "mein", "lie\u00b7bes", "Rad", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "PPOSAT", "ADJA", "NN", "PPER", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Du hast mir geholfen, mich hinaus zu finden wieder aus der Enge der H\u00e4user und der Stra\u00dfen ...", "tokens": ["Du", "hast", "mir", "ge\u00b7hol\u00b7fen", ",", "mich", "hin\u00b7aus", "zu", "fin\u00b7den", "wie\u00b7der", "aus", "der", "En\u00b7ge", "der", "H\u00e4u\u00b7ser", "und", "der", "Stra\u00b7\u00dfen", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "PRF", "APZR", "PTKZU", "VVINF", "ADV", "APPR", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+--+-+-+-+-+-+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "du hast mir geholfen, von all der Frack- und Zylinder-Feierlichkeit los zu kommen ...", "tokens": ["du", "hast", "mir", "ge\u00b7hol\u00b7fen", ",", "von", "all", "der", "Frack", "und", "Zy\u00b7lin\u00b7der\u00b7Fei\u00b7er\u00b7lich\u00b7keit", "los", "zu", "kom\u00b7men", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "APPR", "PIAT", "ART", "TRUNC", "KON", "NN", "PTKVZ", "PTKZU", "VVINF", "$("], "meter": "-+--+--+-+--+-+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "du hast mir geholfen, mich an mir selbst zu freuen und wieder froh zu werden!", "tokens": ["du", "hast", "mir", "ge\u00b7hol\u00b7fen", ",", "mich", "an", "mir", "selbst", "zu", "freu\u00b7en", "und", "wie\u00b7der", "froh", "zu", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "PRF", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "KON", "ADV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+--+--+-+-+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Und nicht blo\u00df mir!", "tokens": ["Und", "nicht", "blo\u00df", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "der ganzen Zeit!", "tokens": ["der", "gan\u00b7zen", "Zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "hab Dank!", "tokens": ["hab", "Dank", "!"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Du hast ihr eine neue Freude gegeben ...", "tokens": ["Du", "hast", "ihr", "ei\u00b7ne", "neu\u00b7e", "Freu\u00b7de", "ge\u00b7ge\u00b7ben", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "und ein St\u00fcckchen Freiheit wieder, zu sich und zur Natur zur\u00fcck ...", "tokens": ["und", "ein", "St\u00fcck\u00b7chen", "Frei\u00b7heit", "wie\u00b7der", ",", "zu", "sich", "und", "zur", "Na\u00b7tur", "zu\u00b7r\u00fcck", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "ADV", "$,", "APPR", "PRF", "KON", "APPRART", "NN", "PTKVZ", "$("], "meter": "--+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "und hast ihr damit ein St\u00fcckchen Jugend wiedergebracht und Frohmut und Harmlosigkeit ...", "tokens": ["und", "hast", "ihr", "da\u00b7mit", "ein", "St\u00fcck\u00b7chen", "Ju\u00b7gend", "wie\u00b7der\u00b7ge\u00b7bracht", "und", "Froh\u00b7mut", "und", "Harm\u00b7lo\u00b7sig\u00b7keit", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PAV", "ART", "NN", "NN", "VVFIN", "KON", "NN", "KON", "NN", "$("], "meter": "-+--+-+-+-+--+-+--+--+", "measure": "iambic.octa.plus.relaxed"}, "line.7": {"text": "hab Dank!", "tokens": ["hab", "Dank", "!"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "NN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}