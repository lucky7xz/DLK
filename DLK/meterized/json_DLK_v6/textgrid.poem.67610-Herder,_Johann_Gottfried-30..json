{"textgrid.poem.67610": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Berg, der kreisete;", "tokens": ["Ein", "Berg", ",", "der", "krei\u00b7se\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Erde bebete,", "tokens": ["Die", "Er\u00b7de", "be\u00b7be\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Welt erwartete.", "tokens": ["Die", "Welt", "er\u00b7war\u00b7te\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was kam heraus?", "tokens": ["Was", "kam", "he\u00b7raus", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ihr wisset's: eine Maus!", "tokens": ["Ihr", "wis\u00b7set's", ":", "ei\u00b7ne", "Maus", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn D\u00e4rme voller Bl\u00e4hung sind,", "tokens": ["Wenn", "D\u00e4r\u00b7me", "vol\u00b7ler", "Bl\u00e4\u00b7hung", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was wird? Ihr sprecht: Prophetenwind.", "tokens": ["Was", "wird", "?", "Ihr", "sprecht", ":", "Pro\u00b7phe\u00b7ten\u00b7wind", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "$.", "PPER", "VVFIN", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch immer was! Ihr aber, Kopf,", "tokens": ["Doch", "im\u00b7mer", "was", "!", "Ihr", "a\u00b7ber", ",", "Kopf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "PWS", "$.", "PPER", "ADV", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der wie ein Berg sich regt", "tokens": ["Der", "wie", "ein", "Berg", "sich", "regt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "KOKOM", "ART", "NN", "PRF", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wie ein Bauch ergrimmt und r\u00fchrt", "tokens": ["Und", "wie", "ein", "Bauch", "er\u00b7grimmt", "und", "r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und r\u00fcttelt Euren Topf", "tokens": ["Und", "r\u00fct\u00b7telt", "Eu\u00b7ren", "Topf"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Was kommt heraus? Sieh, armer Tropf,", "tokens": ["Was", "kommt", "he\u00b7raus", "?", "Sieh", ",", "ar\u00b7mer", "Tropf", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKVZ", "$.", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Q.E.D., ", "tokens": ["Ein", "Q.", "E.", "D.", ","], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation", "punct"], "pos": ["ART", "NN", "NE", "NE", "$,"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Wie sich's geb\u00fchrt,", "tokens": ["Wie", "sich's", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Wohl demonstrirt!", "tokens": ["Wohl", "de\u00b7mons\u00b7trirt", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}