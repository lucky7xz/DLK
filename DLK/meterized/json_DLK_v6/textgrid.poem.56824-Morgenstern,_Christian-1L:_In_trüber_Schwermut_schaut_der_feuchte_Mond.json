{"textgrid.poem.56824": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: In tr\u00fcber Schwermut schaut der feuchte Mond", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In tr\u00fcber Schwermut schaut der feuchte Mond", "tokens": ["In", "tr\u00fc\u00b7ber", "Schwer\u00b7mut", "schaut", "der", "feuch\u00b7te", "Mond"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "wie ein verweintes Auge durch die Nacht ...", "tokens": ["wie", "ein", "ver\u00b7wein\u00b7tes", "Au\u00b7ge", "durch", "die", "Nacht", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Umrauscht vom eignen Odem schl\u00e4ft der See,", "tokens": ["Um\u00b7rauscht", "vom", "eig\u00b7nen", "O\u00b7dem", "schl\u00e4ft", "der", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "breitausgebettet bis zum fernsten Wald ...", "tokens": ["brei\u00b7taus\u00b7ge\u00b7bet\u00b7tet", "bis", "zum", "ferns\u00b7ten", "Wald", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPRART", "ADJA", "NN", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Oft f\u00e4hrt's in Busch und R\u00f6hricht schaudernd auf,", "tokens": ["Oft", "f\u00e4hrt's", "in", "Busch", "und", "R\u00f6h\u00b7richt", "schau\u00b7dernd", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "wie wenn im Halbschlaf sich ein Seufzer l\u00f6st ...", "tokens": ["wie", "wenn", "im", "Halb\u00b7schlaf", "sich", "ein", "Seuf\u00b7zer", "l\u00f6st", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "APPRART", "NN", "PRF", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann wieder Stille, als ob selber Gott", "tokens": ["Dann", "wie\u00b7der", "Stil\u00b7le", ",", "als", "ob", "sel\u00b7ber", "Gott"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "NN", "$,", "KOKOM", "KOUS", "ADV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "als Alp auf seiner Erde lastete ...", "tokens": ["als", "Alp", "auf", "sei\u00b7ner", "Er\u00b7de", "las\u00b7te\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}