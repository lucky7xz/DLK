{"textgrid.poem.26318": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Dezembernacht", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Dezembernacht geht warm ins Land,", "tokens": ["Die", "De\u00b7zem\u00b7ber\u00b7nacht", "geht", "warm", "ins", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wetterleuchten flackt in stummer Ferne.", "tokens": ["Wet\u00b7ter\u00b7leuch\u00b7ten", "flackt", "in", "stum\u00b7mer", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und die dunkelglatte Himmelswand,", "tokens": ["Und", "die", "dun\u00b7kel\u00b7glat\u00b7te", "Him\u00b7mels\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "\u00dcberblinkt von Stichen starker Sterne.", "tokens": ["\u00dc\u00b7berb\u00b7linkt", "von", "Sti\u00b7chen", "star\u00b7ker", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Dort das gelbe L\u00e4mplein leuchtet kaum", "tokens": ["Dort", "das", "gel\u00b7be", "L\u00e4m\u00b7plein", "leuch\u00b7tet", "kaum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Klein am Boden einer armen Klause.", "tokens": ["Klein", "am", "Bo\u00b7den", "ei\u00b7ner", "ar\u00b7men", "Klau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Offen steht die T\u00fcr in Nacht und Raum.", "tokens": ["Of\u00b7fen", "steht", "die", "T\u00fcr", "in", "Nacht", "und", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Einer betet halblaut in dem Hause.", "tokens": ["Ei\u00b7ner", "be\u00b7tet", "hal\u00b7blaut", "in", "dem", "Hau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Manchesmal ein Menschenschatten liegt", "tokens": ["Man\u00b7ches\u00b7mal", "ein", "Men\u00b7schen\u00b7schat\u00b7ten", "liegt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Vor mir lang im grauen Sand der Stra\u00dfe.", "tokens": ["Vor", "mir", "lang", "im", "grau\u00b7en", "Sand", "der", "Stra\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Manchmal f\u00e4llt mich an ein Duft und fliegt", "tokens": ["Manch\u00b7mal", "f\u00e4llt", "mich", "an", "ein", "Duft", "und", "fliegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "KON", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Aus der B\u00e4ume hoher Kronenmasse.", "tokens": ["Aus", "der", "B\u00e4u\u00b7me", "ho\u00b7her", "Kro\u00b7nen\u00b7mas\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Und ich ahne, dort im Dunkel lebt", "tokens": ["Und", "ich", "ah\u00b7ne", ",", "dort", "im", "Dun\u00b7kel", "lebt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "ADV", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Vieles, das verborgen sich geboren,", "tokens": ["Vie\u00b7les", ",", "das", "ver\u00b7bor\u00b7gen", "sich", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "VVPP", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Davon Freude s\u00fc\u00df vor\u00fcberschwebt.", "tokens": ["Da\u00b7von", "Freu\u00b7de", "s\u00fc\u00df", "vor\u00b7\u00fc\u00b7bersc\u00b7hwebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Nacht lacht leis zu meinen Ohren.", "tokens": ["Und", "die", "Nacht", "lacht", "leis", "zu", "mei\u00b7nen", "Oh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}