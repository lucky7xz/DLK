{"textgrid.poem.57581": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "September", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stiefm\u00fctterchen und Rittersporn", "tokens": ["Stief\u00b7m\u00fct\u00b7ter\u00b7chen", "und", "Rit\u00b7ter\u00b7sporn"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bl\u00fchn auf dem Stoppelfeld,", "tokens": ["Bl\u00fchn", "auf", "dem", "Stop\u00b7pel\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Im blumenkahlen Rosendorn", "tokens": ["Im", "blu\u00b7men\u00b7kah\u00b7len", "Ro\u00b7sen\u00b7dorn"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Grasm\u00fccke Rastung h\u00e4lt.", "tokens": ["Gras\u00b7m\u00fc\u00b7cke", "Ras\u00b7tung", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Noch gestern nacht lag Frost und Reif", "tokens": ["Noch", "ge\u00b7stern", "nacht", "lag", "Frost", "und", "Reif"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "NN", "KON", "NN"], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Erk\u00e4ltend auf der Flur,", "tokens": ["Er\u00b7k\u00e4l\u00b7tend", "auf", "der", "Flur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und heut durchzieht ein Sonnenstreif", "tokens": ["Und", "heut", "durch\u00b7zieht", "ein", "Son\u00b7nen\u00b7streif"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erw\u00e4rmend die Natur.", "tokens": ["Er\u00b7w\u00e4r\u00b7mend", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zaunk\u00f6nig singt vom alten Heck", "tokens": ["Zaun\u00b7k\u00f6\u00b7nig", "singt", "vom", "al\u00b7ten", "Heck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sein Liedchen fr\u00fchlingsgr\u00fcn,", "tokens": ["Sein", "Lied\u00b7chen", "fr\u00fch\u00b7lings\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch \u00fcber meinem Haupte weg", "tokens": ["Doch", "\u00fc\u00b7ber", "mei\u00b7nem", "Haup\u00b7te", "weg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die St\u00f6rche s\u00fcdw\u00e4rts ziehn.", "tokens": ["Die", "St\u00f6r\u00b7che", "s\u00fcd\u00b7w\u00e4rts", "ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Auf flockigem Gespinst durchfliegt", "tokens": ["Auf", "flo\u00b7cki\u00b7gem", "Ge\u00b7spinst", "durch\u00b7fliegt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Die Spinnenbrut die Luft \u2013", "tokens": ["Die", "Spin\u00b7nen\u00b7brut", "die", "Luft", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und was der Winter fast besiegt,", "tokens": ["Und", "was", "der", "Win\u00b7ter", "fast", "be\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entflieht der kalten Gruft.", "tokens": ["Ent\u00b7flieht", "der", "kal\u00b7ten", "Gruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Auf mancher Wange hoffnungsm\u00fcd", "tokens": ["Auf", "man\u00b7cher", "Wan\u00b7ge", "hoff\u00b7nungs\u00b7m\u00fcd"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erbl\u00fcht ein roter Schein,", "tokens": ["Er\u00b7bl\u00fcht", "ein", "ro\u00b7ter", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und manch vergessenes Liebeslied", "tokens": ["Und", "manch", "ver\u00b7ges\u00b7se\u00b7nes", "Lie\u00b7bes\u00b7lied"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sucht neue Melodein. \u2013 \u2013", "tokens": ["Sucht", "neu\u00b7e", "Me\u00b7lo\u00b7dein", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es dunkelt \u2013 Nordwind kehrt zur\u00fcck,", "tokens": ["Es", "dun\u00b7kelt", "\u2013", "Nord\u00b7wind", "kehrt", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "NE", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz umkrallt das Weh \u2013", "tokens": ["Mein", "Herz", "um\u00b7krallt", "das", "Weh", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Altweibersommer, kurzes Gl\u00fcck!", "tokens": ["Alt\u00b7wei\u00b7ber\u00b7som\u00b7mer", ",", "kur\u00b7zes", "Gl\u00fcck", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und morgen f\u00e4llt der Schnee.", "tokens": ["Und", "mor\u00b7gen", "f\u00e4llt", "der", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}