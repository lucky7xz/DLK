{"dta.poem.13140": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Krei\u00df bezeigte sich den Vortrag zu bejahen;", "tokens": ["Der", "Krei\u00df", "be\u00b7zeig\u00b7te", "sich", "den", "Vor\u00b7trag", "zu", "be\u00b7ja\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann wir die Neigungen, des Beyfalls Zeichen, sahen.", "tokens": ["Dann", "wir", "die", "Nei\u00b7gun\u00b7gen", ",", "des", "Bey\u00b7falls", "Zei\u00b7chen", ",", "sa\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "$,", "ART", "NN", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Wahrheit gieng hervor. Was ungemeiner Glanz", "tokens": ["Die", "Wahr\u00b7heit", "gieng", "her\u00b7vor", ".", "Was", "un\u00b7ge\u00b7mei\u00b7ner", "Glanz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "PWS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Umschimmerte den Schild, den ein Jubelen-Kranz", "tokens": ["Um\u00b7schim\u00b7mer\u00b7te", "den", "Schild", ",", "den", "ein", "Ju\u00b7be\u00b7len\u00b7Kranz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "115Noch mehr verherrlichte; man sah die bunten Strahlen,", "tokens": ["mehr", "ver\u00b7herr\u00b7lich\u00b7te", ";", "man", "sah", "die", "bun\u00b7ten", "Strah\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "PIS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Die leuchtende Person mit solcher Pracht bemahlen,", "tokens": ["Die", "leuch\u00b7ten\u00b7de", "Per\u00b7son", "mit", "sol\u00b7cher", "Pracht", "be\u00b7mah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df meiner Augen Macht sich durch den Schein geblendt,", "tokens": ["Da\u00df", "mei\u00b7ner", "Au\u00b7gen", "Macht", "sich", "durch", "den", "Schein", "ge\u00b7blendt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und ich dieselbige fast nimmermehr erkennt;", "tokens": ["Und", "ich", "die\u00b7sel\u00b7bi\u00b7ge", "fast", "nim\u00b7mer\u00b7mehr", "er\u00b7kennt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PDS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bi\u00df allgemach des Lichts Verschwendung sich gemindert,", "tokens": ["Bi\u00df", "all\u00b7ge\u00b7mach", "des", "Lichts", "Ver\u00b7schwen\u00b7dung", "sich", "ge\u00b7min\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "120Und ich, da das Gepr\u00e4ng des Scheins nichts mehr gehindert,", "tokens": ["ich", ",", "da", "das", "Ge\u00b7pr\u00e4ng", "des", "Scheins", "nichts", "mehr", "ge\u00b7hin\u00b7dert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "ART", "NN", "ART", "NN", "PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.11": {"text": "Die Freundinn angesehn. Mein Aug erg\u00f6zte sich", "tokens": ["Die", "Freun\u00b7dinn", "an\u00b7ge\u00b7sehn", ".", "Mein", "Aug", "er\u00b7g\u00f6z\u00b7te", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$.", "PPOSAT", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "In ihren Kleidungen, sie waren K\u00f6niglich.", "tokens": ["In", "ih\u00b7ren", "Klei\u00b7dun\u00b7gen", ",", "sie", "wa\u00b7ren", "K\u00f6\u00b7nig\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Sie trat in Majest\u00e4t zum Thron, der Grei\u00df von dannen,", "tokens": ["Sie", "trat", "in", "Ma\u00b7jes\u00b7t\u00e4t", "zum", "Thron", ",", "der", "Grei\u00df", "von", "dan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPRART", "NN", "$,", "ART", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der sagte: \u201eFreundinn komm, das Streiten zu verbannen.", "tokens": ["Der", "sag\u00b7te", ":", "\u201e", "Freun\u00b7dinn", "komm", ",", "das", "Strei\u00b7ten", "zu", "ver\u00b7ban\u00b7nen", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "$(", "NN", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "125\u201dNimm nun den Richter-Plaz! sein l\u00e4chelndes Gesicht", "tokens": ["\"", "Nimm", "nun", "den", "Rich\u00b7ter\u00b7Plaz", "!", "sein", "l\u00e4\u00b7cheln\u00b7des", "Ge\u00b7sicht"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "NE", "ADV", "ART", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Empfieng durch ihren Glanz ein gegenschimmernd Licht.", "tokens": ["Emp\u00b7fi\u00b7eng", "durch", "ih\u00b7ren", "Glanz", "ein", "ge\u00b7gen\u00b7schim\u00b7mernd", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "ART", "VVPP", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.17": {"text": "Sie wiesen beyderseits im Aug und in den Mienen,", "tokens": ["Sie", "wie\u00b7sen", "bey\u00b7der\u00b7seits", "im", "Aug", "und", "in", "den", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Es sey ein stiller Trieb der Freundschaft unter ihnen.", "tokens": ["Es", "sey", "ein", "stil\u00b7ler", "Trieb", "der", "Freund\u00b7schaft", "un\u00b7ter", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wir sahen, da\u00df er ihr erfreut entgegen lief,", "tokens": ["Wir", "sa\u00b7hen", ",", "da\u00df", "er", "ihr", "er\u00b7freut", "ent\u00b7ge\u00b7gen", "lief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADJD", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "130Und sie mit Z\u00e4rtlichkeit bey einer Hand ergriff,", "tokens": ["sie", "mit", "Z\u00e4rt\u00b7lich\u00b7keit", "bey", "ei\u00b7ner", "Hand", "er\u00b7griff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.21": {"text": "Zugleich erbietig war von seinem Siz zu weichen,", "tokens": ["Zu\u00b7gleich", "er\u00b7bie\u00b7tig", "war", "von", "sei\u00b7nem", "Siz", "zu", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Nach dem er sich gewandt, ihr solchen darzureichen.", "tokens": ["Nach", "dem", "er", "sich", "ge\u00b7wandt", ",", "ihr", "sol\u00b7chen", "dar\u00b7zu\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVPP", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Mich wunderte, wie sehr gelassen alles war,", "tokens": ["Mich", "wun\u00b7der\u00b7te", ",", "wie", "sehr", "ge\u00b7las\u00b7sen", "al\u00b7les", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "VVPP", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wie sanft, begierig, still, was vor so wanckelbar.", "tokens": ["Wie", "sanft", ",", "be\u00b7gie\u00b7rig", ",", "still", ",", "was", "vor", "so", "wan\u00b7ckel\u00b7bar", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "PRELS", "APPR", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}