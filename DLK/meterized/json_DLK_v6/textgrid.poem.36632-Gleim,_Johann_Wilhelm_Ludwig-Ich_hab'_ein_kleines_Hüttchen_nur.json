{"textgrid.poem.36632": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Ich hab' ein kleines H\u00fcttchen nur", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' ein kleines H\u00fcttchen nur,", "tokens": ["Ich", "hab'", "ein", "klei\u00b7nes", "H\u00fctt\u00b7chen", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steht vest auf einer Wiesenflur,", "tokens": ["Steht", "vest", "auf", "ei\u00b7ner", "Wie\u00b7sen\u00b7flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wiesenflur, ist gro\u00df, ist sch\u00f6n!", "tokens": ["Die", "Wie\u00b7sen\u00b7flur", ",", "ist", "gro\u00df", ",", "ist", "sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "ADJD", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Willst mit in's H\u00fcttchen gehn?", "tokens": ["Willst", "mit", "in's", "H\u00fctt\u00b7chen", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Am H\u00fcttchen klein, steht gro\u00df ein Baum,", "tokens": ["Am", "H\u00fctt\u00b7chen", "klein", ",", "steht", "gro\u00df", "ein", "Baum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor welchem siehst das H\u00fcttchen kaum,", "tokens": ["Vor", "wel\u00b7chem", "siehst", "das", "H\u00fctt\u00b7chen", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sch\u00fctzt gegen Sonne, K\u00e4lt' und Wind,", "tokens": ["Sch\u00fctzt", "ge\u00b7gen", "Son\u00b7ne", ",", "K\u00e4lt'", "und", "Wind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "All' die darinnen sind!", "tokens": ["All'", "die", "da\u00b7rin\u00b7nen", "sind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADV", "VAFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Sitzt auf dem Baum' ein Nachtigall", "tokens": ["Sitzt", "auf", "dem", "Baum'", "ein", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Singt auf dem Baum so s\u00fc\u00dfen Schall,", "tokens": ["Singt", "auf", "dem", "Baum", "so", "s\u00fc\u00b7\u00dfen", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df jeder, der vor\u00fcber geht", "tokens": ["Da\u00df", "je\u00b7der", ",", "der", "vor\u00b7\u00fc\u00b7ber", "geht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm horcht, und stille steht!", "tokens": ["Ihm", "horcht", ",", "und", "stil\u00b7le", "steht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADJA", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Flie\u00dft unterm Baume hell ein Bach", "tokens": ["Flie\u00dft", "un\u00b7term", "Bau\u00b7me", "hell", "ein", "Bach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schwatzt alles s\u00fc\u00df dem Vogel nach,", "tokens": ["Schwatzt", "al\u00b7les", "s\u00fc\u00df", "dem", "Vo\u00b7gel", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "ART", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In diesem H\u00fcttchen bin allein,", "tokens": ["In", "die\u00b7sem", "H\u00fctt\u00b7chen", "bin", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mag's l\u00e4nger nicht mehr seyn!", "tokens": ["Mag's", "l\u00e4n\u00b7ger", "nicht", "mehr", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKNEG", "ADV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O du, mein Liebstes auf der Welt!", "tokens": ["O", "du", ",", "mein", "Liebs\u00b7tes", "auf", "der", "Welt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das H\u00fcttchen dir gewi\u00df gef\u00e4llt;", "tokens": ["Das", "H\u00fctt\u00b7chen", "dir", "ge\u00b7wi\u00df", "ge\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bist z\u00e4rtlich! Rauhe Winde wehn,", "tokens": ["Bist", "z\u00e4rt\u00b7lich", "!", "Rau\u00b7he", "Win\u00b7de", "wehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$.", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Willst mit in's H\u00fcttchen gehn?", "tokens": ["Willst", "mit", "in's", "H\u00fctt\u00b7chen", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}