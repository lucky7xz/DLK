{"textgrid.poem.25923": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "An deinen Lippen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deine K\u00fcsse halten mich gl\u00fchend wach,", "tokens": ["Dei\u00b7ne", "K\u00fcs\u00b7se", "hal\u00b7ten", "mich", "gl\u00fc\u00b7hend", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sie gehen wie feurige Sterne ums Dach.", "tokens": ["Sie", "ge\u00b7hen", "wie", "feu\u00b7ri\u00b7ge", "Ster\u00b7ne", "ums", "Dach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "An deinen Lippen wird's Blut mir rot,", "tokens": ["An", "dei\u00b7nen", "Lip\u00b7pen", "wird's", "Blut", "mir", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mein Herz springt ins Feuer, mein Auge loht.", "tokens": ["Mein", "Herz", "springt", "ins", "Feu\u00b7er", ",", "mein", "Au\u00b7ge", "loht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Deine Augen wie kleine Monde beim K\u00fcssen", "tokens": ["Dei\u00b7ne", "Au\u00b7gen", "wie", "klei\u00b7ne", "Mon\u00b7de", "beim", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KOKOM", "ADJA", "NE", "APPRART", "NN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Im letzten Himmel verschwinden m\u00fcssen.", "tokens": ["Im", "letz\u00b7ten", "Him\u00b7mel", "ver\u00b7schwin\u00b7den", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}