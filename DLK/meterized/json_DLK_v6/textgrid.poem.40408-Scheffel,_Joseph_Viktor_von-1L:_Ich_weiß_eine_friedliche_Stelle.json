{"textgrid.poem.40408": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wei\u00df eine friedliche Stelle", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df eine friedliche Stelle", "tokens": ["Ich", "wei\u00df", "ei\u00b7ne", "fried\u00b7li\u00b7che", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Im schweigenden Ozean,", "tokens": ["Im", "schwei\u00b7gen\u00b7den", "O\u00b7ze\u00b7an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Kristallhell sch\u00e4umet die Welle", "tokens": ["Kris\u00b7tall\u00b7hell", "sch\u00e4u\u00b7met", "die", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Am Felsengestade hinan.", "tokens": ["Am", "Fel\u00b7sen\u00b7ge\u00b7sta\u00b7de", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Im Hafen erblickst du kein Segel,", "tokens": ["Im", "Ha\u00b7fen", "er\u00b7blickst", "du", "kein", "Se\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Keines Menschen Fu\u00dftritt am Strand;", "tokens": ["Kei\u00b7nes", "Men\u00b7schen", "Fu\u00df\u00b7tritt", "am", "Strand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "APPRART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Viel tausend reinliche V\u00f6gel", "tokens": ["Viel", "tau\u00b7send", "rein\u00b7li\u00b7che", "V\u00f6\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "H\u00fcten das einsame Land.", "tokens": ["H\u00fc\u00b7ten", "das", "ein\u00b7sa\u00b7me", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie sitzen in frommer Beschauung,", "tokens": ["Sie", "sit\u00b7zen", "in", "from\u00b7mer", "Be\u00b7schau\u00b7ung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Kein einz'ger vers\u00e4umt seine Pflicht,", "tokens": ["Kein", "einz'\u00b7ger", "ver\u00b7s\u00e4umt", "sei\u00b7ne", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Gesegnet ist ihre Verdauung", "tokens": ["Ge\u00b7seg\u00b7net", "ist", "ih\u00b7re", "Ver\u00b7dau\u00b7ung"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und fl\u00fcssig als wie ein Gedicht.", "tokens": ["Und", "fl\u00fcs\u00b7sig", "als", "wie", "ein", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die V\u00f6gel sind all' Philosophen,", "tokens": ["Die", "V\u00f6\u00b7gel", "sind", "all'", "Phi\u00b7lo\u00b7so\u00b7phen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ihr oberster Grundsatz gebeut:", "tokens": ["Ihr", "o\u00b7bers\u00b7ter", "Grund\u00b7satz", "ge\u00b7beut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "\u00bbden Leib halt' allezeit offen", "tokens": ["\u00bb", "den", "Leib", "halt'", "al\u00b7le\u00b7zeit", "of\u00b7fen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und alles andre gedeiht.\u00ab", "tokens": ["Und", "al\u00b7les", "and\u00b7re", "ge\u00b7deiht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "PIS", "VVFIN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Was die V\u00e4ter ger\u00e4uschlos begonnen,", "tokens": ["Was", "die", "V\u00e4\u00b7ter", "ge\u00b7r\u00e4uschlos", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die Enkel vollenden das Werk;", "tokens": ["Die", "En\u00b7kel", "voll\u00b7en\u00b7den", "das", "Werk", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gel\u00e4utert von tropischen Sonnen,", "tokens": ["Ge\u00b7l\u00e4u\u00b7tert", "von", "tro\u00b7pi\u00b7schen", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Schon t\u00fcrmt es empor sich zum Berg.", "tokens": ["Schon", "t\u00fcrmt", "es", "em\u00b7por", "sich", "zum", "Berg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "PRF", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Sie sehen im rosigsten Lichte", "tokens": ["Sie", "se\u00b7hen", "im", "ro\u00b7sigs\u00b7ten", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Die Zukunft und sprechen in Ruh':", "tokens": ["Die", "Zu\u00b7kunft", "und", "spre\u00b7chen", "in", "Ruh'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "\u00bbwir bauen im Lauf der Geschichte", "tokens": ["\u00bb", "wir", "bau\u00b7en", "im", "Lauf", "der", "Ge\u00b7schich\u00b7te"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Noch den ganzen Ozean zu.\u00ab", "tokens": ["Noch", "den", "gan\u00b7zen", "O\u00b7ze\u00b7an", "zu", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und die Anerkennung der Besten", "tokens": ["Und", "die", "An\u00b7er\u00b7ken\u00b7nung", "der", "Bes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Fehlt ihren Bestrebungen nicht,", "tokens": ["Fehlt", "ih\u00b7ren", "Be\u00b7stre\u00b7bun\u00b7gen", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn fern im schw\u00e4bischen Westen", "tokens": ["Denn", "fern", "im", "schw\u00e4\u00b7bi\u00b7schen", "Wes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der B\u00f6blinger Repsbauer spricht:", "tokens": ["Der", "B\u00f6b\u00b7lin\u00b7ger", "Reps\u00b7bau\u00b7er", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-++-++-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "\u00bbgott segn' euch, ihr trefflichen V\u00f6gel,", "tokens": ["\u00bb", "gott", "segn'", "euch", ",", "ihr", "treff\u00b7li\u00b7chen", "V\u00f6\u00b7gel", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "An der fernen Guanok\u00fcst', \u2013", "tokens": ["An", "der", "fer\u00b7nen", "Gu\u00b7a\u00b7no\u00b7k\u00fcst'", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "$("], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Trotz meinem Landsmann, dem Hegel,", "tokens": ["Trotz", "mei\u00b7nem", "Lands\u00b7mann", ",", "dem", "He\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Schafft ihr den gediegensten Mist!\u00ab", "tokens": ["Schafft", "ihr", "den", "ge\u00b7die\u00b7gens\u00b7ten", "Mist", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}