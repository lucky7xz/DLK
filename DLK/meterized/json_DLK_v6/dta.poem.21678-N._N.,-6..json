{"dta.poem.21678": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Der wilde Vogel in Korassan,", "tokens": ["Der", "wil\u00b7de", "Vo\u00b7gel", "in", "Ko\u00b7ras\u00b7san", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der seltsame Geselle,", "tokens": ["Der", "selt\u00b7sa\u00b7me", "Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Umschwebt im fernen Ispahan", "tokens": ["Um\u00b7schwebt", "im", "fer\u00b7nen", "Is\u00b7pa\u00b7han"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Immer die gleiche Quelle.", "tokens": ["Im\u00b7mer", "die", "glei\u00b7che", "Quel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Der Kaiser von Catay", "tokens": ["Der", "Kai\u00b7ser", "von", "Ca\u00b7tay"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sucht nach der Fluth des Jugendquells,", "tokens": ["Sucht", "nach", "der", "Fluth", "des", "Ju\u00b7gend\u00b7quells", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der sprudeln soll am Altai-Fels", "tokens": ["Der", "spru\u00b7deln", "soll", "am", "Al\u00b7tai\u00b7Fels"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "VVINF", "VMFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Mongolei, Mandschurei, Tartarei.", "tokens": ["Durch", "Mon\u00b7go\u00b7lei", ",", "Mand\u00b7schu\u00b7rei", ",", "Tar\u00b7ta\u00b7rei", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Tugend sucht Wiedergeb\u00e4rung", "tokens": ["Tu\u00b7gend", "sucht", "Wie\u00b7der\u00b7ge\u00b7b\u00e4\u00b7rung"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "In der Reue Marah-Quelle \u2014", "tokens": ["In", "der", "Reu\u00b7e", "Ma\u00b7rah\u00b7Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht lockt sie von der Stelle", "tokens": ["Nicht", "lockt", "sie", "von", "der", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Manna\u2019s s\u00fc\u00dfe Bescheerung.", "tokens": ["Des", "Man\u00b7na's", "s\u00fc\u00b7\u00dfe", "Be\u00b7schee\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}