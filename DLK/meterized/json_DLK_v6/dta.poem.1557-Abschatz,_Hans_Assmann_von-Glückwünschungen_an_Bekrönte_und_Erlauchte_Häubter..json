{"dta.poem.1557": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckw\u00fcnschungen  \n an  \n Bekr\u00f6nte  \n und  \n Erlauchte H\u00e4ubter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.58", "en:0.27", "so:0.14"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "BuDA CADIT!", "tokens": ["Bu\u00b7DA", "Ca\u00b7DIT", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "CaSU felici Buda RESURGIT", "tokens": ["Ca\u00b7SU", "fe\u00b7li\u00b7ci", "Bu\u00b7da", "Re\u00b7SUR\u00b7GIT"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "NE"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.3": {"text": "Turcaque", "tokens": ["Tur\u00b7ca\u00b7que"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Cum gemitu fugit indignatus ad Orcum.", "tokens": ["Cum", "ge\u00b7mi\u00b7tu", "fu\u00b7git", "in\u00b7dig\u00b7na\u00b7tus", "ad", "Or\u00b7cum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "Spectat Exitium &amp; Flammas Urbis", "tokens": ["Spec\u00b7tat", "E\u00b7xit\u00b7ium", "&amp;", "Flam\u00b7mas", "Ur\u00b7bis"], "token_info": ["word", "word", "XML_entity", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Excidium suorum", "tokens": ["Ex\u00b7ci\u00b7di\u00b7um", "su\u00b7o\u00b7rum"], "token_info": ["word", "word"], "pos": ["FM.la", "FM.la"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Trux Thracum Exercitus", "tokens": ["Trux", "Thra\u00b7cum", "Ex\u00b7er\u00b7ci\u00b7tus"], "token_info": ["word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Fremit, gemit, tremit, &amp; fugit.", "tokens": ["Fre\u00b7mit", ",", "ge\u00b7mit", ",", "tre\u00b7mit", ",", "&amp;", "fu\u00b7git", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "XML_entity", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "VVFIN", "$,", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Urbs redit ad Dominum, qui fuit ante, suum.", "tokens": ["Urbs", "re\u00b7dit", "ad", "Do\u00b7mi\u00b7num", ",", "qui", "fuit", "an\u00b7te", ",", "su\u00b7um", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "FM.la", "$,", "FM", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Felix mutatio", "tokens": ["Fe\u00b7lix", "mu\u00b7ta\u00b7tio"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Captivitatis in Libertatem", "tokens": ["Cap\u00b7ti\u00b7vi\u00b7ta\u00b7tis", "in", "Li\u00b7ber\u00b7ta\u00b7tem"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Superstitionis in Religionem,", "tokens": ["Su\u00b7per\u00b7sti\u00b7ti\u00b7o\u00b7nis", "in", "Re\u00b7li\u00b7gi\u00b7o\u00b7nem", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Barbarae Tyrannidis in Mite Imperium!", "tokens": ["Bar\u00b7ba\u00b7rae", "Ty\u00b7ran\u00b7ni\u00b7dis", "in", "Mi\u00b7te", "Im\u00b7pe\u00b7ri\u00b7um", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "OrIENTI", "tokens": ["O\u00b7rIEN\u00b7TI"], "token_info": ["word"], "pos": ["XY"], "meter": "+--", "measure": "dactylic.init"}, "line.15": {"text": "Sol oritur ab Austro:", "tokens": ["Sol", "o\u00b7ri\u00b7tur", "ab", "Aus\u00b7tro", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Aperto Claustro", "tokens": ["A\u00b7per\u00b7to", "Claust\u00b7ro"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.17": {"text": "reperta Clavi", "tokens": ["re\u00b7per\u00b7ta", "Cla\u00b7vi"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.18": {"text": "Ottomannici vasti Dominatus", "tokens": ["Ot\u00b7to\u00b7man\u00b7ni\u00b7ci", "vas\u00b7ti", "Do\u00b7mi\u00b7na\u00b7tus"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "mole ruentis sua", "tokens": ["mo\u00b7le", "ru\u00b7en\u00b7tis", "sua"], "token_info": ["word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.20": {"text": "Majoribus via victoriis aperitur.", "tokens": ["Ma\u00b7jo\u00b7ri\u00b7bus", "via", "vic\u00b7to\u00b7riis", "a\u00b7pe\u00b7ri\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Oritur Bosphoro Phosphorus", "tokens": ["O\u00b7ri\u00b7tur", "Bosp\u00b7ho\u00b7ro", "Phos\u00b7pho\u00b7rus"], "token_info": ["word", "word", "word"], "pos": ["NN", "NE", "NE"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.22": {"text": "Propinquae lucis nuncius", "tokens": ["Pro\u00b7pin\u00b7qua\u00b7e", "lu\u00b7cis", "nun\u00b7ci\u00b7us"], "token_info": ["word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "Orae Pannonicae Aurora:", "tokens": ["O\u00b7rae", "Pan\u00b7no\u00b7ni\u00b7cae", "Au\u00b7ro\u00b7ra", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.24": {"text": "Noctifugo Solis Austriaci Splendori", "tokens": ["Noc\u00b7ti\u00b7fu\u00b7go", "So\u00b7lis", "Aust\u00b7ri\u00b7a\u00b7ci", "Splen\u00b7do\u00b7ri"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.25": {"text": "in ipso Pleni-Lunio", "tokens": ["in", "ip\u00b7so", "Ple\u00b7ni\u00b7\u00b7L\u00b7unio"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.26": {"text": "Noctiluca Thracum Luna cedit.", "tokens": ["Noc\u00b7ti\u00b7lu\u00b7ca", "Thra\u00b7cum", "Lu\u00b7na", "ce\u00b7dit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.27": {"text": "Siderata Turcarum Arma", "tokens": ["Si\u00b7de\u00b7ra\u00b7ta", "Tur\u00b7ca\u00b7rum", "Ar\u00b7ma"], "token_info": ["word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.28": {"text": "Veluti Sidera", "tokens": ["Ve\u00b7lu\u00b7ti", "Si\u00b7de\u00b7ra"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Sub adventum Diei,", "tokens": ["Sub", "ad\u00b7ven\u00b7tum", "Di\u00b7ei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "Toto fugiunt perterrita C&#339;lo:", "tokens": ["To\u00b7to", "fu\u00b7gi\u00b7unt", "per\u00b7ter\u00b7ri\u00b7ta", "C", "&#339;", "lo", ":"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.31": {"text": "AqUIL&#198; VICTRICIS.", "tokens": ["A\u00b7qUIL", "&#198;", "ViCT\u00b7RICIS", "."], "token_info": ["word", "XML_entity", "word", "punct"], "pos": ["NE", "$(", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.32": {"text": "non", "tokens": ["non"], "token_info": ["word"], "pos": ["NE"], "meter": "-", "measure": "single.down"}, "line.33": {"text": "Carmina, sed Arma", "tokens": ["Car\u00b7mi\u00b7na", ",", "sed", "Ar\u00b7ma"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "FM.la", "FM.la"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.34": {"text": "C&#339;lo possunt deducere Lunam.", "tokens": ["C", "&#339;", "lo", "pos\u00b7sunt", "de\u00b7du\u00b7ce\u00b7re", "Lu\u00b7nam", "."], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.35": {"text": "Vesirius: Vae! Sirius!", "tokens": ["Ve\u00b7si\u00b7rius", ":", "Vae", "!", "Si\u00b7rius", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.36": {"text": "Syriae Canis, occidit.", "tokens": ["Sy\u00b7ri\u00b7ae", "Ca\u00b7nis", ",", "oc\u00b7ci\u00b7dit", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "FM.la", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.37": {"text": "Sol Augusti", "tokens": ["Sol", "Au\u00b7gus\u00b7ti"], "token_info": ["word", "word"], "pos": ["VMFIN", "NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.38": {"text": "Per Augustum in Leone Martio versatus", "tokens": ["Per", "Au\u00b7gus\u00b7tum", "in", "Le\u00b7o\u00b7ne", "Mar\u00b7tio", "ver\u00b7sa\u00b7tus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+-+---+-+-", "measure": "iambic.penta.invert"}, "line.39": {"text": "ViRGINEM", "tokens": ["ViR\u00b7GI\u00b7NEM"], "token_info": ["word"], "pos": ["NE"], "meter": "+-+", "measure": "trochaic.di"}, "line.40": {"text": "Secunda Septembris", "tokens": ["Se\u00b7cun\u00b7da", "Sep\u00b7temb\u00b7ris"], "token_info": ["word", "word"], "pos": ["FM.la", "FM.la"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.41": {"text": "Ante sesqui Seculum raptam", "tokens": ["An\u00b7te", "ses\u00b7qui", "Se\u00b7cu\u00b7lum", "rap\u00b7tam"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.42": {"text": "Secunda Septembris", "tokens": ["Se\u00b7cun\u00b7da", "Sep\u00b7temb\u00b7ris"], "token_info": ["word", "word"], "pos": ["FM.la", "FM.la"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.43": {"text": "Secundis ominibus", "tokens": ["Se\u00b7cun\u00b7dis", "o\u00b7mi\u00b7ni\u00b7bus"], "token_info": ["word", "word"], "pos": ["FM.la", "FM.la"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.44": {"text": "ingreditur.", "tokens": ["in\u00b7gre\u00b7di\u00b7tur", "."], "token_info": ["word", "punct"], "pos": ["FM.la", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.45": {"text": "SpICAMO Regia Spes &amp; Sedes", "tokens": ["SpI\u00b7CA\u00b7MO", "Re\u00b7gia", "Spes", "&amp;", "Se\u00b7des"], "token_info": ["word", "word", "word", "XML_entity", "word"], "pos": ["FM", "FM", "FM", "FM", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.46": {"text": "Sub Bootis Barbari pede languentem", "tokens": ["Sub", "Boo\u00b7tis", "Bar\u00b7ba\u00b7ri", "pe\u00b7de", "lan\u00b7gu\u00b7en\u00b7tem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.47": {"text": "erigis.", "tokens": ["e\u00b7ri\u00b7gis", "."], "token_info": ["word", "punct"], "pos": ["FM.la", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.48": {"text": "Quam Ego", "tokens": ["Quam", "E\u00b7go"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+-+", "measure": "trochaic.di"}, "line.49": {"text": "Post aliquot tua Regna videns mirabor Aristas!", "tokens": ["Post", "a\u00b7liq\u00b7uot", "tua", "Reg\u00b7na", "vi\u00b7dens", "mi\u00b7ra\u00b7bor", "A\u00b7ris\u00b7tas", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.50": {"text": "Vivant Heroes Paranymphi", "tokens": ["Vi\u00b7vant", "He\u00b7roes", "Par\u00b7a\u00b7nym\u00b7phi"], "token_info": ["word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Tibi Constantinopolim, Solymas sociaturi!", "tokens": ["Ti\u00b7bi", "Con\u00b7stan\u00b7ti\u00b7no\u00b7po\u00b7lim", ",", "So\u00b7ly\u00b7mas", "so\u00b7ci\u00b7a\u00b7tu\u00b7ri", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "NE", "$."], "meter": "+--+-+-+-+--+-+-", "measure": "iambic.septa.invert"}, "line.52": {"text": "ViVAT LEOPOLDUS", "tokens": ["Vi\u00b7VAT", "LeO\u00b7POL\u00b7DUS"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.53": {"text": "ViCTOR! TRIUMFATOR!", "tokens": ["ViC\u00b7TOR", "!", "TrI\u00b7UM\u00b7FA\u00b7TOR", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}