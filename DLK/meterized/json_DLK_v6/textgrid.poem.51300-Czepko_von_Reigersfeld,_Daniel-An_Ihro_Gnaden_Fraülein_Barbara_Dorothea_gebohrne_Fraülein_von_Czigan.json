{"textgrid.poem.51300": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An Ihro Gnaden Fra\u00fclein Barbara Dorothea gebohrne Fra\u00fclein von Czigan", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was aus der Aschen ich", "tokens": ["Was", "aus", "der", "A\u00b7schen", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Geschorren und bekommen,", "tokens": ["Ge\u00b7schor\u00b7ren", "und", "be\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als noch das Blat geglommen,", "tokens": ["Als", "noch", "das", "Blat", "ge\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wil hier bedienen dich.", "tokens": ["Wil", "hier", "be\u00b7die\u00b7nen", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVFIN", "PPER", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "O Barbara, es ist", "tokens": ["O", "Bar\u00b7ba\u00b7ra", ",", "es", "ist"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "PPER", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Im Feuer aufgeflogen,", "tokens": ["Im", "Feu\u00b7er", "auf\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was ich auf hundert Bogen", "tokens": ["Was", "ich", "auf", "hun\u00b7dert", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu deinem Lob erkiest.", "tokens": ["Zu", "dei\u00b7nem", "Lob", "er\u00b7kiest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Jedoch, es sol dein Prei\u00df", "tokens": ["Je\u00b7doch", ",", "es", "sol", "dein", "Prei\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nicht seyn des Feuers Beute,", "tokens": ["Nicht", "seyn", "des", "Feu\u00b7ers", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAINF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O Freundin weiser Leute,", "tokens": ["O", "Freun\u00b7din", "wei\u00b7ser", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Glut frist nicht ihren Schwei\u00df.", "tokens": ["Glut", "frist", "nicht", "ih\u00b7ren", "Schwei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Das Ertzt kriegt starcken Schein,", "tokens": ["Das", "Ertzt", "kriegt", "star\u00b7cken", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wo man es schmeltzt zusammen,", "tokens": ["Wo", "man", "es", "schmeltzt", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So wird dein Lob in Flammen", "tokens": ["So", "wird", "dein", "Lob", "in", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Salamander seyn.", "tokens": ["Ein", "Sa\u00b7la\u00b7man\u00b7der", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}}}}