{"textgrid.poem.34077": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Homo sapiens", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch ein Narr ist der Mensch! In Allem mu\u00df er sich spiegeln!", "tokens": ["Welch", "ein", "Narr", "ist", "der", "Mensch", "!", "In", "Al\u00b7lem", "mu\u00df", "er", "sich", "spie\u00b7geln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VAFIN", "ART", "NN", "$.", "APPR", "PIS", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Selbst in Sonne und Mond hat er sein Antlitz entdeckt.", "tokens": ["Selbst", "in", "Son\u00b7ne", "und", "Mond", "hat", "er", "sein", "Ant\u00b7litz", "ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}}}}}