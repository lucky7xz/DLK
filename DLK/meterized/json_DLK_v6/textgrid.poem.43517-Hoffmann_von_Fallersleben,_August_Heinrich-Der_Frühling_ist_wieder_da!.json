{"textgrid.poem.43517": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Fr\u00fchling ist wieder da!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt zum Reigen, la\u00dft uns springen,", "tokens": ["Kommt", "zum", "Rei\u00b7gen", ",", "la\u00dft", "uns", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00dft uns fr\u00f6hlich sein und singen!", "tokens": ["La\u00dft", "uns", "fr\u00f6h\u00b7lich", "sein", "und", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "VAINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja der Wald ist wieder gr\u00fcn", "tokens": ["Ja", "der", "Wald", "ist", "wie\u00b7der", "gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ART", "NN", "VAFIN", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Wiesenblumen bl\u00fch'n.", "tokens": ["Und", "die", "Wie\u00b7sen\u00b7blu\u00b7men", "bl\u00fch'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["KON", "ART", "NN", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und es quackt der Frosch im Ried", "tokens": ["Und", "es", "quackt", "der", "Frosch", "im", "Ried"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Vogel singt sein Lied.", "tokens": ["Und", "der", "Vo\u00b7gel", "singt", "sein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Freud' und Leben fern und nah,", "tokens": ["Freud'", "und", "Le\u00b7ben", "fern", "und", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ja der Fr\u00fchling ist wieder da.", "tokens": ["Ja", "der", "Fr\u00fch\u00b7ling", "ist", "wie\u00b7der", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "VAFIN", "ADV", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Kommt zum Reigen, la\u00dft uns springen,", "tokens": ["Kommt", "zum", "Rei\u00b7gen", ",", "la\u00dft", "uns", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00dft uns fr\u00f6hlich sein und singen!", "tokens": ["La\u00dft", "uns", "fr\u00f6h\u00b7lich", "sein", "und", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "VAINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schmetterling' im Sonnenschein", "tokens": ["Schmet\u00b7ter\u00b7ling'", "im", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie lebend'ge Bl\u00fcmelein.", "tokens": ["Wie", "le\u00b7ben\u00b7d'\u00b7ge", "Bl\u00fc\u00b7me\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und die Bienen mit Gesumm", "tokens": ["Und", "die", "Bie\u00b7nen", "mit", "Ge\u00b7summ"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gaukeln froh um uns herum.", "tokens": ["Gau\u00b7keln", "froh", "um", "uns", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Freud' und Leben fern und nah,", "tokens": ["Freud'", "und", "Le\u00b7ben", "fern", "und", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ja der Fr\u00fchling ist wieder da.", "tokens": ["Ja", "der", "Fr\u00fch\u00b7ling", "ist", "wie\u00b7der", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "VAFIN", "ADV", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}