{"dta.poem.2621": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Erlangte Freyheit der Seelen  \n  Hn. J. C. R. den 28. Junii  1669.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch/ der irrdisch ist/ und Erde Mutter heisset/", "tokens": ["Der", "Mensch", "/", "der", "irr\u00b7disch", "ist", "/", "und", "Er\u00b7de", "Mut\u00b7ter", "heis\u00b7set", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJD", "VAFIN", "$(", "KON", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der die Verwesung selbst zu einem Vater hat/", "tokens": ["Der", "die", "Ver\u00b7we\u00b7sung", "selbst", "zu", "ei\u00b7nem", "Va\u00b7ter", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "APPR", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die W\u00fcrmer Schwestern nennt/ und nur von aussen", "tokens": ["Die", "W\u00fcr\u00b7mer", "Schwes\u00b7tern", "nennt", "/", "und", "nur", "von", "aus\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "$(", "KON", "ADV", "APPR", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist nur ein Spiel der Zeit/ und des Gel\u00fcckes Rad;", "tokens": ["Ist", "nur", "ein", "Spiel", "der", "Zeit", "/", "und", "des", "Ge\u00b7l\u00fc\u00b7ckes", "Rad", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN", "$(", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mu\u00df/ weil er Athem zeucht/ in strenger Knechtschafft leben/", "tokens": ["Mu\u00df", "/", "weil", "er", "A\u00b7them", "zeucht", "/", "in", "stren\u00b7ger", "Knecht\u00b7schafft", "le\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$(", "KOUS", "PPER", "NN", "VVFIN", "$(", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und seine Dienstbarkeit f\u00e4ngt bey der Wiegen an;", "tokens": ["Und", "sei\u00b7ne", "Dienst\u00b7bar\u00b7keit", "f\u00e4ngt", "bey", "der", "Wie\u00b7gen", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Windeln/ so zu erst ein zartes Kind umgeben/", "tokens": ["Die", "Win\u00b7deln", "/", "so", "zu", "erst", "ein", "zar\u00b7tes", "Kind", "um\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "APPR", "ADV", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sind Zeugen/ da\u00df die Zeit ihn st\u00e4rcker fesseln kan.", "tokens": ["Sind", "Zeu\u00b7gen", "/", "da\u00df", "die", "Zeit", "ihn", "st\u00e4r\u00b7cker", "fes\u00b7seln", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$(", "KOUS", "ART", "NN", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und freilich ist der Leib ein Angst-Gemach der Seele/", "tokens": ["Und", "frei\u00b7lich", "ist", "der", "Leib", "ein", "Angst\u00b7Ge\u00b7mach", "der", "See\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein Block-Haus/ das an Qual und grausen Martern reich/", "tokens": ["Ein", "Block\u00b7Haus", "/", "das", "an", "Qual", "und", "grau\u00b7sen", "Mar\u00b7tern", "reich", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "APPR", "NN", "KON", "ADJA", "NN", "ADJD", "$("], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.11": {"text": "Ein arge Folter-Banck und schreckens-volle H\u00f6le/", "tokens": ["Ein", "ar\u00b7ge", "Fol\u00b7ter\u00b7Banck", "und", "schre\u00b7ckens\u00b7vol\u00b7le", "H\u00f6\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wo Kranckheit/ Kummer/ Weh vollf\u00fchren Streich auff", "tokens": ["Wo", "Kran\u00b7ck\u00b7heit", "/", "Kum\u00b7mer", "/", "Weh", "voll\u00b7f\u00fch\u00b7ren", "Streich", "auff"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "$(", "NN", "$(", "NN", "ADJA", "NN", "APPR"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Der Gottheit Ebenbild des Sch\u00f6pffers Meister-St\u00fccke/", "tokens": ["Der", "Got\u00b7theit", "E\u00b7ben\u00b7bild", "des", "Sch\u00f6pf\u00b7fers", "Meis\u00b7ter\u00b7St\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Seele/ kan hier nicht in reiner Freyheit seyn.", "tokens": ["Die", "See\u00b7le", "/", "kan", "hier", "nicht", "in", "rei\u00b7ner", "Frey\u00b7heit", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VMFIN", "ADV", "PTKNEG", "APPR", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Theils flicht der erste Fluch/ theils Fleisch und Blut die Stricke/", "tokens": ["Theils", "flicht", "der", "ers\u00b7te", "Fluch", "/", "theils", "Fleisch", "und", "Blut", "die", "Stri\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$(", "ADV", "NN", "KON", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die ihre Reinligkeit in Netze h\u00fcllen ein.", "tokens": ["Die", "ih\u00b7re", "Rein\u00b7lig\u00b7keit", "in", "Net\u00b7ze", "h\u00fcl\u00b7len", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Welch einen harten Sturm erwecken die Begierden?", "tokens": ["Welch", "ei\u00b7nen", "har\u00b7ten", "Sturm", "er\u00b7we\u00b7cken", "die", "Be\u00b7gier\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und ob sie gleich behertzt die Wellen niederschl\u00e4gt/", "tokens": ["Und", "ob", "sie", "gleich", "be\u00b7hertzt", "die", "Wel\u00b7len", "nie\u00b7der\u00b7schl\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So kommen anderwerts hervor vermummte Zierden/", "tokens": ["So", "kom\u00b7men", "an\u00b7der\u00b7werts", "her\u00b7vor", "ver\u00b7mumm\u00b7te", "Zier\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Von denen sie gewi\u00df mit Banden wird belegt.", "tokens": ["Von", "de\u00b7nen", "sie", "ge\u00b7wi\u00df", "mit", "Ban\u00b7den", "wird", "be\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die edle K\u00e4mpfferin ob sie sich gleich entbrechen/", "tokens": ["Die", "ed\u00b7le", "K\u00e4mpf\u00b7fe\u00b7rin", "ob", "sie", "sich", "gleich", "ent\u00b7bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOUS", "PPER", "PRF", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und zu der Sternen-H\u00f6h gefl\u00fcgelt heben wil/", "tokens": ["Und", "zu", "der", "Ster\u00b7nen\u00b7H\u00f6h", "ge\u00b7fl\u00fc\u00b7gelt", "he\u00b7ben", "wil", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So wird der Leibes Last ihr so die Schwingen schw\u00e4chen", "tokens": ["So", "wird", "der", "Lei\u00b7bes", "Last", "ihr", "so", "die", "Schwin\u00b7gen", "schw\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df sie aus M\u00fcdigkeit verlihrt das rechte Ziel.", "tokens": ["Da\u00df", "sie", "aus", "M\u00fc\u00b7dig\u00b7keit", "ver\u00b7lihrt", "das", "rech\u00b7te", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der Glieder Bley-Gewicht das dr\u00fcckt ihn zu der Erden/", "tokens": ["Der", "Glie\u00b7der", "Bley\u00b7Ge\u00b7wicht", "das", "dr\u00fcckt", "ihn", "zu", "der", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der angehengte Stein des Fleisches ist zu schwer.", "tokens": ["Der", "an\u00b7ge\u00b7heng\u00b7te", "Stein", "des", "Flei\u00b7sches", "ist", "zu", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Rom schweige doch nur still von seinen Marter-Pferden", "tokens": ["Rom", "schwei\u00b7ge", "doch", "nur", "still", "von", "sei\u00b7nen", "Mar\u00b7ter\u00b7P\u00b7fer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und br\u00e4cht\u2019 es Pfahl und Rad/ und Pech und Fackeln her!", "tokens": ["Und", "br\u00e4cht'", "es", "Pfahl", "und", "Rad", "/", "und", "Pech", "und", "Fa\u00b7ckeln", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "$(", "KON", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Des H\u00f6chsten Feuer-Zorn ob den begangnen S\u00fcnden/", "tokens": ["Des", "H\u00f6chs\u00b7ten", "Feu\u00b7er\u00b7Zorn", "ob", "den", "be\u00b7gang\u00b7nen", "S\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOUS", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So unsre Kr\u00e4ffte frist und gleich den Scherben macht/", "tokens": ["So", "uns\u00b7re", "Kr\u00e4ff\u00b7te", "frist", "und", "gleich", "den", "Scher\u00b7ben", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "KON", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die innern Seelen Ri\u00df\u2019/ und was wir mehr empfinden", "tokens": ["Die", "in\u00b7nern", "See\u00b7len", "Ri\u00df'", "/", "und", "was", "wir", "mehr", "emp\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "KON", "PWS", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wenn der geklemmte Geist in seinem Kercker schmacht/", "tokens": ["Wenn", "der", "ge\u00b7klemm\u00b7te", "Geist", "in", "sei\u00b7nem", "Ker\u00b7cker", "schmacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die \u00fcbertreffen auch der Hencker Schl\u00e4g\u2019 und Striemen!", "tokens": ["Die", "\u00fc\u00b7bert\u00b7ref\u00b7fen", "auch", "der", "Hen\u00b7cker", "Schl\u00e4g'", "und", "Strie\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ach! unsre Seel\u2019 ist mehr gefangen als man denckt;", "tokens": ["Ach", "!", "uns\u00b7re", "Seel'", "ist", "mehr", "ge\u00b7fan\u00b7gen", "als", "man", "denckt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "KOKOM", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Mit seinem Labyrint mag sich der ", "tokens": ["Mit", "sei\u00b7nem", "La\u00b7by\u00b7rint", "mag", "sich", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PRF", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Der Tausend hat verf\u00fchrt und mit dem Tod\u2019 umschrenckt.", "tokens": ["Der", "Tau\u00b7send", "hat", "ver\u00b7f\u00fchrt", "und", "mit", "dem", "Tod'", "um\u00b7schrenckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Was mehr? ", "tokens": ["Was", "mehr", "?"], "token_info": ["word", "word", "punct"], "pos": ["PWS", "ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.38": {"text": "Dem unterirrdschen Haus/ erf\u00fcllt von Leich und Stanck/", "tokens": ["Dem", "un\u00b7ter\u00b7irrd\u00b7schen", "Haus", "/", "er\u00b7f\u00fcllt", "von", "Leich", "und", "Stanck", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "VVPP", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und/ von der ", "tokens": ["Und", "/", "von", "der"], "token_info": ["word", "punct", "word", "word"], "pos": ["KON", "$(", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.40": {"text": "In welche niemand nicht/ als der verdammte sanck", "tokens": ["In", "wel\u00b7che", "nie\u00b7mand", "nicht", "/", "als", "der", "ver\u00b7damm\u00b7te", "san\u00b7ck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PIS", "PTKNEG", "$(", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wem ist das ", "tokens": ["Wem", "ist", "das"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.42": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.43": {"text": "Der Abgrund zu Athen/ so frembd und ungemeine?", "tokens": ["Der", "Ab\u00b7grund", "zu", "A\u00b7then", "/", "so", "frembd", "und", "un\u00b7ge\u00b7mei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$(", "ADV", "ADJD", "KON", "ADJA", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.44": {"text": "