{"textgrid.poem.46729": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[es kommt der lieblichste der Lenze]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kommt der lieblichste der Lenze,", "tokens": ["Es", "kommt", "der", "lieb\u00b7lichs\u00b7te", "der", "Len\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Glanz umringt,", "tokens": ["Von", "Glanz", "um\u00b7ringt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Doch alle Kr\u00e4nze, die er bringt,", "tokens": ["Doch", "al\u00b7le", "Kr\u00e4n\u00b7ze", ",", "die", "er", "bringt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind Todtenkr\u00e4nze.", "tokens": ["Sind", "Tod\u00b7ten\u00b7kr\u00e4n\u00b7ze", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Den Fr\u00fchling sah ich diesmal l\u00e4cheln", "tokens": ["Den", "Fr\u00fch\u00b7ling", "sah", "ich", "dies\u00b7mal", "l\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So d\u00e4mmerklar,", "tokens": ["So", "d\u00e4m\u00b7mer\u00b7klar", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und f\u00fchlte seinen Odem f\u00e4cheln", "tokens": ["Und", "f\u00fchl\u00b7te", "sei\u00b7nen", "O\u00b7dem", "f\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wunderbar,", "tokens": ["So", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ich wu\u00dfte gar nicht, was es war,", "tokens": ["Ich", "wu\u00df\u00b7te", "gar", "nicht", ",", "was", "es", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bis das Gef\u00fchl mir auf sich dringt,", "tokens": ["Bis", "das", "Ge\u00b7f\u00fchl", "mir", "auf", "sich", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Warum so matt der Kranz ihm gl\u00e4nze:", "tokens": ["Wa\u00b7rum", "so", "matt", "der", "Kranz", "ihm", "gl\u00e4n\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es sind doch immer Todtenkr\u00e4nze,", "tokens": ["Es", "sind", "doch", "im\u00b7mer", "Tod\u00b7ten\u00b7kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn auch der sch\u00f6nste Lenz sie bringt.", "tokens": ["Wenn", "auch", "der", "sch\u00f6ns\u00b7te", "Lenz", "sie", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Fr\u00fchling m\u00f6cht', und kann nicht, trauern,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "m\u00f6cht'", ",", "und", "kann", "nicht", ",", "trau\u00b7ern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$,", "KON", "VMFIN", "PTKNEG", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es steht ihm nicht,", "tokens": ["Es", "steht", "ihm", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es kann nicht lang in Falten dauern", "tokens": ["Es", "kann", "nicht", "lang", "in", "Fal\u00b7ten", "dau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Angesicht;", "tokens": ["Sein", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und wenn sich bricht sein Freudenlicht", "tokens": ["Und", "wenn", "sich", "bricht", "sein", "Freu\u00b7den\u00b7licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PRF", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Kummerwolken, so entspringt", "tokens": ["In", "Kum\u00b7mer\u00b7wol\u00b7ken", ",", "so", "ent\u00b7springt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der hellste Kranz an dunkler Grenze:", "tokens": ["Der", "hells\u00b7te", "Kranz", "an", "dunk\u00b7ler", "Gren\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es ist der lieblichste der Lenze,", "tokens": ["Es", "ist", "der", "lieb\u00b7lichs\u00b7te", "der", "Len\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn er auch Trauerkr\u00e4nze schlingt.", "tokens": ["Wenn", "er", "auch", "Trau\u00b7er\u00b7kr\u00e4n\u00b7ze", "schlingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}