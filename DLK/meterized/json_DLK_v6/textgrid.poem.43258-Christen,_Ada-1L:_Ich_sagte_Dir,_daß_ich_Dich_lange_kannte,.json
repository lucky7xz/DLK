{"textgrid.poem.43258": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sagte Dir, da\u00df ich Dich lange kannte,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sagte Dir, da\u00df ich Dich lange kannte,", "tokens": ["Ich", "sag\u00b7te", "Dir", ",", "da\u00df", "ich", "Dich", "lan\u00b7ge", "kann\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bevor Dein Auge jemals mich geschaut.", "tokens": ["Be\u00b7vor", "Dein", "Au\u00b7ge", "je\u00b7mals", "mich", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gedenke nur der hellen Sommertage,", "tokens": ["Ge\u00b7den\u00b7ke", "nur", "der", "hel\u00b7len", "Som\u00b7mer\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Du verlebt hast in den Alpen einst.", "tokens": ["Die", "Du", "ver\u00b7lebt", "hast", "in", "den", "Al\u00b7pen", "einst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da war ein Morgen, wo mich Vogelzwitschern", "tokens": ["Da", "war", "ein", "Mor\u00b7gen", ",", "wo", "mich", "Vo\u00b7gel\u00b7zwit\u00b7schern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u2013 Noch halbverhalten und doch traumgeschw\u00e4tzig \u2013", "tokens": ["\u2013", "Noch", "halb\u00b7ver\u00b7hal\u00b7ten", "und", "doch", "traum\u00b7ge\u00b7schw\u00e4t\u00b7zig", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "KON", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hinausrief auf die blanke Holzaltane.", "tokens": ["Hin\u00b7aus\u00b7rief", "auf", "die", "blan\u00b7ke", "Holz\u00b7al\u00b7ta\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es fl\u00fcsterte ein schwacher, k\u00fchler Wind,", "tokens": ["Es", "fl\u00fcs\u00b7ter\u00b7te", "ein", "schwa\u00b7cher", ",", "k\u00fch\u00b7ler", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und feuchte Nachtluft str\u00f6mte aus den B\u00fcschen,", "tokens": ["Und", "feuch\u00b7te", "Nacht\u00b7luft", "str\u00f6m\u00b7te", "aus", "den", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und jeder noch so leise Ton war h\u00f6rbar.", "tokens": ["Und", "je\u00b7der", "noch", "so", "lei\u00b7se", "Ton", "war", "h\u00f6r\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "ADV", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": ". . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}}, "stanza.3": {"line.1": {"text": "Da stand ich lange, lauschte in die Ferne,", "tokens": ["Da", "stand", "ich", "lan\u00b7ge", ",", "lauschte", "in", "die", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mein Herz erbebte und schlug freudevoll,", "tokens": ["Mein", "Herz", "er\u00b7beb\u00b7te", "und", "schlug", "freu\u00b7de\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+++-+", "measure": "zehnsilber"}, "line.3": {"text": "Als harrte holder Zukunft es entgegen,", "tokens": ["Als", "harr\u00b7te", "hol\u00b7der", "Zu\u00b7kunft", "es", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die sacht heraufzog mit dem jungen Tag,", "tokens": ["Die", "sacht", "her\u00b7auf\u00b7zog", "mit", "dem", "jun\u00b7gen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der schon mit zartem Roth die Berge f\u00e4rbte.", "tokens": ["Der", "schon", "mit", "zar\u00b7tem", "Roth", "die", "Ber\u00b7ge", "f\u00e4rb\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wie ich also lauschend, betend stand,", "tokens": ["Und", "wie", "ich", "al\u00b7so", "lau\u00b7schend", ",", "be\u00b7tend", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADJD", "$,", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Kam aus dem dunklen Thal ein Mann herauf", "tokens": ["Kam", "aus", "dem", "dunk\u00b7len", "Thal", "ein", "Mann", "her\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und schritt auch achtlos-still an mir vor\u00fcber.", "tokens": ["Und", "schritt", "auch", "acht\u00b7los\u00b7still", "an", "mir", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der Wanderer ging einsam seinen Weg", "tokens": ["Der", "Wan\u00b7de\u00b7rer", "ging", "ein\u00b7sam", "sei\u00b7nen", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Hin durch die w\u00fcrzig klare Morgenluft,", "tokens": ["Hin", "durch", "die", "w\u00fcr\u00b7zig", "kla\u00b7re", "Mor\u00b7gen\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Kein Strauch, kein Baum stand auf dem Felsenfirst,", "tokens": ["Kein", "Strauch", ",", "kein", "Baum", "stand", "auf", "dem", "Fel\u00b7sen\u00b7first", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Nichts als dies Eine Wesen war zu sehen,", "tokens": ["Nichts", "als", "dies", "Ei\u00b7ne", "We\u00b7sen", "war", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "PDS", "ART", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Das langsam unerm\u00fcdlich aufw\u00e4rts stieg ...", "tokens": ["Das", "lang\u00b7sam", "un\u00b7er\u00b7m\u00fcd\u00b7lich", "auf\u00b7w\u00e4rts", "stieg", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJD", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Bald hob die schlanke, m\u00e4nnliche Gestalt", "tokens": ["Bald", "hob", "die", "schlan\u00b7ke", ",", "m\u00e4nn\u00b7li\u00b7che", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Befremdlich-scharf sich ab von Luft und Himmel", "tokens": ["Be\u00b7fremd\u00b7lich\u00b7scha\u00b7rf", "sich", "ab", "von", "Luft", "und", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PTKVZ", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Hoch oben auf dem langgestreckten Grat.", "tokens": ["Hoch", "o\u00b7ben", "auf", "dem", "lang\u00b7ge\u00b7streck\u00b7ten", "Grat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Mit einmal aber schaute ich den Mann", "tokens": ["Mit", "ein\u00b7mal", "a\u00b7ber", "schau\u00b7te", "ich", "den", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Vor meinem Blick urpl\u00f6tzlich ganz verwandelt,", "tokens": ["Vor", "mei\u00b7nem", "Blick", "ur\u00b7pl\u00f6tz\u00b7lich", "ganz", "ver\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Denn aufgewachsen war er j\u00e4hlings jetzt", "tokens": ["Denn", "auf\u00b7ge\u00b7wach\u00b7sen", "war", "er", "j\u00e4h\u00b7lings", "jetzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Zu einer ", "tokens": ["Zu", "ei\u00b7ner"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Zu einer ", "tokens": ["Zu", "ei\u00b7ner"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "So ragte er schier dr\u00e4uend in den Himmel", "tokens": ["So", "rag\u00b7te", "er", "schier", "dr\u00e4u\u00b7end", "in", "den", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+--+-++-+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Und hob mit wilder, schmerzlicher Geberde", "tokens": ["Und", "hob", "mit", "wil\u00b7der", ",", "schmerz\u00b7li\u00b7cher", "Ge\u00b7ber\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Die Arme auf, der Riese, der Titan! ...", "tokens": ["Die", "Ar\u00b7me", "auf", ",", "der", "Rie\u00b7se", ",", "der", "Ti\u00b7tan", "!", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "$,", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und wie des Falken Schrei flog auf ein Laut,", "tokens": ["Und", "wie", "des", "Fal\u00b7ken", "Schrei", "flog", "auf", "ein", "Laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Vom Echo gellend wieder r\u00fcckgegeben.", "tokens": ["Vom", "E\u00b7cho", "gel\u00b7lend", "wie\u00b7der", "r\u00fcck\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da fa\u00dfte mich ein unaussprechlich Weh,", "tokens": ["Da", "fa\u00df\u00b7te", "mich", "ein", "un\u00b7aus\u00b7sprech\u00b7lich", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein gro\u00dfes, unverscheuchbar-tiefes Mitleid;", "tokens": ["Ein", "gro\u00b7\u00dfes", ",", "un\u00b7ver\u00b7scheuch\u00b7ba\u00b7rtie\u00b7fes", "Mit\u00b7leid", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Mir war, als m\u00fc\u00dfte ich zu ihm hinauf", "tokens": ["Mir", "war", ",", "als", "m\u00fc\u00df\u00b7te", "ich", "zu", "ihm", "hin\u00b7auf"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PPER", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und leise mich an seine Seite stellen", "tokens": ["Und", "lei\u00b7se", "mich", "an", "sei\u00b7ne", "Sei\u00b7te", "stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PRF", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und so geduldig harren, demuthsvoll,", "tokens": ["Und", "so", "ge\u00b7dul\u00b7dig", "har\u00b7ren", ",", "de\u00b7muths\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVINF", "$,", "ADJD", "$,"], "meter": "-+-+-+----", "measure": "unknown.measure.tri"}, "line.6": {"text": "Bis selber meine Hand er fassen w\u00fcrde", "tokens": ["Bis", "sel\u00b7ber", "mei\u00b7ne", "Hand", "er", "fas\u00b7sen", "w\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PPOSAT", "NN", "PPER", "VVINF", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und an des Weibes Herz die Qualen legen,", "tokens": ["Und", "an", "des", "Wei\u00b7bes", "Herz", "die", "Qua\u00b7len", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die er hinauftrug in die Einsamkeit.", "tokens": ["Die", "er", "hin\u00b7auf\u00b7trug", "in", "die", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Zum erstenmal erschrak ich vor dem Sein,", "tokens": ["Zum", "ers\u00b7ten\u00b7mal", "er\u00b7schrak", "ich", "vor", "dem", "Sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und unklar \u00fcberfiel mich eine Ahnung,", "tokens": ["Und", "un\u00b7klar", "\u00fc\u00b7berf\u00b7iel", "mich", "ei\u00b7ne", "Ah\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wie viel des Elends liegt auf jeder Seele,", "tokens": ["Wie", "viel", "des", "E\u00b7lends", "liegt", "auf", "je\u00b7der", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Wie viel ich h\u00fclflos selbst seit jeher trug.", "tokens": ["Wie", "viel", "ich", "h\u00fcl\u00b7flos", "selbst", "seit", "je\u00b7her", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ADJD", "ADV",