{"dta.poem.9890": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Laurette/ seit du mich besieget", "tokens": ["Lau\u00b7ret\u00b7te", "/", "seit", "du", "mich", "be\u00b7sie\u00b7get"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$(", "KOUS", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ich durch dich verwundet bin/", "tokens": ["Und", "ich", "durch", "dich", "ver\u00b7wun\u00b7det", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So f\u00fchl ich nichts/ das meinen sinn", "tokens": ["So", "f\u00fchl", "ich", "nichts", "/", "das", "mei\u00b7nen", "sinn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "$(", "ART", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lebens-geister mehr vergn\u00fcget/", "tokens": ["Und", "le\u00b7bens\u00b7geis\u00b7ter", "mehr", "ver\u00b7gn\u00fc\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als wenn durch deine freundligkeit", "tokens": ["Als", "wenn", "durch", "dei\u00b7ne", "freund\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein brennend hertze wird erfreut.", "tokens": ["Mein", "bren\u00b7nend", "hert\u00b7ze", "wird", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein strahl von deinen liebes-blicken", "tokens": ["Ein", "strahl", "von", "dei\u00b7nen", "lie\u00b7bes\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat meine geister angesteckt;", "tokens": ["Hat", "mei\u00b7ne", "geis\u00b7ter", "an\u00b7ge\u00b7steckt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was nun die flammen mir erweckt/", "tokens": ["Was", "nun", "die", "flam\u00b7men", "mir", "er\u00b7weckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "VVFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das kan hinwieder auch erquicken:", "tokens": ["Das", "kan", "hin\u00b7wie\u00b7der", "auch", "er\u00b7qui\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Drum such ich auch bey dir allein", "tokens": ["Drum", "such", "ich", "auch", "bey", "dir", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die s\u00fcsse lindrung meiner pein.", "tokens": ["Die", "s\u00fcs\u00b7se", "lind\u00b7rung", "mei\u00b7ner", "pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Kein blitz der sonst verliebt sich zeiget/", "tokens": ["Kein", "blitz", "der", "sonst", "ver\u00b7liebt", "sich", "zei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADV", "VVFIN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein ku\u00df/ wie hei\u00df er angebracht/", "tokens": ["Kein", "ku\u00df", "/", "wie", "hei\u00df", "er", "an\u00b7ge\u00b7bracht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "PWAV", "ADJD", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein freundlich-seyn hat solche macht/", "tokens": ["Kein", "freund\u00b7lich\u00b7seyn", "hat", "sol\u00b7che", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als deine liebligkeit; sie neiget", "tokens": ["Als", "dei\u00b7ne", "lieb\u00b7lig\u00b7keit", ";", "sie", "nei\u00b7get"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mein gantzes hertze zu dir hin/", "tokens": ["Mein", "gant\u00b7zes", "hert\u00b7ze", "zu", "dir", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich auch nicht mehr meine bin.", "tokens": ["Da\u00df", "ich", "auch", "nicht", "mehr", "mei\u00b7ne", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ADV", "PPOSAT", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich f\u00fchle t\u00e4glich noch die blitze/", "tokens": ["Ich", "f\u00fch\u00b7le", "t\u00e4g\u00b7lich", "noch", "die", "blit\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "ART", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Damit du erstlich mich ger\u00fchrt", "tokens": ["Da\u00b7mit", "du", "erst\u00b7lich", "mich", "ge\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und gleichsam mich mir selbst entf\u00fchrt/", "tokens": ["Und", "gleich\u00b7sam", "mich", "mir", "selbst", "ent\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein auge/ das verr\u00e4th die hitze/", "tokens": ["Mein", "au\u00b7ge", "/", "das", "ver\u00b7r\u00e4\u00b7th", "die", "hit\u00b7ze", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PDS", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die ich zu erst von dir empfand/", "tokens": ["Die", "ich", "zu", "erst", "von", "dir", "emp\u00b7fand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und macht sie dir genug bekant.", "tokens": ["Und", "macht", "sie", "dir", "ge\u00b7nug", "be\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dein bildni\u00df pr\u00e4gt sich meinem hertzen", "tokens": ["Dein", "bild\u00b7ni\u00df", "pr\u00e4gt", "sich", "mei\u00b7nem", "hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch st\u00fcndlich immer fester ein/", "tokens": ["Noch", "st\u00fcnd\u00b7lich", "im\u00b7mer", "fes\u00b7ter", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ADJD", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich mu\u00df entfernt auch um dich seyn", "tokens": ["Ich", "mu\u00df", "ent\u00b7fernt", "auch", "um", "dich", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "ADV", "APPR", "PPER", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und in gebancken mit dir schertzen:", "tokens": ["Und", "in", "ge\u00b7ban\u00b7cken", "mit", "dir", "schert\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mein geist wohnt \u00fcberall bey dir", "tokens": ["Mein", "geist", "wohnt", "\u00fc\u00b7be\u00b7rall", "bey", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und stellt dich mir im schlaffe f\u00fcr.", "tokens": ["Und", "stellt", "dich", "mir", "im", "schlaf\u00b7fe", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPRART", "VVFIN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich glaube/ da\u00df mir diese flammen", "tokens": ["Ich", "glau\u00b7be", "/", "da\u00df", "mir", "die\u00b7se", "flam\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "PDS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der himmel selber hat erregt/", "tokens": ["Der", "him\u00b7mel", "sel\u00b7ber", "hat", "er\u00b7regt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df nur zu dir ", "tokens": ["Da\u00df", "nur", "zu", "dir"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und andre liebe mu\u00df verdammen;", "tokens": ["Und", "and\u00b7re", "lie\u00b7be", "mu\u00df", "ver\u00b7dam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein sondrer trieb der reitzt mich an/", "tokens": ["Ein", "sond\u00b7rer", "trieb", "der", "reitzt", "mich", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich nur dir bin zugethan.", "tokens": ["Da\u00df", "ich", "nur", "dir", "bin", "zu\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wolan/ ich will der deine bleiben/", "tokens": ["Wo\u00b7lan", "/", "ich", "will", "der", "dei\u00b7ne", "blei\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PPER", "VMFIN", "ART", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein leitstern/ bleib du meine nur/", "tokens": ["Mein", "leits\u00b7tern", "/", "bleib", "du", "mei\u00b7ne", "nur", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "$(", "VVFIN", "PPER", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und la\u00df von dieser liebes-spur", "tokens": ["Und", "la\u00df", "von", "die\u00b7ser", "lie\u00b7bes\u00b7spur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch keine widrigkeit dich treiben;", "tokens": ["Durch", "kei\u00b7ne", "wid\u00b7rig\u00b7keit", "dich", "trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So schwer ich: keiner zeiten lauff", "tokens": ["So", "schwer", "ich", ":", "kei\u00b7ner", "zei\u00b7ten", "lauff"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "$.", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Soll unsre liebe halten auff.", "tokens": ["Soll", "uns\u00b7re", "lie\u00b7be", "hal\u00b7ten", "auff", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}