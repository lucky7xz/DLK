{"textgrid.poem.23964": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Neujahrs-Choral", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist des Weges Wende!", "tokens": ["Das", "ist", "des", "We\u00b7ges", "Wen\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun hebt voll Dank die H\u00e4nde:", "tokens": ["Nun", "hebt", "voll", "Dank", "die", "H\u00e4n\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Heil uns, wir stehn am Thor!", "tokens": ["Heil", "uns", ",", "wir", "stehn", "am", "Thor", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dahinter ist es helle,", "tokens": ["Da\u00b7hin\u00b7ter", "ist", "es", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es leuchtet auf der Schwelle", "tokens": ["Es", "leuch\u00b7tet", "auf", "der", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das junge Licht hervor.", "tokens": ["Das", "jun\u00b7ge", "Licht", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was werden wir nun sehen,", "tokens": ["Was", "wer\u00b7den", "wir", "nun", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn sich die Fl\u00fcgel drehen?", "tokens": ["Wenn", "sich", "die", "Fl\u00fc\u00b7gel", "dre\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die immer gleiche Bahn.", "tokens": ["Die", "im\u00b7mer", "glei\u00b7che", "Bahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Heil uns: das Ziel gewonnen!", "tokens": ["Heil", "uns", ":", "das", "Ziel", "ge\u00b7won\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Heil uns: aufs neu begonnen!", "tokens": ["Heil", "uns", ":", "aufs", "neu", "be\u00b7gon\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "APPRART", "ADJD", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Der Gang hebt wieder an.", "tokens": ["Der", "Gang", "hebt", "wie\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es geht von Thor zu Thoren,", "tokens": ["Es", "geht", "von", "Thor", "zu", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und kein Schritt ist verloren,", "tokens": ["Und", "kein", "Schritt", "ist", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Geht nur die Liebe mit.", "tokens": ["Geht", "nur", "die", "Lie\u00b7be", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wohl dem, den sie begleitet!", "tokens": ["Wohl", "dem", ",", "den", "sie", "be\u00b7glei\u00b7tet", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gl\u00fcck ist, wohin er schreitet,", "tokens": ["Gl\u00fcck", "ist", ",", "wo\u00b7hin", "er", "schrei\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und fr\u00f6hlich jeder Schritt.", "tokens": ["Und", "fr\u00f6h\u00b7lich", "je\u00b7der", "Schritt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und mag in Nacht und Tagen", "tokens": ["Und", "mag", "in", "Nacht", "und", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Uns b\u00f6ses Schicksal schlagen,", "tokens": ["Uns", "b\u00f6\u00b7ses", "Schick\u00b7sal", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir bleiben doch getrost:", "tokens": ["Wir", "blei\u00b7ben", "doch", "ge\u00b7trost", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Uns ist zu jeder Stunde,", "tokens": ["Uns", "ist", "zu", "je\u00b7der", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Uns ist f\u00fcr jede Wunde", "tokens": ["Uns", "ist", "f\u00fcr", "je\u00b7de", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Balsam zugelost.", "tokens": ["Ein", "Bal\u00b7sam", "zu\u00b7ge\u00b7lost", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Liebe l\u00e4\u00dft auf Erden", "tokens": ["Die", "Lie\u00b7be", "l\u00e4\u00dft", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht m\u00fcd und irre werden", "tokens": ["Nicht", "m\u00fcd", "und", "ir\u00b7re", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "KON", "NN", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und keinen einsam stehn.", "tokens": ["Und", "kei\u00b7nen", "ein\u00b7sam", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Auf Jahr mit Lust und Schmerzen!", "tokens": ["Auf", "Jahr", "mit", "Lust", "und", "Schmer\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir wolln mit reinen Herzen", "tokens": ["Wir", "wolln", "mit", "rei\u00b7nen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch deine Pforte gehn!", "tokens": ["Durch", "dei\u00b7ne", "Pfor\u00b7te", "gehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}