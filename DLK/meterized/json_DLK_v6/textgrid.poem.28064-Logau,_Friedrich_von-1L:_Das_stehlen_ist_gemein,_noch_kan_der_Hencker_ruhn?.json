{"textgrid.poem.28064": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das stehlen ist gemein, noch kan der Hencker ruhn?", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das stehlen ist gemein, noch kan der Hencker ruhn?", "tokens": ["Das", "steh\u00b7len", "ist", "ge\u00b7mein", ",", "noch", "kan", "der", "Hen\u00b7cker", "ruhn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADJD", "$,", "ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das stehlen ist zu gro\u00df, der Teuffel hat zu thun.", "tokens": ["Das", "steh\u00b7len", "ist", "zu", "gro\u00df", ",", "der", "Teuf\u00b7fel", "hat", "zu", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PTKA", "ADJD", "$,", "ART", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Dieb in alter Zeit geh\u00f6ren in die Lufft;", "tokens": ["Die", "Dieb", "in", "al\u00b7ter", "Zeit", "ge\u00b7h\u00f6\u00b7ren", "in", "die", "Lufft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Dieb in dieser Zeit geh\u00f6ren in die Klufft.", "tokens": ["Die", "Dieb", "in", "die\u00b7ser", "Zeit", "ge\u00b7h\u00f6\u00b7ren", "in", "die", "Klufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT