{"textgrid.poem.39382": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Theurer, dem ich dieses Lied gesendet,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Theurer, dem ich dieses Lied gesendet,", "tokens": ["Du", "Theu\u00b7rer", ",", "dem", "ich", "die\u00b7ses", "Lied", "ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "PPER", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mu\u00df ich dich selbst schon suchen bei den Todten?", "tokens": ["Mu\u00df", "ich", "dich", "selbst", "schon", "su\u00b7chen", "bei", "den", "Tod\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zur Todtenfeier hab' ich dich entboten:", "tokens": ["Zur", "Tod\u00b7ten\u00b7fei\u00b7er", "hab'", "ich", "dich", "ent\u00b7bo\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nun werd' ein Todtenopfer dir gespendet.", "tokens": ["Nun", "werd'", "ein", "Tod\u00b7ten\u00b7op\u00b7fer", "dir", "ge\u00b7spen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wer sich zu ferner Lieben Heimat wendet,", "tokens": ["Wer", "sich", "zu", "fer\u00b7ner", "Lie\u00b7ben", "Hei\u00b7mat", "wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "PTKA", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem wird gar mancher zarte Gru\u00df geboten;", "tokens": ["Dem", "wird", "gar", "man\u00b7cher", "zar\u00b7te", "Gru\u00df", "ge\u00b7bo\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So find' in dir mein Sehnen einen Boten,", "tokens": ["So", "find'", "in", "dir", "mein", "Seh\u00b7nen", "ei\u00b7nen", "Bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn je mein Herz dir liebend sich verpf\u00e4ndet.", "tokens": ["Wenn", "je", "mein", "Herz", "dir", "lie\u00b7bend", "sich", "ver\u00b7pf\u00e4n\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PPER", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sag' ihr: \u2013 doch in der Sprache jener Sph\u00e4ren", "tokens": ["Sag'", "ihr", ":", "\u2013", "doch", "in", "der", "Spra\u00b7che", "je\u00b7ner", "Sph\u00e4\u00b7ren"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "$.", "$(", "ADV", "APPR", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verstummt der Laut des Schmerzen, den ich meine,", "tokens": ["Ver\u00b7stummt", "der", "Laut", "des", "Schmer\u00b7zen", ",", "den", "ich", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und diese Trauer l\u00e4\u00dft sich dort nicht nennen.", "tokens": ["Und", "die\u00b7se", "Trau\u00b7er", "l\u00e4\u00dft", "sich", "dort", "nicht", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PRF", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O k\u00f6nntest du den Perlenschmuck der Z\u00e4hren", "tokens": ["O", "k\u00f6nn\u00b7test", "du", "den", "Per\u00b7len\u00b7schmuck", "der", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr bringen, die ich ihr und dir nun weine!", "tokens": ["Ihr", "brin\u00b7gen", ",", "die", "ich", "ihr", "und", "dir", "nun", "wei\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PRELS", "PPER", "PPER", "KON", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr wen sie flie\u00dfen, wei\u00df ich nicht zu trennen.", "tokens": ["F\u00fcr", "wen", "sie", "flie\u00b7\u00dfen", ",", "wei\u00df", "ich", "nicht", "zu", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "PPER", "VVINF", "$,", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}