{"textgrid.poem.63090": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Ironische Landschaft", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Brauner \u00c4cker welliger Zug,", "tokens": ["Brau\u00b7ner", "\u00c4\u00b7cker", "wel\u00b7li\u00b7ger", "Zug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Draus zweiarmig eine M\u00fchle w\u00e4chst.", "tokens": ["Draus", "zwei\u00b7ar\u00b7mig", "ei\u00b7ne", "M\u00fch\u00b7le", "w\u00e4chst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ein paar Pflaumenb\u00e4ume, wahllos hingeklext,", "tokens": ["Ein", "paar", "Pflau\u00b7men\u00b7b\u00e4u\u00b7me", ",", "wahl\u00b7los", "hin\u00b7ge\u00b7klext", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Ruhn auf eines H\u00fcgels schlankem Bug.", "tokens": ["Ruhn", "auf", "ei\u00b7nes", "H\u00fc\u00b7gels", "schlan\u00b7kem", "Bug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "In der Ferne seh ich ein paar F\u00f6hren,", "tokens": ["In", "der", "Fer\u00b7ne", "seh", "ich", "ein", "paar", "F\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stolzen Wuchses, mit Giraffenbeinen,", "tokens": ["Stol\u00b7zen", "Wuch\u00b7ses", ",", "mit", "Gir\u00b7af\u00b7fen\u00b7bei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und sie scheinen", "tokens": ["Und", "sie", "schei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Mir dem Fiskus zu geh\u00f6ren.", "tokens": ["Mir", "dem", "Fis\u00b7kus", "zu", "ge\u00b7h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}