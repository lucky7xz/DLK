{"dta.poem.15839": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Flur, auf deren Gr\u00fcn geliebte Blicke weilten,", "tokens": ["Die", "Flur", ",", "auf", "de\u00b7ren", "Gr\u00fcn", "ge\u00b7lieb\u00b7te", "Bli\u00b7cke", "weil\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch deren Morgenthau geliebte Tritt' enteilten,", "tokens": ["Durch", "de\u00b7ren", "Mor\u00b7gent\u00b7hau", "ge\u00b7lieb\u00b7te", "Tritt'", "ent\u00b7eil\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hat einen Farbenschmelz, hat einen Sonnenglanz,", "tokens": ["Hat", "ei\u00b7nen", "Far\u00b7ben\u00b7schmelz", ",", "hat", "ei\u00b7nen", "Son\u00b7nen\u00b7glanz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit dem wetteifern kann kein bl\u00fchndster Fr\u00fchlingskranz.", "tokens": ["Mit", "dem", "wett\u00b7ei\u00b7fern", "kann", "kein", "bl\u00fchnds\u00b7ter", "Fr\u00fch\u00b7lings\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVINF", "VMFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Fr\u00fchling kommt und geht, kehrt wieder, wird vergessen;", "tokens": ["Der", "Fr\u00fch\u00b7ling", "kommt", "und", "geht", ",", "kehrt", "wie\u00b7der", ",", "wird", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "ADV", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo Mirten dufteten, da schauern nun Zipressen:", "tokens": ["Wo", "Mir\u00b7ten", "duf\u00b7te\u00b7ten", ",", "da", "schau\u00b7ern", "nun", "Zip\u00b7res\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$,", "ADV", "VVFIN", "ADV", "NE", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch nie vergi\u00dft mein Herz ein Gl\u00fcck, einst hier besessen.", "tokens": ["Doch", "nie", "ver\u00b7gi\u00dft", "mein", "Herz", "ein", "Gl\u00fcck", ",", "einst", "hier", "be\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$,", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}