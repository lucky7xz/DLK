{"dta.poem.19175": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der junge Vogel wo lernt er den frohen Sinn,", "tokens": ["Der", "jun\u00b7ge", "Vo\u00b7gel", "wo", "lernt", "er", "den", "fro\u00b7hen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Flug und Gesang? lernt' ichs von ihm, welch ein Gewinn!", "tokens": ["Flug", "und", "Ge\u00b7sang", "?", "lernt'", "ichs", "von", "ihm", ",", "welch", "ein", "Ge\u00b7winn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$.", "VVFIN", "PIS", "APPR", "PPER", "$,", "PWAT", "ART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Im schwanken Neste schwankt er ob der Fr\u00fchlingsflur,", "tokens": ["Im", "schwan\u00b7ken", "Nes\u00b7te", "schwankt", "er", "ob", "der", "Fr\u00fch\u00b7lings\u00b7flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und athmet um sich her frisch athmende Natur.", "tokens": ["Und", "ath\u00b7met", "um", "sich", "her", "frisch", "ath\u00b7men\u00b7de", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "APZR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Von diesem Athem ist ihm Mark und Bein durchdrungen,", "tokens": ["Von", "die\u00b7sem", "A\u00b7them", "ist", "ihm", "Mark", "und", "Bein", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Brust gehoben und die junge Schwing' erschwungen.", "tokens": ["Die", "Brust", "ge\u00b7ho\u00b7ben", "und", "die", "jun\u00b7ge", "Schwing'", "er\u00b7schwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er sieht nur freie Luft, und f\u00fchlt nur frischen Duft,", "tokens": ["Er", "sieht", "nur", "frei\u00b7e", "Luft", ",", "und", "f\u00fchlt", "nur", "fri\u00b7schen", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$,", "KON", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und h\u00f6rt den Vater froh wie er der Mutter ruft.", "tokens": ["Und", "h\u00f6rt", "den", "Va\u00b7ter", "froh", "wie", "er", "der", "Mut\u00b7ter", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "KOKOM", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nur nachzusingen, nachzufliegen, nachzuahmen", "tokens": ["Nur", "nach\u00b7zu\u00b7sin\u00b7gen", ",", "nach\u00b7zu\u00b7flie\u00b7gen", ",", "nach\u00b7zu\u00b7ah\u00b7men"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VVINF", "$,", "VVIZU", "$,", "VVIZU"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat ers, und nie wird er verkr\u00fcppeln und erlahmen.", "tokens": ["Hat", "ers", ",", "und", "nie", "wird", "er", "ver\u00b7kr\u00fcp\u00b7peln", "und", "er\u00b7lah\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "KON", "ADV", "VAFIN", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "H\u00e4tt' eine S\u00e4ngerinn mein Wiegenkind zur Amme,", "tokens": ["H\u00e4tt'", "ei\u00b7ne", "S\u00e4n\u00b7ge\u00b7rinn", "mein", "Wie\u00b7gen\u00b7kind", "zur", "Am\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ihm des Wohllauts Oel tr\u00e4uft' in die zarte Flamme;", "tokens": ["Die", "ihm", "des", "Wohl\u00b7lauts", "O\u00b7el", "tr\u00e4uft'", "in", "die", "zar\u00b7te", "Flam\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Ein farbenbuntes Bett, ein k\u00fchles Laubgemach,", "tokens": ["Ein", "far\u00b7ben\u00b7bun\u00b7tes", "Bett", ",", "ein", "k\u00fch\u00b7les", "Laub\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Pf\u00fchl des Fr\u00fchlings und des Himmels goldnes Dach!", "tokens": ["Den", "Pf\u00fchl", "des", "Fr\u00fch\u00b7lings", "und", "des", "Him\u00b7mels", "gold\u00b7nes", "Dach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Auf seinem gr\u00fcnen Pf\u00fchl, unter dem goldnen Dach,", "tokens": ["Auf", "sei\u00b7nem", "gr\u00fc\u00b7nen", "Pf\u00fchl", ",", "un\u00b7ter", "dem", "gold\u00b7nen", "Dach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Wiegt' ihn der Mond in Schlaf, k\u00fc\u00dft' ihn die Sonne wach!", "tokens": ["Wiegt'", "ihn", "der", "Mond", "in", "Schlaf", ",", "k\u00fc\u00dft'", "ihn", "die", "Son\u00b7ne", "wach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Er pfl\u00fcckte jede Bl\u00fct', und br\u00e4che jede Frucht,", "tokens": ["Er", "pfl\u00fcck\u00b7te", "je\u00b7de", "Bl\u00fct'", ",", "und", "br\u00e4\u00b7che", "je\u00b7de", "Frucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ohn' Erziehung w\u00fcchs' er auf, ein Bild der Zucht.", "tokens": ["Und", "ohn'", "Er\u00b7zie\u00b7hung", "w\u00fcchs'", "er", "auf", ",", "ein", "Bild", "der", "Zucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Er m\u00fc\u00dfte frank und frei, froh wie ein Vogel werden,", "tokens": ["Er", "m\u00fc\u00df\u00b7te", "frank", "und", "frei", ",", "froh", "wie", "ein", "Vo\u00b7gel", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "KON", "ADJD", "$,", "ADJD", "KOKOM", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenn nicht fliegen, doch vor Lust sich so geberden.", "tokens": ["Und", "wenn", "nicht", "flie\u00b7gen", ",", "doch", "vor", "Lust", "sich", "so", "ge\u00b7ber\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PTKNEG", "VVINF", "$,", "ADV", "APPR", "NN", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}}}}}