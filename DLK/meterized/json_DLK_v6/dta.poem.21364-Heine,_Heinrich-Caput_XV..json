{"dta.poem.21364": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er sa\u00df nicht mehr auf steinernem Stuhl,", "tokens": ["Er", "sa\u00df", "nicht", "mehr", "auf", "stei\u00b7ner\u00b7nem", "Stuhl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Am steinernen Tisch, wie ein Steinbild;", "tokens": ["Am", "stei\u00b7ner\u00b7nen", "Tisch", ",", "wie", "ein", "Stein\u00b7bild", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Auch sah er nicht so ehrw\u00fcrdig aus,", "tokens": ["Auch", "sah", "er", "nicht", "so", "ehr\u00b7w\u00fcr\u00b7dig", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wie man sich gew\u00f6hnlich einbild\u2019t.", "tokens": ["Wie", "man", "sich", "ge\u00b7w\u00f6hn\u00b7lich", "ein\u00b7bild'", "t."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "PIS", "PRF", "ADJD", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Er watschelte durch die S\u00e4\u00e4le herum", "tokens": ["Er", "wat\u00b7schel\u00b7te", "durch", "die", "S\u00e4\u00b7\u00e4\u00b7le", "he\u00b7rum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APZR"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Mit mir im trauten Geschw\u00e4tze.", "tokens": ["Mit", "mir", "im", "trau\u00b7ten", "Ge\u00b7schw\u00e4t\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er zeigte wie ein Antiquar", "tokens": ["Er", "zeig\u00b7te", "wie", "ein", "An\u00b7ti\u00b7quar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir seine Curiosa und Sch\u00e4tze.", "tokens": ["Mir", "sei\u00b7ne", "Cu\u00b7ri\u00b7o\u00b7sa", "und", "Sch\u00e4t\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Im Saale der Waffen erkl\u00e4rte er mir", "tokens": ["Im", "Saa\u00b7le", "der", "Waf\u00b7fen", "er\u00b7kl\u00e4r\u00b7te", "er", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "PPER"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wie man sich der Kolben bediene,", "tokens": ["Wie", "man", "sich", "der", "Kol\u00b7ben", "be\u00b7die\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Von einigen Schwertern rieb er den Rost", "tokens": ["Von", "ei\u00b7ni\u00b7gen", "Schwer\u00b7tern", "rieb", "er", "den", "Rost"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit seinem Hermeline.", "tokens": ["Mit", "sei\u00b7nem", "Her\u00b7me\u00b7li\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er nahm einen Pfauenwedel zur Hand,", "tokens": ["Er", "nahm", "ei\u00b7nen", "Pfau\u00b7en\u00b7we\u00b7del", "zur", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und reinigte vom Staube", "tokens": ["Und", "rei\u00b7nig\u00b7te", "vom", "Stau\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gar manchen Harnisch, gar manchen Helm,", "tokens": ["Gar", "man\u00b7chen", "Har\u00b7nisch", ",", "gar", "man\u00b7chen", "Helm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auch manche Pickelhaube.", "tokens": ["Auch", "man\u00b7che", "Pi\u00b7ckel\u00b7hau\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}