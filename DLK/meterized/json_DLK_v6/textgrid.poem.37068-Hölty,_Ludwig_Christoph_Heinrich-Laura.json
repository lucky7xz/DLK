{"textgrid.poem.37068": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Laura", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Blick der Hoffnung heitert die Seele mir,", "tokens": ["Kein", "Blick", "der", "Hoff\u00b7nung", "hei\u00b7tert", "die", "See\u00b7le", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kein Blick der Freude! Nimmer, ach, nimmer wird", "tokens": ["Kein", "Blick", "der", "Freu\u00b7de", "!", "Nim\u00b7mer", ",", "ach", ",", "nim\u00b7mer", "wird"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN", "$.", "NE", "$,", "ITJ", "$,", "ADV", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dein Auge, Laura, meinem Auge", "tokens": ["Dein", "Au\u00b7ge", ",", "Lau\u00b7ra", ",", "mei\u00b7nem", "Au\u00b7ge"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NE", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wieder begegnen, und Liebe sprechen.", "tokens": ["Wie\u00b7der", "be\u00b7geg\u00b7nen", ",", "und", "Lie\u00b7be", "spre\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "KON", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Dein ehrner Fu\u00dftritt hallte mir oft, o Tod,", "tokens": ["Dein", "ehr\u00b7ner", "Fu\u00df\u00b7tritt", "hall\u00b7te", "mir", "oft", ",", "o", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,", "FM", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In meiner Kindheit werdenden D\u00e4mmerung,", "tokens": ["In", "mei\u00b7ner", "Kind\u00b7heit", "wer\u00b7den\u00b7den", "D\u00e4m\u00b7me\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und manche Mutterthr\u00e4ne rann mir", "tokens": ["Und", "man\u00b7che", "Mut\u00b7ter\u00b7thr\u00e4\u00b7ne", "rann", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf die verbl\u00fchende Knabenwange.", "tokens": ["Auf", "die", "ver\u00b7bl\u00fc\u00b7hen\u00b7de", "Kna\u00b7ben\u00b7wan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Wer hemmte deinen Bogen? O Seraphim,", "tokens": ["Wer", "hemm\u00b7te", "dei\u00b7nen", "Bo\u00b7gen", "?", "O", "Se\u00b7ra\u00b7phim", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$.", "NE", "NE", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was flogt ihr mit der Krone zur\u00fcck, und mit", "tokens": ["Was", "flogt", "ihr", "mit", "der", "Kro\u00b7ne", "zu\u00b7r\u00fcck", ",", "und", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,", "KON", "APPR"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Siegespalmen, die ihr eurer", "tokens": ["Den", "Sie\u00b7ge\u00b7spal\u00b7men", ",", "die", "ihr", "eu\u00b7rer"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Scheidenden Schwester entgegenhieltet?", "tokens": ["Schei\u00b7den\u00b7den", "Schwes\u00b7ter", "ent\u00b7ge\u00b7gen\u00b7hiel\u00b7tet", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "O Kronengeber, welcher den Sterblichen", "tokens": ["O", "Kro\u00b7nen\u00b7ge\u00b7ber", ",", "wel\u00b7cher", "den", "Sterb\u00b7li\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Ketten abrei\u00dft, komm, und entfe\u00dfle mich,", "tokens": ["Die", "Ket\u00b7ten", "ab\u00b7rei\u00dft", ",", "komm", ",", "und", "ent\u00b7fe\u00df\u00b7le", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "$,", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O Wonnetod! Dann schweb ich Lauren,", "tokens": ["O", "Won\u00b7ne\u00b7tod", "!", "Dann", "schweb", "ich", "Lau\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lauren entgegen, und bin ihr Engel!", "tokens": ["Lau\u00b7ren", "ent\u00b7ge\u00b7gen", ",", "und", "bin", "ihr", "En\u00b7gel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "KON", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}