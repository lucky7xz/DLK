{"textgrid.poem.38251": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Verlobung", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Haben die G\u00f6tter es also versehen,", "tokens": ["Ha\u00b7ben", "die", "G\u00f6t\u00b7ter", "es", "al\u00b7so", "ver\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Liebet euch lieblich, ich willige zu,", "tokens": ["Lie\u00b7bet", "euch", "lieb\u00b7lich", ",", "ich", "wil\u00b7li\u00b7ge", "zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "PPER", "ADJA", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Wollet euch ehrlich und ehlich begehen,", "tokens": ["Wol\u00b7let", "euch", "ehr\u00b7lich", "und", "eh\u00b7lich", "be\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Mehren und ehren in lieblicher Ruh.", "tokens": ["Meh\u00b7ren", "und", "eh\u00b7ren", "in", "lieb\u00b7li\u00b7cher", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}