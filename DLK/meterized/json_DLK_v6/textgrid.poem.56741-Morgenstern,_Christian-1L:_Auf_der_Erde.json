{"textgrid.poem.56741": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf der Erde", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf der Erde", "tokens": ["Auf", "der", "Er\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "steht eine hohe, gewaltige,", "tokens": ["steht", "ei\u00b7ne", "ho\u00b7he", ",", "ge\u00b7wal\u00b7ti\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "tausendsaitige Regenharfe.", "tokens": ["tau\u00b7send\u00b7sai\u00b7ti\u00b7ge", "Re\u00b7gen\u00b7har\u00b7fe", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und Phanta", "tokens": ["Und", "Phan\u00b7ta"], "token_info": ["word", "word"], "pos": ["KON", "NE"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "greift mit beiden", "tokens": ["greift", "mit", "bei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "PIAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "H\u00e4nden hinein", "tokens": ["H\u00e4n\u00b7den", "hin\u00b7ein"], "token_info": ["word", "word"], "pos": ["NN", "APZR"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "und singt dazu \u2013:", "tokens": ["und", "singt", "da\u00b7zu", "\u2013", ":"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PAV", "$(", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Monoton,", "tokens": ["Mo\u00b7no\u00b7ton", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "wie ein Indianerweib,", "tokens": ["wie", "ein", "In\u00b7di\u00b7a\u00b7ner\u00b7weib", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "immer dasselbe.", "tokens": ["im\u00b7mer", "das\u00b7sel\u00b7be", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PDAT", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Die Lider werden mir", "tokens": ["Die", "Li\u00b7der", "wer\u00b7den", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "schwer und schwerer.", "tokens": ["schwer", "und", "schwe\u00b7rer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Nach langem Halbschlaf", "tokens": ["Nach", "lan\u00b7gem", "Halb\u00b7schlaf"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "erwach ich wieder, \u2013", "tokens": ["er\u00b7wach", "ich", "wie\u00b7der", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "reibe verst\u00f6rt mir", "tokens": ["rei\u00b7be", "ver\u00b7st\u00f6rt", "mir"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "VVFIN", "PPER"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "die tr\u00e4gen Augen \u2013:", "tokens": ["die", "tr\u00e4\u00b7gen", "Au\u00b7gen", "\u2013", ":"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "auf der Erde", "tokens": ["auf", "der", "Er\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "steht eine hohe, gewaltige,", "tokens": ["steht", "ei\u00b7ne", "ho\u00b7he", ",", "ge\u00b7wal\u00b7ti\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.19": {"text": "tausendsaitige Regenharfe.", "tokens": ["tau\u00b7send\u00b7sai\u00b7ti\u00b7ge", "Re\u00b7gen\u00b7har\u00b7fe", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}