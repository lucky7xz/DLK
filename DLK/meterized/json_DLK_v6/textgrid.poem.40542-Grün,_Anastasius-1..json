{"textgrid.poem.40542": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fccklicher, dir ward gegeben", "tokens": ["Gl\u00fcck\u00b7li\u00b7cher", ",", "dir", "ward", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gar ein sch\u00f6ner gro\u00dfer Schmerz,", "tokens": ["Gar", "ein", "sch\u00f6\u00b7ner", "gro\u00b7\u00dfer", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr dein ganzes reiches Leben,", "tokens": ["F\u00fcr", "dein", "gan\u00b7zes", "rei\u00b7ches", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr dein ganzes volles Herz!", "tokens": ["F\u00fcr", "dein", "gan\u00b7zes", "vol\u00b7les", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eine Sonnenblume deuten", "tokens": ["Ei\u00b7ne", "Son\u00b7nen\u00b7blu\u00b7me", "deu\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6cht' ich deinen tiefen Schmerz,", "tokens": ["M\u00f6cht'", "ich", "dei\u00b7nen", "tie\u00b7fen", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die, all deine Tageszeiten", "tokens": ["Die", ",", "all", "dei\u00b7ne", "Ta\u00b7ges\u00b7zei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "PIAT", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gr\u00fc\u00dfend, kreiset um dein Herz.", "tokens": ["Gr\u00fc\u00b7\u00dfend", ",", "krei\u00b7set", "um", "dein", "Herz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "W\u00e4r's nur Unkraut kleiner Schmerzen,", "tokens": ["W\u00e4r's", "nur", "Un\u00b7kraut", "klei\u00b7ner", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unmuths d\u00fcrftig Dornenreis,", "tokens": ["Un\u00b7muths", "d\u00fcrf\u00b7tig", "Dor\u00b7nen\u00b7reis", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spr\u00e4ch' ich: Rei\u00df' es aus dem Herzen,", "tokens": ["Spr\u00e4ch'", "ich", ":", "Rei\u00df'", "es", "aus", "dem", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$.", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gib es allen Winden preis!", "tokens": ["Gib", "es", "al\u00b7len", "Win\u00b7den", "preis", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Spr\u00e4che: La\u00df es nicht umstricken", "tokens": ["Spr\u00e4\u00b7che", ":", "La\u00df", "es", "nicht", "um\u00b7stri\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVIMP", "PPER", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wuchernd deinen Lebenspfad,", "tokens": ["Wu\u00b7chernd", "dei\u00b7nen", "Le\u00b7ben\u00b7spfad", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df das Schlingkraut nicht erdr\u00fccken", "tokens": ["La\u00df", "das", "Schling\u00b7kraut", "nicht", "er\u00b7dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine junge Rosensaat!", "tokens": ["Dei\u00b7ne", "jun\u00b7ge", "Ro\u00b7sen\u00b7saat", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch es ward im Gartenraume,", "tokens": ["Doch", "es", "ward", "im", "Gar\u00b7ten\u00b7rau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welchen sonst du nennst dein Herz,", "tokens": ["Wel\u00b7chen", "sonst", "du", "nennst", "dein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wohl zum h\u00f6chsten gr\u00fcnen Baume", "tokens": ["Wohl", "zum", "h\u00f6chs\u00b7ten", "gr\u00fc\u00b7nen", "Bau\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser heil'ge gro\u00dfe Schmerz;", "tokens": ["Die\u00b7ser", "heil'\u00b7ge", "gro\u00b7\u00dfe", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Eine Palme, der Gehege", "tokens": ["Ei\u00b7ne", "Pal\u00b7me", ",", "der", "Ge\u00b7he\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Deines Gartens Kron' und Preis,", "tokens": ["Dei\u00b7nes", "Gar\u00b7tens", "Kron'", "und", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zu der sich alle Wege", "tokens": ["Und", "zu", "der", "sich", "al\u00b7le", "We\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRELS", "PRF", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schl\u00e4ngeln sch\u00f6n zur\u00fcck im Kreis!", "tokens": ["Schl\u00e4n\u00b7geln", "sch\u00f6n", "zu\u00b7r\u00fcck", "im", "Kreis", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Die ihr Haupt hoch in den Himmel,", "tokens": ["Die", "ihr", "Haupt", "hoch", "in", "den", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wurzeln tief zur Erde kehrt,", "tokens": ["Wur\u00b7zeln", "tief", "zur", "Er\u00b7de", "kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df du zweifelst, ob dem Himmel,", "tokens": ["Da\u00df", "du", "zwei\u00b7felst", ",", "ob", "dem", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Ob der Erde sie geh\u00f6rt?", "tokens": ["Ob", "der", "Er\u00b7de", "sie", "ge\u00b7h\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Hingestellt so zwischen beide", "tokens": ["Hin\u00b7ge\u00b7stellt", "so", "zwi\u00b7schen", "bei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als die sch\u00f6nste Mittlerin,", "tokens": ["Als", "die", "sch\u00f6ns\u00b7te", "Mitt\u00b7le\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4chst sie aus der Blumenheide", "tokens": ["W\u00e4chst", "sie", "aus", "der", "Blu\u00b7men\u00b7hei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wipfelnd in die Sterne hin.", "tokens": ["Wip\u00b7felnd", "in", "die", "Ster\u00b7ne", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "La\u00df kein Bl\u00e4ttlein ihr entwenden", "tokens": ["La\u00df", "kein", "Bl\u00e4tt\u00b7lein", "ihr", "ent\u00b7wen\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PIAT", "NN", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch der L\u00fcfte Schmeichelspiel!", "tokens": ["Durch", "der", "L\u00fcf\u00b7te", "Schmei\u00b7chel\u00b7spiel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df unheil'ge Hand nicht sch\u00e4nden", "tokens": ["La\u00df", "un\u00b7heil'\u00b7ge", "Hand", "nicht", "sch\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADJA", "NN", "PTKNEG", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ihres Stammes schlanken Kiel!", "tokens": ["Ih\u00b7res", "Stam\u00b7mes", "schlan\u00b7ken", "Kiel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Halte fern die Epheuranken,", "tokens": ["Hal\u00b7te", "fern", "die", "E\u00b7pheu\u00b7ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welche Menschentrost drum schwellt,", "tokens": ["Wel\u00b7che", "Men\u00b7schent\u00b7rost", "drum", "schwellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die den Baum nicht machen wanken,", "tokens": ["Die", "den", "Baum", "nicht", "ma\u00b7chen", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKNEG", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch durch die sein Schaft entstellt!", "tokens": ["Doch", "durch", "die", "sein", "Schaft", "ent\u00b7stellt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}}, "stanza.11": {"line.1": {"text": "Nicht bedarf's, ihn zu begie\u00dfen,", "tokens": ["Nicht", "be\u00b7da\u00b7rf's", ",", "ihn", "zu", "be\u00b7gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Deiner Thr\u00e4nen k\u00f6stlich Na\u00df;", "tokens": ["Dei\u00b7ner", "Thr\u00e4\u00b7nen", "k\u00f6st\u00b7lich", "Na\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00fch- und Abendthaue flie\u00dfen", "tokens": ["Fr\u00fch", "und", "A\u00b7bendt\u00b7hau\u00b7e", "flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["TRUNC", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ja auf ihn ohn' Unterla\u00df.", "tokens": ["Ja", "auf", "ihn", "ohn'", "Un\u00b7ter\u00b7la\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Aus den stillen gr\u00fcnen Matten", "tokens": ["Aus", "den", "stil\u00b7len", "gr\u00fc\u00b7nen", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rag' er schweigend, hoch, allein!", "tokens": ["Rag'", "er", "schwei\u00b7gend", ",", "hoch", ",", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ADJD", "$,", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einst in seinem Abendschatten", "tokens": ["Einst", "in", "sei\u00b7nem", "A\u00b7bend\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird ein s\u00fc\u00dfer Schlummer sein.", "tokens": ["Wird", "ein", "s\u00fc\u00b7\u00dfer", "Schlum\u00b7mer", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}