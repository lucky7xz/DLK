{"dta.poem.16220": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "274.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was du erlangen kanst, das stillt nicht dein Verlangen;", "tokens": ["Was", "du", "er\u00b7lan\u00b7gen", "kanst", ",", "das", "stillt", "nicht", "dein", "Ver\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "$,", "PDS", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was dein Verlangen stillt, das kanst du nicht erlangen.", "tokens": ["Was", "dein", "Ver\u00b7lan\u00b7gen", "stillt", ",", "das", "kanst", "du", "nicht", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Viel niedre G\u00fcter hat dein Hochsinn aufgegeben,", "tokens": ["Viel", "nied\u00b7re", "G\u00fc\u00b7ter", "hat", "dein", "Hoch\u00b7sinn", "auf\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aufgeben aber kanst du nicht dein h\u00f6chstes Streben.", "tokens": ["Auf\u00b7ge\u00b7ben", "a\u00b7ber", "kanst", "du", "nicht", "dein", "h\u00f6chs\u00b7tes", "Stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VMFIN", "PPER", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vertrau! umsonst ist nicht in dich gelegt der Trieb;", "tokens": ["Ver\u00b7trau", "!", "um\u00b7sonst", "ist", "nicht", "in", "dich", "ge\u00b7legt", "der", "Trieb", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VAFIN", "PTKNEG", "APPR", "PPER", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erschlie\u00dfen wird sich dort, was hier verschlossen blieb.", "tokens": ["Er\u00b7schlie\u00b7\u00dfen", "wird", "sich", "dort", ",", "was", "hier", "ver\u00b7schlos\u00b7sen", "blieb", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ADV", "$,", "PRELS", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dann wirst du sehen, wann du schaust was du geahnt;", "tokens": ["Dann", "wirst", "du", "se\u00b7hen", ",", "wann", "du", "schaust", "was", "du", "ge\u00b7ahnt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dis Ahnen hat den Weg zu jenem Schaun gebahnt.", "tokens": ["Dis", "Ah\u00b7nen", "hat", "den", "Weg", "zu", "je\u00b7nem", "Schaun", "ge\u00b7bahnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "ART", "NN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}