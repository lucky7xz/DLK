{"dta.poem.12300": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Drey Schwestern, Glaube, Liebe, Hoffnung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wollt ein J\u00e4ger jagen,               ", "tokens": ["Es", "wollt", "ein", "J\u00e4\u00b7ger", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dort wohl vor jenem Holz,", "tokens": ["Dort", "wohl", "vor", "je\u00b7nem", "Holz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was sah er auf der Heiden?", "tokens": ["Was", "sah", "er", "auf", "der", "Hei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Drey Fr\u00e4ulein h\u00fcbsch und stolz.", "tokens": ["Drey", "Fr\u00e4u\u00b7lein", "h\u00fcbsch", "und", "stolz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die erste hie\u00df Frau Glaube,", "tokens": ["Die", "ers\u00b7te", "hie\u00df", "Frau", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Frau Liebe hie\u00df die zweyt,", "tokens": ["Frau", "Lie\u00b7be", "hie\u00df", "die", "zweyt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ART", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Frau Hoffnung hie\u00df die dritte,", "tokens": ["Frau", "Hoff\u00b7nung", "hie\u00df", "die", "drit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des J\u00e4gers wollt sie seyn.", "tokens": ["Des", "J\u00e4\u00b7gers", "wollt", "sie", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er nahm sie in der Mitte,", "tokens": ["Er", "nahm", "sie", "in", "der", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sprach: \u201eHoffnung nicht von mir la\u00df!\u201c", "tokens": ["Sprach", ":", "\u201e", "Hoff\u00b7nung", "nicht", "von", "mir", "la\u00df", "!", "\u201c"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "$(", "NN", "PTKNEG", "APPR", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Schwanks hinter sich zur\u00fccke,", "tokens": ["Schwanks", "hin\u00b7ter", "sich", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl auf sein hohes Ro\u00df.", "tokens": ["Wohl", "auf", "sein", "ho\u00b7hes", "Ro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es f\u00fchrt sie gar behende", "tokens": ["Es", "f\u00fchrt", "sie", "gar", "be\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl durch das gr\u00fcne Gras,", "tokens": ["Wohl", "durch", "das", "gr\u00fc\u00b7ne", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Behielts bis an sein Ende,", "tokens": ["Be\u00b7hielts", "bis", "an", "sein", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und nimmer reut ihm das.", "tokens": ["Und", "nim\u00b7mer", "reut", "ihm", "das", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PDS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hoffnung macht nicht zu Schanden,", "tokens": ["Hoff\u00b7nung", "macht", "nicht", "zu", "Schan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Im Glauben fest an Gott,", "tokens": ["Im", "Glau\u00b7ben", "fest", "an", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem N\u00e4chsten geht zu Handen", "tokens": ["Dem", "N\u00e4chs\u00b7ten", "geht", "zu", "Han\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "die Liebe in der Noth.", "tokens": ["die", "Lie\u00b7be", "in", "der", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Hoffnung, Liebe und Glaube,", "tokens": ["Hoff\u00b7nung", ",", "Lie\u00b7be", "und", "Glau\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Die sch\u00f6nen Schwestern drey,", "tokens": ["Die", "sch\u00f6\u00b7nen", "Schwes\u00b7tern", "drey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "CARD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ich die Lieb anschaue,", "tokens": ["Wenn", "ich", "die", "Lieb", "an\u00b7schau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich sag die gr\u00f6\u00dft sie sey.", "tokens": ["Ich", "sag", "die", "gr\u00f6\u00dft", "sie", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "VVFIN", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}