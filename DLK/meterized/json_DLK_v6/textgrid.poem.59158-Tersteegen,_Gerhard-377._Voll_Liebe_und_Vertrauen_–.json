{"textgrid.poem.59158": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "377. Voll Liebe und Vertrauen \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sag, ob ich dich nicht liebe recht;", "tokens": ["Sag", ",", "ob", "ich", "dich", "nicht", "lie\u00b7be", "recht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du kannst es in der Krippe schauen!", "tokens": ["Du", "kannst", "es", "in", "der", "Krip\u00b7pe", "schau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es w\u00e4r' wohl ungeschickt und schlecht,", "tokens": ["Es", "w\u00e4r'", "wohl", "un\u00b7ge\u00b7schickt", "und", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn du mir ", "tokens": ["Wenn", "du", "mir"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PPER"], "meter": "+-+", "measure": "trochaic.di"}}}}}