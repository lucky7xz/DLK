{"dta.poem.8152": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "I .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Das Blut ist danach geflossen;", "tokens": ["Das", "Blut", "ist", "da\u00b7nach", "ge\u00b7flos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Du hast es gewollt, ich wei\u00df es wohl,", "tokens": ["Du", "hast", "es", "ge\u00b7wollt", ",", "ich", "wei\u00df", "es", "wohl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VMPP", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Weil einst mein Mund sie verschlossen.", "tokens": ["Weil", "einst", "mein", "Mund", "sie", "ver\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Entf\u00e4rben lie\u00df'st du dein braunes Haar", "tokens": ["Ent\u00b7f\u00e4r\u00b7ben", "lie\u00df'st", "du", "dein", "brau\u00b7nes", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In Sonnenbrand und Regen;", "tokens": ["In", "Son\u00b7nen\u00b7brand", "und", "Re\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du hast es gewollt, weil meine Hand", "tokens": ["Du", "hast", "es", "ge\u00b7wollt", ",", "weil", "mei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VMPP", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Liebkosend darauf gelegen.", "tokens": ["Lieb\u00b7ko\u00b7send", "da\u00b7rauf", "ge\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PAV", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Du stehst am Heerd in Flammen und Rauch,", "tokens": ["Du", "stehst", "am", "Heerd", "in", "Flam\u00b7men", "und", "Rauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da\u00df die feinen H\u00e4nde dir sprangen;", "tokens": ["Da\u00df", "die", "fei\u00b7nen", "H\u00e4n\u00b7de", "dir", "spran\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du hast es gewollt, ich wei\u00df es wohl,", "tokens": ["Du", "hast", "es", "ge\u00b7wollt", ",", "ich", "wei\u00df", "es", "wohl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VMPP", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Weil mein Auge daran gehangen.", "tokens": ["Weil", "mein", "Au\u00b7ge", "da\u00b7ran", "ge\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PAV", "VVPP", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}