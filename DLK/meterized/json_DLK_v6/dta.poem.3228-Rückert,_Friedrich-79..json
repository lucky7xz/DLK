{"dta.poem.3228": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "79.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Allgemeine zum Besondern zu gestalten,", "tokens": ["Das", "All\u00b7ge\u00b7mei\u00b7ne", "zum", "Be\u00b7son\u00b7dern", "zu", "ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Allgemeinen auch Besondres zu entfalten;", "tokens": ["Zum", "All\u00b7ge\u00b7mei\u00b7nen", "auch", "Be\u00b7sond\u00b7res", "zu", "ent\u00b7fal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das ist die Kunst, dein Ich weltg\u00fcltig auszupr\u00e4gen,", "tokens": ["Das", "ist", "die", "Kunst", ",", "dein", "Ich", "welt\u00b7g\u00fcl\u00b7tig", "aus\u00b7zu\u00b7pr\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PPOSAT", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und den Gehalt der Welt dir richtig zuzuw\u00e4gen.", "tokens": ["Und", "den", "Ge\u00b7halt", "der", "Welt", "dir", "rich\u00b7tig", "zu\u00b7zu\u00b7w\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "PPER", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}