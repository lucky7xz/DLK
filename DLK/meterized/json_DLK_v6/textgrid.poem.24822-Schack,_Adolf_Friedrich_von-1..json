{"textgrid.poem.24822": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer bist du aus dem Reich der Schatten,", "tokens": ["Wer", "bist", "du", "aus", "dem", "Reich", "der", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der mit mir wallt durch gr\u00fcne Matten", "tokens": ["Der", "mit", "mir", "wallt", "durch", "gr\u00fc\u00b7ne", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihre Bl\u00fcten welken hei\u00dft?", "tokens": ["Und", "ih\u00b7re", "Bl\u00fc\u00b7ten", "wel\u00b7ken", "hei\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der in dem Morgenglanz, dem roten,", "tokens": ["Der", "in", "dem", "Mor\u00b7gen\u00b7glanz", ",", "dem", "ro\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mich anstarrt mit dem Blick der Toten", "tokens": ["Mich", "an\u00b7starrt", "mit", "dem", "Blick", "der", "To\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und mit den Sternen mich umkreist?", "tokens": ["Und", "mit", "den", "Ster\u00b7nen", "mich", "um\u00b7kreist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im Lied, das teure Lippen singen,", "tokens": ["Im", "Lied", ",", "das", "teu\u00b7re", "Lip\u00b7pen", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "T\u00f6nt mir das Rauschen deiner Schwingen,", "tokens": ["T\u00f6nt", "mir", "das", "Rau\u00b7schen", "dei\u00b7ner", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Fl\u00fcstern h\u00f6r' ich f\u00fcr und f\u00fcr;", "tokens": ["Dein", "Fl\u00fcs\u00b7tern", "h\u00f6r'", "ich", "f\u00fcr", "und", "f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nachts legst du dich zu mir aufs Bette,", "tokens": ["Nachts", "legst", "du", "dich", "zu", "mir", "aufs", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und flieh' ich von der Lagerst\u00e4tte,", "tokens": ["Und", "flieh'", "ich", "von", "der", "La\u00b7ger\u00b7st\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So schleichst du mit mir durch die Th\u00fcr.", "tokens": ["So", "schleichst", "du", "mit", "mir", "durch", "die", "Th\u00fcr."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "APPR", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Im Wald auf menschenleeren Wegen,", "tokens": ["Im", "Wald", "auf", "men\u00b7schen\u00b7lee\u00b7ren", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verh\u00fcllter, trittst du mir entgegen", "tokens": ["Ver\u00b7h\u00fcll\u00b7ter", ",", "trittst", "du", "mir", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "PPER", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und schreckst mich von der Ruhebank;", "tokens": ["Und", "schreckst", "mich", "von", "der", "Ru\u00b7he\u00b7bank", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Freundekreis, beim Freudenmahle", "tokens": ["Im", "Freun\u00b7de\u00b7kreis", ",", "beim", "Freu\u00b7den\u00b7mah\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ziehst du vom Munde mir die Schale", "tokens": ["Ziehst", "du", "vom", "Mun\u00b7de", "mir", "die", "Scha\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und tropfst mir Wermut in den Trank.", "tokens": ["Und", "tropfst", "mir", "Wer\u00b7mut", "in", "den", "Trank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mit D\u00fcnsten, wie mit gift'gem Taue,", "tokens": ["Mit", "D\u00fcns\u00b7ten", ",", "wie", "mit", "gift'\u00b7gem", "Tau\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcllst du das Himmelsdach, das blaue,", "tokens": ["F\u00fcllst", "du", "das", "Him\u00b7mels\u00b7dach", ",", "das", "blau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du mir den Lenz mit Leichenduft.", "tokens": ["Du", "mir", "den", "Lenz", "mit", "Lei\u00b7chen\u00b7duft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wenn ich nun zum Grabe wanke,", "tokens": ["Und", "wenn", "ich", "nun", "zum", "Gra\u00b7be", "wan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sprich, finstrer Schatten, sprich, Gedanke,", "tokens": ["Sprich", ",", "finst\u00b7rer", "Schat\u00b7ten", ",", "sprich", ",", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVIMP", "$,", "ADJA", "NN", "$,", "ADJD", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie bann' ich dich von meiner Gruft?", "tokens": ["Wie", "bann'", "ich", "dich", "von", "mei\u00b7ner", "Gruft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}