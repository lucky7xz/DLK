{"textgrid.poem.37219": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Tafellied", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kr\u00e4nzt, edler Lust", "tokens": ["Kr\u00e4nzt", ",", "ed\u00b7ler", "Lust"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Gleich edel lieb zu kosen,", "tokens": ["Gleich", "e\u00b7del", "lieb", "zu", "ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kr\u00e4nzt Haar und Brust", "tokens": ["Kr\u00e4nzt", "Haar", "und", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Mit ihren j\u00fcngsten Rosen!", "tokens": ["Mit", "ih\u00b7ren", "j\u00fcng\u00b7sten", "Ro\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Menschlicher Fr\u00fchling, sch\u00f6n und kurz,", "tokens": ["Menschli\u00b7cher", "Fr\u00fch\u00b7ling", ",", "sch\u00f6n", "und", "kurz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Tropfet Genu\u00df nur auf Tage;", "tokens": ["Trop\u00b7fet", "Ge\u00b7nu\u00df", "nur", "auf", "Ta\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "APPR", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Fliehen nun dies' in zu raschem Sturz,", "tokens": ["Flie\u00b7hen", "nun", "dies'", "in", "zu", "ra\u00b7schem", "Sturz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Eitel ist dann auch die Klage.", "tokens": ["Ei\u00b7tel", "ist", "dann", "auch", "die", "Kla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dem frohen Kreis", "tokens": ["Dem", "fro\u00b7hen", "Kreis"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ruft Freude schon im schweben:", "tokens": ["Ruft", "Freu\u00b7de", "schon", "im", "schwe\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "APPRART", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Geneu\u00df! geneu\u00df!", "tokens": ["Ge\u00b7neu\u00df", "!", "ge\u00b7neu\u00df", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Kurz daur' ich wie das Leben.", "tokens": ["Kurz", "daur'", "ich", "wie", "das", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Aehnlich dem Freunde scheidet Lust,", "tokens": ["A\u00b7ehn\u00b7lich", "dem", "Freun\u00b7de", "schei\u00b7det", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der auf der Flucht uns begegnet,", "tokens": ["Der", "auf", "der", "Flucht", "uns", "be\u00b7geg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Herzlich uns dr\u00fccket an Mund und Brust", "tokens": ["Herz\u00b7lich", "uns", "dr\u00fc\u00b7cket", "an", "Mund", "und", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Und sich entrei\u00dfend noch segnet.", "tokens": ["Und", "sich", "ent\u00b7rei\u00b7\u00dfend", "noch", "seg\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVPP", "ADV", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Des h\u00f6chsten Rangs", "tokens": ["Des", "h\u00f6chs\u00b7ten", "Rangs"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Bist du, o Wein, hienieden.", "tokens": ["Bist", "du", ",", "o", "Wein", ",", "hien\u00b7ie\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "FM", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sei des Gesangs", "tokens": ["Sei", "des", "Ge\u00b7sangs"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Triumph auch dir beschieden!", "tokens": ["Tri\u00b7umph", "auch", "dir", "be\u00b7schie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Lastet des Lebens B\u00fcrde schwer", "tokens": ["Las\u00b7tet", "des", "Le\u00b7bens", "B\u00fcr\u00b7de", "schwer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und uns ergl\u00e4nzt nur die Traube,", "tokens": ["Und", "uns", "er\u00b7gl\u00e4nzt", "nur", "die", "Trau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Leichter sofort sind wir, nicht mehr", "tokens": ["Leich\u00b7ter", "so\u00b7fort", "sind", "wir", ",", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "VAFIN", "PPER", "$,", "PTKNEG", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Nagender Sorge zum Raube.", "tokens": ["Na\u00b7gen\u00b7der", "Sor\u00b7ge", "zum", "Rau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Begegn' also", "tokens": ["Be\u00b7gegn'", "al\u00b7so"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Becher seinem Becher,", "tokens": ["Der", "Be\u00b7cher", "sei\u00b7nem", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und trinke froh", "tokens": ["Und", "trin\u00b7ke", "froh"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und leer' ihn jeder Zecher!", "tokens": ["Und", "leer'", "ihn", "je\u00b7der", "Ze\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Jeglichem edlen Herzen Heil,", "tokens": ["Jeg\u00b7li\u00b7chem", "ed\u00b7len", "Her\u00b7zen", "Heil", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Das im beeiseten Norden,", "tokens": ["Das", "im", "be\u00b7ei\u00b7se\u00b7ten", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Warmes Gef\u00fchles, ein warmer Theil", "tokens": ["War\u00b7mes", "Ge\u00b7f\u00fch\u00b7les", ",", "ein", "war\u00b7mer", "Theil"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Unserer Kette geworden!", "tokens": ["Un\u00b7se\u00b7rer", "Ket\u00b7te", "ge\u00b7wor\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}