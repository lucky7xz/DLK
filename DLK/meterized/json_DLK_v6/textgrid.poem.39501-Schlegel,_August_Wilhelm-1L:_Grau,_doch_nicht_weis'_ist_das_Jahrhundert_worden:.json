{"textgrid.poem.39501": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Grau, doch nicht weis' ist das Jahrhundert worden:", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Grau, doch nicht weis' ist das Jahrhundert worden:", "tokens": ["Grau", ",", "doch", "nicht", "weis'", "ist", "das", "Jahr\u00b7hun\u00b7dert", "wor\u00b7den", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "PTKNEG", "ADJD", "VAFIN", "ART", "NN", "VAPP", "$."], "meter": "+--+---+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ihm ist umsonst die Weltgeschicht' erschollen.", "tokens": ["Ihm", "ist", "um\u00b7sonst", "die", "Welt\u00b7ge\u00b7schicht'", "er\u00b7schol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch th\u00fcrmen sich im Strom des Eises Schollen,", "tokens": ["Noch", "th\u00fcr\u00b7men", "sich", "im", "Strom", "des", "Ei\u00b7ses", "Schol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPRART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und heft'ger brausen Aeols wilde Horden.", "tokens": ["Und", "heft'\u00b7ger", "brau\u00b7sen", "A\u00b7e\u00b7ols", "wil\u00b7de", "Hor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wird blindlings hin und her stets Mavors morden?", "tokens": ["Wird", "blind\u00b7lings", "hin", "und", "her", "stets", "Ma\u00b7vors", "mor\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKVZ", "KON", "ADV", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird stets das Gl\u00fcck sein Rad zertr\u00fcmmernd rollen?", "tokens": ["Wird", "stets", "das", "Gl\u00fcck", "sein", "Rad", "zer\u00b7tr\u00fcm\u00b7mernd", "rol\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gilt freches Wollen blo\u00df, nie ernstes Sollen?", "tokens": ["Gilt", "fre\u00b7ches", "Wol\u00b7len", "blo\u00df", ",", "nie", "erns\u00b7tes", "Sol\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADV", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und einigt V\u00f6lker nur der Selbstsucht Orden?", "tokens": ["Und", "ei\u00b7nigt", "V\u00f6l\u00b7ker", "nur", "der", "Selbst\u00b7sucht", "Or\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Steigt niemals, die, wie jenes Greisen T\u00f6chter,", "tokens": ["Steigt", "nie\u00b7mals", ",", "die", ",", "wie", "je\u00b7nes", "Grei\u00b7sen", "T\u00f6ch\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PRELS", "$,", "PWAV", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verwegenheit und wilder Wahn zerfleischet,", "tokens": ["Ver\u00b7we\u00b7gen\u00b7heit", "und", "wil\u00b7der", "Wahn", "zer\u00b7flei\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verj\u00fcngt die Menschheit aus den Zauberke\u00dfeln?", "tokens": ["Ver\u00b7j\u00fcngt", "die", "Menschheit", "aus", "den", "Zau\u00b7ber\u00b7ke\u00b7\u00dfeln", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "So mag die Hoffnung, welche die Geschlechter", "tokens": ["So", "mag", "die", "Hoff\u00b7nung", ",", "wel\u00b7che", "die", "Ge\u00b7schlech\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Weissagungen goldner Zukunft t\u00e4uschet,", "tokens": ["Mit", "Weis\u00b7sa\u00b7gun\u00b7gen", "gold\u00b7ner", "Zu\u00b7kunft", "t\u00e4u\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu ew'ger Flucht Pandora's Urn' entfe\u00dfeln.", "tokens": ["Zu", "ew'\u00b7ger", "Flucht", "Pan\u00b7dora's", "Urn'", "ent\u00b7fe\u00b7\u00dfeln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NE", "NN", "VVINF", "$."], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}}}}}