{"textgrid.poem.55778": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Groteske", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das waren die grauen Gespenster,", "tokens": ["Das", "wa\u00b7ren", "die", "grau\u00b7en", "Ge\u00b7spens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die glitten in schweigender Nacht", "tokens": ["Die", "glit\u00b7ten", "in", "schwei\u00b7gen\u00b7der", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Durch leise klirrende Fenster", "tokens": ["Durch", "lei\u00b7se", "klir\u00b7ren\u00b7de", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und haben getobt und gelacht.", "tokens": ["Und", "ha\u00b7ben", "ge\u00b7tobt", "und", "ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "KON", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Sie wogten auf und nieder", "tokens": ["Sie", "wog\u00b7ten", "auf", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf gr\u00fcnlichem Mondenglast,", "tokens": ["Auf", "gr\u00fcn\u00b7li\u00b7chem", "Mon\u00b7den\u00b7glast", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie dehnten die farblosen Glieder", "tokens": ["Sie", "dehn\u00b7ten", "die", "farb\u00b7lo\u00b7sen", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und tanzten in fiebernder Hast.", "tokens": ["Und", "tanz\u00b7ten", "in", "fie\u00b7bern\u00b7der", "Hast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Mit eisigen Fingern durchkrallten", "tokens": ["Mit", "ei\u00b7si\u00b7gen", "Fin\u00b7gern", "durch\u00b7krall\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mein Fleisch sie bis auf's Bein,", "tokens": ["Mein", "Fleisch", "sie", "bis", "auf's", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und seltsame Worte lallten", "tokens": ["Und", "selt\u00b7sa\u00b7me", "Wor\u00b7te", "lall\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie gr\u00f6hlend und kichernd darein.", "tokens": ["Sie", "gr\u00f6h\u00b7lend", "und", "ki\u00b7chernd", "da\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Mein Leib in wilden Schauern", "tokens": ["Mein", "Leib", "in", "wil\u00b7den", "Schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu winden sich begann,", "tokens": ["Zu", "win\u00b7den", "sich", "be\u00b7gann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die grauen Gespenster sie lauern \u2013", "tokens": ["Die", "grau\u00b7en", "Ge\u00b7spens\u00b7ter", "sie", "lau\u00b7ern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und kreischend fl\u00fcchten sie dann.", "tokens": ["Und", "krei\u00b7schend", "fl\u00fcch\u00b7ten", "sie", "dann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}