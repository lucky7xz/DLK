{"textgrid.poem.46624": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[mit dem Himmel zanken]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit dem Himmel zanken", "tokens": ["Mit", "dem", "Him\u00b7mel", "zan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Kann ich freilich nicht,", "tokens": ["Kann", "ich", "frei\u00b7lich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Aber ihm zu danken", "tokens": ["A\u00b7ber", "ihm", "zu", "dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "F\u00fcr das Strafgericht,", "tokens": ["F\u00fcr", "das", "Straf\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Scheint mir eine schwere Pflicht,", "tokens": ["Scheint", "mir", "ei\u00b7ne", "schwe\u00b7re", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df von meinen Kranken", "tokens": ["Da\u00df", "von", "mei\u00b7nen", "Kran\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Zwei ins Grab nur, und nicht mehr noch sanken", "tokens": ["Zwei", "ins", "Grab", "nur", ",", "und", "nicht", "mehr", "noch", "san\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["CARD", "APPRART", "NN", "ADV", "$,", "KON", "PTKNEG", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn der Sturm den kranken", "tokens": ["Wenn", "der", "Sturm", "den", "kran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Baum nicht ganz zerbricht,", "tokens": ["Baum", "nicht", "ganz", "zer\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ihm noch ein'ge Ranken", "tokens": ["Ihm", "noch", "ein'\u00b7ge", "Ran\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "L\u00e4\u00dft sein Zorngewicht;", "tokens": ["L\u00e4\u00dft", "sein", "Zorn\u00b7ge\u00b7wicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ist es wohl des Baumes Pflicht,", "tokens": ["Ist", "es", "wohl", "des", "Bau\u00b7mes", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihm daf\u00fcr zu danken,", "tokens": ["Ihm", "da\u00b7f\u00fcr", "zu", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PAV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Da\u00df er ganz nicht \u00fcbertobt die Schranken?", "tokens": ["Da\u00df", "er", "ganz", "nicht", "\u00fc\u00b7ber\u00b7tobt", "die", "Schran\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}