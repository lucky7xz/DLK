{"dta.poem.21508": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Ade!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Ade! Du schreitest zum Altare,", "tokens": ["A\u00b7de", "!", "Du", "schrei\u00b7test", "zum", "Al\u00b7ta\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Zu schlie\u00dfen froh das frohe Band,", "tokens": ["Zu", "schlie\u00b7\u00dfen", "froh", "das", "fro\u00b7he", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ich, vertraut dir manche Jahre,", "tokens": ["Und", "ich", ",", "ver\u00b7traut", "dir", "man\u00b7che", "Jah\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seh\u2019 stumm sich f\u00fcgen Hand in Hand", "tokens": ["Seh'", "stumm", "sich", "f\u00fc\u00b7gen", "Hand", "in", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PRF", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus meinen Lippen weicht das Blut,", "tokens": ["Aus", "mei\u00b7nen", "Lip\u00b7pen", "weicht", "das", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Herzen zuckt empor das Weh, \u2014", "tokens": ["Im", "Her\u00b7zen", "zuckt", "em\u00b7por", "das", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKVZ", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sei still da drin \u2026 Es ist so gut \u2014", "tokens": ["Sei", "still", "da", "drin", "\u2026", "Es", "ist", "so", "gut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ADV", "ADV", "$(", "PPER", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ade!", "tokens": ["A\u00b7de", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Es ist so gut. Ob auch mein Streben", "tokens": ["Es", "ist", "so", "gut", ".", "Ob", "auch", "mein", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$.", "KOUS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich nur um deinen Beifall hob,", "tokens": ["Sich", "nur", "um", "dei\u00b7nen", "Bei\u00b7fall", "hob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob, was die Muse eingegeben,", "tokens": ["Ob", ",", "was", "die", "Mu\u00b7se", "ein\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr ", "tokens": ["F\u00fcr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Das Leben braucht der festen Hand,", "tokens": ["Das", "Le\u00b7ben", "braucht", "der", "fes\u00b7ten", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Weg, den ich, der Tr\u00e4umer geh\u2019,", "tokens": ["Der", "Weg", ",", "den", "ich", ",", "der", "Tr\u00e4u\u00b7mer", "geh'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Tr\u00e4gt Unkraut nur und Flittertand, \u2014", "tokens": ["Tr\u00e4gt", "Un\u00b7kraut", "nur", "und", "Flit\u00b7ter\u00b7tand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "NN", "ADV", "KON", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ade!", "tokens": ["A\u00b7de", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Umdunkelt ist mein Weg. Doch deinen", "tokens": ["Um\u00b7dun\u00b7kelt", "ist", "mein", "Weg", ".", "Doch", "dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$.", "KON", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umflie\u00dfe hell der Sonne Licht:", "tokens": ["Um\u00b7flie\u00b7\u00dfe", "hell", "der", "Son\u00b7ne", "Licht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und keine Stunde soll erscheinen,", "tokens": ["Und", "kei\u00b7ne", "Stun\u00b7de", "soll", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da dir das Wort, die Hoffnung bricht.", "tokens": ["Da", "dir", "das", "Wort", ",", "die", "Hoff\u00b7nung", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Eintracht kr\u00f6ne deinen Bund,", "tokens": ["Die", "Ein\u00b7tracht", "kr\u00f6\u00b7ne", "dei\u00b7nen", "Bund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ich, der still im Schatten steh\u2019,", "tokens": ["Und", "ich", ",", "der", "still", "im", "Schat\u00b7ten", "steh'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich seg\u2019ne dich mit zitterndem Mund \u2026", "tokens": ["Ich", "seg'\u00b7ne", "dich", "mit", "zit\u00b7tern\u00b7dem", "Mund", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Ade!", "tokens": ["A\u00b7de", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}