{"textgrid.poem.51080": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "14", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wieder schl\u00e4gt's \u2013 ein Viertel erst und zw\u00f6lfe!", "tokens": ["Und", "wie\u00b7der", "schl\u00e4gt's", "\u2013", "ein", "Vier\u00b7tel", "erst", "und", "zw\u00f6l\u00b7fe", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$(", "ART", "NN", "ADV", "KON", "CARD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Viertelst\u00fcndchen erst, da\u00df Gott mir helfe,", "tokens": ["Ein", "Vier\u00b7tel\u00b7st\u00fcnd\u00b7chen", "erst", ",", "da\u00df", "Gott", "mir", "hel\u00b7fe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verging, seit ich mich wieder regen kann!", "tokens": ["Ver\u00b7ging", ",", "seit", "ich", "mich", "wie\u00b7der", "re\u00b7gen", "kann", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "ADJA", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich tr\u00e4umte, da\u00df schon mancher Tag verrann!", "tokens": ["Ich", "tr\u00e4um\u00b7te", ",", "da\u00df", "schon", "man\u00b7cher", "Tag", "ver\u00b7rann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch bin ich frei, das Weh hat sich gewendet.", "tokens": ["Doch", "bin", "ich", "frei", ",", "das", "Weh", "hat", "sich", "ge\u00b7wen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "ART", "NN", "VAFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der seine Strahlen durch das Weltall sendet,", "tokens": ["Der", "sei\u00b7ne", "Strah\u00b7len", "durch", "das", "Wel\u00b7tall", "sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er l\u00f6st auch Zeit und Raum in diesem Schrein \u2013", "tokens": ["Er", "l\u00f6st", "auch", "Zeit", "und", "Raum", "in", "die\u00b7sem", "Schrein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "NN", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich bin allein und dennoch nicht allein!", "tokens": ["Ich", "bin", "al\u00b7lein", "und", "den\u00b7noch", "nicht", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KON", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Getrennt bin ich von meinem herben Leiden,", "tokens": ["Ge\u00b7trennt", "bin", "ich", "von", "mei\u00b7nem", "her\u00b7ben", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wie ein Meer, von dem ich mich will scheiden,", "tokens": ["Und", "wie", "ein", "Meer", ",", "von", "dem", "ich", "mich", "will", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df brausen ich mein siedendhei\u00dfes Blut", "tokens": ["La\u00df", "brau\u00b7sen", "ich", "mein", "sie\u00b7dend\u00b7hei\u00b7\u00dfes", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und steh am Ufer als ein Mann von Mut.", "tokens": ["Und", "steh", "am", "U\u00b7fer", "als", "ein", "Mann", "von", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "KOKOM", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So toset nur, ihr ungetreuen Wogen,", "tokens": ["So", "to\u00b7set", "nur", ",", "ihr", "un\u00b7ge\u00b7treu\u00b7en", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lange genug bin ich mit euch gezogen!", "tokens": ["Lan\u00b7ge", "ge\u00b7nug", "bin", "ich", "mit", "euch", "ge\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Ich \u00fcbersing euch, wie ein Ferg am Strand,", "tokens": ["Ich", "\u00fc\u00b7ber\u00b7sing", "euch", ",", "wie", "ein", "Ferg", "am", "Strand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und tausch euch an ein gutes Heimatland!", "tokens": ["Und", "tausch", "euch", "an", "ein", "gu\u00b7tes", "Hei\u00b7mat\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Schon seh ich schimmernd flie\u00dfen Zeit in Zeiten,", "tokens": ["Schon", "seh", "ich", "schim\u00b7mernd", "flie\u00b7\u00dfen", "Zeit", "in", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verlieren sich in unbegrenzte Weiten", "tokens": ["Ver\u00b7lie\u00b7ren", "sich", "in", "un\u00b7be\u00b7grenz\u00b7te", "Wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gefilde, Bergesh\u00f6hen, Wolkenflug:", "tokens": ["Ge\u00b7fil\u00b7de", ",", "Ber\u00b7ges\u00b7h\u00f6\u00b7hen", ",", "Wol\u00b7ken\u00b7flug", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Ewigkeit in ", "tokens": ["Die", "E\u00b7wig\u00b7keit", "in"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Der letzte Hauch ein wallend Meer von Leben,", "tokens": ["Der", "letz\u00b7te", "Hauch", "ein", "wal\u00b7lend", "Meer", "von", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo fliehend die Gedanken mir entschweben!", "tokens": ["Wo", "flie\u00b7hend", "die", "Ge\u00b7dan\u00b7ken", "mir", "ent\u00b7schwe\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fahr hin, o Selbst! verg\u00e4ngliches Idol,", "tokens": ["Fahr", "hin", ",", "o", "Selbst", "!", "ver\u00b7g\u00e4ng\u00b7li\u00b7ches", "I\u00b7dol", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "FM", "FM", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wer du auch bist, leb wohl du, fahre wohl!", "tokens": ["Wer", "du", "auch", "bist", ",", "leb", "wohl", "du", ",", "fah\u00b7re", "wohl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VAFIN", "$,", "VVFIN", "ADV", "PPER", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}