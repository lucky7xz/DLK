{"dta.poem.4068": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "So der Gewalt'gen gedenk und des herzerhebenden", "tokens": ["So", "der", "Ge\u00b7walt'\u00b7gen", "ge\u00b7denk", "und", "des", "her\u00b7zer\u00b7he\u00b7ben\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "KON", "ART", "NN"], "meter": "---+--+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schicksals,", "tokens": ["Schick\u00b7sals", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Thatlos selber und leicht, aber vom Aether doch", "tokens": ["That\u00b7los", "sel\u00b7ber", "und", "leicht", ",", "a\u00b7ber", "vom", "A\u00b7e\u00b7ther", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "KON", "ADJD", "$,", "ADV", "APPRART", "NN", "ADV"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "auch", "tokens": ["auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Angeschauet und fromm, wie die Alten, die g\u00f6tt-", "tokens": ["An\u00b7ge\u00b7schau\u00b7et", "und", "fromm", ",", "wie", "die", "Al\u00b7ten", ",", "die", "g\u00f6t\u00b7t"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "KON", "ADJD", "$,", "PWAV", "ART", "NN", "$,", "ART", "TRUNC"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "licherzognen", "tokens": ["li\u00b7cher\u00b7zog\u00b7nen"], "token_info": ["word"], "pos": ["ADJA"], "meter": "--+-", "measure": "anapaest.init"}, "line.7": {"text": "Freudigen Dichter, ziehn freudig das Land wir", "tokens": ["Freu\u00b7di\u00b7gen", "Dich\u00b7ter", ",", "ziehn", "freu\u00b7dig", "das", "Land", "wir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADJD", "ART", "NN", "PPER"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "hinauf.", "tokens": ["hin\u00b7auf", "."], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Gro\u00df ist das Werden umher. Dort von den \u00e4u-", "tokens": ["Gro\u00df", "ist", "das", "Wer\u00b7den", "um\u00b7her", ".", "Dort", "von", "den", "\u00e4u"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "PTKVZ", "$.", "ADV", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "\u00dfersten Bergen", "tokens": ["\u00dfers\u00b7ten", "Ber\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Stammen der J\u00fcnglinge viel, steigen die H\u00fcgel", "tokens": ["Stam\u00b7men", "der", "J\u00fcng\u00b7lin\u00b7ge", "viel", ",", "stei\u00b7gen", "die", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "$,", "VVFIN", "ART", "NN"], "meter": "+--++--+--+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "herab.", "tokens": ["her\u00b7ab", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Quellen rauschen von dort und hundert gesch\u00e4ftige", "tokens": ["Quel\u00b7len", "rau\u00b7schen", "von", "dort", "und", "hun\u00b7dert", "ge\u00b7sch\u00e4f\u00b7ti\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADV", "KON", "CARD", "ADJA"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "B\u00e4che,", "tokens": ["B\u00e4\u00b7che", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Kommen bei Tag und bei Nacht nieder und", "tokens": ["Kom\u00b7men", "bei", "Tag", "und", "bei", "Nacht", "nie\u00b7der", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "APPR", "NN", "PTKVZ", "KON"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.16": {"text": "bauen das Land.", "tokens": ["bau\u00b7en", "das", "Land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Aber der Meister pfl\u00fcgt in der Mitte des Landes", "tokens": ["A\u00b7ber", "der", "Meis\u00b7ter", "pfl\u00fcgt", "in", "der", "Mit\u00b7te", "des", "Lan\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.18": {"text": "die Furchen", "tokens": ["die", "Fur\u00b7chen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Ziehet der Neckarstrom, ziehet der Segen herab.", "tokens": ["Zie\u00b7het", "der", "Ne\u00b7ckars\u00b7trom", ",", "zie\u00b7het", "der", "Se\u00b7gen", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.20": {"text": "Und es kommen mit ihm Italiens L\u00fcfte, die See", "tokens": ["Und", "es", "kom\u00b7men", "mit", "ihm", "I\u00b7ta\u00b7li\u00b7ens", "L\u00fcf\u00b7te", ",", "die", "See"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "NE", "NN", "$,", "ART", "NN"], "meter": "+-+-+--+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "schickt", "tokens": ["schickt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.22": {"text": "Ihre Wolken, sie schickt pr\u00e4chtige Sonnen mit ihm;", "tokens": ["Ih\u00b7re", "Wol\u00b7ken", ",", "sie", "schickt", "pr\u00e4ch\u00b7ti\u00b7ge", "Son\u00b7nen", "mit", "ihm", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "+-+--++--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Darum w\u00e4chset uns auch fast \u00fcber das Haupt die", "tokens": ["Da\u00b7rum", "w\u00e4ch\u00b7set", "uns", "auch", "fast", "\u00fc\u00b7ber", "das", "Haupt", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "ART"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "gewalt'ge", "tokens": ["ge\u00b7walt'\u00b7ge"], "token_info": ["word"], "pos": ["ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "F\u00fclle, denn hieher ward hier in die Ebne das", "tokens": ["F\u00fcl\u00b7le", ",", "denn", "hie\u00b7her", "ward", "hier", "in", "die", "Eb\u00b7ne", "das"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "PAV", "VAFIN", "ADV", "APPR", "ART", "NN", "ART"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.26": {"text": "Gute.", "tokens": ["Gu\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "Reicher den Lieben gebracht, den Landsleuten,", "tokens": ["Rei\u00b7cher", "den", "Lie\u00b7ben", "ge\u00b7bracht", ",", "den", "Lands\u00b7leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "VVPP", "$,", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.28": {"text": "doch neidet", "tokens": ["doch", "nei\u00b7det"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.29": {"text": "Keiner an Bergen dort ihnen die G\u00e4rten, den Wein,", "tokens": ["Kei\u00b7ner", "an", "Ber\u00b7gen", "dort", "ih\u00b7nen", "die", "G\u00e4r\u00b7ten", ",", "den", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "ADV", "PPER", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.30": {"text": "Oder das \u00fcppige Gras und das Korn und die", "tokens": ["O\u00b7der", "das", "\u00fcp\u00b7pi\u00b7ge", "Gras", "und", "das", "Korn", "und", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "KON", "ART", "NN", "KON", "ART"], "meter": "---+--+--+-+", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "gl\u00fchenden B\u00e4ume,", "tokens": ["gl\u00fc\u00b7hen\u00b7den", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.32": {"text": "Die am Wege gereiht \u00fcber den Wanderern stehn.", "tokens": ["Die", "am", "We\u00b7ge", "ge\u00b7reiht", "\u00fc\u00b7ber", "den", "Wan\u00b7de\u00b7rern", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVPP", "APPR", "ART", "NN", "VVINF", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}