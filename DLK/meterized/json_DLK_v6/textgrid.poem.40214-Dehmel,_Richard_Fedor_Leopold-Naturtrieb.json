{"textgrid.poem.40214": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Naturtrieb", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Lerche singt, der Rabe schreit \u2013", "tokens": ["Die", "Ler\u00b7che", "singt", ",", "der", "Ra\u00b7be", "schreit", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist nun so seit Ewigkeit;", "tokens": ["Das", "ist", "nun", "so", "seit", "E\u00b7wig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "s\u00e4nge er ", "tokens": ["s\u00e4n\u00b7ge", "er"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+--", "measure": "dactylic.init"}, "line.4": {"text": "und dennoch kann er 's Schrein nicht lassen.", "tokens": ["und", "den\u00b7noch", "kann", "er", "'s", "Schrein", "nicht", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PPER", "NN", "PTKNEG", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}