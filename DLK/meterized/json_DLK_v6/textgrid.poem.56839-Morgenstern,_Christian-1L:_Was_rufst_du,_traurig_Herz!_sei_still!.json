{"textgrid.poem.56839": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was rufst du, traurig Herz! sei still!", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was rufst du, traurig Herz! sei still!", "tokens": ["Was", "rufst", "du", ",", "trau\u00b7rig", "Herz", "!", "sei", "still", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJD", "NN", "$.", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es kann nicht sein \u2013", "tokens": ["Es", "kann", "nicht", "sein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VAINF", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "ergieb dich drein.", "tokens": ["er\u00b7gieb", "dich", "drein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Es kann nicht alles also sein,", "tokens": ["Es", "kann", "nicht", "al\u00b7les", "al\u00b7so", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PIS", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "wie deine Sehnsucht will.", "tokens": ["wie", "dei\u00b7ne", "Sehn\u00b7sucht", "will", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nimm Abschied, Herz, von deinem Traum,", "tokens": ["Nimm", "Ab\u00b7schied", ",", "Herz", ",", "von", "dei\u00b7nem", "Traum", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "er war zu sch\u00f6n.", "tokens": ["er", "war", "zu", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Von lichten H\u00f6hn", "tokens": ["Von", "lich\u00b7ten", "H\u00f6hn"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "wieder hinab", "tokens": ["wie\u00b7der", "hin\u00b7ab"], "token_info": ["word", "word"], "pos": ["ADV", "PTKVZ"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "ins einsame Grab!", "tokens": ["ins", "ein\u00b7sa\u00b7me", "Grab", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Schau, dort fliegt's,", "tokens": ["Schau", ",", "dort", "fliegt's", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "was du getr\u00e4umt ...", "tokens": ["was", "du", "ge\u00b7tr\u00e4umt", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Die Welle wiegts", "tokens": ["Die", "Wel\u00b7le", "wiegts"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "hinab zu Tal ... \u2013", "tokens": ["hin\u00b7ab", "zu", "Tal", "...", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "NN", "$(", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Zersch\u00e4umt, zersch\u00e4umt!", "tokens": ["Zer\u00b7sch\u00e4umt", ",", "zer\u00b7sch\u00e4umt", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Es ", "tokens": ["Es"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.12": {"text": "O Dunst und Schaum!", "tokens": ["O", "Dunst", "und", "Schaum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Nimm Abschied, Herz, von deinem Traum,", "tokens": ["Nimm", "Ab\u00b7schied", ",", "Herz", ",", "von", "dei\u00b7nem", "Traum", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "er war zu sch\u00f6n.", "tokens": ["er", "war", "zu", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Weine, mein Herz, soviel du magst,", "tokens": ["Wei\u00b7ne", ",", "mein", "Herz", ",", "so\u00b7viel", "du", "magst", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VMFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "klag und wein!", "tokens": ["klag", "und", "wein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Es wird dein letztes Weinen sein", "tokens": ["Es", "wird", "dein", "letz\u00b7tes", "Wei\u00b7nen", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "auf lang.", "tokens": ["auf", "lang", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Ich wei\u00df, da\u00df du nicht f\u00fcrder klagst,", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "du", "nicht", "f\u00fcr\u00b7der", "klagst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "wenn dieser Schmerz sich niederzwang.", "tokens": ["wenn", "die\u00b7ser", "Schmerz", "sich", "nie\u00b7der\u00b7zwang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann wirst du hart", "tokens": ["Dann", "wirst", "du", "hart"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "und schweigst erstarrt ...", "tokens": ["und", "schweigst", "er\u00b7starrt", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Weine, mein Herz! klag und wein!", "tokens": ["Wei\u00b7ne", ",", "mein", "Herz", "!", "klag", "und", "wein", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "VVFIN", "KON", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.10": {"text": "Es wird dein letztes Weinen sein", "tokens": ["Es", "wird", "dein", "letz\u00b7tes", "Wei\u00b7nen", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "auf lang.", "tokens": ["auf", "lang", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}}}}