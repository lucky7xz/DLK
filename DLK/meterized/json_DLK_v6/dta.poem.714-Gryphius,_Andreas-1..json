{"dta.poem.714": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Jn dem das Jahr in nichts verschwindet: ", "tokens": ["In", "dem", "das", "Jahr", "in", "nichts", "ver\u00b7schwin\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd eine newe zeit sich findet:", "tokens": ["Vnd", "ei\u00b7ne", "ne\u00b7we", "zeit", "sich", "fin\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "In dem die letzte Nacht vergehet ", "tokens": ["In", "dem", "die", "letz\u00b7te", "Nacht", "ver\u00b7ge\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd gleich al\u00df au\u00df der grufft entstehet.", "tokens": ["Vnd", "gleich", "al\u00df", "au\u00df", "der", "grufft", "ent\u00b7ste\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df vn\u00df mein Geist/ dessen Allmacht: La\u00df vn\u00df diesen K\u00f6nig", "tokens": ["La\u00df", "vn\u00df", "mein", "Geist", "/", "des\u00b7sen", "All\u00b7macht", ":", "La\u00df", "vn\u00df", "die\u00b7sen", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$(", "PRELAT", "NN", "$.", "VVIMP", "APPR", "PDAT", "NN"], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.6": {"text": "ehren.", "tokens": ["eh\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Dessen Crone/ Thron vnd wesen/ wird mit keiner zeit auff-", "tokens": ["Des\u00b7sen", "Cro\u00b7ne", "/", "Thron", "vnd", "we\u00b7sen", "/", "wird", "mit", "kei\u00b7ner", "zeit", "auf\u00b7f"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "$(", "NN", "KON", "VVINF", "$(", "VAFIN", "APPR", "PIAT", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "h\u00f6ren.", "tokens": ["h\u00f6\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Wir/ die wir eine weile bl\u00fchen.", "tokens": ["Wir", "/", "die", "wir", "ei\u00b7ne", "wei\u00b7le", "bl\u00fc\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd mit der zeit von hinnen ziehen.", "tokens": ["Vnd", "mit", "der", "zeit", "von", "hin\u00b7nen", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wir werden mit der zeit in Erden", "tokens": ["Wir", "wer\u00b7den", "mit", "der", "zeit", "in", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd leichten Staub verkehret werden/", "tokens": ["Vnd", "leich\u00b7ten", "Staub", "ver\u00b7keh\u00b7ret", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der vor Ewigkeit geherrschet: vnd in ", "tokens": ["Der", "vor", "E\u00b7wig\u00b7keit", "ge\u00b7herr\u00b7schet", ":", "vnd", "in"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "NN", "VVFIN", "$.", "KON", "APPR"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Heist vns Menschen wieder kommen: Doch jhn selbst mag ", "tokens": ["Heist", "vns", "Men\u00b7schen", "wie\u00b7der", "kom\u00b7men", ":", "Doch", "jhn", "selbst", "mag"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "NN", "ADV", "VVINF", "$.", "KON", "PPER", "ADV", "VMFIN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "nichts vertreiben.", "tokens": ["nichts", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Er hat vn\u00df Ma\u00df vnd Ziel gesetzet:", "tokens": ["Er", "hat", "vn\u00df", "Ma\u00df", "vnd", "Ziel", "ge\u00b7set\u00b7zet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In dem die Welt vn\u00df jhre sch\u00e4tzet:", "tokens": ["In", "dem", "die", "Welt", "vn\u00df", "jhre", "sch\u00e4t\u00b7zet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So bald wir arme di\u00df erreichen ", "tokens": ["So", "bald", "wir", "ar\u00b7me", "di\u00df", "er\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "PDS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df dieser Wangen Ros\u2019 erbleichen", "tokens": ["Mu\u00df", "die\u00b7ser", "Wan\u00b7gen", "Ros'", "er\u00b7blei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PDAT", "NN", "NE", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vnter dessen l\u00e4st er st\u00fcndlich/ seinen jmmerreichen ", "tokens": ["Vn\u00b7ter", "des\u00b7sen", "l\u00e4st", "er", "st\u00fcnd\u00b7lich", "/", "sei\u00b7nen", "jm\u00b7mer\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDS", "VVFIN", "PPER", "ADJD", "$(", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.4": {"line.1": {"text": "Vber di\u00df was Athem holet/ fallen al\u00df geh\u00e4ufften ", "tokens": ["Vber", "di\u00df", "was", "A\u00b7them", "ho\u00b7let", "/", "fal\u00b7len", "al\u00df", "ge\u00b7h\u00e4uff\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDS", "PRELS", "NN", "VVFIN", "$(", "VVINF", "KOKOM", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So bald die Morgen r\u00f6tte lachet:", "tokens": ["So", "bald", "die", "Mor\u00b7gen", "r\u00f6t\u00b7te", "la\u00b7chet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So bald die g\u00fcld'ne Sonn' erwachet/ ", "tokens": ["So", "bald", "die", "g\u00fcld'\u00b7ne", "Sonn'", "er\u00b7wa\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So offt der heisse Mittag schmachtet/ ", "tokens": ["So", "offt", "der", "heis\u00b7se", "Mit\u00b7tag", "schmach\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So offt die Nacht den Tag nicht achtet: ", "tokens": ["So", "offt", "die", "Nacht", "den", "Tag", "nicht", "ach\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mu\u00df sich/ was nur ist verwundern/ \u00fcber s", "tokens": ["Mu\u00df", "sich", "/", "was", "nur", "ist", "ver\u00b7wun\u00b7dern", "/", "\u00fc\u00b7ber", "s"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PRF", "$(", "PWS", "ADV", "VAFIN", "VVINF", "$(", "APPR", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Die ob allem was hier lebet/ jeden Augenblick wird newe,", "tokens": ["Die", "ob", "al\u00b7lem", "was", "hier", "le\u00b7bet", "/", "je\u00b7den", "Au\u00b7gen\u00b7blick", "wird", "ne\u00b7we", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "PIS", "PWS", "ADV", "VVFIN", "$(", "PIAT", "NN", "VAFIN", "NE", "$,"], "meter": "--+-++--+-+--+-+", "measure": "iambic.septa.relaxed"}}, "stanza.6": {"line.1": {"text": "So bald wir dieses Licht gek\u00fcsset.", "tokens": ["So", "bald", "wir", "die\u00b7ses", "Licht", "ge\u00b7k\u00fcs\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd die geschm\u00fcckte welt gegr\u00fcsset;", "tokens": ["Vnd", "die", "ge\u00b7schm\u00fcck\u00b7te", "welt", "ge\u00b7gr\u00fcs\u00b7set", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn wir die zarte Zungen zwingen", "tokens": ["Wenn", "wir", "die", "zar\u00b7te", "Zun\u00b7gen", "zwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von seiner wunder ruhm zu singen", "tokens": ["Von", "sei\u00b7ner", "wun\u00b7der", "ruhm", "zu", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn wir leben/ wenn wir alten/ wenn die greissen Haar ", "tokens": ["Wenn", "wir", "le\u00b7ben", "/", "wenn", "wir", "al\u00b7ten", "/", "wenn", "die", "greis\u00b7sen", "Haar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVINF", "$(", "KOUS", "PPER", "ADJA", "$(", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "vns f\u00e4rben/", "tokens": ["vns", "f\u00e4r\u00b7ben", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Steht vn\u00df seine gunst vor augen/ die (sterb alles!) nicht kan", "tokens": ["Steht", "vn\u00df", "sei\u00b7ne", "gunst", "vor", "au\u00b7gen", "/", "die", "(", "sterb", "al\u00b7les", "!", ")", "nicht", "kan"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "APPR", "NN", "$(", "ART", "$(", "VVFIN", "PIS", "$.", "$(", "PTKNEG", "VMFIN"], "meter": "+-+-+-+--++--+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "sterben.", "tokens": ["ster\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.7": {"line.1": {"text": "Wenn der Schnee die Felder kleidet/", "tokens": ["Wenn", "der", "Schnee", "die", "Fel\u00b7der", "klei\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der s\u00fcsse ", "tokens": ["Wenn", "der", "s\u00fcs\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Wenn die heissen ", "tokens": ["Wenn", "die", "heis\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Wenn ein Baum l\u00e4st frucht erkennen.", "tokens": ["Wenn", "ein", "Baum", "l\u00e4st", "frucht", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kennet man die frucht der Liebe: die vn\u00df f\u00fcr vnd f\u00fcr vmb-", "tokens": ["Ken\u00b7net", "man", "die", "frucht", "der", "Lie\u00b7be", ":", "die", "vn\u00df", "f\u00fcr", "vnd", "f\u00fcr", "vm\u00b7b"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ART", "NN", "ART", "NN", "$.", "ART", "NN", "APPR", "KON", "APPR", "TRUNC"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "gibet/", "tokens": ["gi\u00b7bet", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Die vn\u00df auff h\u00e4lt wenn wir straucheln: vnd erquickt wenn", "tokens": ["Die", "vn\u00df", "auff", "h\u00e4lt", "wenn", "wir", "strau\u00b7cheln", ":", "vnd", "er\u00b7quickt", "wenn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "VVFIN", "KOUS", "PPER", "VVFIN", "$.", "KON", "VVFIN", "KOUS"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "wir betr\u00fcbet.", "tokens": ["wir", "be\u00b7tr\u00fc\u00b7bet", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Wenn wir vn\u00df in wehmutt stecken: ", "tokens": ["Wenn", "wir", "vn\u00df", "in", "weh\u00b7mutt", "ste\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn vn\u00df Angst vnd ach erschrecken:", "tokens": ["Wenn", "vn\u00df", "Angst", "vnd", "ach", "er\u00b7schre\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "XY", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn vn\u00df ", "tokens": ["Wenn", "vn\u00df"], "token_info": ["word", "word"], "pos": ["KOUS", "NN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Wenn wir auff der gruben stehen.", "tokens": ["Wenn", "wir", "auff", "der", "gru\u00b7ben", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn wir diesen Geist hingeben/ vnd di\u00df m\u00fcde ", "tokens": ["Wenn", "wir", "die\u00b7sen", "Geist", "hin\u00b7ge\u00b7ben", "/", "vnd", "di\u00df", "m\u00fc\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVINF", "$(", "KON", "PDS", "ADJD"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "segnen:", "tokens": ["seg\u00b7nen", ":"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Pflegt mit offnen Hertz vnd Armen/ Er al\u00df Vater zu bege-", "tokens": ["Pflegt", "mit", "off\u00b7nen", "Hertz", "vnd", "Ar\u00b7men", "/", "Er", "al\u00df", "Va\u00b7ter", "zu", "be\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "KON", "NN", "$(", "PPER", "KOUS", "NN", "APPR", "TRUNC"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "gnen.", "tokens": ["gnen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-", "measure": "single.down"}}, "stanza.9": {"line.1": {"text": "Last die schnellen Jahre flihen", "tokens": ["Last", "die", "schnel\u00b7len", "Jah\u00b7re", "fli\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wenn wir au\u00df dem leben zihen", "tokens": ["Wenn", "wir", "au\u00df", "dem", "le\u00b7ben", "zi\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "p