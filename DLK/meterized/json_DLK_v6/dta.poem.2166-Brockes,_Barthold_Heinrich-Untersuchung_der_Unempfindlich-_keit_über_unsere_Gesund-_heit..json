{"dta.poem.2166": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Untersuchung der Unempfindlich-  \n keit \u00fcber unsere Gesund-  \n heit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn unsers Blutes reger Lauf in seinem richtgen", "tokens": ["Wenn", "un\u00b7sers", "Blu\u00b7tes", "re\u00b7ger", "Lauf", "in", "sei\u00b7nem", "richt\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zirkel gehet,", "tokens": ["Zir\u00b7kel", "ge\u00b7het", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Wenn sonder \u00e4u\u00dferlichen Schmerz es wohl um die Gesund-", "tokens": ["Wenn", "son\u00b7der", "\u00e4u\u00b7\u00dfer\u00b7li\u00b7chen", "Schmerz", "es", "wohl", "um", "die", "Ge\u00b7sun\u00b7d"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJA", "NN", "PPER", "ADV", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "heit stehet,", "tokens": ["heit", "ste\u00b7het", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "So scheints, ob f\u00fchle man dar\u00fcber kein recht, kein wirk-", "tokens": ["So", "scheints", ",", "ob", "f\u00fch\u00b7le", "man", "da\u00b7r\u00fc\u00b7ber", "kein", "recht", ",", "kein", "wirk"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "VVFIN", "PIS", "PAV", "PIAT", "ADJD", "$,", "PIAT", "TRUNC"], "meter": "-+-+---+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "liches Vergn\u00fcgen.", "tokens": ["li\u00b7ches", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Allein, die\u00df k\u00f6mmt nur blo\u00df daher, weil etwan andre", "tokens": ["Al\u00b7lein", ",", "die\u00df", "k\u00f6mmt", "nur", "blo\u00df", "da\u00b7her", ",", "weil", "et\u00b7wan", "and\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PDS", "VVFIN", "ADV", "ADV", "PAV", "$,", "KOUS", "ADV", "PIS"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Lust uns r\u00fchrt,", "tokens": ["Lust", "uns", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Wie oder da\u00df sich andre Vorw\u00fcrf\u2019 zu unseren Gedanken", "tokens": ["Wie", "o\u00b7der", "da\u00df", "sich", "and\u00b7re", "Vor\u00b7w\u00fcr\u00b7f'", "zu", "un\u00b7se\u00b7ren", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "KON", "KOUS", "PRF", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.10": {"text": "f\u00fcgen.", "tokens": ["f\u00fc\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Es ist gewi\u00df, da\u00df man im Denken: man sey gesund,", "tokens": ["Es", "ist", "ge\u00b7wi\u00df", ",", "da\u00df", "man", "im", "Den\u00b7ken", ":", "man", "sey", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KOUS", "PIS", "APPRART", "NN", "$.", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Vergn\u00fcgen sp\u00fcrt,", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", "sp\u00fcrt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Ob solche Lust gleich nicht so lebhaft, als wie die strenge", "tokens": ["Ob", "sol\u00b7che", "Lust", "gleich", "nicht", "so", "leb\u00b7haft", ",", "als", "wie", "die", "stren\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "ADV", "PTKNEG", "ADV", "ADJD", "$,", "KOUS", "KOKOM", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Lust der Liebe,", "tokens": ["Lust", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Wovon die aufgebrachten Triebe", "tokens": ["Wo\u00b7von", "die", "auf\u00b7ge\u00b7brach\u00b7ten", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Sich einer Art von Anfall gleichen. Wann aber der", "tokens": ["Sich", "ei\u00b7ner", "Art", "von", "An\u00b7fall", "glei\u00b7chen", ".", "Wann", "a\u00b7ber", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "APPR", "NN", "VVINF", "$.", "PWAV", "ADV", "ART"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Gesundheit Stand", "tokens": ["Ge\u00b7sund\u00b7heit", "Stand"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "In einer Ebenmaa\u00df bestehet, im sanft- und langsamen Be-", "tokens": ["In", "ei\u00b7ner", "E\u00b7ben\u00b7maa\u00df", "be\u00b7ste\u00b7het", ",", "im", "sanft", "und", "lang\u00b7sa\u00b7men", "Be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "APPRART", "TRUNC", "KON", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+--+", "measure": "iambic.septa.relaxed"}, "line.19": {"text": "wegen", "tokens": ["we\u00b7gen"], "token_info": ["word"], "pos": ["APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Der Feuchtigkeit in unserm K\u00f6rper; so wird man durch", "tokens": ["Der", "Feuch\u00b7tig\u00b7keit", "in", "un\u00b7serm", "K\u00f6r\u00b7per", ";", "so", "wird", "man", "durch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$.", "ADV", "VAFIN", "PIS", "APPR"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "so sanftes Regen", "tokens": ["so", "sanf\u00b7tes", "Re\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "In eine Art von Zubereitung zu mancher nahen Lust", "tokens": ["In", "ei\u00b7ne", "Art", "von", "Zu\u00b7be\u00b7rei\u00b7tung", "zu", "man\u00b7cher", "na\u00b7hen", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.23": {"text": "gesetzt,", "tokens": ["ge\u00b7setzt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.24": {"text": "In eine Art von F\u00e4higkeit, da\u00df man sich gern und leicht", "tokens": ["In", "ei\u00b7ne", "Art", "von", "F\u00e4\u00b7hig\u00b7keit", ",", "da\u00df", "man", "sich", "gern", "und", "leicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,", "KOUS", "PIS", "PRF", "ADV", "KON", "ADJD"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.25": {"text": "ergetzt.", "tokens": ["er\u00b7getzt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.26": {"text": "Die Seel empfindet ein Vergn\u00fcgen von Ruh und stillem", "tokens": ["Die", "Seel", "emp\u00b7fin\u00b7det", "ein", "Ver\u00b7gn\u00fc\u00b7gen", "von", "Ruh", "und", "stil\u00b7lem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Ueberlegen,", "tokens": ["Ue\u00b7ber\u00b7le\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}}}}}