{"textgrid.poem.66170": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcr den Trupp", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hell auf, Kameraden! der kriegrische Ton", "tokens": ["Hell", "auf", ",", "Ka\u00b7me\u00b7ra\u00b7den", "!", "der", "krie\u00b7gri\u00b7sche", "Ton"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "NN", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Der Trommel und Pfeife ermuntert uns schon.", "tokens": ["Der", "Trom\u00b7mel", "und", "Pfei\u00b7fe", "er\u00b7mun\u00b7tert", "uns", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Frisch, schnallt den Tornister den R\u00fccken herum,", "tokens": ["Frisch", ",", "schnallt", "den", "Tor\u00b7nis\u00b7ter", "den", "R\u00fc\u00b7cken", "he\u00b7rum", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und schickt euch zum Marsche, nur seht euch nicht um.", "tokens": ["Und", "schickt", "euch", "zum", "Mar\u00b7sche", ",", "nur", "seht", "euch", "nicht", "um", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Denn Abschied von Freunden und M\u00e4dchen f\u00e4llt schwer,", "tokens": ["Denn", "Ab\u00b7schied", "von", "Freun\u00b7den", "und", "M\u00e4d\u00b7chen", "f\u00e4llt", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und Weinen ziemt braven Soldaten nicht sehr;", "tokens": ["Und", "Wei\u00b7nen", "ziemt", "bra\u00b7ven", "Sol\u00b7da\u00b7ten", "nicht", "sehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJA", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Sie folgen gehorsam des F\u00fchrers Gebot,", "tokens": ["Sie", "fol\u00b7gen", "ge\u00b7hor\u00b7sam", "des", "F\u00fch\u00b7rers", "Ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und r\u00fcsten sich freudig zum Abschied und Tod.", "tokens": ["Und", "r\u00fcs\u00b7ten", "sich", "freu\u00b7dig", "zum", "Ab\u00b7schied", "und", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Scheint nicht auch die Sonn' und der Mond auf dem Kap,", "tokens": ["Scheint", "nicht", "auch", "die", "Sonn'", "und", "der", "Mond", "auf", "dem", "Kap", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ART", "NN", "KON", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und leuchten die Sterne nicht dorten herab?", "tokens": ["Und", "leuch\u00b7ten", "die", "Ster\u00b7ne", "nicht", "dor\u00b7ten", "her\u00b7ab", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und wehen nicht Winde im bl\u00fchenden Hain?", "tokens": ["Und", "we\u00b7hen", "nicht", "Win\u00b7de", "im", "bl\u00fc\u00b7hen\u00b7den", "Hain", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Giebt's dorten nicht Wildpret, nicht Fische, nicht Wein?", "tokens": ["Giebt's", "dor\u00b7ten", "nicht", "Wild\u00b7pret", ",", "nicht", "Fi\u00b7sche", ",", "nicht", "Wein", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Auch sagt man, es gebe von rosiger Laun'", "tokens": ["Auch", "sagt", "man", ",", "es", "ge\u00b7be", "von", "ro\u00b7si\u00b7ger", "Laun'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Dort M\u00e4dels h\u00fcbsch schw\u00e4rzlich, h\u00fcbsch wei\u00dflich und braun:", "tokens": ["Dort", "M\u00e4\u00b7dels", "h\u00fcbsch", "schw\u00e4rz\u00b7lich", ",", "h\u00fcbsch", "wei\u00df\u00b7lich", "und", "braun", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "ADJD", "$,", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und haben Soldaten Gold, M\u00e4dchen und Wein,", "tokens": ["Und", "ha\u00b7ben", "Sol\u00b7da\u00b7ten", "Gold", ",", "M\u00e4d\u00b7chen", "und", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So k\u00f6nnen die F\u00fcrsten nicht gl\u00fccklicher sein.", "tokens": ["So", "k\u00f6n\u00b7nen", "die", "F\u00fcrs\u00b7ten", "nicht", "gl\u00fcck\u00b7li\u00b7cher", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Drum munter, Soldaten! der Marsch ist zwar weit,", "tokens": ["Drum", "mun\u00b7ter", ",", "Sol\u00b7da\u00b7ten", "!", "der", "Marsch", "ist", "zwar", "weit", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "$,", "NN", "$.", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Doch frohe Ges\u00e4nge verk\u00fcrzen die Zeit;", "tokens": ["Doch", "fro\u00b7he", "Ge\u00b7s\u00e4n\u00b7ge", "ver\u00b7k\u00fcr\u00b7zen", "die", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und wenn uns das Wasser des Meeres umfleu\u00dft,", "tokens": ["Und", "wenn", "uns", "das", "Was\u00b7ser", "des", "Mee\u00b7res", "um\u00b7fleu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "So giebt Gott Gesundheit und fr\u00f6hlichen Geist.", "tokens": ["So", "giebt", "Gott", "Ge\u00b7sund\u00b7heit", "und", "fr\u00f6h\u00b7li\u00b7chen", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Ha, wenn wir die Spitze von Afrika sehn,", "tokens": ["Ha", ",", "wenn", "wir", "die", "Spit\u00b7ze", "von", "Af\u00b7ri\u00b7ka", "sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ART", "NN", "APPR", "NE", "VVINF", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und Winde vom Ufer im Federbusch wehn,", "tokens": ["Und", "Win\u00b7de", "vom", "U\u00b7fer", "im", "Fe\u00b7der\u00b7busch", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Dann jauchzen wir alle im Wonnegef\u00fchl:", "tokens": ["Dann", "jauch\u00b7zen", "wir", "al\u00b7le", "im", "Won\u00b7ne\u00b7ge\u00b7f\u00fchl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Hell auf, Kameraden! nun sind wir am Ziel!", "tokens": ["Hell", "auf", ",", "Ka\u00b7me\u00b7ra\u00b7den", "!", "nun", "sind", "wir", "am", "Ziel", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "NN", "$.", "ADV", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.7": {"line.1": {"text": "Wir leben drauf selig und handeln nach Pflicht,", "tokens": ["Wir", "le\u00b7ben", "drauf", "se\u00b7lig", "und", "han\u00b7deln", "nach", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ADJD", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und achten Hy\u00e4nen, die Wilden selbst nicht.", "tokens": ["Und", "ach\u00b7ten", "Hy\u00b7\u00e4\u00b7nen", ",", "die", "Wil\u00b7den", "selbst", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ART", "NN", "ADV", "PTKNEG", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "So wird auf dem Wasser, so wird auf dem Land", "tokens": ["So", "wird", "auf", "dem", "Was\u00b7ser", ",", "so", "wird", "auf", "dem", "Land"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "$,", "ADV", "VAFIN", "APPR", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der Name des braven Soldaten bekannt.", "tokens": ["Der", "Na\u00b7me", "des", "bra\u00b7ven", "Sol\u00b7da\u00b7ten", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}