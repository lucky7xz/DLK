{"textgrid.poem.48281": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "General Sir John Moores Begr\u00e4bnis", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Trommelwirbel, kein Grablied hohl,", "tokens": ["Kein", "Trom\u00b7mel\u00b7wir\u00b7bel", ",", "kein", "Grab\u00b7lied", "hohl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als wir an den Wallrand lenkten,", "tokens": ["Als", "wir", "an", "den", "Wall\u00b7rand", "lenk\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kein Schu\u00df rief \u00fcber ihn hin: \u00bbFahr wohl\u00ab,", "tokens": ["Kein", "Schu\u00df", "rief", "\u00fc\u00b7ber", "ihn", "hin", ":", "\u00bb", "Fahr", "wohl", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$.", "$(", "VVIMP", "ADV", "$(", "$,"], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als wir ihn niedersenkten;", "tokens": ["Als", "wir", "ihn", "nie\u00b7der\u00b7senk\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir senkten ihn nieder um Mitternacht,", "tokens": ["Wir", "senk\u00b7ten", "ihn", "nie\u00b7der", "um", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Sein Grab, ohne Prunk und Flimmer,", "tokens": ["Sein", "Grab", ",", "oh\u00b7ne", "Prunk", "und", "Flim\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wir hatten's mit Bajonetten gemacht,", "tokens": ["Wir", "hat\u00b7ten's", "mit", "Ba\u00b7jo\u00b7net\u00b7ten", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Bei Mond- und Windlicht-Schimmer.", "tokens": ["Bei", "Mon\u00b7d", "und", "Wind\u00b7licht\u00b7Schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Viel Zeit zum Beten hatten wir nicht,", "tokens": ["Viel", "Zeit", "zum", "Be\u00b7ten", "hat\u00b7ten", "wir", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Nicht Zeit zu Klagen und Sorgen,", "tokens": ["Nicht", "Zeit", "zu", "Kla\u00b7gen", "und", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wir starrten dem Toten ins Angesicht", "tokens": ["Wir", "starr\u00b7ten", "dem", "To\u00b7ten", "ins", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und dachten: \u203aWas nun morgen?\u2039", "tokens": ["Und", "dach\u00b7ten", ":", "\u203a", "Was", "nun", "mor\u00b7gen", "?", "\u2039"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PWS", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kein Grabtuch da, kein Priester nah,", "tokens": ["Kein", "Grab\u00b7tuch", "da", ",", "kein", "Pries\u00b7ter", "nah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$,", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Sterbekleid und kein Schragen,", "tokens": ["Kein", "Ster\u00b7be\u00b7kleid", "und", "kein", "Schra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wie ein schlafender Krieger lag er da,", "tokens": ["Wie", "ein", "schla\u00b7fen\u00b7der", "Krie\u00b7ger", "lag", "er", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Seinen Mantel umgeschlagen.", "tokens": ["Sei\u00b7nen", "Man\u00b7tel", "um\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und kaum noch, da\u00df unser Tun vollbracht,", "tokens": ["Und", "kaum", "noch", ",", "da\u00df", "un\u00b7ser", "Tun", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "KOUS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Heim rief uns die Glock' von den Schiffen,", "tokens": ["Heim", "rief", "uns", "die", "Glock'", "von", "den", "Schif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Und \u00fcber uns hin jetzt, durch die Nacht,", "tokens": ["Und", "\u00fc\u00b7ber", "uns", "hin", "jetzt", ",", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "ADV", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.4": {"text": "Des Feindes Kugeln pfiffen;", "tokens": ["Des", "Fein\u00b7des", "Ku\u00b7geln", "pfif\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So lie\u00dfen wir ihn auf ", "tokens": ["So", "lie\u00b7\u00dfen", "wir", "ihn", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Blutfeucht von Heldentume,", "tokens": ["Blut\u00b7feucht", "von", "Hel\u00b7den\u00b7tu\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da liegt er und schl\u00e4ft er ", "tokens": ["Da", "liegt", "er", "und", "schl\u00e4ft", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Allein mit seinem Ruhme.", "tokens": ["Al\u00b7lein", "mit", "sei\u00b7nem", "Ruh\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wir dachten, als wir den H\u00fcgel gemacht", "tokens": ["Wir", "dach\u00b7ten", ",", "als", "wir", "den", "H\u00fc\u00b7gel", "ge\u00b7macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00dcber seinem Bette der Ehre:", "tokens": ["\u00dc\u00b7ber", "sei\u00b7nem", "Bet\u00b7te", "der", "Eh\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "\u203abald dr\u00fcber hin zieht Feindes Macht,", "tokens": ["\u203a", "bald", "dr\u00fc\u00b7ber", "hin", "zieht", "Fein\u00b7des", "Macht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PAV", "APZR", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wir \u2013 weit, weit auf dem Meere;", "tokens": ["Und", "wir", "\u2013", "weit", ",", "weit", "auf", "dem", "Mee\u00b7re", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "ADJD", "$,", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie werden schw\u00e4tzen viel auf und ab", "tokens": ["Sie", "wer\u00b7den", "schw\u00e4t\u00b7zen", "viel", "auf", "und", "ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVFIN", "ADV", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Von Ehre, die kaum gerettet \u2013", "tokens": ["Von", "Eh\u00b7re", ",", "die", "kaum", "ge\u00b7ret\u00b7tet", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "VVPP", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Doch nichts von allem dringt in sein Grab,", "tokens": ["Doch", "nichts", "von", "al\u00b7lem", "dringt", "in", "sein", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Drin wir Britischen ihn gebettet.\u2039", "tokens": ["Drin", "wir", "Bri\u00b7ti\u00b7schen", "ihn", "ge\u00b7bet\u00b7tet", ".", "\u2039"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "VVPP", "$.", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}