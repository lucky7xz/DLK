{"dta.poem.10076": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Sinn-reiche Bestraffung der Un-  \n achtsamkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst h\u00f6rt' ich abermahl, du Auszug weiser Geister,", "tokens": ["J\u00fcngst", "h\u00f6rt'", "ich", "a\u00b7ber\u00b7mahl", ",", "du", "Aus\u00b7zug", "wei\u00b7ser", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PPER", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der edlen Hammon-Stadt ber\u00fchmter B\u00fcrgermeister,", "tokens": ["Der", "ed\u00b7len", "Ham\u00b7mon\u00b7Stadt", "be\u00b7r\u00fchm\u00b7ter", "B\u00fcr\u00b7ger\u00b7meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du theurer ", "tokens": ["Du", "theu\u00b7rer"], "token_info": ["word", "word"], "pos": ["PPER", "ADJD"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "So w\u00fcrdig, damit es best\u00e4ndig bleibe,", "tokens": ["So", "w\u00fcr\u00b7dig", ",", "da\u00b7mit", "es", "be\u00b7st\u00e4n\u00b7dig", "blei\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Da\u00df man\u2019s in festen Stahl, und harten Marmor schreibe.", "tokens": ["Da\u00df", "man's", "in", "fes\u00b7ten", "Stahl", ",", "und", "har\u00b7ten", "Mar\u00b7mor", "schrei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Als iemand dich besch\u00e4fftigt fand,", "tokens": ["Als", "ie\u00b7mand", "dich", "be\u00b7sch\u00e4ff\u00b7tigt", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch ein Vergr\u00f6ssrungs-Glas ein W\u00fcrmchen anzuseheu,", "tokens": ["Durch", "ein", "Ver\u00b7gr\u00f6ss\u00b7rungs\u00b7Glas", "ein", "W\u00fcrm\u00b7chen", "an\u00b7zu\u00b7se\u00b7heu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+----+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Und, wie es Dein bedachtsamer Verstand", "tokens": ["Und", ",", "wie", "es", "Dein", "be\u00b7dacht\u00b7sa\u00b7mer", "Ver\u00b7stand"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nach allen Theilen durchzugehen", "tokens": ["Nach", "al\u00b7len", "Thei\u00b7len", "durch\u00b7zu\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sich viele M\u00fche nahm, gewahr ward, auch sogleich,", "tokens": ["Sich", "vie\u00b7le", "M\u00fc\u00b7he", "nahm", ",", "ge\u00b7wahr", "ward", ",", "auch", "sog\u00b7leich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "VVFIN", "$,", "ADJD", "VAFIN", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie es doch m\u00f6glich sey, ein so ver\u00e4chtlich Thier", "tokens": ["Wie", "es", "doch", "m\u00f6g\u00b7lich", "sey", ",", "ein", "so", "ver\u00b7\u00e4cht\u00b7lich", "Thier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VAFIN", "$,", "ART", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So m\u00fchsam zu besehn, mit ernstem Schertz, ja schier", "tokens": ["So", "m\u00fch\u00b7sam", "zu", "be\u00b7sehn", ",", "mit", "erns\u00b7tem", "Schertz", ",", "ja", "schier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "$,", "APPR", "ADJA", "NN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit halben Hohn-Gel\u00e4chter, fragte;", "tokens": ["Mit", "hal\u00b7ben", "Hohn\u00b7Ge\u00b7l\u00e4ch\u00b7ter", ",", "frag\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So h\u00f6rt\u2019 ich, wie Dein Mund ihm die\u00df zur Antwort sagte:", "tokens": ["So", "h\u00f6rt'", "ich", ",", "wie", "Dein", "Mund", "ihm", "die\u00df", "zur", "Ant\u00b7wort", "sag\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPOSAT", "NN", "PPER", "PDS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es hielte GOTT der HERR, der Sch\u00f6pfer aller Dinge", "tokens": ["Es", "hiel\u00b7te", "GoTT", "der", "HeRR", ",", "der", "Sch\u00f6p\u00b7fer", "al\u00b7ler", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "ART", "NN", "$,", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die\u00df kleine Thier nicht zu geringe:", "tokens": ["Die\u00df", "klei\u00b7ne", "Thier", "nicht", "zu", "ge\u00b7rin\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "PTKNEG", "PTKZU", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das ew\u2019ge Wort, die Quell der Himmel und der Erden", "tokens": ["Das", "ew'\u00b7ge", "Wort", ",", "die", "Quell", "der", "Him\u00b7mel", "und", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hielt\u2019 es der Sch\u00f6pfung wehrt, und hie\u00df die\u00df W\u00fcrmchen", "tokens": ["Hielt'", "es", "der", "Sch\u00f6p\u00b7fung", "wehrt", ",", "und", "hie\u00df", "die\u00df", "W\u00fcrm\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "PDS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und ich sollt es nicht wehrt, es zu betrachten,", "tokens": ["Und", "ich", "sollt", "es", "nicht", "wehrt", ",", "es", "zu", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ja nicht einmahl des Ansehns w\u00fcrdig, achten?", "tokens": ["Ja", "nicht", "ein\u00b7mahl", "des", "An\u00b7sehns", "w\u00fcr\u00b7dig", ",", "ach\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "PTKNEG", "ADV", "ART", "NN", "ADJD", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}