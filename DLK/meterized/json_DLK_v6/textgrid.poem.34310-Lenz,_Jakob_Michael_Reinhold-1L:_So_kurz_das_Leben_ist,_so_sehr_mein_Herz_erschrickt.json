{"textgrid.poem.34310": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: So kurz das Leben ist, so sehr mein Herz erschrickt", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So kurz das Leben ist, so sehr mein Herz erschrickt", "tokens": ["So", "kurz", "das", "Le\u00b7ben", "ist", ",", "so", "sehr", "mein", "Herz", "er\u00b7schrickt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "VAFIN", "$,", "ADV", "ADV", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor seinem k\u00fchnen Unterfangen", "tokens": ["Vor", "sei\u00b7nem", "k\u00fch\u00b7nen", "Un\u00b7ter\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und still steht vorm Gedanken, da\u00df die bangen", "tokens": ["Und", "still", "steht", "vorm", "Ge\u00b7dan\u00b7ken", ",", "da\u00df", "die", "ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Accente seines Grams bis zu ihr selbst gelangen,", "tokens": ["Ac\u00b7cen\u00b7te", "sei\u00b7nes", "Grams", "bis", "zu", "ihr", "selbst", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des Grams, den ich so gern verschwieg,", "tokens": ["Des", "Grams", ",", "den", "ich", "so", "gern", "ver\u00b7schwieg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der nie zur Sprache sich verstieg, \u2013", "tokens": ["Der", "nie", "zur", "Spra\u00b7che", "sich", "ver\u00b7stieg", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "PRF", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch jetzt, ach jetzt, wer kann ihm wiederstehen,", "tokens": ["Doch", "jetzt", ",", "ach", "jetzt", ",", "wer", "kann", "ihm", "wie\u00b7ders\u00b7te\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ITJ", "ADV", "$,", "PWS", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dem Wunsch, sich wenigstens von ihr bedaurt zu sehen \u2013", "tokens": ["Dem", "Wunsch", ",", "sich", "we\u00b7nigs\u00b7tens", "von", "ihr", "be\u00b7daurt", "zu", "se\u00b7hen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRF", "ADV", "APPR", "PPOSAT", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Von euch, ihr himmelvollen Augen, deren Licht", "tokens": ["Von", "euch", ",", "ihr", "him\u00b7mel\u00b7vol\u00b7len", "Au\u00b7gen", ",", "de\u00b7ren", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mir Witz und Sprache leiht, die meinem Schmerz gefehlet", "tokens": ["Mir", "Witz", "und", "Spra\u00b7che", "leiht", ",", "die", "mei\u00b7nem", "Schmerz", "ge\u00b7feh\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und jeden Mund der von euch spricht,", "tokens": ["Und", "je\u00b7den", "Mund", "der", "von", "euch", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mit g\u00f6ttlicher Beredsamkeit beseelet.", "tokens": ["Mit", "g\u00f6tt\u00b7li\u00b7cher", "Be\u00b7red\u00b7sam\u00b7keit", "be\u00b7see\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}