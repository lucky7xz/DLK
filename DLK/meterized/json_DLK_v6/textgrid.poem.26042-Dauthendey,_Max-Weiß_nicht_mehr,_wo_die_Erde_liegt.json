{"textgrid.poem.26042": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Wei\u00df nicht mehr, wo die Erde liegt", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Raben schreien wie verwundet", "tokens": ["Die", "Ra\u00b7ben", "schrei\u00b7en", "wie", "ver\u00b7wun\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und prophezeien Nacht und Not;", "tokens": ["Und", "pro\u00b7phe\u00b7zei\u00b7en", "Nacht", "und", "Not", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Frost hat jede T\u00fcr umstellt,", "tokens": ["Der", "Frost", "hat", "je\u00b7de", "T\u00fcr", "um\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und der Hungerhund bellt.", "tokens": ["Und", "der", "Hun\u00b7ge\u00b7rhund", "bellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "--+---", "measure": "anapaest.init"}, "line.5": {"text": "Wir halten uns immer noch eng umschlungen,", "tokens": ["Wir", "hal\u00b7ten", "uns", "im\u00b7mer", "noch", "eng", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Im K\u00fcssen fanden wir noch kein Wort,", "tokens": ["Im", "K\u00fcs\u00b7sen", "fan\u00b7den", "wir", "noch", "kein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Die Lerchen haben sich tot gesungen,", "tokens": ["Die", "Ler\u00b7chen", "ha\u00b7ben", "sich", "tot", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und Wolken w\u00e4lzten den Sommer fort.", "tokens": ["Und", "Wol\u00b7ken", "w\u00e4lz\u00b7ten", "den", "Som\u00b7mer", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Doch dein Haupt, das in meinem Arm sich wiegt,", "tokens": ["Doch", "dein", "Haupt", ",", "das", "in", "mei\u00b7nem", "Arm", "sich", "wiegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Wei\u00df nicht mehr, wo die Erde liegt.", "tokens": ["Wei\u00df", "nicht", "mehr", ",", "wo", "die", "Er\u00b7de", "liegt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}