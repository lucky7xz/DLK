{"textgrid.poem.50611": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Schuldner oder Gl\u00e4ubiger?", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Manch ungehobelt Holz wird zum Merkur gemacht,", "tokens": ["Manch", "un\u00b7ge\u00b7ho\u00b7belt", "Holz", "wird", "zum", "Mer\u00b7kur", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil mancher theure Mann, aus aller H\u00f6fling' Acht, ", "tokens": ["Weil", "man\u00b7cher", "theu\u00b7re", "Mann", ",", "aus", "al\u00b7ler", "H\u00f6f\u00b7ling'", "Acht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "$,", "APPR", "PIAT", "NN", "CARD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sich sonst bei keinem Maa\u00df, als seinem Schatten mi\u00dft.", "tokens": ["Sich", "sonst", "bei", "kei\u00b7nem", "Maa\u00df", ",", "als", "sei\u00b7nem", "Schat\u00b7ten", "mi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "PIAT", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Viel hebt das Gl\u00fcck empor, Viel' h\u00e4lt es auch zur\u00fcck;", "tokens": ["Viel", "hebt", "das", "Gl\u00fcck", "em\u00b7por", ",", "Viel'", "h\u00e4lt", "es", "auch", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch wer die Welt recht kennt, der findet, da\u00df das Gl\u00fcck", "tokens": ["Doch", "wer", "die", "Welt", "recht", "kennt", ",", "der", "fin\u00b7det", ",", "da\u00df", "das", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "ADJD", "VVFIN", "$,", "PRELS", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mehr Schulden ausstehn hat, als es selbst schuldig ist.", "tokens": ["Mehr", "Schul\u00b7den", "aus\u00b7stehn", "hat", ",", "als", "es", "selbst", "schul\u00b7dig", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "VAFIN", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}}}}}