{"textgrid.poem.25290": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus schwarzem Sarge starrt,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus schwarzem Sarge starrt,", "tokens": ["Aus", "schwar\u00b7zem", "Sar\u00b7ge", "starrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Von Morgengrau erhellt,", "tokens": ["Von", "Mor\u00b7gen\u00b7grau", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Toter bleich und ernsthaft", "tokens": ["Ein", "To\u00b7ter", "bleich", "und", "ernst\u00b7haft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In die verlassne Welt.", "tokens": ["In", "die", "ver\u00b7lass\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein m\u00fcdes Schluchzen irrt", "tokens": ["Ein", "m\u00fc\u00b7des", "Schluch\u00b7zen", "irrt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Umher im Beigemach;", "tokens": ["Um\u00b7her", "im", "Bei\u00b7ge\u00b7mach", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im starren Totenantlitz", "tokens": ["Im", "star\u00b7ren", "To\u00b7ten\u00b7ant\u00b7litz"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird keine R\u00fchrung wach.", "tokens": ["Wird", "kei\u00b7ne", "R\u00fch\u00b7rung", "wach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In Wonne bricht der Morgen", "tokens": ["In", "Won\u00b7ne", "bricht", "der", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Herein mit roter Glut,", "tokens": ["Her\u00b7ein", "mit", "ro\u00b7ter", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Begr\u00fc\u00dft von Vogelzwitschern;", "tokens": ["Be\u00b7gr\u00fc\u00dft", "von", "Vo\u00b7gel\u00b7zwit\u00b7schern", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tief ernst der Tote ruht.", "tokens": ["Tief", "ernst", "der", "To\u00b7te", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Er starrt empor und gr\u00fcbelt,", "tokens": ["Er", "starrt", "em\u00b7por", "und", "gr\u00fc\u00b7belt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie es nur m\u00f6glich war,", "tokens": ["Wie", "es", "nur", "m\u00f6g\u00b7lich", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df er von Lust und Leide", "tokens": ["Da\u00df", "er", "von", "Lust", "und", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gebebt so manches Jahr.", "tokens": ["Ge\u00b7bebt", "so", "man\u00b7ches", "Jahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}