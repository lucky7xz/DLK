{"textgrid.poem.33975": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle Wunden h\u00f6ren auf, zu bluten,", "tokens": ["Al\u00b7le", "Wun\u00b7den", "h\u00f6\u00b7ren", "auf", ",", "zu", "blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Alle Schmerzen h\u00f6ren auf, zu brennen,", "tokens": ["Al\u00b7le", "Schmer\u00b7zen", "h\u00f6\u00b7ren", "auf", ",", "zu", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Doch, entkrochen seines Jammers Fluten,", "tokens": ["Doch", ",", "ent\u00b7kro\u00b7chen", "sei\u00b7nes", "Jam\u00b7mers", "Flu\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Kann der Mensch sich selbst nicht mehr erkennen,", "tokens": ["Kann", "der", "Mensch", "sich", "selbst", "nicht", "mehr", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PRF", "ADV", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mund und Auge sind ihm zugefroren,", "tokens": ["Mund", "und", "Au\u00b7ge", "sind", "ihm", "zu\u00b7ge\u00b7fro\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Selbst des Abgrunds Tiefe ist vergessen,", "tokens": ["Selbst", "des", "Ab\u00b7grunds", "Tie\u00b7fe", "ist", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und ihm ist, als h\u00e4tt' er Nichts verloren,", "tokens": ["Und", "ihm", "ist", ",", "als", "h\u00e4tt'", "er", "Nichts", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Aber auch, als h\u00e4tt' er Nichts besessen.", "tokens": ["A\u00b7ber", "auch", ",", "als", "h\u00e4tt'", "er", "Nichts", "be\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOKOM", "VAFIN", "PPER", "PIS", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Denn das ewige Gesetz, das waltet,", "tokens": ["Denn", "das", "e\u00b7wi\u00b7ge", "Ge\u00b7setz", ",", "das", "wal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Will die Harmonie noch im Verderben,", "tokens": ["Will", "die", "Har\u00b7mo\u00b7nie", "noch", "im", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und im Gleichmaa\u00df, wie es sich entfaltet,", "tokens": ["Und", "im", "Gleich\u00b7maa\u00df", ",", "wie", "es", "sich", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mu\u00df ein Wesen auch vergeh'n und sterben.", "tokens": ["Mu\u00df", "ein", "We\u00b7sen", "auch", "ver\u00b7geh'n", "und", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Alle Theile stimmen nach dem einen", "tokens": ["Al\u00b7le", "Thei\u00b7le", "stim\u00b7men", "nach", "dem", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ART", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sich herunter, den der Tod beschlichen,", "tokens": ["Sich", "her\u00b7un\u00b7ter", ",", "den", "der", "Tod", "be\u00b7schli\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKVZ", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und so kann es ganz gesund erscheinen,", "tokens": ["Und", "so", "kann", "es", "ganz", "ge\u00b7sund", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wenn das Leben ganz aus ihm gewichen.", "tokens": ["Wenn", "das", "Le\u00b7ben", "ganz", "aus", "ihm", "ge\u00b7wi\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Ja, ein Weh giebt's, das man nicht ertr\u00fcge,", "tokens": ["Ja", ",", "ein", "Weh", "giebt's", ",", "das", "man", "nicht", "er\u00b7tr\u00fc\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VVFIN", "$,", "PRELS", "PIS", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn es nicht sein eig'nes Maa\u00df zerbr\u00e4che,", "tokens": ["Wenn", "es", "nicht", "sein", "eig'\u00b7nes", "Maa\u00df", "zer\u00b7br\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und, wie einer abgeschmackten L\u00fcge,", "tokens": ["Und", ",", "wie", "ei\u00b7ner", "ab\u00b7ge\u00b7schmack\u00b7ten", "L\u00fc\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der Erinn'rung selber widerspr\u00e4che.", "tokens": ["Der", "Er\u00b7inn'\u00b7rung", "sel\u00b7ber", "wi\u00b7der\u00b7spr\u00e4\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Dann, vergessend in der innern Oede,", "tokens": ["Dann", ",", "ver\u00b7ges\u00b7send", "in", "der", "in\u00b7nern", "O\u00b7e\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Da\u00df einst frisch das Herz geschlagen habe,", "tokens": ["Da\u00df", "einst", "frisch", "das", "Herz", "ge\u00b7schla\u00b7gen", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Ist ein Mensch der Nessel gleich, die schn\u00f6de", "tokens": ["Ist", "ein", "Mensch", "der", "Nes\u00b7sel", "gleich", ",", "die", "schn\u00f6\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "ADV", "$,", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wuchert \u00fcber seinem eig'nen Grabe.", "tokens": ["Wu\u00b7chert", "\u00fc\u00b7ber", "sei\u00b7nem", "eig'\u00b7nen", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}