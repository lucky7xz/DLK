{"textgrid.poem.64018": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Blutende Eiche", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blumen sind hervorgebrochen,", "tokens": ["Blu\u00b7men", "sind", "her\u00b7vor\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die zittern voll Blut", "tokens": ["Die", "zit\u00b7tern", "voll", "Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und k\u00f6nnen nicht sagen,", "tokens": ["Und", "k\u00f6n\u00b7nen", "nicht", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Was da war ....", "tokens": ["Was", "da", "war", "...."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Klagende Farben ....", "tokens": ["Kla\u00b7gen\u00b7de", "Far\u00b7ben", "...."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Blutende Eiche.", "tokens": ["Blu\u00b7ten\u00b7de", "Ei\u00b7che", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}