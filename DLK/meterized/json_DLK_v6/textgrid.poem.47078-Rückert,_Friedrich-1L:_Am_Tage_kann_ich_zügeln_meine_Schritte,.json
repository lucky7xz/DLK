{"textgrid.poem.47078": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Am Tage kann ich z\u00fcgeln meine Schritte,", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Tage kann ich z\u00fcgeln meine Schritte,", "tokens": ["Am", "Ta\u00b7ge", "kann", "ich", "z\u00fc\u00b7geln", "mei\u00b7ne", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich nicht zu tragen mehr zu deiner H\u00fctte;", "tokens": ["Mich", "nicht", "zu", "tra\u00b7gen", "mehr", "zu", "dei\u00b7ner", "H\u00fct\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "PTKZU", "VVINF", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nachts kann ich es dem Traume nicht verwehren,", "tokens": ["Nachts", "kann", "ich", "es", "dem", "Trau\u00b7me", "nicht", "ver\u00b7weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Noch oft daselbst, wie vormals, einzukehren.", "tokens": ["Noch", "oft", "da\u00b7selbst", ",", "wie", "vor\u00b7mals", ",", "ein\u00b7zu\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "PAV", "$,", "PWAV", "ADV", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Am Tage kann ich wenden meine Blicke,", "tokens": ["Am", "Ta\u00b7ge", "kann", "ich", "wen\u00b7den", "mei\u00b7ne", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df sie kein neuer Blick von dir bestricke;", "tokens": ["Da\u00df", "sie", "kein", "neu\u00b7er", "Blick", "von", "dir", "be\u00b7stri\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nachts kann ich so nicht die Gedanken zwingen,", "tokens": ["Nachts", "kann", "ich", "so", "nicht", "die", "Ge\u00b7dan\u00b7ken", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Da\u00df sie dein Bild mir nicht vor Augen bringen.", "tokens": ["Da\u00df", "sie", "dein", "Bild", "mir", "nicht", "vor", "Au\u00b7gen", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PPER", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nachts kann ich nicht gebieten diesen Trieben,", "tokens": ["Nachts", "kann", "ich", "nicht", "ge\u00b7bie\u00b7ten", "die\u00b7sen", "Trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "VVFIN", "PDAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die eigenwillig fahren fort zu lieben;", "tokens": ["Die", "ei\u00b7gen\u00b7wil\u00b7lig", "fah\u00b7ren", "fort", "zu", "lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Allein am Tage soll mein Geist sich fassen,", "tokens": ["Al\u00b7lein", "am", "Ta\u00b7ge", "soll", "mein", "Geist", "sich", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VMFIN", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dich zu vergessen, ach, und dich zu lassen.", "tokens": ["Dich", "zu", "ver\u00b7ges\u00b7sen", ",", "ach", ",", "und", "dich", "zu", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "ITJ", "$,", "KON", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}