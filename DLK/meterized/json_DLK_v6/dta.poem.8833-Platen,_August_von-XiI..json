{"dta.poem.8833": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XiI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Oft auch nehm' ich mich zusammen vor den Leuten, dir", "tokens": ["Oft", "auch", "nehm'", "ich", "mich", "zu\u00b7sam\u00b7men", "vor", "den", "Leu\u00b7ten", ",", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "$,", "PPER"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Oft in deiner Freunde Zirkel hab' ich angeh\u00f6rt geduldig", "tokens": ["Oft", "in", "dei\u00b7ner", "Freun\u00b7de", "Zir\u00b7kel", "hab'", "ich", "an\u00b7ge\u00b7h\u00f6rt", "ge\u00b7dul\u00b7dig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN", "VAFIN", "PPER", "VVFIN", "ADJD"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Worte, welche nichts verfangen, nichts bedeuten, dir", "tokens": ["Wor\u00b7te", ",", "wel\u00b7che", "nichts", "ver\u00b7fan\u00b7gen", ",", "nichts", "be\u00b7deu\u00b7ten", ",", "dir"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "PRELS", "PIS", "VVPP", "$,", "PIS", "VVINF", "$,", "PPER"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Und damit des Lenzes Reize sich erh\u00f6hn in meinen", "tokens": ["Und", "da\u00b7mit", "des", "Len\u00b7zes", "Rei\u00b7ze", "sich", "er\u00b7h\u00f6hn", "in", "mei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "ART", "ADJA", "NN", "PRF", "VVFIN", "APPR", "PPOSAT"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Denk' ich, da\u00df sich Flur und Garten nur erneuten dir", "tokens": ["Denk'", "ich", ",", "da\u00df", "sich", "Flur", "und", "Gar\u00b7ten", "nur", "er\u00b7neu\u00b7ten", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "PRF", "NN", "KON", "NN", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "Auf verschiednen Wegen haben sich der Trunkenheit er-", "tokens": ["Auf", "ver\u00b7schied\u00b7nen", "We\u00b7gen", "ha\u00b7ben", "sich", "der", "Trun\u00b7ken\u00b7heit", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PRF", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "F\u00fcr sich selbst die Stumpfgesinnten, die Gescheuten dir", "tokens": ["F\u00fcr", "sich", "selbst", "die", "Stumpf\u00b7ge\u00b7sinn\u00b7ten", ",", "die", "Ge\u00b7scheu\u00b7ten", "dir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRF", "ADV", "ART", "NN", "$,", "ART", "NN", "PPER"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "La\u00df in deinem Schatten endlich schlummern uns, o", "tokens": ["La\u00df", "in", "dei\u00b7nem", "Schat\u00b7ten", "end\u00b7lich", "schlum\u00b7mern", "uns", ",", "o"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "ADV", "VVFIN", "PPER", "$,", "FM"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "Da wir nur zu lang' an Schatten uns erfreuten, dir", "tokens": ["Da", "wir", "nur", "zu", "lang'", "an", "Schat\u00b7ten", "uns", "er\u00b7freu\u00b7ten", ",", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADV", "APPR", "NN", "PPER", "VVFIN", "$,", "PPER"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}