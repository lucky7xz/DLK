{"dta.poem.15687": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Beneide nicht den Mann um Ruhm, den er nicht hat", "tokens": ["Be\u00b7nei\u00b7de", "nicht", "den", "Mann", "um", "Ruhm", ",", "den", "er", "nicht", "hat"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKNEG", "ART", "NN", "APPR", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erworben ohne M\u00fch, durch Leiden oder That.", "tokens": ["Er\u00b7wor\u00b7ben", "oh\u00b7ne", "M\u00fch", ",", "durch", "Lei\u00b7den", "o\u00b7der", "That", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Bist du bereit die That zu thun, die er gethan?", "tokens": ["Bist", "du", "be\u00b7reit", "die", "That", "zu", "thun", ",", "die", "er", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kannst du das Leiden, das er litt, auf dich empfahn?", "tokens": ["Kannst", "du", "das", "Lei\u00b7den", ",", "das", "er", "litt", ",", "auf", "dich", "em\u00b7pfahn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und wenn er weder litt f\u00fcr seinen Ruhm noch stritt;", "tokens": ["Und", "wenn", "er", "we\u00b7der", "litt", "f\u00fcr", "sei\u00b7nen", "Ruhm", "noch", "stritt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verdienstlos m\u00f6chtest du dich schm\u00fccken nie damit!", "tokens": ["Ver\u00b7dienst\u00b7los", "m\u00f6ch\u00b7test", "du", "dich", "schm\u00fc\u00b7cken", "nie", "da\u00b7mit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "PRF", "VVFIN", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}