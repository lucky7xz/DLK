{"textgrid.poem.53726": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Park Monceau", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier ist es h\u00fcbsch. Hier kann ich ruhig tr\u00e4umen.", "tokens": ["Hier", "ist", "es", "h\u00fcbsch", ".", "Hier", "kann", "ich", "ru\u00b7hig", "tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$.", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier bin ich Mensch \u2013 und nicht nur Zivilist.", "tokens": ["Hier", "bin", "ich", "Mensch", "\u2013", "und", "nicht", "nur", "Zi\u00b7vi\u00b7list", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$(", "KON", "PTKNEG", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hier darf ich links gehn. Unter gr\u00fcnen B\u00e4umen", "tokens": ["Hier", "darf", "ich", "links", "gehn", ".", "Un\u00b7ter", "gr\u00fc\u00b7nen", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "sagt keine Tafel, was verboten ist.", "tokens": ["sagt", "kei\u00b7ne", "Ta\u00b7fel", ",", "was", "ver\u00b7bo\u00b7ten", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "PRELS", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein dicker Kullerball liegt auf dem Rasen.", "tokens": ["Ein", "di\u00b7cker", "Kul\u00b7ler\u00b7ball", "liegt", "auf", "dem", "Ra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Vogel zupft an einem hellen Blatt.", "tokens": ["Ein", "Vo\u00b7gel", "zupft", "an", "ei\u00b7nem", "hel\u00b7len", "Blatt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein kleiner Junge gr\u00e4bt sich in der Nasen", "tokens": ["Ein", "klei\u00b7ner", "Jun\u00b7ge", "gr\u00e4bt", "sich", "in", "der", "Na\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und freut sich, wenn er was gefunden hat.", "tokens": ["und", "freut", "sich", ",", "wenn", "er", "was", "ge\u00b7fun\u00b7den", "hat", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "KOUS", "PPER", "PIS", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es pr\u00fcfen vier Amerikanerinnen,", "tokens": ["Es", "pr\u00fc\u00b7fen", "vier", "A\u00b7me\u00b7ri\u00b7ka\u00b7ne\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ob Cook auch recht hat und hier B\u00e4ume stehn.", "tokens": ["ob", "Cook", "auch", "recht", "hat", "und", "hier", "B\u00e4u\u00b7me", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "ADV", "VAFIN", "KON", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Paris von au\u00dfen und Paris von innen:", "tokens": ["Pa\u00b7ris", "von", "au\u00b7\u00dfen", "und", "Pa\u00b7ris", "von", "in\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADV", "KON", "NE", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "sie sehen nichts und m\u00fcssen alles sehn.", "tokens": ["sie", "se\u00b7hen", "nichts", "und", "m\u00fcs\u00b7sen", "al\u00b7les", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "KON", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Kinder l\u00e4rmen auf den bunten Steinen.", "tokens": ["Die", "Kin\u00b7der", "l\u00e4r\u00b7men", "auf", "den", "bun\u00b7ten", "Stei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sonne scheint und glitzert auf ein Haus.", "tokens": ["Die", "Son\u00b7ne", "scheint", "und", "glit\u00b7zert", "auf", "ein", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich sitze still und lasse mich bescheinen", "tokens": ["Ich", "sit\u00b7ze", "still", "und", "las\u00b7se", "mich", "be\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und ruh von meinem Vaterlande aus.", "tokens": ["und", "ruh", "von", "mei\u00b7nem", "Va\u00b7ter\u00b7lan\u00b7de", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}