{"textgrid.poem.65119": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gewichen war der helle Tag", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gewichen war der helle Tag", "tokens": ["Ge\u00b7wi\u00b7chen", "war", "der", "hel\u00b7le", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Fr\u00fchlingsnacht, der warmen;", "tokens": ["Der", "Fr\u00fch\u00b7lings\u00b7nacht", ",", "der", "war\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit Schauern der Entz\u00fcckung lag", "tokens": ["Mit", "Schau\u00b7ern", "der", "Ent\u00b7z\u00fc\u00b7ckung", "lag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich still in deinen Armen.", "tokens": ["Ich", "still", "in", "dei\u00b7nen", "Ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Herab auf deine theure Hand", "tokens": ["Her\u00b7ab", "auf", "dei\u00b7ne", "theu\u00b7re", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Flo\u00df meine Freudenthr\u00e4ne,", "tokens": ["Flo\u00df", "mei\u00b7ne", "Freu\u00b7dent\u00b7hr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Begeistert st\u00fcrzte in den Brand", "tokens": ["Be\u00b7geis\u00b7tert", "st\u00fcrz\u00b7te", "in", "den", "Brand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Seele, die Phal\u00e4ne!", "tokens": ["Die", "See\u00b7le", ",", "die", "Pha\u00b7l\u00e4\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ausging von deinem Angesicht", "tokens": ["Aus\u00b7ging", "von", "dei\u00b7nem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein wundersam Geleuchte,", "tokens": ["Ein", "wun\u00b7der\u00b7sam", "Ge\u00b7leuch\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Das von des Himmels Friedenslicht", "tokens": ["Das", "von", "des", "Him\u00b7mels", "Frie\u00b7dens\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ein Abglanz mich bed\u00e4uchte!", "tokens": ["Ein", "Ab\u00b7glanz", "mich", "be\u00b7d\u00e4uch\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "O wie so s\u00fc\u00df die Stunde war!", "tokens": ["O", "wie", "so", "s\u00fc\u00df", "die", "Stun\u00b7de", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Heut' ist's ein Jahr.", "tokens": ["Heut'", "ist's", "ein", "Jahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Vorbei! vorbei! der Blitzstrahl fiel,", "tokens": ["Vor\u00b7bei", "!", "vor\u00b7bei", "!", "der", "Blitz\u00b7strahl", "fiel", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PTKVZ", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er hat mein Gl\u00fcck getroffen!", "tokens": ["Er", "hat", "mein", "Gl\u00fcck", "ge\u00b7trof\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Schicksal treibt ein frevles Spiel", "tokens": ["Das", "Schick\u00b7sal", "treibt", "ein", "frev\u00b7les", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit unserm besten Hoffen;", "tokens": ["Mit", "un\u00b7serm", "bes\u00b7ten", "Hof\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von dir geschieden und getrennt,", "tokens": ["Von", "dir", "ge\u00b7schie\u00b7den", "und", "ge\u00b7trennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verblutend losgerissen,", "tokens": ["Ver\u00b7blu\u00b7tend", "los\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Von dir, nach dem die Seele brennt,", "tokens": ["Von", "dir", ",", "nach", "dem", "die", "See\u00b7le", "brennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In ewigem Vermissen,", "tokens": ["In", "e\u00b7wi\u00b7gem", "Ver\u00b7mis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Scheint mir das Leben arm und leer,", "tokens": ["Scheint", "mir", "das", "Le\u00b7ben", "arm", "und", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ver\u00e4chtlich jede Spende,", "tokens": ["Ver\u00b7\u00e4cht\u00b7lich", "je\u00b7de", "Spen\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Nichts w\u00fcnsche, nichts verlang ich mehr", "tokens": ["Nichts", "w\u00fcn\u00b7sche", ",", "nichts", "ver\u00b7lang", "ich", "mehr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Als nur ein rasches Ende!", "tokens": ["Als", "nur", "ein", "ra\u00b7sches", "En\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "O st\u00e4ndest du an meiner Vahr'", "tokens": ["O", "st\u00e4n\u00b7dest", "du", "an", "mei\u00b7ner", "Vahr'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Heut' \u00fcber's Jahr!", "tokens": ["Heut'", "\u00fc\u00b7ber's", "Jahr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}