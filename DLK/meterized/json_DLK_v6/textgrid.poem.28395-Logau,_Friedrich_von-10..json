{"textgrid.poem.28395": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die W\u00e4rmde zeucht empor, was vor der Frost verdeckte;", "tokens": ["Die", "W\u00e4rm\u00b7de", "zeucht", "em\u00b7por", ",", "was", "vor", "der", "Frost", "ver\u00b7deck\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verfolgung gibt an Tag, was Sicherheit versteckte;", "tokens": ["Ver\u00b7fol\u00b7gung", "gibt", "an", "Tag", ",", "was", "Si\u00b7cher\u00b7heit", "ver\u00b7steck\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,", "PWS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum, sey Verfolgung gleich so sch\u00e4dlich als sie wil,", "tokens": ["Drum", ",", "sey", "Ver\u00b7fol\u00b7gung", "gleich", "so", "sch\u00e4d\u00b7lich", "als", "sie", "wil", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "VAFIN", "NN", "ADV", "ADV", "ADJD", "KOKOM", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist di\u00df doch gut; sie ist de\u00df Christenthums April.", "tokens": ["Ist", "di\u00df", "doch", "gut", ";", "sie", "ist", "de\u00df", "Chris\u00b7ten\u00b7thums", "Ap\u00b7ril", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "ADJD", "$.", "PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}