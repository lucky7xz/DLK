{"textgrid.poem.26556": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heilige Klugheit,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heilige Klugheit,", "tokens": ["Hei\u00b7li\u00b7ge", "Klug\u00b7heit", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sicherste B\u00fcrgschaft des Gl\u00fccks \u2013", "tokens": ["Si\u00b7chers\u00b7te", "B\u00fcrg\u00b7schaft", "des", "Gl\u00fccks", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sehns\u00fcchtig preis't dich heute mein Lied!", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "preis't", "dich", "heu\u00b7te", "mein", "Lied", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Was sind alle Gaben des Schicksals", "tokens": ["Was", "sind", "al\u00b7le", "Ga\u00b7ben", "des", "Schick\u00b7sals"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PIAT", "NN", "ART", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ohne dich, o Klugheit?", "tokens": ["Oh\u00b7ne", "dich", ",", "o", "Klug\u00b7heit", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Was Kraft?", "tokens": ["Was", "Kraft", "?"], "token_info": ["word", "word", "punct"], "pos": ["PWS", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Was Sch\u00f6nheit?", "tokens": ["Was", "Sch\u00f6n\u00b7heit", "?"], "token_info": ["word", "word", "punct"], "pos": ["PWS", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Was des Reichthums G\u00fcter \u2013", "tokens": ["Was", "des", "Reicht\u00b7hums", "G\u00fc\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ja selbst der Genius,", "tokens": ["Ja", "selbst", "der", "Ge\u00b7nius", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Wenn ihn deine Hand nicht lenkt?", "tokens": ["Wenn", "ihn", "dei\u00b7ne", "Hand", "nicht", "lenkt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und doch \u2013", "tokens": ["Und", "doch", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$("], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Wenn ich erf\u00fcllt dich mir denke", "tokens": ["Wenn", "ich", "er\u00b7f\u00fcllt", "dich", "mir", "den\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "PPER", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als der Menschheit erw\u00fcnschtestes Ziel:", "tokens": ["Als", "der", "Menschheit", "er\u00b7w\u00fcnschtes\u00b7tes", "Ziel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie klein und kleinlich erscheinst du mir!", "tokens": ["Wie", "klein", "und", "klein\u00b7lich", "er\u00b7scheinst", "du", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "H\u00f6her nicht als bis zur Noth", "tokens": ["H\u00f6\u00b7her", "nicht", "als", "bis", "zur", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "KOKOM", "APPR", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Erh\u00f6be sich des Geistes Flug,", "tokens": ["Er\u00b7h\u00f6\u00b7be", "sich", "des", "Geis\u00b7tes", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "M\u00e4\u00dfiger stets", "tokens": ["M\u00e4\u00b7\u00dfi\u00b7ger", "stets"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "W\u00fcrde der Empfindungen Pendelschwung,", "tokens": ["W\u00fcr\u00b7de", "der", "Emp\u00b7fin\u00b7dun\u00b7gen", "Pen\u00b7del\u00b7schwung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Bis das Dasein dem Uhrwerk gliche,", "tokens": ["Bis", "das", "Da\u00b7sein", "dem", "Uhr\u00b7werk", "gli\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Stunden weisend,", "tokens": ["Stun\u00b7den", "wei\u00b7send", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Nicht werth gelebt zu werden.", "tokens": ["Nicht", "werth", "ge\u00b7lebt", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und so will ich getrost mich", "tokens": ["Und", "so", "will", "ich", "ge\u00b7trost", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVFIN", "PPER"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Meine Spanne Zeit noch,", "tokens": ["Mei\u00b7ne", "Span\u00b7ne", "Zeit", "noch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie die Menschheit schon seit Jahrtausenden,", "tokens": ["Wie", "die", "Menschheit", "schon", "seit", "Jahr\u00b7tau\u00b7sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Hinqu\u00e4len", "tokens": ["Hin\u00b7qu\u00e4\u00b7len"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Zwischen lachender Thorheit \u2013 und unfroher Weisheit.", "tokens": ["Zwi\u00b7schen", "la\u00b7chen\u00b7der", "Thor\u00b7heit", "\u2013", "und", "un\u00b7fro\u00b7her", "Weis\u00b7heit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "KON", "ADJA", "NN", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}}}}}