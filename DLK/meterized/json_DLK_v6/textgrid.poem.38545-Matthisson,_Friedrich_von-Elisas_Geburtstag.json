{"textgrid.poem.38545": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Elisas Geburtstag", "genre": "verse", "period": "N.A.", "pub_year": 1779, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein gedenk' ich, o Freundin, mit Thr\u00e4nen des Danks und der Freude,", "tokens": ["Dein", "ge\u00b7denk'", "ich", ",", "o", "Freun\u00b7din", ",", "mit", "Thr\u00e4\u00b7nen", "des", "Danks", "und", "der", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "APPR", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Dein mit Gef\u00fchlen der Ruh',", "tokens": ["Dein", "mit", "Ge\u00b7f\u00fch\u00b7len", "der", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "NN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Hier auf dem schwellenden Rasen, beschattet vom bl\u00fchenden Kirschbaum,", "tokens": ["Hier", "auf", "dem", "schwel\u00b7len\u00b7den", "Ra\u00b7sen", ",", "be\u00b7schat\u00b7tet", "vom", "bl\u00fc\u00b7hen\u00b7den", "Kirschbaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+", "measure": "dactylic.tetra.plus"}, "line.4": {"text": "Wo, bei der Nachtigall Lied,", "tokens": ["Wo", ",", "bei", "der", "Nach\u00b7ti\u00b7gall", "Lied", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "J\u00fcngst dein weinendes Auge sich hellte, wo uns des Abends", "tokens": ["J\u00fcngst", "dein", "wei\u00b7nen\u00b7des", "Au\u00b7ge", "sich", "hell\u00b7te", ",", "wo", "uns", "des", "A\u00b7bends"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "PRF", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN"], "meter": "+-+--+--+-++-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Freundlicher Schimmer umflo\u00df,", "tokens": ["Freund\u00b7li\u00b7cher", "Schim\u00b7mer", "um\u00b7flo\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Ach! und begr\u00fcsse mit Himmelsempfindung den Morgen des Tages", "tokens": ["Ach", "!", "und", "be\u00b7gr\u00fcs\u00b7se", "mit", "Him\u00b7mel\u00b7semp\u00b7fin\u00b7dung", "den", "Mor\u00b7gen", "des", "Ta\u00b7ges"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KON", "VVFIN", "APPR", "NN", "ART", "NN", "ART", "NN"], "meter": "---+--+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Welcher der Erde dich gab.", "tokens": ["Wel\u00b7cher", "der", "Er\u00b7de", "dich", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.9": {"text": "Ruhig fliesse mein Lied und sanft, wie dein Leben, du Edle,", "tokens": ["Ru\u00b7hig", "flies\u00b7se", "mein", "Lied", "und", "sanft", ",", "wie", "dein", "Le\u00b7ben", ",", "du", "Ed\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "KON", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "$,", "PPER", "ADJA", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Wenn, am errungenen Ziel,", "tokens": ["Wenn", ",", "am", "er\u00b7run\u00b7ge\u00b7nen", "Ziel", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Einst die lohnende Mirthe dich kr\u00e4nzt und die Liebe zum Eden", "tokens": ["Einst", "die", "loh\u00b7nen\u00b7de", "Mir\u00b7the", "dich", "kr\u00e4nzt", "und", "die", "Lie\u00b7be", "zum", "E\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "PPER", "VVFIN", "KON", "ART", "NN", "APPRART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Dir deine Pfade versch\u00f6nt.", "tokens": ["Dir", "dei\u00b7ne", "Pfa\u00b7de", "ver\u00b7sch\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.13": {"text": "Als die erste Freudenthr\u00e4ne der redlichen Mutter", "tokens": ["Als", "die", "ers\u00b7te", "Freu\u00b7dent\u00b7hr\u00e4\u00b7ne", "der", "red\u00b7li\u00b7chen", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Ueber die Wange dir rann,", "tokens": ["Ue\u00b7ber", "die", "Wan\u00b7ge", "dir", "rann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.15": {"text": "Als zum erstenmal ihr Arm, mit s\u00fcsser Entz\u00fcckung,", "tokens": ["Als", "zum", "ers\u00b7ten\u00b7mal", "ihr", "Arm", ",", "mit", "s\u00fcs\u00b7ser", "Ent\u00b7z\u00fc\u00b7ckung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "ADV", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Um die Erflehte sich schlang:", "tokens": ["Um", "die", "Er\u00b7fleh\u00b7te", "sich", "schlang", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Siehe! da t\u00f6nte das Lied der Engel aus leuchtenden Wolken,", "tokens": ["Sie\u00b7he", "!", "da", "t\u00f6n\u00b7te", "das", "Lied", "der", "En\u00b7gel", "aus", "leuch\u00b7ten\u00b7den", "Wol\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Dich zu begr\u00fcssen, herab!", "tokens": ["Dich", "zu", "be\u00b7gr\u00fcs\u00b7sen", ",", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.19": {"text": "Schwester nannten sie dich und B\u00fcrgerin seliger Welten,", "tokens": ["Schwes\u00b7ter", "nann\u00b7ten", "sie", "dich", "und", "B\u00fcr\u00b7ge\u00b7rin", "se\u00b7li\u00b7ger", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "KON", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+---+-", "measure": "trochaic.hexa.relaxed"}, "line.20": {"text": "Weihten der Unschuld dein Herz,", "tokens": ["Weih\u00b7ten", "der", "Un\u00b7schuld", "dein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.21": {"text": "Stimmten zum lautersten Einklang mit Gottes Natur deine Seele,", "tokens": ["Stimm\u00b7ten", "zum", "lau\u00b7ters\u00b7ten", "Ein\u00b7klang", "mit", "Got\u00b7tes", "Na\u00b7tur", "dei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "APPR", "NN", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.22": {"text": "Gossen f\u00fcr Alles, was gro\u00df", "tokens": ["Gos\u00b7sen", "f\u00fcr", "Al\u00b7les", ",", "was", "gro\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "APPR", "PIS", "$,", "PRELS", "ADJD"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.23": {"text": "Gut und erhaben und sch\u00f6n ist, dir Flammengef\u00fchl in den Busen,", "tokens": ["Gut", "und", "er\u00b7ha\u00b7ben", "und", "sch\u00f6n", "ist", ",", "dir", "Flam\u00b7men\u00b7ge\u00b7f\u00fchl", "in", "den", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "KON", "ADJD", "VAFIN", "$,", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Bildeten sorgsam den Keim", "tokens": ["Bil\u00b7de\u00b7ten", "sorg\u00b7sam", "den", "Keim"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.25": {"text": "Zum begl\u00fcckenden Wonnegedanken: da\u00df Freundschaft und Liebe", "tokens": ["Zum", "be\u00b7gl\u00fc\u00b7cken\u00b7den", "Won\u00b7ne\u00b7ge\u00b7dan\u00b7ken", ":", "da\u00df", "Freund\u00b7schaft", "und", "Lie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "KOUS", "NN", "KON", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.26": {"text": "Jenseits der Gr\u00fcfte noch bl\u00fchn.", "tokens": ["Jen\u00b7seits", "der", "Gr\u00fcf\u00b7te", "noch", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.27": {"text": "Also begannen die S\u00f6hne des Lichtes, vereint mit der Harfe", "tokens": ["Al\u00b7so", "be\u00b7gan\u00b7nen", "die", "S\u00f6h\u00b7ne", "des", "Lich\u00b7tes", ",", "ver\u00b7eint", "mit", "der", "Har\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.28": {"text": "Bebendem Silberget\u00f6n:", "tokens": ["Be\u00b7ben\u00b7dem", "Sil\u00b7ber\u00b7ge\u00b7t\u00f6n", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.29": {"text": "\u00bbschwesterseele, willkommen auf Erden, holdseliges M\u00e4dchen,", "tokens": ["\u00bb", "schwes\u00b7ter\u00b7see\u00b7le", ",", "will\u00b7kom\u00b7men", "auf", "Er\u00b7den", ",", "hold\u00b7se\u00b7li\u00b7ges", "M\u00e4d\u00b7chen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "ADJD", "APPR", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.30": {"text": "Sei uns mit Wonne gegr\u00fc\u00dft!", "tokens": ["Sei", "uns", "mit", "Won\u00b7ne", "ge\u00b7gr\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.31": {"text": "Sanft umw\u00f6lkt sich dein Auge voll Unschuld am Busen der Mutter;", "tokens": ["Sanft", "um\u00b7w\u00f6lkt", "sich", "dein", "Au\u00b7ge", "voll", "Un\u00b7schuld", "am", "Bu\u00b7sen", "der", "Mut\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PPOSAT", "NN", "ADJD", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.32": {"text": "Ahndet, Geliebte, dein Herz", "tokens": ["Ahn\u00b7det", ",", "Ge\u00b7lieb\u00b7te", ",", "dein", "Herz"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "PPOSAT", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.33": {"text": "Schon in der Morgenr\u00f6the des Lebens die St\u00fcrme des Mittags?", "tokens": ["Schon", "in", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "des", "Le\u00b7bens", "die", "St\u00fcr\u00b7me", "des", "Mit\u00b7tags", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Zittert ein d\u00e4mmernd Gef\u00fchl", "tokens": ["Zit\u00b7tert", "ein", "d\u00e4m\u00b7mernd", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJD", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.35": {"text": "Jener n\u00e4chtlichen Tage des hofnungslosen Ermattens,", "tokens": ["Je\u00b7ner", "n\u00e4cht\u00b7li\u00b7chen", "Ta\u00b7ge", "des", "hof\u00b7nungs\u00b7lo\u00b7sen", "Er\u00b7mat\u00b7tens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.36": {"text": "Unter der beugenden Last", "tokens": ["Un\u00b7ter", "der", "beu\u00b7gen\u00b7den", "Last"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.37": {"text": "Unverschuldeter Schmerzen der Zukunft, dir bang durch die Seele?", "tokens": ["Un\u00b7ver\u00b7schul\u00b7de\u00b7ter", "Schmer\u00b7zen", "der", "Zu\u00b7kunft", ",", "dir", "bang", "durch", "die", "See\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.38": {"text": "Dornicht und rauh ist der Pfad,", "tokens": ["Dor\u00b7nicht", "und", "rauh", "ist", "der", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.39": {"text": "Den die ewige Liebe dich leiten wird, aber am Ausgang", "tokens": ["Den", "die", "e\u00b7wi\u00b7ge", "Lie\u00b7be", "dich", "lei\u00b7ten", "wird", ",", "a\u00b7ber", "am", "Aus\u00b7gang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "PPER", "VVINF", "VAFIN", "$,", "ADV", "APPRART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.40": {"text": "Schimmert die Krone des Lohns.", "tokens": ["Schim\u00b7mert", "die", "Kro\u00b7ne", "des", "Lohns", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.41": {"text": "Unschuld, Einfalt und Liebe, und jede gef\u00e4llige Tugend", "tokens": ["Un\u00b7schuld", ",", "Ein\u00b7falt", "und", "Lie\u00b7be", ",", "und", "je\u00b7de", "ge\u00b7f\u00e4l\u00b7li\u00b7ge", "Tu\u00b7gend"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "NN", "KON", "NN", "$,", "KON", "PIAT", "ADJA", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.42": {"text": "Schm\u00fccke, Geliebte, dich einst!", "tokens": ["Schm\u00fc\u00b7cke", ",", "Ge\u00b7lieb\u00b7te", ",", "dich", "einst", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.43": {"text": "Dann wird voll Hofnung und Ruh' und siegender Kraft, deine Seele", "tokens": ["Dann", "wird", "voll", "Hof\u00b7nung", "und", "Ruh'", "und", "sie\u00b7gen\u00b7der", "Kraft", ",", "dei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ADJD", "NN", "KON", "NN", "KON", "ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.44": {"text": "Mitten im Thale der Nacht,", "tokens": ["Mit\u00b7ten", "im", "Tha\u00b7le", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.45": {"text": "Wo kein leitendes Sternchen dir funkelt, die Vaterhand segnen", "tokens": ["Wo", "kein", "lei\u00b7ten\u00b7des", "Stern\u00b7chen", "dir", "fun\u00b7kelt", ",", "die", "Va\u00b7ter\u00b7hand", "seg\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "PPER", "VVFIN", "$,", "ART", "NN", "VVINF"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.46": {"text": "Welche durch W\u00fcsten dich f\u00fchrt.", "tokens": ["Wel\u00b7che", "durch", "W\u00fcs\u00b7ten", "dich", "f\u00fchrt", "."], "token_info": ["word", 