{"textgrid.poem.56998": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Philanthropisch", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein nerv\u00f6ser Mensch auf einer Wiese", "tokens": ["Ein", "ner\u00b7v\u00f6\u00b7ser", "Mensch", "auf", "ei\u00b7ner", "Wie\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "w\u00e4re besser ohne sie daran;", "tokens": ["w\u00e4\u00b7re", "bes\u00b7ser", "oh\u00b7ne", "sie", "da\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PPER", "PAV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "darum seh er, wie er ohne diese", "tokens": ["da\u00b7rum", "seh", "er", ",", "wie", "er", "oh\u00b7ne", "die\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "APPR", "PDAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "(meistens mindstens) leben kann.", "tokens": ["(", "meis\u00b7tens", "minds\u00b7tens", ")", "le\u00b7ben", "kann", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "$(", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kaum da\u00df er gelegt sich auf die Gr\u00e4ser,", "tokens": ["Kaum", "da\u00df", "er", "ge\u00b7legt", "sich", "auf", "die", "Gr\u00e4\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVPP", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "naht der Ameis, Heuschreck, M\u00fcck und Wurm,", "tokens": ["naht", "der", "Am\u00b7eis", ",", "Heu\u00b7schreck", ",", "M\u00fcck", "und", "Wurm", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "naht der Tausendfu\u00df und Ohrenbl\u00e4ser,", "tokens": ["naht", "der", "Tau\u00b7send\u00b7fu\u00df", "und", "Oh\u00b7ren\u00b7bl\u00e4\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und der Hummel ruft zum Sturm.", "tokens": ["und", "der", "Hum\u00b7mel", "ruft", "zum", "Sturm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein nerv\u00f6ser Mensch auf einer Wiese", "tokens": ["Ein", "ner\u00b7v\u00f6\u00b7ser", "Mensch", "auf", "ei\u00b7ner", "Wie\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "tut drum besser, wieder aufzustehn", "tokens": ["tut", "drum", "bes\u00b7ser", ",", "wie\u00b7der", "auf\u00b7zu\u00b7stehn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PAV", "ADJD", "$,", "ADV", "VVINF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "und daf\u00fcr in andre Paradiese", "tokens": ["und", "da\u00b7f\u00fcr", "in", "and\u00b7re", "Pa\u00b7ra\u00b7die\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "(beispielshalber: weg) zu gehn.", "tokens": ["(", "bei\u00b7spiels\u00b7hal\u00b7ber", ":", "weg", ")", "zu", "gehn", "."], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "ADV", "$(", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}