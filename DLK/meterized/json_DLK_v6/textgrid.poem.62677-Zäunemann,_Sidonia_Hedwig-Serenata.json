{"textgrid.poem.62677": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Serenata", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wehle mir ein Weib", "tokens": ["Ich", "weh\u00b7le", "mir", "ein", "Weib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Von hundert tausend Gulden,", "tokens": ["Von", "hun\u00b7dert", "tau\u00b7send", "Gul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu Tilgung meiner Schulden.", "tokens": ["Zu", "Til\u00b7gung", "mei\u00b7ner", "Schul\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir macht der Secten-Schwarm,", "tokens": ["Mir", "macht", "der", "Sec\u00b7ten\u00b7Schwarm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Manich\u00e4er warm.", "tokens": ["Der", "Ma\u00b7nic\u00b7h\u00e4\u00b7er", "warm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Drum ist vor meinen Leib", "tokens": ["Drum", "ist", "vor", "mei\u00b7nen", "Leib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der beste Zeitvertreib", "tokens": ["Der", "bes\u00b7te", "Zeit\u00b7ver\u00b7treib"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ein wohlbeg\u00fctert Weib.", "tokens": ["Ein", "wohl\u00b7be\u00b7g\u00fc\u00b7tert", "Weib", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ich lobe mir ein reiches M\u00e4dgen", "tokens": ["Ich", "lo\u00b7be", "mir", "ein", "rei\u00b7ches", "M\u00e4d\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Das Haus und Hof, und viele Gelder hat.", "tokens": ["Das", "Haus", "und", "Hof", ",", "und", "vie\u00b7le", "Gel\u00b7der", "hat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "KON", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Bey angef\u00fcllten Kasten", "tokens": ["Bey", "an\u00b7ge\u00b7f\u00fcll\u00b7ten", "Kas\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Ists warlich wohl zurasten.", "tokens": ["Ists", "war\u00b7lich", "wohl", "zu\u00b7ras\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Die Sch\u00f6nheit macht den Leib nicht sat.", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "macht", "den", "Leib", "nicht", "sat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wer sich ein Weib erwehlet ohne Geld,", "tokens": ["Wer", "sich", "ein", "Weib", "er\u00b7weh\u00b7let", "oh\u00b7ne", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der hat sein Haus, sein Gl\u00fcck auf schlechten Grund gestellt.", "tokens": ["Der", "hat", "sein", "Haus", ",", "sein", "Gl\u00fcck", "auf", "schlech\u00b7ten", "Grund", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Weg Sch\u00f6nheit und Verstand, weg artge Gaben;", "tokens": ["Weg", "Sch\u00f6n\u00b7heit", "und", "Ver\u00b7stand", ",", "weg", "art\u00b7ge", "Ga\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Weg Munterkeit und Jugend;", "tokens": ["Weg", "Mun\u00b7ter\u00b7keit", "und", "Ju\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Weg H\u00e4u\u00dflichkeit; weg Zucht und Tugend,", "tokens": ["Weg", "H\u00e4u\u00df\u00b7lich\u00b7keit", ";", "weg", "Zucht", "und", "Tu\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Mich soll ein reiches M\u00e4dgen laben.", "tokens": ["Mich", "soll", "ein", "rei\u00b7ches", "M\u00e4d\u00b7gen", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Ich finde mein Vergn\u00fcgen,", "tokens": ["Ich", "fin\u00b7de", "mein", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Wo alte Thaler liegen.", "tokens": ["Wo", "al\u00b7te", "Tha\u00b7ler", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Mir gef\u00e4llt,", "tokens": ["Mir", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "Nur das Geld.", "tokens": ["Nur", "das", "Geld", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}