{"textgrid.poem.25027": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: O rede fort! Wie Weihges\u00e4nge", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O rede fort! Wie Weihges\u00e4nge", "tokens": ["O", "re\u00b7de", "fort", "!", "Wie", "Weih\u00b7ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PTKVZ", "$.", "PWAV", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "T\u00f6nt deine Stimme mir ans Ohr;", "tokens": ["T\u00f6nt", "dei\u00b7ne", "Stim\u00b7me", "mir", "ans", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was herrlich in der Welt der Kl\u00e4nge,", "tokens": ["Was", "herr\u00b7lich", "in", "der", "Welt", "der", "Kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Eint sich in ihr zum vollen Chor;", "tokens": ["Eint", "sich", "in", "ihr", "zum", "vol\u00b7len", "Chor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In ihr der Plauderton der Quelle,", "tokens": ["In", "ihr", "der", "Plau\u00b7der\u00b7ton", "der", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Felsengrotten Wiederhall", "tokens": ["Der", "Fel\u00b7sen\u00b7grot\u00b7ten", "Wie\u00b7der\u00b7hall"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit dem Gebraus der Wasserf\u00e4lle,", "tokens": ["Mit", "dem", "Ge\u00b7braus", "der", "Was\u00b7ser\u00b7f\u00e4l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Fr\u00fchlingslied der Nachtigall,", "tokens": ["Dem", "Fr\u00fch\u00b7lings\u00b7lied", "der", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In ihr mit m\u00e4cht'gem Waldesrauschen", "tokens": ["In", "ihr", "mit", "m\u00e4cht'\u00b7gem", "Wal\u00b7des\u00b7rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Lenzluft erster Atemzug; \u2013", "tokens": ["Der", "Lenz\u00b7luft", "ers\u00b7ter", "A\u00b7tem\u00b7zug", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr eine Stunde stumm zu lauschen,", "tokens": ["Ihr", "ei\u00b7ne", "Stun\u00b7de", "stumm", "zu", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist f\u00fcr das Leben Gl\u00fcck genug.", "tokens": ["Ist", "f\u00fcr", "das", "Le\u00b7ben", "Gl\u00fcck", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}