{"dta.poem.2865": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "66.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Begriffen hast du, doch damit ists nicht gethan;", "tokens": ["Be\u00b7grif\u00b7fen", "hast", "du", ",", "doch", "da\u00b7mit", "ists", "nicht", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "ADV", "PAV", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun lern' es auch, dann erst geh\u00f6rt es ganz dir an.", "tokens": ["Nun", "lern'", "es", "auch", ",", "dann", "erst", "ge\u00b7h\u00f6rt", "es", "ganz", "dir", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ADV", "ADV", "VVFIN", "PPER", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es ist ein Unterschied, begriffen und gelernt;", "tokens": ["Es", "ist", "ein", "Un\u00b7ter\u00b7schied", ",", "be\u00b7grif\u00b7fen", "und", "ge\u00b7lernt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beim ersten Schritt ist man noch weit vom Ziel entfernt.", "tokens": ["Beim", "ers\u00b7ten", "Schritt", "ist", "man", "noch", "weit", "vom", "Ziel", "ent\u00b7fernt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PIS", "ADV", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch, ist auf rechter Bahn der erste Schritt gethan,", "tokens": ["Doch", ",", "ist", "auf", "rech\u00b7ter", "Bahn", "der", "ers\u00b7te", "Schritt", "ge\u00b7than", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So kommt das Ziel von selbst, halt nur den Schritt nicht an!", "tokens": ["So", "kommt", "das", "Ziel", "von", "selbst", ",", "halt", "nur", "den", "Schritt", "nicht", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADV", "$,", "VVFIN", "ADV", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das recht begriffene ist leicht zu lernen nun;", "tokens": ["Das", "recht", "be\u00b7grif\u00b7fe\u00b7ne", "ist", "leicht", "zu", "ler\u00b7nen", "nun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "VAFIN", "ADJD", "PTKZU", "VVINF", "ADV", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Doch lernen mu\u00dft du es, sonst kannst du es nicht thun.", "tokens": ["Doch", "ler\u00b7nen", "mu\u00dft", "du", "es", ",", "sonst", "kannst", "du", "es", "nicht", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PPER", "$,", "ADV", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}