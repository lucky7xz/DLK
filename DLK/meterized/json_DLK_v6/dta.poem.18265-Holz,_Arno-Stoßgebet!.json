{"dta.poem.18265": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Sto\u00dfgebet!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Lehre mich vollbringen hier,", "tokens": ["Leh\u00b7re", "mich", "voll\u00b7brin\u00b7gen", "hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mein Schutzpatron, der Heine,", "tokens": ["Und", "mein", "Schutz\u00b7pat\u00b7ron", ",", "der", "Hei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00e4rfe meine Klingen mir;", "tokens": ["Sch\u00e4r\u00b7fe", "mei\u00b7ne", "Klin\u00b7gen", "mir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "G\u00fcrt mein Herz mit Siegfriedsleder,", "tokens": ["G\u00fcrt", "mein", "Herz", "mit", "Sieg\u00b7frieds\u00b7le\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gie\u00df ins Hirn mir tausend Lichter", "tokens": ["Gie\u00df", "ins", "Hirn", "mir", "tau\u00b7send", "Lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und befiehl in meine Feder", "tokens": ["Und", "be\u00b7fiehl", "in", "mei\u00b7ne", "Fe\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Unsre sogenannten Dichter;", "tokens": ["Uns\u00b7re", "so\u00b7ge\u00b7nann\u00b7ten", "Dich\u00b7ter", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dichter, deren ganzer Codex", "tokens": ["Dich\u00b7ter", ",", "de\u00b7ren", "gan\u00b7zer", "Co\u00b7dex"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELAT", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Essen, Trinken, Trinken, Essen,", "tokens": ["Es\u00b7sen", ",", "Trin\u00b7ken", ",", "Trin\u00b7ken", ",", "Es\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Dichter, die sich in den Podex,", "tokens": ["Dich\u00b7ter", ",", "die", "sich", "in", "den", "Po\u00b7dex", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "H\u00e4morrhoiden eingesessen!", "tokens": ["H\u00e4\u00b7morr\u00b7ho\u00b7i\u00b7den", "ein\u00b7ge\u00b7ses\u00b7sen", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Gr\u00fc\u00df Gott, ihr Folianten,", "tokens": ["Gr\u00fc\u00df", "Gott", ",", "ihr", "Fo\u00b7li\u00b7an\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Hurrah in den Tod!", "tokens": ["Hur\u00b7rah", "in", "den", "Tod", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Spielt auf, Musikanten,", "tokens": ["Spielt", "auf", ",", "Mu\u00b7si\u00b7kan\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Das Eine thut Noth!", "tokens": ["Das", "Ei\u00b7ne", "thut", "Noth", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}