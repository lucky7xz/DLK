{"textgrid.poem.48136": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Flimmernde Tiefen", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schau durch die schimmernden Wasser", "tokens": ["Ich", "schau", "durch", "die", "schim\u00b7mern\u00b7den", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "in die flimmernden Tiefen hinein:", "tokens": ["in", "die", "flim\u00b7mern\u00b7den", "Tie\u00b7fen", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "da schl\u00e4ft eine tote Hexe", "tokens": ["da", "schl\u00e4ft", "ei\u00b7ne", "to\u00b7te", "He\u00b7xe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "auf einem gr\u00fcnen Flutgestein.", "tokens": ["auf", "ei\u00b7nem", "gr\u00fc\u00b7nen", "Flut\u00b7ge\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Noch liegt die l\u00e4chelnde L\u00fcge", "tokens": ["Noch", "liegt", "die", "l\u00e4\u00b7cheln\u00b7de", "L\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "um den schmalen, gepre\u00dften Mund . . .", "tokens": ["um", "den", "schma\u00b7len", ",", "ge\u00b7pre\u00df\u00b7ten", "Mund", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$.", "$.", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "ihre roten Haare lodern", "tokens": ["ih\u00b7re", "ro\u00b7ten", "Haa\u00b7re", "lo\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wie Flammen aus dem Grund.", "tokens": ["wie", "Flam\u00b7men", "aus", "dem", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}