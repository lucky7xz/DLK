{"dta.poem.19350": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "83.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein eisernes Gesetz hat gleiche Strafe, Tod,", "tokens": ["Ein", "ei\u00b7ser\u00b7nes", "Ge\u00b7setz", "hat", "glei\u00b7che", "Stra\u00b7fe", ",", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verschiedensten Vergehn, gro\u00df oder klein, gedroht.", "tokens": ["Ver\u00b7schie\u00b7dens\u00b7ten", "Ver\u00b7gehn", ",", "gro\u00df", "o\u00b7der", "klein", ",", "ge\u00b7droht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein mildres aber raubt ihm seine Kraft, und glaubt,", "tokens": ["Ein", "mild\u00b7res", "a\u00b7ber", "raubt", "ihm", "sei\u00b7ne", "Kraft", ",", "und", "glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch gegen T\u00f6dtung selbst sei T\u00f6dtung unerlaubt.", "tokens": ["Auch", "ge\u00b7gen", "T\u00f6d\u00b7tung", "selbst", "sei", "T\u00f6d\u00b7tung", "un\u00b7er\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADV", "VAFIN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Von beiden welch's hat Recht? hat Recht villeicht das dritte,", "tokens": ["Von", "bei\u00b7den", "welch's", "hat", "Recht", "?", "hat", "Recht", "vil\u00b7leicht", "das", "drit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PIS", "VAFIN", "NN", "$.", "VAFIN", "NN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das zwischen beiden h\u00e4lt der Unterscheidung Mitte?", "tokens": ["Das", "zwi\u00b7schen", "bei\u00b7den", "h\u00e4lt", "der", "Un\u00b7ter\u00b7schei\u00b7dung", "Mit\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PIS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Recht haben beide. Tod verdienen all, die s\u00fcndigen;", "tokens": ["Recht", "ha\u00b7ben", "bei\u00b7de", ".", "Tod", "ver\u00b7die\u00b7nen", "all", ",", "die", "s\u00fcn\u00b7di\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "$.", "NN", "VVFIN", "PIAT", "$,", "PRELS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Doch wer ist s\u00fcndlos gnug, es ihnen anzuk\u00fcndigen?", "tokens": ["Doch", "wer", "ist", "s\u00fcnd\u00b7los", "gnug", ",", "es", "ih\u00b7nen", "an\u00b7zu\u00b7k\u00fcn\u00b7di\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "ADJD", "ADV", "$,", "PPER", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}