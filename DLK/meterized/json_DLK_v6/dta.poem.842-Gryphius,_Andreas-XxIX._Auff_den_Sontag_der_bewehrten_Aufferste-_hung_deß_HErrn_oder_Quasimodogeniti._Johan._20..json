{"dta.poem.842": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxIX.  Auff den Sontag der bewehrten Aufferste-  \n hung de\u00df HErrn/ oder  Quasimodogeniti.  \n Johan. 20.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Was trawr ich? Hat der Feind gleich f\u00fcr vnd f\u00fcr gespon- ", "tokens": ["Was", "trawr", "ich", "?", "Hat", "der", "Feind", "gleich", "f\u00fcr", "vnd", "f\u00fcr", "ge\u00b7spon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$.", "VAFIN", "ART", "NN", "ADV", "APPR", "KON", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "uen ", "tokens": ["uen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Zu meinem J\u00e4gergarn: mu\u00df gleich mein K\u00e4mmerlein", "tokens": ["Zu", "mei\u00b7nem", "J\u00e4\u00b7ger\u00b7garn", ":", "mu\u00df", "gleich", "mein", "K\u00e4m\u00b7mer\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "VMFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Di\u00df enge Bilgerhau\u00df stets fest-verrigelt seyn!", "tokens": ["Di\u00df", "en\u00b7ge", "Bil\u00b7ger\u00b7hau\u00df", "stets", "fest\u00b7ver\u00b7ri\u00b7gelt", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was trawr ich! ob es Nacht! dieweil die helle Sonnen", "tokens": ["Was", "trawr", "ich", "!", "ob", "es", "Nacht", "!", "die\u00b7weil", "die", "hel\u00b7le", "Son\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$.", "KOUS", "PPER", "NN", "$.", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dem Printz der ", "tokens": ["Dem", "Printz", "der"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Vnd Licht vnd Frieden bringt! wenn aller trost zu klein/", "tokens": ["Vnd", "Licht", "vnd", "Frie\u00b7den", "bringt", "!", "wenn", "al\u00b7ler", "trost", "zu", "klein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "$.", "KOUS", "PIAT", "NN", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd ich verlassen bin/ tritt ", "tokens": ["Vnd", "ich", "ver\u00b7las\u00b7sen", "bin", "/", "tritt"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "VVPP", "VAFIN", "$(", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "So bald er komm\u2019t/ so dald ist was mich kr\u00e4nckt/ zerronnen!", "tokens": ["So", "bald", "er", "kom\u00b7m't", "/", "so", "dald", "ist", "was", "mich", "kr\u00e4nckt", "/", "zer\u00b7ron\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$(", "ADV", "ADV", "VAFIN", "PWS", "PPER", "VVFIN", "$(", "VVINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Er ist mein ", "tokens": ["Er", "ist", "mein"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Ich schaw\u2019 durch seine Seitt/ wie tieff sein Hertz entbrand.", "tokens": ["Ich", "scha\u00b7w'", "durch", "sei\u00b7ne", "Seitt", "/", "wie", "tieff", "sein", "Hertz", "ent\u00b7brand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$(", "PWAV", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Wer zweifelt/ komm\u2019 vnd schaw. vor diesen offnen Wunden", "tokens": ["Wer", "zwei\u00b7felt", "/", "komm'", "vnd", "schaw", ".", "vor", "die\u00b7sen", "off\u00b7nen", "Wun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$(", "VVFIN", "KON", "ADJD", "$.", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Verf\u00e4lt der wortte pracht/ di\u00df zeugn\u00fc\u00df ist zu war", "tokens": ["Ver\u00b7f\u00e4lt", "der", "wort\u00b7te", "pracht", "/", "di\u00df", "zeug\u00b7n\u00fc\u00df", "ist", "zu", "war"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "$(", "PDS", "VVFIN", "VAFIN", "ADV", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Di\u00df Blutt fleu\u00dft viel zu frisch/ die Liebe scheint zu klar", "tokens": ["Di\u00df", "Blutt", "fleu\u00dft", "viel", "zu", "frisch", "/", "die", "Lie\u00b7be", "scheint", "zu", "klar"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "VVFIN", "ADV", "PTKA", "ADJD", "$(", "ART", "NN", "VVFIN", "PTKA", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier wird ein offen Hertz mit ofner hand gefunden.", "tokens": ["Hier", "wird", "ein", "of\u00b7fen", "Hertz", "mit", "of\u00b7ner", "hand", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJD", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}