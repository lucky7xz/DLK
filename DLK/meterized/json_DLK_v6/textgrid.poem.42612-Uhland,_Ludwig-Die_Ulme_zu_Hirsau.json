{"textgrid.poem.42612": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Ulme zu Hirsau", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Hirsau in den Tr\u00fcmmern,", "tokens": ["Zu", "Hir\u00b7sau", "in", "den", "Tr\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da wiegt ein Ulmenbaum", "tokens": ["Da", "wiegt", "ein", "Ul\u00b7men\u00b7baum"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Frischgr\u00fcnend seine Krone", "tokens": ["Frischgr\u00fc\u00b7nend", "sei\u00b7ne", "Kro\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Hoch \u00fcberm Giebelsaum.", "tokens": ["Hoch", "\u00fc\u00b7berm", "Gie\u00b7bel\u00b7saum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er wurzelt tief im Grunde", "tokens": ["Er", "wur\u00b7zelt", "tief", "im", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom alten Klosterbau,", "tokens": ["Vom", "al\u00b7ten", "Klos\u00b7ter\u00b7bau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er w\u00f6lbt sich statt des Daches", "tokens": ["Er", "w\u00f6lbt", "sich", "statt", "des", "Da\u00b7ches"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hinaus in Himmelsblau.", "tokens": ["Hin\u00b7aus", "in", "Him\u00b7mels\u00b7blau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Weil des Gem\u00e4uers Enge", "tokens": ["Weil", "des", "Ge\u00b7m\u00e4u\u00b7ers", "En\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihm Luft und Sonne nahm,", "tokens": ["Ihm", "Luft", "und", "Son\u00b7ne", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So trieb's ihn hoch und h\u00f6her,", "tokens": ["So", "trie\u00b7b's", "ihn", "hoch", "und", "h\u00f6\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bis er zum Lichte kam.", "tokens": ["Bis", "er", "zum", "Lich\u00b7te", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es ragen die vier W\u00e4nde,", "tokens": ["Es", "ra\u00b7gen", "die", "vier", "W\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als ob sie nur bestimmt,", "tokens": ["Als", "ob", "sie", "nur", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den k\u00fchnen Wuchs zu schirmen,", "tokens": ["Den", "k\u00fch\u00b7nen", "Wuchs", "zu", "schir\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der zu den Wolken klimmt.", "tokens": ["Der", "zu", "den", "Wol\u00b7ken", "klimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wenn dort im gr\u00fcnen Tale", "tokens": ["Wenn", "dort", "im", "gr\u00fc\u00b7nen", "Ta\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich einsam mich erging,", "tokens": ["Ich", "ein\u00b7sam", "mich", "er\u00b7ging", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Ulme war's, die hehre,", "tokens": ["Die", "Ul\u00b7me", "wa\u00b7r's", ",", "die", "heh\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+----+-", "measure": "dactylic.init"}, "line.4": {"text": "Woran mein Sinnen hing.", "tokens": ["Wo\u00b7ran", "mein", "Sin\u00b7nen", "hing", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wenn in dem dumpfen, stummen", "tokens": ["Wenn", "in", "dem", "dum\u00b7pfen", ",", "stum\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Getr\u00fcmmer ich gelauscht,", "tokens": ["Ge\u00b7tr\u00fcm\u00b7mer", "ich", "ge\u00b7lauscht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da hat ihr reger Wipfel", "tokens": ["Da", "hat", "ihr", "re\u00b7ger", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Windesflug gerauscht.", "tokens": ["Im", "Win\u00b7des\u00b7flug", "ge\u00b7rauscht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich sah ihn oft ergl\u00fchen", "tokens": ["Ich", "sah", "ihn", "oft", "er\u00b7gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im ersten Morgenstrahl;", "tokens": ["Im", "ers\u00b7ten", "Mor\u00b7gen\u00b7strahl", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sah ihn noch erleuchtet,", "tokens": ["Ich", "sah", "ihn", "noch", "er\u00b7leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wann schattig rings das Tal.", "tokens": ["Wann", "schat\u00b7tig", "rings", "das", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Zu Wittenberg im Kloster", "tokens": ["Zu", "Wit\u00b7ten\u00b7berg", "im", "Klos\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wuchs auch ein solcher Strau\u00df", "tokens": ["Wuchs", "auch", "ein", "sol\u00b7cher", "Strau\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und brach mit Riesen\u00e4sten", "tokens": ["Und", "brach", "mit", "Rie\u00b7sen\u00b7\u00e4s\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Klausendach hinaus.", "tokens": ["Zum", "Klau\u00b7sen\u00b7dach", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O Strahl des Lichts! du dringest", "tokens": ["O", "Strahl", "des", "Lichts", "!", "du", "drin\u00b7gest"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinab in jede Gruft.", "tokens": ["Hin\u00b7ab", "in", "je\u00b7de", "Gruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Geist der Welt! du ringest", "tokens": ["O", "Geist", "der", "Welt", "!", "du", "rin\u00b7gest"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hinauf in Licht und Luft.", "tokens": ["Hin\u00b7auf", "in", "Licht", "und", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}