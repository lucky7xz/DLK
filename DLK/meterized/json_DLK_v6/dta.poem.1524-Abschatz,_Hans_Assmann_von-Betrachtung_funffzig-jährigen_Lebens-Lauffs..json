{"dta.poem.1524": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung funffzig-j\u00e4hrigen  \n Lebens-Lauffs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Des Leydens Maa\u00df ist voll/ mein Heyland ist erblichen! ", "tokens": ["Des", "Ley\u00b7dens", "Maa\u00df", "ist", "voll", "/", "mein", "Hey\u00b7land", "ist", "er\u00b7bli\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "$(", "PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Seht/ wie der holde Mund nach leztem Ruffen schweigt:", "tokens": ["Seht", "/", "wie", "der", "hol\u00b7de", "Mund", "nach", "lez\u00b7tem", "Ruf\u00b7fen", "schweigt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOKOM", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie durch des Speeres Stich das lezte Blutt entwichen/", "tokens": ["Wie", "durch", "des", "Spee\u00b7res", "Stich", "das", "lez\u00b7te", "Blutt", "ent\u00b7wi\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie sich das m\u00fcde Haubt vom Creutz herunter neigt:", "tokens": ["Wie", "sich", "das", "m\u00fc\u00b7de", "Haubt", "vom", "Creutz", "her\u00b7un\u00b7ter", "neigt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "ADJA", "NN", "APPRART", "NN", "APZR", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Glieder hangen welck/ die ausgespannten Sehnen", "tokens": ["Die", "Glie\u00b7der", "han\u00b7gen", "welck", "/", "die", "aus\u00b7ge\u00b7spann\u00b7ten", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erstarrt: Die fromme Schaar vergeust viel Todten-Thr\u00e4-", "tokens": ["Er\u00b7starrt", ":", "Die", "from\u00b7me", "Schaar", "ver\u00b7geust", "viel", "Tod\u00b7ten\u00b7Thr\u00e4"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$.", "ART", "ADJA", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O Seele/ billig ists/ den HErren zu beweinen:", "tokens": ["O", "See\u00b7le", "/", "bil\u00b7lig", "ists", "/", "den", "Her\u00b7ren", "zu", "be\u00b7wei\u00b7nen", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "ADJD", "VAFIN", "$(", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das reinste Seelen-Blutt soll ihm geopffert seyn/", "tokens": ["Das", "reins\u00b7te", "See\u00b7len\u00b7Blutt", "soll", "ihm", "ge\u00b7opf\u00b7fert", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "La\u00df deine Lieb und Treu zu ihm noch ferner scheinen/", "tokens": ["La\u00df", "dei\u00b7ne", "Lieb", "und", "Treu", "zu", "ihm", "noch", "fer\u00b7ner", "schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "KON", "NN", "APPR", "PPER", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und seinen Leichnam nicht am Holtze mehr entweyhn:", "tokens": ["Und", "sei\u00b7nen", "Leich\u00b7nam", "nicht", "am", "Holt\u00b7ze", "mehr", "ent\u00b7weyhn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es bitt ihn Joseph lo\u00df/ so wirst du dich nicht sch\u00e4men/", "tokens": ["Es", "bitt", "ihn", "Jo\u00b7se\u00b7ph", "lo\u00df", "/", "so", "wirst", "du", "dich", "nicht", "sch\u00e4\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NE", "PTKVZ", "$(", "ADV", "VAFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$("], "meter": "-+----+-+-+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Ihn von des Creutzes Stamm in deinen Arm zu nehmen.", "tokens": ["Ihn", "von", "des", "Creut\u00b7zes", "Stamm", "in", "dei\u00b7nen", "Arm", "zu", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bringt Leitern her/ den Baum des Sieges zu besteigen/", "tokens": ["Bringt", "Lei\u00b7tern", "her", "/", "den", "Baum", "des", "Sie\u00b7ges", "zu", "be\u00b7stei\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "$(", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Daran der grosse Held im Tode triumphirt:", "tokens": ["Da\u00b7ran", "der", "gros\u00b7se", "Held", "im", "To\u00b7de", "tri\u00b7um\u00b7phirt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Di\u00df ist die Seule die der Simson muste neigen/", "tokens": ["Di\u00df", "ist", "die", "Seu\u00b7le", "die", "der", "Sim\u00b7son", "mus\u00b7te", "nei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "ART", "NE", "VMFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Dagons Schwarm verf\u00e4llt/ wenn diese wird ger\u00fchrt.", "tokens": ["Des", "Da\u00b7gons", "Schwarm", "ver\u00b7f\u00e4llt", "/", "wenn", "die\u00b7se", "wird", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "KOUS", "PDS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zieht nun die N\u00e4gel aus/ und legt die bleichen Glieder/", "tokens": ["Zieht", "nun", "die", "N\u00e4\u00b7gel", "aus", "/", "und", "legt", "die", "blei\u00b7chen", "Glie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$(", "KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zur Reinigung vom Blutt/ auff saubre T\u00fccher nieder.", "tokens": ["Zur", "Rei\u00b7ni\u00b7gung", "vom", "Blutt", "/", "auff", "saub\u00b7re", "T\u00fc\u00b7cher", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$(", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wo find ich einen Ort bey diesen rauhen Klippen/", "tokens": ["Wo", "find", "ich", "ei\u00b7nen", "Ort", "bey", "die\u00b7sen", "rau\u00b7hen", "Klip\u00b7pen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da man des HErren Leib gem\u00e4chlich strecken kan?", "tokens": ["Da", "man", "des", "Her\u00b7ren", "Leib", "ge\u00b7m\u00e4ch\u00b7lich", "stre\u00b7cken", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "NN", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach wehle meine Scho\u00df! Ach nimm von meinen Lippen/", "tokens": ["Ach", "weh\u00b7le", "mei\u00b7ne", "Scho\u00df", "!", "Ach", "nimm", "von", "mei\u00b7nen", "Lip\u00b7pen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPOSAT", "NN", "$.", "ITJ", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verstorbner Lebens-HErr/ den Ku\u00df im Glauben an.", "tokens": ["Ver\u00b7storb\u00b7ner", "Le\u00b7bens\u00b7HErr", "/", "den", "Ku\u00df", "im", "Glau\u00b7ben", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "La\u00df mich dein Todten-Bild fest in mein Hertze dr\u00fccken/", "tokens": ["La\u00df", "mich", "dein", "Tod\u00b7ten\u00b7Bild", "fest", "in", "mein", "Hert\u00b7ze", "dr\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "ADJD", "APPR", "PPOSAT", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und mich in lezter Angst mit reichem Trost erquicken.", "tokens": ["Und", "mich", "in", "lez\u00b7ter", "Angst", "mit", "rei\u00b7chem", "Trost", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich kan dich nicht so bald aus meinen Armen geben;", "tokens": ["Ich", "kan", "dich", "nicht", "so", "bald", "aus", "mei\u00b7nen", "Ar\u00b7men", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zwar ob dem Schmertzen-Bild erschauret mir die Haut.", "tokens": ["Zwar", "ob", "dem", "Schmert\u00b7zen\u00b7Bild", "er\u00b7schau\u00b7ret", "mir", "die", "Haut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Haubt ist voller Blutt/ an dem die Haare kleben/", "tokens": ["Dein", "Haubt", "ist", "vol\u00b7ler", "Blutt", "/", "an", "dem", "die", "Haa\u00b7re", "kle\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$(", "APPR", "PRELS", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Stirne gantz durchrizt/ die man verfallen schaut/", "tokens": ["Die", "Stir\u00b7ne", "gantz", "durch\u00b7rizt", "/", "die", "man", "ver\u00b7fal\u00b7len", "schaut", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$(", "PRELS", "PIS", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Augen Glantz ist weg/ der Wangen Schein entwichen/", "tokens": ["Der", "Au\u00b7gen", "Glantz", "ist", "weg", "/", "der", "Wan\u00b7gen", "Schein", "ent\u00b7wi\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "$(", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der s\u00fcsse Mund gesperrt/ der Lippen Zier verblichen.", "tokens": ["Der", "s\u00fcs\u00b7se", "Mund", "ge\u00b7sperrt", "/", "der", "Lip\u00b7pen", "Zier", "ver\u00b7bli\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$(", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die H\u00e4nde sind durchbort/ die F\u00fcsse sind durchgraben/", "tokens": ["Die", "H\u00e4n\u00b7de", "sind", "durch\u00b7bort", "/", "die", "F\u00fcs\u00b7se", "sind", "durch\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der R\u00fccken ist durchpfl\u00fcgt/ die Glieder ausgereckt/", "tokens": ["Der", "R\u00fc\u00b7cken", "ist", "durch\u00b7pfl\u00fcgt", "/", "die", "Glie\u00b7der", "aus\u00b7ge\u00b7reckt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als sie dir Arm und Bein an Pfahl gespannet haben/", "tokens": ["Als", "sie", "dir", "Arm", "und", "Bein", "an", "Pfahl", "ge\u00b7span\u00b7net", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "KON", "NN", "APPR", "NN", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der gantze Leib mit Blutt und Striemen \u00fcberdeckt.", "tokens": ["Der", "gant\u00b7ze", "Leib", "mit", "Blutt", "und", "Strie\u00b7men", "\u00fc\u00b7berd\u00b7eckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJD", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo find ich einen Ort an Schultern/ Lenden/ Rieben/", "tokens": ["Wo", "find", "ich", "ei\u00b7nen", "Ort", "an", "Schul\u00b7tern", "/", "Len\u00b7den", "/", "Rie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$(", "NE", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der frey von Striemen/ Blutt und Beul und Eiter blieben?", "tokens": ["Der", "frey", "von", "Strie\u00b7men", "/", "Blutt", "und", "Beul", "und", "Ei\u00b7ter", "blie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "$(", "ADJD", "KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "So warst du durch und durch bi\u00df an den Tod voll Schmertzen:", "tokens": ["So", "warst", "du", "durch", "und", "durch", "bi\u00df", "an", "den", "Tod", "voll", "Schmert\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "KON", "APPR", "ADV", "APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Voll Blutt/ doch aber auch voll heisser Liebes-Glutt.", "tokens": ["Voll", "Blutt", "/", "doch", "a\u00b7ber", "auch", "voll", "heis\u00b7ser", "Lie\u00b7bes\u00b7Glutt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$(", "ADV", "ADV", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drauff sucht der scharffe Speer dir noch den Weg zum Her-", "tokens": ["Drauff", "sucht", "der", "scharf\u00b7fe", "Speer", "dir", "noch", "den", "Weg", "zum", "Her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "PPER", "ADV", "ART", "NN", "APPRART", "TRUNC"], "meter": "-+-+-+-+-+-+", "meas