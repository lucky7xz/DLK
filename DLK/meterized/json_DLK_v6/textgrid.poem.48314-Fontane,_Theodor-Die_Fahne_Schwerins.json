{"textgrid.poem.48314": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Die Fahne Schwerins", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Arsenal, dem alten,", "tokens": ["Im", "Ar\u00b7se\u00b7nal", ",", "dem", "al\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu Petersburg am Dock,", "tokens": ["Zu", "Pe\u00b7ters\u00b7burg", "am", "Dock", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zersplittert und zerspalten", "tokens": ["Zer\u00b7split\u00b7tert", "und", "zer\u00b7spal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Steht ein alter Fahnenstock;", "tokens": ["Steht", "ein", "al\u00b7ter", "Fah\u00b7nen\u00b7stock", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Er steht in seiner Ecken", "tokens": ["Er", "steht", "in", "sei\u00b7ner", "E\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "An die hundert Jahre nun,", "tokens": ["An", "die", "hun\u00b7dert", "Jah\u00b7re", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit den andern Fahnenst\u00f6cken", "tokens": ["Mit", "den", "an\u00b7dern", "Fah\u00b7nen\u00b7st\u00f6\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hat er nichts zu tun.", "tokens": ["Hat", "er", "nichts", "zu", "tun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "PTKZU", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Der Fahnen j\u00fcngste schmunzelt:", "tokens": ["Der", "Fah\u00b7nen", "j\u00fcngs\u00b7te", "schmun\u00b7zelt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbhe, Kamerad im Eck,", "tokens": ["\u00bb", "he", ",", "Ka\u00b7me\u00b7rad", "im", "Eck", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "$,", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Warum so viel gerunzelt?", "tokens": ["Wa\u00b7rum", "so", "viel", "ge\u00b7run\u00b7zelt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das bringt uns nicht vom Fleck;", "tokens": ["Das", "bringt", "uns", "nicht", "vom", "Fleck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nicht ewig stumm und einsam", "tokens": ["Nicht", "e\u00b7wig", "stumm", "und", "ein\u00b7sam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und nicht so steif-apart,", "tokens": ["Und", "nicht", "so", "steif\u00b7a\u00b7part", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gesellig h\u00fcbsch, gemeinsam,", "tokens": ["Ge\u00b7sel\u00b7lig", "h\u00fcbsch", ",", "ge\u00b7mein\u00b7sam", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und etwas Lebensart.\u00ab", "tokens": ["Und", "et\u00b7was", "Le\u00b7ben\u00b7sart", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der drauf: \u00bbAn Schaftes Runde", "tokens": ["Der", "drauf", ":", "\u00bb", "An", "Schaf\u00b7tes", "Run\u00b7de"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "PTKVZ", "$.", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sieh hier den Silberring,", "tokens": ["Sieh", "hier", "den", "Sil\u00b7ber\u00b7ring", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Er deckt die breite Wunde,", "tokens": ["Er", "deckt", "die", "brei\u00b7te", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ich bei Prag empfing,", "tokens": ["Die", "ich", "bei", "Prag", "emp\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zersplittert hat, zerspalten", "tokens": ["Zer\u00b7split\u00b7tert", "hat", ",", "zer\u00b7spal\u00b7ten"], "token_info": ["word", "word", "punct", "word"], "pos": ["VVPP", "VAFIN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Kugel mich von Erz,", "tokens": ["Die", "Ku\u00b7gel", "mich", "von", "Erz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dem ging sie durch das Herz.", "tokens": ["Dem", "ging", "sie", "durch", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wen solch ein Held getragen", "tokens": ["Wen", "solch", "ein", "Held", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PIAT", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In solcher ", "tokens": ["In", "sol\u00b7cher"], "token_info": ["word", "word"], "pos": ["APPR", "PIAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Dem will es nicht behagen", "tokens": ["Dem", "will", "es", "nicht", "be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf fremdem, russischem Grund,", "tokens": ["Auf", "frem\u00b7dem", ",", "rus\u00b7si\u00b7schem", "Grund", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Der will unter Trommelch\u00f6ren", "tokens": ["Der", "will", "un\u00b7ter", "Trom\u00b7melc\u00b7h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Und den \u203aDessauer\u2039 wieder h\u00f6ren,", "tokens": ["Und", "den", "\u203a", "Des\u00b7sau\u00b7er", "\u2039", "wie\u00b7der", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$(", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und von Hohenfriedberg den.\u00ab", "tokens": ["Und", "von", "Ho\u00b7hen\u00b7fried\u00b7berg", "den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "NE", "ART", "$.", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Im Arsenal, dem alten,", "tokens": ["Im", "Ar\u00b7se\u00b7nal", ",", "dem", "al\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu Petersburg am Dock,", "tokens": ["Zu", "Pe\u00b7ters\u00b7burg", "am", "Dock", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zersplittert und zerspalten,", "tokens": ["Zer\u00b7split\u00b7tert", "und", "zer\u00b7spal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sprach so der Fahnenstock.", "tokens": ["Sprach", "so", "der", "Fah\u00b7nen\u00b7stock", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Die andern nickten leise,", "tokens": ["Die", "an\u00b7dern", "nick\u00b7ten", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Zugwind wehte sacht,", "tokens": ["Der", "Zug\u00b7wind", "weh\u00b7te", "sacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Immer stiller ward's im Kreise; \u2013", "tokens": ["Im\u00b7mer", "stil\u00b7ler", "ward's", "im", "Krei\u00b7se", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ein Stern schien durch die Nacht", "tokens": ["Ein", "Stern", "schien", "durch", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}