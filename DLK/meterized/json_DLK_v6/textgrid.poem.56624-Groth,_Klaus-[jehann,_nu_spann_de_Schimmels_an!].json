{"textgrid.poem.56624": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "[jehann, nu spann de Schimmels an!]", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jehann, nu spann de Schimmels an!", "tokens": ["Je\u00b7hann", ",", "nu", "spann", "de", "Schim\u00b7mels", "an", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "NE", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nu fahrt wi na de Brut!", "tokens": ["Nu", "fahrt", "wi", "na", "de", "Brut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Un hebbt wi nix as brune P\u0119r,", "tokens": ["Un", "hebbt", "wi", "nix", "as", "bru\u00b7ne", "P\u0119r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.4": {"text": "Jehann, so is't ok gut!", "tokens": ["Je\u00b7hann", ",", "so", "is't", "ok", "gut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "FM", "FM", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Un hebbt wi nix as swarte P\u0119r,", "tokens": ["Un", "hebbt", "wi", "nix", "as", "swar\u00b7te", "P\u0119r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Jehann, so is't ok recht!", "tokens": ["Je\u00b7hann", ",", "so", "is't", "ok", "recht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "FM", "FM", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Un b\u00fcn ik nich uns Weerth sin S\u0153n,", "tokens": ["Un", "b\u00fcn", "ik", "nich", "uns", "Weerth", "sin", "S\u0153n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So b\u00fcn'k sin j\u00fcngste Knecht!", "tokens": ["So", "b\u00fcn'k", "sin", "j\u00fcngs\u00b7te", "Knecht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Un hebbt wi gar keen P\u0119r un Wag',", "tokens": ["Un", "hebbt", "wi", "gar", "ke\u00b7en", "P\u0119r", "un", "Wag'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KOKOM", "ADV", "VVFIN", "FM", "FM", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "So hebbt wi junge Been!", "tokens": ["So", "hebbt", "wi", "jun\u00b7ge", "Be\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Un de so gl\u00fccklich is as ik,", "tokens": ["Un", "de", "so", "gl\u00fcck\u00b7lich", "is", "as", "ik", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "ADJD", "FM", "FM", "FM", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Jehann, dat w\u00fcllt wi sehn!", "tokens": ["Je\u00b7hann", ",", "dat", "w\u00fcllt", "wi", "sehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PAV", "VVFIN", "KOKOM", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}