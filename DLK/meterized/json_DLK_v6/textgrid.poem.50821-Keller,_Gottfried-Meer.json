{"textgrid.poem.50821": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Meer", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel h\u00e4ngt, wie Blei so schwer,", "tokens": ["Der", "Him\u00b7mel", "h\u00e4ngt", ",", "wie", "Blei", "so", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dicht auf dem wildemp\u00f6rten Meer;", "tokens": ["Dicht", "auf", "dem", "wil\u00b7dem\u00b7p\u00f6r\u00b7ten", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein englisch Segel, fast die Quer,", "tokens": ["Ein", "eng\u00b7lisch", "Se\u00b7gel", ",", "fast", "die", "Quer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schie\u00dft wie ein Pfeil dar\u00fcber her.", "tokens": ["Schie\u00dft", "wie", "ein", "Pfeil", "da\u00b7r\u00fc\u00b7ber", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Messer, so das Meer sich schliff,", "tokens": ["Ein", "Mes\u00b7ser", ",", "so", "das", "Meer", "sich", "schliff", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da starrt ein blankes Felsenriff", "tokens": ["Da", "starrt", "ein", "blan\u00b7kes", "Fel\u00b7sen\u00b7riff"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schlitzt das Engell\u00e4nderschiff \u2013", "tokens": ["Und", "schlitzt", "das", "En\u00b7gel\u00b7l\u00e4n\u00b7der\u00b7schiff", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Meer tut einen guten Griff.", "tokens": ["Das", "Meer", "tut", "ei\u00b7nen", "gu\u00b7ten", "Griff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Viel tausend Bibeln sind die Fracht,", "tokens": ["Viel", "tau\u00b7send", "Bi\u00b7beln", "sind", "die", "Fracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die sinken in die Wassernacht;", "tokens": ["Die", "sin\u00b7ken", "in", "die", "Was\u00b7ser\u00b7nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schon hat in d\u00fcstrer Schuppenpracht", "tokens": ["Schon", "hat", "in", "d\u00fcst\u00b7rer", "Schup\u00b7pen\u00b7pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Seevolk sich herbeigemacht.", "tokens": ["Das", "See\u00b7volk", "sich", "her\u00b7bei\u00b7ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da wimmelt es von Schlang und Fisch,", "tokens": ["Da", "wim\u00b7melt", "es", "von", "Schlang", "und", "Fisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sitzen am Korallentisch;", "tokens": ["Sie", "sit\u00b7zen", "am", "Ko\u00b7ral\u00b7len\u00b7tisch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Her schie\u00dft der Leviathan risch:", "tokens": ["Her", "schie\u00dft", "der", "Le\u00b7vi\u00b7a\u00b7than", "risch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "++-++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "\u00bbwas ist das f\u00fcr ein Flederwisch?\u00ab", "tokens": ["\u00bb", "was", "ist", "das", "f\u00fcr", "ein", "Fle\u00b7der\u00b7wisch", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VAFIN", "PDS", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Meerschlang, als die K\u00f6nigin,", "tokens": ["Die", "Meer\u00b7schlang", ",", "als", "die", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kommt auch und bl\u00e4ttert her und hin;", "tokens": ["Kommt", "auch", "und", "bl\u00e4t\u00b7tert", "her", "und", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie alle lesen emsig drin", "tokens": ["Sie", "al\u00b7le", "le\u00b7sen", "em\u00b7sig", "drin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PIS", "VVFIN", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und forschen nach dem dunkeln Sinn.", "tokens": ["Und", "for\u00b7schen", "nach", "dem", "dun\u00b7keln", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie ziehn den Mission\u00e4r empor", "tokens": ["Sie", "ziehn", "den", "Mis\u00b7si\u00b7o\u00b7n\u00e4r", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und halten ihm die Bibel vor;", "tokens": ["Und", "hal\u00b7ten", "ihm", "die", "Bi\u00b7bel", "vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch der zu schweigen sich verschwor \u2013", "tokens": ["Doch", "der", "zu", "schwei\u00b7gen", "sich", "ver\u00b7schwor", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PTKZU", "VVFIN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Meer durchbraust sein totes Ohr.", "tokens": ["Das", "Meer", "durch\u00b7braust", "sein", "to\u00b7tes", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}