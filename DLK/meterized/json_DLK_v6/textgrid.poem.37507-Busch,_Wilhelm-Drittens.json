{"textgrid.poem.37507": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Drittens", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein hoffnungsvoller junger Mann", "tokens": ["Ein", "hoff\u00b7nungs\u00b7vol\u00b7ler", "jun\u00b7ger", "Mann"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gew\u00f6hnt sich leicht das Malen an! \u2013", "tokens": ["Ge\u00b7w\u00f6hnt", "sich", "leicht", "das", "Ma\u00b7len", "an", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "PRF", "ADJD", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auch Bruder Antonio, welcher nun,", "tokens": ["Auch", "Bru\u00b7der", "An\u00b7to\u00b7nio", ",", "wel\u00b7cher", "nun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "NE", "$,", "PRELS", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von seinen S\u00fcnden auszuruhn,", "tokens": ["Von", "sei\u00b7nen", "S\u00fcn\u00b7den", "aus\u00b7zu\u00b7ruhn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu Padua im Kloster lebt", "tokens": ["Zu", "Pa\u00b7dua", "im", "Klos\u00b7ter", "lebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und geistlicher Bildung sich bestrebt,", "tokens": ["Und", "geist\u00b7li\u00b7cher", "Bil\u00b7dung", "sich", "be\u00b7strebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Hat es gar bald herausgebracht,", "tokens": ["Hat", "es", "gar", "bald", "her\u00b7aus\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie man die sch\u00f6nen Bilder macht,", "tokens": ["Wie", "man", "die", "sch\u00f6\u00b7nen", "Bil\u00b7der", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und malt auf Gold, sch\u00f6n rot und blau,", "tokens": ["Und", "malt", "auf", "Gold", ",", "sch\u00f6n", "rot", "und", "blau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Bildnis unsrer lieben Frau.", "tokens": ["Das", "Bild\u00b7nis", "uns\u00b7rer", "lie\u00b7ben", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Umflattert von der Englein Chor", "tokens": ["Um\u00b7flat\u00b7tert", "von", "der", "En\u00b7glein", "Chor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Tritt sie hervor aus des Himmels Tor.", "tokens": ["Tritt", "sie", "her\u00b7vor", "aus", "des", "Him\u00b7mels", "Tor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Den blauen Mantel fa\u00dft die Linke,", "tokens": ["Den", "blau\u00b7en", "Man\u00b7tel", "fa\u00dft", "die", "Lin\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Rechte sieht man sanft erhoben,", "tokens": ["Die", "Rech\u00b7te", "sieht", "man", "sanft", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Halb drohend, halb zum Gnadenwinke,", "tokens": ["Halb", "dro\u00b7hend", ",", "halb", "zum", "Gna\u00b7den\u00b7win\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So kommt die K\u00f6nigin von oben.", "tokens": ["So", "kommt", "die", "K\u00f6\u00b7ni\u00b7gin", "von", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch ihr zu F\u00fc\u00dfen windet sich", "tokens": ["Doch", "ihr", "zu", "F\u00fc\u00b7\u00dfen", "win\u00b7det", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Teufel schwarz und f\u00fcrchterlich.", "tokens": ["Der", "Teu\u00b7fel", "schwarz", "und", "f\u00fcrch\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Teufel war's nicht einerlei,", "tokens": ["Dem", "Teu\u00b7fel", "wa\u00b7r's", "nicht", "ei\u00b7ner\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df er so gar abscheulich sei.", "tokens": ["Da\u00df", "er", "so", "gar", "ab\u00b7scheu\u00b7lich", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er f\u00e4ngt alsbald das Gr\u00fcbeln an,", "tokens": ["Er", "f\u00e4ngt", "als\u00b7bald", "das", "Gr\u00fc\u00b7beln", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie er den Bruder kr\u00e4nken kann.", "tokens": ["Wie", "er", "den", "Bru\u00b7der", "kr\u00e4n\u00b7ken", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein Kloster lag nicht weit von hinnen,", "tokens": ["Ein", "Klos\u00b7ter", "lag", "nicht", "weit", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADJD", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Besetzt mit Karmeliterinnen,", "tokens": ["Be\u00b7setzt", "mit", "Kar\u00b7me\u00b7li\u00b7te\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und war als Kustorin allda", "tokens": ["Und", "war", "als", "Kus\u00b7to\u00b7rin", "all\u00b7da"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "KOKOM", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die keusche Jungfrau Laurentia. \u2013", "tokens": ["Die", "keu\u00b7sche", "Jung\u00b7frau", "Lau\u00b7ren\u00b7tia", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bescheiden, still und glaubensfroh,", "tokens": ["Be\u00b7schei\u00b7den", ",", "still", "und", "glau\u00b7bens\u00b7froh", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hat sie der gute Antonio,", "tokens": ["Hat", "sie", "der", "gu\u00b7te", "An\u00b7to\u00b7nio", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den alles Gute stets ergetzt,", "tokens": ["Den", "al\u00b7les", "Gu\u00b7te", "stets", "er\u00b7getzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Schon l\u00e4ngst von Herzen hochgesch\u00e4tzt.", "tokens": ["Schon", "l\u00e4ngst", "von", "Her\u00b7zen", "hoch\u00b7ge\u00b7sch\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nat\u00fcrlich im allgemeinen und \u00fcberhaupt,", "tokens": ["Na\u00b7t\u00fcr\u00b7lich", "im", "all\u00b7ge\u00b7mei\u00b7nen", "und", "\u00fc\u00b7ber\u00b7haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "KON", "ADV", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Wie's unsere heilige Kirche erlaubt.", "tokens": ["Wie's", "un\u00b7se\u00b7re", "hei\u00b7li\u00b7ge", "Kir\u00b7che", "er\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Einst als er so in stiller Nacht,", "tokens": ["Einst", "als", "er", "so", "in", "stil\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Tr\u00e4umen umgaukelt, halb schl\u00e4ft, halb wacht,", "tokens": ["Von", "Tr\u00e4u\u00b7men", "um\u00b7gau\u00b7kelt", ",", "halb", "schl\u00e4ft", ",", "halb", "wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "ADJD", "VVFIN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Tritt bei des Mondes D\u00e4mmerhelle", "tokens": ["Tritt", "bei", "des", "Mon\u00b7des", "D\u00e4m\u00b7mer\u00b7hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schwester Laurentia in seine Zelle", "tokens": ["Schwes\u00b7ter", "Lau\u00b7ren\u00b7tia", "in", "sei\u00b7ne", "Zel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NE", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Und beugt sich nieder und seufzt und spricht:", "tokens": ["Und", "beugt", "sich", "nie\u00b7der", "und", "seufzt", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbantonio, Lieber, kennst du mich nicht?", "tokens": ["\u00bb", "an\u00b7to\u00b7nio", ",", "Lie\u00b7ber", ",", "kennst", "du", "mich", "nicht", "?"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "$,", "ADJD", "$,", "VVFIN", "PPER", "PRF", "PTKNEG", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ich bin entflohn aus des Klosters Zwang,", "tokens": ["Ich", "bin", "ent\u00b7flohn", "aus", "des", "Klos\u00b7ters", "Zwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "K\u00f6nnt' nicht widerstehn meines Herzens Drang,", "tokens": ["K\u00f6nnt'", "nicht", "wi\u00b7der\u00b7stehn", "mei\u00b7nes", "Her\u00b7zens", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Bin aus Liebe zu dir und gro\u00dfem Verlangen", "tokens": ["Bin", "aus", "Lie\u00b7be", "zu", "dir", "und", "gro\u00b7\u00dfem", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "APPR", "PPER", "KON", "ADJA", "NN"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Mit dem Silberger\u00e4t davongegangen.", "tokens": ["Mit", "dem", "Sil\u00b7ber\u00b7ge\u00b7r\u00e4t", "da\u00b7von\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Auf auf, Antonio! tue desgleichen", "tokens": ["Auf", "auf", ",", "An\u00b7to\u00b7nio", "!", "tue", "des\u00b7glei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "PTKVZ", "$,", "NE", "$.", "VVFIN", "ADV"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Und la\u00df uns in fremde Lande entweichen!\u00ab", "tokens": ["Und", "la\u00df", "uns", "in", "frem\u00b7de", "Lan\u00b7de", "ent\u00b7wei\u00b7chen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Dem Bruder t\u00e4t die Sache scheinen,", "tokens": ["Dem", "Bru\u00b7der", "t\u00e4t", "die", "Sa\u00b7che", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nimmt die heiligen Gef\u00e4\u00dfe aus den Schreinen,", "tokens": ["Nimmt", "die", "hei\u00b7li\u00b7gen", "Ge\u00b7f\u00e4\u00b7\u00dfe", "aus", "den", "Schrei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Packt's in die Kutten emsiglich", "tokens": ["Packt's", "in", "die", "Kut\u00b7ten", "em\u00b7sig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "ADJD"], "meter": "++-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Und l\u00e4\u00dft das Kloster hinter sich. \u2013", "tokens": ["Und", "l\u00e4\u00dft", "das", "Klos\u00b7ter", "hin\u00b7ter", "sich", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PRF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Aber da drau\u00dfen im freien Feld", "tokens": ["A\u00b7ber", "da", "drau\u00b7\u00dfen", "im", "frei\u00b7en", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ward ihm die Lieb und Lust verg\u00e4llt.", "tokens": ["Ward", "ihm", "die", "Lieb", "und", "Lust", "ver\u00b7g\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Statt der guten Jungfrau Laurentia", "tokens": ["Statt", "der", "gu\u00b7ten", "Jung\u00b7frau", "Lau\u00b7ren\u00b7tia"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "War pl\u00f6tzlich der leidige Satan da.", "tokens": ["War", "pl\u00f6tz\u00b7lich", "der", "lei\u00b7di\u00b7ge", "Sa\u00b7tan", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.10": {"line.1": {"text": "\u00bbheihei!\u00ab \u2013 lacht der Teufel \u2013 \u00bbso ist's der Brauch!", "tokens": ["\u00bb", "hei\u00b7hei", "!", "\u00ab", "\u2013", "lacht", "der", "Teu\u00b7fel", "\u2013", "\u00bb", "so", "ist's", "der", "Brauch", "!"], "toke