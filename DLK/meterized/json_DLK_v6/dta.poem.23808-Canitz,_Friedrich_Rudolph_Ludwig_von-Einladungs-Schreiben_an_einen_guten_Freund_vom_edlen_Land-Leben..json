, "punct"], "pos": ["PRF", "ADV", "NE", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.73": {"text": "Charlotten/ Christian/ und deinen theuren Fritzen/", "tokens": ["Char\u00b7lot\u00b7ten", "/", "Chris\u00b7ti\u00b7an", "/", "und", "dei\u00b7nen", "theu\u00b7ren", "Frit\u00b7zen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NE", "$(", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Die seh ich eingepackt aufs schmale B\u00e4nckgen sitzen;", "tokens": ["Die", "seh", "ich", "ein\u00b7ge\u00b7packt", "aufs", "schma\u00b7le", "B\u00e4nck\u00b7gen", "sit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVPP", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Doch wo die Pape bleibt mit ihrer breiten Brust/", "tokens": ["Doch", "wo", "die", "Pa\u00b7pe", "bleibt", "mit", "ih\u00b7rer", "brei\u00b7ten", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "Und aufgeth\u00fcrmten Kopff/ das ist mir unbewust.", "tokens": ["Und", "auf\u00b7ge\u00b7th\u00fcrm\u00b7ten", "Kopff", "/", "das", "ist", "mir", "un\u00b7be\u00b7wust", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$(", "PDS", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Ich dencke da\u00df sie sich vor dismahl wird bequemen/", "tokens": ["Ich", "den\u00b7cke", "da\u00df", "sie", "sic