{"dta.poem.23808": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Einladungs-Schreiben an einen guten  \n Freund/ vom edlen Land-Leben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Die Zeilen die itzund mir aus der Feder fliessen/", "tokens": ["Die", "Zei\u00b7len", "die", "it\u00b7zund", "mir", "aus", "der", "Fe\u00b7der", "flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die werde\u0303 abgeschickt/ Herr Bruder/ dich zu gr\u00fcssen/", "tokens": ["Die", "werd\u1ebd", "ab\u00b7ge\u00b7schickt", "/", "Herr", "Bru\u00b7der", "/", "dich", "zu", "gr\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "NN", "NN", "$(", "PPER", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Ob ich gleich einsam bin/ so wil ich doch dabey/", "tokens": ["Ob", "ich", "gleich", "ein\u00b7sam", "bin", "/", "so", "wil", "ich", "doch", "da\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$(", "ADV", "VMFIN", "PPER", "ADV", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df ich nicht unbekandt bey meinen Freunden sey", "tokens": ["Da\u00df", "ich", "nicht", "un\u00b7be\u00b7kandt", "bey", "mei\u00b7nen", "Freun\u00b7den", "sey"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "APPR", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zu Blumberg ist mein Sitz/ da nach der alten Weise/", "tokens": ["Zu", "Blum\u00b7berg", "ist", "mein", "Sitz", "/", "da", "nach", "der", "al\u00b7ten", "Wei\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPOSAT", "NN", "$(", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit dem was GOtt beschert/ ich mich recht gl\u00fccklich", "tokens": ["Mit", "dem", "was", "Gott", "be\u00b7schert", "/", "ich", "mich", "recht", "gl\u00fcck\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PWS", "NN", "VVPP", "$(", "PPER", "PRF", "ADJD", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da ich aus meinem Sinn die Sorgen weggereumt/", "tokens": ["Da", "ich", "aus", "mei\u00b7nem", "Sinn", "die", "Sor\u00b7gen", "weg\u00b7ge\u00b7reumt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So da\u00df mir nicht von Geitz/ noch eitler Ehre tr\u00e4umt.", "tokens": ["So", "da\u00df", "mir", "nicht", "von", "Geitz", "/", "noch", "eit\u00b7ler", "Eh\u00b7re", "tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PTKNEG", "APPR", "NN", "$(", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich kan das Spiel der Welt/ und ihr verwirrtes Wesen", "tokens": ["Ich", "kan", "das", "Spiel", "der", "Welt", "/", "und", "ihr", "ver\u00b7wirr\u00b7tes", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "$(", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Aus dem gedr\u00fcckten Blat des Zeitung-Schreibers lesen/", "tokens": ["Aus", "dem", "ge\u00b7dr\u00fcck\u00b7ten", "Blat", "des", "Zei\u00b7tung\u00b7Schrei\u00b7bers", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und wen\u0303 gleich alles wird in Blut und Krieg gest\u00fcrtzt/", "tokens": ["Und", "we\u00f1", "gleich", "al\u00b7les", "wird", "in", "Blut", "und", "Krieg", "ge\u00b7st\u00fcrtzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "PIS", "VAFIN", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wird im geringste\u0303 nicht dadurch mein Schlaf gek\u00fcrtzt.", "tokens": ["Wird", "im", "ge\u00b7ringst\u1ebd", "nicht", "da\u00b7durch", "mein", "Schlaf", "ge\u00b7k\u00fcrtzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "PTKNEG", "PAV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Bleibt Fridrich nur gesund/ und hat sein ", "tokens": ["Bleibt", "Frid\u00b7rich", "nur", "ge\u00b7sund", "/", "und", "hat", "sein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NE", "ADV", "ADJD", "$(", "KON", "VAFIN", "PPOSAT"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Was ist mir an Namur und Pignerol gelegen?", "tokens": ["Was", "ist", "mir", "an", "Na\u00b7mur", "und", "Pig\u00b7ne\u00b7rol", "ge\u00b7le\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "NN", "KON", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und wenn ich ohne Streit die Garben binden kan/", "tokens": ["Und", "wenn", "ich", "oh\u00b7ne", "Streit", "die", "Gar\u00b7ben", "bin\u00b7den", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "NN", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ficht Franckreich mich so viel/ als wie der Mogol an.", "tokens": ["Ficht", "Fran\u00b7ck\u00b7reich", "mich", "so", "viel", "/", "als", "wie", "der", "Mo\u00b7gol", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "ADV", "ADV", "$(", "KOUS", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Hier merck ich da\u00df die Ruh in schlechten H\u00fctten wohnet/", "tokens": ["Hier", "merck", "ich", "da\u00df", "die", "Ruh", "in", "schlech\u00b7ten", "H\u00fct\u00b7ten", "woh\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wenn Ungl\u00fcck und Verdru\u00df nicht der Pall\u00e4ste schonet/", "tokens": ["Wenn", "Un\u00b7gl\u00fcck", "und", "Ver\u00b7dru\u00df", "nicht", "der", "Pal\u00b7l\u00e4s\u00b7te", "scho\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PTKNEG", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da\u00df es viel besser ist/ bey Kohl und R\u00fcben stehn/", "tokens": ["Da\u00df", "es", "viel", "bes\u00b7ser", "ist", "/", "bey", "Kohl", "und", "R\u00fc\u00b7ben", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$(", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Als in dem Labyrinth des Hofes irre gehn.", "tokens": ["Als", "in", "dem", "La\u00b7by\u00b7rinth", "des", "Ho\u00b7fes", "ir\u00b7re", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Hier ist mein eigner Grund/ der mir ist angestorben/", "tokens": ["Hier", "ist", "mein", "eig\u00b7ner", "Grund", "/", "der", "mir", "ist", "an\u00b7ge\u00b7stor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$(", "PRELS", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Hier ist kein Fu\u00df breit nicht durch schlim\u0303es Recht erworbe\u0303/", "tokens": ["Hier", "ist", "kein", "Fu\u00df", "breit", "nicht", "durch", "schlim\u0303es", "Recht", "er\u00b7wor\u00b7b\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ADJD", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Kein Stein/ der Witwen dr\u00fcckt/ und W\u00e4ysen Thr\u00e4nen", "tokens": ["Kein", "Stein", "/", "der", "Wit\u00b7wen", "dr\u00fcckt", "/", "und", "W\u00e4y\u00b7sen", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$(", "ART", "NN", "VVFIN", "$(", "KON", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Kein Ort der einen Fluch zum Echo schallen l\u00e4\u00dft.", "tokens": ["Kein", "Ort", "der", "ei\u00b7nen", "Fluch", "zum", "E\u00b7cho", "schal\u00b7len", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ART", "NN", "APPRART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Hier kan ich Schaaf und Rind in den begr\u00fcnten Auen/", "tokens": ["Hier", "kan", "ich", "Schaaf", "und", "Rind", "in", "den", "be\u00b7gr\u00fcn\u00b7ten", "Au\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die Scheunen voller Frucht/ das Feld voll Hoffnung", "tokens": ["Die", "Scheu\u00b7nen", "vol\u00b7ler", "Frucht", "/", "das", "Feld", "voll", "Hoff\u00b7nung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Und wenn kein grosser Hecht hier in die Darge bei\u00dft/", "tokens": ["Und", "wenn", "kein", "gros\u00b7ser", "Hecht", "hier", "in", "die", "Dar\u00b7ge", "bei\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So gilt mein Giebel-Fang/ der offt das Netze rei\u00dft.", "tokens": ["So", "gilt", "mein", "Gie\u00b7bel\u00b7Fang", "/", "der", "offt", "das", "Net\u00b7ze", "rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$(", "ART", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ja wil ein stoltzer Hirsch nicht als ein R\u00e4uber sterben/", "tokens": ["Ja", "wil", "ein", "stolt\u00b7zer", "Hirsch", "nicht", "als", "ein", "R\u00e4u\u00b7ber", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "KOUS", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So mu\u00df er meine Saat sich scheuen zu verderben.", "tokens": ["So", "mu\u00df", "er", "mei\u00b7ne", "Saat", "sich", "scheu\u00b7en", "zu", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "PRF", "VVINF", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Von allem bin ich Herr/ was in dem Paradie\u00df", "tokens": ["Von", "al\u00b7lem", "bin", "ich", "Herr", "/", "was", "in", "dem", "Pa\u00b7ra\u00b7die\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "NN", "$(", "PWS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der Vater Adam erst mit eignen Namen hie\u00df.", "tokens": ["Der", "Va\u00b7ter", "A\u00b7dam", "erst", "mit", "eig\u00b7nen", "Na\u00b7men", "hie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Mein Reden darff ich hier auf keiner Schalen w\u00e4gen/", "tokens": ["Mein", "Re\u00b7den", "darff", "ich", "hier", "auf", "kei\u00b7ner", "Scha\u00b7len", "w\u00e4\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Auch nicht gew\u00e4rtig seyn/ wenn mir es ungelegen/", "tokens": ["Auch", "nicht", "ge\u00b7w\u00e4r\u00b7tig", "seyn", "/", "wenn", "mir", "es", "un\u00b7ge\u00b7le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJD", "VAINF", "$(", "KOUS", "PPER", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Da\u00df aus Gewohnheit mich ein falscher Freund be-", "tokens": ["Da\u00df", "aus", "Ge\u00b7wohn\u00b7heit", "mich", "ein", "fal\u00b7scher", "Freund", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "PPER", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Und wol aus H\u00f6fflichkeit in seinem Sinn verflucht.", "tokens": ["Und", "wol", "aus", "H\u00f6ff\u00b7lich\u00b7keit", "in", "sei\u00b7nem", "Sinn", "ver\u00b7flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Hier leb ich wie ich soll/ mein Wille giebt Gesetze/", "tokens": ["Hier", "leb", "ich", "wie", "ich", "soll", "/", "mein", "Wil\u00b7le", "giebt", "Ge\u00b7set\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PWAV", "PPER", "VMFIN", "$(", "PPOSAT", "NN", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und keinem Rechenschafft/ ich f\u00fcrchte kein Geschw\u00e4tze/", "tokens": ["Und", "kei\u00b7nem", "Re\u00b7chen\u00b7schafft", "/", "ich", "f\u00fcrch\u00b7te", "kein", "Ge\u00b7schw\u00e4t\u00b7ze", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$(", "PPER", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wenn da der Hundes-Stern am Firmamente gl\u00fcht/", "tokens": ["Wenn", "da", "der", "Hun\u00b7des\u00b7S\u00b7tern", "am", "Fir\u00b7ma\u00b7men\u00b7te", "gl\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.40": {"text": "Man mich bey dem Camin im Fuchspeltz sitzen sieht.", "tokens": ["Man", "mich", "bey", "dem", "Ca\u00b7min", "im", "Fuchs\u00b7peltz", "sit\u00b7zen", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"