{"dta.poem.21097": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "An Rosens Schatten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer bist du, Lichtgestalt, die durch die               ", "tokens": ["Wer", "bist", "du", ",", "Licht\u00b7ge\u00b7stalt", ",", "die", "durch", "die"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "$,", "PRELS", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "D\u00e4mmerungen,", "tokens": ["D\u00e4m\u00b7me\u00b7run\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Von Glorie umstrahlt, von Glanzgew\u00f6lk um-", "tokens": ["Von", "Glo\u00b7rie", "um\u00b7strahlt", ",", "von", "Glanz\u00b7ge\u00b7w\u00f6lk", "um"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$,", "APPR", "NN", "TRUNC"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "schlungen,", "tokens": ["schlun\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Von goldnem Haar umwallt, von Mondenblitz um-", "tokens": ["Von", "gold\u00b7nem", "Haar", "um\u00b7wallt", ",", "von", "Mon\u00b7den\u00b7blitz", "um"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "APPR", "NE", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "bebt,", "tokens": ["bebt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "In schlanker Majest\u00e4t vor mir vor\u00fcberschwebt?", "tokens": ["In", "schlan\u00b7ker", "Ma\u00b7jes\u00b7t\u00e4t", "vor", "mir", "vor\u00b7\u00fc\u00b7bersc\u00b7hwebt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es schwirrt im Abendhauch der silberhelle Schleyer;", "tokens": ["Es", "schwirrt", "im", "A\u00b7bend\u00b7hauch", "der", "sil\u00b7ber\u00b7hel\u00b7le", "Schle\u00b7yer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dem blauen Aug' entspr\u00fcht ein \u00fcberirdisch Feuer;", "tokens": ["Dem", "blau\u00b7en", "Aug'", "ent\u00b7spr\u00fcht", "ein", "\u00fc\u00b7be\u00b7rir\u00b7disch", "Feu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der wonnetrunkne Blick, die hochgehabne Hand", "tokens": ["Der", "won\u00b7ne\u00b7trunk\u00b7ne", "Blick", ",", "die", "hoch\u00b7ge\u00b7hab\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Meint jenes Vaterland!", "tokens": ["Meint", "je\u00b7nes", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bist du es, Herrliche? bist du es, Fr\u00fchver-", "tokens": ["Bist", "du", "es", ",", "Herr\u00b7li\u00b7che", "?", "bist", "du", "es", ",", "Fr\u00fch\u00b7ver"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "PPER", "$,", "NN", "$.", "VAFIN", "PPER", "PPER", "$,", "TRUNC"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "kl\u00e4rte,", "tokens": ["kl\u00e4r\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Du ewger Huldigung und langer Thr\u00e4nen werthe,", "tokens": ["Du", "ew\u00b7ger", "Hul\u00b7di\u00b7gung", "und", "lan\u00b7ger", "Thr\u00e4\u00b7nen", "wert\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die du aus Edens Flur zu uns herunterstiegst,", "tokens": ["Die", "du", "aus", "E\u00b7dens", "Flur", "zu", "uns", "her\u00b7un\u00b7ter\u00b7stiegst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NE", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und duldend l\u00e4cheltest, und qualumrungen", "tokens": ["Und", "dul\u00b7dend", "l\u00e4\u00b7chel\u00b7test", ",", "und", "qua\u00b7lum\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "$,", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "schwiegst?", "tokens": ["schwiegst", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Ach, viel zu streng f\u00fcr dich war unsers Zembla", "tokens": ["Ach", ",", "viel", "zu", "streng", "f\u00fcr", "dich", "war", "un\u00b7sers", "Zem\u00b7bla"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "PTKA", "ADJD", "APPR", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Boden,", "tokens": ["Bo\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Zu rauh der Frost der Nacht, zu barsch des Nord-", "tokens": ["Zu", "rauh", "der", "Frost", "der", "Nacht", ",", "zu", "barsch", "des", "Nord"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "ART", "NN", "ART", "NN", "$,", "PTKA", "ADJD", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "winds Oden;", "tokens": ["winds", "O\u00b7den", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Du bl\u00fchtest, welktest, sankst, und bargst dich kla-", "tokens": ["Du", "bl\u00fch\u00b7test", ",", "welk\u00b7test", ",", "sankst", ",", "und", "bargst", "dich", "kla"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "KON", "VVFIN", "PPER", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "gelos", "tokens": ["ge\u00b7los"], "token_info": ["word"], "pos": ["ADJD"], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "In Tellus mildem Schooss.", "tokens": ["In", "Tel\u00b7lus", "mil\u00b7dem", "Scho\u00b7oss", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O Rosa, dein gedenkt mein Geist mit Wonn'", "tokens": ["O", "Ro\u00b7sa", ",", "dein", "ge\u00b7denkt", "mein", "Geist", "mit", "Wonn'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PPOSAT", "VVFIN", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und Wehmuth,", "tokens": ["und", "Weh\u00b7muth", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Gedenkt mit s\u00fcssem Schmerz der ungeschminkten", "tokens": ["Ge\u00b7denkt", "mit", "s\u00fcs\u00b7sem", "Schmerz", "der", "un\u00b7ge\u00b7schmink\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Demuth,", "tokens": ["De\u00b7muth", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Des ungetr\u00fcbten Sinns, der ungekr\u00e4nkten Huld,", "tokens": ["Des", "un\u00b7ge\u00b7tr\u00fcb\u00b7ten", "Sinns", ",", "der", "un\u00b7ge\u00b7kr\u00e4nk\u00b7ten", "Huld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der nieermattenden, gerntragenden Geduld.", "tokens": ["Der", "nie\u00b7er\u00b7mat\u00b7ten\u00b7den", ",", "gern\u00b7tra\u00b7gen\u00b7den", "Ge\u00b7duld", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Holdselig schwebtest du in deiner Lieben", "tokens": ["Hold\u00b7se\u00b7lig", "schweb\u00b7test", "du", "in", "dei\u00b7ner", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Mitte,", "tokens": ["Mit\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Mit Engelfreundlichkeit, mit leisem Rehes-", "tokens": ["Mit", "En\u00b7gel\u00b7freund\u00b7lich\u00b7keit", ",", "mit", "lei\u00b7sem", "Re\u00b7hes"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "tritte,", "tokens": ["trit\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Mit W\u00fcrde sonder Stolz, mit G\u00fcte sonder", "tokens": ["Mit", "W\u00fcr\u00b7de", "son\u00b7der", "Stolz", ",", "mit", "G\u00fc\u00b7te", "son\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Schein,", "tokens": ["Schein", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Treu, einfach, keusch und rein.", "tokens": ["Treu", ",", "ein\u00b7fach", ",", "keusch", "und", "rein", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wo seyd, wo seyd ihr hin, zu schnell ver-", "tokens": ["Wo", "seyd", ",", "wo", "seyd", "ihr", "hin", ",", "zu", "schnell", "ver"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "$,", "PWAV", "VAFIN", "PPER", "PTKVZ", "$,", "PTKA", "ADJD", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "flossne Zeiten,", "tokens": ["floss\u00b7ne", "Zei\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ihr Tage reich an Qual und reich an Selig-", "tokens": ["Ihr", "Ta\u00b7ge", "reich", "an", "Qual", "und", "reich", "an", "Se\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "NN", "KON", "ADJD", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "keiten,", "tokens": ["kei\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Wo ich im Abendlicht an Rosens Seite sass", "tokens": ["Wo", "ich", "im", "A\u00b7bend\u00b7licht", "an", "Ro\u00b7sens", "Sei\u00b7te", "sass"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "APPR", "NE", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und jeden Erdengram in ihrem Arm vergass;", "tokens": ["Und", "je\u00b7den", "Er\u00b7den\u00b7gram", "in", "ih\u00b7rem", "Arm", "ver\u00b7gass", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wo schnell dem ihrigen mein Herz entgegen-", "tokens": ["Wo", "schnell", "dem", "ih\u00b7ri\u00b7gen", "mein", "Herz", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "PPOSS", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "brannte,", "tokens": ["brann\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Wo sie mich schnell begriff, mich feurig Bruder!", "tokens": ["Wo", "sie", "mich", "schnell", "be\u00b7griff", ",", "mich", "feu\u00b7rig", "Bru\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "VVFIN", "$,", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "nannte,", "tokens": ["nann\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Wo an ihr Schwesterherz das meine fest sich", "tokens": ["Wo", "an", "ihr", "Schwes\u00b7ter\u00b7herz", "das", "mei\u00b7ne", "fest", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "ART", "PPOSAT", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "schloss,", "tokens": ["schloss", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Ganz Geist in Geist zerfloss!", "tokens": ["Ganz", "Geist", "in", "Geist", "zer\u00b7floss", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie oft, wenn ich versengt von deines Sa-", "tokens": ["Wie", "oft", ",", "wenn", "ich", "ver\u00b7sengt", "von", "dei\u00b7nes", "Sa"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "mum Brande,", "tokens": ["mum", "Bran\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Tyrannin Leidenschaft, und kaum dem schroffen", "tokens": ["Ty\u00b7ran\u00b7nin", "Lei\u00b7den\u00b7schaft", ",", "und", "kaum", "dem", "schrof\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "KON", "ADV", "ART", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Rande", "tokens": ["Ran\u00b7de"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Des Untergangs entschl\u00fcpft, an Rosens Busen flog,", "tokens": ["Des", "Un\u00b7ter\u00b7gangs", "ent\u00b7schl\u00fcpft", ",", "an", "Ro\u00b7sens", "Bu\u00b7sen", "flog", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "APPR", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und Trost aus ihrem Blick und ihrem Handdruck", "tokens": ["Und", "Trost", "aus", "ih\u00b7rem", "Blick", "und", "ih\u00b7rem", "Hand\u00b7druck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "sog;", "tokens": ["sog", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Wie troff so heilend dann aus ihrem Honigmunde", "tokens": ["Wie", "troff", "so", "hei\u00b7lend", "dann", "aus", "ih\u00b7rem", "Ho\u00b7nig\u00b7mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "ADJD", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Weisheit \u00d6l und Wein auf meine Herzens-", "tokens": ["Der", "Weis\u00b7heit", "\u00d6l", "und", "Wein", "auf", "mei\u00b7ne", "Her\u00b7zens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "KON", "NN", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "wunde!", "tokens": ["wun\u00b7de", "!"], "token_info": ["word", "punct"], "pos": ["ADJA", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Besch\u00e4mt, gest\u00e4rkt, vers\u00f6hnt mit Welt und mit", "tokens": ["Be\u00b7sch\u00e4mt", ",", "ge\u00b7st\u00e4rkt", ",", "ver\u00b7s\u00f6hnt", "mit", "Welt", "und", "mit"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "VVPP", "$,", "VVFIN", "APPR", "NN", "KON", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Geschick", "tokens": ["Ge\u00b7schick"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Kehrt' ich ins Joch zur\u00fcck.", "tokens": ["Kehrt'", "ich", "ins", "Joch", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und, o der schmerzlichen, der nie vergessnen", "tokens": ["Und", ",", "o", "der", "schmerz\u00b7li\u00b7chen", ",", "der", "nie", "ver\u00b7gess\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "FM", "ART", "ADJA", "$,", "PRELS", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Stunde,", "tokens": ["Stun\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wo ich zu fr\u00fch entwinkt dem sch\u00f6nen Schwester-", "tokens": ["Wo", "ich", "zu", "fr\u00fch", "ent\u00b7winkt", "dem", "sch\u00f6\u00b7nen", "Schwes\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PTKA", "ADJD", "VVFIN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "bunde,", "tokens": ["bun\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["ADJA", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Zum letztenmal sie sah, sie in den Arm mir", "tokens": ["Zum", "letz\u00b7ten\u00b7mal", "sie", "sah", ",", "sie", "in", "den", "Arm", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADV", "PPER", "VVFIN", "$,", "PPER", "APPR", "ART", "NN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "sank,", "tokens": ["sank", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Heissweinend mich umfing, lautschluchzend mich", "tokens": ["Heiss\u00b7wei\u00b7nend", "mich", "um\u00b7fing", ",", "laut\u00b7schluch\u00b7zend", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "VVFIN", "$,", "ADJD", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "umschlang;", "tokens": ["um\u00b7schlang", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Wo ich, dem finstrer Gram das starre Auge n\u00e4sste,", "tokens": ["Wo", "ich", ",", "dem", "finst\u00b7rer", "Gram", "das", "star\u00b7re", "Au\u00b7ge", "n\u00e4ss\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das herbe Lebewohl auf ihre Lippen presste,", "tokens": ["Das", "her\u00b7be", "Le\u00b7be\u00b7wohl", "auf", "ih\u00b7re", "Lip\u00b7pen", "press\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dann schnell mich losriss \u2014 Ha, wann strahlst,", "tokens": ["Dann", "schnell", "mich", "los\u00b7riss", "Ha", ",", "wann", "strahlst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$(", "ITJ", "$,", "PWAV", "VVFIN", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "wann winkst du, ach!", "tokens": ["wann", "winkst", "du", ",", "ach", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "ITJ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Des Wiedersehens Tag!", "tokens": ["Des", "Wie\u00b7der\u00b7se\u00b7hens", "Tag", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Tagt es im Grabe? \u2014 Nie, nie werd' ich Rosa", "tokens": ["Tagt", "es", "im", "Gra\u00b7be", "?", "Nie", ",", "nie", "werd'", "ich", "Ro\u00b7sa"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$.", "$(", "ADV", "$,", "ADV", "VAFIN", "PPER", "NE"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "schauen.", "tokens": ["schau\u00b7en", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Die Ausgepr\u00fcfte wallt auf ewiggr\u00fcnen Auen.", "tokens": ["Die", "Aus\u00b7ge\u00b7pr\u00fcf\u00b7te", "wallt", "auf", "e\u00b7wig\u00b7gr\u00fc\u00b7nen", "Au\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Heil ihr! Getauscht hat sie der Erde Brodem-", "tokens": ["Heil", "ihr", "!", "Ge\u00b7tauscht", "hat", "sie", "der", "Er\u00b7de", "Bro\u00b7dem"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$.", "NN", "VAFIN", "PPER", "ART", "NN", "TRUNC"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "luft", "tokens": ["luft"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Mit Edens lindem Wehn und Amaranthenduft.", "tokens": ["Mit", "E\u00b7dens", "lin\u00b7dem", "Wehn", "und", "A\u00b7ma\u00b7ran\u00b7then\u00b7duft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gek\u00fcsst vom lauen Strahl, erfrischt vom leisen", "tokens": ["Ge\u00b7k\u00fcsst", "vom", "lau\u00b7en", "Strahl", ",", "er\u00b7frischt", "vom", "lei\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,", "VVFIN", "APPRART", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Kosen", "tokens": ["Ko\u00b7sen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Des Athers, schau, wie bl\u00fchn die gramerblichnen", "tokens": ["Des", "A\u00b7thers", ",", "schau", ",", "wie", "bl\u00fchn", "die", "gra\u00b7mer\u00b7blich\u00b7nen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PTKVZ", "$,", "PWAV", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Rosen", "tokens": ["Ro\u00b7sen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Der Wange! sieh, wie schwillt vom Athem h\u00f6h'rer", "tokens": ["Der", "Wan\u00b7ge", "!", "sieh", ",", "wie", "schwillt", "vom", "A\u00b7them", "h\u00f6h'\u00b7rer"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "VVFIN", "$,", "PWAV", "VVFIN", "APPRART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Lust", "tokens": ["Lust"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Die schmerzgebrochne Brust!", "tokens": ["Die", "schmerz\u00b7ge\u00b7broch\u00b7ne", "Brust", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O Rosa, schau herab aus deiner lichten Ferne,", "tokens": ["O", "Ro\u00b7sa", ",", "schau", "her\u00b7ab", "aus", "dei\u00b7ner", "lich\u00b7ten", "Fer\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJD", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schau nieder, Heilige, zum m\u00fctterlichen Sterne!", "tokens": ["Schau", "nie\u00b7der", ",", "Hei\u00b7li\u00b7ge", ",", "zum", "m\u00fct\u00b7ter\u00b7li\u00b7chen", "Ster\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADJA", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf deinen Bruder schau, auf den verlassnen Freund,", "tokens": ["Auf", "dei\u0