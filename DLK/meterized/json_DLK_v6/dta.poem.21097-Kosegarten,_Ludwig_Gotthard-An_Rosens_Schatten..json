, "VVFIN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Kraft", "tokens": ["Kraft"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Der Tieger Leidenschaft.", "tokens": ["Der", "Tie\u00b7ger", "Lei\u00b7den\u00b7schaft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O Rosa, schweb' herab aus deinem hellen Sterne!", "tokens": ["O", "Ro\u00b7sa", ",", "schweb'", "her\u00b7ab", "aus", "dei\u00b7nem", "hel\u00b7len", "Ster\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schweb' nieder, Selige, in diese \u00f6de Ferne!", "tokens": ["Schweb'", "nie\u00b7der", ",", "Se\u00b7li\u00b7ge", ",", "in", "die\u00b7se", "\u00f6\u00b7de", "Fer\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "NN", "$,", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Umschimmre traulich mich im stillen Mondenlicht!", "tokens": ["Um\u00b7schimm\u00b7re", "trau\u00b7lich", "mich", "im", "stil\u00b7len", "Mon\u00b7den\u00b7licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Umlisple mich im Hauch, der in den Espen spricht!", "tokens": ["Um\u00b7lisp\u00b7le", "mich", "im", "Hauch", ",", "der", "in", "den", "Es\u00b7pen", "spricht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sey Feuers\u00e4ule mir auf meines Irrsals Pfaden!", "tokens": ["Sey", "Feu\u00b7er\u00b7s\u00e4u\u00b7le", "mir", "auf", "mei\u00b7nes", "Irr\u00b7sals", "Pfa\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sey mir im Sturm ein Strahl auf winkenden Ge-", "tokens": ["Sey", "mir", "im", "Sturm", "ein", "Strahl", "auf", "win\u00b7ken\u00b7den", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "staden!", "tokens": ["sta\u00b7den", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Und endet mein Exil, so leit' an deiner Hand", "tokens": ["Und", "en\u00b7det", "mein", "E\u00b7xil", ",", "so", "leit'", "an", "dei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mich heim ins Vaterland!", "tokens": ["Mich", "heim", "ins", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}