{"dta.poem.1189": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die blauen Augen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Will noch die schwartze Nacht den Tag bestreiten/ ", "tokens": ["Will", "noch", "die", "schwart\u00b7ze", "Nacht", "den", "Tag", "be\u00b7strei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und als ein irrend Licht bey duncklen Zeiten", "tokens": ["Und", "als", "ein", "ir\u00b7rend", "Licht", "bey", "dunck\u00b7len", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "VVPP", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der \u00fcbereitlen Welt/", "tokens": ["Der", "\u00fc\u00b7be\u00b7reit\u00b7len", "Welt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die/ was ihr wohlgef\u00e4llt/", "tokens": ["Die", "/", "was", "ihr", "wohl\u00b7ge\u00b7f\u00e4llt", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr einen Abgott h\u00e4lt/", "tokens": ["F\u00fcr", "ei\u00b7nen", "Ab\u00b7gott", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Den Sinn verleiten?", "tokens": ["Den", "Sinn", "ver\u00b7lei\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Des Monden Silber kan bey Nacht erquicken/", "tokens": ["Des", "Mon\u00b7den", "Sil\u00b7ber", "kan", "bey", "Nacht", "er\u00b7qui\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und durch den Schatten bricht der Sterne Blicken.", "tokens": ["Und", "durch", "den", "Schat\u00b7ten", "bricht", "der", "Ster\u00b7ne", "Bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein stoltzer Diamant", "tokens": ["Ein", "stolt\u00b7zer", "Di\u00b7a\u00b7mant"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Dunckelheit verwandt", "tokens": ["Der", "Dun\u00b7ckel\u00b7heit", "ver\u00b7wandt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "u\u00df manche F\u00fcrsten-Hand", "tokens": ["u\u00df", "man\u00b7che", "F\u00fcrs\u00b7ten\u00b7Hand"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "or andern schm\u00fccken.", "tokens": ["or", "an\u00b7dern", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Doch/ kan der Mond den Glantz der Sonn erreichen?", "tokens": ["Doch", "/", "kan", "der", "Mond", "den", "Glantz", "der", "Sonn", "er\u00b7rei\u00b7chen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VMFIN", "ART", "NN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Will sich der Sternen Licht dem Tage gleichen?", "tokens": ["Will", "sich", "der", "Ster\u00b7nen", "Licht", "dem", "Ta\u00b7ge", "glei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "nd mu\u00df der Demant nicht", "tokens": ["nd", "mu\u00df", "der", "De\u00b7mant", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "PTKNEG"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "o des Carfunckels Licht", "tokens": ["o", "des", "Car\u00b7fun\u00b7ckels", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["FM", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "urch Nacht und Schatten bricht/", "tokens": ["urch", "Nacht", "und", "Schat\u00b7ten", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit Scham entweichen?", "tokens": ["Mit", "Scham", "ent\u00b7wei\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Verliebte/ wollt ihr wohl die Schiffahrt enden/", "tokens": ["Ver\u00b7lieb\u00b7te", "/", "wollt", "ihr", "wohl", "die", "Schif\u00b7fahrt", "en\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und an den sichern Port des Gl\u00fcckes l\u00e4nden.", "tokens": ["Und", "an", "den", "si\u00b7chern", "Port", "des", "Gl\u00fc\u00b7ckes", "l\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Last blauer Augen Schein", "tokens": ["Last", "blau\u00b7er", "Au\u00b7gen", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Liebe Leitstern seyn/", "tokens": ["Der", "Lie\u00b7be", "Leits\u00b7tern", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So wird sich eure Pein", "tokens": ["So", "wird", "sich", "eu\u00b7re", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In Freude wenden.", "tokens": ["In", "Freu\u00b7de", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Traut schwartzen Augen nicht und ihrem Blincken/", "tokens": ["Traut", "schwart\u00b7zen", "Au\u00b7gen", "nicht", "und", "ih\u00b7rem", "Blin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKNEG", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn sie Sirenen gleich ins Netze wincken.", "tokens": ["Wenn", "sie", "Si\u00b7re\u00b7nen", "gleich", "ins", "Net\u00b7ze", "win\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sieht man in schwartzer Flutt", "tokens": ["Sieht", "man", "in", "schwart\u00b7zer", "Flutt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Voll Falsch und Wanckelmutt", "tokens": ["Voll", "Falsch", "und", "Wan\u00b7ckel\u00b7mutt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nicht offters Schiff und Gutt", "tokens": ["Nicht", "off\u00b7ters", "Schiff", "und", "Gutt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Zu Grunde sincken?", "tokens": ["Zu", "Grun\u00b7de", "sin\u00b7cken", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Ein blaues Auge spielt mit sanfften Wellen:", "tokens": ["Ein", "blau\u00b7es", "Au\u00b7ge", "spielt", "mit", "sanff\u00b7ten", "Wel\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Man sah aus blauer See die Venus quellen.", "tokens": ["Man", "sah", "aus", "blau\u00b7er", "See", "die", "Ve\u00b7nus", "quel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was Wunder/ wenn noch izt", "tokens": ["Was", "Wun\u00b7der", "/", "wenn", "noch", "izt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "NN", "$(", "KOUS", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Cupido drinnen sizt/", "tokens": ["Cu\u00b7pi\u00b7do", "drin\u00b7nen", "sizt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und goldne Pfeile spizt/", "tokens": ["Und", "gold\u00b7ne", "Pfei\u00b7le", "spizt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die Welt zu f\u00e4llen?", "tokens": ["Die", "Welt", "zu", "f\u00e4l\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Welch kaltes Hertze will nicht Flammen fangen/", "tokens": ["Welch", "kal\u00b7tes", "Hert\u00b7ze", "will", "nicht", "Flam\u00b7men", "fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VVFIN", "VMFIN", "PTKNEG", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn mitten in dem Schnee der Rosen-Wangen", "tokens": ["Wenn", "mit\u00b7ten", "in", "dem", "Schnee", "der", "Ro\u00b7sen\u00b7Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit blauer Liebligkeit/", "tokens": ["Mit", "blau\u00b7er", "Lieb\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Daraus ihm selbst ein Kleid", "tokens": ["Da\u00b7raus", "ihm", "selbst", "ein", "Kleid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Himmel zubereit/", "tokens": ["Der", "Him\u00b7mel", "zu\u00b7be\u00b7reit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die Augen prangen!", "tokens": ["Die", "Au\u00b7gen", "pran\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}