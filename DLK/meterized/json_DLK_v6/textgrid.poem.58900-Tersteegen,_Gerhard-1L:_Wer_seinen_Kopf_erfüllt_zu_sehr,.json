{"textgrid.poem.58900": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer seinen Kopf erf\u00fcllt zu sehr,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer seinen Kopf erf\u00fcllt zu sehr,", "tokens": ["Wer", "sei\u00b7nen", "Kopf", "er\u00b7f\u00fcllt", "zu", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem bleibt sein Herze d\u00fcrr und leer;", "tokens": ["Dem", "bleibt", "sein", "Her\u00b7ze", "d\u00fcrr", "und", "leer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAINF", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein stiller Geist erkennt die Wahrheit,", "tokens": ["Ein", "stil\u00b7ler", "Geist", "er\u00b7kennt", "die", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Viel Bilder hindern seine Klarheit.", "tokens": ["Viel", "Bil\u00b7der", "hin\u00b7dern", "sei\u00b7ne", "Klar\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}