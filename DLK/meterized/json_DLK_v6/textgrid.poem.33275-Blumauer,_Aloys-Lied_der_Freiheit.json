{"textgrid.poem.33275": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Lied der Freiheit", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer unter eines M\u00e4dchens Hand", "tokens": ["Wer", "un\u00b7ter", "ei\u00b7nes", "M\u00e4d\u00b7chens", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich als ein Sklave schmiegt,", "tokens": ["Sich", "als", "ein", "Skla\u00b7ve", "schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und von der Liebe festgebannt,", "tokens": ["Und", "von", "der", "Lie\u00b7be", "fest\u00b7ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In schn\u00f6den Fesseln liegt,", "tokens": ["In", "schn\u00f6\u00b7den", "Fes\u00b7seln", "liegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Weh dem! der ist ein armer Wicht,", "tokens": ["Weh", "dem", "!", "der", "ist", "ein", "ar\u00b7mer", "Wicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$.", "ART", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er kennt die gold'ne Freiheit nicht.", "tokens": ["Er", "kennt", "die", "gold'\u00b7ne", "Frei\u00b7heit", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer sich um F\u00fcrstengunst und Rang", "tokens": ["Wer", "sich", "um", "F\u00fcrs\u00b7ten\u00b7gunst", "und", "Rang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit saurem Schwei\u00df bem\u00fcht,", "tokens": ["Mit", "sau\u00b7rem", "Schwei\u00df", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und eingespannt sein lebelang,", "tokens": ["Und", "ein\u00b7ge\u00b7spannt", "sein", "le\u00b7be\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Pflug des Staates zieht.", "tokens": ["Am", "Pflug", "des", "Staa\u00b7tes", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Weh dem! der ist ein armer Wicht,", "tokens": ["Weh", "dem", "!", "der", "ist", "ein", "ar\u00b7mer", "Wicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$.", "ART", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er kennt die gold'ne Freiheit nicht.", "tokens": ["Er", "kennt", "die", "gold'\u00b7ne", "Frei\u00b7heit", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer um ein schimmerndes Metall", "tokens": ["Wer", "um", "ein", "schim\u00b7mern\u00b7des", "Me\u00b7tall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem b\u00f6sen Mamon dient,", "tokens": ["Dem", "b\u00f6\u00b7sen", "Ma\u00b7mon", "dient", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und seiner vollen S\u00e4cke Zahl", "tokens": ["Und", "sei\u00b7ner", "vol\u00b7len", "S\u00e4\u00b7cke", "Zahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur zu vermehren sinnt!", "tokens": ["Nur", "zu", "ver\u00b7meh\u00b7ren", "sinnt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Weh dem! der ist ein armer Wicht,", "tokens": ["Weh", "dem", "!", "der", "ist", "ein", "ar\u00b7mer", "Wicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$.", "ART", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er kennt die gold'ne Freiheit nicht.", "tokens": ["Er", "kennt", "die", "gold'\u00b7ne", "Frei\u00b7heit", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch wer dies alles leicht entbehrt,", "tokens": ["Doch", "wer", "dies", "al\u00b7les", "leicht", "ent\u00b7behrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PDS", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wornach der Thor nur strebt,", "tokens": ["Wor\u00b7nach", "der", "Thor", "nur", "strebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und froh bei seinem eignen Heerd", "tokens": ["Und", "froh", "bei", "sei\u00b7nem", "eig\u00b7nen", "Heerd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur sich, nie Andern lebt,", "tokens": ["Nur", "sich", ",", "nie", "An\u00b7dern", "lebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "$,", "ADV", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der ist's allein, der sagen kann:", "tokens": ["Der", "ist's", "al\u00b7lein", ",", "der", "sa\u00b7gen", "kann", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$,", "PRELS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wohl mir, ich bin ein freier Mann!", "tokens": ["Wohl", "mir", ",", "ich", "bin", "ein", "frei\u00b7er", "Mann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}