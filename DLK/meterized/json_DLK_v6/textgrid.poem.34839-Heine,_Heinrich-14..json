{"textgrid.poem.34839": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ali Bei, der Held des Glaubens,", "tokens": ["A\u00b7li", "Bei", ",", "der", "Held", "des", "Glau\u00b7bens", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegt begl\u00fcckt in M\u00e4dchenarmen.", "tokens": ["Liegt", "be\u00b7gl\u00fcckt", "in", "M\u00e4d\u00b7chen\u00b7ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vorgeschmack des Paradieses", "tokens": ["Vor\u00b7ge\u00b7schmack", "des", "Pa\u00b7ra\u00b7die\u00b7ses"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "G\u00f6nnt ihm Allah schon auf Erden.", "tokens": ["G\u00f6nnt", "ihm", "Al\u00b7lah", "schon", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADV", "APPR", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Odalisken, sch\u00f6n wie Huris,", "tokens": ["O\u00b7da\u00b7lis\u00b7ken", ",", "sch\u00f6n", "wie", "Hu\u00b7ris", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KOKOM", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und geschmeidig wie Gazellen \u2013", "tokens": ["Und", "ge\u00b7schmei\u00b7dig", "wie", "Ga\u00b7zel\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kr\u00e4uselt ihm den Bart die eine,", "tokens": ["Kr\u00e4u\u00b7selt", "ihm", "den", "Bart", "die", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00e4ttet seine Stirn die andre.", "tokens": ["Gl\u00e4t\u00b7tet", "sei\u00b7ne", "Stirn", "die", "and\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und die dritte schl\u00e4gt die Laute,", "tokens": ["Und", "die", "drit\u00b7te", "schl\u00e4gt", "die", "Lau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singt und tanzt, und k\u00fc\u00dft ihn lachend", "tokens": ["Singt", "und", "tanzt", ",", "und", "k\u00fc\u00dft", "ihn", "la\u00b7chend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "$,", "KON", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf das Herz, worin die Flammen", "tokens": ["Auf", "das", "Herz", ",", "wo\u00b7rin", "die", "Flam\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aller Seligkeiten lodern.", "tokens": ["Al\u00b7ler", "Se\u00b7lig\u00b7kei\u00b7ten", "lo\u00b7dern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber drau\u00dfen pl\u00f6tzlich schmettern", "tokens": ["A\u00b7ber", "drau\u00b7\u00dfen", "pl\u00f6tz\u00b7lich", "schmet\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Trompeten, Schwerter rasseln,", "tokens": ["Die", "Trom\u00b7pe\u00b7ten", ",", "Schwer\u00b7ter", "ras\u00b7seln", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Waffenruf und Flintensch\u00fcsse \u2013", "tokens": ["Waf\u00b7fen\u00b7ruf", "und", "Flin\u00b7ten\u00b7sch\u00fcs\u00b7se", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbherr, die Franken sind im Anmarsch!\u00ab", "tokens": ["\u00bb", "herr", ",", "die", "Fran\u00b7ken", "sind", "im", "An\u00b7marsch", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$,", "ART", "NN", "VAFIN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und der Held besteigt sein Schlachtro\u00df,", "tokens": ["Und", "der", "Held", "be\u00b7steigt", "sein", "Schlach\u00b7tro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Fliegt zum Kampf, doch wie im Traume; \u2013", "tokens": ["Fliegt", "zum", "Kampf", ",", "doch", "wie", "im", "Trau\u00b7me", ";", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "ADV", "KOKOM", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ihm ist zu Sinn, als l\u00e4g er", "tokens": ["Denn", "ihm", "ist", "zu", "Sinn", ",", "als", "l\u00e4g", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "NN", "$,", "KOUS", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer noch in M\u00e4dchenarmen.", "tokens": ["Im\u00b7mer", "noch", "in", "M\u00e4d\u00b7chen\u00b7ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "W\u00e4hrend er die Frankenk\u00f6pfe", "tokens": ["W\u00e4h\u00b7rend", "er", "die", "Fran\u00b7ken\u00b7k\u00f6p\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dutzendweis' herunters\u00e4belt,", "tokens": ["Dut\u00b7zend\u00b7weis'", "her\u00b7un\u00b7ter\u00b7s\u00e4\u00b7belt", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4chelt er wie ein Verliebter,", "tokens": ["L\u00e4\u00b7chelt", "er", "wie", "ein", "Ver\u00b7lieb\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ja, er l\u00e4chelt sanft und z\u00e4rtlich.", "tokens": ["Ja", ",", "er", "l\u00e4\u00b7chelt", "sanft", "und", "z\u00e4rt\u00b7lich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}