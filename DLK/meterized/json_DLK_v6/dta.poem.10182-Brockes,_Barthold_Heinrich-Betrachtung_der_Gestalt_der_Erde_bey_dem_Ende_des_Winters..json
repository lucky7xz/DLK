{"dta.poem.10182": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung der Gestalt der Erde bey  \n dem Ende des Winters.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der rauhe Februarius", "tokens": ["Der", "rau\u00b7he", "Feb\u00b7ru\u00b7a\u00b7rius"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "War allbereit zum l\u00e4ngst verlangten Schlu\u00df,", "tokens": ["War", "all\u00b7be\u00b7reit", "zum", "l\u00e4ngst", "ver\u00b7lang\u00b7ten", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bey einer heitern Lufft, gekommen.", "tokens": ["Bey", "ei\u00b7ner", "hei\u00b7tern", "Lufft", ",", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mertz hatt seinen Platz kaum wieder eingenommen.", "tokens": ["Der", "Mertz", "hatt", "sei\u00b7nen", "Platz", "kaum", "wie\u00b7der", "ein\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Sonne zeigte schon, bey unsrer Wiederkehr,", "tokens": ["Die", "Son\u00b7ne", "zeig\u00b7te", "schon", ",", "bey", "uns\u00b7rer", "Wie\u00b7der\u00b7kehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Die Erstling\u2019 ihrer neuen Liebe,", "tokens": ["Die", "Erst\u00b7ling'", "ih\u00b7rer", "neu\u00b7en", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Indem ihr sanfter Strahl ie mehr und mehr", "tokens": ["In\u00b7dem", "ihr", "sanf\u00b7ter", "Strahl", "ie", "mehr", "und", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die W\u00e4rme durch den Lufft-Kreis triebe.", "tokens": ["Die", "W\u00e4r\u00b7me", "durch", "den", "Lufft\u00b7Kreis", "trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Das Feld war hie und da mit Schnee annoch bedeckt,", "tokens": ["Das", "Feld", "war", "hie", "und", "da", "mit", "Schnee", "an\u00b7noch", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "KON", "ADV", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Doch ward er hie und da schon gleichsam aufgeleckt,", "tokens": ["Doch", "ward", "er", "hie", "und", "da", "schon", "gleich\u00b7sam", "auf\u00b7ge\u00b7leckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "KON", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Theils flo\u00df er aufgel\u00f6st, und dort verband er sich", "tokens": ["Theils", "flo\u00df", "er", "auf\u00b7ge\u00b7l\u00f6st", ",", "und", "dort", "ver\u00b7band", "er", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "VVPP", "$,", "KON", "ADV", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Aufs neue durch den Frost, im Schatten sonderlich,", "tokens": ["Aufs", "neu\u00b7e", "durch", "den", "Frost", ",", "im", "Schat\u00b7ten", "son\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "APPR", "ART", "NN", "$,", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und ward daselbst zu glattem Eise.", "tokens": ["Und", "ward", "da\u00b7selbst", "zu", "glat\u00b7tem", "Ei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Die\u00df funckelte nun hie und dort,", "tokens": ["Die\u00df", "fun\u00b7ckel\u00b7te", "nun", "hie", "und", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Zumahl an manchem andern Ort,", "tokens": ["Zu\u00b7mahl", "an", "man\u00b7chem", "an\u00b7dern", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und sonderlich in mancher Wagen-Gleise.", "tokens": ["Und", "son\u00b7der\u00b7lich", "in", "man\u00b7cher", "Wa\u00b7gen\u00b7Glei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die\u00df sah\u2019 ich j\u00fcngst mit Lust, zur hellen Mittags-Zeit,", "tokens": ["Die\u00df", "sah'", "ich", "j\u00fcngst", "mit", "Lust", ",", "zur", "hel\u00b7len", "Mit\u00b7tags\u00b7Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In einer reinen Heiterkeit,", "tokens": ["In", "ei\u00b7ner", "rei\u00b7nen", "Hei\u00b7ter\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es gl\u00e4ntzt\u2019 und schien das Feld, als w\u00e4ren Berg-Crystallen", "tokens": ["Es", "gl\u00e4ntzt'", "und", "schien", "das", "Feld", ",", "als", "w\u00e4\u00b7ren", "Ber\u00b7gCry\u00b7stal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,", "KOKOM", "VAFIN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dar\u00fcber hergelegt. Die Landschaft glimmt und gl\u00fcht,", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "her\u00b7ge\u00b7legt", ".", "Die", "Land\u00b7schaft", "glimmt", "und", "gl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "$.", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wann unser Blick den Strahl der Sonnen r\u00fcckw\u00e4rts prallen,", "tokens": ["Wann", "un\u00b7ser", "Blick", "den", "Strahl", "der", "Son\u00b7nen", "r\u00fcck\u00b7w\u00e4rts", "pral\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und doppelt heller gl\u00e4ntzen sieht.", "tokens": ["Und", "dop\u00b7pelt", "hel\u00b7ler", "gl\u00e4nt\u00b7zen", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die\u00df that den Augen wol. Allein,", "tokens": ["Die\u00df", "that", "den", "Au\u00b7gen", "wol", ".", "Al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "$.", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "An andern Orten lie\u00df uns eben dieser Schein", "tokens": ["An", "an\u00b7dern", "Or\u00b7ten", "lie\u00df", "uns", "e\u00b7ben", "die\u00b7ser", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein traurigs Schau-Spiel sehn.", "tokens": ["Ein", "trau\u00b7rigs", "Schau\u00b7Spiel", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Da, wo der Schnee geschmoltzen war,", "tokens": ["Da", ",", "wo", "der", "Schnee", "ge\u00b7schmolt\u00b7zen", "war", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Erblickte man der Erden d\u00fcnnes Haar", "tokens": ["Er\u00b7blick\u00b7te", "man", "der", "Er\u00b7den", "d\u00fcn\u00b7nes", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Verwirrt, vermodert, welck. Das gelblich bleiche Gras", "tokens": ["Ver\u00b7wirrt", ",", "ver\u00b7mo\u00b7dert", ",", "welck", ".", "Das", "gelb\u00b7lich", "blei\u00b7che", "Gras"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "VVPP", "$,", "PWAT", "$.", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Lie\u00df der vorhin erhabnen Spitzen prangen", "tokens": ["Lie\u00df", "der", "vor\u00b7hin", "er\u00b7hab\u00b7nen", "Spit\u00b7zen", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "In schmutzig grauer Farb\u2019 erb\u00e4rmlich abw\u00e4rts hangen.", "tokens": ["In", "schmut\u00b7zig", "grau\u00b7er", "Fa\u00b7rb'", "er\u00b7b\u00e4rm\u00b7lich", "ab\u00b7w\u00e4rts", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Erde selber war morastig, h\u00e4\u00dflich na\u00df,", "tokens": ["Die", "Er\u00b7de", "sel\u00b7ber", "war", "mo\u00b7ras\u00b7tig", ",", "h\u00e4\u00df\u00b7lich", "na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr morscher C\u00f6rper schien zu faulen und zu g\u00e4hren,", "tokens": ["Ihr", "mor\u00b7scher", "C\u00f6r\u00b7per", "schien", "zu", "fau\u00b7len", "und", "zu", "g\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr Fleisch in z\u00e4hen Schlamm, in Wust ihr Nahrungs-", "tokens": ["Ihr", "Fleisch", "in", "z\u00e4\u00b7hen", "Schlamm", ",", "in", "Wust", "ihr", "Nah\u00b7rungs"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$,", "APPR", "NN", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Und ihre Haut in Koth, sich zu verkehren.", "tokens": ["Und", "ih\u00b7re", "Haut", "in", "Koth", ",", "sich", "zu", "ver\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NE", "$,", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was man von ihr erblickt\u2019, war schl\u00fcpfrig, eckelhaft,", "tokens": ["Was", "man", "von", "ihr", "er\u00b7blickt'", ",", "war", "schl\u00fcpf\u00b7rig", ",", "ec\u00b7kel\u00b7haft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Schwartz, sumpfig, ungestalt. Der Anblick r\u00fchrte mich,", "tokens": ["Schwartz", ",", "sump\u00b7fig", ",", "un\u00b7ge\u00b7stalt", ".", "Der", "An\u00b7blick", "r\u00fchr\u00b7te", "mich", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADJD", "$.", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die traurige Figur und Farbe stellte sich", "tokens": ["Die", "trau\u00b7ri\u00b7ge", "Fi\u00b7gur", "und", "Far\u00b7be", "stell\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VVFIN", "PRF"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.8": {"text": "Den Sinnen eigentlich,", "tokens": ["Den", "Sin\u00b7nen", "ei\u00b7gent\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Als eine Art Verwesung, dar.", "tokens": ["Als", "ei\u00b7ne", "Art", "Ver\u00b7we\u00b7sung", ",", "dar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mu\u00df unsre Mutter auch so gar", "tokens": ["Mu\u00df", "uns\u00b7re", "Mut\u00b7ter", "auch", "so", "gar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die F\u00e4ulni\u00df und Verwesung leiden!", "tokens": ["Die", "F\u00e4ul\u00b7ni\u00df", "und", "Ver\u00b7we\u00b7sung", "lei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Rieff ich betr\u00fcbt. Allein,", "tokens": ["Rieff", "ich", "be\u00b7tr\u00fcbt", ".", "Al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$.", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie bald verschwand das Trauren! da der Schein", "tokens": ["Wie", "bald", "ver\u00b7schwand", "das", "Trau\u00b7ren", "!", "da", "der", "Schein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "NN", "$.", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Glantz der Wahrheit mich ein besseres belehrte:", "tokens": ["Und", "Glantz", "der", "Wahr\u00b7heit", "mich", "ein", "bes\u00b7se\u00b7res", "be\u00b7lehr\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "PPER", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df diese F\u00e4ulni\u00df sich in Fruchtbarkeit,", "tokens": ["Da\u00df", "die\u00b7se", "F\u00e4ul\u00b7ni\u00df", "sich", "in", "Frucht\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die H\u00e4\u00dflichkeit in Sch\u00f6nheit sich verkehrte:", "tokens": ["Die", "H\u00e4\u00df\u00b7lich\u00b7keit", "in", "Sch\u00f6n\u00b7heit", "sich", "ver\u00b7kehr\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und da\u00df sie, in gar kurtzer Zeit,", "tokens": ["Und", "da\u00df", "sie", ",", "in", "gar", "kurt\u00b7zer", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$,", "APPR", "A