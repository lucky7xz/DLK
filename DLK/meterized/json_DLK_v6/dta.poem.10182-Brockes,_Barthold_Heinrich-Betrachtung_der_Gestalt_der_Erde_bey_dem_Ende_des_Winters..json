bic.tetra"}, "line.7": {"text": "Als Dem es ja an Macht und Liebe nicht gebricht,", "tokens": ["Als", "Dem", "es", "ja", "an", "Macht", "und", "Lie\u00b7be", "nicht", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPER", "ADV", "APPR", "NN", "KON", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In jenes seelgen Lebens Lentzen", "tokens": ["In", "je\u00b7nes", "seel\u00b7gen", "Le\u00b7bens", "Lent\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Geschm\u00fcckt, verherrlichet, in einem ew\u2019gen Licht,", "tokens": ["Ge\u00b7schm\u00fcckt", ",", "ver\u00b7herr\u00b7li\u00b7chet", ",", "in", "ei\u00b7nem", "ew'\u00b7gen", "Licht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und unverg\u00e4nglicher Verkl\u00e4rung, werden gl\u00e4ntzen.", "tokens": ["Und", "un\u00b7ver\u00b7g\u00e4ng\u00b7li\u00b7cher", "Ver\u00b7kl\u00e4\u00b7rung", ",", "wer\u00b7den", "gl\u00e4nt\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}