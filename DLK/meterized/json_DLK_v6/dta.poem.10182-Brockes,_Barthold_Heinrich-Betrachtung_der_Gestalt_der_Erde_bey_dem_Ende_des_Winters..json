in:", "tokens": ["Ach", "!", "fiel", "mir", "fer\u00b7ner", "noch", ",", "bey", "der", "Be\u00b7trach\u00b7tung", ",", "ein", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "ADV", "$,", "APPR", "ART", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie kann uns Menschen doch die\u00df Bild der Erden", "tokens": ["Wie", "kann", "uns", "Men\u00b7schen", "doch", "die\u00df", "Bild", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "NN", "ADV", "PDS", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Solch ein vortreffliches und Lehr-reich Sinn-Bild seyn!", "tokens": ["Solch", "ein", "vor\u00b7treff\u00b7li\u00b7ches", "und", "Lehr\u00b7reich", "Sinn\u00b7Bild", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "KON", "NE", "NN", "VAINF", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Wer glaubte wol, wenn er es sonst nicht w\u00fcste,", "tokens": ["Wer", "glaub\u00b7te", "wol", ",", "wenn", "er", "es", "sonst", "nicht", "w\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und der Erfahrung weichen m\u00fcste,", "tokens": ["Und", "der", "Er\u00b7fah\u00b7rung", "wei\u00b7chen", "m\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00d