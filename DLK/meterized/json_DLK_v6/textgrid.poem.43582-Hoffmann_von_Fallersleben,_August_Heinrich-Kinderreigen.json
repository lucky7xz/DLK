{"textgrid.poem.43582": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Kinderreigen", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So geht's auf unsre Weise,", "tokens": ["So", "geht's", "auf", "uns\u00b7re", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So ist es Kinderbrauch:", "tokens": ["So", "ist", "es", "Kin\u00b7der\u00b7brauch", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir dreh'n uns um im Kreise", "tokens": ["Wir", "dreh'n", "uns", "um", "im", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und tanzen auch \u2013", "tokens": ["Und", "tan\u00b7zen", "auch", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Immer herum,", "tokens": ["Im\u00b7mer", "he\u00b7rum", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Bum bidibum! bum!", "tokens": ["Bum", "bi\u00b7di\u00b7bum", "!", "bum", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["FM", "FM", "$.", "XY", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wir brauchen keine Geigen", "tokens": ["Wir", "brau\u00b7chen", "kei\u00b7ne", "Gei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und brauchen keinen Ba\u00df,", "tokens": ["Und", "brau\u00b7chen", "kei\u00b7nen", "Ba\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir spielen uns zum Reigen", "tokens": ["Wir", "spie\u00b7len", "uns", "zum", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schon selber was.", "tokens": ["Schon", "sel\u00b7ber", "was", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Immer herum,", "tokens": ["Im\u00b7mer", "he\u00b7rum", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Bum bidibum! bum!", "tokens": ["Bum", "bi\u00b7di\u00b7bum", "!", "bum", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["FM", "FM", "$.", "XY", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Und ist ein Tanz zu Ende,", "tokens": ["Und", "ist", "ein", "Tanz", "zu", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gleich f\u00e4ngt ein andrer an,", "tokens": ["Gleich", "f\u00e4ngt", "ein", "an\u00b7drer", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir reichen uns die H\u00e4nde", "tokens": ["Wir", "rei\u00b7chen", "uns", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und tanzen dann", "tokens": ["Und", "tan\u00b7zen", "dann"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Immer herum,", "tokens": ["Im\u00b7mer", "he\u00b7rum", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Bum bidibum! bum!", "tokens": ["Bum", "bi\u00b7di\u00b7bum", "!", "bum", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["FM", "FM", "$.", "XY", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}