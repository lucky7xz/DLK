{"dta.poem.10016": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Vergn\u00fcgungen  \n  der Melancholey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20674-3", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Heil dir, also, geweyhte Betrachtung! o G\u00f6ttin,", "tokens": ["Heil", "dir", ",", "al\u00b7so", ",", "ge\u00b7weyh\u00b7te", "Be\u00b7trach\u00b7tung", "!", "o", "G\u00f6t\u00b7tin", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADV", "$,", "ADJA", "NN", "$.", "FM", "NN", "$,"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dieser Gesang sich an, mit dir auch soll er sich enden.", "tokens": ["Die\u00b7ser", "Ge\u00b7sang", "sich", "an", ",", "mit", "dir", "auch", "soll", "er", "sich", "en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PRF", "PTKVZ", "$,", "APPR", "PPER", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+--+-+---+--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Du bist sch\u00f6ner, als alle die Nymphen der Grotte von", "tokens": ["Du", "bist", "sch\u00f6\u00b7ner", ",", "als", "al\u00b7le", "die", "Nym\u00b7phen", "der", "Grot\u00b7te", "von"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "KOUS", "PIS", "ART", "NN", "ART", "NN", "APPR"], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und du kanst den Gedanken zu h\u00f6hern Entz\u00fcckungen", "tokens": ["Und", "du", "kanst", "den", "Ge\u00b7dan\u00b7ken", "zu", "h\u00f6\u00b7hern", "Ent\u00b7z\u00fc\u00b7ckun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "--+--+--+-+-+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Als die gepriesene Schaar von allen G\u00f6ttern der Fabel.", "tokens": ["Als", "die", "ge\u00b7prie\u00b7se\u00b7ne", "Schaar", "von", "al\u00b7len", "G\u00f6t\u00b7tern", "der", "Fa\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Heil dir, o G\u00f6ttin! dich fand, so wie die Sage", "tokens": ["Heil", "dir", ",", "o", "G\u00f6t\u00b7tin", "!", "dich", "fand", ",", "so", "wie", "die", "Sa\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "FM", "NN", "$.", "PPER", "VVFIN", "$,", "ADV", "KOKOM", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Einst ein Druide, so wie er am Abend die W\u00e4lder von", "tokens": ["Einst", "ein", "Drui\u00b7de", ",", "so", "wie", "er", "am", "A\u00b7bend", "die", "W\u00e4l\u00b7der", "von"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "KOKOM", "PPER", "APPRART", "NN", "ART", "NN", "APPR"], "meter": "--+--+--+--+-+", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Einsam durchirrt; er trug dich sogleich mit g\u00fctigen", "tokens": ["Ein\u00b7sam", "durch\u00b7irrt", ";", "er", "trug", "dich", "sog\u00b7leich", "mit", "g\u00fc\u00b7ti\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$.", "PPER", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Zum beschirmenden Dach von seiner Laube von Eichen.", "tokens": ["Zum", "be\u00b7schir\u00b7men\u00b7den", "Dach", "von", "sei\u00b7ner", "Lau\u00b7be", "von", "Ei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Hier bemerkte gar bald der bewundernde Weise den An-", "tokens": ["Hier", "be\u00b7merk\u00b7te", "gar", "bald", "der", "be\u00b7wun\u00b7dern\u00b7de", "Wei\u00b7se", "den", "An"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "ART", "TRUNC"], "meter": "--+--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.11": {"text": "Deiner Schwermuth, den m\u00e4chtigen Hang zu ernsten", "tokens": ["Dei\u00b7ner", "Schwer\u00b7muth", ",", "den", "m\u00e4ch\u00b7ti\u00b7gen", "Hang", "zu", "erns\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Noch als ein l\u00e4chelndes Kind hast du am Ufer des", "tokens": ["Noch", "als", "ein", "l\u00e4\u00b7cheln\u00b7des", "Kind", "hast", "du", "am", "U\u00b7fer", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "VAFIN", "PPER", "APPRART", "NN", "ART"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Diesem verewigten Strom der alten Druiden, gelegen,", "tokens": ["Die\u00b7sem", "ve\u00b7re\u00b7wig\u00b7ten", "Strom", "der", "al\u00b7ten", "Dru\u00b7i\u00b7den", ",", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "ART", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Und dich am wilden Ger\u00e4usch von seinen Fluthen er-", "tokens": ["Und", "dich", "am", "wil\u00b7den", "Ge\u00b7r\u00e4usch", "von", "sei\u00b7nen", "Flut\u00b7hen", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}