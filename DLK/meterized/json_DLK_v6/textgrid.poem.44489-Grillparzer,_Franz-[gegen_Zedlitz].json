{"textgrid.poem.44489": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[gegen Zedlitz]", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.85", "no:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott erhalte unsern **", "tokens": ["Gott", "er\u00b7hal\u00b7te", "un\u00b7sern", "*", "*"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "XY", "XY"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gott erhalt ihn fett und feist,", "tokens": ["Gott", "er\u00b7halt", "ihn", "fett", "und", "feist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Allen Menschen Gutes g\u00f6nnend,", "tokens": ["Al\u00b7len", "Men\u00b7schen", "Gu\u00b7tes", "g\u00f6n\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch, wie billig, sich zumeist.", "tokens": ["Doch", ",", "wie", "bil\u00b7lig", ",", "sich", "zu\u00b7meist", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ADJD", "$,", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Halt ihn frei von Nahrungsorgen,", "tokens": ["Halt", "ihn", "frei", "von", "Nah\u00b7rungs\u00b7or\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00fcnz ihm durch die Presse Geld,", "tokens": ["M\u00fcnz", "ihm", "durch", "die", "Pres\u00b7se", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und will Harpagon nicht borgen,", "tokens": ["Und", "will", "Har\u00b7pa\u00b7gon", "nicht", "bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NE", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei ein Sosias geprellt.", "tokens": ["Sei", "ein", "So\u00b7si\u00b7as", "ge\u00b7prellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Von der tiefsten Herzensmeinung", "tokens": ["Von", "der", "tiefs\u00b7ten", "Her\u00b7zens\u00b7mei\u00b7nung"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schneid er ab der Verse Zahl,", "tokens": ["Schneid", "er", "ab", "der", "Ver\u00b7se", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ists mit den Koupons zu Ende,", "tokens": ["Ists", "mit", "den", "Kou\u00b7pons", "zu", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Geht zu Markt das Kapital.", "tokens": ["Geht", "zu", "Markt", "das", "Ka\u00b7pi\u00b7tal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wahre gn\u00e4dig seinen Leichnam,", "tokens": ["Wah\u00b7re", "gn\u00e4\u00b7dig", "sei\u00b7nen", "Leich\u00b7nam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der du Bauch und Sph\u00e4ren w\u00f6lbst,", "tokens": ["Der", "du", "Bauch", "und", "Sph\u00e4\u00b7ren", "w\u00f6lbst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die abgescho\u00dfne Kugel", "tokens": ["Da\u00df", "die", "ab\u00b7ge\u00b7scho\u00df\u00b7ne", "Ku\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Streng sich drehe um sich selbst,", "tokens": ["Streng", "sich", "dre\u00b7he", "um", "sich", "selbst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVFIN", "APPR", "PRF", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Deck ihm t\u00e4glich seine Tafel,", "tokens": ["Deck", "ihm", "t\u00e4g\u00b7lich", "sei\u00b7ne", "Ta\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Im Palast, im M\u00e4klersaal,", "tokens": ["Im", "Pa\u00b7last", ",", "im", "M\u00e4k\u00b7ler\u00b7saal", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bei Karlisten, bei Christinos,", "tokens": ["Bei", "Kar\u00b7lis\u00b7ten", ",", "bei", "Chris\u00b7ti\u00b7nos", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Wer traktiert, ist liberal.", "tokens": ["Wer", "trak\u00b7tiert", ",", "ist", "li\u00b7be\u00b7ral", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Halt gel\u00e4ufig seine Zunge,", "tokens": ["Halt", "ge\u00b7l\u00e4u\u00b7fig", "sei\u00b7ne", "Zun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "G\u00f6nnt im h\u00f6chsten Freiheitston", "tokens": ["G\u00f6nnt", "im", "h\u00f6chs\u00b7ten", "Frei\u00b7heits\u00b7ton"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er zur leichteren Verdauung", "tokens": ["Er", "zur", "leich\u00b7te\u00b7ren", "Ver\u00b7dau\u00b7ung"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr nach Tische Motion,", "tokens": ["Ihr", "nach", "Ti\u00b7sche", "Mo\u00b7ti\u00b7on", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aber auch la\u00df nie ihn ahnen,", "tokens": ["A\u00b7ber", "auch", "la\u00df", "nie", "ihn", "ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVIMP", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ein Wort zugleich ein Mann,", "tokens": ["Da\u00df", "ein", "Wort", "zu\u00b7gleich", "ein", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Mund, der eitel geifert,", "tokens": ["Und", "der", "Mund", ",", "der", "ei\u00b7tel", "gei\u00b7fert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seinen Eigner spucket an.", "tokens": ["Sei\u00b7nen", "Eig\u00b7ner", "spu\u00b7cket", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und so leb er bis ans Ende,", "tokens": ["Und", "so", "leb", "er", "bis", "ans", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein verz\u00e4rtelt gl\u00fccklich Kind,", "tokens": ["Ein", "ver\u00b7z\u00e4r\u00b7telt", "gl\u00fcck\u00b7lich", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wei\u00df man doch, da\u00df Dichterworte", "tokens": ["Wei\u00df", "man", "doch", ",", "da\u00df", "Dich\u00b7ter\u00b7wor\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "$,", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dichtung, ei, und Worte sind.", "tokens": ["Dich\u00b7tung", ",", "ei", ",", "und", "Wor\u00b7te", "sind", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "KON", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}