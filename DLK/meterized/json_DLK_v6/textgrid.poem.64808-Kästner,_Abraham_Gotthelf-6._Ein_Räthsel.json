{"textgrid.poem.64808": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "6. Ein R\u00e4thsel", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Beystand mehrt des Weisen Wissen,", "tokens": ["Mein", "Beys\u00b7tand", "mehrt", "des", "Wei\u00b7sen", "Wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn er mit festverkn\u00fcpften Schl\u00fcssen", "tokens": ["Wenn", "er", "mit", "fest\u00b7ver\u00b7kn\u00fcpf\u00b7ten", "Schl\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Ich kann des Staatsmanns Ehrgeiz stillen,", "tokens": ["Ich", "kann", "des", "Staats\u00b7manns", "Ehr\u00b7geiz", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich pflege sein Gehirn zu f\u00fcllen,", "tokens": ["Ich", "pfle\u00b7ge", "sein", "Ge\u00b7hirn", "zu", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und bin oft seiner Arbeit Frucht:", "tokens": ["Und", "bin", "oft", "sei\u00b7ner", "Ar\u00b7beit", "Frucht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein weiter Raum mu\u00df Alles fassen,", "tokens": ["Mein", "wei\u00b7ter", "Raum", "mu\u00df", "Al\u00b7les", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auch das, was nichts umfassen kann;", "tokens": ["Auch", "das", ",", "was", "nichts", "um\u00b7fas\u00b7sen", "kann", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "$,", "PRELS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Man trifft mich in der Gro\u00dfen Cassen,", "tokens": ["Man", "trifft", "mich", "in", "der", "Gro\u00b7\u00dfen", "Cas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und in der Dichter Liedern an.", "tokens": ["Und", "in", "der", "Dich\u00b7ter", "Lie\u00b7dern", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Das Eintrachtsband von Deutschlands Prinzen,", "tokens": ["Das", "Ein\u00b7trachts\u00b7band", "von", "Deutschlands", "Prin\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Des schlauen Frankreichs Redlichkeit,", "tokens": ["Des", "schlau\u00b7en", "Fran\u00b7kreichs", "Red\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Den Muth der handelnden Provinzen", "tokens": ["Den", "Muth", "der", "han\u00b7deln\u00b7den", "Pro\u00b7vin\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Besitz' ich schon seit langer Zeit.", "tokens": ["Be\u00b7sitz'", "ich", "schon", "seit", "lan\u00b7ger", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Mir gleichen *** kluge Werke,", "tokens": ["Mir", "glei\u00b7chen", "*", "*", "*", "klu\u00b7ge", "Wer\u00b7ke", ","], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "XY", "XY", "XY", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "*** Witz, und *** St\u00e4rke.", "tokens": ["*", "*", "*", "Witz", ",", "und", "*", "*", "*", "St\u00e4r\u00b7ke", "."], "token_info": ["punct", "punct", "punct", "word", "punct", "word", "punct", "punct", "punct", "word", "punct"], "pos": ["XY", "XY", "XY", "NN", "$,", "KON", "XY", "XY", "XY", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}