{"dta.poem.575": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Allerhand sch\u00f6ne Rarit\u00e4ten und sch\u00f6ne  \n Spiele - Wercke/  \n Welche auf dem  \n Binnisch - und Gr\u00e4ntzischen  \n Hochzeit-Feste vorgestellet  \n der Teutsche Welsche Mann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Dis Traum-Buch/ welches ist zu Amsterdam gedruckt/", "tokens": ["Dis", "Traum\u00b7Buch", "/", "wel\u00b7ches", "ist", "zu", "A\u00b7mster\u00b7dam", "ge\u00b7druckt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$(", "PWS", "VAFIN", "APPR", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat ", "tokens": ["Hat"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Die Weiber ", "tokens": ["Die", "Wei\u00b7ber"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Sie nehen insgesammt Brabandsche feine Spitzen.", "tokens": ["Sie", "ne\u00b7hen", "ins\u00b7ge\u00b7sammt", "Bra\u00b7band\u00b7sche", "fei\u00b7ne", "Spit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nun wird die W\u00f6lffin euch gezeigt", "tokens": ["Nun", "wird", "die", "W\u00f6lf\u00b7fin", "euch", "ge\u00b7zeigt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die vormahls ", "tokens": ["Die", "vor\u00b7mahls"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Sch\u00f6ne Rarit\u00e4t/ sch\u00f6ne Spiele-Werckerc.", "tokens": ["Sch\u00f6\u00b7ne", "Ra\u00b7ri\u00b7t\u00e4t", "/", "sch\u00f6\u00b7ne", "Spie\u00b7le\u00b7\u00b7Wer\u00b7ckerc", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}}}}