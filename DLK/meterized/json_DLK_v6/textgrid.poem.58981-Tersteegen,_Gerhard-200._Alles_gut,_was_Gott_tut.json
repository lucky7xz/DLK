{"textgrid.poem.58981": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "200. Alles gut, was Gott tut", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eigne Sorg' und eignes W\u00e4hlen", "tokens": ["Eig\u00b7ne", "Sor\u00b7g'", "und", "eig\u00b7nes", "W\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Bringt nur Furcht und stetes Qu\u00e4len;", "tokens": ["Bringt", "nur", "Furcht", "und", "ste\u00b7tes", "Qu\u00e4\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was Gott gibt und nimmt und tut,", "tokens": ["Was", "Gott", "gibt", "und", "nimmt", "und", "tut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist der Seele alles gut.", "tokens": ["Ist", "der", "See\u00b7le", "al\u00b7les", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PIS", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}