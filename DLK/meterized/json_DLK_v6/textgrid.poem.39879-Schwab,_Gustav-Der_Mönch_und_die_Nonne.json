{"textgrid.poem.39879": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der M\u00f6nch und die Nonne", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst auf der Wartburg Abends frisch,", "tokens": ["Einst", "auf", "der", "Wart\u00b7burg", "A\u00b7bends", "frisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor seinem braunen Eichentisch,", "tokens": ["Vor", "sei\u00b7nem", "brau\u00b7nen", "Ei\u00b7chen\u00b7tisch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem theuren Erbst\u00fcck von der Mutter", "tokens": ["Dem", "theu\u00b7ren", "Erb\u00b7st\u00fcck", "von", "der", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sa\u00df bei der Arbeit Doktor Luther.", "tokens": ["Sa\u00df", "bei", "der", "Ar\u00b7beit", "Dok\u00b7tor", "Lu\u00b7ther", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Am deutschen Bibelbuch, dem lieben,", "tokens": ["Am", "deut\u00b7schen", "Bi\u00b7bel\u00b7buch", ",", "dem", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hatt' er ein gutes Theil geschrieben:", "tokens": ["Hatt'", "er", "ein", "gu\u00b7tes", "Theil", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er legte hin die Feder sein,", "tokens": ["Er", "leg\u00b7te", "hin", "die", "Fe\u00b7der", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er schaute nach dem Gitterlein,", "tokens": ["Er", "schau\u00b7te", "nach", "dem", "Git\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "An Berg und Thal, den Gotteswerken,", "tokens": ["An", "Berg", "und", "Thal", ",", "den", "Got\u00b7tes\u00b7wer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sich Auge, Herz und Hand zu st\u00e4rken.", "tokens": ["Sich", "Au\u00b7ge", ",", "Herz", "und", "Hand", "zu", "st\u00e4r\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "$,", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Was tr\u00fcbt ihm seinen frommen Mut?", "tokens": ["Was", "tr\u00fcbt", "ihm", "sei\u00b7nen", "from\u00b7men", "Mut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Was treibt ihm nach der Stirn das Blut?", "tokens": ["Was", "treibt", "ihm", "nach", "der", "Stirn", "das", "Blut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja kl\u00e4rlich auf dem Berge dr\u00fcben", "tokens": ["Ja", "kl\u00e4r\u00b7lich", "auf", "dem", "Ber\u00b7ge", "dr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADJD", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sieht er sein Spiel den Argen \u00fcben.", "tokens": ["Sieht", "er", "sein", "Spiel", "den", "Ar\u00b7gen", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da steht von Felsen aufgebaut,", "tokens": ["Da", "steht", "von", "Fel\u00b7sen", "auf\u00b7ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er hat's bis heut noch nicht geschaut,", "tokens": ["Er", "hat's", "bis", "heut", "noch", "nicht", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ganz hell ein M\u00f6nch und eine Nonne,", "tokens": ["Ganz", "hell", "ein", "M\u00f6nch", "und", "ei\u00b7ne", "Non\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die k\u00fcssen sich beim Schein der Sonne.", "tokens": ["Die", "k\u00fcs\u00b7sen", "sich", "beim", "Schein", "der", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "O schamlos greuliches Gebild!", "tokens": ["O", "scham\u00b7los", "greu\u00b7li\u00b7ches", "Ge\u00b7bild", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist's nicht genug, da\u00df frech und wild", "tokens": ["Ist's", "nicht", "ge\u00b7nug", ",", "da\u00df", "frech", "und", "wild"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKNEG", "ADV", "$,", "KOUS", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "In den verschlossnen Klostermauern", "tokens": ["In", "den", "ver\u00b7schloss\u00b7nen", "Klos\u00b7ter\u00b7mau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Des Satans b\u00f6se L\u00fcste dauern,", "tokens": ["Des", "Sa\u00b7tans", "b\u00f6\u00b7se", "L\u00fcs\u00b7te", "dau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Darf er sie offen aller Welt", "tokens": ["Darf", "er", "sie", "of\u00b7fen", "al\u00b7ler", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "ADJD", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Noch malen unter's Himmelszelt?", "tokens": ["Noch", "ma\u00b7len", "un\u00b7ter's", "Him\u00b7mels\u00b7zelt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Der Doctor schauet nach den Feldern,", "tokens": ["Der", "Doc\u00b7tor", "schau\u00b7et", "nach", "den", "Fel\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ob kein Entsetzen in den W\u00e4ldern,", "tokens": ["Ob", "kein", "Ent\u00b7set\u00b7zen", "in", "den", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Ob nicht die Luft in Zornesflammen", "tokens": ["Ob", "nicht", "die", "Luft", "in", "Zor\u00b7nes\u00b7flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Ein schwarz Gewitter zieh' zusammen?", "tokens": ["Ein", "schwarz", "Ge\u00b7wit\u00b7ter", "zieh'", "zu\u00b7sam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Doch in dem hellsten Sonnenstral", "tokens": ["Doch", "in", "dem", "hells\u00b7ten", "Son\u00b7nen\u00b7stral"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die B\u00e4ume rauschen allzumal,", "tokens": ["Die", "B\u00e4u\u00b7me", "rau\u00b7schen", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und in den wunderlichen Stein", "tokens": ["Und", "in", "den", "wun\u00b7der\u00b7li\u00b7chen", "Stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Schlingt Moos und Blume sich hinein.", "tokens": ["Schlingt", "Moos", "und", "Blu\u00b7me", "sich", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Ist das von Gott, kommt das vom Uebel? \u2013", "tokens": ["Ist", "das", "von", "Gott", ",", "kommt", "das", "vom", "Ue\u00b7bel", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PDS", "APPR", "NN", "$,", "VVFIN", "ART", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Wie er noch sinnt, f\u00e4llt auf die Bibel", "tokens": ["Wie", "er", "noch", "sinnt", ",", "f\u00e4llt", "auf", "die", "Bi\u00b7bel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Ein lichter Abendsonnenstreif,", "tokens": ["Ein", "lich\u00b7ter", "A\u00b7bend\u00b7son\u00b7nen\u00b7streif", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Just auf 'nen Spruch, als goldner Reif.", "tokens": ["Just", "auf", "'nen", "Spruch", ",", "als", "gold\u00b7ner", "Reif", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "\u00bbwie konnt' ich \u2013 spricht er \u2013 lange sinnen!", "tokens": ["\u00bb", "wie", "konnt'", "ich", "\u2013", "spricht", "er", "\u2013", "lan\u00b7ge", "sin\u00b7nen", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "$(", "VVFIN", "PPER", "$(", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Antwort mu\u00df doch wohl sein da drinnen.", "tokens": ["Ant\u00b7wort", "mu\u00df", "doch", "wohl", "sein", "da", "drin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "ADV", "PPOSAT", "ADV", "ADV", "$."], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.27": {"text": "O gieb mir, du wahrhaftigs Buch,", "tokens": ["O", "gieb", "mir", ",", "du", "wahr\u00b7haf\u00b7tigs", "Buch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVIMP", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Aufschlu\u00df zu Segen oder Fluch!\u00ab", "tokens": ["Auf\u00b7schlu\u00df", "zu", "Se\u00b7gen", "o\u00b7der", "Fluch", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "So lies't er fort, wo er geblieben,", "tokens": ["So", "lies't", "er", "fort", ",", "wo", "er", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Da steht's im Sonnengold geschrieben:", "tokens": ["Da", "steht's", "im", "Son\u00b7nen\u00b7gold", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "\u00bbein Bischof soll unstr\u00e4flich rein,", "tokens": ["\u00bb", "ein", "Bi\u00b7schof", "soll", "uns\u00b7tr\u00e4f\u00b7lich", "rein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Soll Mann von Einem Weibe sein!\u00ab", "tokens": ["Soll", "Mann", "von", "Ei\u00b7nem", "Wei\u00b7be", "sein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "NN", "APPR", "ART", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Da geht ihm auf ein helles Licht:", "tokens": ["Da", "geht", "ihm", "auf", "ein", "hel\u00b7les", "Licht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Ach nein, das kommt vom B\u00f6sen nicht!", "tokens": ["Ach", "nein", ",", "das", "kommt", "vom", "B\u00f6\u00b7sen", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$,", "PDS", "VVFIN", "APPRART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Spricht Gottes Wort auch von den D\u00e4chern,", "tokens": ["Spricht", "Got\u00b7tes", "Wort", "auch", "von", "den", "D\u00e4\u00b7chern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Nicht blos in einsamen Gem\u00e4chern,", "tokens": ["Nicht", "blos", "in", "ein\u00b7sa\u00b7men", "Ge\u00b7m\u00e4\u00b7chern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.37": {"text": "So darf's in Felsen und Gestein", "tokens": ["So", "dar\u00b7f's", "in", "Fel\u00b7sen", "und", "Ge\u00b7stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PAV", "APPR", "NN", "KON", "NN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.38": {"text": "Wohl auch klar ausgesprochen sein.", "tokens": ["Wohl", "auch", "klar", "aus\u00b7ge\u00b7spro\u00b7chen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "So hat er drauf gek\u00e4mpft, gestritten,", "tokens": ["So", "hat", "er", "drauf", "ge\u00b7k\u00e4mpft", ",", "ge\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PAV", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Und bald gef\u00fchrt in seine H\u00fctten", "tokens": ["Und", "bald", "ge\u00b7f\u00fchrt", "in", "sei\u00b7ne", "H\u00fct\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Trotz Papst und Teufel, keck und laut", "tokens": ["Trotz", "Papst", "und", "Teu\u00b7fel", ",", "keck", "und", "laut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Aus einem Kloster sich die Braut.", "tokens": ["Aus", "ei\u00b7nem", "Klos\u00b7ter", "sich", "die", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Seit \u00f6ffnen sich die ernsten Pforten", "tokens": ["Seit", "\u00f6ff\u00b7nen", "sich", "die", "erns\u00b7ten", "Pfor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der dunkeln Kl\u00f6ster aller Orten;", "tokens": ["Der", "dun\u00b7keln", "Kl\u00f6s\u00b7ter", "al\u00b7ler", "Or\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Viel Schleier sind zur\u00fcckgewallt,", "tokens": ["Viel", "Schlei\u00b7er", "sind", "zu\u00b7r\u00fcck\u00b7ge\u00b7wallt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Manch eine liebliche Gestalt", "tokens": ["Manch", "ei\u00b7ne", "lieb\u00b7li\u00b7che", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Steht betend wohl noch am Altare,", "tokens": ["Steht", "be\u00b7tend", "wohl", "noch", "am", "Al\u00b7ta\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch mit dem Brautschmuck in dem Haare;", "tokens": ["Doch", "mit", "dem", "Brautsc\u00b7hmuck", "in", "dem", "Haa\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Ja Nonn' und M\u00f6nch mit Steineshaupte,", "tokens": ["Ja", "Nonn'", "und", "M\u00f6nch", "mit", "Stei\u00b7nes\u00b7haup\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Weil Doctor Luther es erlaubte,", "tokens": ["Weil", "Doc\u00b7tor", "Lu\u00b7ther", "es", "er\u00b7laub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NE", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sie k\u00fcssen sich auf diesen Tag,", "tokens": ["Sie", "k\u00fcs\u00b7sen", "sich", "auf", "die\u00b7sen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Geh' schauen, wer es schauen mag;", "tokens": ["Geh'", "schau\u00b7en", ",", "wer", "es", "schau\u00b7en", "mag", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ich hab's gesehn im Abendschein,", "tokens": ["Ich", "hab's", "ge\u00b7sehn", "im", "A\u00b7bend\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die Berge blickten freundlich drein,", "tokens": ["Die", "Ber\u00b7ge", "blick\u00b7ten", "freund\u00b7lich", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die Sonne hatt' ihr Wohlgefallen \u2013", "tokens": ["Die", "Son\u00b7ne", "hatt'", "ihr", "Wohl\u00b7ge\u00b7fal\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Gott schenk' so s\u00fc\u00dfen Ku\u00df uns Allen!", "tokens": ["Gott", "schenk'", "so", "s\u00fc\u00b7\u00dfen", "Ku\u00df", "uns", "Al\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADJA", "NN", "PPER", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}