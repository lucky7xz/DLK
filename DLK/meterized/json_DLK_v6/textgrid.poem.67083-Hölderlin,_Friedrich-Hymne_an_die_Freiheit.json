{"textgrid.poem.67083": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Hymne an die Freiheit", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie den Aar im grauen Felsenhange", "tokens": ["Wie", "den", "Aar", "im", "grau\u00b7en", "Fel\u00b7sen\u00b7han\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wildes Sehnen zu der Sterne Bahn,", "tokens": ["Wil\u00b7des", "Seh\u00b7nen", "zu", "der", "Ster\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Flammt zu majest\u00e4tischem Gesange", "tokens": ["Flammt", "zu", "ma\u00b7jes\u00b7t\u00e4\u00b7ti\u00b7schem", "Ge\u00b7san\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Meiner Freuden Ungest\u00fcm mich an;", "tokens": ["Mei\u00b7ner", "Freu\u00b7den", "Un\u00b7ge\u00b7st\u00fcm", "mich", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ha! das neue niegeno\u00dfne Leben", "tokens": ["Ha", "!", "das", "neu\u00b7e", "nie\u00b7ge\u00b7no\u00df\u00b7ne", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Schaffet neuen gl\u00fchenden Entschlu\u00df!", "tokens": ["Schaf\u00b7fet", "neu\u00b7en", "gl\u00fc\u00b7hen\u00b7den", "Ent\u00b7schlu\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "\u00dcber Wahn und Stolz emporzuschweben,", "tokens": ["\u00dc\u00b7ber", "Wahn", "und", "Stolz", "em\u00b7por\u00b7zu\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "S\u00fc\u00dfer, unaussprechlicher Genu\u00df!", "tokens": ["S\u00fc\u00b7\u00dfer", ",", "un\u00b7aus\u00b7sprech\u00b7li\u00b7cher", "Ge\u00b7nu\u00df", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sint dem Staube mich ihr Arm entrissen,", "tokens": ["Sint", "dem", "Stau\u00b7be", "mich", "ihr", "Arm", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schl\u00e4gt das Herz so k\u00fchn und selig ihr;", "tokens": ["Schl\u00e4gt", "das", "Herz", "so", "k\u00fchn", "und", "se\u00b7lig", "ihr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Angeflammt von ihren G\u00f6tterk\u00fcssen", "tokens": ["An\u00b7ge\u00b7flammt", "von", "ih\u00b7ren", "G\u00f6t\u00b7ter\u00b7k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gl\u00fchet noch die hei\u00dfe Wange mir;", "tokens": ["Gl\u00fc\u00b7het", "noch", "die", "hei\u00b7\u00dfe", "Wan\u00b7ge", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Jeder Laut von ihrem Zaubermunde", "tokens": ["Je\u00b7der", "Laut", "von", "ih\u00b7rem", "Zau\u00b7ber\u00b7mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "APPR", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Adelt noch den neugeschaffnen Sinn \u2013", "tokens": ["A\u00b7delt", "noch", "den", "neu\u00b7ge\u00b7schaff\u00b7nen", "Sinn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "H\u00f6rt, o Geister! meiner G\u00f6ttin Kunde,", "tokens": ["H\u00f6rt", ",", "o", "Geis\u00b7ter", "!", "mei\u00b7ner", "G\u00f6t\u00b7tin", "Kun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "NN", "$.", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "H\u00f6rt, und huldiget der Herrscherin!", "tokens": ["H\u00f6rt", ",", "und", "hul\u00b7di\u00b7get", "der", "Herr\u00b7sche\u00b7rin", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbals die Liebe noch im Sch\u00e4ferkleide", "tokens": ["\u00bb", "als", "die", "Lie\u00b7be", "noch", "im", "Sch\u00e4\u00b7fer\u00b7klei\u00b7de"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mit der Unschuld unter Blumen ging,", "tokens": ["Mit", "der", "Un\u00b7schuld", "un\u00b7ter", "Blu\u00b7men", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und der Erdensohn in Ruh und Freude", "tokens": ["Und", "der", "Er\u00b7den\u00b7sohn", "in", "Ruh", "und", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der Natur am Mutterbusen hing,", "tokens": ["Der", "Na\u00b7tur", "am", "Mut\u00b7ter\u00b7bu\u00b7sen", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Nicht der \u00dcbermut auf Richterst\u00fchlen", "tokens": ["Nicht", "der", "\u00dc\u00b7ber\u00b7mut", "auf", "Rich\u00b7ter\u00b7st\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Blind und f\u00fcrchterlich das Band zerri\u00df,", "tokens": ["Blind", "und", "f\u00fcrch\u00b7ter\u00b7lich", "das", "Band", "zer\u00b7ri\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Tauscht ich gerne mit der G\u00f6tter Spielen", "tokens": ["Tauscht", "ich", "ger\u00b7ne", "mit", "der", "G\u00f6t\u00b7ter", "Spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Meiner Kinder stilles Paradies.", "tokens": ["Mei\u00b7ner", "Kin\u00b7der", "stil\u00b7les", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Liebe rief die jugendlichen Triebe", "tokens": ["Lie\u00b7be", "rief", "die", "ju\u00b7gend\u00b7li\u00b7chen", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sch\u00f6pferisch zu hoher stiller Tat,", "tokens": ["Sch\u00f6p\u00b7fe\u00b7risch", "zu", "ho\u00b7her", "stil\u00b7ler", "Tat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Jeden Keim entfaltete der Liebe", "tokens": ["Je\u00b7den", "Keim", "ent\u00b7fal\u00b7te\u00b7te", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "W\u00e4rm und Licht zu schwelgerischer Saat;", "tokens": ["W\u00e4rm", "und", "Licht", "zu", "schwel\u00b7ge\u00b7ri\u00b7scher", "Saat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Deine Fl\u00fcgel, hohe Liebe! trugen", "tokens": ["Dei\u00b7ne", "Fl\u00fc\u00b7gel", ",", "ho\u00b7he", "Lie\u00b7be", "!", "tru\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$.", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "L\u00e4chelnd nieder die Olympier;", "tokens": ["L\u00e4\u00b7chelnd", "nie\u00b7der", "die", "O\u00b7lym\u00b7pier", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Jubelt\u00f6ne klangen \u2013 Herzen schlugen", "tokens": ["Ju\u00b7bel\u00b7t\u00f6\u00b7ne", "klan\u00b7gen", "\u2013", "Her\u00b7zen", "schlu\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$(", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "An der G\u00f6tter Busen g\u00f6ttlicher.", "tokens": ["An", "der", "G\u00f6t\u00b7ter", "Bu\u00b7sen", "g\u00f6tt\u00b7li\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADJA", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.5": {"line.1": {"text": "Freundlich bot der Freuden s\u00fc\u00dfe F\u00fclle", "tokens": ["Freund\u00b7lich", "bot", "der", "Freu\u00b7den", "s\u00fc\u00b7\u00dfe", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Meinen Lieblingen die Unschuld dar;", "tokens": ["Mei\u00b7nen", "Lieb\u00b7lin\u00b7gen", "die", "Un\u00b7schuld", "dar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-++--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Unverkennbar in der sch\u00f6nen H\u00fclle", "tokens": ["Un\u00b7ver\u00b7kenn\u00b7bar", "in", "der", "sch\u00f6\u00b7nen", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wu\u00dfte Tugend nicht, wie sch\u00f6n sie war;", "tokens": ["Wu\u00df\u00b7te", "Tu\u00b7gend", "nicht", ",", "wie", "sch\u00f6n", "sie", "war", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Friedlich hausten in der Blumenh\u00fcgel", "tokens": ["Fried\u00b7lich", "haus\u00b7ten", "in", "der", "Blu\u00b7men\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "K\u00fchlem Schatten die Gen\u00fcgsamen \u2013", "tokens": ["K\u00fch\u00b7lem", "Schat\u00b7ten", "die", "Ge\u00b7n\u00fcg\u00b7sa\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Ach! des Haders und der Sorge Fl\u00fcgel", "tokens": ["Ach", "!", "des", "Ha\u00b7ders", "und", "der", "Sor\u00b7ge", "Fl\u00fc\u00b7gel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "NN", "KON", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Rauschte ferne von den Gl\u00fccklichen.", "tokens": ["Rauschte", "fer\u00b7ne", "von", "den", "Gl\u00fcck\u00b7li\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.6": {"line.1": {"text": "Wehe nun! \u2013 mein Paradies erbebte!", "tokens": ["We\u00b7he", "nun", "!", "\u2013", "mein", "Pa\u00b7ra\u00b7dies", "er\u00b7beb\u00b7te", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "$(", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Fluch verhie\u00df der Elemente Wut!", "tokens": ["Fluch", "ver\u00b7hie\u00df", "der", "E\u00b7le\u00b7men\u00b7te", "Wut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und der N\u00e4chte schwarzem Scho\u00df entschwebte", "tokens": ["Und", "der", "N\u00e4ch\u00b7te", "schwar\u00b7zem", "Scho\u00df", "ent\u00b7schweb\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mit des Geiers Blick der \u00dcbermut;", "tokens": ["Mit", "des", "Gei\u00b7ers", "Blick", "der", "\u00dc\u00b7ber\u00b7mut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wehe! weinend floh ich mit der Liebe,", "tokens": ["We\u00b7he", "!", "wei\u00b7nend", "floh", "ich", "mit", "der", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Mit der Unschuld in die Himmel hin \u2013", "tokens": ["Mit", "der", "Un\u00b7schuld", "in", "die", "Him\u00b7mel", "hin", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Welke, Blume! rief ich ernst und tr\u00fcbe,", "tokens": ["Wel\u00b7ke", ",", "Blu\u00b7me", "!", "rief", "ich", "ernst", "und", "tr\u00fc\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "VVFIN", "PPER", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Welke, nimmer, nimmer aufzubl\u00fchn!", "tokens": ["Wel\u00b7ke", ",", "nim\u00b7mer", ",", "nim\u00b7mer", "auf\u00b7zu\u00b7bl\u00fchn", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Keck erhub sich des Gesetzes Rute,", "tokens": ["Keck", "er\u00b7hub", "sich", "des", "Ge\u00b7set\u00b7zes", "Ru\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nachzubilden, was die Liebe schuf;", "tokens": ["Nach\u00b7zu\u00b7bil\u00b7den", ",", "was", "die", "Lie\u00b7be", "schuf", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach! gegei\u00dfelt von dem \u00dcbermute", "tokens": ["Ach", "!", "ge\u00b7gei\u00b7\u00dfelt", "von", "dem", "\u00dc\u00b7ber\u00b7mu\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVPP", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "F\u00fchlte keiner g\u00f6ttlichen Beruf;", "tokens": ["F\u00fchl\u00b7te", "kei\u00b7ner", "g\u00f6tt\u00b7li\u00b7chen", "Be\u00b7ruf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Vor dem Geist in schwarzen Ungewittern,", "tokens": ["Vor", "dem", "Geist", "in", "schw