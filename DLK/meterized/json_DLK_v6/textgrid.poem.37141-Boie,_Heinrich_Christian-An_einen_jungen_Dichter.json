{"textgrid.poem.37141": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "An einen jungen Dichter", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verstecke dich und statt zu fliegen krieche!", "tokens": ["Ver\u00b7ste\u00b7cke", "dich", "und", "statt", "zu", "flie\u00b7gen", "krie\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So sprach mit Recht ein weiser alter Grieche,", "tokens": ["So", "sprach", "mit", "Recht", "ein", "wei\u00b7ser", "al\u00b7ter", "Grie\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und traun! der Mann sah tief in unser Herz.", "tokens": ["Und", "traun", "!", "der", "Mann", "sah", "tief", "in", "un\u00b7ser", "Herz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$.", "ART", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Bruders Gl\u00fcck ist seinem Bruder Schmerz;", "tokens": ["Des", "Bru\u00b7ders", "Gl\u00fcck", "ist", "sei\u00b7nem", "Bru\u00b7der", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Stets ungerecht, voll Neid ist unsre Seele,", "tokens": ["Stets", "un\u00b7ge\u00b7recht", ",", "voll", "Neid", "ist", "uns\u00b7re", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie leidet, wenn geehrt ein andrer ist.", "tokens": ["Sie", "lei\u00b7det", ",", "wenn", "ge\u00b7ehrt", "ein", "an\u00b7drer", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VVPP", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Verdiene Ruhm! Doch da\u00df dir Gl\u00fcck nicht fehle,", "tokens": ["Ver\u00b7die\u00b7ne", "Ruhm", "!", "Doch", "da\u00df", "dir", "Gl\u00fcck", "nicht", "feh\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "KON", "KOUS", "PPER", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So werde nicht genannt, eh du gestorben bist.", "tokens": ["So", "wer\u00b7de", "nicht", "ge\u00b7nannt", ",", "eh", "du", "ge\u00b7stor\u00b7ben", "bist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "VVPP", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}