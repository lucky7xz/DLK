{"dta.poem.22660": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "211. Da\u00df Vieh lebt nach den Sin\u0303en.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Wer nach den Sin\u0303en lebt/ den sch\u00e4tz ich f\u00fcr ein Vieh:", "tokens": ["Wer", "nach", "den", "Si\u00f1en", "lebt", "/", "den", "sch\u00e4tz", "ich", "f\u00fcr", "ein", "Vieh", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$(", "ART", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wer aber G\u00f6ttlich wird/ dem beug ich meine Knie.", "tokens": ["Wer", "a\u00b7ber", "G\u00f6tt\u00b7lich", "wird", "/", "dem", "beug", "ich", "mei\u00b7ne", "Knie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VAFIN", "$(", "ART", "NN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}