{"textgrid.poem.64780": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "2. Klage der Gutth\u00e4tigkeit bey dem Grabe der Frau Friederike Sophie, gebornen Freyinn von Bibra, verm\u00e4hlten von Holzendorf", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So eilst du schon, o Freundinn, von der Erde", "tokens": ["So", "eilst", "du", "schon", ",", "o", "Freun\u00b7dinn", ",", "von", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "FM", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hin an den Ort, wo steter Glanz dich schm\u00fcckt!", "tokens": ["Hin", "an", "den", "Ort", ",", "wo", "ste\u00b7ter", "Glanz", "dich", "schm\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PWAV", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hin an den Ort, wo ich vergessen werde,", "tokens": ["Hin", "an", "den", "Ort", ",", "wo", "ich", "ver\u00b7ges\u00b7sen", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil keine Noth befreyte Geister dr\u00fcckt!", "tokens": ["Weil", "kei\u00b7ne", "Noth", "be\u00b7frey\u00b7te", "Geis\u00b7ter", "dr\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So schw\u00e4cht bereits von Denen die mich ehren,", "tokens": ["So", "schw\u00e4cht", "be\u00b7reits", "von", "De\u00b7nen", "die", "mich", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PDS", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dein fr\u00fcher Tod die schon so schwache Zahl!", "tokens": ["Dein", "fr\u00fc\u00b7her", "Tod", "die", "schon", "so", "schwa\u00b7che", "Zahl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O d\u00fcrft' ein Wunsch dein gr\u00f6\u00dfer Gl\u00fccke st\u00f6ren!", "tokens": ["O", "d\u00fcrft'", "ein", "Wunsch", "dein", "gr\u00f6\u00b7\u00dfer", "Gl\u00fc\u00b7cke", "st\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der niedern Welt w\u00fcnscht' ich dich noch einmal.", "tokens": ["Der", "nie\u00b7dern", "Welt", "w\u00fcnscht'", "ich", "dich", "noch", "ein\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zu wenig sind, die mit ger\u00fchrtem Blicke", "tokens": ["Zu", "we\u00b7nig", "sind", ",", "die", "mit", "ge\u00b7r\u00fchr\u00b7tem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VAFIN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Pflichten sehn, die Elend fodern kann;", "tokens": ["Die", "Pflich\u00b7ten", "sehn", ",", "die", "E\u00b7lend", "fo\u00b7dern", "kann", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu wenig spornt f\u00fcr ihres N\u00e4chsten Gl\u00fccke", "tokens": ["Zu", "we\u00b7nig", "spornt", "f\u00fcr", "ih\u00b7res", "N\u00e4chs\u00b7ten", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein edler Trieb voll reiner Wollust an.", "tokens": ["Ein", "ed\u00b7ler", "Trieb", "voll", "rei\u00b7ner", "Wol\u00b7lust", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Lernt, Sterbliche, der Gottheit Zwecke kennen,", "tokens": ["Lernt", ",", "Sterb\u00b7li\u00b7che", ",", "der", "Got\u00b7theit", "Zwe\u00b7cke", "ken\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn ihre Huld mit G\u00fctern euch erf\u00fcllt:", "tokens": ["Wenn", "ih\u00b7re", "Huld", "mit", "G\u00fc\u00b7tern", "euch", "er\u00b7f\u00fcllt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie will euch nur die Lust, die Ehre g\u00f6nnen,", "tokens": ["Sie", "will", "euch", "nur", "die", "Lust", ",", "die", "Eh\u00b7re", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df Andrer Wohl aus eurem Gl\u00fccke quillt.", "tokens": ["Da\u00df", "A\u00b7ndrer", "Wohl", "aus", "eu\u00b7rem", "Gl\u00fc\u00b7cke", "quillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Verla\u00dfner Noth stets h\u00fclfreich zuzueilen,", "tokens": ["Ver\u00b7la\u00df\u00b7ner", "Noth", "stets", "h\u00fclf\u00b7reich", "zu\u00b7zu\u00b7ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Mit milder Hand die G\u00fcter auszutheilen,", "tokens": ["Mit", "mil\u00b7der", "Hand", "die", "G\u00fc\u00b7ter", "aus\u00b7zu\u00b7thei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur dies hast du f\u00fcr ihren Brauch gesch\u00e4tzt;", "tokens": ["Nur", "dies", "hast", "du", "f\u00fcr", "ih\u00b7ren", "Brauch", "ge\u00b7sch\u00e4tzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So klagt kein Kind um die, die es geboren,", "tokens": ["So", "klagt", "kein", "Kind", "um", "die", ",", "die", "es", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "APPR", "ART", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie deinen Tod dein Unterthan beklagt,", "tokens": ["Wie", "dei\u00b7nen", "Tod", "dein", "Un\u00b7ter\u00b7than", "be\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der wehmuthvoll, was er mit dir verloren,", "tokens": ["Der", "weh\u00b7muth\u00b7voll", ",", "was", "er", "mit", "dir", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "PWS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Enkeln noch nach vielen Jahren sagt.", "tokens": ["Den", "En\u00b7keln", "noch", "nach", "vie\u00b7len", "Jah\u00b7ren", "sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Kein Dichter mahlt mit kunsterf\u00fcllten Z\u00e4hren,", "tokens": ["Kein", "Dich\u00b7ter", "mahlt", "mit", "kuns\u00b7ter\u00b7f\u00fcll\u00b7ten", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie tief dein Tod in so viel Seelen dringt;", "tokens": ["Wie", "tief", "dein", "Tod", "in", "so", "viel", "See\u00b7len", "dringt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "APPR", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Dichterwitz kann dir mehr Ruhm gew\u00e4hren,", "tokens": ["Kein", "Dich\u00b7ter\u00b7witz", "kann", "dir", "mehr", "Ruhm", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als dir der Schmerz von so viel Seelen bringt;", "tokens": ["Als", "dir", "der", "Schmerz", "von", "so", "viel", "See\u00b7len", "bringt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch, ", "tokens": ["Doch", ","], "token_info": ["word", "punct"], "pos": ["KON", "$,"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Den hohen Blick, der stets f\u00fcr Sachsen wacht,", "tokens": ["Den", "ho\u00b7hen", "Blick", ",", "der", "stets", "f\u00fcr", "Sach\u00b7sen", "wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ach, hat vielleicht die dir entrissnen Tage", "tokens": ["Ach", ",", "hat", "viel\u00b7leicht", "die", "dir", "ent\u00b7riss\u00b7nen", "Ta\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VAFIN", "ADV", "ART", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Vorsicht Schlu\u00df den Seinen zugedacht!", "tokens": ["Der", "Vor\u00b7sicht", "Schlu\u00df", "den", "Sei\u00b7nen", "zu\u00b7ge\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "How amiable a thing is doing good! 'Tis all I envy great folks for.", "tokens": ["How", "a\u00b7miab\u00b7le", "a", "thing", "is", "do\u00b7ing", "good", "!", "'tis", "all", "I", "en\u00b7vy", "great", "folks", "for", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "$.", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+--+--+--+-+", "measure": "trochaic.octa.plus.relaxed"}}}}}