{"textgrid.poem.26068": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Als sitzen die Fr\u00fchlingsg\u00f6tter auf jedem Dach", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wilder V\u00f6gel Gewimmel", "tokens": ["Wie", "wil\u00b7der", "V\u00f6\u00b7gel", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Verschieben sich Berge und Himmel,", "tokens": ["Ver\u00b7schie\u00b7ben", "sich", "Ber\u00b7ge", "und", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die Wolken die Berge vertrieben.", "tokens": ["Die", "Wol\u00b7ken", "die", "Ber\u00b7ge", "ver\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wolken haben die Berge begraben,", "tokens": ["Wol\u00b7ken", "ha\u00b7ben", "die", "Ber\u00b7ge", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Fr\u00fchlingswolken, die donnernd traben.", "tokens": ["Fr\u00fch\u00b7lings\u00b7wol\u00b7ken", ",", "die", "don\u00b7nernd", "tra\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Donnerf\u00fc\u00dfe die \u00c4cker wachtreten,", "tokens": ["Don\u00b7ner\u00b7f\u00fc\u00b7\u00dfe", "die", "\u00c4\u00b7cker", "wach\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Wolkenh\u00e4nde die Erdklumpen kneten,", "tokens": ["Wol\u00b7ken\u00b7h\u00e4n\u00b7de", "die", "Erd\u00b7klum\u00b7pen", "kne\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Als sitzen die Fr\u00fchlingsg\u00f6tter auf jedem Dach", "tokens": ["Als", "sit\u00b7zen", "die", "Fr\u00fch\u00b7lings\u00b7g\u00f6t\u00b7ter", "auf", "je\u00b7dem", "Dach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+--+-+-++-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Und bilden sich Menschen ihrem Herzen nach.", "tokens": ["Und", "bil\u00b7den", "sich", "Men\u00b7schen", "ih\u00b7rem", "Her\u00b7zen", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}