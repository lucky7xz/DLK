{"textgrid.poem.61013": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Fr\u00fchling ist ein starker Held,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ist", "ein", "star\u00b7ker", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Ritter sondergleichen,", "tokens": ["Ein", "Rit\u00b7ter", "son\u00b7derg\u00b7lei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die rote Ros' im gr\u00fcnen Feld", "tokens": ["Die", "ro\u00b7te", "Ros'", "im", "gr\u00fc\u00b7nen", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist sein Wappen und Zeichen.", "tokens": ["Das", "ist", "sein", "Wap\u00b7pen", "und", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sein Schwert von Sonnenglanze schwang", "tokens": ["Sein", "Schwert", "von", "Son\u00b7nen\u00b7glan\u00b7ze", "schwang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er k\u00fchn und unerm\u00fcdet,", "tokens": ["Er", "k\u00fchn", "und", "un\u00b7er\u00b7m\u00fc\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis hell der silberne Panzer sprang,", "tokens": ["Bis", "hell", "der", "sil\u00b7ber\u00b7ne", "Pan\u00b7zer", "sprang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den sich der Winter geschmiedet.", "tokens": ["Den", "sich", "der", "Win\u00b7ter", "ge\u00b7schmie\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Und nun mit triumphierendem Schall", "tokens": ["Und", "nun", "mit", "tri\u00b7um\u00b7phie\u00b7ren\u00b7dem", "Schall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Durchzieht er Land und Wogen;", "tokens": ["Durch\u00b7zieht", "er", "Land", "und", "Wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als Herold kommt die Nachtigall", "tokens": ["Als", "He\u00b7rold", "kommt", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor ihm daher geflogen.", "tokens": ["Vor", "ihm", "da\u00b7her", "ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PAV", "VVPP", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Und rings erschallt an jedes Herz", "tokens": ["Und", "rings", "er\u00b7schallt", "an", "je\u00b7des", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Aufruf allerorten,", "tokens": ["Sein", "Auf\u00b7ruf", "al\u00b7le\u00b7ror\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und h\u00fcllt' es sich in dreifach Erz,", "tokens": ["Und", "h\u00fcllt'", "es", "sich", "in", "drei\u00b7fach", "Erz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es mu\u00df ihm \u00f6ffnen die Pforten;", "tokens": ["Es", "mu\u00df", "ihm", "\u00f6ff\u00b7nen", "die", "Pfor\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Es mu\u00df ihm \u00f6ffnen die Pforten dicht", "tokens": ["Es", "mu\u00df", "ihm", "\u00f6ff\u00b7nen", "die", "Pfor\u00b7ten", "dicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und darf sich nimmer entschuld'gen", "tokens": ["Und", "darf", "sich", "nim\u00b7mer", "ent\u00b7schuld'\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PRF", "ADV", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und mu\u00df der K\u00f6nigin, die er verficht,", "tokens": ["Und", "mu\u00df", "der", "K\u00f6\u00b7ni\u00b7gin", ",", "die", "er", "ver\u00b7ficht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der K\u00f6nigin Minne huld'gen.", "tokens": ["Der", "K\u00f6\u00b7ni\u00b7gin", "Min\u00b7ne", "huld'\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}}}}}