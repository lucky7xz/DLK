{"textgrid.poem.57953": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Ist alles dunkel ...", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Gas, kein Petroljum", "tokens": ["Kein", "Gas", ",", "kein", "Petr\u00b7ol\u00b7jum"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Kann brennen so schlecht,", "tokens": ["Kann", "bren\u00b7nen", "so", "schlecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Als wie 'ne Bogenlampe,", "tokens": ["Als", "wie", "'ne", "Bo\u00b7gen\u00b7lam\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die man nicht anstecht.", "tokens": ["Die", "man", "nicht", "an\u00b7stecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Eine Funzel mit \u00d6lje", "tokens": ["Ei\u00b7ne", "Fun\u00b7zel", "mit", "\u00d6l\u00b7je"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Brennt noch mal so sch\u00f6n,", "tokens": ["Brennt", "noch", "mal", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Als wie 'ne Bogenlampe,", "tokens": ["Als", "wie", "'ne", "Bo\u00b7gen\u00b7lam\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An der keiner tut drehn.", "tokens": ["An", "der", "kei\u00b7ner", "tut", "drehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "VVINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Ein Talglicht f\u00fcr'n Groschen,", "tokens": ["Ein", "Talg\u00b7licht", "f\u00fcr'n", "Gro\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Das leuchtet viel mehr", "tokens": ["Das", "leuch\u00b7tet", "viel", "mehr"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Als wie 'ne Bogenlampe", "tokens": ["Als", "wie", "'ne", "Bo\u00b7gen\u00b7lam\u00b7pe"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gie\u00dft man drauf kein Amp\u00e8re.", "tokens": ["Gie\u00dft", "man", "drauf", "kein", "Amp\u00e8\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PAV", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "F\u00fcr'n Pfennig 'n Kienspan", "tokens": ["F\u00fcr'n", "Pfen\u00b7nig", "'n", "Kien\u00b7span"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gibt zehnmal mehr Schein", "tokens": ["Gibt", "zehn\u00b7mal", "mehr", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PIAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Als die Bahnhofsplatzlampen,", "tokens": ["Als", "die", "Bahn\u00b7hofs\u00b7platz\u00b7lam\u00b7pen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Denn sie lassen es sein.", "tokens": ["Denn", "sie", "las\u00b7sen", "es", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VAINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Ein Streichholz, ein d\u00fcnnes,", "tokens": ["Ein", "Streich\u00b7holz", ",", "ein", "d\u00fcn\u00b7nes", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ist heller im Licht,", "tokens": ["Ist", "hel\u00b7ler", "im", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Denn die Bahnhofskandelaber,", "tokens": ["Denn", "die", "Bahn\u00b7hofs\u00b7kan\u00b7de\u00b7la\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die tun's nun mal nicht.", "tokens": ["Die", "tun's", "nun", "mal", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "ADV", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Drum ist es das Beste:", "tokens": ["Drum", "ist", "es", "das", "Bes\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Nehmt die Dinger da weg!", "tokens": ["Nehmt", "die", "Din\u00b7ger", "da", "weg", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Denn was keinen Zweck hat,", "tokens": ["Denn", "was", "kei\u00b7nen", "Zweck", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Hat gar keinen Zweck.", "tokens": ["Hat", "gar", "kei\u00b7nen", "Zweck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}