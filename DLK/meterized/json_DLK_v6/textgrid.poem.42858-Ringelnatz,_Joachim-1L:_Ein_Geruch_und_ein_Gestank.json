{"textgrid.poem.42858": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Geruch und ein Gestank", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Geruch und ein Gestank", "tokens": ["Ein", "Ge\u00b7ruch", "und", "ein", "Ge\u00b7stank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hatten einen Zank.", "tokens": ["Hat\u00b7ten", "ei\u00b7nen", "Zank", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbich lasse mich nicht,\u00ab rief der Gestank,", "tokens": ["\u00bb", "ich", "las\u00b7se", "mich", "nicht", ",", "\u00ab", "rief", "der", "Ge\u00b7stank", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "$(", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "\u00bbvon deiner S\u00fc\u00dflichkeit \u00fcberschminken!\u00ab", "tokens": ["\u00bb", "von", "dei\u00b7ner", "S\u00fc\u00df\u00b7lich\u00b7keit", "\u00fc\u00b7bersc\u00b7hmin\u00b7ken", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbmein Herr, sind Sie denn riechnervenkrank?", "tokens": ["\u00bb", "mein", "Herr", ",", "sind", "Sie", "denn", "riech\u00b7ner\u00b7ven\u00b7krank", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Merken Sie gar nicht, wie Sie stinken?\u00ab", "tokens": ["Mer\u00b7ken", "Sie", "gar", "nicht", ",", "wie", "Sie", "stin\u00b7ken", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "ADV", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$.", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "\u00bbwas k\u00fcmmert's dich, du bisamischer Schuft?", "tokens": ["\u00bb", "was", "k\u00fcm\u00b7mert's", "dich", ",", "du", "bi\u00b7sa\u00b7mi\u00b7scher", "Schuft", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bleib mir vom Leibe!\u00ab", "tokens": ["Bleib", "mir", "vom", "Lei\u00b7be", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbnein, solch ein Stunk geh\u00f6rt an die Luft!", "tokens": ["\u00bb", "nein", ",", "solch", "ein", "Stunk", "ge\u00b7h\u00f6rt", "an", "die", "Luft", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "PIAT", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sie werden sehen, wie ich Sie vertreibe.\u00ab", "tokens": ["Sie", "wer\u00b7den", "se\u00b7hen", ",", "wie", "ich", "Sie", "ver\u00b7trei\u00b7be", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "\u00bbdu L\u00fcftchen, ich werde dich gleich verschlucken!", "tokens": ["\u00bb", "du", "L\u00fcft\u00b7chen", ",", "ich", "wer\u00b7de", "dich", "gleich", "ver\u00b7schlu\u00b7cken", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Dich scheint der Moschus am Nabel zu jucken.\u00ab", "tokens": ["Dich", "scheint", "der", "Mo\u00b7schus", "am", "Na\u00b7bel", "zu", "ju\u00b7cken", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "APPRART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "\u00bbgenug, mein Herr, ich merke, Sie sind", "tokens": ["\u00bb", "ge\u00b7nug", ",", "mein", "Herr", ",", "ich", "mer\u00b7ke", ",", "Sie", "sind"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADV", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "$,", "PPER", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kein Gent. Ich spreche hier gegen den Wind.\u00ab \u2013", "tokens": ["Kein", "Gent", ".", "Ich", "spre\u00b7che", "hier", "ge\u00b7gen", "den", "Wind", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PIAT", "NN", "$.", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Es schwebten gerade zwei", "tokens": ["Es", "schweb\u00b7ten", "ge\u00b7ra\u00b7de", "zwei"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "CARD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u00c4ltere Damennasen vorbei.", "tokens": ["\u00c4l\u00b7te\u00b7re", "Da\u00b7men\u00b7na\u00b7sen", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sie wu\u00dften ihren Unmut zu z\u00fcgeln,", "tokens": ["Sie", "wu\u00df\u00b7ten", "ih\u00b7ren", "Un\u00b7mut", "zu", "z\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "R\u00fcmpften und zitterten mit den Fl\u00fcgeln.", "tokens": ["R\u00fcmpf\u00b7ten", "und", "zit\u00b7ter\u00b7ten", "mit", "den", "Fl\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}