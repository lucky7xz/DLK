{"textgrid.poem.52038": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr, die ihr Adel habt in euern Bauden feil,", "tokens": ["Ihr", ",", "die", "ihr", "A\u00b7del", "habt", "in", "eu\u00b7ern", "Bau\u00b7den", "feil", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu welcher alle Welt nach Tituln kommt gelauffen,", "tokens": ["Zu", "wel\u00b7cher", "al\u00b7le", "Welt", "nach", "Ti\u00b7tuln", "kommt", "ge\u00b7lauf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIAT", "NN", "APPR", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und einem ieden misst sein Maa\u00df und Grad und Theil,", "tokens": ["Und", "ei\u00b7nem", "ie\u00b7den", "misst", "sein", "Maa\u00df", "und", "Grad", "und", "Theil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "PPOSAT", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie theuer meint ihr wohl die Tugend zu verkauffen?", "tokens": ["Wie", "theu\u00b7er", "meint", "ihr", "wohl", "die", "Tu\u00b7gend", "zu", "ver\u00b7kauf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+