{"textgrid.poem.59989": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch erst abends bei der Bowle", "tokens": ["Doch", "erst", "a\u00b7bends", "bei", "der", "Bow\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wirst du deine Gr\u00f6\u00dfe zeigen;", "tokens": ["Wirst", "du", "dei\u00b7ne", "Gr\u00f6\u00b7\u00dfe", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Marseillaise, Carmagnole", "tokens": ["Mar\u00b7seil\u00b7laise", ",", "Car\u00b7ma\u00b7gno\u00b7le"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Werden frech zum Himmel steigen.", "tokens": ["Wer\u00b7den", "frech", "zum", "Him\u00b7mel", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schwer bezopft wirst du die letzte", "tokens": ["Schwer", "be\u00b7zopft", "wirst", "du", "die", "letz\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "VAFIN", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hose von den Lenden streifen,", "tokens": ["Ho\u00b7se", "von", "den", "Len\u00b7den", "strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Drei\u00dfig dir von Gott gesetzte", "tokens": ["Drei\u00b7\u00dfig", "dir", "von", "Gott", "ge\u00b7setz\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "PPER", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlingel heimlich auszupeitschen.", "tokens": ["Schlin\u00b7gel", "heim\u00b7lich", "aus\u00b7zu\u00b7peit\u00b7schen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Spielen mit den drei\u00dfig Kronen", "tokens": ["Spie\u00b7len", "mit", "den", "drei\u00b7\u00dfig", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wirst du wie mit Eierschalen,", "tokens": ["Wirst", "du", "wie", "mit", "Ei\u00b7er\u00b7scha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOKOM", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lehren drei\u00dfig Millionen,", "tokens": ["Leh\u00b7ren", "drei\u00b7\u00dfig", "Mil\u00b7lion\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Mit der Faust im Sack zu prahlen.", "tokens": ["Mit", "der", "Faust", "im", "Sack", "zu", "prah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nationalvereinsgermane,", "tokens": ["Na\u00b7ti\u00b7o\u00b7nal\u00b7ver\u00b7eins\u00b7ger\u00b7ma\u00b7ne", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "So verwegen, so gef\u00e4hrlich", "tokens": ["So", "ver\u00b7we\u00b7gen", ",", "so", "ge\u00b7f\u00e4hr\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann der Mensch in seinem Wahne", "tokens": ["Kann", "der", "Mensch", "in", "sei\u00b7nem", "Wah\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden um einen Taler j\u00e4hrlich!", "tokens": ["Wer\u00b7den", "um", "ei\u00b7nen", "Ta\u00b7ler", "j\u00e4hr\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}