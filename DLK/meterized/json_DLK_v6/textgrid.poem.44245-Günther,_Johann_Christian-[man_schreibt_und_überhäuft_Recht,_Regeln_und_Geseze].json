{"textgrid.poem.44245": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[man schreibt und \u00fcberh\u00e4uft Recht, Regeln und Geseze]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man schreibt und \u00fcberh\u00e4uft Recht, Regeln und Geseze,", "tokens": ["Man", "schreibt", "und", "\u00fc\u00b7berh\u00b7\u00e4uft", "Recht", ",", "Re\u00b7geln", "und", "Ge\u00b7se\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der allgemeinen Ruh das Wachsthum zu verleihn:", "tokens": ["Der", "all\u00b7ge\u00b7mei\u00b7nen", "Ruh", "das", "Wach\u00b7sthum", "zu", "ver\u00b7leihn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durchgeht man oft das Buch, so findet man Geschweze;", "tokens": ["Durch\u00b7geht", "man", "oft", "das", "Buch", ",", "so", "fin\u00b7det", "man", "Ge\u00b7schwe\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "$,", "ADV", "VVFIN", "PIS", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zwey Worte sind genug und schlie\u00dfen alles ein.", "tokens": ["Zwey", "Wor\u00b7te", "sind", "ge\u00b7nug", "und", "schlie\u00b7\u00dfen", "al\u00b7les", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADV", "KON", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer gute Tage sucht, der mag die Warheit h\u00f6ren:", "tokens": ["Wer", "gu\u00b7te", "Ta\u00b7ge", "sucht", ",", "der", "mag", "die", "War\u00b7heit", "h\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "$,", "PRELS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Man gebe (denn so will Natur und Christenthum)", "tokens": ["Man", "ge\u00b7be", "(", "denn", "so", "will", "Na\u00b7tur", "und", "Chris\u00b7ten\u00b7thum", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "KON", "ADV", "VMFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was jedem zugeh\u00f6rt, dem H\u00f6chsten Danck und Ruhm,", "tokens": ["Was", "je\u00b7dem", "zu\u00b7ge\u00b7h\u00f6rt", ",", "dem", "H\u00f6chs\u00b7ten", "Danck", "und", "Ruhm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVPP", "$,", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dem Nechsten Lieb und Rath, sich selber Straf und Lehren.", "tokens": ["Dem", "Nechs\u00b7ten", "Lieb", "und", "Rath", ",", "sich", "sel\u00b7ber", "Straf", "und", "Leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,", "PRF", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}