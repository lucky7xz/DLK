{"textgrid.poem.63485": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: De Knakenhauermeister Timm", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "De Knakenhauermeister Timm", "tokens": ["De", "Kna\u00b7ken\u00b7hau\u00b7er\u00b7meis\u00b7ter", "Timm"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "de was sau klok grad as 'ne Imm,", "tokens": ["de", "was", "sau", "klok", "grad", "as", "'ne", "Imm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "sau klok, Fl\u00e4uh k\u00fcnn he hosten h\u00fcr'n.", "tokens": ["sau", "klok", ",", "Fl\u00e4uh", "k\u00fcnn", "he", "hos\u00b7ten", "h\u00fcr'", "n."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADJD", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "He s\u00e4d, he hadd man sullt studiern,", "tokens": ["He", "s\u00e4d", ",", "he", "hadd", "man", "sullt", "stu\u00b7di\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "FM", "FM", "PIS", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "he hadd dat tum Senator br\u00f6cht", "tokens": ["he", "hadd", "dat", "tum", "Se\u00b7na\u00b7tor", "br\u00f6cht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "un tum Professor von dat Recht.", "tokens": ["un", "tum", "Pro\u00b7fes\u00b7sor", "von", "dat", "Recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "He kennt dat allens von de Akten,", "tokens": ["He", "kennt", "dat", "al\u00b7lens", "von", "de", "Ak\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "von de Komtehts un de Kuntrakten,", "tokens": ["von", "de", "Kom\u00b7tehts", "un", "de", "Kun\u00b7trak\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "FM", "FM", "FM", "FM", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Konkurs un Testaments un Pakten,", "tokens": ["Kon\u00b7kurs", "un", "Tes\u00b7ta\u00b7ments", "un", "Pak\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "FM", "FM", "FM", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "von de Replik un de Duplik,", "tokens": ["von", "de", "Re\u00b7plik", "un", "de", "Du\u00b7plik", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "FM", "FM", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "dat seg he d\u00f6rch, dat w\u00fc\u00dft he glik.", "tokens": ["dat", "seg", "he", "d\u00f6rch", ",", "dat", "w\u00fc\u00dft", "he", "glik", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "PAV", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ja ja, Herr Timm was gor tau klok,", "tokens": ["Ja", "ja", ",", "Herr", "Timm", "was", "gor", "tau", "klok", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PTKANT", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "un in den Utschott set he ok,", "tokens": ["un", "in", "den", "Ut\u00b7schott", "set", "he", "ok", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "APPR", "ART", "NN", "VVFIN", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "un all de Utschottb\u00f6rgers s\u00e4den:", "tokens": ["un", "all", "de", "Ut\u00b7schott\u00b7b\u00f6r\u00b7gers", "s\u00e4\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "PIAT", "NE", "NE", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.15": {"text": "\u00bbje Timm, den lat ji man tofr\u00e4den!", "tokens": ["\u00bb", "je", "Timm", ",", "den", "lat", "ji", "man", "tof\u00b7r\u00e4\u00b7den", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NE", "$,", "ART", "FM", "FM", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Fustdick hett de't achter sin Uhren,", "tokens": ["Fust\u00b7dick", "hett", "de't", "ach\u00b7ter", "sin", "Uh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.17": {"text": "de weet Bescheed, un em to buren,", "tokens": ["de", "weet", "Be\u00b7scheed", ",", "un", "em", "to", "bu\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "FM", "FM", "FM", "VVINF", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.18": {"text": "kann eener noch so fr\u00fch upstahn,", "tokens": ["kann", "e\u00b7e\u00b7ner", "noch", "so", "fr\u00fch", "ups\u00b7tahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.19": {"text": "Timm ward em doch een Knippschen slahn.\u00ab", "tokens": ["Timm", "ward", "em", "doch", "e\u00b7en", "Knipp\u00b7schen", "slahn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VAFIN", "PIS", "ADV", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}