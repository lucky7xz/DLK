{"textgrid.poem.58355": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "393. Was geschmolzen ist, leicht in Eines flie\u00dft", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durchs Leiden wird die Seel' bes\u00e4nftigt und gebeugt", "tokens": ["Durchs", "Lei\u00b7den", "wird", "die", "Seel'", "be\u00b7s\u00e4nf\u00b7tigt", "und", "ge\u00b7beugt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wie das Wachs im Feu'r geschmolzen und erweicht,", "tokens": ["Und", "wie", "das", "Wachs", "im", "Feu'r", "ge\u00b7schmol\u00b7zen", "und", "er\u00b7weicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bis endlich Gott in sie und sie in Gott kann flie\u00dfen,", "tokens": ["Bis", "end\u00b7lich", "Gott", "in", "sie", "und", "sie", "in", "Gott", "kann", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "APPR", "PPER", "KON", "PPER", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da seine Gnade wird des Kreuzes Pein vers\u00fc\u00dfen.", "tokens": ["Da", "sei\u00b7ne", "Gna\u00b7de", "wird", "des", "Kreu\u00b7zes", "Pein", "ver\u00b7s\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}