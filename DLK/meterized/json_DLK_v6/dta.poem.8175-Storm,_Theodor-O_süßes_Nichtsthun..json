{"dta.poem.8175": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "O s\u00fc\u00dfes Nichtsthun.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Zu ruhen auf des Bergs besonnter Kuppe;", "tokens": ["Zu", "ru\u00b7hen", "auf", "des", "Bergs", "be\u00b7sonn\u00b7ter", "Kup\u00b7pe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bald abw\u00e4rts zu des St\u00e4dtchens H\u00e4usergruppe", "tokens": ["Bald", "ab\u00b7w\u00e4rts", "zu", "des", "St\u00e4dt\u00b7chens", "H\u00e4u\u00b7ser\u00b7grup\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Blick zu senden, bald in ferne Weite!", "tokens": ["Den", "Blick", "zu", "sen\u00b7den", ",", "bald", "in", "fer\u00b7ne", "Wei\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O s\u00fc\u00dfes Nichtsthun, lieblich so gebannt", "tokens": ["O", "s\u00fc\u00b7\u00dfes", "Nichtst\u00b7hun", ",", "lieb\u00b7lich", "so", "ge\u00b7bannt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "ADJD", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zu athmen in den neubefreiten D\u00fcften;", "tokens": ["Zu", "ath\u00b7men", "in", "den", "neu\u00b7be\u00b7frei\u00b7ten", "D\u00fcf\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sich locken lassen von den Fr\u00fchlingsl\u00fcften,", "tokens": ["Sich", "lo\u00b7cken", "las\u00b7sen", "von", "den", "Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Hinab zu ziehn in das begl\u00e4nzte Land;", "tokens": ["Hin\u00b7ab", "zu", "ziehn", "in", "das", "be\u00b7gl\u00e4nz\u00b7te", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "R\u00fcckkehren dann aus aller Wunderferne", "tokens": ["R\u00fcck\u00b7keh\u00b7ren", "dann", "aus", "al\u00b7ler", "Wun\u00b7der\u00b7fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "In deiner Augen heimathliche Sterne.", "tokens": ["In", "dei\u00b7ner", "Au\u00b7gen", "hei\u00b7math\u00b7li\u00b7che", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}