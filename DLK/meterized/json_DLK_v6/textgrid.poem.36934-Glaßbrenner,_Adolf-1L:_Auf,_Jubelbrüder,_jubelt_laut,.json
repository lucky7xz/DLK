{"textgrid.poem.36934": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf, Jubelbr\u00fcder, jubelt laut,", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, Jubelbr\u00fcder, jubelt laut,", "tokens": ["Auf", ",", "Ju\u00b7bel\u00b7br\u00fc\u00b7der", ",", "ju\u00b7belt", "laut", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "NN", "$,", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Singt trunkensel'ge Lieder!", "tokens": ["Singt", "trun\u00b7ken\u00b7sel'\u00b7ge", "Lie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der ewig jugendliche Gott", "tokens": ["Der", "e\u00b7wig", "ju\u00b7gend\u00b7li\u00b7che", "Gott"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Weines naht uns wieder.", "tokens": ["Des", "Wei\u00b7nes", "naht", "uns", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sein Leben ist das sch\u00f6nste Blut!", "tokens": ["Sein", "Le\u00b7ben", "ist", "das", "sch\u00f6ns\u00b7te", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Sonne seine Liebe!", "tokens": ["Die", "Son\u00b7ne", "sei\u00b7ne", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sein Athmen ist die Poesie", "tokens": ["Sein", "Ath\u00b7men", "ist", "die", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der reinsten Menschentriebe.", "tokens": ["Der", "reins\u00b7ten", "Men\u00b7schen\u00b7trie\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Den Himmel dr\u00fcckt er an die Brust", "tokens": ["Den", "Him\u00b7mel", "dr\u00fcckt", "er", "an", "die", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seinen goldnen Sternen;", "tokens": ["Mit", "sei\u00b7nen", "gold\u00b7nen", "Ster\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf Blitzesschwingen tr\u00e4gt er uns", "tokens": ["Auf", "Blit\u00b7zes\u00b7schwin\u00b7gen", "tr\u00e4gt", "er", "uns"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis in die fernsten Fernen.", "tokens": ["Bis", "in", "die", "ferns\u00b7ten", "Fer\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Staub des Erdenlebens flieht,", "tokens": ["Der", "Staub", "des", "Er\u00b7den\u00b7le\u00b7bens", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn er die Glut entz\u00fcndet,", "tokens": ["Wenn", "er", "die", "Glut", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und in das Diamantenkleid", "tokens": ["Und", "in", "das", "Di\u00b7a\u00b7man\u00b7ten\u00b7kleid"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der G\u00f6ttlichkeit uns windet!", "tokens": ["Der", "G\u00f6tt\u00b7lich\u00b7keit", "uns", "win\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Sonne wirft er in die Nacht,", "tokens": ["Die", "Son\u00b7ne", "wirft", "er", "in", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So lang sein Blut wir trinken,", "tokens": ["So", "lang", "sein", "Blut", "wir", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis da\u00df wir voll Begeisterung", "tokens": ["Bis", "da\u00df", "wir", "voll", "Be\u00b7geis\u00b7te\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und selig niedersinken.", "tokens": ["Und", "se\u00b7lig", "nie\u00b7der\u00b7sin\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er kr\u00f6net mit Unsterblichkeit,", "tokens": ["Er", "kr\u00f6\u00b7net", "mit", "U\u00b7nsterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Baut Tempel auf aus Splittern!", "tokens": ["Baut", "Tem\u00b7pel", "auf", "aus", "Split\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sein ist das Reich der Phantasie,", "tokens": ["Sein", "ist", "das", "Reich", "der", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vor dem Tyrannen zittern.", "tokens": ["Vor", "dem", "Ty\u00b7ran\u00b7nen", "zit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die sch\u00f6nsten Weiber dieser Welt", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "Wei\u00b7ber", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schmelzt er in ein's zusammen!", "tokens": ["Schmelzt", "er", "in", "ein's", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIS", "PTKVZ", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und l\u00e4\u00dft uns da zu Himmelslust", "tokens": ["Und", "l\u00e4\u00dft", "uns", "da", "zu", "Him\u00b7mels\u00b7lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An ihrem Leib entflammen!", "tokens": ["An", "ih\u00b7rem", "Leib", "ent\u00b7flam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sein Muth, sein ewiger Humor", "tokens": ["Sein", "Muth", ",", "sein", "e\u00b7wi\u00b7ger", "Hu\u00b7mor"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Befreit uns von dem Z\u00fcgel,", "tokens": ["Be\u00b7freit", "uns", "von", "dem", "Z\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und Dem, der nicht mehr stehen kann,", "tokens": ["Und", "Dem", ",", "der", "nicht", "mehr", "ste\u00b7hen", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PRELS", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Leiht er der Wonne Fl\u00fcgel!", "tokens": ["Leiht", "er", "der", "Won\u00b7ne", "Fl\u00fc\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Drum, Jubelbr\u00fcder, jubelt laut,", "tokens": ["Drum", ",", "Ju\u00b7bel\u00b7br\u00fc\u00b7der", ",", "ju\u00b7belt", "laut", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$,", "VVFIN", "ADJ