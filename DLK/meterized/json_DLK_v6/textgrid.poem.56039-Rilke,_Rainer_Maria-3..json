{"textgrid.poem.56039": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Teichen und den eingerahmten Weihern", "tokens": ["Den", "Tei\u00b7chen", "und", "den", "ein\u00b7ge\u00b7rahm\u00b7ten", "Wei\u00b7hern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "verheimlicht man noch immer das Verh\u00f6r", "tokens": ["ver\u00b7heim\u00b7licht", "man", "noch", "im\u00b7mer", "das", "Ver\u00b7h\u00f6r"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der K\u00f6nige. Sie warten unter Schleiern,", "tokens": ["der", "K\u00f6\u00b7ni\u00b7ge", ".", "Sie", "war\u00b7ten", "un\u00b7ter", "Schlei\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und jeden Augenblick kann Monseigneur", "tokens": ["und", "je\u00b7den", "Au\u00b7gen\u00b7blick", "kann", "Mon\u00b7seig\u00b7neur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VMFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "vor\u00fcberkommen; und dann wollen sie", "tokens": ["vor\u00b7\u00fc\u00b7ber\u00b7kom\u00b7men", ";", "und", "dann", "wol\u00b7len", "sie"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$.", "KON", "ADV", "VMFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "des K\u00f6nigs Laune oder Trauer mildern", "tokens": ["des", "K\u00f6\u00b7nigs", "Lau\u00b7ne", "o\u00b7der", "Trau\u00b7er", "mil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und von den Marmorr\u00e4ndern wieder die", "tokens": ["und", "von", "den", "Mar\u00b7mor\u00b7r\u00e4n\u00b7dern", "wie\u00b7der", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Teppiche mit alten Spiegelbildern", "tokens": ["Tep\u00b7pi\u00b7che", "mit", "al\u00b7ten", "Spie\u00b7gel\u00b7bil\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "hinunterh\u00e4ngen, wie um einen Platz:", "tokens": ["hin\u00b7un\u00b7ter\u00b7h\u00e4n\u00b7gen", ",", "wie", "um", "ei\u00b7nen", "Platz", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "auf gr\u00fcnem Grund, mit Silber, Rosa, Grau,", "tokens": ["auf", "gr\u00fc\u00b7nem", "Grund", ",", "mit", "Sil\u00b7ber", ",", "Ro\u00b7sa", ",", "Grau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "$,", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "gew\u00e4hrtem Wei\u00df und leicht ger\u00fchrtem Blau", "tokens": ["ge\u00b7w\u00e4hr\u00b7tem", "Wei\u00df", "und", "leicht", "ge\u00b7r\u00fchr\u00b7tem", "Blau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und einem K\u00f6nige und einer Frau", "tokens": ["und", "ei\u00b7nem", "K\u00f6\u00b7ni\u00b7ge", "und", "ei\u00b7ner", "Frau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "und Blumen in dem wellenden Besatz.", "tokens": ["und", "Blu\u00b7men", "in", "dem", "wel\u00b7len\u00b7den", "Be\u00b7satz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}