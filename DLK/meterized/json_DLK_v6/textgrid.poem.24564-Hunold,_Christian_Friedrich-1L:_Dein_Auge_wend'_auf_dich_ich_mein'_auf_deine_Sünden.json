{"textgrid.poem.24564": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein Auge wend' auf dich/ ich mein' auf deine S\u00fcnden/", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Auge wend' auf dich/ ich mein' auf deine S\u00fcnden/", "tokens": ["Dein", "Au\u00b7ge", "wend'", "auf", "dich", "/", "ich", "mein'", "auf", "dei\u00b7ne", "S\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf deine Schwachheit selbst/ und was an dir zu finden.", "tokens": ["Auf", "dei\u00b7ne", "Schwach\u00b7heit", "selbst", "/", "und", "was", "an", "dir", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$(", "KON", "PWS", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erkl\u00e4r' in b\u00f6sen nicht di\u00df was dein Nechster thut/", "tokens": ["Er\u00b7kl\u00e4r'", "in", "b\u00f6\u00b7sen", "nicht", "di\u00df", "was", "dein", "Nechs\u00b7ter", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "PTKNEG", "PDS", "PRELS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schau nicht/ ob andre schlimm/ nein ob du selber gut.", "tokens": ["Schau", "nicht", "/", "ob", "and\u00b7re", "schlimm", "/", "nein", "ob", "du", "sel\u00b7ber", "gut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$(", "KOUS", "PIS", "ADJD", "$(", "PTKANT", "KOUS", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die M\u00fche bleibt umsonst/ in solchen Urtheil f\u00e4llen/", "tokens": ["Die", "M\u00fc\u00b7he", "bleibt", "um\u00b7sonst", "/", "in", "sol\u00b7chen", "Ur\u00b7theil", "f\u00e4l\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$(", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Man irrt und kan sich offt/ was falsch/ vor Augen stellen.", "tokens": ["Man", "irrt", "und", "kan", "sich", "offt", "/", "was", "falsch", "/", "vor", "Au\u00b7gen", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VMFIN", "PRF", "ADV", "$(", "PWS", "ADJD", "$(", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und de\u00dfen Zunge stets ein k\u00fchnes Urtheil spricht/", "tokens": ["Und", "de\u00b7\u00dfen", "Zun\u00b7ge", "stets", "ein", "k\u00fch\u00b7nes", "Ur\u00b7theil", "spricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den stellt sein b\u00f6ses Hertz vor Gottes Zorn-Gericht.", "tokens": ["Den", "stellt", "sein", "b\u00f6\u00b7ses", "Hertz", "vor", "Got\u00b7tes", "Zorn\u00b7Ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Erbaulich aber ist/ sich selber anzusehen/", "tokens": ["Er\u00b7bau\u00b7lich", "a\u00b7ber", "ist", "/", "sich", "sel\u00b7ber", "an\u00b7zu\u00b7se\u00b7hen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VAFIN", "$(", "PRF", "ADV", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In Demuht in sein Hertz/ sein eigen Hertz zugehen.", "tokens": ["In", "De\u00b7muht", "in", "sein", "Hertz", "/", "sein", "ei\u00b7gen", "Hertz", "zu\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "$(", "PPOSAT", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Zuschauen was uns fehlt/ welch Laster uns gemein.", "tokens": ["Zu\u00b7schau\u00b7en", "was", "uns", "fehlt", "/", "welch", "Las\u00b7ter", "uns", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "PPER", "VVFIN", "$(", "PWAT", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sein eigner Kenner erst/ denn Richter auch zu seyn.", "tokens": ["Sein", "eig\u00b7ner", "Ken\u00b7ner", "erst", "/", "denn", "Rich\u00b7ter", "auch", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$(", "KON", "NN", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Woher mag aber doch ein schlimmes Urtheil r\u00fchren?", "tokens": ["Wo\u00b7her", "mag", "a\u00b7ber", "doch", "ein", "schlim\u00b7mes", "Ur\u00b7theil", "r\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Aus Liebe/ denn ich m\u00f6cht ihn gern zum guten f\u00fchren/", "tokens": ["Aus", "Lie\u00b7be", "/", "denn", "ich", "m\u00f6cht", "ihn", "gern", "zum", "gu\u00b7ten", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "KON", "PPER", "VMFIN", "PPER", "ADV", "APPRART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So spricht des Tadlers Mund; doch f\u00fchlt sein Hertz hierbey/", "tokens": ["So", "spricht", "des", "Tad\u00b7lers", "Mund", ";", "doch", "f\u00fchlt", "sein", "Hertz", "hier\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wie kalt die Lieb' in ihm/ wie hei\u00df die Feindschafft sey.", "tokens": ["Wie", "kalt", "die", "Lieb'", "in", "ihm", "/", "wie", "hei\u00df", "die", "Feind\u00b7schafft", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "APPR", "PPER", "$(", "PWAV", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie/ da\u00df man andere unordentlich will nennen/", "tokens": ["Wie", "/", "da\u00df", "man", "an\u00b7de\u00b7re", "un\u00b7or\u00b7dent\u00b7lich", "will", "nen\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "KOUS", "PIS", "PIS", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+--+-+-+--", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Da Sinnen und Gem\u00fcht die Ordnung selbst nicht kennen?", "tokens": ["Da", "Sin\u00b7nen", "und", "Ge\u00b7m\u00fcht", "die", "Ord\u00b7nung", "selbst", "nicht", "ken\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Mit einem Krancken komt ein Neider \u00fcberein/", "tokens": ["Mit", "ei\u00b7nem", "Kran\u00b7cken", "komt", "ein", "Nei\u00b7der", "\u00fc\u00b7be\u00b7re\u00b7in", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.20": {"text": "Dort ist der Leib zuschwach/ hier der Verstand zu klein.", "tokens": ["Dort", "ist", "der", "Leib", "zu\u00b7schwach", "/", "hier", "der", "Ver\u00b7stand", "zu", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "$(", "ADV", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wenn unsre Meinung gut/ und nur auf Gott gerichtet/", "tokens": ["Wenn", "uns\u00b7re", "Mei\u00b7nung", "gut", "/", "und", "nur", "auf", "Gott", "ge\u00b7rich\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "$(", "KON", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wenn man so himmlisch ist/ da\u00df man sich selbst zernichtet/", "tokens": ["Wenn", "man", "so", "himm\u00b7lisch", "ist", "/", "da\u00df", "man", "sich", "selbst", "zer\u00b7nich\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADJD", "VAFIN", "$(", "KOUS", "PIS", "PRF", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die weil man Erd und Staub/ so wird man nicht so leicht/", "tokens": ["Die", "weil", "man", "Erd", "und", "Staub", "/", "so", "wird", "man", "nicht", "so", "leicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "PIS", "NN", "KON", "NN", "$(", "ADV", "VAFIN", "PIS", "PTKNEG", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Bey fremder Tadelsucht vom Brand des Zorns erreicht.", "tokens": ["Bey", "frem\u00b7der", "Ta\u00b7del\u00b7sucht", "vom", "Brand", "des", "Zorns", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Allein so sind wir nicht/ und andere desgleichen.", "tokens": ["Al\u00b7lein", "so", "sind", "wir", "nicht", "/", "und", "an\u00b7de\u00b7re", "des\u00b7glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PTKNEG", "$(", "KON", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die Neigung/ die sich mu\u00df in unsre Hertzen schleichen/", "tokens": ["Die", "Nei\u00b7gung", "/", "die", "sich", "mu\u00df", "in", "uns\u00b7re", "Hert\u00b7zen", "schlei\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PRF", "VMFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ein Gegenstand/ so uns von au\u00dfen an sich zieht/", "tokens": ["Ein", "Ge\u00b7gen\u00b7stand", "/", "so", "uns", "von", "au\u00b7\u00dfen", "an", "sich", "zieht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PPER", "APPR", "ADV", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Das ist der Grund/ aus dem man jedes Urtheil sieht.", "tokens": ["Das", "ist", "der", "Grund", "/", "aus", "dem", "man", "je\u00b7des", "Ur\u00b7theil", "sieht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$(", "APPR", "PRELS", "PIS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die meisten meinen zwar/ sie kennten ihr Gewi\u00dfen/", "tokens": ["Die", "meis\u00b7ten", "mei\u00b7nen", "zwar", "/", "sie", "kenn\u00b7ten", "ihr", "Ge\u00b7wi\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da\u00df sie kein eintzigmahl zu pr\u00fcfen sich befli\u00dfen.", "tokens": ["Da\u00df", "sie", "kein", "eint\u00b7zig\u00b7mahl", "zu", "pr\u00fc\u00b7fen", "sich", "be\u00b7fli\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "PIS", "PTKZU", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Sie dencken: sind wir nur in allem wohl begl\u00fcckt/", "tokens": ["Sie", "den\u00b7cken", ":", "sind", "wir", "nur", "in", "al\u00b7lem", "wohl", "be\u00b7gl\u00fcckt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "VAFIN", "PPER", "ADV", "APPR", "PIS", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So sey der Seelen Fried in ihre Brust ger\u00fcckt", "tokens": ["So", "sey", "der", "See\u00b7len", "Fried", "in", "ih\u00b7re", "Brust", "ge\u00b7r\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Allein/ wenn Sturm entsteht/ wenn die Verdrie\u00dflichkeiten/", "tokens": ["Al\u00b7lein", "/", "wenn", "Sturm", "ent\u00b7steht", "/", "wenn", "die", "Ver\u00b7drie\u00df\u00b7lich\u00b7kei\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "NN", "VVFIN", "$(", "KOUS", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die ein Ger\u00fccht erweckt/ zu ihren Ohren schreiten/", "tokens": ["Die", "ein", "Ge\u00b7r\u00fccht", "er\u00b7weckt", "/", "zu", "ih\u00b7ren", "Oh\u00b7ren", "schrei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$(", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So dringt ein jedes Wort/ das ihren Ruhm verletzt/", "tokens": ["So", "dringt", "ein", "je\u00b7des", "Wort", "/", "das", "ih\u00b7ren", "Ruhm", "ver\u00b7letzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PIAT", "NN", "$(", "PDS", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ins Hertz als w\u00e4r es da in Marmor einge\u00e4tzt.", "tokens": ["Ins", "Hertz", "als", "w\u00e4r", "es", "da", "in", "Mar\u00b7mor", "ein\u00b7ge\u00b7\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOKOM", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Inzwischen aber wird nur andern bey geme\u00dfen/", "tokens": ["I\u00b7nzwi\u00b7schen", "a\u00b7ber", "wird", "nur", "an\u00b7dern", "bey", "ge\u00b7me\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "PIS", "APPR", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Da\u00df sie unruhig sind/ da\u00df sie der Ha\u00df bese\u00dfen.", "tokens": ["Da\u00df", "sie", "un\u00b7ru\u00b7hig", "sind", "/", "da\u00df", "sie", "der", "Ha\u00df", "be\u00b7se\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$(", "KOUS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "Ihr Seelen-Friede spricht nicht eher wieder ein/", "tokens": ["Ihr", "See\u00b7len\u00b7Frie\u00b7de", "spricht", "nicht", "e\u00b7her", "wie\u00b7der", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "ADV", "ADV", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Bi\u00df/ welch Gewi\u00dfen doch/ sie vorgerochen seyn.", "tokens": ["Bi\u00df", "/", "welch", "Ge\u00b7wi\u00b7\u00dfen", "doch", "/", "sie", "vor\u00b7ge\u00b7ro\u00b7chen", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$(", "PWAT", "NN", "ADV", "$(", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ach Herr/ wer hat die Schuld/ wenn uns die Unruh plaget?", "tokens": ["Ach", "Herr", "/", "wer", "hat", "die", "Schuld", "/", "wenn", "uns", "die", "Un\u00b7ruh", "pla\u00b7get", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$(", "PWS", "VAFIN", "ART", "NN", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Nur der Begierden Macht/ die unsre Hertzen naget.", "tokens": ["Nur", "der", "Be\u00b7gier\u00b7den", "Macht", "/", "die", "uns\u00b7re", "Hert\u00b7zen", "na\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Gl\u00fcckseelig/ welcher nicht nach seinem Willen thut;", "tokens": ["Gl\u00fcck\u00b7see\u00b7lig", "/", "wel\u00b7cher", "nicht", "nach", "sei\u00b7nem", "Wil\u00b7len", "thut", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "PRELS", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und de\u00dfen wohl in Gott/ und keinem Menschen ruht.", "tokens": ["Und", "de\u00b7\u00dfen", "wohl", "in", "Gott", "/", "und", "kei\u00b7nem", "Men\u00b7schen", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "APPR", "NN", "$(", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Wer lange Zeit gewohnt nach seinem Sinn zu leben;", "tokens": ["Wer", "lan\u00b7ge", "Zeit", "ge\u00b7wohnt", "nach", "sei\u00b7nem", "Sinn", "zu", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "VVPP", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Der findet viele M\u00fch/ ihm recht zu wiederstreben;", "tokens": ["Der", "fin\u00b7det", "vie\u00b7le", "M\u00fch", "/", "ihm", "recht", "zu", "wie\u00b7der\u00b7stre\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$(", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Wer wieder Willen soll auf andern Wegen gehn/", "tokens": ["Wer", "wie\u00b7der", "Wil\u00b7len", "soll", "auf", "an\u00b7dern", "We\u00b7gen", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Der l\u00e4\u00dfet viel Verdru\u00df bey seiner Leitung sehn.", "tokens": ["Der", "l\u00e4\u00b7\u00dfet", "viel", "Ver\u00b7dru\u00df", "bey", "sei\u00b7ner", "Lei\u00b7tung", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Ein Himmlisch Feuer soll die kalten Hertzen nehren.", "tokens": ["Ein", "Himm\u00b7lisch", "Feu\u00b7er", "soll", "die", "kal\u00b7ten", "Hert\u00b7zen", "neh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Herr deine Flamme mu\u00df die Eigen-Lust verzehren.", "tokens": ["Herr", "dei\u00b7ne", "Flam\u00b7me", "mu\u00df", "die", "Ei\u00b7gen\u00b7Lust", "ver\u00b7zeh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.51": {"text": "Es hebt uns deine Hand/ sind wir dir unterthan/", "tokens": ["Es", "hebt", "uns", "dei\u00b7ne", "Hand", "/", "sind", "wir", "dir", "un\u00b7ter\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$(", "VAFIN", "PPER", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Weit \u00fcber die Vernunfft/ weit \u00fcber allen Wahn.", "tokens": ["Weit", "\u00fc\u00b7ber", "die", "Ver\u00b7nunfft", "/", "weit", "\u00fc\u00b7ber", "al\u00b7len", "Wahn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$(", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}