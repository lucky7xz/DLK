{"textgrid.poem.41929": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Haltet mir Ruh' in der wogenden Brust, ihr verzogenen Kinder!", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Haltet mir Ruh' in der wogenden Brust, ihr verzogenen Kinder!", "tokens": ["Hal\u00b7tet", "mir", "Ruh'", "in", "der", "wo\u00b7gen\u00b7den", "Brust", ",", "ihr", "ver\u00b7zo\u00b7ge\u00b7nen", "Kin\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "ART", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Sorgen der Liebe, beschlie\u00dft endlich das neckende Spiel!", "tokens": ["Sor\u00b7gen", "der", "Lie\u00b7be", ",", "be\u00b7schlie\u00dft", "end\u00b7lich", "das", "ne\u00b7cken\u00b7de", "Spiel", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Undank nenn' ich f\u00fcrwahr ein solch unziemliches Treiben,", "tokens": ["Un\u00b7dank", "nenn'", "ich", "f\u00fcr\u00b7wahr", "ein", "solch", "un\u00b7ziem\u00b7li\u00b7ches", "Trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ART", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Wenn von dem eigenen Heerd l\u00e4rmend der Gast uns verjagt.", "tokens": ["Wenn", "von", "dem", "ei\u00b7ge\u00b7nen", "Heerd", "l\u00e4r\u00b7mend", "der", "Gast", "uns", "ver\u00b7jagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "ADJD", "ART", "NN", "PPER", "VVPP", "$."], "meter": "---+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wehe mir, da\u00df ich euch fr\u00fcher verw\u00f6hnt! Einst fehlte der Muth mir,", "tokens": ["We\u00b7he", "mir", ",", "da\u00df", "ich", "euch", "fr\u00fc\u00b7her", "ver\u00b7w\u00f6hnt", "!", "Einst", "fehl\u00b7te", "der", "Muth", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVPP", "$.", "ADV", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Euch zu bestrafen, doch jetzt fehlen die Kr\u00e4fte mir schon.", "tokens": ["Euch", "zu", "be\u00b7stra\u00b7fen", ",", "doch", "jetzt", "feh\u00b7len", "die", "Kr\u00e4f\u00b7te", "mir", "schon", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "ADV", "ADV", "VVFIN", "ART", "NN", "PPER", "ADV", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Denn nun seyd ihr zu gro\u00df und dem Z\u00fcrnenden einzeln gewachsen,", "tokens": ["Denn", "nun", "seyd", "ihr", "zu", "gro\u00df", "und", "dem", "Z\u00fcr\u00b7nen\u00b7den", "ein\u00b7zeln", "ge\u00b7wach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PTKA", "ADJD", "KON", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "--+--+--+--++-+-", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Und, wie die Bienen im Schwarm, wehrt ihr euch alle vereint.", "tokens": ["Und", ",", "wie", "die", "Bie\u00b7nen", "im", "Schwarm", ",", "wehrt", "ihr", "euch", "al\u00b7le", "ver\u00b7eint", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "$,", "VVFIN", "PPER", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Ach, wie habt ihr mir l\u00e4ngst den geordneten Garten zertreten,", "tokens": ["Ach", ",", "wie", "habt", "ihr", "mir", "l\u00e4ngst", "den", "ge\u00b7ord\u00b7ne\u00b7ten", "Gar\u00b7ten", "zer\u00b7tre\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VAFIN", "PPER", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Wenn ihr in kindischer Jagd \u00fcber die Beete mir sprangt!", "tokens": ["Wenn", "ihr", "in", "kin\u00b7di\u00b7scher", "Jagd", "\u00fc\u00b7ber", "die", "Bee\u00b7te", "mir", "sprangt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Unkraut wuchert darin; das ist euch Freud' und Erbauung,", "tokens": ["Un\u00b7kraut", "wu\u00b7chert", "da\u00b7rin", ";", "das", "ist", "euch", "Freud'", "und", "Er\u00b7bau\u00b7ung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PAV", "$.", "PDS", "VAFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Weil ihr den Nutzen verschm\u00e4ht, weil euch das Zierliche hemmt.", "tokens": ["Weil", "ihr", "den", "Nut\u00b7zen", "ver\u00b7schm\u00e4ht", ",", "weil", "euch", "das", "Zier\u00b7li\u00b7che", "hemmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Denn nicht reizen die Lilien euch, die schlanken, erhabnen,", "tokens": ["Denn", "nicht", "rei\u00b7zen", "die", "Li\u00b7li\u00b7en", "euch", ",", "die", "schlan\u00b7ken", ",", "er\u00b7hab\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "ART", "NN", "PPER", "$,", "PRELS", "VVINF", "$,", "VVFIN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Oder die Rosen, die lang keimen, um lange zu bl\u00fchn.", "tokens": ["O\u00b7der", "die", "Ro\u00b7sen", ",", "die", "lang", "kei\u00b7men", ",", "um", "lan\u00b7ge", "zu", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADJD", "VVINF", "$,", "KOUI", "ADV", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.15": {"text": "Wie es die Laune gebeut, so soll es im Augenblick dastehn,", "tokens": ["Wie", "es", "die", "Lau\u00b7ne", "ge\u00b7beut", ",", "so", "soll", "es", "im", "Au\u00b7gen\u00b7blick", "das\u00b7tehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "---+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Wenn es auch morgen verwelkt, heute verge\u00dft ihr es schon.", "tokens": ["Wenn", "es", "auch", "mor\u00b7gen", "ver\u00b7welkt", ",", "heu\u00b7te", "ver\u00b7ge\u00dft", "ihr", "es", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP", "$,", "ADV", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.17": {"text": "Wo ich den Lorbeer zog, da\u00df er hochaufgr\u00fcnend mir schatte,", "tokens": ["Wo", "ich", "den", "Lor\u00b7beer", "zog", ",", "da\u00df", "er", "hoch\u00b7auf\u00b7gr\u00fc\u00b7nend", "mir", "schat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Pflanzt ihr ein Veilchen, das rings jegliche Wiese verleiht.", "tokens": ["Pflanzt", "ihr", "ein", "Veil\u00b7chen", ",", "das", "rings", "jeg\u00b7li\u00b7che", "Wie\u00b7se", "ver\u00b7leiht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Freut an dem Veilchen sich schon die Geliebteste, der ich es biete,", "tokens": ["Freut", "an", "dem", "Veil\u00b7chen", "sich", "schon", "die", "Ge\u00b7lieb\u00b7tes\u00b7te", ",", "der", "ich", "es", "bie\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PRF", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}, "line.20": {"text": "Ach, wie w\u00fcrde des Baums ewiges Gr\u00fcn sie erfreun!", "tokens": ["Ach", ",", "wie", "w\u00fcr\u00b7de", "des", "Baums", "e\u00b7wi\u00b7ges", "Gr\u00fcn", "sie", "er\u00b7freun", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VAFIN", "ART", "NN", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "Seyd doch ruhig einmal! Nicht leb' ich ja einzig f\u00fcr euch nur;", "tokens": ["Seyd", "doch", "ru\u00b7hig", "ein\u00b7mal", "!", "Nicht", "leb'", "ich", "ja", "ein\u00b7zig", "f\u00fcr", "euch", "nur", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "ADJD", "ADV", "$.", "PTKNEG", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.22": {"text": "Kinder, ihr wi\u00dft noch nicht, was man vom Manne begehrt!", "tokens": ["Kin\u00b7der", ",", "ihr", "wi\u00dft", "noch", "nicht", ",", "was", "man", "vom", "Man\u00b7ne", "be\u00b7gehrt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PRELS", "PIS", "APPRART", "NN", "VVPP", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.23": {"text": "Wild seyn will ich mit euch, wenn die m\u00fc\u00dfige D\u00e4mmerung anbricht,", "tokens": ["Wild", "seyn", "will", "ich", "mit", "euch", ",", "wenn", "die", "m\u00fc\u00b7\u00dfi\u00b7ge", "D\u00e4m\u00b7me\u00b7rung", "an\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "VMFIN", "PPER", "APPR", "PPER", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+--+---+", "measure": "trochaic.septa.relaxed"}, "line.24": {"text": "Aber den ruhigen Tag la\u00dft mir zum ernsteren Werk!", "tokens": ["A\u00b7ber", "den", "ru\u00b7hi\u00b7gen", "Tag", "la\u00dft", "mir", "zum", "erns\u00b7te\u00b7ren", "Werk", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}}}}