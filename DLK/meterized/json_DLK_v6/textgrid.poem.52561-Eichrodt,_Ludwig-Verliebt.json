{"textgrid.poem.52561": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Verliebt", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schweifen die Gedanken", "tokens": ["Es", "schwei\u00b7fen", "die", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinaus in die weite Welt,", "tokens": ["Hin\u00b7aus", "in", "die", "wei\u00b7te", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Und suchen was einer Seele", "tokens": ["Und", "su\u00b7chen", "was", "ei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PWS", "ART", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Vor allem Andern gef\u00e4llt.", "tokens": ["Vor", "al\u00b7lem", "An\u00b7dern", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJA", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Sie ziehen \u00fcber die Berge,", "tokens": ["Sie", "zie\u00b7hen", "\u00fc\u00b7ber", "die", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zu Stadt und Wald und Flur,", "tokens": ["Zu", "Stadt", "und", "Wald", "und", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durchjagen in allen Gestalten", "tokens": ["Durch\u00b7ja\u00b7gen", "in", "al\u00b7len", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Kunst und die Natur.", "tokens": ["Die", "Kunst", "und", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie eilen zu lachender Br\u00fcder", "tokens": ["Sie", "ei\u00b7len", "zu", "la\u00b7chen\u00b7der", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Viellauter Zecherlust,", "tokens": ["Viel\u00b7lau\u00b7ter", "Ze\u00b7cher\u00b7lust", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie fliegen zu rauschenden Festen", "tokens": ["Sie", "flie\u00b7gen", "zu", "rau\u00b7schen\u00b7den", "Fes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "An fr\u00f6hlicher Damen Brust.", "tokens": ["An", "fr\u00f6h\u00b7li\u00b7cher", "Da\u00b7men", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Zu Spiel und Tanz, zum Gastmahl,", "tokens": ["Zu", "Spiel", "und", "Tanz", ",", "zum", "Gast\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu jeglichem Ohrenschmaus,", "tokens": ["Zu", "jeg\u00b7li\u00b7chem", "Oh\u00b7ren\u00b7schmaus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Auf Kanzel und Katheder,", "tokens": ["Auf", "Kan\u00b7zel", "und", "Ka\u00b7the\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Und in der Redner Haus.", "tokens": ["Und", "in", "der", "Red\u00b7ner", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie dringen in alle Tiefen", "tokens": ["Sie", "drin\u00b7gen", "in", "al\u00b7le", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der menschlichen Wissenschaft,", "tokens": ["Der", "menschli\u00b7chen", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie tr\u00e4umen von der Zukunft,", "tokens": ["Sie", "tr\u00e4u\u00b7men", "von", "der", "Zu\u00b7kunft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von herrlicher M\u00e4nnerkraft.", "tokens": ["Von", "herr\u00b7li\u00b7cher", "M\u00e4n\u00b7ner\u00b7kraft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Von Waffen und von Schlachten,", "tokens": ["Von", "Waf\u00b7fen", "und", "von", "Schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Lorbeern, Heldenthum,", "tokens": ["Von", "Lor\u00b7beern", ",", "Hel\u00b7den\u00b7thum", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Unsterblichen gro\u00dfen Thaten", "tokens": ["U\u00b7nsterb\u00b7li\u00b7chen", "gro\u00b7\u00dfen", "Tha\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Und ihrem ewigen Ruhm.", "tokens": ["Und", "ih\u00b7rem", "e\u00b7wi\u00b7gen", "Ruhm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Es schweifen die Gedanken", "tokens": ["Es", "schwei\u00b7fen", "die", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In die weite Welt hinaus \u2013", "tokens": ["In", "die", "wei\u00b7te", "Welt", "hin\u00b7aus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APZR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wo sie einzig weilen,", "tokens": ["Und", "wo", "sie", "ein\u00b7zig", "wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Steht der Geliebten Haus.", "tokens": ["Steht", "der", "Ge\u00b7lieb\u00b7ten", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}