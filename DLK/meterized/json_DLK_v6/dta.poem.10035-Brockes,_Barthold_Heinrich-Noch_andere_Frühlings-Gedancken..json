{"dta.poem.10035": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Noch andere Fr\u00fchlings-Gedancken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der laue Strahl der wiederkehr'nden Sonne", "tokens": ["Der", "lau\u00b7e", "Strahl", "der", "wie\u00b7der\u00b7kehr'n\u00b7den", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcllt die verd\u00fcnnte Lufft mit neuer Lebens-Gluth,", "tokens": ["F\u00fcllt", "die", "ver\u00b7d\u00fcnn\u00b7te", "Lufft", "mit", "neu\u00b7er", "Le\u00b7bens\u00b7Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Fl\u00f6sst rege Fruchtbarkeit in aller Thiere Blut,", "tokens": ["Fl\u00f6sst", "re\u00b7ge", "Frucht\u00b7bar\u00b7keit", "in", "al\u00b7ler", "Thie\u00b7re", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Und, in der Menschen Geist, lang\u2019 ungesp\u00fchrte Wonne.", "tokens": ["Und", ",", "in", "der", "Men\u00b7schen", "Geist", ",", "lang'", "un\u00b7ge\u00b7sp\u00fchr\u00b7te", "Won\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Viel tausend Knospen \u00f6ffnen sich,", "tokens": ["Viel", "tau\u00b7send", "Knos\u00b7pen", "\u00f6ff\u00b7nen", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bey diesem lauen Fr\u00fchlings-Wetter.", "tokens": ["Bey", "die\u00b7sem", "lau\u00b7en", "Fr\u00fch\u00b7lings\u00b7Wet\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Man sieht viel tausend junge Bl\u00e4tter", "tokens": ["Man", "sieht", "viel", "tau\u00b7send", "jun\u00b7ge", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Aus ihrem Schoo\u00df fast sichtbarlich", "tokens": ["Aus", "ih\u00b7rem", "Schoo\u00df", "fast", "sicht\u00b7bar\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Entstehen und gebohren werden.", "tokens": ["Ent\u00b7ste\u00b7hen", "und", "ge\u00b7boh\u00b7ren", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht minder brechen aus der Erden", "tokens": ["Nicht", "min\u00b7der", "bre\u00b7chen", "aus", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVINF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch unl\u00e4ngst braun-, seit gestern, gr\u00fcner Brust,", "tokens": ["Noch", "un\u00b7l\u00e4ngst", "braun", ",", "seit", "ge\u00b7stern", ",", "gr\u00fc\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "TRUNC", "$,", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zu noch sich mehrender Gem\u00fcths- und Augen-Luft,", "tokens": ["Zu", "noch", "sich", "meh\u00b7ren\u00b7der", "Ge\u00b7m\u00fcths", "und", "Au\u00b7gen\u00b7Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PRF", "ADV", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Viel tausend bunte Fr\u00fchlings-Kinder,", "tokens": ["Viel", "tau\u00b7send", "bun\u00b7te", "Fr\u00fch\u00b7lings\u00b7Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In dem bebl\u00fchmten Klee. Nicht minder", "tokens": ["In", "dem", "be\u00b7bl\u00fchm\u00b7ten", "Klee", ".", "Nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "PTKNEG", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00e4ngt auf begr\u00fcnter B\u00e4ume Zweigen", "tokens": ["F\u00e4ngt", "auf", "be\u00b7gr\u00fcn\u00b7ter", "B\u00e4u\u00b7me", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein angenehmer Schatz sich an zu zeigen.", "tokens": ["Ein", "an\u00b7ge\u00b7neh\u00b7mer", "Schatz", "sich", "an", "zu", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "APZR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie Silber gl\u00e4ntzt die reine Bl\u00fchte;", "tokens": ["Wie", "Sil\u00b7ber", "gl\u00e4ntzt", "die", "rei\u00b7ne", "Bl\u00fch\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Jhr lieblicher Geruch labt C\u00f6rper und Gem\u00fcthe.", "tokens": ["Ihr", "lieb\u00b7li\u00b7cher", "Ge\u00b7ruch", "labt", "C\u00f6r\u00b7per", "und", "Ge\u00b7m\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und sie versprechen uns, daneben", "tokens": ["Und", "sie", "ver\u00b7spre\u00b7chen", "uns", ",", "da\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "PAV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Noch manch erfrischendes Gerichte,", "tokens": ["Noch", "manch", "er\u00b7fri\u00b7schen\u00b7des", "Ge\u00b7rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "In s\u00e4urlich-s\u00fcsser Frucht, zu geben.", "tokens": ["In", "s\u00e4ur\u00b7lich\u00b7s\u00fcs\u00b7ser", "Frucht", ",", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schau mit bedachtsamen und aufgewecktem Sinn,", "tokens": ["Schau", "mit", "be\u00b7dacht\u00b7sa\u00b7men", "und", "auf\u00b7ge\u00b7weck\u00b7tem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "VVINF", "KON", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "O Mensch, ietzt allenthalben hin!", "tokens": ["O", "Mensch", ",", "ietzt", "al\u00b7len\u00b7thal\u00b7ben", "hin", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schau tausend, tausend Lieblichkeiten!", "tokens": ["Schau", "tau\u00b7send", ",", "tau\u00b7send", "Lieb\u00b7lich\u00b7kei\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schau, in der Silber-klaren Fluth,", "tokens": ["Schau", ",", "in", "der", "Sil\u00b7ber\u00b7kla\u00b7ren", "Fluth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Auf einem gleichsam g\u00fcldnen Sande,", "tokens": ["Auf", "ei\u00b7nem", "gleich\u00b7sam", "g\u00fcld\u00b7nen", "San\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf welchem sie, ohn\u2019 alle Ruhe, ruht,", "tokens": ["Auf", "wel\u00b7chem", "sie", ",", "ohn'", "al\u00b7le", "Ru\u00b7he", ",", "ruht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "$,", "KOUI", "PIAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Von einem Bluhmen-Klee- und Binsen-reichem Rande", "tokens": ["Von", "ei\u00b7nem", "Bluh\u00b7men\u00b7Klee", "und", "Bin\u00b7sen\u00b7rei\u00b7chem", "Ran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bekr\u00e4ntzt und eingefasst, viel tausend blaue Fische", "tokens": ["Be\u00b7kr\u00b7\u00e4ntzt", "und", "ein\u00b7ge\u00b7fasst", ",", "viel", "tau\u00b7send", "blau\u00b7e", "Fi\u00b7sche"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "VVFIN", "$,", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Den fliessenden Crystall durchdringen,", "tokens": ["Den", "flies\u00b7sen\u00b7den", "Crys\u00b7tall", "durch\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und bald gemach, bald wie der Blitz", "tokens": ["Und", "bald", "ge\u00b7mach", ",", "bald", "wie", "der", "Blitz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "$,", "ADV", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sich durch den klar- und feuchten Sitz", "tokens": ["Sich", "durch", "den", "kla\u00b7r", "und", "feuch\u00b7ten", "Sitz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Bald auf-bald abwerts schwingen.", "tokens": ["Bald", "auf\u00b7bald", "ab\u00b7werts", "schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hier murmelt, rieselt, rauscht der glatte Bach,", "tokens": ["Hier", "mur\u00b7melt", ",", "rie\u00b7selt", ",", "rauscht", "der", "glat\u00b7te", "Bach", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bald dunckel, in der B\u00e4ume Schatten,", "tokens": ["Bald", "dun\u00b7ckel", ",", "in", "der", "B\u00e4u\u00b7me", "Schat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bald hell bestrahlt, durch unbewachsne Matten.", "tokens": ["Bald", "hell", "be\u00b7strahlt", ",", "durch", "un\u00b7be\u00b7wachs\u00b7ne", "Mat\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er wird des Waldes, dort", "tokens": ["Er", "wird", "des", "Wal\u00b7des", ",", "dort"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Begr\u00fcnter, und allhier des Himmels blauer Spiegel.", "tokens": ["Be\u00b7gr\u00fcn\u00b7ter", ",", "und", "all\u00b7hier", "des", "Him\u00b7mels", "blau\u00b7er", "Spie\u00b7gel", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Die Aecker gr\u00fcnen ietzt, es bl\u00fchen Thal und H\u00fcgel,", "tokens": ["Die", "A\u00b7e\u00b7cker", "gr\u00fc\u00b7nen", "ietzt", ",", "es", "bl\u00fc\u00b7hen", "Thal", "und", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PPER", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Den sch\u00f6nsten G\u00e4rten gleich. In den bethauten Gr\u00fcnden", "tokens": ["Den", "sch\u00f6ns\u00b7ten", "G\u00e4r\u00b7ten", "gleich", ".", "In", "den", "be\u00b7thau\u00b7ten", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Springt ietzt manch schneller Hirsch, zusamt den Hinden,", "tokens": ["Springt", "ietzt", "manch", "schnel\u00b7ler", "Hirsch", ",", "zu\u00b7samt", "den", "Hin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit leichtem Fu\u00df und aufgerecktem Ohr,", "tokens": ["Mit", "leich\u00b7tem", "Fu\u00df", "und", "auf\u00b7ge\u00b7reck\u00b7tem", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Aus dem belaubten Wald\u2019 hervor.", "tokens": ["Aus", "dem", "be\u00b7laub\u00b7ten", "Wald'", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da rennt in dem bebl\u00fchmten Grase", "tokens": ["Da", "rennt", "in", "dem", "be\u00b7bl\u00fchm\u00b7ten", "Gra\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein fl\u00fcchtiger geschwinder Hase.", "tokens": ["Ein", "fl\u00fcch\u00b7ti\u00b7ger", "ge\u00b7schwin\u00b7der", "Ha\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Seht, wie er pl\u00f6tzlich stutzt, sich setzet,", "tokens": ["Seht", ",", "wie", "er", "pl\u00f6tz\u00b7lich", "stutzt", ",", "sich", "set\u00b7zet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$,", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ein M\u00e4nnchen macht,", "tokens": ["Ein", "M\u00e4nn\u00b7chen", "macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Und, wenn er niemand sieht, ob aller Fr\u00fchlings-Pracht", "tokens": ["Und", ",", "wenn", "er", "nie\u00b7mand", "sieht", ",", "ob", "al\u00b7ler", "Fr\u00fch\u00b7lings\u00b7Pracht"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$,", "KOUS", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mit tausend Spr\u00fcngen sich ergetzet.", "tokens": ["Mit", "tau\u00b7send", "Spr\u00fcn\u00b7gen", "sich", "er\u00b7get\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Beschaue denn die sch\u00f6ne Welt:", "tokens": ["Be\u00b7schau\u00b7e", "denn", "die", "sch\u00f6\u00b7