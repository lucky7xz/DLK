{"textgrid.poem.25866": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[regen gittert alle Fenster]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.85", "no:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Regen gittert alle Fenster,", "tokens": ["Re\u00b7gen", "git\u00b7tert", "al\u00b7le", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wolkentr\u00fcbe engt den Himmel,", "tokens": ["Wol\u00b7ken\u00b7tr\u00fc\u00b7be", "engt", "den", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zw\u00e4ngt Totenw\u00fcrmer aus der Erde,", "tokens": ["Zw\u00e4ngt", "To\u00b7ten\u00b7w\u00fcr\u00b7mer", "aus", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und das Blut, das rote, zittert.", "tokens": ["Und", "das", "Blut", ",", "das", "ro\u00b7te", ",", "zit\u00b7tert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lag einmal,", "tokens": ["Lag", "ein\u00b7mal", ","], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Blauen Himmelsklang im Munde,", "tokens": ["Blau\u00b7en", "Him\u00b7mels\u00b7klang", "im", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Runde schwangen tausend Lerchen,", "tokens": ["In", "der", "Run\u00b7de", "schwan\u00b7gen", "tau\u00b7send", "Ler\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "CARD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Tausend Lerchen h\u00f6her sang mein Mut.", "tokens": ["Tau\u00b7send", "Ler\u00b7chen", "h\u00f6\u00b7her", "sang", "mein", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Klanglos sanken meine Lieder.", "tokens": ["Klang\u00b7los", "san\u00b7ken", "mei\u00b7ne", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sanglos faulen meine G\u00e4rten,", "tokens": ["Sang\u00b7los", "fau\u00b7len", "mei\u00b7ne", "G\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Meine Himmel regnen Blut.", "tokens": ["Mei\u00b7ne", "Him\u00b7mel", "reg\u00b7nen", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}