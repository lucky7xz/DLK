{"textgrid.poem.43747": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Wein, der ist gerathen heuer,", "tokens": ["Der", "Wein", ",", "der", "ist", "ge\u00b7ra\u00b7then", "heu\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun ist die Freude gar nicht theuer.", "tokens": ["Nun", "ist", "die", "Freu\u00b7de", "gar", "nicht", "theu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Leicht freut sich jetzo Jung und Alt,", "tokens": ["Leicht", "freut", "sich", "jet\u00b7zo", "Jung", "und", "Alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ADV", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und jubelt, da\u00df es wiederhallt.", "tokens": ["Und", "ju\u00b7belt", ",", "da\u00df", "es", "wie\u00b7der\u00b7hallt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O duftigs\u00fc\u00dfes Rebenblut,", "tokens": ["O", "duf\u00b7tig\u00b7s\u00fc\u00b7\u00dfes", "Re\u00b7ben\u00b7blut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie schmeckst du schon so wundergut!", "tokens": ["Wie", "schmeckst", "du", "schon", "so", "wun\u00b7der\u00b7gut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was wirst du uns dereinst doch sein,", "tokens": ["Was", "wirst", "du", "uns", "de\u00b7reinst", "doch", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PRF", "ADV", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn du geworden bist zu Wein!", "tokens": ["Wenn", "du", "ge\u00b7wor\u00b7den", "bist", "zu", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAPP", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wohl hat's der Herbst gar gut gemacht,", "tokens": ["Wohl", "hat's", "der", "Herbst", "gar", "gut", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum sei ihm tausend Dank gebracht.", "tokens": ["Drum", "sei", "ihm", "tau\u00b7send", "Dank", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "CARD", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}