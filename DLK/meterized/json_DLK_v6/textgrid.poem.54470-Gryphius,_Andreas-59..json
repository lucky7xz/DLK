{"textgrid.poem.54470": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "59.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh! aller Herren Herr/ O geh nicht ins Gerichte", "tokens": ["Geh", "!", "al\u00b7ler", "Her\u00b7ren", "Herr", "/", "O", "geh", "nicht", "ins", "Ge\u00b7rich\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PIAT", "NN", "NN", "$(", "NE", "VVFIN", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit deiner H\u00e4nde Werck' das stracks verzagt/", "tokens": ["Mit", "dei\u00b7ner", "H\u00e4n\u00b7de", "Werck", "das", "stracks", "ver\u00b7zagt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dafern dein Grimm zu rechnen vns au\u00dftagt.", "tokens": ["Da\u00b7fern", "dein", "Grimm", "zu", "rech\u00b7nen", "vns", "au\u00df\u00b7tagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NE", "PTKZU", "VVINF", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vnd vnser Schuld beschaut mit grimmigem Gesichte!", "tokens": ["Vnd", "vn\u00b7ser", "Schuld", "be\u00b7schaut", "mit", "grim\u00b7mi\u00b7gem", "Ge\u00b7sich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was \u00fcberlegst du Herr/ mit scharffer Augen-Lichte.", "tokens": ["Was", "\u00fc\u00b7ber\u00b7legst", "du", "Herr", "/", "mit", "scharf\u00b7fer", "Au\u00b7gen\u00b7Lich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "$(", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den grossen Rest/ der mein Gewissen nagt", "tokens": ["Den", "gros\u00b7sen", "Rest", "/", "der", "mein", "Ge\u00b7wis\u00b7sen", "nagt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "H\u00f6rst du den an/ der mich so hoch verklagt?", "tokens": ["H\u00f6rst", "du", "den", "an", "/", "der", "mich", "so", "hoch", "ver\u00b7klagt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "PTKVZ", "$(", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ich bin dir schuldig/ ach! die Hauptsu i vnd die Fr\u00fcchte.", "tokens": ["Ich", "bin", "dir", "schul\u00b7dig", "/", "ach", "!", "die", "Haupt\u00b7su", "i", "vnd", "die", "Fr\u00fcch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$(", "XY", "$.", "ART", "NN", "NE", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Dafern ich auch verkauffen wolt/", "tokens": ["Da\u00b7fern", "ich", "auch", "ver\u00b7kauf\u00b7fen", "wolt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Was ich besitze; wird kein Gold", "tokens": ["Was", "ich", "be\u00b7sit\u00b7ze", ";", "wird", "kein", "Gold"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$.", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Kein Geld/ kein Blutt den Au\u00dfstand Herr/ erreichen.", "tokens": ["Kein", "Geld", "/", "kein", "Blutt", "den", "Au\u00df\u00b7stand", "Herr", "/", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$(", "PIAT", "ADJD", "ART", "NN", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ein B\u00fcrg/ ein Zahl-Mann steht f\u00fcr mich/", "tokens": ["Ein", "B\u00fcrg", "/", "ein", "Zahl\u00b7Mann", "steht", "f\u00fcr", "mich", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "VVFIN", "APPR", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Der durch den Tod vers\u00f6hnet dich/", "tokens": ["Der", "durch", "den", "Tod", "ver\u00b7s\u00f6h\u00b7net", "dich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Das Blutt/ das von ihm fleust/ kan deine Schrifft au\u00dfstreichen.", "tokens": ["Das", "Blutt", "/", "das", "von", "ihm", "fleust", "/", "kan", "dei\u00b7ne", "Schrifft", "au\u00df\u00b7strei\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "$(", "ART", "APPR", "PPER", "VVFIN", "$(", "VMFIN", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}