{"textgrid.poem.35656": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "19. An die Dichter", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dichter, deren Gesang Nachwelten richten,", "tokens": ["Dich\u00b7ter", ",", "de\u00b7ren", "Ge\u00b7sang", "Nach\u00b7wel\u00b7ten", "rich\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELAT", "NN", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Weh euch einst, wo der Spruch des Afterrichters,", "tokens": ["Weh", "euch", "einst", ",", "wo", "der", "Spruch", "des", "Af\u00b7ter\u00b7rich\u00b7ters", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und sein murmelnder Nachhall", "tokens": ["Und", "sein", "mur\u00b7meln\u00b7der", "Nach\u00b7hall"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Unter dem P\u00f6bel, euch weilt!", "tokens": ["Un\u00b7ter", "dem", "P\u00f6\u00b7bel", ",", "euch", "weilt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Stolz durch Geniuskraft, blickt Hohn dem D\u00fcnkling,", "tokens": ["Stolz", "durch", "Ge\u00b7nius\u00b7kraft", ",", "blickt", "Hohn", "dem", "D\u00fcnk\u00b7ling", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "VVFIN", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und den Knechten des Knechts! Des Sumpfes Schreier", "tokens": ["Und", "den", "Knech\u00b7ten", "des", "Knechts", "!", "Des", "Sump\u00b7fes", "Schrei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "$.", "ART", "NN", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Schm\u00e4ht der Leu zu zerstampfen;", "tokens": ["Schm\u00e4ht", "der", "Leu", "zu", "zer\u00b7stamp\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wandelt durch W\u00e4lder, und herrscht!", "tokens": ["Wan\u00b7delt", "durch", "W\u00e4l\u00b7der", ",", "und", "herrscht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "KON", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}