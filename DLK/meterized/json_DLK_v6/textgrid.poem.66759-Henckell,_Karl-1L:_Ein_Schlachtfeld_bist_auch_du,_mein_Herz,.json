{"textgrid.poem.66759": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Schlachtfeld bist auch du, mein Herz,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Schlachtfeld bist auch du, mein Herz,", "tokens": ["Ein", "Schlacht\u00b7feld", "bist", "auch", "du", ",", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Leichen \u00fcbers\u00e4t.", "tokens": ["Von", "Lei\u00b7chen", "\u00fc\u00b7bers\u00b7\u00e4t", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch sieh! Wer wagt sich himmelw\u00e4rts,", "tokens": ["Doch", "sieh", "!", "Wer", "wagt", "sich", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "PWS", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Wo rings der Tod gem\u00e4ht?", "tokens": ["Wo", "rings", "der", "Tod", "ge\u00b7m\u00e4ht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Flieg zu, flieg zu,", "tokens": ["Flieg", "zu", ",", "flieg", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.6": {"text": "Lichtkehlchen du,", "tokens": ["Licht\u00b7kehl\u00b7chen", "du", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$,"], "meter": "++-+", "measure": "iambic.di"}, "line.7": {"text": "Nun lohnt es, stark zu singen.", "tokens": ["Nun", "lohnt", "es", ",", "stark", "zu", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Willst doch ein tapfres Lerchlein sein,", "tokens": ["Willst", "doch", "ein", "tapf\u00b7res", "Lerc\u00b7hlein", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus schwarzem Schlund und Rohr", "tokens": ["Aus", "schwar\u00b7zem", "Schlund", "und", "Rohr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wirfst du beim ersten Morgenschein", "tokens": ["Wirfst", "du", "beim", "ers\u00b7ten", "Mor\u00b7gen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hellwirbelnd dich empor,", "tokens": ["Hell\u00b7wir\u00b7belnd", "dich", "em\u00b7por", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hoch, hoch im Blaun", "tokens": ["Hoch", ",", "hoch", "im", "Blaun"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "APPRART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Voll Weltvertraun", "tokens": ["Voll", "Welt\u00b7ver\u00b7traun"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Dein Lied sonnauf zu schwingen.", "tokens": ["Dein", "Lied", "son\u00b7nauf", "zu", "schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}