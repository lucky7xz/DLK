{"textgrid.poem.42037": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "N\u00e4chtliche Insel", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der See flie\u00dft langsam zu dem fernen Land.", "tokens": ["Der", "See", "flie\u00dft", "lang\u00b7sam", "zu", "dem", "fer\u00b7nen", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vielleicht, er findet irgendwo das Land.", "tokens": ["Viel\u00b7leicht", ",", "er", "fin\u00b7det", "ir\u00b7gend\u00b7wo", "das", "Land", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die blasse K\u00fcste weint Verlassenheit.", "tokens": ["Die", "blas\u00b7se", "K\u00fcs\u00b7te", "weint", "Ver\u00b7las\u00b7sen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im R\u00f6hricht ist so viel Verlassenheit.", "tokens": ["Im", "R\u00f6h\u00b7richt", "ist", "so", "viel", "Ver\u00b7las\u00b7sen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und ward die Wiese aller Bl\u00fcten kahl.", "tokens": ["Und", "ward", "die", "Wie\u00b7se", "al\u00b7ler", "Bl\u00fc\u00b7ten", "kahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es steht die H\u00fctte ganz in Armut kahl.", "tokens": ["Es", "steht", "die", "H\u00fct\u00b7te", "ganz", "in", "Ar\u00b7mut", "kahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die sp\u00e4ten V\u00f6gel suchten lange Rast.", "tokens": ["Die", "sp\u00e4\u00b7ten", "V\u00f6\u00b7gel", "such\u00b7ten", "lan\u00b7ge", "Rast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es fand der letzte Falter dunkle Rast:", "tokens": ["Es", "fand", "der", "letz\u00b7te", "Fal\u00b7ter", "dunk\u00b7le", "Rast", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Am Ende wird ein gro\u00dfer Schatten sein,", "tokens": ["Am", "En\u00b7de", "wird", "ein", "gro\u00b7\u00dfer", "Schat\u00b7ten", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es wird der Morgen fast vergessen sein,", "tokens": ["Es", "wird", "der", "Mor\u00b7gen", "fast", "ver\u00b7ges\u00b7sen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch eine wei\u00dfe Birke h\u00e4ngt im Abend.", "tokens": ["Noch", "ei\u00b7ne", "wei\u00b7\u00dfe", "Bir\u00b7ke", "h\u00e4ngt", "im", "A\u00b7bend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch eine wei\u00dfe Nonne betet in den Abend.", "tokens": ["Noch", "ei\u00b7ne", "wei\u00b7\u00dfe", "Non\u00b7ne", "be\u00b7tet", "in", "den", "A\u00b7bend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}