{"textgrid.poem.33836": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "In Palermo's Dom", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Orgelklang und Gesang durchwogen", "tokens": ["Or\u00b7gel\u00b7klang", "und", "Ge\u00b7sang", "durch\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Den Dom und seine S\u00e4ulenpracht,", "tokens": ["Den", "Dom", "und", "sei\u00b7ne", "S\u00e4u\u00b7len\u00b7pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die hohe W\u00f6lbung und der Apsis Bogen;", "tokens": ["Die", "ho\u00b7he", "W\u00f6l\u00b7bung", "und", "der", "Ap\u00b7sis", "Bo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom Meer her rollt gewitterschwer die Nacht.", "tokens": ["Vom", "Meer", "her", "rollt", "ge\u00b7wit\u00b7ter\u00b7schwer", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APZR", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Durch bunte Fenster, d\u00e4mmerhell, nach innen", "tokens": ["Durch", "bun\u00b7te", "Fens\u00b7ter", ",", "d\u00e4m\u00b7mer\u00b7hell", ",", "nach", "in\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "$,", "APPR", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dringt noch ein letzter Sonnenschein", "tokens": ["Dringt", "noch", "ein", "letz\u00b7ter", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und strahlt auf Gold und Edelstein,", "tokens": ["Und", "strahlt", "auf", "Gold", "und", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Heil'genbild und sch\u00f6ne Beterinnen.", "tokens": ["Auf", "Heil'\u00b7gen\u00b7bild", "und", "sch\u00f6\u00b7ne", "Be\u00b7te\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr T\u00f6ne wogt so fromm und ernst dahin!", "tokens": ["Ihr", "T\u00f6\u00b7ne", "wogt", "so", "fromm", "und", "ernst", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich seh' mit euch aus l\u00e4ngst verklungnen Tagen", "tokens": ["Ich", "seh'", "mit", "euch", "aus", "l\u00e4ngst", "ver\u00b7klung\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum s\u00e4ulengetragenen Baldachin", "tokens": ["Zum", "s\u00e4u\u00b7len\u00b7ge\u00b7tra\u00b7ge\u00b7nen", "Bal\u00b7da\u00b7ch\u00b7in"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+--+---", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Heranziehn geistbeschwingte Scharen", "tokens": ["Her\u00b7an\u00b7ziehn", "geist\u00b7be\u00b7schwing\u00b7te", "Scha\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und an den Gr\u00fcften niederknien.", "tokens": ["Und", "an", "den", "Gr\u00fcf\u00b7ten", "nie\u00b7der\u00b7kni\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hie ruhen sie, die auf Siziliens Thron", "tokens": ["Hie", "ru\u00b7hen", "sie", ",", "die", "auf", "Si\u00b7zi\u00b7li\u00b7ens", "Thron"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "APPR", "NE", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Den K\u00f6nigsgoldreif um die Stirn getragen,", "tokens": ["Den", "K\u00f6\u00b7nigs\u00b7gol\u00b7dreif", "um", "die", "Stirn", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den stets im Kampf errungnen Siegeslohn,", "tokens": ["Den", "stets", "im", "Kampf", "er\u00b7rung\u00b7nen", "Sie\u00b7ges\u00b7lohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hier ruhen sie in Porphyrsarkophagen.", "tokens": ["Hier", "ru\u00b7hen", "sie", "in", "Por\u00b7phyr\u00b7sar\u00b7ko\u00b7pha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "In Frieden ruht hier Roger, der Normanne,", "tokens": ["In", "Frie\u00b7den", "ruht", "hier", "Ro\u00b7ger", ",", "der", "Nor\u00b7man\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Kaiser Heinrich und nach langem Streit", "tokens": ["Und", "Kai\u00b7ser", "Hein\u00b7rich", "und", "nach", "lan\u00b7gem", "Streit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "NE", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sein gro\u00dfer Sohn, die Sonne jener Zeit,", "tokens": ["Sein", "gro\u00b7\u00dfer", "Sohn", ",", "die", "Son\u00b7ne", "je\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der noch im Tode ringend mit dem Banne,", "tokens": ["Der", "noch", "im", "To\u00b7de", "rin\u00b7gend", "mit", "dem", "Ban\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der T\u00fccke seiner Feinde nur erlag \u2013", "tokens": ["Der", "T\u00fc\u00b7cke", "sei\u00b7ner", "Fein\u00b7de", "nur", "er\u00b7lag", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Beh\u00fctet, L\u00f6wen, seinen Sarkophag!", "tokens": ["Be\u00b7h\u00fc\u00b7tet", ",", "L\u00f6\u00b7wen", ",", "sei\u00b7nen", "Sar\u00b7ko\u00b7phag", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wo bei des Doriers Bau das rasche Zelt", "tokens": ["Wo", "bei", "des", "Do\u00b7ri\u00b7ers", "Bau", "das", "ra\u00b7sche", "Zelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "ART", "ADJA", "NN"], "meter": "----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Der Punier band und \u00fcber beider Fall", "tokens": ["Der", "Pu\u00b7nier", "band", "und", "\u00fc\u00b7ber", "bei\u00b7der", "Fall"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der R\u00f6mer trat und eine Welt", "tokens": ["Der", "R\u00f6\u00b7mer", "trat", "und", "ei\u00b7ne", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Prunk und Stolz umspann die V\u00f6lker all',", "tokens": ["Von", "Prunk", "und", "Stolz", "um\u00b7spann", "die", "V\u00f6l\u00b7ker", "all'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "PIS", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wo nahend mit dem sturmerprobten Schiff", "tokens": ["Wo", "na\u00b7hend", "mit", "dem", "stur\u00b7mer\u00b7prob\u00b7ten", "Schiff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Normann k\u00fchn vom Land Besitz ergriff,", "tokens": ["Der", "Nor\u00b7mann", "k\u00fchn", "vom", "Land", "Be\u00b7sitz", "er\u00b7griff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da h\u00e4lt nun Hof in niegeseh'ner Pracht", "tokens": ["Da", "h\u00e4lt", "nun", "Hof", "in", "nie\u00b7ge\u00b7seh'\u00b7ner", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In h\u00f6chstem Glanz des deutschen Kaisers Macht.", "tokens": ["In", "h\u00f6chs\u00b7tem", "Glanz", "des", "deut\u00b7schen", "Kai\u00b7sers", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ihm huldigt Meer und Berggebiet,", "tokens": ["Ihm", "hul\u00b7digt", "Meer", "und", "Berg\u00b7ge\u00b7biet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der Normann weicht, der Sarazene kniet,", "tokens": ["Der", "Nor\u00b7mann", "weicht", ",", "der", "Sa\u00b7ra\u00b7ze\u00b7ne", "kniet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und unter Dornen schm\u00fcckt, lorbeerumlaubt,", "tokens": ["Und", "un\u00b7ter", "Dor\u00b7nen", "schm\u00fcckt", ",", "lor\u00b7bee\u00b7rum\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die Krone von Jerusalem sein Haupt.", "tokens": ["Die", "Kro\u00b7ne", "von", "Je\u00b7ru\u00b7sa\u00b7lem", "sein", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "O wie viel Glanz und Gr\u00f6\u00dfe ruht verschlossen", "tokens": ["O", "wie", "viel", "Glanz", "und", "Gr\u00f6\u00b7\u00dfe", "ruht", "ver\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "PIAT", "NN", "KON", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Sarg mit ihm, welch minnefroher Stunden", "tokens": ["Im", "Sarg", "mit", "ihm", ",", "welch", "min\u00b7ne\u00b7fro\u00b7her", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "PPER", "$,", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erinnerung \u2013 und welcher Wunden!", "tokens": ["E\u00b7rin\u00b7ne\u00b7rung", "\u2013", "und", "wel\u00b7cher", "Wun\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KON", "PWAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie viele Tr\u00e4nen wurden da vergossen,", "tokens": ["Wie", "vie\u00b7le", "Tr\u00e4\u00b7nen", "wur\u00b7den", "da", "ver\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Als Kron' um Krone, Macht um Macht zerfiel,", "tokens": ["Als", "Kron'", "um", "Kro\u00b7ne", ",", "Macht", "um", "Macht", "zer\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "$,", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Als Haupt um Haupt dahinsank! O wie viel", "tokens": ["Als", "Haupt", "um", "Haupt", "da\u00b7hin\u00b7sank", "!", "O", "wie", "viel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "NN", "PTKVZ", "$.", "NE", "KOKOM", "PIAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der herbsten Tr\u00e4nen vor dem Leichentuch", "tokens": ["Der", "herbs\u00b7ten", "Tr\u00e4\u00b7nen", "vor", "dem", "Lei\u00b7chen\u00b7tuch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Geweint von holden Frau'n in Klaggewanden!", "tokens": ["Ge\u00b7weint", "von", "hol\u00b7den", "Frau'n", "in", "Klag\u00b7ge\u00b7wan\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wie mancher Ha\u00df ist und wie mancher Fluch", "tokens": ["Wie", "man\u00b7cher", "Ha\u00df", "ist", "und", "wie", "man\u00b7cher", "Fluch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "KON", "PWAV", "PIAT", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Hier, knirschend ins Gebet, vor Gott gestanden!", "tokens": ["Hier", ",", "knir\u00b7schend", "ins", "Ge\u00b7bet", ",", "vor", "Gott", "ge\u00b7stan\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "APPRART", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wie manches Wort, das nur von Rache sprach,", "tokens": ["Wie", "man\u00b7ches", "Wort", ",", "das", "nur", "von", "Ra\u00b7che", "sprach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "So dunkel, schwer und wie in Blut getaucht!", "tokens": ["So", "dun\u00b7kel", ",", "schwer", "und", "wie", "in", "Blut", "ge\u00b7taucht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON", "PWAV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wie mancher Seufzer \u00fcbers Meer gehaucht,", "tokens": ["Wie", "man\u00b7cher", "Seuf\u00b7zer", "\u00fc\u00b7bers", "Meer", "ge\u00b7haucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Der klagend sich an diesen Mauern brach!", "tokens": ["Der", "kla\u00b7gend", "sich", "an", "die\u00b7sen", "Mau\u00b7ern", "brach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ein letztes Echo, bis auch dies verhallt,", "tokens": ["Ein", "letz\u00b7tes", "E\u00b7cho", ",", "bis", "auch", "dies", "ver\u00b7hallt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$