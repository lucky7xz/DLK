{"textgrid.poem.35450": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Oh! Welche namenlose M\u00fcdigkeit", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oh! Welche namenlose M\u00fcdigkeit", "tokens": ["Oh", "!", "Wel\u00b7che", "na\u00b7men\u00b7lo\u00b7se", "M\u00fc\u00b7dig\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hat sich in meiner Seele festgenistet!", "tokens": ["Hat", "sich", "in", "mei\u00b7ner", "See\u00b7le", "fest\u00b7ge\u00b7nis\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Stumpf jeder Lust, stumpf jedem Leid,", "tokens": ["Stumpf", "je\u00b7der", "Lust", ",", "stumpf", "je\u00b7dem", "Leid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gibt's ", "tokens": ["Gibt's"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Gibt's ", "tokens": ["Gibt's"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Doch wie bedeutsam spiegelt's ", "tokens": ["Doch", "wie", "be\u00b7deut\u00b7sam", "spie\u00b7gelt's"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Lebens Inhalt, Mittelpunkt, Symbol \u2013", "tokens": ["Des", "Le\u00b7bens", "In\u00b7halt", ",", "Mit\u00b7tel\u00b7punkt", ",", "Sym\u00b7bol", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "$,", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sein ganzes aberwitz'ges Auf und Nieder ...", "tokens": ["Sein", "gan\u00b7zes", "a\u00b7ber\u00b7witz'\u00b7ges", "Auf", "und", "Nie\u00b7der", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}