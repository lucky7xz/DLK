{"textgrid.poem.38966": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie sich nach Norden der Magnet bewegt", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sich nach Norden der Magnet bewegt", "tokens": ["Wie", "sich", "nach", "Nor\u00b7den", "der", "Mag\u00b7net", "be\u00b7wegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "APPR", "NN", "ART", "NN", "VVFIN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "So wird mein Herz zu dir gezogen,", "tokens": ["So", "wird", "mein", "Herz", "zu", "dir", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Getreu es dir, nur dir entgegenschl\u00e4gt,", "tokens": ["Ge\u00b7treu", "es", "dir", ",", "nur", "dir", "ent\u00b7ge\u00b7gen\u00b7schl\u00e4gt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PPER", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie sich der Pol nicht r\u00fcckt am Himmelsbogen;", "tokens": ["Wie", "sich", "der", "Pol", "nicht", "r\u00fcckt", "am", "Him\u00b7mels\u00b7bo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "PTKNEG", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ihr L\u00fcfte, o ihr bringt mir s\u00fc\u00dfe Kunde,", "tokens": ["Ihr", "L\u00fcf\u00b7te", ",", "o", "ihr", "bringt", "mir", "s\u00fc\u00b7\u00dfe", "Kun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du sanfter Hauch, der meine Wange gr\u00fc\u00dft,", "tokens": ["Du", "sanf\u00b7ter", "Hauch", ",", "der", "mei\u00b7ne", "Wan\u00b7ge", "gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mir ist, ich f\u00fchl' den Athem, der dem Munde", "tokens": ["Mir", "ist", ",", "ich", "f\u00fchl'", "den", "A\u00b7them", ",", "der", "dem", "Mun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dem s\u00fc\u00dfen Glanz der Lippen sanft entflie\u00dft.", "tokens": ["Dem", "s\u00fc\u00b7\u00dfen", "Glanz", "der", "Lip\u00b7pen", "sanft", "ent\u00b7flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "O k\u00f6nnt ihr ihre Gegenwart vermeiden", "tokens": ["O", "k\u00f6nnt", "ihr", "ih\u00b7re", "Ge\u00b7gen\u00b7wart", "ver\u00b7mei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und durch die Blumen, durch Gestr\u00e4uche ziehn?", "tokens": ["Und", "durch", "die", "Blu\u00b7men", ",", "durch", "Ge\u00b7str\u00e4u\u00b7che", "ziehn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Beth\u00f6rt, miskennt ihr ach! die h\u00f6chsten Freuden,", "tokens": ["Be\u00b7th\u00f6rt", ",", "mis\u00b7kennt", "ihr", "ach", "!", "die", "h\u00f6chs\u00b7ten", "Freu\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "PPER", "ADV", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "An ihren rothen Wangen zu ergl\u00fchn,", "tokens": ["An", "ih\u00b7ren", "ro\u00b7then", "Wan\u00b7gen", "zu", "er\u00b7gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Die sch\u00f6ner als das Purpurblut der Rosen,", "tokens": ["Die", "sch\u00f6\u00b7ner", "als", "das", "Pur\u00b7pur\u00b7blut", "der", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und holder als der Lilien wei\u00dfe Pracht;", "tokens": ["Und", "hol\u00b7der", "als", "der", "Li\u00b7li\u00b7en", "wei\u00b7\u00dfe", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Die Augen, die ihr sonst mit sanftem Kosen", "tokens": ["Die", "Au\u00b7gen", ",", "die", "ihr", "sonst", "mit", "sanf\u00b7tem", "Ko\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Umweht und die euch dankbar angelacht. \u2013", "tokens": ["Um\u00b7weht", "und", "die", "euch", "dank\u00b7bar", "an\u00b7ge\u00b7lacht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "KON", "PRELS", "PPER", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Ihr seid, weil es gebot ihr Silberton,", "tokens": ["Ihr", "seid", ",", "weil", "es", "ge\u00b7bot", "ihr", "Sil\u00b7ber\u00b7ton", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Dem Aufenthalt der Seeligkeit entflohn,", "tokens": ["Dem", "Auf\u00b7ent\u00b7halt", "der", "See\u00b7lig\u00b7keit", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Ihr habt die weite Reise machen m\u00fcssen,", "tokens": ["Ihr", "habt", "die", "wei\u00b7te", "Rei\u00b7se", "ma\u00b7chen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Um mich Verirrten sch\u00f6n von ihr zu gr\u00fc\u00dfen:", "tokens": ["Um", "mich", "Ver\u00b7irr\u00b7ten", "sch\u00f6n", "von", "ihr", "zu", "gr\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "NN", "ADJD", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Du Abendroth flie\u00df golden zu ihr nieder,", "tokens": ["Du", "A\u00b7ben\u00b7droth", "flie\u00df", "gol\u00b7den", "zu", "ihr", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Bring ihr den Dank des treusten Herzens wieder.", "tokens": ["Bring", "ihr", "den", "Dank", "des", "treus\u00b7ten", "Her\u00b7zens", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}