{"textgrid.poem.36489": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "13. Der Bauer und der Sch\u00e4ferhund", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Bauer sa\u00df in einer Schenke,", "tokens": ["Ein", "Bau\u00b7er", "sa\u00df", "in", "ei\u00b7ner", "Schen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nahm seinen schweren Krug, und trank sich mehr als satt.", "tokens": ["Nahm", "sei\u00b7nen", "schwe\u00b7ren", "Krug", ",", "und", "trank", "sich", "mehr", "als", "satt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,", "KON", "VVFIN", "PRF", "PIAT", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ei! dacht' ein Sch\u00e4ferhund, ei, was doch f\u00fcr Getr\u00e4nke", "tokens": ["Ei", "!", "dacht'", "ein", "Sch\u00e4\u00b7fer\u00b7hund", ",", "ei", ",", "was", "doch", "f\u00fcr", "Ge\u00b7tr\u00e4n\u00b7ke"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "$,", "ITJ", "$,", "PRELS", "ADV", "APPR", "NN"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Der wohl in seinem Kruge hat?", "tokens": ["Der", "wohl", "in", "sei\u00b7nem", "Kru\u00b7ge", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Sch\u00e4ferhunde sind so klug, da\u00df in der Stadt", "tokens": ["Die", "Sch\u00e4\u00b7fer\u00b7hun\u00b7de", "sind", "so", "klug", ",", "da\u00df", "in", "der", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Scho\u00dfhund kl\u00fcger ist. Der Bauer ging hinaus;", "tokens": ["Kein", "Scho\u00df\u00b7hund", "kl\u00fc\u00b7ger", "ist", ".", "Der", "Bau\u00b7er", "ging", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VAFIN", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und weil er seinen Krug stehn ohne Deckel lie\u00df,", "tokens": ["Und", "weil", "er", "sei\u00b7nen", "Krug", "stehn", "oh\u00b7ne", "De\u00b7ckel", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So schlich der Hund sich hin um Kruge, trank daraus,", "tokens": ["So", "schlich", "der", "Hund", "sich", "hin", "um", "Kru\u00b7ge", ",", "trank", "da\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "PRF", "ADV", "APPR", "NN", "$,", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und sagte: das schmeckt s\u00fc\u00df!", "tokens": ["Und", "sag\u00b7te", ":", "das", "schmeckt", "s\u00fc\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PDS", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Bauer kam dazu. Der arme Hund! er kroch", "tokens": ["Der", "Bau\u00b7er", "kam", "da\u00b7zu", ".", "Der", "ar\u00b7me", "Hund", "!", "er", "kroch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PAV", "$.", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf allen Vieren. Warte! sprach der Bauer,", "tokens": ["Auf", "al\u00b7len", "Vie\u00b7ren", ".", "War\u00b7te", "!", "sprach", "der", "Bau\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$.", "NN", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und drohte mit dem Stocke; doch", "tokens": ["Und", "droh\u00b7te", "mit", "dem", "Sto\u00b7cke", ";", "doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$.", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er drohte nur!", "tokens": ["Er", "droh\u00b7te", "nur", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Herr Wirt, das Bier ist sauer!", "tokens": ["Herr", "Wirt", ",", "das", "Bier", "ist", "sau\u00b7er", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Rief er, und: eine Kanne noch!", "tokens": ["Rief", "er", ",", "und", ":", "ei\u00b7ne", "Kan\u00b7ne", "noch", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "$.", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Bier ist sauer! war ein Scherz,", "tokens": ["Das", "Bier", "ist", "sau\u00b7er", "!", "war", "ein", "Scherz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Allein der Hund verstand ihn nicht,", "tokens": ["Al\u00b7lein", "der", "Hund", "ver\u00b7stand", "ihn", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und wies die Z\u00e4hne.", "tokens": ["Und", "wies", "die", "Z\u00e4h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Hast du Herz?", "tokens": ["Hast", "du", "Herz", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Fragt da der Bauer ihn, und drohet ihn zu schlagen.", "tokens": ["Fragt", "da", "der", "Bau\u00b7er", "ihn", ",", "und", "dro\u00b7het", "ihn", "zu", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPER", "$,", "KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Herz? spricht der Hund: mein Herr, ", "tokens": ["Herz", "?", "spricht", "der", "Hund", ":", "mein", "Herr", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}