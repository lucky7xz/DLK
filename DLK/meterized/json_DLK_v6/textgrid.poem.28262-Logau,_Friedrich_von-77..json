{"textgrid.poem.28262": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "77.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df Schlesier haben den Esel gefressen,", "tokens": ["Da\u00df", "Schle\u00b7sier", "ha\u00b7ben", "den", "E\u00b7sel", "ge\u00b7fres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ist entweder nichts oder bleibet vergessen;", "tokens": ["Ist", "ent\u00b7we\u00b7der", "nichts", "o\u00b7der", "blei\u00b7bet", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "KON", "VVFIN", "VVPP", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sonst w\u00fcrden die fremden sich eigen gewehnen,", "tokens": ["Sonst", "w\u00fcr\u00b7den", "die", "frem\u00b7den", "sich", "ei\u00b7gen", "ge\u00b7weh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "PRF", "ADJD", "ADJA", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Nach Schlesischem Futter sich nimmer zu sehnen.", "tokens": ["Nach", "Schle\u00b7si\u00b7schem", "Fut\u00b7ter", "sich", "nim\u00b7mer", "zu", "seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}