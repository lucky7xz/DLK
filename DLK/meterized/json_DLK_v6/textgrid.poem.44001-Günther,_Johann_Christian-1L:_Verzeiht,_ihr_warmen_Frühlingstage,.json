{"textgrid.poem.44001": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verzeiht, ihr warmen Fr\u00fchlingstage,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verzeiht, ihr warmen Fr\u00fchlingstage,", "tokens": ["Ver\u00b7zeiht", ",", "ihr", "war\u00b7men", "Fr\u00fch\u00b7lings\u00b7ta\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr seyd zwar sch\u00f6n, doch nicht vor mich.", "tokens": ["Ihr", "seyd", "zwar", "sch\u00f6n", ",", "doch", "nicht", "vor", "mich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sommer macht mir hei\u00dfe Plage,", "tokens": ["Der", "Som\u00b7mer", "macht", "mir", "hei\u00b7\u00dfe", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Herbstluft ist ver\u00e4nderlich;", "tokens": ["Die", "Herbst\u00b7luft", "ist", "ver\u00b7\u00e4n\u00b7der\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum stimmt die Liebe mit mir ein:", "tokens": ["Drum", "stimmt", "die", "Lie\u00b7be", "mit", "mir", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Winter soll mein Fr\u00fchling seyn.", "tokens": ["Der", "Win\u00b7ter", "soll", "mein", "Fr\u00fch\u00b7ling", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Winter zeigt an seinen Gaben", "tokens": ["Der", "Win\u00b7ter", "zeigt", "an", "sei\u00b7nen", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sch\u00e4ze g\u00fctiger Natur,", "tokens": ["Die", "Sch\u00e4\u00b7ze", "g\u00fc\u00b7ti\u00b7ger", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er kan mit Most und \u00c4pfeln laben,", "tokens": ["Er", "kan", "mit", "Most", "und", "\u00c4p\u00b7feln", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er st\u00e4rckt den Leib und hilft der Cur,", "tokens": ["Er", "st\u00e4rckt", "den", "Leib", "und", "hilft", "der", "Cur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er bricht die Raserey der Pest", "tokens": ["Er", "bricht", "die", "Ra\u00b7se\u00b7rey", "der", "Pest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und dient zu Amors Jubelfest.", "tokens": ["Und", "dient", "zu", "A\u00b7mors", "Ju\u00b7bel\u00b7fest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Knaster schmeckt bey kaltem Wetter", "tokens": ["Der", "Knas\u00b7ter", "schmeckt", "bey", "kal\u00b7tem", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch halb so kr\u00e4ftig und so rein,", "tokens": ["Noch", "halb", "so", "kr\u00e4f\u00b7tig", "und", "so", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Jagd erg\u00f6zt der Erden G\u00f6tter", "tokens": ["Die", "Jagd", "er\u00b7g\u00f6zt", "der", "Er\u00b7den", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bringt im Schnee mehr Vortheil ein,", "tokens": ["Und", "bringt", "im", "Schnee", "mehr", "Vor\u00b7theil", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der freyen K\u00fcnste Ruhm und Preis", "tokens": ["Der", "frey\u00b7en", "K\u00fcns\u00b7te", "Ruhm", "und", "Preis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erhebt sich durch den Winterflei\u00df.", "tokens": ["Er\u00b7hebt", "sich", "durch", "den", "Win\u00b7ter\u00b7flei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Z\u00e4rtligkeit der s\u00fc\u00dfen Liebe", "tokens": ["Die", "Z\u00e4rt\u00b7lig\u00b7keit", "der", "s\u00fc\u00b7\u00dfen", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erwehlt vor andern diese Zeit;", "tokens": ["Er\u00b7wehlt", "vor", "an\u00b7dern", "die\u00b7se", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIS", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Zunder innerlicher Triebe", "tokens": ["Der", "Zun\u00b7der", "in\u00b7ner\u00b7li\u00b7cher", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verlacht des Frostes Grausamkeit;", "tokens": ["Ver\u00b7lacht", "des", "Fros\u00b7tes", "Grau\u00b7sam\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Morgenroth bricht sp\u00e4ter an,", "tokens": ["Das", "Mor\u00b7gen\u00b7roth", "bricht", "sp\u00e4\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Damit man l\u00e4nger k\u00fc\u00dfen kan.", "tokens": ["Da\u00b7mit", "man", "l\u00e4n\u00b7ger", "k\u00fc\u00b7\u00dfen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Sch\u00f6nen in den Armen liegen,", "tokens": ["Der", "Sch\u00f6\u00b7nen", "in", "den", "Ar\u00b7men", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn drau\u00dfen Nord und Regen pfeift,", "tokens": ["Wenn", "drau\u00b7\u00dfen", "Nord", "und", "Re\u00b7gen", "pfeift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Macht so ein inniglich Vergn\u00fcgen,", "tokens": ["Macht", "so", "ein", "in\u00b7nig\u00b7lich", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Dergleichen niemand recht begreift,", "tokens": ["Derg\u00b7lei\u00b7chen", "nie\u00b7mand", "recht", "be\u00b7greift", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er habe denn mit mir gef\u00fchlt,", "tokens": ["Er", "ha\u00b7be", "denn", "mit", "mir", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Wie sanfte sich's im Finstern spielt.", "tokens": ["Wie", "sanf\u00b7te", "sich's", "im", "Fins\u00b7tern", "spielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da ringen die getreuen Armen", "tokens": ["Da", "rin\u00b7gen", "die", "ge\u00b7treu\u00b7en", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Eintracht und Erg\u00f6zligkeit,", "tokens": ["Mit", "Ein\u00b7tracht", "und", "Er\u00b7g\u00f6z\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da la\u00dfen sie den Pfiehl erwarmen,", "tokens": ["Da", "la\u00b7\u00dfen", "sie", "den", "Pfiehl", "er\u00b7war\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den oft ein falsches Dach beschneit,", "tokens": ["Den", "oft", "ein", "fal\u00b7sches", "Dach", "be\u00b7schneit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da streiten sie mit Ku\u00df und Bi\u00df", "tokens": ["Da", "strei\u00b7ten", "sie", "mit", "Ku\u00df", "und", "Bi\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und w\u00fcntschen lange Finstern\u00fc\u00df.", "tokens": ["Und", "w\u00fcnt\u00b7schen", "lan\u00b7ge", "Fins\u00b7ter\u00b7n\u00fc\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Das Ei\u00df beweist den Hofnungsspiegel,", "tokens": ["Das", "Ei\u00df", "be\u00b7weist", "den", "Hof\u00b7nungs\u00b7spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der viel entwirft und leicht zerf\u00e4llt;", "tokens": ["Der", "viel", "ent\u00b7wirft", "und", "leicht", "zer\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich k\u00fc\u00dfe den gefrornen Riegel,", "tokens": ["Ich", "k\u00fc\u00b7\u00dfe", "den", "ge\u00b7fror\u00b7nen", "Rie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der mir Amanden vorenth\u00e4lt,", "tokens": ["Der", "mir", "A\u00b7man\u00b7den", "vor\u00b7ent\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "So oft mein Spiel ein St\u00e4ndchen bringt", "tokens": ["So", "oft", "mein", "Spiel", "ein", "St\u00e4nd\u00b7chen", "bringt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Sayth und Fl\u00f6the sch\u00e4rfer klingt.", "tokens": ["Und", "Say\u00b7th", "und", "Fl\u00f6\u00b7the", "sch\u00e4r\u00b7fer", "klingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Ich zieh den Mond- und Sternenschimmer", "tokens": ["Ich", "zieh", "den", "Mon\u00b7d", "und", "Ster\u00b7nen\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "TRUNC", "KON", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dem angenehmsten Tage vor;", "tokens": ["Dem", "an\u00b7ge\u00b7nehms\u00b7ten", "Ta\u00b7ge", "vor", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da heb ich oft aus meinem Zimmer", "tokens": ["Da", "heb", "ich", "oft", "aus", "mei\u00b7nem", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Haupt, Augen, Herz und Geist empor,", "tokens": ["Haupt", ",", "Au\u00b7gen", ",", "Herz", "und", "Geist", "em\u00b7por", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da findet mein Verwundern kaum", "tokens": ["Da", "fin\u00b7det", "mein", "Ver\u00b7wun\u00b7dern", "kaum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In diesem weiten Raume Raum.", "tokens": ["In", "die\u00b7sem", "wei\u00b7ten", "Rau\u00b7me", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Euch Br\u00fcder h\u00e4tt ich bald verge\u00dfen,", "tokens": ["Euch", "Br\u00fc\u00b7der", "h\u00e4tt", "ich", "bald", "ver\u00b7ge\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Euch, die ihr nebst der deutschen Treu", "tokens": ["Euch", ",", "die", "ihr", "nebst", "der", "deut\u00b7schen", "Treu"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit mir viel N\u00e4chte durch gese\u00dfen;", "tokens": ["Mit", "mir", "viel", "N\u00e4ch\u00b7te", "durch", "ge\u00b7se\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PIAT", "NN", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sagt, ob wo etwas Be\u00dfres sey,", "tokens": ["Sagt", ",", "ob", "wo", "et\u00b7was", "Be\u00df\u00b7res", "sey", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PWAV", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als hier bey Pfeifen und Camin", "tokens": ["Als", "hier", "bey", "Pfei\u00b7fen", "und", "Ca\u00b7min"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Welt mitsamt den Grillen fliehn.", "tokens": ["Die", "Welt", "mit\u00b7samt", "den", "Gril\u00b7len", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Der Winter bleibt der Kern vom Jahre,", "tokens": ["Der", "Win\u00b7ter", "bleibt", "der", "Kern", "vom", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Winter bin ich munter dran,", "tokens": ["Im", "Win\u00b7ter", "bin", "ich", "mun\u00b7ter", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Winter ist ein Bild der Baare", "tokens": ["Der", "Win\u00b7ter", "ist", "ein", "Bild", "der", "Baa\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und lehrt mich leben, weil ich kan;", "tokens": ["Und", "lehrt", "mich", "le\u00b7ben", ",", "weil", "ich", "kan", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Sp\u00f6tter redet mir nicht ein;", "tokens": ["Ihr", "Sp\u00f6t\u00b7ter", "re\u00b7det", "mir", "nicht", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Winter soll mein Fr\u00fchling seyn.", "tokens": ["Der", "Win\u00b7ter", "soll", "mein", "Fr\u00fch\u00b7ling", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}