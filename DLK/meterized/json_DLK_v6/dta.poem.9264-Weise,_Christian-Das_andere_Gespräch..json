{"dta.poem.9264": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das andere Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Hat der nicht ein sch\u00f6nes gl\u00fccke", "tokens": ["Hat", "der", "nicht", "ein", "sch\u00f6\u00b7nes", "gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem ein m\u00e4dgen die bar\u00fccke", "tokens": ["Dem", "ein", "m\u00e4d\u00b7gen", "die", "ba\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "ADJA", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unversehns vom kopffe raubt:", "tokens": ["Un\u00b7ver\u00b7sehns", "vom", "kopf\u00b7fe", "raubt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn sie zeigt in diesem schertze/", "tokens": ["Denn", "sie", "zeigt", "in", "die\u00b7sem", "schert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PDAT", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ihr lust-begierig hertze", "tokens": ["Da\u00df", "ihr", "lust\u00b7be\u00b7gie\u00b7rig", "hert\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jhm ingleichen was erlaubt.", "tokens": ["Jhm", "in\u00b7glei\u00b7chen", "was", "er\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIS", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Jungfern/ welche furchtsam leben/", "tokens": ["Jung\u00b7fern", "/", "wel\u00b7che", "furcht\u00b7sam", "le\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PRELS", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und in scham und hoffnung schweben/", "tokens": ["Und", "in", "scham", "und", "hoff\u00b7nung", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "KON", "NN", "VVINF", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Wollen nur geraubet seyn/", "tokens": ["Wol\u00b7len", "nur", "ge\u00b7rau\u00b7bet", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVPP", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und indem sie was verg\u00f6nnen/", "tokens": ["Und", "in\u00b7dem", "sie", "was", "ver\u00b7g\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn sie sich nicht wehren k\u00f6nnen/", "tokens": ["Wenn", "sie", "sich", "nicht", "weh\u00b7ren", "k\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "VMINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Z\u00fcrnen sie nur auff den schein.", "tokens": ["Z\u00fcr\u00b7nen", "sie", "nur", "auff", "den", "schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Drum/ so bald sie inne werden/", "tokens": ["Drum", "/", "so", "bald", "sie", "in\u00b7ne", "wer\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "ADV", "ADV", "PPER", "PTKVZ", "VAFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ein ander in geberden", "tokens": ["Da\u00df", "ein", "an\u00b7der", "in", "ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJD", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Will zu still und bl\u00f6de gehn/", "tokens": ["Will", "zu", "still", "und", "bl\u00f6\u00b7de", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKA", "ADJD", "KON", "ADJA", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Geben diese spiel-genossen", "tokens": ["Ge\u00b7ben", "die\u00b7se", "spiel\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter solchen kinder-possen", "tokens": ["Un\u00b7ter", "sol\u00b7chen", "kin\u00b7der\u00b7pos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jhre meynung zu verstehn.", "tokens": ["Ih\u00b7re", "mey\u00b7nung", "zu", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Ach f\u00fcrwahr die blossen haare", "tokens": ["Ach", "f\u00fcr\u00b7wahr", "die", "blos\u00b7sen", "haa\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind nicht eine solche waare", "tokens": ["Sind", "nicht", "ei\u00b7ne", "sol\u00b7che", "waa\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ART", "PIAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die so grosse lust erweckt/", "tokens": ["Die", "so", "gros\u00b7se", "lust", "er\u00b7weckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich will mich hoch verwetten/", "tokens": ["Und", "ich", "will", "mich", "hoch", "ver\u00b7wet\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die verliebten kinder h\u00e4tten", "tokens": ["Die", "ver\u00b7lieb\u00b7ten", "kin\u00b7der", "h\u00e4t\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieber was darunter steckt.", "tokens": ["Lie\u00b7ber", "was", "da\u00b7run\u00b7ter", "steckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PWS", "PAV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Nun wolan/ man mu\u00df sich r\u00e4chen/", "tokens": ["Nun", "wo\u00b7lan", "/", "man", "mu\u00df", "sich", "r\u00e4\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "PIS", "VMFIN", "PRF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil sie selbst die bahne brechen/", "tokens": ["Weil", "sie", "selbst", "die", "bah\u00b7ne", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die der liebe wohl gef\u00e4lt:", "tokens": ["Die", "der", "lie\u00b7be", "wohl", "ge\u00b7f\u00e4lt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "VVFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Last die sinnen ungekr\u00e4ncket/", "tokens": ["Last", "die", "sin\u00b7nen", "un\u00b7ge\u00b7kr\u00e4n\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schertzt hingegen und gedencket", "tokens": ["Schertzt", "hin\u00b7ge\u00b7gen", "und", "ge\u00b7den\u00b7cket"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf den sch\u00f6nsten wiedergelt.", "tokens": ["Auf", "den", "sch\u00f6ns\u00b7ten", "wie\u00b7der\u00b7gelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}