{"textgrid.poem.30013": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stichus hat ein b\u00f6ses Weib, wil sie willig nur vertragen;", "tokens": ["Stic\u00b7hus", "hat", "ein", "b\u00f6\u00b7ses", "Weib", ",", "wil", "sie", "wil\u00b7lig", "nur", "ver\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$,", "VMFIN", "PPER", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "Meint, ihr Grimm werd endlich schw\u00e4ch- und sich m\u00fcden von den plagen,", "tokens": ["Meint", ",", "ihr", "Grimm", "werd", "end\u00b7lich", "schw\u00e4ch", "und", "sich", "m\u00fc\u00b7den", "von", "den", "pla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "NE", "VAFIN", "ADV", "TRUNC", "KON", "PRF", "ADJD", "APPR", "ART", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Da ihn sonst ein frisches Weib werde frisch auffs neue nagen.", "tokens": ["Da", "ihn", "sonst", "ein", "fri\u00b7sches", "Weib", "wer\u00b7de", "frisch", "auffs", "neu\u00b7e", "na\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VAFIN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}