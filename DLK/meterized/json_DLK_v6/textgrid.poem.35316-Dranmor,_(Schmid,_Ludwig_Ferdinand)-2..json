{"textgrid.poem.35316": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die zum Triumphe sich berufen w\u00e4hnen", "tokens": ["Die", "zum", "Tri\u00b7um\u00b7phe", "sich", "be\u00b7ru\u00b7fen", "w\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "PRF", "VVPP", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und um ein modernd Kreuz sich gl\u00e4ubig scharen,", "tokens": ["Und", "um", "ein", "mo\u00b7dernd", "Kreuz", "sich", "gl\u00e4u\u00b7big", "scha\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJD", "NN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer sind sie? Gro\u00dfges\u00e4ugt mit bittern Thr\u00e4nen,", "tokens": ["Wer", "sind", "sie", "?", "Gro\u00df\u00b7ge\u00b7s\u00e4ugt", "mit", "bit\u00b7tern", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$.", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein junges Volk von nur zweitausend Jahren.", "tokens": ["Ein", "jun\u00b7ges", "Volk", "von", "nur", "zweit\u00b7au\u00b7send", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zweitausend Jahre? Und die kaum erweckte", "tokens": ["Zweit\u00b7au\u00b7send", "Jah\u00b7re", "?", "Und", "die", "kaum", "er\u00b7weck\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "$.", "KON", "ART", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gemeinde will den Weltenscepter f\u00fchren?", "tokens": ["Ge\u00b7mein\u00b7de", "will", "den", "Wel\u00b7ten\u00b7scep\u00b7ter", "f\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nach Jahrtausenden, welch neuer Sekte", "tokens": ["Und", "nach", "Jahr\u00b7tau\u00b7sen\u00b7den", ",", "welch", "neu\u00b7er", "Sek\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird dann der Bildung erster Rang geb\u00fchren?", "tokens": ["Wird", "dann", "der", "Bil\u00b7dung", "ers\u00b7ter", "Rang", "ge\u00b7b\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wollt ihr jene Zahl ums Tausendfache,", "tokens": ["Und", "wollt", "ihr", "je\u00b7ne", "Zahl", "ums", "Tau\u00b7send\u00b7fa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PDAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nein, bis ins Unerme\u00dfliche vermehren,", "tokens": ["Nein", ",", "bis", "ins", "Un\u00b7er\u00b7me\u00df\u00b7li\u00b7che", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So kann das Menschenherz, das eitle, schwache,", "tokens": ["So", "kann", "das", "Men\u00b7schen\u00b7herz", ",", "das", "eit\u00b7le", ",", "schwa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich ewiger Entsagung nicht erwehren \u2013 \u2013", "tokens": ["Sich", "e\u00b7wi\u00b7ger", "Ent\u00b7sa\u00b7gung", "nicht", "er\u00b7weh\u00b7ren", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "ADJA", "NN", "PTKNEG", "VVINF", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.2": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u201