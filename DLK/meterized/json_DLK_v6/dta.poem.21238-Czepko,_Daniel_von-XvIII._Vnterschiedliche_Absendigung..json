{"dta.poem.21238": {"metadata": {"author": {"name": "Czepko, Daniel von", "birth": "N.A.", "death": "N.A."}, "title": "XvIII.  \n  Vnterschiedliche Absendigung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20871-8", "language": ["de:0.99"], "booktitle": "Czepko, Daniel von: Triumph Bogen. Breslau, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Der wilde Tartar kompt von Pferde Blutt erhalten/", "tokens": ["Der", "wil\u00b7de", "Tar\u00b7tar", "kompt", "von", "Pfer\u00b7de", "Blutt", "er\u00b7hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von fernen Tanais in vnser Land gedrabt:", "tokens": ["Von", "fer\u00b7nen", "Ta\u00b7nais", "in", "vn\u00b7ser", "Land", "ge\u00b7drabt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VAPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vnd der so zu ", "tokens": ["Vnd", "der", "so", "zu"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADV", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Schickt seine Tittull her vnd hat den Thron begabt.", "tokens": ["Schickt", "sei\u00b7ne", "Tit\u00b7tull", "her", "vnd", "hat", "den", "Thron", "be\u00b7gabt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "KON", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Geschneihtes Land herschwtmbt/ schwimbt auch mit Seegeln her/", "tokens": ["Ge\u00b7schneih\u00b7tes", "Land", "her\u00b7schwtmbt", "/", "schwimbt", "auch", "mit", "See\u00b7geln", "her", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$(", "VVFIN", "ADV", "APPR", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vnd ", "tokens": ["Vnd"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Schantz ", "tokens": ["Schantz"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Die am ", "tokens": ["Die", "am"], "token_info": ["word", "word"], "pos": ["ART", "APPRART"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "H\u00f6hrn Vnsre strenge Sieg au\u00df sichrer Mi", "tokens": ["H\u00f6hrn", "Vns\u00b7re", "stren\u00b7ge", "Sieg", "au\u00df", "sich\u00b7rer", "Mi"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Vnd der ", "tokens": ["Vnd", "der"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Erkennet ", "tokens": ["Er\u00b7ken\u00b7net"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "So Barbar ist kein ", "tokens": ["So", "Bar\u00b7bar", "ist", "kein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PIAT"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Er bett di\u00df ", "tokens": ["Er", "bett", "di\u00df"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PDS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Ist Er nicht vnser Freund! so w\u00fcnscht Er es zu werden.", "tokens": ["Ist", "Er", "nicht", "vn\u00b7ser", "Freund", "!", "so", "w\u00fcnscht", "Er", "es", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPER", "PPER", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Vnd der ist lieb vnd werth/ der vngebeten k\u00f6mbt.", "tokens": ["Vnd", "der", "ist", "lieb", "vnd", "werth", "/", "der", "vn\u00b7ge\u00b7be\u00b7ten", "k\u00f6mbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADJD", "KON", "ADJD", "$(", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}