nfo": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wenige sahn's ihr an.\u00ab \u2013", "tokens": ["We\u00b7ni\u00b7ge", "sahn's", "ihr", "an", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PIAT", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}