{"dta.poem.23544": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vox Dei Vox Populi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit euren siebzig Stimmen habet", "tokens": ["Mit", "eu\u00b7ren", "sieb\u00b7zig", "Stim\u00b7men", "ha\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "CARD", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr uns gar manchen Sang gemacht,", "tokens": ["Ihr", "uns", "gar", "man\u00b7chen", "Sang", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch hat der Sang uns nie gelabet,", "tokens": ["Doch", "hat", "der", "Sang", "uns", "nie", "ge\u00b7la\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nie gute Stimmung uns gebracht.", "tokens": ["Nie", "gu\u00b7te", "Stim\u00b7mung", "uns", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wenn ihr auch in allen Dingen", "tokens": ["Und", "wenn", "ihr", "auch", "in", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die siebzig Stimmen richtig z\u00e4hlt,", "tokens": ["Die", "sieb\u00b7zig", "Stim\u00b7men", "rich\u00b7tig", "z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was kann dem Menschen doch gelingen,", "tokens": ["Was", "kann", "dem", "Men\u00b7schen", "doch", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn's ihm an ", "tokens": ["Wenn's", "ihm", "an"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PTKVZ"], "meter": "+-+", "measure": "trochaic.di"}}}}}