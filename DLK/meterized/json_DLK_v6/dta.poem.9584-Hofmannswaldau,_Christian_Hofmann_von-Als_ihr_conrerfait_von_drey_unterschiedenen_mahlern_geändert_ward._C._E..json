{"dta.poem.9584": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als ihr conrerfait von drey unterschiedenen  \n mahlern ge\u00e4ndert ward.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Doch/ Amor/ deine faust/ die wird es wohl verrichten/", "tokens": ["Doch", "/", "A\u00b7mor", "/", "dei\u00b7ne", "faust", "/", "die", "wird", "es", "wohl", "ver\u00b7rich\u00b7ten", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "NE", "$(", "PPOSAT", "NN", "$(", "PDS", "VAFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was andern saur und schwer/ ja gar unm\u00f6glich f\u00e4llt.", "tokens": ["Was", "an\u00b7dern", "saur", "und", "schwer", "/", "ja", "gar", "un\u00b7m\u00f6g\u00b7lich", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADJD", "KON", "ADJD", "$(", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nim deinen s\u00fcssen kiel und zeige dann der welt", "tokens": ["Nim", "dei\u00b7nen", "s\u00fcs\u00b7sen", "kiel", "und", "zei\u00b7ge", "dann", "der", "welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "KON", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein bild/ das weder zeit noch unfall wird zernichten:", "tokens": ["Ein", "bild", "/", "das", "we\u00b7der", "zeit", "noch", "un\u00b7fall", "wird", "zer\u00b7nich\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "KON", "NN", "ADV", "ADJD", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Entwirff dein meisterst\u00fcck! du wirst uns h\u00f6chst verpflichten/", "tokens": ["Ent\u00b7wirff", "dein", "meis\u00b7ter\u00b7st\u00fcck", "!", "du", "wirst", "uns", "h\u00f6chst", "ver\u00b7pflich\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und mach ein antlitz/ das die sch\u00f6nheit in sich h\u00e4lt/", "tokens": ["Und", "mach", "ein", "ant\u00b7litz", "/", "das", "die", "sch\u00f6n\u00b7heit", "in", "sich", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$(", "PRELS", "ART", "NN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Witz/ klugheit und verstand werd\u2019 an die stirn gestellt/", "tokens": ["Witz", "/", "klug\u00b7heit", "und", "ver\u00b7stand", "werd'", "an", "die", "stirn", "ge\u00b7stellt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "KON", "VVFIN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die anmuth in den mund nebst ihren s\u00fcssen fr\u00fcchten:", "tokens": ["Die", "an\u00b7muth", "in", "den", "mund", "nebst", "ih\u00b7ren", "s\u00fcs\u00b7sen", "fr\u00fcch\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "PPOSAT", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bild\u2019 augen/ derer glut mit sanfftmuth angef\u00fcllt/", "tokens": ["Bild'", "au\u00b7gen", "/", "de\u00b7rer", "glut", "mit", "sanfft\u00b7muth", "an\u00b7ge\u00b7f\u00fcllt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "PDS", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mahl wangen/ welche sich in freundlichkeit verh\u00fcllt/", "tokens": ["Mahl", "wan\u00b7gen", "/", "wel\u00b7che", "sich", "in", "freund\u00b7lich\u00b7keit", "ver\u00b7h\u00fcllt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "PRELS", "PRF", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die h\u00e4nde stell uns vor/ als zweene k\u00fcnstlerinnen;", "tokens": ["Die", "h\u00e4n\u00b7de", "stell", "uns", "vor", "/", "als", "zwee\u00b7ne", "k\u00fcnst\u00b7le\u00b7rin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "PPER", "PTKVZ", "$(", "KOUS", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dann brings Liboren hin/ und gib ihr den bericht:", "tokens": ["Dann", "brings", "Li\u00b7bo\u00b7ren", "hin", "/", "und", "gib", "ihr", "den", "be\u00b7richt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "$(", "KON", "VVIMP", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Drey mahler k\u00f6nnen es mit farb\u2019 und pinseln nicht/", "tokens": ["Drey", "mah\u00b7ler", "k\u00f6n\u00b7nen", "es", "mit", "fa\u00b7rb'", "und", "pin\u00b7seln", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VMFIN", "PPER", "APPR", "NN", "KON", "VVFIN", "PTKNEG", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Was hier mein kiel gethan/ entwerffen noch beginnen.", "tokens": ["Was", "hier", "mein", "kiel", "ge\u00b7than", "/", "ent\u00b7werf\u00b7fen", "noch", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "VVPP", "$(", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}