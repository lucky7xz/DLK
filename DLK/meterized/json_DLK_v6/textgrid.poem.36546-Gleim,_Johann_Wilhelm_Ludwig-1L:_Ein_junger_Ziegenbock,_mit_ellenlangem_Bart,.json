{"textgrid.poem.36546": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein junger Ziegenbock, mit ellenlangem Bart,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein junger Ziegenbock, mit ellenlangem Bart,", "tokens": ["Ein", "jun\u00b7ger", "Zie\u00b7gen\u00b7bock", ",", "mit", "el\u00b7len\u00b7lan\u00b7gem", "Bart", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und spitzem, festen Horn, ein Held! nach seiner Art", "tokens": ["Und", "spit\u00b7zem", ",", "fes\u00b7ten", "Horn", ",", "ein", "Held", "!", "nach", "sei\u00b7ner", "Art"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "$,", "ADJA", "NN", "$,", "ART", "NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Eisenfresser! stand auf einem hohen Dache,", "tokens": ["Ein", "Ei\u00b7sen\u00b7fres\u00b7ser", "!", "stand", "auf", "ei\u00b7nem", "ho\u00b7hen", "Da\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich umzusehn. Ein Wolf erschien.", "tokens": ["Sich", "um\u00b7zu\u00b7sehn", ".", "Ein", "Wolf", "er\u00b7schien", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "VVIZU", "$.", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Ziegenbock, der Held, sah ihn,", "tokens": ["Der", "Zie\u00b7gen\u00b7bock", ",", "der", "Held", ",", "sah", "ihn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ward mutig, sch\u00e4umte Rache!", "tokens": ["Ward", "mu\u00b7tig", ",", "sch\u00e4um\u00b7te", "Ra\u00b7che", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du M\u00f6rder du! du Dieb!", "tokens": ["Du", "M\u00f6r\u00b7der", "du", "!", "du", "Dieb", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "PPER", "$.", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Komm mir nur nicht herauf, ist dir dein Leben lieb!", "tokens": ["Komm", "mir", "nur", "nicht", "her\u00b7auf", ",", "ist", "dir", "dein", "Le\u00b7ben", "lieb", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$,", "VAFIN", "PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du bist's, Tyrann! du bist's, \u2013 die Mutter weinet noch \u2013", "tokens": ["Du", "bist's", ",", "Ty\u00b7rann", "!", "du", "bist's", ",", "\u2013", "die", "Mut\u00b7ter", "wei\u00b7net", "noch", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "NE", "$.", "PPER", "NE", "$,", "$(", "ART", "NN", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du bist's, o Frevelthat!", "tokens": ["Du", "bist's", ",", "o", "Fre\u00b7vel\u00b7that", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der mir mein Lamm geraubt und aufgefressen hat!", "tokens": ["Der", "mir", "mein", "Lamm", "ge\u00b7raubt", "und", "auf\u00b7ge\u00b7fres\u00b7sen", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVPP", "KON", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Herr Bock, bem\u00fchen Sie sich doch", "tokens": ["Herr", "Bock", ",", "be\u00b7m\u00fc\u00b7hen", "Sie", "sich", "doch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "VVFIN", "PPER", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu mir herunter! sprach der Wolf.", "tokens": ["Zu", "mir", "her\u00b7un\u00b7ter", "!", "sprach", "der", "Wolf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$.", "VVFIN", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zwar haben sie erhabnen Geist", "tokens": ["Zwar", "ha\u00b7ben", "sie", "er\u00b7hab\u00b7nen", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Lust zu fechten, das beweist", "tokens": ["Und", "Lust", "zu", "fech\u00b7ten", ",", "das", "be\u00b7weist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "PTKZU", "VVINF", "$,", "PDS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr langer Bart! und ihres Horns Gestalt", "tokens": ["Ihr", "lan\u00b7ger", "Bart", "!", "und", "ih\u00b7res", "Horns", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ist f\u00fcrchterlich, und ich bin alt;", "tokens": ["Ist", "f\u00fcrch\u00b7ter\u00b7lich", ",", "und", "ich", "bin", "alt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch, solchen Schimpf zu r\u00e4chen, wird man munter,", "tokens": ["Doch", ",", "sol\u00b7chen", "Schimpf", "zu", "r\u00e4\u00b7chen", ",", "wird", "man", "mun\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PIAT", "NN", "PTKZU", "VVINF", "$,", "VAFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und scheut nicht Bart, nicht Horn; Sie kommen nur herunter!", "tokens": ["Und", "scheut", "nicht", "Bart", ",", "nicht", "Horn", ";", "Sie", "kom\u00b7men", "nur", "her\u00b7un\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "NN", "$,", "PTKNEG", "NE", "$.", "PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was that der Bock, der Held? Er schnob gerechten Zorn,", "tokens": ["Was", "that", "der", "Bock", ",", "der", "Held", "?", "Er", "schnob", "ge\u00b7rech\u00b7ten", "Zorn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NE", "$,", "ART", "NN", "$.", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sch\u00fcttelte den Bart; sein Horn", "tokens": ["Und", "sch\u00fct\u00b7tel\u00b7te", "den", "Bart", ";", "sein", "Horn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gebraucht er aber nicht; genug, er lie\u00df es sehn,", "tokens": ["Ge\u00b7braucht", "er", "a\u00b7ber", "nicht", ";", "ge\u00b7nug", ",", "er", "lie\u00df", "es", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PTKNEG", "$.", "ADV", "$,", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sagte: R\u00e4uber! willst du gehn!", "tokens": ["Und", "sag\u00b7te", ":", "R\u00e4u\u00b7ber", "!", "willst", "du", "gehn", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NN", "$.", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und was der Wolf? Herr Bock, sprach er,", "tokens": ["Und", "was", "der", "Wolf", "?", "Herr", "Bock", ",", "sprach", "er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NE", "$.", "NN", "NE", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Ich r\u00e4chte mich, und w\u00e4ren sie ein B\u00e4r!", "tokens": ["Ich", "r\u00e4ch\u00b7te", "mich", ",", "und", "w\u00e4\u00b7ren", "sie", "ein", "B\u00e4r", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was aber hindert meine Rache?", "tokens": ["Was", "a\u00b7ber", "hin\u00b7dert", "mei\u00b7ne", "Ra\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie nicht; das Dach! Herunter von dem Dache!", "tokens": ["Sie", "nicht", ";", "das", "Dach", "!", "Her\u00b7un\u00b7ter", "von", "dem", "Da\u00b7che", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$.", "ART", "NN", "$.", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}