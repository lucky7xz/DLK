{"textgrid.poem.42651": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als, rings bepflanzt mit wolkennahen Th\u00fcrmen,", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als, rings bepflanzt mit wolkennahen Th\u00fcrmen,", "tokens": ["Als", ",", "rings", "be\u00b7pflanzt", "mit", "wol\u00b7ken\u00b7na\u00b7hen", "Th\u00fcr\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADV", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das stolze Wien mir aus den Augen kam,", "tokens": ["Das", "stol\u00b7ze", "Wi\u00b7en", "mir", "aus", "den", "Au\u00b7gen", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und, vor der Glut der Sonne mich zu schirmen,", "tokens": ["Und", ",", "vor", "der", "Glut", "der", "Son\u00b7ne", "mich", "zu", "schir\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Br\u00fchl mich drauf in seine Schatten nahm,", "tokens": ["Der", "Br\u00fchl", "mich", "drauf", "in", "sei\u00b7ne", "Schat\u00b7ten", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PAV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verband ich mich bey mehr als zwanzig G\u00f6ttern", "tokens": ["Ver\u00b7band", "ich", "mich", "bey", "mehr", "als", "zwan\u00b7zig", "G\u00f6t\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PIAT", "KOKOM", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit einem Eid: die Sonne sollte nicht", "tokens": ["Mit", "ei\u00b7nem", "Eid", ":", "die", "Son\u00b7ne", "soll\u00b7te", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "ART", "NN", "VMFIN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zum zweytenmal den Berg her\u00fcberklettern,", "tokens": ["Zum", "zwey\u00b7ten\u00b7mal", "den", "Berg", "her\u00b7\u00fc\u00b7berk\u00b7let\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es l\u00e4ge denn das stattlichste Gedicht,", "tokens": ["Es", "l\u00e4\u00b7ge", "denn", "das", "statt\u00b7lichs\u00b7te", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So elegant, wie meines Wissens keiner", "tokens": ["So", "e\u00b7le\u00b7gant", ",", "wie", "mei\u00b7nes", "Wis\u00b7sens", "kei\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Im deutschen Reich, als etwan Unsereiner,", "tokens": ["Im", "deut\u00b7schen", "Reich", ",", "als", "et\u00b7wan", "Un\u00b7ser\u00b7ei\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "KOUS", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Zu schreiben pflegt, an dich, o Freund! bereit.", "tokens": ["Zu", "schrei\u00b7ben", "pflegt", ",", "an", "dich", ",", "o", "Freund", "!", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "$,", "APPR", "PPER", "$,", "FM", "NN", "$.", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Doch da nun schon wir Dichter jederzeit", "tokens": ["Doch", "da", "nun", "schon", "wir", "Dich\u00b7ter", "je\u00b7der\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADV", "PPER", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Beym Layenvolk f\u00fcr L\u00fcgenschmiede galten,", "tokens": ["Beym", "La\u00b7yen\u00b7volk", "f\u00fcr", "L\u00fc\u00b7gen\u00b7schmie\u00b7de", "gal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "So liess es denn auch meine Wenigkeit,", "tokens": ["So", "liess", "es", "denn", "auch", "mei\u00b7ne", "We\u00b7nig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "So sehr ich sonst der Mann bin, Wort zu halten,", "tokens": ["So", "sehr", "ich", "sonst", "der", "Mann", "bin", ",", "Wort", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "ART", "NN", "VAFIN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Dem Handwerksbrauch zu Liebe, h\u00fcbsch beym alten;", "tokens": ["Dem", "Hand\u00b7werks\u00b7brauch", "zu", "Lie\u00b7be", ",", "h\u00fcbsch", "beym", "al\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "ADJD", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Denn wirklich hat bereits zum viertenmal", "tokens": ["Denn", "wirk\u00b7lich", "hat", "be\u00b7reits", "zum", "vier\u00b7ten\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "APPRART", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Die k\u00fchle Nacht nun Fl\u00e4chen, Berg und Thal", "tokens": ["Die", "k\u00fch\u00b7le", "Nacht", "nun", "Fl\u00e4\u00b7chen", ",", "Berg", "und", "Thal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und Feld und Wald mit Dunkel rings umh\u00fcllet.", "tokens": ["Und", "Feld", "und", "Wald", "mit", "Dun\u00b7kel", "rings", "um\u00b7h\u00fcl\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Und doch ist noch mein Eidschwur unerf\u00fcllet,", "tokens": ["Und", "doch", "ist", "noch", "mein", "Eid\u00b7schwur", "un\u00b7er\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Und blieb' es auch, h\u00e4tt' ein Gewitter hier", "tokens": ["Und", "blieb'", "es", "auch", ",", "h\u00e4tt'", "ein", "Ge\u00b7wit\u00b7ter", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "In's Gartenhaus mich nicht hereingeschrecket,", "tokens": ["In's", "Gar\u00b7ten\u00b7haus", "mich", "nicht", "her\u00b7ein\u00b7ge\u00b7schre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Und h\u00e4tte nicht der Donner \u00fcber mir", "tokens": ["Und", "h\u00e4t\u00b7te", "nicht", "der", "Don\u00b7ner", "\u00fc\u00b7ber", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PTKNEG", "ART", "NN", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Mein schlafendes Gewissen aufgewecket.", "tokens": ["Mein", "schla\u00b7fen\u00b7des", "Ge\u00b7wis\u00b7sen", "auf\u00b7ge\u00b7we\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "So h\u00f6re denn, was meine Neubegier", "tokens": ["So", "h\u00f6\u00b7re", "denn", ",", "was", "mei\u00b7ne", "Neu\u00b7be\u00b7gier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Von Ort zu Ort auf meiner Fahrt entdecket.", "tokens": ["Von", "Ort", "zu", "Ort", "auf", "mei\u00b7ner", "Fahrt", "ent\u00b7de\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So wie ich mich durch einen breiten Strom", "tokens": ["So", "wie", "ich", "mich", "durch", "ei\u00b7nen", "brei\u00b7ten", "Strom"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von wallendem Getreide durchgewunden,", "tokens": ["Von", "wal\u00b7len\u00b7dem", "Ge\u00b7trei\u00b7de", "durch\u00b7ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Stand Medling da, wo G\u00e4nse, wie zu Rom", "tokens": ["Stand", "Med\u00b7ling", "da", ",", "wo", "G\u00e4n\u00b7se", ",", "wie", "zu", "Rom"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "ADV", "$,", "PWAV", "NN", "$,", "PWAV", "APPR", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Kapitol, am Thore Wache stunden.", "tokens": ["Im", "Ka\u00b7pi\u00b7tol", ",", "am", "Tho\u00b7re", "Wa\u00b7che", "stun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von dannen gien'gs ganz sachte durch den Br\u00fchl,", "tokens": ["Von", "dan\u00b7nen", "gien'gs", "ganz", "sach\u00b7te", "durch", "den", "Br\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wo pl\u00f6tzlich links der Rest von \u00f6den Mauern", "tokens": ["Wo", "pl\u00f6tz\u00b7lich", "links", "der", "Rest", "von", "\u00f6\u00b7den", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auf einem Fels, zu dem man ohne Schauern", "tokens": ["Auf", "ei\u00b7nem", "Fels", ",", "zu", "dem", "man", "oh\u00b7ne", "Schau\u00b7ern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PRELS", "PIS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nicht aufsehn kann, mir in das Auge fiel.", "tokens": ["Nicht", "auf\u00b7sehn", "kann", ",", "mir", "in", "das", "Au\u00b7ge", "fiel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "$,", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Hier hatten einst in jenen Ritterzeiten,", "tokens": ["Hier", "hat\u00b7ten", "einst", "in", "je\u00b7nen", "Rit\u00b7ter\u00b7zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Als man bey uns Begier und Muth zu streiten", "tokens": ["Als", "man", "bey", "uns", "Be\u00b7gier", "und", "Muth", "zu", "strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "PPER", "NN", "KON", "NN", "PTKZU", "VVINF"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Noch h\u00f6her hielt als Wissenschaft und Witz,", "tokens": ["Noch", "h\u00f6\u00b7her", "hielt", "als", "Wis\u00b7sen\u00b7schaft", "und", "Witz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Viel Herzoge von \u00d6streich ihren Sitz.", "tokens": ["Viel", "Her\u00b7zo\u00b7ge", "von", "\u00d6s\u00b7treich", "ih\u00b7ren", "Sitz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun schl\u00e4ngelte die schmale Bahn sich mitten", "tokens": ["Nun", "schl\u00e4n\u00b7gel\u00b7te", "die", "schma\u00b7le", "Bahn", "sich", "mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PRF", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch Klippen fort und durch das frische Gr\u00fcn", "tokens": ["Durch", "Klip\u00b7pen", "fort", "und", "durch", "das", "fri\u00b7sche", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PTKVZ", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Wienerwalds, an B\u00e4chen, die mit H\u00fctten", "tokens": ["Des", "Wie\u00b7ner\u00b7walds", ",", "an", "B\u00e4\u00b7chen", ",", "die", "mit", "H\u00fct\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Umzingelt sind, bis zu dem Ziele hin.", "tokens": ["Um\u00b7zin\u00b7gelt", "sind", ",", "bis", "zu", "dem", "Zie\u00b7le", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "KOUS", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hier leb' ich nun so g\u00e4nzlich abgeschieden", "tokens": ["Hier", "leb'", "ich", "nun", "so", "g\u00e4nz\u00b7lich", "ab\u00b7ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von eurer Welt und ihren Plackereyn,", "tokens": ["Von", "eu\u00b7rer", "Welt", "und", "ih\u00b7ren", "Pla\u00b7cke\u00b7reyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dass ich nicht weiss, wie's ausser meinem Hain", "tokens": ["Dass", "ich", "nicht", "weiss", ",", "wie's", "aus\u00b7ser", "mei\u00b7nem", "Hain"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Indessen geht, ob Krieg ist oder Frieden.", "tokens": ["In\u00b7des\u00b7sen", "geht", ",", "ob", "Krieg", "ist", "o\u00b7der", "Frie\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "NN", "VAFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Heut morgens, Freund! als kaum die Sonne sich", "tokens": ["Heut", "mor\u00b7gens", ",", "Freund", "!", "als", "kaum", "die", "Son\u00b7ne", "sich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "NN", "$.", "KOUS", "ADV", "ART", "NN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Berg emp