{"textgrid.poem.43394": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr sprecht so sch\u00f6n vom Hohen, Magnifiken,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr sprecht so sch\u00f6n vom Hohen, Magnifiken,", "tokens": ["Ihr", "sprecht", "so", "sch\u00f6n", "vom", "Ho\u00b7hen", ",", "Mag\u00b7ni\u00b7fi\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr gebt so sch\u00f6n die geistlichen Musiken", "tokens": ["Ihr", "gebt", "so", "sch\u00f6n", "die", "geist\u00b7li\u00b7chen", "Mu\u00b7si\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Bach und Graun, von H\u00e4ndel, Mozart, Haydn,", "tokens": ["Von", "Bach", "und", "Graun", ",", "von", "H\u00e4n\u00b7del", ",", "Mo\u00b7zart", ",", "Haydn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$,", "APPR", "NN", "$,", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Engel selber m\u00fcssen euch beneiden.", "tokens": ["Die", "En\u00b7gel", "sel\u00b7ber", "m\u00fcs\u00b7sen", "euch", "be\u00b7nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ja, was ihr treibt ist Alles gro\u00df und pr\u00e4chtig \u2013", "tokens": ["Ja", ",", "was", "ihr", "treibt", "ist", "Al\u00b7les", "gro\u00df", "und", "pr\u00e4ch\u00b7tig", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWS", "PPER", "VVFIN", "VAFIN", "PIS", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ihr aber seid und bleibt so niedertr\u00e4chtig.", "tokens": ["Ihr", "a\u00b7ber", "seid", "und", "bleibt", "so", "nie\u00b7der\u00b7tr\u00e4ch\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr rei\u00dfet fort durch eure T\u00f6n' und Kl\u00e4nge", "tokens": ["Ihr", "rei\u00b7\u00dfet", "fort", "durch", "eu\u00b7re", "T\u00f6n'", "und", "Kl\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Kenner und die anspruchlose Menge,", "tokens": ["Die", "Ken\u00b7ner", "und", "die", "an\u00b7spruch\u00b7lo\u00b7se", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr schafft das Staunen und das dumme G\u00e4hnen", "tokens": ["Ihr", "schafft", "das", "Stau\u00b7nen", "und", "das", "dum\u00b7me", "G\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um in Begeistrung und in Freudenthr\u00e4nen.", "tokens": ["Um", "in", "Be\u00b7geis\u00b7trung", "und", "in", "Freu\u00b7dent\u00b7hr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ja, was ihr treibt ist alles gro\u00df und pr\u00e4chtig \u2013", "tokens": ["Ja", ",", "was", "ihr", "treibt", "ist", "al\u00b7les", "gro\u00df", "und", "pr\u00e4ch\u00b7tig", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWS", "PPER", "VVFIN", "VAFIN", "PIS", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ihr aber seid und bleibt so niedertr\u00e4chtig.", "tokens": ["Ihr", "a\u00b7ber", "seid", "und", "bleibt", "so", "nie\u00b7der\u00b7tr\u00e4ch\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr zeigt Gef\u00fchl bis in das Allerkleinste", "tokens": ["Ihr", "zeigt", "Ge\u00b7f\u00fchl", "bis", "in", "das", "Al\u00b7ler\u00b7kleins\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "APPR", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und die Begeistrung selbst die w\u00e4rmste, reinste;", "tokens": ["Und", "die", "Be\u00b7geis\u00b7trung", "selbst", "die", "w\u00e4rms\u00b7te", ",", "reins\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bei jedem Takt, bei jeder Paus' und Note", "tokens": ["Bei", "je\u00b7dem", "Takt", ",", "bei", "je\u00b7der", "Paus'", "und", "No\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Thut ihr so heilig wie ein Friedensbote.", "tokens": ["Thut", "ihr", "so", "hei\u00b7lig", "wie", "ein", "Frie\u00b7dens\u00b7bo\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ja, was ihr treibt ist Alles gro\u00df und pr\u00e4chtig \u2013", "tokens": ["Ja", ",", "was", "ihr", "treibt", "ist", "Al\u00b7les", "gro\u00df", "und", "pr\u00e4ch\u00b7tig", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWS", "PPER", "VVFIN", "VAFIN", "PIS", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ihr aber bleibt doch ewig niedertr\u00e4chtig.", "tokens": ["Ihr", "a\u00b7ber", "bleibt", "doch", "e\u00b7wig", "nie\u00b7der\u00b7tr\u00e4ch\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}