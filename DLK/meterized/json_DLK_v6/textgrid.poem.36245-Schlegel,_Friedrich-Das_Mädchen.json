{"textgrid.poem.36245": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das M\u00e4dchen", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie so innig, m\u00f6cht ich sagen,", "tokens": ["Wie", "so", "in\u00b7nig", ",", "m\u00f6cht", "ich", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich der Meine mir ergibt,", "tokens": ["Sich", "der", "Mei\u00b7ne", "mir", "er\u00b7gibt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um zu lindern meine Klagen,", "tokens": ["Um", "zu", "lin\u00b7dern", "mei\u00b7ne", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er nicht so innig liebt.", "tokens": ["Da\u00df", "er", "nicht", "so", "in\u00b7nig", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Will ich's sagen, so entschwebt es;", "tokens": ["Will", "ich's", "sa\u00b7gen", ",", "so", "ent\u00b7schwebt", "es", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4ren T\u00f6ne mir verliehen,", "tokens": ["W\u00e4\u00b7ren", "T\u00f6\u00b7ne", "mir", "ver\u00b7lie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fl\u00f6ss' es hin in Harmonien,", "tokens": ["Fl\u00f6ss'", "es", "hin", "in", "Har\u00b7mo\u00b7ni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn in jenen T\u00f6nen lebt es.", "tokens": ["Denn", "in", "je\u00b7nen", "T\u00f6\u00b7nen", "lebt", "es", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur die Nachtigall kann sagen,", "tokens": ["Nur", "die", "Nach\u00b7ti\u00b7gall", "kann", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie er innig sich mir gibt,", "tokens": ["Wie", "er", "in\u00b7nig", "sich", "mir", "gibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PRF", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Um zu lindern meine Klagen,", "tokens": ["Um", "zu", "lin\u00b7dern", "mei\u00b7ne", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df er nicht so innig liebt.", "tokens": ["Da\u00df", "er", "nicht", "so", "in\u00b7nig", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}