{"textgrid.poem.67351": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An den Schlaf", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Schlummer, sanfter Sohn der schattenreichen,", "tokens": ["O", "Schlum\u00b7mer", ",", "sanf\u00b7ter", "Sohn", "der", "schat\u00b7ten\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Thauenden Nacht, der armen Menschen Zuflucht,", "tokens": ["Thau\u00b7en\u00b7den", "Nacht", ",", "der", "ar\u00b7men", "Men\u00b7schen", "Zu\u00b7flucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ein s\u00fc\u00df Vergessen aller, aller Uebel,", "tokens": ["Ein", "s\u00fc\u00df", "Ver\u00b7ges\u00b7sen", "al\u00b7ler", ",", "al\u00b7ler", "Ue\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die, ach, so schwer, so hart das Leben dr\u00fccken,", "tokens": ["Die", ",", "ach", ",", "so", "schwer", ",", "so", "hart", "das", "Le\u00b7ben", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ITJ", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Komm endlich, komm und gieb dem schmachtend matten,", "tokens": ["Komm", "end\u00b7lich", ",", "komm", "und", "gieb", "dem", "schmach\u00b7tend", "mat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "KON", "VVIMP", "ART", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ruhlosen Herzen Ruhe! diese Glieder,", "tokens": ["Ruh\u00b7lo\u00b7sen", "Her\u00b7zen", "Ru\u00b7he", "!", "die\u00b7se", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$.", "PDAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "So schwach, so welk, erquicke sie und breite,", "tokens": ["So", "schwach", ",", "so", "welk", ",", "er\u00b7qui\u00b7cke", "sie", "und", "brei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "VVFIN", "PPER", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O Schlummer, \u00fcber mich die braunen Schwingen!", "tokens": ["O", "Schlum\u00b7mer", ",", "\u00fc\u00b7ber", "mich", "die", "brau\u00b7nen", "Schwin\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wo ist das Schweigen, das vor Licht und Tage", "tokens": ["Wo", "ist", "das", "Schwei\u00b7gen", ",", "das", "vor", "Licht", "und", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So furchtsam fliehet? wo die leichten Tr\u00e4ume,", "tokens": ["So", "furcht\u00b7sam", "flie\u00b7het", "?", "wo", "die", "leich\u00b7ten", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$.", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die sonst mit gaukelnd ungesporntem Tritte", "tokens": ["Die", "sonst", "mit", "gau\u00b7kelnd", "un\u00b7ge\u00b7sporn\u00b7tem", "Trit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So bald, so gerne Dir zu folgen pflegen?", "tokens": ["So", "bald", ",", "so", "ger\u00b7ne", "Dir", "zu", "fol\u00b7gen", "pfle\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Vergebens ruf' ich Dir, vergebens winsle", "tokens": ["Ver\u00b7ge\u00b7bens", "ruf'", "ich", "Dir", ",", "ver\u00b7ge\u00b7bens", "wins\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Elender Ich Euch vor, Ihr schwarzen, kalten,", "tokens": ["E\u00b7len\u00b7der", "Ich", "Euch", "vor", ",", "Ihr", "schwar\u00b7zen", ",", "kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PTKVZ", "$,", "PPER", "VVINF", "$,", "ADJA", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Trostlosen Schatten. O der harten Flaume!", "tokens": ["Trost\u00b7lo\u00b7sen", "Schat\u00b7ten", ".", "O", "der", "har\u00b7ten", "Flau\u00b7me", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NE", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und o der herben, bittern langen N\u00e4chte!", "tokens": ["Und", "o", "der", "her\u00b7ben", ",", "bit\u00b7tern", "lan\u00b7gen", "N\u00e4ch\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "FM", "ART", "ADJA", "$,", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}