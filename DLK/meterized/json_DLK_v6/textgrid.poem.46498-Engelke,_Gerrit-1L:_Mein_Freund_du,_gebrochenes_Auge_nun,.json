{"textgrid.poem.46498": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Freund du, gebrochenes Auge nun,", "genre": "verse", "period": "N.A.", "pub_year": 1917, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Freund du, gebrochenes Auge nun,", "tokens": ["Mein", "Freund", "du", ",", "ge\u00b7bro\u00b7che\u00b7nes", "Au\u00b7ge", "nun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$,", "ADJA", "NN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Gebrochener Blick wie der des erschossenen Hasen", "tokens": ["Ge\u00b7bro\u00b7che\u00b7ner", "Blick", "wie", "der", "des", "er\u00b7schos\u00b7se\u00b7nen", "Ha\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KOKOM", "ART", "ART", "ADJA", "NN"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Oder ver\u00e4chtlichen, kalten Verr\u00e4ters \u2013", "tokens": ["O\u00b7der", "ver\u00b7\u00e4cht\u00b7li\u00b7chen", ",", "kal\u00b7ten", "Ver\u00b7r\u00e4\u00b7ters", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "ADJA", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Zw\u00f6lf Jahre gemeinsam sprang uns der Zeitwind entgegen,", "tokens": ["Zw\u00f6lf", "Jah\u00b7re", "ge\u00b7mein\u00b7sam", "sprang", "uns", "der", "Zeit\u00b7wind", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Schweigsam teilten wir B\u00fccher und Brot,", "tokens": ["Schweig\u00b7sam", "teil\u00b7ten", "wir", "B\u00fc\u00b7cher", "und", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Teilten im Schulhaus die B\u00e4nke,", "tokens": ["Teil\u00b7ten", "im", "Schul\u00b7haus", "die", "B\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Des Lebenshindranges rauschende Not,", "tokens": ["Des", "Le\u00b7bens\u00b7hin\u00b7dran\u00b7ges", "rau\u00b7schen\u00b7de", "Not", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Einigen Sinnes Erkennung und Lehre,", "tokens": ["Ei\u00b7ni\u00b7gen", "Sin\u00b7nes", "Er\u00b7ken\u00b7nung", "und", "Leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Freund, dein Auge ist tot.", "tokens": ["Freund", ",", "dein", "Au\u00b7ge", "ist", "tot", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Darum deine Mutter im Kummer nun geht,", "tokens": ["Da\u00b7rum", "dei\u00b7ne", "Mut\u00b7ter", "im", "Kum\u00b7mer", "nun", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Harmvoll, seufzend, doch schlicht in der Menge,", "tokens": ["Harm\u00b7voll", ",", "seuf\u00b7zend", ",", "doch", "schlicht", "in", "der", "Men\u00b7ge", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVPP", "$,", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Darum Klein-Schwester, Klein-Br\u00fcder zu fr\u00fche schon sp\u00fcren", "tokens": ["Da\u00b7rum", "Klein\u00b7Schwes\u00b7ter", ",", "Klein\u00b7Br\u00fc\u00b7der", "zu", "fr\u00fc\u00b7he", "schon", "sp\u00fc\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "NN", "$,", "NN", "APPR", "ADJA", "ADV", "VVINF"], "meter": "---+--+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Verfinsternd qualmendes Schicksalgewitter", "tokens": ["Ver\u00b7fins\u00b7ternd", "qual\u00b7men\u00b7des", "Schick\u00b7sal\u00b7ge\u00b7wit\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und m\u00e4chtiges M\u00e4hen des Todes.", "tokens": ["Und", "m\u00e4ch\u00b7ti\u00b7ges", "M\u00e4\u00b7hen", "des", "To\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Leer ist dein Bett in der \u00e4rmlichen Kammer", "tokens": ["Leer", "ist", "dein", "Bett", "in", "der", "\u00e4rm\u00b7li\u00b7chen", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.7": {"text": "Und dein Platz am Tische des Mittags.", "tokens": ["Und", "dein", "Platz", "am", "Ti\u00b7sche", "des", "Mit\u00b7tags", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und darum, da\u00df niemand mehr wartet auf dich,", "tokens": ["Und", "da\u00b7rum", ",", "da\u00df", "nie\u00b7mand", "mehr", "war\u00b7tet", "auf", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "$,", "KOUS", "PIS", "ADV", "VVFIN", "APPR", "PPER", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Geht grau deine Mutter im Kummer.", "tokens": ["Geht", "grau", "dei\u00b7ne", "Mut\u00b7ter", "im", "Kum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Du w\u00e4rst eine Wurzel, ein Saatkorn,", "tokens": ["Du", "w\u00e4rst", "ei\u00b7ne", "Wur\u00b7zel", ",", "ein", "Saat\u00b7korn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ein trotzender Keim in den Furchen des Lebens,", "tokens": ["Ein", "trot\u00b7zen\u00b7der", "Keim", "in", "den", "Fur\u00b7chen", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ein b\u00e4rtiger Vater von freundlichen Kindern geworden.", "tokens": ["Ein", "b\u00e4r\u00b7ti\u00b7ger", "Va\u00b7ter", "von", "freund\u00b7li\u00b7chen", "Kin\u00b7dern", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VAPP", "$."], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.4": {"text": "Ein schmerzenzerpfl\u00fcgtes Ackerland fra\u00df dich,", "tokens": ["Ein", "schmer\u00b7zen\u00b7zer\u00b7pfl\u00fcg\u00b7tes", "A\u00b7cker\u00b7land", "fra\u00df", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ein blutbed\u00fcngter Acker verdarb dich,", "tokens": ["Ein", "blut\u00b7be\u00b7d\u00fcng\u00b7ter", "A\u00b7cker", "ver\u00b7darb", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der weise und ewige S\u00e4er zertrat dich.", "tokens": ["Der", "wei\u00b7se", "und", "e\u00b7wi\u00b7ge", "S\u00e4\u00b7er", "zer\u00b7trat", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Wer hadert und redet von Schuld?", "tokens": ["Wer", "ha\u00b7dert", "und", "re\u00b7det", "von", "Schuld", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Doch w\u00e4rst du ein Saatkorn und w\u00e4rest ein Vater!", "tokens": ["Doch", "w\u00e4rst", "du", "ein", "Saat\u00b7korn", "und", "w\u00e4\u00b7rest", "ein", "Va\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "KON", "VAFIN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Du w\u00e4rest das Saatkorn \u2013 und wurdest doch Opfer;", "tokens": ["Du", "w\u00e4\u00b7rest", "das", "Saat\u00b7korn", "\u2013", "und", "wur\u00b7dest", "doch", "Op\u00b7fer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "KON", "VAFIN", "ADV", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ein tausendstel Gramm nur, ein blutendes Fleisch", "tokens": ["Ein", "tau\u00b7send\u00b7stel", "Gramm", "nur", ",", "ein", "blu\u00b7ten\u00b7des", "Fleisch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Fielst du auf blutleerer Leichen unendlich Gebirge.", "tokens": ["Fielst", "du", "auf", "blut\u00b7lee\u00b7rer", "Lei\u00b7chen", "un\u00b7end\u00b7lich", "Ge\u00b7bir\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "ADJD", "NN", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.4": {"text": "Ist auch dein Tod nicht mehr denn ein anderer Tod.", "tokens": ["Ist", "auch", "dein", "Tod", "nicht", "mehr", "denn", "ein", "an\u00b7de\u00b7rer", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Marschierten doch Tausend und Tausende rhythmischen Schrittes", "tokens": ["Mar\u00b7schier\u00b7ten", "doch", "Tau\u00b7send", "und", "Tau\u00b7sen\u00b7de", "rhyth\u00b7mi\u00b7schen", "Schrit\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "CARD", "KON", "NN", "ADJA", "NN"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.6": {"text": "Hinweg in das qualschwarze Nichts,", "tokens": ["Hin\u00b7weg", "in", "das", "qual\u00b7schwar\u00b7ze", "Nichts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Regiment und Brigade, Armee und Armeen", "tokens": ["Re\u00b7gi\u00b7ment", "und", "Bri\u00b7ga\u00b7de", ",", "Ar\u00b7mee", "und", "Ar\u00b7me\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Ins blutigbefleckte Ruhm-Reich des toten Soldaten.", "tokens": ["Ins", "blu\u00b7tig\u00b7be\u00b7fleck\u00b7te", "Ruhm\u00b7Reich", "des", "to\u00b7ten", "Sol\u00b7da\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Du wurdest ein Opfer.", "tokens": ["Du", "wur\u00b7dest", "ein", "Op\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Der Brimont ist kahl und sein Wald ist zerschroten,", "tokens": ["Der", "Bri\u00b7mont", "ist", "kahl", "und", "sein", "Wald", "ist", "zer\u00b7schro\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Keine Fichte verschont, dir daraus ein Grabkreuz zu schlagen.", "tokens": ["Kei\u00b7ne", "Fich\u00b7te", "ver\u00b7schont", ",", "dir", "da\u00b7raus", "ein", "Grab\u00b7kreuz", "zu", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,", "PPER", "PAV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.3": {"text": "So liegst du stumm in zertr\u00fcmmertem Boden,", "tokens": ["So", "liegst", "du", "stumm", "in", "zer\u00b7tr\u00fcm\u00b7mer\u00b7tem", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In brustbedr\u00fcckendem, traumlosen Schlummer.", "tokens": ["In", "brust\u00b7be\u00b7dr\u00fc\u00b7cken\u00b7dem", ",", "traum\u00b7lo\u00b7sen", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nicht Held, noch F\u00fchrer \u2013 Soldat nur, unbekannt.", "tokens": ["Nicht", "Held", ",", "noch", "F\u00fch\u00b7rer", "\u2013", "Sol\u00b7dat", "nur", ",", "un\u00b7be\u00b7kannt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "ADV", "NN", "$(", "NN", "ADV", "$,", "ADJD", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Gebein im Wind der Verwesung.", "tokens": ["Ge\u00b7bein", "im", "Wind", "der", "Ver\u00b7we\u00b7sung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Doch des gewaltigen Friedens unz\u00e4hlbare, selige Glanzlegionen,", "tokens": ["Doch", "des", "ge\u00b7wal\u00b7ti\u00b7gen", "Frie\u00b7dens", "un\u00b7z\u00e4hl\u00b7ba\u00b7re", ",", "se\u00b7li\u00b7ge", "Glanz\u00b7le\u00b7gi\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-++--+--+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.8": {"text": "Wenn ehern und klirrend sie \u00fcber dein Grabfeld marschieren,", "tokens": ["Wenn", "e\u00b7hern", "und", "klir\u00b7rend", "sie", "\u00fc\u00b7ber", "dein", "Grab\u00b7feld", "mar\u00b7schie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "KON", "ADJD", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.9": {"text": "Wirst du erschauernd einst h\u00f6ren,", "tokens": ["Wirst", "du", "er\u00b7schau\u00b7ernd", "einst", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ADV", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.10": {"text": "So horche und harre darauf.", "tokens": ["So", "hor\u00b7che", "und", "har\u00b7re", "da\u00b7rauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "KON", "VVFIN", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}