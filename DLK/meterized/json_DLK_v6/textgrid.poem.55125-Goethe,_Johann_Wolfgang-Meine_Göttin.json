{"textgrid.poem.55125": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Meine G\u00f6ttin", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welcher Unsterblichen", "tokens": ["Wel\u00b7cher", "U\u00b7nsterb\u00b7li\u00b7chen"], "token_info": ["word", "word"], "pos": ["PWAT", "NN"], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.2": {"text": "Soll der h\u00f6chste Preis sein'?", "tokens": ["Soll", "der", "h\u00f6chs\u00b7te", "Preis", "sein'", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mit niemand streit ich,", "tokens": ["Mit", "nie\u00b7mand", "streit", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Aber ich geb ihn", "tokens": ["A\u00b7ber", "ich", "geb", "ihn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER"], "meter": "+-++-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Der ewig beweglichen,", "tokens": ["Der", "e\u00b7wig", "be\u00b7weg\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.6": {"text": "Immer neuen,", "tokens": ["Im\u00b7mer", "neu\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJA", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Seltsamen Tochter Jovis,", "tokens": ["Selt\u00b7sa\u00b7men", "Toch\u00b7ter", "Jo\u00b7vis", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "Seinem Scho\u00dfkinde,", "tokens": ["Sei\u00b7nem", "Scho\u00df\u00b7kin\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.9": {"text": "Der Phantasie.", "tokens": ["Der", "Phan\u00b7ta\u00b7sie", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Denn ihr hat er", "tokens": ["Denn", "ihr", "hat", "er"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Alle Launen,", "tokens": ["Al\u00b7le", "Lau\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Die er sonst nur allein", "tokens": ["Die", "er", "sonst", "nur", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "ADV"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "Sich vorbeh\u00e4lt,", "tokens": ["Sich", "vor\u00b7be\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Zugestanden", "tokens": ["Zu\u00b7ge\u00b7stan\u00b7den"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Und hat seine Freude", "tokens": ["Und", "hat", "sei\u00b7ne", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "An der T\u00f6rin.", "tokens": ["An", "der", "T\u00f6\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Sie mag rosenbekr\u00e4nzt", "tokens": ["Sie", "mag", "ro\u00b7sen\u00b7be\u00b7kr\u00e4nzt"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Mit dem Lilienstengel", "tokens": ["Mit", "dem", "Li\u00b7li\u00b7ens\u00b7ten\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blument\u00e4ler betreten,", "tokens": ["Blu\u00b7men\u00b7t\u00e4\u00b7ler", "be\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Sommerv\u00f6geln gebieten", "tokens": ["Som\u00b7mer\u00b7v\u00f6\u00b7geln", "ge\u00b7bie\u00b7ten"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Und leichtn\u00e4hrenden Tau", "tokens": ["Und", "leicht\u00b7n\u00e4h\u00b7ren\u00b7den", "Tau"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Mit Bienenlippen", "tokens": ["Mit", "Bie\u00b7nen\u00b7lip\u00b7pen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Von Bl\u00fcten saugen,", "tokens": ["Von", "Bl\u00fc\u00b7ten", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Oder sie mag", "tokens": ["O\u00b7der", "sie", "mag"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VMFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Mit fliegendem Haar", "tokens": ["Mit", "flie\u00b7gen\u00b7dem", "Haar"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mit d\u00fcsterm Blicke", "tokens": ["Mit", "d\u00fcs\u00b7term", "Bli\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Im Winde sausen", "tokens": ["Im", "Win\u00b7de", "sau\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Um Felsenw\u00e4nde", "tokens": ["Um", "Fel\u00b7sen\u00b7w\u00e4n\u00b7de"], "token_info": ["word", "word"], "pos": ["KOUI", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Und tausendfarbig,", "tokens": ["Und", "tau\u00b7send\u00b7far\u00b7big", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Wie Morgen und Abend,", "tokens": ["Wie", "Mor\u00b7gen", "und", "A\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Immer wechselnd,", "tokens": ["Im\u00b7mer", "wech\u00b7selnd", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Wie Mondesblicke,", "tokens": ["Wie", "Mon\u00b7des\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Den Sterblichen scheinen.", "tokens": ["Den", "Sterb\u00b7li\u00b7chen", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "La\u00dft uns alle", "tokens": ["La\u00dft", "uns", "al\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "PIAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Den Vater preisen!", "tokens": ["Den", "Va\u00b7ter", "prei\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Den alten, hohen,", "tokens": ["Den", "al\u00b7ten", ",", "ho\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der solch eine sch\u00f6ne", "tokens": ["Der", "solch", "ei\u00b7ne", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Unverwelkliche Gattin", "tokens": ["Un\u00b7ver\u00b7wel\u00b7kli\u00b7che", "Gat\u00b7tin"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Dem sterblichen Menschen", "tokens": ["Dem", "sterb\u00b7li\u00b7chen", "Men\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Gesellen m\u00f6gen!", "tokens": ["Ge\u00b7sel\u00b7len", "m\u00f6\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VMFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Denn uns allein", "tokens": ["Denn", "uns", "al\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Hat er sie verbunden", "tokens": ["Hat", "er", "sie", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mit Himmelsband", "tokens": ["Mit", "Him\u00b7mels\u00b7band"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und ihr geboten,", "tokens": ["Und", "ihr", "ge\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "In Freud und Elend", "tokens": ["In", "Freud", "und", "E\u00b7lend"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Als treue Gattin", "tokens": ["Als", "treu\u00b7e", "Gat\u00b7tin"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Nicht zu entweichen.", "tokens": ["Nicht", "zu", "ent\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Alle die andern", "tokens": ["Al\u00b7le", "die", "an\u00b7dern"], "token_info": ["word", "word", "word"], "pos": ["PIS", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Armen Geschlechter", "tokens": ["Ar\u00b7men", "Ge\u00b7schlech\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der kinderreichen", "tokens": ["Der", "kin\u00b7der\u00b7rei\u00b7chen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Lebendigen Erde", "tokens": ["Le\u00b7ben\u00b7di\u00b7gen", "Er\u00b7de"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wandeln und weiden", "tokens": ["Wan\u00b7deln", "und", "wei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "In dunkelm Genu\u00df", "tokens": ["In", "dun\u00b7kelm", "Ge\u00b7nu\u00df"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und tr\u00fcben Schmerzen", "tokens": ["Und", "tr\u00fc\u00b7ben", "Schmer\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Des augenblicklichen,", "tokens": ["Des", "au\u00b7gen\u00b7blick\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.9": {"text": "Beschr\u00e4nkten Lebens,", "tokens": ["Be\u00b7schr\u00e4nk\u00b7ten", "Le\u00b7bens", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Gebeugt vom Joche", "tokens": ["Ge\u00b7beugt", "vom", "Jo\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Der Notdurft.", "tokens": ["Der", "Not\u00b7durft", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.8": {"line.1": {"text": "Uns aber hat er", "tokens": ["Uns", "a\u00b7ber", "hat", "er"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Seine gewandteste,", "tokens": ["Sei\u00b7ne", "ge\u00b7wand\u00b7tes\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Verz\u00e4rtelte Tochter,", "tokens": ["Ver\u00b7z\u00e4r\u00b7tel\u00b7te", "Toch\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Freut euch! geg\u00f6nnt.", "tokens": ["Freut", "euch", "!", "ge\u00b7g\u00f6nnt", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Begegnet ihr lieblich,", "tokens": ["Be\u00b7geg\u00b7net", "ihr", "lieb\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wie einer Geliebten!", "tokens": ["Wie", "ei\u00b7ner", "Ge\u00b7lieb\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "La\u00dft ihr die W\u00fcrde", "tokens": ["La\u00dft", "ihr", "die", "W\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Der Frauen im Haus!", "tokens": ["Der", "Frau\u00b7en", "im", "Haus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Und da\u00df die alte", "tokens": ["Und", "da\u00df", "die", "al\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Schwiegermutter Weisheit", "tokens": ["Schwie\u00b7ger\u00b7mut\u00b7ter", "Weis\u00b7heit"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Das zarte Seelchen", "tokens": ["Das", "zar\u00b7te", "Seel\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ja nicht beleid'ge!", "tokens": ["Ja", "nicht", "be\u00b7lei\u00b7d'\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKANT", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Doch kenn ich ihre Schwester,", "tokens": ["Doch", "kenn", "ich", "ih\u00b7re", "Schwes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die \u00e4ltere, gesetztere,", "tokens": ["Die", "\u00e4l\u00b7te\u00b7re", ",", "ge\u00b7setz\u00b7te\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Meine stille Freundin:", "tokens": ["Mei\u00b7ne", "stil\u00b7le", "Freun\u00b7din", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "O da\u00df die erst", "tokens": ["O", "da\u00df", "die", "erst"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KOUS", "ART", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Mit dem Lichte des Lebens", "tokens": ["Mit", "dem", "Lich\u00b7te", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sich von mir wende,", "tokens": ["Sich", "von", "mir", "wen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die edle Treiberin,", "tokens": ["Die", "ed\u00b7le", "Trei\u00b7be\u00b7rin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Tr\u00f6sterin Hoffnung!", "tokens": ["Tr\u00f6s\u00b7te\u00b7rin", "Hoff\u00b7nung", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}