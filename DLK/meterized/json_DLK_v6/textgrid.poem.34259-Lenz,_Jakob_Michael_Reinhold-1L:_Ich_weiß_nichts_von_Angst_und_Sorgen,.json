{"textgrid.poem.34259": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wei\u00df nichts von Angst und Sorgen,", "genre": "verse", "period": "N.A.", "pub_year": 1768, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nichts von Angst und Sorgen,", "tokens": ["Ich", "wei\u00df", "nichts", "von", "Angst", "und", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Denn, erwach' ich jeden Morgen,", "tokens": ["Denn", ",", "er\u00b7wach'", "ich", "je\u00b7den", "Mor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seh' ich, da\u00df mein Gott noch lebt,", "tokens": ["Seh'", "ich", ",", "da\u00df", "mein", "Gott", "noch", "lebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der die ganze Welt belebt.", "tokens": ["Der", "die", "gan\u00b7ze", "Welt", "be\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dem hab' ich mich \u00fcbergeben,", "tokens": ["Dem", "hab'", "ich", "mich", "\u00fc\u00b7ber\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Er mag auf mich Achtung geben,", "tokens": ["Er", "mag", "auf", "mich", "Ach\u00b7tung", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er ist Vater, ich das Kind,", "tokens": ["Er", "ist", "Va\u00b7ter", ",", "ich", "das", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinem Vater folg' ich blind.", "tokens": ["Mei\u00b7nem", "Va\u00b7ter", "fol\u00b7g'", "ich", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Ich bins so gewohnt von Langem,", "tokens": ["Ich", "bins", "so", "ge\u00b7wohnt", "von", "Lan\u00b7gem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Unverr\u00fcckt an ihm zu hangen.", "tokens": ["Un\u00b7ver\u00b7r\u00fcckt", "an", "ihm", "zu", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ich bin, da ist auch er,", "tokens": ["Wo", "ich", "bin", ",", "da", "ist", "auch", "er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "$,", "ADV", "VAFIN", "ADV", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn es auch bei'm Teufel w\u00e4r'.", "tokens": ["Wenn", "es", "auch", "bei'm", "Teu\u00b7fel", "w\u00e4r'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Toben St\u00fcrme, Ungl\u00fccks-Wellen,", "tokens": ["To\u00b7ben", "St\u00fcr\u00b7me", ",", "Un\u00b7gl\u00fccks\u00b7Wel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die Feinde noch so bellen,", "tokens": ["Wenn", "die", "Fein\u00b7de", "noch", "so", "bel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bin ich ruhig, denn mein Gott", "tokens": ["Bin", "ich", "ru\u00b7hig", ",", "denn", "mein", "Gott"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Half mir noch aus aller Noth.", "tokens": ["Half", "mir", "noch", "aus", "al\u00b7ler", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wenn auch die Noth am gr\u00f6\u00dften,", "tokens": ["Und", "wenn", "auch", "die", "Noth", "am", "gr\u00f6\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "APPRART", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Eben recht, so dient's am besten:", "tokens": ["E\u00b7ben", "recht", ",", "so", "dient's", "am", "bes\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADV", "PTKA", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die Wege wunderlich,", "tokens": ["Wenn", "die", "We\u00b7ge", "wun\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gehn sie immer seliglich.", "tokens": ["Gehn", "sie", "im\u00b7mer", "se\u00b7lig\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn du willst an Ihm verzagen,", "tokens": ["Wenn", "du", "willst", "an", "Ihm", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich mit eitlen Sorgen plagen,", "tokens": ["Dich", "mit", "eit\u00b7len", "Sor\u00b7gen", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ei so sag' nicht, da\u00df du bist", "tokens": ["Ei", "so", "sag'", "nicht", ",", "da\u00df", "du", "bist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gotteskind, ein wahrer Christ.", "tokens": ["Got\u00b7tes\u00b7kind", ",", "ein", "wah\u00b7rer", "Christ", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Der aus Nichts die Welten machte", "tokens": ["Der", "aus", "Nichts", "die", "Wel\u00b7ten", "mach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PIS", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unser Gott im Himmel sagte:", "tokens": ["Un\u00b7ser", "Gott", "im", "Him\u00b7mel", "sag\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruf' mich an, so f\u00fchr' ich dich,", "tokens": ["Ruf'", "mich", "an", ",", "so", "f\u00fchr'", "ich", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Helf' dir, und errette dich.", "tokens": ["Hel\u00b7f'", "dir", ",", "und", "er\u00b7ret\u00b7te", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Gott hat Jesum uns gegeben,", "tokens": ["Gott", "hat", "Je\u00b7sum", "uns", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df wir m\u00f6chten durch Ihn leben:", "tokens": ["Da\u00df", "wir", "m\u00f6ch\u00b7ten", "durch", "Ihn", "le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jesum, Seinen lieben Sohn,", "tokens": ["Je\u00b7sum", ",", "Sei\u00b7nen", "lie\u00b7ben", "Sohn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sandte Er vom Himmelsthron.", "tokens": ["Sand\u00b7te", "Er", "vom", "Him\u00b7melst\u00b7hron", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Er ist unser F\u00fcrst geworden,", "tokens": ["Er", "ist", "un\u00b7ser", "F\u00fcrst", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VAPP", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Er soll helfen aller Orten,", "tokens": ["Er", "soll", "hel\u00b7fen", "al\u00b7ler", "Or\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denen, die sich Seiner freu'n,", "tokens": ["De\u00b7nen", ",", "die", "sich", "Sei\u00b7ner", "freu'n", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PRF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ihr Herz der Liebe weih'n.", "tokens": ["Und", "ihr", "Herz", "der", "Lie\u00b7be", "weih'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wird denn Der dich lassen sterben,", "tokens": ["Wird", "denn", "Der", "dich", "las\u00b7sen", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "PPER", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der dich hat gesetzt zum Erben?", "tokens": ["Der", "dich", "hat", "ge\u00b7setzt", "zum", "Er\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "VVPP", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der f\u00fcr dich geschmeckt den Tod?", "tokens": ["Der", "f\u00fcr", "dich", "ge\u00b7schmeckt", "den", "Tod", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVPP", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gott bleibt immer Gott, dein Gott!", "tokens": ["Gott", "bleibt", "im\u00b7mer", "Gott", ",", "dein", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Hoffe nun, steh' fest im Glauben,", "tokens": ["Hof\u00b7fe", "nun", ",", "steh'", "fest", "im", "Glau\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df dir nichts die Hoffnung rauben;", "tokens": ["La\u00df", "dir", "nichts", "die", "Hoff\u00b7nung", "rau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lie\u00dfe dich dein F\u00fcrst in Noth,", "tokens": ["Lie\u00b7\u00dfe", "dich", "dein", "F\u00fcrst", "in", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcrd' Er selbst der Feinde Spott.", "tokens": ["W\u00fcrd'", "Er", "selbst", "der", "Fein\u00b7de", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}