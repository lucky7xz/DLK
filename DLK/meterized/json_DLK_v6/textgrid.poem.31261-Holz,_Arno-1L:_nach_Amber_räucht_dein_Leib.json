{"textgrid.poem.31261": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: nach Amber r\u00e4ucht dein Leib/", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "nach Amber r\u00e4ucht dein Leib/", "tokens": ["nach", "Am\u00b7ber", "r\u00e4ucht", "dein", "Leib", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "gantz Gold sind deine Hahre;", "tokens": ["gantz", "Gold", "sind", "dei\u00b7ne", "Hah\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "doch da\u00df zu gleicher Frist", "tokens": ["doch", "da\u00df", "zu", "glei\u00b7cher", "Frist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "dein Hertz ein Agtstein ist/", "tokens": ["dein", "Hertz", "ein", "Agt\u00b7stein", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "bringt mich noch auff die Bahre.", "tokens": ["bringt", "mich", "noch", "auff", "die", "Bah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Stets hengt dir wo am Rokk", "tokens": ["Stets", "hengt", "dir", "wo", "am", "Rokk"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PWAV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "dein Mann/ der K\u00fclster-Bokk/", "tokens": ["dein", "Mann", "/", "der", "K\u00fcls\u00b7ter\u00b7Bokk", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "der alte Krippen-S\u00f6zzer;", "tokens": ["der", "al\u00b7te", "Krip\u00b7pen\u00b7S\u00f6z\u00b7zer", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "statt Alicanten-Wein", "tokens": ["statt", "A\u00b7li\u00b7can\u00b7ten\u00b7Wein"], "token_info": ["word", "word"], "pos": ["APPR", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "druppt er dir Wermuth ein/", "tokens": ["druppt", "er", "dir", "Wer\u00b7muth", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "NN", "ART", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "pfy Teiffel/ so ein Kr\u00f6zzer!", "tokens": ["pfy", "Teif\u00b7fel", "/", "so", "ein", "Kr\u00f6z\u00b7zer", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zu Kuchen und Koffee", "tokens": ["Zu", "Ku\u00b7chen", "und", "Kof\u00b7fee"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "auff deinem Canape", "tokens": ["auff", "dei\u00b7nem", "Ca\u00b7na\u00b7pe"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "dreff ich ihn d\u00e4glich sizzen/", "tokens": ["dreff", "ich", "ihn", "d\u00e4g\u00b7lich", "siz\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "dein H\u00e4ndgen stopfft Conf\u00e4kkt", "tokens": ["dein", "H\u00e4nd\u00b7gen", "stopfft", "Con\u00b7f\u00e4kkt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "und wa\u00df mir sonst noch schm\u00e4kkt", "tokens": ["und", "wa\u00df", "mir", "sonst", "noch", "schm\u00e4kkt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "in alle seine Rizzen.", "tokens": ["in", "al\u00b7le", "sei\u00b7ne", "Riz\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hohl-Hippckens/ Hindbeer-Schm\u00e4ltz", "tokens": ["Hohl\u00b7Hipp\u00b7ckens", "/", "Hin\u00b7dbeer\u00b7Schm\u00e4ltz"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$(", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "schl\u00e4kkt er sich in den B\u00e4ltz", "tokens": ["schl\u00e4kkt", "er", "sich", "in", "den", "B\u00e4ltz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "zu einer Plunder-Br\u00e4htzel;", "tokens": ["zu", "ei\u00b7ner", "Plun\u00b7der\u00b7Br\u00e4ht\u00b7zel", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "worhin er das bloh\u00df dreibt/", "tokens": ["wor\u00b7hin", "er", "das", "bloh\u00df", "dreibt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PDS", "ADV", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "da\u00df ist for mir und bleibt", "tokens": ["da\u00df", "ist", "for", "mir", "und", "bleibt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "APPR", "PPER", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "durchau\u00df ein schwartzes R\u00e4htsel.", "tokens": ["durch\u00b7au\u00df", "ein", "schwart\u00b7zes", "R\u00e4ht\u00b7sel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Streu-Strihtzel/ Mandel-Klein/", "tokens": ["Streu\u00b7Striht\u00b7zel", "/", "Man\u00b7del\u00b7Klein", "/"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$(", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "da\u00df mu\u00df ihm alles rein/", "tokens": ["da\u00df", "mu\u00df", "ihm", "al\u00b7les", "rein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "PIS", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "mir fehlt es fast an Bildern/", "tokens": ["mir", "fehlt", "es", "fast", "an", "Bil\u00b7dern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "benebst Bappihr und Zeit", "tokens": ["be\u00b7nebst", "Bap\u00b7pihr", "und", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "for so vihl Traurigkeit/", "tokens": ["for", "so", "vihl", "Trau\u00b7rig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "\u00fcmb solches au\u00df zuschildern.", "tokens": ["\u00fcmb", "sol\u00b7ches", "au\u00df", "zu\u00b7schil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sordan sezzstu dich hin", "tokens": ["Sor\u00b7dan", "sezz\u00b7stu", "dich", "hin"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "for ihn ans Klawizin/", "tokens": ["for", "ihn", "ans", "Kla\u00b7wi\u00b7zin", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "ich kriege gleich die Krencke/", "tokens": ["ich", "krie\u00b7ge", "gleich", "die", "Kren\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Molchs-Milch und Drachen-Rauch", "tokens": ["Molchs\u00b7Milch", "und", "Dra\u00b7chen\u00b7Rauch"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "befillt mir Brust und Bauch/", "tokens": ["be\u00b7fillt", "mir", "Brust", "und", "Bauch", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "sorbald ich bloh\u00df dran dencke.", "tokens": ["sor\u00b7bald", "ich", "bloh\u00df", "dran", "den\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PAV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Darzu wird scharmuzzirt", "tokens": ["Dar\u00b7zu", "wird", "schar\u00b7muz\u00b7zirt"], "token_info": ["word", "word", "word"], "pos": ["PAV", "VAFIN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "durchau\u00df und ohngenirt/", "tokens": ["durch\u00b7au\u00df", "und", "ohn\u00b7ge\u00b7nirt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKVZ", "KON", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "ein B\u00fcfgen h\u00f6rt man lachen!", "tokens": ["ein", "B\u00fcf\u00b7gen", "h\u00f6rt", "man", "la\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "ich sehe steiff vorbey/", "tokens": ["ich", "se\u00b7he", "steiff", "vor\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "verfluchte L\u00f6ffeley/", "tokens": ["ver\u00b7fluch\u00b7te", "L\u00f6f\u00b7fe\u00b7ley", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "da\u00df dich der Hellen-Rachen!", "tokens": ["da\u00df", "dich", "der", "Hel\u00b7len\u00b7Ra\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Zurlezzt so heissts adrett/", "tokens": ["Zur\u00b7lezzt", "so", "heissts", "ad\u00b7rett", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "atch\u00f6/ wir gehn zu Bett/", "tokens": ["at\u00b7ch\u00f6", "/", "wir", "gehn", "zu", "Bett", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["XY", "$(", "PPER", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "er seh uns r\u00e4cht bald wihder.", "tokens": ["er", "seh", "uns", "r\u00e4cht", "bald", "wih\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dr\u00fcmb/ g\u00fcnge es nach mir/", "tokens": ["Dr\u00fcmb", "/", "g\u00fcn\u00b7ge", "es", "nach", "mir", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "VVFIN", "PPER", "APPR", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "so kehmstu/ stoltzes Thier/", "tokens": ["so", "kehms\u00b7tu", "/", "stolt\u00b7zes", "Thier", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "mit einem Blind-Worm nihder!", "tokens": ["mit", "ei\u00b7nem", "Blin\u00b7dWorm", "nih\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}