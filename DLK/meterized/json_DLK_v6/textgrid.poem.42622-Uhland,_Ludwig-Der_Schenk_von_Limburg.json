{"textgrid.poem.42622": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Schenk von Limburg", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Limburg auf der Veste,", "tokens": ["Zu", "Lim\u00b7burg", "auf", "der", "Ves\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da wohnt' ein edler Graf,", "tokens": ["Da", "wohnt'", "ein", "ed\u00b7ler", "Graf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den keiner seiner G\u00e4ste", "tokens": ["Den", "kei\u00b7ner", "sei\u00b7ner", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jemals zu Hause traf.", "tokens": ["Je\u00b7mals", "zu", "Hau\u00b7se", "traf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Er trieb sich allerwegen", "tokens": ["Er", "trieb", "sich", "al\u00b7ler\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gebirg und Wald entlang,", "tokens": ["Ge\u00b7birg", "und", "Wald", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kein Sturm und auch kein Regen", "tokens": ["Kein", "Sturm", "und", "auch", "kein", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verleidet' ihm den Gang.", "tokens": ["Ver\u00b7lei\u00b7det'", "ihm", "den", "Gang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er trug ein Wams von Leder", "tokens": ["Er", "trug", "ein", "Wams", "von", "Le\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und einen J\u00e4gerhut", "tokens": ["Und", "ei\u00b7nen", "J\u00e4\u00b7ger\u00b7hut"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit mancher wilden Feder,", "tokens": ["Mit", "man\u00b7cher", "wil\u00b7den", "Fe\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das steht den J\u00e4gern gut;", "tokens": ["Das", "steht", "den", "J\u00e4\u00b7gern", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es hing ihm an der Seiten", "tokens": ["Es", "hing", "ihm", "an", "der", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Trinkgef\u00e4\u00df von Buchs;", "tokens": ["Ein", "Trink\u00b7ge\u00b7f\u00e4\u00df", "von", "Buchs", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gewaltig konnt er schreiten", "tokens": ["Ge\u00b7wal\u00b7tig", "konnt", "er", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und war von hohem Wuchs.", "tokens": ["Und", "war", "von", "ho\u00b7hem", "Wuchs", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wohl hatt er Knecht' und Mannen", "tokens": ["Wohl", "hatt", "er", "Knecht'", "und", "Man\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und hatt ein t\u00fcchtig Ro\u00df,", "tokens": ["Und", "hatt", "ein", "t\u00fcch\u00b7tig", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ging doch zu Fu\u00df von dannen", "tokens": ["Ging", "doch", "zu", "Fu\u00df", "von", "dan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NN", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und lie\u00df daheim den Tro\u00df.", "tokens": ["Und", "lie\u00df", "da\u00b7heim", "den", "Tro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es war sein ganz Geleite", "tokens": ["Es", "war", "sein", "ganz", "Ge\u00b7lei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Jagdspie\u00df, stark und lang,", "tokens": ["Ein", "Jagd\u00b7spie\u00df", ",", "stark", "und", "lang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "An dem er \u00fcber breite", "tokens": ["An", "dem", "er", "\u00fc\u00b7ber", "brei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Waldstr\u00f6me k\u00fchn sich schwang.", "tokens": ["Wald\u00b7str\u00f6\u00b7me", "k\u00fchn", "sich", "schwang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nun hielt auf Hohenstaufen", "tokens": ["Nun", "hielt", "auf", "Ho\u00b7hen\u00b7stau\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der deutsche Kaiser Haus.", "tokens": ["Der", "deut\u00b7sche", "Kai\u00b7ser", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der zog mit hellen Haufen", "tokens": ["Der", "zog", "mit", "hel\u00b7len", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Einsmals zu jagen aus.", "tokens": ["Eins\u00b7mals", "zu", "ja\u00b7gen", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Er rannt auf eine Hinde", "tokens": ["Er", "rannt", "auf", "ei\u00b7ne", "Hin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So hei\u00df und hastig vor,", "tokens": ["So", "hei\u00df", "und", "has\u00b7tig", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df ihn sein Jagdgesinde", "tokens": ["Da\u00df", "ihn", "sein", "Jagd\u00b7ge\u00b7sin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Im wilden Forst verlor.", "tokens": ["Im", "wil\u00b7den", "Forst", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Bei einer k\u00fchlen Quelle,", "tokens": ["Bei", "ei\u00b7ner", "k\u00fch\u00b7len", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da macht' er endlich Halt;", "tokens": ["Da", "macht'", "er", "end\u00b7lich", "Halt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gezieret war die Stelle", "tokens": ["Ge\u00b7zie\u00b7ret", "war", "die", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Blumen mannigfalt.", "tokens": ["Mit", "Blu\u00b7men", "man\u00b7nig\u00b7falt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hier dacht er sich zu legen", "tokens": ["Hier", "dacht", "er", "sich", "zu", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu einem Mittagschlaf,", "tokens": ["Zu", "ei\u00b7nem", "Mit\u00b7tag\u00b7schlaf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da rauscht' es in den H\u00e4gen", "tokens": ["Da", "rauscht'", "es", "in", "den", "H\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und stand vor ihm der Graf.", "tokens": ["Und", "stand", "vor", "ihm", "der", "Graf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da hub er an zu schelten:", "tokens": ["Da", "hub", "er", "an", "zu", "schel\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbtreff ich den Nachbar hie?", "tokens": ["\u00bb", "treff", "ich", "den", "Nach\u00b7bar", "hie", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu Hause weilt er selten,", "tokens": ["Zu", "Hau\u00b7se", "weilt", "er", "sel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Hofe kommt er nie:", "tokens": ["Zu", "Ho\u00b7fe", "kommt", "er", "nie", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Man mu\u00df im Walde streifen,", "tokens": ["Man", "mu\u00df", "im", "Wal\u00b7de", "strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wenn man ihn fahen will,", "tokens": ["Wenn", "man", "ihn", "fa\u00b7hen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Man mu\u00df ihn tapfer greifen,", "tokens": ["Man", "mu\u00df", "ihn", "tap\u00b7fer", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sonst h\u00e4lt er nirgend still.\u00ab", "tokens": ["Sonst", "h\u00e4lt", "er", "nir\u00b7gend", "still", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Als drauf ohn alle F\u00e4hrde", "tokens": ["Als", "drauf", "ohn", "al\u00b7le", "F\u00e4hr\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PAV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Graf sich niederlie\u00df", "tokens": ["Der", "Graf", "sich", "nie\u00b7der\u00b7lie\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und neben in die Erde", "tokens": ["Und", "ne\u00b7ben", "in", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "APPR", "ART", "NN"], "meter": "+----+-", "measure": "dactylic.init"}, "line.4": {"text": "Die J\u00e4gerstange stie\u00df,", "tokens": ["Die", "J\u00e4\u00b7ger\u00b7stan\u00b7ge", "stie\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da griff mit beiden H\u00e4nden", "tokens": ["Da", "griff", "mit", "bei\u00b7den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Kaiser nach dem Schaft:", "tokens": ["Der", "Kai\u00b7ser", "nach", "dem", "Schaft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbden Spie\u00df mu\u00df ich mir pf\u00e4nden,", "tokens": ["\u00bb", "den", "Spie\u00df", "mu\u00df", "ich", "mir", "pf\u00e4n\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich nehm ihn mir zu Haft.", "tokens": ["Ich", "nehm", "ihn", "mir", "zu", "Haft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Spie\u00df ist mir verfangen,", "tokens": ["Der", "Spie\u00df", "ist", "mir", "ver\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des ich so lang begehrt,", "tokens": ["Des", "ich", "so", "lang", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du sollst daf\u00fcr empfangen", "tokens": ["Du", "sollst", "da\u00b7f\u00fcr", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PAV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier dies mein bestes Pferd.", "tokens": ["Hier", "dies", "mein", "bes\u00b7tes", "Pferd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nicht schweifen im Gew\u00e4lde", "tokens": ["Nicht", "schwei\u00b7fen", "im", "Ge\u00b7w\u00e4l\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Darf mir ein solcher Mann,", "tokens": ["Darf", "mir", "ein", "sol\u00b7cher", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der mir zu Hof und Felde", "tokens": ["Der", "mir", "zu", "Hof", "und", "Fel\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Viel besser dienen kann.\u00ab", "tokens": ["Viel", "bes\u00b7ser", "die\u00b7nen", "kann", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbherr Kaiser, wollt vergeben!", "tokens": ["\u00bb", "herr", "Kai\u00b7ser", ",", "wollt", "ver\u00b7ge\u00b7ben", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NN", "NN", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr macht das Herz mir schwer.", "tokens": ["Ihr", "macht", "das", "Herz", "mir", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00dft mir mein freies Leben,", "tokens": ["La\u00dft", "mir", "mein", "frei\u00b7es", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und la\u00dft mir meinen Speer!", "tokens": ["Und", "la\u00dft", "mir", "mei\u00b7nen", "Speer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Pferd hab ich schon eigen,", "tokens": ["Ein", "Pferd", "hab", "ich", "schon", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr Eures sag ich Dank;", "tokens": ["F\u00fcr", "Eu\u00b7res", "sag", "ich", "Dank", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zu Rosse will ich steigen,", "tokens": ["Zu", "Ros\u00b7se", "will", "ich", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bin ich mal alt und krank.\u00ab", "tokens": ["Bin", "ich", "mal", "alt", "und", "krank", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbmit dir ist nicht zu streiten,", "tokens": ["\u00bb", "mit", "dir", "ist", "nicht", "zu", "strei\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du bist mir allzu stolz,", "tokens": ["Du", "bist", "mir", "all\u00b7zu", "stolz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch f\u00fchrst du an der Seiten", "tokens": ["Doch", "f\u00fchrst", "du", "an", "der", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Trinkgef\u00e4\u00df von Holz:", "tokens": ["Ein", "Trink\u00b7ge\u00b7f\u00e4\u00df", "von", "Holz", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun macht die Jagd mich d\u00fcrsten,", "tokens": ["Nun", "macht", "die", "Jagd", "mich", "d\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Drum tu mir das, Gesell,", "tokens": ["Drum", "tu", "mir", "das", ",", "Ge\u00b7sell", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PDS", "$,", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und gib mir eins zu b\u00fcrsten", "tokens": ["Und", "gib", "mir", "eins", "zu", "b\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "PIS", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aus diesem Wasserquell!\u00ab", "tokens": ["Aus", "die\u00b7sem", "Was\u00b7ser\u00b7quell", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der Graf hat sich erhoben,", "tokens": ["Der", "Graf", "hat", "sich", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er schwenkt den Becher klar,", "tokens": ["Er", "schwenkt", "den", "Be\u00b7cher", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er f\u00fcllt ihn an bis oben,", "tokens": ["Er", "f\u00fcllt", "ihn", "an", "bis", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "KON", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "H\u00e4lt ihn dem Kaiser dar.", "tokens": ["H\u00e4lt", "ihn", "dem", "Kai\u00b7ser", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der schl\u00fcrft mit vollen Z\u00fcgen", "tokens": ["Der", "schl\u00fcrft", "mit", "vol\u00b7len", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den k\u00fchlen Trank hinein", "tokens": ["Den", "k\u00fch\u00b7len", "Trank", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APZR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und zeigt ein solch Vergn\u00fcgen,", "tokens": ["Und", "zeigt", "ein", "solch", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als w\u00e4r's der beste Wein.", "tokens": ["Als", "w\u00e4r's", "der", "bes\u00b7te", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Dann fa\u00dft der schlaue Zecher", "tokens": ["Dann", "fa\u00dft", "der", "schlau\u00b7e", "Ze\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Grafen bei der Hand:", "tokens": ["Den", "Gra\u00b7fen", "bei", "der", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbdu schwenktest mir den Becher", "tokens": ["\u00bb", "du", "schwenk\u00b7test", "mir", "den", "Be\u00b7cher"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und f\u00fclltest ihn zum Rand,", "tokens": ["Und", "f\u00fcll\u00b7test", "ihn", "zum", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du hieltest mir zum Munde", "tokens": ["Du", "hiel\u00b7test", "mir", "zum", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das labende Getr\u00e4nk:", "tokens": ["Das", "la\u00b7ben\u00b7de", "Ge\u00b7tr\u00e4nk", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du bist von dieser Stunde", "tokens": ["Du", "bist", "von", "die\u00b7ser", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des deutschen Reiches Schenk!\u00ab", "tokens": ["Des", "deut\u00b7schen", "Rei\u00b7ches", "Schenk", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}