{"textgrid.poem.26044": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Sehnsucht gab mir ihr weites Kleid", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sehnsucht gab mir ihr weites Kleid,", "tokens": ["Sehn\u00b7sucht", "gab", "mir", "ihr", "wei\u00b7tes", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Seine Naht ist lang wie die Ewigkeit.", "tokens": ["Sei\u00b7ne", "Naht", "ist", "lang", "wie", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Streicht die Sehnsucht um das Haus,", "tokens": ["Streicht", "die", "Sehn\u00b7sucht", "um", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Trocknen die plaudernden Brunnen aus;", "tokens": ["Trock\u00b7nen", "die", "plau\u00b7dern\u00b7den", "Brun\u00b7nen", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Die Tage kommen wie Tiere daher,", "tokens": ["Die", "Ta\u00b7ge", "kom\u00b7men", "wie", "Tie\u00b7re", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KOKOM", "NN", "PAV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Du rufst ihre Namen, sie atmen nur schwer;", "tokens": ["Du", "rufst", "ih\u00b7re", "Na\u00b7men", ",", "sie", "at\u00b7men", "nur", "schwer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Du suchst dich im Spiegel, der Spiegel ist leer,", "tokens": ["Du", "suchst", "dich", "im", "Spie\u00b7gel", ",", "der", "Spie\u00b7gel", "ist", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "H\u00f6rst nur der Sehnsucht Schritt,", "tokens": ["H\u00f6rst", "nur", "der", "Sehn\u00b7sucht", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Du selbst bist nicht mehr.", "tokens": ["Du", "selbst", "bist", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}