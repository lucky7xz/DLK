{"textgrid.poem.44610": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier ging er, stand und schrieb, sa\u00df nieder,", "genre": "verse", "period": "N.A.", "pub_year": 1866, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier ging er, stand und schrieb, sa\u00df nieder,", "tokens": ["Hier", "ging", "er", ",", "stand", "und", "schrieb", ",", "sa\u00df", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Unh\u00f6rbar schwebten ringsum ewge Lieder.", "tokens": ["Un\u00b7h\u00f6r\u00b7bar", "schweb\u00b7ten", "ring\u00b7sum", "ew\u00b7ge", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Den Weg, wir haben ihn mit ihm gemacht,", "tokens": ["Den", "Weg", ",", "wir", "ha\u00b7ben", "ihn", "mit", "ihm", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Indem wir h\u00f6rten, was er hier gedacht.", "tokens": ["In\u00b7dem", "wir", "h\u00f6r\u00b7ten", ",", "was", "er", "hier", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nur arm der Platz, kaum sch\u00f6n zur Ruhestatt.", "tokens": ["Nur", "arm", "der", "Platz", ",", "kaum", "sch\u00f6n", "zur", "Ru\u00b7he\u00b7statt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wer sind wir, die wir ihn weihten?", "tokens": ["Und", "wer", "sind", "wir", ",", "die", "wir", "ihn", "weih\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "PPER", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbder Ort, den je ein edler Mann betrat,", "tokens": ["\u00bb", "der", "Ort", ",", "den", "je", "ein", "ed\u00b7ler", "Mann", "be\u00b7trat", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er ist geweiht f\u00fcr alle Zeiten.\u00ab", "tokens": ["Er", "ist", "ge\u00b7weiht", "f\u00fcr", "al\u00b7le", "Zei\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}