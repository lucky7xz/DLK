{"textgrid.poem.48049": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Bild", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bla\u00df und herbe dein Gesicht,", "tokens": ["Bla\u00df", "und", "her\u00b7be", "dein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "tief im Blick erloschne Funken,", "tokens": ["tief", "im", "Blick", "er\u00b7lo\u00b7schne", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "hast den Kelch der Bitternis", "tokens": ["hast", "den", "Kelch", "der", "Bit\u00b7ter\u00b7nis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "bis zum Grund du leergetrunken.", "tokens": ["bis", "zum", "Grund", "du", "leer\u00b7ge\u00b7trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Langsam hebst du ihn vom Mund,", "tokens": ["Lang\u00b7sam", "hebst", "du", "ihn", "vom", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "lautlos starrst du in die Ferne \u2013", "tokens": ["laut\u00b7los", "starrst", "du", "in", "die", "Fer\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00fcber deinem Haupte gehn", "tokens": ["\u00fc\u00b7ber", "dei\u00b7nem", "Haup\u00b7te", "gehn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "klar und kalt die ewigen Sterne.", "tokens": ["klar", "und", "kalt", "die", "e\u00b7wi\u00b7gen", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}