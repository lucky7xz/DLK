{"dta.poem.21084": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Walder und Oda.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Walder.               ", "tokens": ["Wal\u00b7der", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Oda, Oda, meine Fr&#252;herw&#228;hlte,               ", "tokens": ["O\u00b7da", ",", "O\u00b7da", ",", "mei\u00b7ne", "Fr", "&#252;", "herw", "&#228;", "hl\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "XML_entity", "word", "XML_entity", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "PPOSAT", "NN", "$(", "NE", "$(", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Meine Langverlobte, meine Neuverm\u00e4hlte,", "tokens": ["Mei\u00b7ne", "Lang\u00b7ver\u00b7lob\u00b7te", ",", "mei\u00b7ne", "Neu\u00b7ver\u00b7m\u00e4hl\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Meine Eine, Eigne, Einzige!", "tokens": ["Mei\u00b7ne", "Ei\u00b7ne", ",", "Eig\u00b7ne", ",", "Ein\u00b7zi\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "ADJA", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Horch, sie schl\u00e4gt, die heissersehnte Stunde;", "tokens": ["Horch", ",", "sie", "schl\u00e4gt", ",", "die", "heis\u00b7ser\u00b7sehn\u00b7te", "Stun\u00b7de", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ewge Weihe winket unserm Bunde;", "tokens": ["Ew\u00b7ge", "Wei\u00b7he", "win\u00b7ket", "un\u00b7serm", "Bun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wonne wird der Sehnsucht schmachtend Weh.", "tokens": ["Won\u00b7ne", "wird", "der", "Sehn\u00b7sucht", "schmach\u00b7tend", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Oda.               ", "tokens": ["O\u00b7da", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Walder, Walder, welche s\u00fcsse Trauer", "tokens": ["Wal\u00b7der", ",", "Wal\u00b7der", ",", "wel\u00b7che", "s\u00fcs\u00b7se", "Trau\u00b7er"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "PWAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u00dcberw\u00f6lkt mich! Welche Wonneschauer", "tokens": ["\u00dc\u00b7berw\u00b7\u00f6lkt", "mich", "!", "Wel\u00b7che", "Won\u00b7ne\u00b7schau\u00b7er"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "PWAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "\u00dcbergl\u00fchn dein M\u00e4dchen Guss auf Guss.", "tokens": ["\u00dc\u00b7berg\u00b7l\u00fchn", "dein", "M\u00e4d\u00b7chen", "Guss", "auf", "Guss", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "APPR", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "\u00dcberwunden, Walder, \u00fcberwunden", "tokens": ["\u00dc\u00b7berw\u00b7un\u00b7den", ",", "Wal\u00b7der", ",", "\u00fc\u00b7berw\u00b7un\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "VVPP"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Sind der Treue schw\u00fcle Pr\u00fcfungsstunden,", "tokens": ["Sind", "der", "Treu\u00b7e", "schw\u00fc\u00b7le", "Pr\u00fc\u00b7fungs\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und ich k\u00fcsse dich mit Gattinkuss.", "tokens": ["Und", "ich", "k\u00fcs\u00b7se", "dich", "mit", "Gat\u00b7tin\u00b7kuss", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Walder.               ", "tokens": ["Wal\u00b7der", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Also, Traute, bist du mein auf immer!", "tokens": ["Al\u00b7so", ",", "Trau\u00b7te", ",", "bist", "du", "mein", "auf", "im\u00b7mer", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VAFIN", "PPER", "PPOSAT", "APPR", "ADV", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Mein f\u00fcr Zeit und Ewigkeit! und nimmer", "tokens": ["Mein", "f\u00fcr", "Zeit", "und", "E\u00b7wig\u00b7keit", "!", "und", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "APPR", "NN", "KON", "NN", "$.", "KON", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Reisst mich Zeit noch Ewigkeit von dir!", "tokens": ["Reisst", "mich", "Zeit", "noch", "E\u00b7wig\u00b7keit", "von", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADV", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Oda.               ", "tokens": ["O\u00b7da", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Dein, Geliebter, bin ich, dein auf immer;", "tokens": ["Dein", ",", "Ge\u00b7lieb\u00b7ter", ",", "bin", "ich", ",", "dein", "auf", "im\u00b7mer", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "NN", "$,", "VAFIN", "PPER", "$,", "PPOSAT", "APPR", "ADV", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Dein vor Welt und Himmel. Nimmer, nimmer", "tokens": ["Dein", "vor", "Welt", "und", "Him\u00b7mel", ".", "Nim\u00b7mer", ",", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPOSAT", "APPR", "NN", "KON", "NN", "$.", "NE", "$,", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Trennen Welt und Himmel mich von dir.", "tokens": ["Tren\u00b7nen", "Welt", "und", "Him\u00b7mel", "mich", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Walder.               ", "tokens": ["Wal\u00b7der", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Aber Oda, meine Oda, sage:", "tokens": ["A\u00b7ber", "O\u00b7da", ",", "mei\u00b7ne", "O\u00b7da", ",", "sa\u00b7ge", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NE", "$,", "PPOSAT", "NE", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wirst du nach, wie vor dem Bundestage", "tokens": ["Wirst", "du", "nach", ",", "wie", "vor", "dem", "Bun\u00b7des\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKVZ", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mich auch lieben voll so lieb? so warm?", "tokens": ["Mich", "auch", "lie\u00b7ben", "voll", "so", "lieb", "?", "so", "warm", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "ADV", "ADJD", "$.", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Oda.               ", "tokens": ["O\u00b7da", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Walder, Walder, du mein Fr\u00fcherw\u00e4hlter,", "tokens": ["Wal\u00b7der", ",", "Wal\u00b7der", ",", "du", "mein", "Fr\u00fc\u00b7her\u00b7w\u00e4hl\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Du mein Langgew\u00fcnschter, du mein Neuverm\u00e4hlter,", "tokens": ["Du", "mein", "Lang\u00b7ge\u00b7w\u00fcnschter", ",", "du", "mein", "Neu\u00b7ver\u00b7m\u00e4hl\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "PPER", "PPOSAT", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Deine Frage weckt mir leisen Harm.", "tokens": ["Dei\u00b7ne", "Fra\u00b7ge", "weckt", "mir", "lei\u00b7sen", "Harm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Ahnet' ich nicht deines Geistes Tugend", "tokens": ["Ah\u00b7net'", "ich", "nicht", "dei\u00b7nes", "Geis\u00b7tes", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schon im Knospen meiner Rosenjugend,", "tokens": ["Schon", "im", "Knos\u00b7pen", "mei\u00b7ner", "Ro\u00b7sen\u00b7ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schloss mich fest an dich, Geliebter, an?", "tokens": ["Schloss", "mich", "fest", "an", "dich", ",", "Ge\u00b7lieb\u00b7ter", ",", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "PPER", "$,", "NN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wies zur\u00fcck des Stutzers s\u00fcsslich Heucheln,", "tokens": ["Wies", "zu\u00b7r\u00fcck", "des", "Stut\u00b7zers", "s\u00fcss\u00b7lich", "Heu\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Blickte Hohn des Gecken ekelm Schmeicheln,", "tokens": ["Blick\u00b7te", "Hohn", "des", "Ge\u00b7cken", "e\u00b7kelm", "Schmei\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Hing an dir, du deutscher, grader Mann?", "tokens": ["Hing", "an", "dir", ",", "du", "deut\u00b7scher", ",", "gra\u00b7der", "Mann", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "PPER", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Weiht' ich dir nicht meine sch\u00f6nsten Kr\u00e4fte?", "tokens": ["Weiht'", "ich", "dir", "nicht", "mei\u00b7ne", "sch\u00f6ns\u00b7ten", "Kr\u00e4f\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dachte dein bey jedem Tagsgesch\u00e4fte,", "tokens": ["Dach\u00b7te", "dein", "bey", "je\u00b7dem", "Tags\u00b7ge\u00b7sch\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dein, wann Schlummer meine Wimper schloss?", "tokens": ["Dein", ",", "wann", "Schlum\u00b7mer", "mei\u00b7ne", "Wim\u00b7per", "schloss", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "PWAV", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Dein, sobald des Morgens Rosenschimmer", "tokens": ["Dein", ",", "so\u00b7bald", "des", "Mor\u00b7gens", "Ro\u00b7sen\u00b7schim\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "$,", "KOUS", "ART", "ADV", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mich umstrahlten? dein, wenn seine Flimmer", "tokens": ["Mich", "um\u00b7strahl\u00b7ten", "?", "dein", ",", "wenn", "sei\u00b7ne", "Flim\u00b7mer"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Blass der Vollmond in mein Fenster goss?", "tokens": ["Blass", "der", "Voll\u00b7mond", "in", "mein", "Fens\u00b7ter", "goss", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPOSAT", "NN", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "O, wie oft an deine Brust gesunken,", "tokens": ["O", ",", "wie", "oft", "an", "dei\u00b7ne", "Brust", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und vom Kelch der Liebe wonnetrunken,", "tokens": ["Und", "vom", "Kelch", "der", "Lie\u00b7be", "won\u00b7ne\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sehnt' ich mich, erst ewig dein zu seyn!", "tokens": ["Sehnt'", "ich", "mich", ",", "erst", "e\u00b7wig", "dein", "zu", "seyn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "ADV", "ADJD", "PPOSAT", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Heute, heute hab' ich dich erwunden;", "tokens": ["Heu\u00b7te", ",", "heu\u00b7te", "hab'", "ich", "dich", "er\u00b7wun\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und vollendet sind der Pr\u00fcfung Stunden,", "tokens": ["Und", "voll\u00b7en\u00b7det", "sind", "der", "Pr\u00fc\u00b7fung", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und mein Walder ist nun ewig mein!", "tokens": ["Und", "mein", "Wal\u00b7der", "ist", "nun", "e\u00b7wig", "mein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "PPOSAT", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Walder, Walder, du mein Theurerrungner,", "tokens": ["Wal\u00b7der", ",", "Wal\u00b7der", ",", "du", "mein", "Theu\u00b7rer\u00b7rung\u00b7ner", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mein nun ganz Umfangner, mein nun ganz Um-", "tokens": ["Mein", "nun", "ganz", "Um\u00b7fang\u00b7ner", ",", "mein", "nun", "ganz", "Um"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADV", "ADV", "NN", "$,", "PPOSAT", "ADV", "ADV", "TRUNC"], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "schlungner,", "tokens": ["schlung\u00b7ner", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Und du f\u00fcrchtest, deiner Gattin Arm", "tokens": ["Und", "du", "f\u00fcrch\u00b7test", ",", "dei\u00b7ner", "Gat\u00b7tin", "Arm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Werde minder innig dich umschmiegen?", "tokens": ["Wer\u00b7de", "min\u00b7der", "in\u00b7nig", "dich", "um\u00b7schmie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Minder traut ihr Herz sich zu dir f\u00fcgen?", "tokens": ["Min\u00b7der", "traut", "ihr", "Herz", "sich", "zu", "dir", "f\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "PRF", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Ihre Brust dir klopfen minder warm?", "tokens": ["Ih\u00b7re", "Brust", "dir", "klop\u00b7fen", "min\u00b7der", "warm", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Walder nein, mit jedes Morgens Spriessen", "tokens": ["Wal\u00b7der", "nein", ",", "mit", "je\u00b7des", "Mor\u00b7gens", "Spries\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKANT", "$,", "APPR", "PIAT", "ADV", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Will ich inniger mich an dich schliessen;", "tokens": ["Will", "ich", "in\u00b7ni\u00b7ger", "mich", "an", "dich", "schlies\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Will mich n\u00e4her dir, mein Edler, nahn.", "tokens": ["Will", "mich", "n\u00e4\u00b7her", "dir", ",", "mein", "Ed\u00b7ler", ",", "nahn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PPER", "$,", "PPOSAT", "NN", "$,", "ADJA", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wie die Rebe um den Ulmbaum ranket,", "tokens": ["Wie", "die", "Re\u00b7be", "um", "den", "Ulm\u00b7baum", "ran\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mit ihm steigt und mit ihm niederschwanket,", "tokens": ["Mit", "ihm", "steigt", "und", "mit", "ihm", "nie\u00b7der\u00b7schwan\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "KON", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Will ich dich in Freud' und Leid umfahn.", "tokens": ["Will", "ich", "dich", "in", "Freud'", "und", "Leid", "um\u00b7fahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Fest mich lehnend, Freund, an deine Rechte,", "tokens": ["Fest", "mich", "leh\u00b7nend", ",", "Freund", ",", "an", "dei\u00b7ne", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Will ich mit dir durch des Lebens N\u00e4chte,", "tokens": ["Will", "ich", "mit", "dir", "durch", "des", "Le\u00b7bens", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und des Todes Grauenthale gehn;", "tokens": ["Und", "des", "To\u00b7des", "Grau\u00b7en\u00b7tha\u00b7le", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nimmer von dir wanken, nimmer von dir lassen,", "tokens": ["Nim\u00b7mer", "von", "dir", "wan\u00b7ken", ",", "nim\u00b7mer", "von", "dir", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVINF", "$,", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Dir am Busen athmen, dir im Arm erblassen,", "tokens": ["Dir", "am", "Bu\u00b7sen", "ath\u00b7men", ",", "dir", "im", "Arm", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVINF", "$,", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Dir zur Seite schlummern, mit dir auferstehn!", "tokens": ["Dir", "zur", "Sei\u00b7te", "schlum\u00b7mern", ",", "mit", "dir", "auf\u00b7er\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVINF", "$,", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.13": {"line.1": {"text": "Walder.               ", "tokens": ["Wal\u00b7der", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Halt, Geliebte, deine Lieb' und Treue,", "tokens": ["Halt", ",", "Ge\u00b7lieb\u00b7te", ",", "dei\u00b7ne", "Lieb'", "und", "Treu\u00b7e", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Warm, wie Fr\u00fchlingsodem, rein, wie Tempelweihe,", "tokens": ["Warm", ",", "wie", "Fr\u00fch\u00b7lin\u00b7gso\u00b7dem", ",", "rein", ",", "wie", "Tem\u00b7pel\u00b7wei\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "$,", "ADJD", "$,", "PWAV", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "\u00dcbermannet meine Mannlichkeit.", "tokens": ["\u00dc\u00b7berm\u00b7an\u00b7net", "mei\u00b7ne", "Mann\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Deine Lieb' ist stark, wie Mark der Jugend,", "tokens": ["Dei\u00b7ne", "Lieb'", "ist", "stark", ",", "wie", "Mark", "der", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PWAV", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Seelelabend, wie der Wein der Tugend,", "tokens": ["See\u00b7lel\u00b7a\u00b7bend", ",", "wie", "der", "Wein", "der", "Tu\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Unverletzlich, wie ein Altareid.", "tokens": ["Un\u00b7ver\u00b7letz\u00b7lich", ",", "wie", "ein", "Al\u00b7ta\u00b7reid", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.14": {"line.1": {"text": "Welcher Friede, meine Vielgetreue,", "tokens": ["Wel\u00b7cher", "Frie\u00b7de", ",", "mei\u00b7ne", "Viel\u00b7ge\u00b7treu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Welcher Freuden ungebrochne Reihe", "tokens": ["Wel\u00b7cher", "Freu\u00b7den", "un\u00b7ge\u00b7broch\u00b7ne", "Rei\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Harret mein an deiner treuen Brust.", "tokens": ["Har\u00b7ret", "mein", "an", "dei\u00b7ner", "treu\u00b7en", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "M\u00f6gen Menschen und Verh\u00e4ngniss schmollen;", "tokens": ["M\u00f6\u00b7gen", "Men\u00b7schen", "und", "Ver\u00b7h\u00e4ng\u00b7niss", "schmol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "M\u00f6gen St\u00fcrme st\u00fcrmen; m\u00f6gen Donner grollen:", "tokens": ["M\u00f6\u00b7gen", "St\u00fcr\u00b7me", "st\u00fcr\u00b7men", ";", "m\u00f6\u00b7gen", "Don\u00b7ner", "grol\u00b7len", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$.", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Dir am Busen bl\u00fchn mir Trost und Lust.", "tokens": ["Dir", "am", "Bu\u00b7sen", "bl\u00fchn", "mir", "Trost", "und", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.15": {"line.1": {"text": "Dir am Busen w\u00e4r' die Welt voll M\u00e4ngel", "tokens": ["Dir", "am", "Bu\u00b7sen", "w\u00e4r'", "die", "Welt", "voll", "M\u00e4n\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "VAFIN", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mir Elisium, der Mensch mir Engel,", "tokens": ["Mir", "E\u00b7li\u00b7si\u00b7um", ",", "der", "Mensch", "mir", "En\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "PPER", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und das Leben mir ein Jubelreihn;", "tokens": ["Und", "das", "Le\u00b7ben", "mir", "ein", "Ju\u00b7bel\u00b7reihn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn mich nicht der Nachtgedanke tr\u00fcbte,", "tokens": ["Wenn", "mich", "nicht", "der", "Nacht\u00b7ge\u00b7dan\u00b7ke", "tr\u00fcb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Meine Oda, dass auch die Geliebte,", "tokens": ["Mei\u00b7ne", "O\u00b7da", ",", "dass", "auch", "die", "Ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "$,", "KOUS", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und die selge Liebe sterblich seyn!", "tokens": ["Und", "