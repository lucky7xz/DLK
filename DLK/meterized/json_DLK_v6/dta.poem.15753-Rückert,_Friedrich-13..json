{"dta.poem.15753": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du klagst, unm\u00f6glich sei f\u00fcrs Volk zu dichten heut.", "tokens": ["Du", "klagst", ",", "un\u00b7m\u00f6g\u00b7lich", "sei", "f\u00fcrs", "Volk", "zu", "dich\u00b7ten", "heut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "VAFIN", "APPRART", "NN", "APPR", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann aber hat des Volks die Dichtkunst sich erfreut?", "tokens": ["Wann", "a\u00b7ber", "hat", "des", "Volks", "die", "Dicht\u00b7kunst", "sich", "er\u00b7freut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Selbst in der sch\u00f6nsten Zeit der Kunst ward dargeboten", "tokens": ["Selbst", "in", "der", "sch\u00f6ns\u00b7ten", "Zeit", "der", "Kunst", "ward", "dar\u00b7ge\u00b7bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "PAV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ihre Gabe nur Hellenen, nicht Heloten.", "tokens": ["Doch", "ih\u00b7re", "Ga\u00b7be", "nur", "Hel\u00b7le\u00b7nen", ",", "nicht", "He\u00b7lo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "NN", "$,", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nun sind verschmolzen zwar Heloten und Hellenen,", "tokens": ["Nun", "sind", "ver\u00b7schmol\u00b7zen", "zwar", "He\u00b7lo\u00b7ten", "und", "Hel\u00b7le\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch immer weiht die Kunst nur diesen sich, nicht jenen.", "tokens": ["Doch", "im\u00b7mer", "weiht", "die", "Kunst", "nur", "die\u00b7sen", "sich", ",", "nicht", "je\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ADV", "PDAT", "PRF", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}