{"dta.poem.9664": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Mora.  \n * * v. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.71", "no:0.14", "cy:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mora/ die will keine jungfer/ sondern eine fr\u00e4ulein seyn.", "tokens": ["Mo\u00b7ra", "/", "die", "will", "kei\u00b7ne", "jung\u00b7fer", "/", "son\u00b7dern", "ei\u00b7ne", "fr\u00e4u\u00b7lein", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PDS", "VMFIN", "PIAT", "ADJA", "$(", "KON", "ART", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und was wunder? weil von jungfern nichts an ihr als", "tokens": ["Und", "was", "wun\u00b7der", "?", "weil", "von", "jung\u00b7fern", "nichts", "an", "ihr", "als"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "NN", "$.", "KOUS", "APPR", "ADJA", "PIS", "APPR", "PPER", "KOUS"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "nahm und schein.", "tokens": ["nahm", "und", "schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}