{"textgrid.poem.25352": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie aus den Armen Gottes", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie aus den Armen Gottes", "tokens": ["Wie", "aus", "den", "Ar\u00b7men", "Got\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "glitt ich in den Arm der Welt:", "tokens": ["glitt", "ich", "in", "den", "Arm", "der", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch wars das Streichen seiner H\u00e4nde,", "tokens": ["Noch", "wars", "das", "Strei\u00b7chen", "sei\u00b7ner", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "das mir meine Br\u00fcste aufgeschwellt,", "tokens": ["das", "mir", "mei\u00b7ne", "Br\u00fcs\u00b7te", "auf\u00b7ge\u00b7schwellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und seiner Liebe Schwert,", "tokens": ["Und", "sei\u00b7ner", "Lie\u00b7be", "Schwert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "das lustvoll sehrend meinen Leib durchstie\u00df", "tokens": ["das", "lust\u00b7voll", "seh\u00b7rend", "mei\u00b7nen", "Leib", "durch\u00b7stie\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVPP", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und das in Wollust weilend", "tokens": ["Und", "das", "in", "Wol\u00b7lust", "wei\u00b7lend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "sich im Dunkel meines Blutes niederlie\u00df,", "tokens": ["sich", "im", "Dun\u00b7kel", "mei\u00b7nes", "Blu\u00b7tes", "nie\u00b7der\u00b7lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.9": {"text": "Als schon mein Leib, den Vielen ausgeliefert,", "tokens": ["Als", "schon", "mein", "Leib", ",", "den", "Vie\u00b7len", "aus\u00b7ge\u00b7lie\u00b7fert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "sich auf armen Polstern streckte.", "tokens": ["sich", "auf", "ar\u00b7men", "Pols\u00b7tern", "streck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und wenn ich unter Schauern mich vergrub,", "tokens": ["Und", "wenn", "ich", "un\u00b7ter", "Schau\u00b7ern", "mich", "ver\u00b7grub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "war ers, dem sich mein Scho\u00df entgegenreckte,", "tokens": ["war", "ers", ",", "dem", "sich", "mein", "Scho\u00df", "ent\u00b7ge\u00b7gen\u00b7reck\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und wenn mit rohem Wort", "tokens": ["Und", "wenn", "mit", "ro\u00b7hem", "Wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "die Welt mich \u00fcberfiel,", "tokens": ["die", "Welt", "mich", "\u00fc\u00b7berf\u00b7iel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Flo\u00df selige Marter", "tokens": ["Flo\u00df", "se\u00b7li\u00b7ge", "Mar\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "und im Fernen leuchtete der Pr\u00fcfung Ziel.", "tokens": ["und", "im", "Fer\u00b7nen", "leuch\u00b7te\u00b7te", "der", "Pr\u00fc\u00b7fung", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.17": {"text": "Und ekle Speise,", "tokens": ["Und", "ek\u00b7le", "Spei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "die aus Graun und Schmach an mich erging,", "tokens": ["die", "aus", "Graun", "und", "Schmach", "an", "mich", "er\u00b7ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.19": {"text": "War die geweihte Hostie,", "tokens": ["War", "die", "ge\u00b7weih\u00b7te", "Hos\u00b7tie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.20": {"text": "die mein Mund aus seiner Hand empfing,", "tokens": ["die", "mein", "Mund", "aus", "sei\u00b7ner", "Hand", "emp\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "Und jede Lust", "tokens": ["Und", "je\u00b7de", "Lust"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "war tief im Blute seiner Wunden eingek\u00fchlt,", "tokens": ["war", "tief", "im", "Blu\u00b7te", "sei\u00b7ner", "Wun\u00b7den", "ein\u00b7ge\u00b7k\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPRART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und jedes Wehe", "tokens": ["Und", "je\u00b7des", "We\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "vom Gefunkel seiner Liebe \u00fcbersp\u00fclt,", "tokens": ["vom", "Ge\u00b7fun\u00b7kel", "sei\u00b7ner", "Lie\u00b7be", "\u00fc\u00b7bers\u00b7p\u00fclt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.25": {"text": "Aus Kellern, Hafenkneipen, Dirnengassen,", "tokens": ["Aus", "Kel\u00b7lern", ",", "Ha\u00b7fen\u00b7knei\u00b7pen", ",", "Dir\u00b7nen\u00b7gas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "wo die Seele wie vom Leib verirrt", "tokens": ["wo", "die", "See\u00b7le", "wie", "vom", "Leib", "ver\u00b7irrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "KOKOM", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.27": {"text": "dem Traum entgegenschlief,", "tokens": ["dem", "Traum", "ent\u00b7ge\u00b7gen\u00b7schlief", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Wuchs mailich schon die Stimme,", "tokens": ["Wuchs", "mai\u00b7lich", "schon", "die", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "die zu Hochzeit und zu Auferstehung rief.", "tokens": ["die", "zu", "Hoch\u00b7zeit", "und", "zu", "Auf\u00b7er\u00b7ste\u00b7hung", "rief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}