{"dta.poem.2411": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sinnenspruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Ach seht! da ligt das Kind von \u00e4delem G.m\u00fchte/", "tokens": ["Ach", "seht", "!", "da", "ligt", "das", "Kind", "von", "\u00e4\u00b7de\u00b7lem", "G.", "m\u00fch\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "abbreviation", "word", "punct"], "pos": ["ITJ", "VVFIN", "$.", "ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "VMFIN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Stammes sch\u00f6nster Zweig/ in einer Todtenbar/", "tokens": ["Des", "Stam\u00b7mes", "sch\u00f6ns\u00b7ter", "Zweig", "/", "in", "ei\u00b7ner", "Tod\u00b7ten\u00b7bar", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der seiner Eltern lust/ und h\u00f6chste Hoffnung war/", "tokens": ["Der", "sei\u00b7ner", "El\u00b7tern", "lust", "/", "und", "h\u00f6chs\u00b7te", "Hoff\u00b7nung", "war", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$(", "KON", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vom Tode hingemetzt in seiner sch\u00f6nsten Bl\u00fchte.", "tokens": ["Vom", "To\u00b7de", "hin\u00b7ge\u00b7metzt", "in", "sei\u00b7ner", "sch\u00f6ns\u00b7ten", "Bl\u00fch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gott nimmt die Seeli weg; das Grab den C\u00f6rper h\u00e4lte", "tokens": ["Gott", "nimmt", "die", "See\u00b7li", "weg", ";", "das", "Grab", "den", "C\u00f6r\u00b7per", "h\u00e4l\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$.", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sein \u00e4dles Tugendlob bleibt aber in der Welt.", "tokens": ["Sein", "\u00e4d\u00b7les", "Tu\u00b7gen\u00b7dlob", "bleibt", "a\u00b7ber", "in", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}