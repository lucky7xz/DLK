{"textgrid.poem.59679": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Gerechtigkeit Gottes", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du findest keinen festen Weg zum Gl\u00fccke", "tokens": ["Du", "fin\u00b7dest", "kei\u00b7nen", "fes\u00b7ten", "Weg", "zum", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier, wo das bleiche Mondlicht niederschauet;", "tokens": ["Hier", ",", "wo", "das", "blei\u00b7che", "Mond\u00b7licht", "nie\u00b7der\u00b7schau\u00b7et", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur, wer sich gleich der Regenbogenbr\u00fccke", "tokens": ["Nur", ",", "wer", "sich", "gleich", "der", "Re\u00b7gen\u00b7bo\u00b7gen\u00b7br\u00fc\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWS", "PRF", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus leichten, bunten Steinen ihn erbauet,", "tokens": ["Aus", "leich\u00b7ten", ",", "bun\u00b7ten", "Stei\u00b7nen", "ihn", "er\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ist weise, wer wie Kinder fort sich spielet,", "tokens": ["Ist", "wei\u00b7se", ",", "wer", "wie", "Kin\u00b7der", "fort", "sich", "spie\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "PWS", "KOKOM", "NN", "PTKVZ", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auf keinen Fang wie auf Gewisses zielet.", "tokens": ["Auf", "kei\u00b7nen", "Fang", "wie", "auf", "Ge\u00b7wis\u00b7ses", "zie\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KOKOM", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es war der Mann gekommen aus dem Lande,", "tokens": ["Es", "war", "der", "Mann", "ge\u00b7kom\u00b7men", "aus", "dem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo Honig Gift ist", "tokens": ["Wo", "Ho\u00b7nig", "Gift", "ist"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "NN", "VAFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Er schlug die Welt durch Schwert und Trug in Bande,", "tokens": ["Er", "schlug", "die", "Welt", "durch", "Schwert", "und", "Trug", "in", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch Gro\u00dfes zu dem Gr\u00f6\u00dften stolz erk\u00fchnet \u2013", "tokens": ["Durch", "Gro\u00b7\u00dfes", "zu", "dem", "Gr\u00f6\u00df\u00b7ten", "stolz", "er\u00b7k\u00fch\u00b7net", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "\u00bbmir\u00ab, sprach er trotzig, \u00bbgab der Herr die Reiche,", "tokens": ["\u00bb", "mir", "\u00ab", ",", "sprach", "er", "trot\u00b7zig", ",", "\u00bb", "gab", "der", "Herr", "die", "Rei\u00b7che", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$(", "$,", "VVFIN", "PPER", "ADJD", "$,", "$(", "VVFIN", "ART", "NN", "ART", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wer wider mich und Gott will, der erbleiche!\u00ab", "tokens": ["Wer", "wi\u00b7der", "mich", "und", "Gott", "will", ",", "der", "er\u00b7blei\u00b7che", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWS", "APPR", "PPER", "KON", "NN", "VMFIN", "$,", "ART", "ADJA", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So rief der Korse, doch es schmolz zusammen", "tokens": ["So", "rief", "der", "Kor\u00b7se", ",", "doch", "es", "schmolz", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Stolz im Schnee und Eis der Moskowiten,", "tokens": ["Sein", "Stolz", "im", "Schnee", "und", "Eis", "der", "Mos\u00b7ko\u00b7wi\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aufschlugen da aus allen Herzen Flammen,", "tokens": ["Auf\u00b7schlu\u00b7gen", "da", "aus", "al\u00b7len", "Her\u00b7zen", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Zorn und Liebe trieben hei\u00dfe Bl\u00fcten,", "tokens": ["Und", "Zorn", "und", "Lie\u00b7be", "trie\u00b7ben", "hei\u00b7\u00dfe", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zu einem Strau\u00df des sch\u00f6nsten Kampfs gebunden:", "tokens": ["Zu", "ei\u00b7nem", "Strau\u00df", "des", "sch\u00f6ns\u00b7ten", "Kampfs", "ge\u00b7bun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des B\u00f6sen Zauber war wie Dunst verschwunden.", "tokens": ["Des", "B\u00f6\u00b7sen", "Zau\u00b7ber", "war", "wie", "Dunst", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "KOKOM", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Verkr\u00e4chzet war das Lied der Schicksalsraben,", "tokens": ["Ver\u00b7kr\u00e4ch\u00b7zet", "war", "das", "Lied", "der", "Schick\u00b7sals\u00b7ra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und es erklang das Siegeslied der Christen", "tokens": ["Und", "es", "er\u00b7klang", "das", "Sie\u00b7ges\u00b7lied", "der", "Chris\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum Gotteskampf vom Greise bis zum Knaben,", "tokens": ["Zum", "Got\u00b7tes\u00b7kampf", "vom", "Grei\u00b7se", "bis", "zum", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gebunden ward der F\u00fcrst der Hinterlisten;", "tokens": ["Ge\u00b7bun\u00b7den", "ward", "der", "F\u00fcrst", "der", "Hin\u00b7ter\u00b7lis\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Er fiel, und zweimal heulte die Hy\u00e4ne", "tokens": ["Er", "fiel", ",", "und", "zwei\u00b7mal", "heul\u00b7te", "die", "Hy\u00b7\u00e4\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Europas, deutsch gegei\u00dfelt, an der Seine.", "tokens": ["Eu\u00b7ro\u00b7pas", ",", "deutsch", "ge\u00b7gei\u00b7\u00dfelt", ",", "an", "der", "Sei\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "VVPP", "$,", "APPR", "ART", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Da, als die V\u00f6lker wachten auf vom Staunen", "tokens": ["Da", ",", "als", "die", "V\u00f6l\u00b7ker", "wach\u00b7ten", "auf", "vom", "Stau\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und lie\u00dfen frisch dem hei\u00dfen Mut den Willen,", "tokens": ["Und", "lie\u00b7\u00dfen", "frisch", "dem", "hei\u00b7\u00dfen", "Mut", "den", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Blies ich mein Pfeifchen auch mit Kriegsposaunen,", "tokens": ["Blies", "ich", "mein", "Pfei\u00b7fchen", "auch", "mit", "Kriegs\u00b7po\u00b7sau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Freiheit lang erstickte Lust zu stillen \u2013", "tokens": ["Der", "Frei\u00b7heit", "lang", "er\u00b7stick\u00b7te", "Lust", "zu", "stil\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da winkten manche Fromme mir den Segen,", "tokens": ["Da", "wink\u00b7ten", "man\u00b7che", "From\u00b7me", "mir", "den", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich f\u00fchrte Federn nur, die bessern Degen.", "tokens": ["Ich", "f\u00fchr\u00b7te", "Fe\u00b7dern", "nur", ",", "die", "bes\u00b7sern", "De\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Bald klingt es Frieden, nach gewalt'gen Dingen", "tokens": ["Bald", "klingt", "es", "Frie\u00b7den", ",", "nach", "ge\u00b7walt'\u00b7gen", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist's allen Herzen wieder stiller worden,", "tokens": ["Ist's", "al\u00b7len", "Her\u00b7zen", "wie\u00b7der", "stil\u00b7ler", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "ADV", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Man l\u00e4\u00dft zum Rhein zur\u00fcck die Trommeln klingen,", "tokens": ["Man", "l\u00e4\u00dft", "zum", "Rhein", "zu\u00b7r\u00fcck", "die", "Trom\u00b7meln", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NE", "PTKVZ", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Man l\u00e4\u00dft die Fahnen wieder wehn gen Norden,", "tokens": ["Man", "l\u00e4\u00dft", "die", "Fah\u00b7nen", "wie\u00b7der", "wehn", "gen", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und jeder suchet froh das liebe Seine,", "tokens": ["Und", "je\u00b7der", "su\u00b7chet", "froh", "das", "lie\u00b7be", "Sei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADJD", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich suche, finde meines mir am Rheine.", "tokens": ["Ich", "su\u00b7che", ",", "fin\u00b7de", "mei\u00b7nes", "mir", "am", "Rhei\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPOSAT", "PPER", "APPRART", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Da tr\u00e4umt' ich Ruh' dem kurzen Rest der Tage,", "tokens": ["Da", "tr\u00e4umt'", "ich", "Ruh'", "dem", "kur\u00b7zen", "Rest", "der", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ach, Traum ist Menschenw\u00fcnschen, Menschenmeinen!", "tokens": ["Ach", ",", "Traum", "ist", "Men\u00b7schen\u00b7w\u00fcn\u00b7schen", ",", "Men\u00b7schen\u00b7mei\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "NN", "VAFIN", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der droben alles w\u00e4gt auf h\u00f6chster Wage,", "tokens": ["Der", "dro\u00b7ben", "al\u00b7les", "w\u00e4gt", "auf", "h\u00f6chs\u00b7ter", "Wa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor dem erlischt der Schein von Erdenscheinen,", "tokens": ["Vor", "dem", "er\u00b7lischt", "der", "Schein", "von", "Er\u00b7den\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Er dr\u00e4ute Wetter meinem stillen Sitze", "tokens": ["Er", "dr\u00e4u\u00b7te", "Wet\u00b7ter", "mei\u00b7nem", "stil\u00b7len", "Sit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und scho\u00df durch meinen Himmel manche Blitze.", "tokens": ["Und", "scho\u00df", "durch", "mei\u00b7nen", "Him\u00b7mel", "man\u00b7che", "Blit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Und endlich lie\u00df er mir von jenen Streichen", "tokens": ["Und", "end\u00b7lich", "lie\u00df", "er", "mir", "von", "je\u00b7nen", "Strei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus hellen Wolken einen niederschmettern,", "tokens": ["Aus", "hel\u00b7len", "Wol\u00b7ken", "ei\u00b7nen", "nie\u00b7der\u00b7schmet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von jenen, welche Locken pl\u00f6tzlich bleichen,", "tokens": ["Von", "je\u00b7nen", ",", "wel\u00b7che", "Lo\u00b7cken", "pl\u00f6tz\u00b7lich", "blei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "$,", "PWAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Leben flugs entbl\u00fcten und entbl\u00e4ttern,", "tokens": ["Das", "Le\u00b7ben", "flugs", "ent\u00b7bl\u00fc\u00b7ten", "und", "ent\u00b7bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Er scho\u00df auf meinen sch\u00f6nsten, schnellsten Knaben,", "tokens": ["Er", "scho\u00df", "auf", "mei\u00b7nen", "sch\u00f6ns\u00b7ten", ",", "schnells\u00b7ten", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ihn hat der Rhein genommen und begraben.", "tokens": ["Ihn", "hat", "der", "Rhein", "ge\u00b7nom\u00b7men", "und", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NE", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Gerecht ist Gott und gut allein und weise,", "tokens": ["Ge\u00b7recht", "ist", "Gott", "und", "gut", "al\u00b7lein", "und", "wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "KON", "ADJD", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er misset jedem zu mit rechtem Ma\u00dfe:", "tokens": ["Er", "mis\u00b7set", "je\u00b7dem", "zu", "mit", "rech\u00b7tem", "Ma\u00b7\u00dfe", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKZU", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer nur die Blumen sucht der Pilgerreise,", "tokens": ["Wer", "nur", "die", "Blu\u00b7men", "sucht", "der", "Pil\u00b7ger\u00b7rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den treibt er fort zur b\u00f6sen Dornenstra\u00dfe:", "to