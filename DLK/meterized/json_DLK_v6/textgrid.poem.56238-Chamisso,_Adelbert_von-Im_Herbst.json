{"textgrid.poem.56238": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Im Herbst", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Niedrig schleicht bla\u00df hin die entnervte Sonne,", "tokens": ["Nied\u00b7rig", "schleicht", "bla\u00df", "hin", "die", "ent\u00b7nerv\u00b7te", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Herbstlich goldgelb f\u00e4rbt sich das Laub, es trauert", "tokens": ["Herbst\u00b7lich", "gold\u00b7gelb", "f\u00e4rbt", "sich", "das", "Laub", ",", "es", "trau\u00b7ert"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PRF", "ART", "NN", "$,", "PPER", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Rings das Feld schon nackt und die Nebel ziehen", "tokens": ["Rings", "das", "Feld", "schon", "nackt", "und", "die", "Ne\u00b7bel", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "ADJD", "KON", "ART", "NN", "VVINF"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "\u00dcber die Stoppeln.", "tokens": ["\u00dc\u00b7ber", "die", "Stop\u00b7peln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Sieh, der Herbst schleicht her und der arge Winter", "tokens": ["Sieh", ",", "der", "Herbst", "schleicht", "her", "und", "der", "ar\u00b7ge", "Win\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "PTKVZ", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Schleicht dem Herbst bald nach, es erstarrt das Leben;", "tokens": ["Schleicht", "dem", "Herbst", "bald", "nach", ",", "es", "er\u00b7starrt", "das", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Ja, das Jahr wird alt, wie ich alt mich f\u00fchle", "tokens": ["Ja", ",", "das", "Jahr", "wird", "alt", ",", "wie", "ich", "alt", "mich", "f\u00fch\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "ADJD", "$,", "PWAV", "PPER", "ADJD", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Selber geworden!", "tokens": ["Sel\u00b7ber", "ge\u00b7wor\u00b7den", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Gute, schreckhaft siehst du mich an, erschrick nicht;", "tokens": ["Gu\u00b7te", ",", "schreck\u00b7haft", "siehst", "du", "mich", "an", ",", "er\u00b7schrick", "nicht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "VVIMP", "PTKNEG", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sieh, das Haupthaar wei\u00df, und des Auges Sehkraft", "tokens": ["Sieh", ",", "das", "Haupt\u00b7haar", "wei\u00df", ",", "und", "des", "Au\u00b7ges", "Seh\u00b7kraft"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Abgestumpft; warm schl\u00e4gt in der Brust das Herz zwar,", "tokens": ["Ab\u00b7ge\u00b7stumpft", ";", "warm", "schl\u00e4gt", "in", "der", "Brust", "das", "Herz", "zwar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "ADJD", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Aber es friert mich!", "tokens": ["A\u00b7ber", "es", "friert", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Naht der Unhold, la\u00df mich ins Aug ihm scharf sehn:", "tokens": ["Naht", "der", "Un\u00b7hold", ",", "la\u00df", "mich", "ins", "Aug", "ihm", "scharf", "sehn", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVIMP", "PPER", "APPRART", "NN", "PPER", "ADJD", "VVINF", "$."], "meter": "+----+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Wahrlich, Furcht nicht fl\u00f6\u00dft er mir ein, er komme,", "tokens": ["Wahr\u00b7lich", ",", "Furcht", "nicht", "fl\u00f6\u00dft", "er", "mir", "ein", ",", "er", "kom\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "PTKNEG", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Nicht bewu\u00dftlos raff er mich hin, ich will ihn", "tokens": ["Nicht", "be\u00b7wu\u00dft\u00b7los", "raff", "er", "mich", "hin", ",", "ich", "will", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "PPER", "VMFIN", "PPER"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Sehen und kennen.", "tokens": ["Se\u00b7hen", "und", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "La\u00df den Wermutstrank mich, den letzten, schl\u00fcrfen,", "tokens": ["La\u00df", "den", "Wer\u00b7muts\u00b7trank", "mich", ",", "den", "letz\u00b7ten", ",", "schl\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPER", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht ein Leichnam l\u00e4ngst, ein verge\u00dfner, schleichen", "tokens": ["Nicht", "ein", "Leich\u00b7nam", "l\u00e4ngst", ",", "ein", "ver\u00b7ge\u00df\u00b7ner", ",", "schlei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PTKNEG", "ART", "NN", "ADV", "$,", "ART", "ADJA", "$,", "VVFIN"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo ich markvoll einst in den Boden Spuren", "tokens": ["Wo", "ich", "mark\u00b7voll", "einst", "in", "den", "Bo\u00b7den", "Spu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Habe getreten.", "tokens": ["Ha\u00b7be", "ge\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Ach! ein Blutstrahl quillt aus dem lieben Herzen:", "tokens": ["Ach", "!", "ein", "Bluts\u00b7trahl", "quillt", "aus", "dem", "lie\u00b7ben", "Her\u00b7zen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Fasse Mut, bleib stark; es vernarbt die Wunde,", "tokens": ["Fas\u00b7se", "Mut", ",", "bleib", "stark", ";", "es", "ver\u00b7narbt", "die", "Wun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "ADJD", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Rein und liebwert hegst du mein Bild im Herzen", "tokens": ["Rein", "und", "lieb\u00b7wert", "hegst", "du", "mein", "Bild", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Nimmer verg\u00e4nglich.", "tokens": ["Nim\u00b7mer", "ver\u00b7g\u00e4ng\u00b7lich", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}