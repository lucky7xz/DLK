{"textgrid.poem.47836": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der Gruft bei den alten S\u00e4rgen", "tokens": ["In", "der", "Gruft", "bei", "den", "al\u00b7ten", "S\u00e4r\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Steht nun ein neuer Sarg,", "tokens": ["Steht", "nun", "ein", "neu\u00b7er", "Sarg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darin vor meiner Liebe", "tokens": ["Da\u00b7rin", "vor", "mei\u00b7ner", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich das s\u00fc\u00dfeste Antlitz barg.", "tokens": ["Sich", "das", "s\u00fc\u00b7\u00dfes\u00b7te", "Ant\u00b7litz", "barg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Den schwarzen Deckel der Truhe", "tokens": ["Den", "schwar\u00b7zen", "De\u00b7ckel", "der", "Tru\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Verh\u00e4ngen die Kr\u00e4nze ganz;", "tokens": ["Ver\u00b7h\u00e4n\u00b7gen", "die", "Kr\u00e4n\u00b7ze", "ganz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein Kranz von Myrtenreisern,", "tokens": ["Ein", "Kranz", "von", "Myr\u00b7ten\u00b7rei\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein wei\u00dfer Syringenkranz.", "tokens": ["Ein", "wei\u00b7\u00dfer", "Sy\u00b7rin\u00b7gen\u00b7kranz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Was noch vor wenig Tagen", "tokens": ["Was", "noch", "vor", "we\u00b7nig", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Wald die Sonne beschien,", "tokens": ["Im", "Wald", "die", "Son\u00b7ne", "be\u00b7schien", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das duftet nun hier unten:", "tokens": ["Das", "duf\u00b7tet", "nun", "hier", "un\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Maililien und Buchengr\u00fcn.", "tokens": ["Mai\u00b7li\u00b7li\u00b7en", "und", "Bu\u00b7chen\u00b7gr\u00fcn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Geschlossen sind die Steine,", "tokens": ["Ge\u00b7schlos\u00b7sen", "sind", "die", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nur oben ein Gitterlein;", "tokens": ["Nur", "o\u00b7ben", "ein", "Git\u00b7ter\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es liegt die geliebte Tote", "tokens": ["Es", "liegt", "die", "ge\u00b7lieb\u00b7te", "To\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Verlassen und allein.", "tokens": ["Ver\u00b7las\u00b7sen", "und", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Vielleicht im Mondenlichte,", "tokens": ["Viel\u00b7leicht", "im", "Mon\u00b7den\u00b7lich\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn die Welt zur Ruhe ging,", "tokens": ["Wenn", "die", "Welt", "zur", "Ru\u00b7he", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Summt noch um die wei\u00dfen Bl\u00fcten", "tokens": ["Summt", "noch", "um", "die", "wei\u00b7\u00dfen", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein dunkler Schmetterling.", "tokens": ["Ein", "dunk\u00b7ler", "Schmet\u00b7ter\u00b7ling", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}