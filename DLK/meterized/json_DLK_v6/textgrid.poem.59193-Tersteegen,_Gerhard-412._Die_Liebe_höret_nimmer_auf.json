{"textgrid.poem.59193": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "412. Die Liebe h\u00f6ret nimmer auf", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die letzte Tugend ist die Liebe,", "tokens": ["Die", "letz\u00b7te", "Tu\u00b7gend", "ist", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein lieblich Salz der Tugend, die ich \u00fcbe;", "tokens": ["Ein", "lieb\u00b7lich", "Salz", "der", "Tu\u00b7gend", ",", "die", "ich", "\u00fc\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie macht die bittern Speisen s\u00fc\u00df,", "tokens": ["Sie", "macht", "die", "bit\u00b7tern", "Spei\u00b7sen", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich das Widrigste mit Lust genie\u00df'.", "tokens": ["Da\u00df", "ich", "das", "Wid\u00b7rigs\u00b7te", "mit", "Lust", "ge\u00b7ni\u00b7e\u00df'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-++--+--+", "measure": "iambic.penta.relaxed"}}}}}