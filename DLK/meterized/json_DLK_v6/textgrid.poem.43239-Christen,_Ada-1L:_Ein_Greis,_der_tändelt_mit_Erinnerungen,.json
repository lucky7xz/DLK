{"textgrid.poem.43239": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Greis, der t\u00e4ndelt mit Erinnerungen,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Greis, der t\u00e4ndelt mit Erinnerungen,", "tokens": ["Ein", "Greis", ",", "der", "t\u00e4n\u00b7delt", "mit", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Welch seltsam Bild! ... Oft mu\u00df das Haupt ich sch\u00fctteln,", "tokens": ["Welch", "selt\u00b7sam", "Bild", "!", "...", "Oft", "mu\u00df", "das", "Haupt", "ich", "sch\u00fct\u00b7teln", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "$.", "$(", "ADV", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mu\u00df mich an meinen eig'nen Schultern r\u00fctteln,", "tokens": ["Mu\u00df", "mich", "an", "mei\u00b7nen", "eig'\u00b7nen", "Schul\u00b7tern", "r\u00fct\u00b7teln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mich d\u00fcnkt, ein Lied h\u00f6r' ich, das Du gesungen;", "tokens": ["Mich", "d\u00fcnkt", ",", "ein", "Lied", "h\u00f6r'", "ich", ",", "das", "Du", "ge\u00b7sun\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die alte Weise hat sich aufgerungen", "tokens": ["Die", "al\u00b7te", "Wei\u00b7se", "hat", "sich", "auf\u00b7ge\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Aus tiefen, reinen, unsichtbaren Saiten,", "tokens": ["Aus", "tie\u00b7fen", ",", "rei\u00b7nen", ",", "un\u00b7sicht\u00b7ba\u00b7ren", "Sai\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Die T\u00f6ne h\u00f6r' ich leis' und leiser gleiten,", "tokens": ["Die", "T\u00f6\u00b7ne", "h\u00f6r'", "ich", "leis'", "und", "lei\u00b7ser", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wer wei\u00df, ob morgen sie nicht ganz verklungen?", "tokens": ["Wer", "wei\u00df", ",", "ob", "mor\u00b7gen", "sie", "nicht", "ganz", "ver\u00b7klun\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "ADV", "PPER", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ob morgen jene Saiten nicht zersprungen,", "tokens": ["Ob", "mor\u00b7gen", "je\u00b7ne", "Sai\u00b7ten", "nicht", "zer\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PDAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ihr letzter Ton nur bebt in diesen R\u00e4umen,", "tokens": ["Ihr", "letz\u00b7ter", "Ton", "nur", "bebt", "in", "die\u00b7sen", "R\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und Reue, Schmerzen, hoffnungsloses Tr\u00e4umen", "tokens": ["Und", "Reu\u00b7e", ",", "Schmer\u00b7zen", ",", "hoff\u00b7nungs\u00b7lo\u00b7ses", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "F\u00fcr ewig \u00fcberwunden und bezwungen ...", "tokens": ["F\u00fcr", "e\u00b7wig", "\u00fc\u00b7berw\u00b7un\u00b7den", "und", "be\u00b7zwun\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}