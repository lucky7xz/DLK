{"textgrid.poem.53495": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "An einen garnisondienstf\u00e4higen Dichter", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du schl\u00e4gst die kriegerisch-verstimmte Leier,", "tokens": ["Du", "schl\u00e4gst", "die", "krie\u00b7ge\u00b7rischver\u00b7stimm\u00b7te", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "du singst von Ha\u00df und Blut und Pulverrauch \u2013", "tokens": ["du", "singst", "von", "Ha\u00df", "und", "Blut", "und", "Pul\u00b7ver\u00b7rauch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und hei\u00dft vielleicht nur Gottlob Emil Meier,", "tokens": ["und", "hei\u00dft", "viel\u00b7leicht", "nur", "Gott\u00b7lob", "E\u00b7mil", "Mei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "NN", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "sanft w\u00f6lbt sich dir der Zwei-Terrassen-Bauch . . .", "tokens": ["sanft", "w\u00f6lbt", "sich", "dir", "der", "Zwei\u00b7Ter\u00b7ras\u00b7sen\u00b7Bauch", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "PPER", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du singst vom Sturmangriff, von roten Hosen,", "tokens": ["Du", "singst", "vom", "Stur\u00b7man\u00b7griff", ",", "von", "ro\u00b7ten", "Ho\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "von England-Ha\u00df, von Not und Schlachtengraus,", "tokens": ["von", "Eng\u00b7lan\u00b7dHa\u00df", ",", "von", "Not", "und", "Schlach\u00b7ten\u00b7graus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "vom Panjefeind und von den Erzfranzosen \u2013", "tokens": ["vom", "Pan\u00b7je\u00b7feind", "und", "von", "den", "Erz\u00b7fran\u00b7zo\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Komm raus!", "tokens": ["Komm", "raus", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Komm einmal raus! Besieh dir das pers\u00f6nlich \u2013", "tokens": ["Komm", "ein\u00b7mal", "raus", "!", "Be\u00b7sieh", "dir", "das", "per\u00b7s\u00f6n\u00b7lich", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$.", "VVFIN", "PPER", "ART", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "gewi\u00df: es ist nicht immer ideal,", "tokens": ["ge\u00b7wi\u00df", ":", "es", "ist", "nicht", "im\u00b7mer", "i\u00b7deal", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "mitunter gehts im Kriege ganz gew\u00f6hnlich", "tokens": ["mi\u00b7tun\u00b7ter", "gehts", "im", "Krie\u00b7ge", "ganz", "ge\u00b7w\u00f6hn\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und schmutzig zu \u2013 besiehs dir nur einmal.", "tokens": ["und", "schmut\u00b7zig", "zu", "\u2013", "be\u00b7siehs", "dir", "nur", "ein\u00b7mal", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKZU", "$(", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nein! das gen\u00fcgt noch nicht: du mu\u00dft es auch erleben,", "tokens": ["Nein", "!", "das", "ge\u00b7n\u00fcgt", "noch", "nicht", ":", "du", "mu\u00dft", "es", "auch", "er\u00b7le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PDS", "VVFIN", "ADV", "PTKNEG", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "zieh an die schlichte Farbe unsres Graus.", "tokens": ["zieh", "an", "die", "schlich\u00b7te", "Far\u00b7be", "uns\u00b7res", "Graus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mach mit! Wir wolln dir f\u00fcnf Mark drei\u00dfig geben \u2013", "tokens": ["Mach", "mit", "!", "Wir", "wolln", "dir", "f\u00fcnf", "Mark", "drei\u00b7\u00dfig", "ge\u00b7ben", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "PPER", "VMFIN", "PPER", "CARD", "NN", "CARD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Komm raus!", "tokens": ["Komm", "raus", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Vielleicht wirst du dann endlich, endlich lernen:", "tokens": ["Viel\u00b7leicht", "wirst", "du", "dann", "end\u00b7lich", ",", "end\u00b7lich", "ler\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer seine Pflicht tut, k\u00e4mpft und steht und schweigt.", "tokens": ["Wer", "sei\u00b7ne", "Pflicht", "tut", ",", "k\u00e4mpft", "und", "steht", "und", "schweigt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Steigt auch der Ruhm der Kameraden zu den Sternen \u2013", "tokens": ["Steigt", "auch", "der", "Ruhm", "der", "Ka\u00b7me\u00b7ra\u00b7den", "zu", "den", "Ster\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "sieh nur, wie lautlos und wie still das steigt!", "tokens": ["sieh", "nur", ",", "wie", "laut\u00b7los", "und", "wie", "still", "das", "steigt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PWAV", "ADJD", "KON", "PWAV", "ADJD", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch ziehn wir sp\u00e4ter einmal (Gott mag wissen,", "tokens": ["Doch", "ziehn", "wir", "sp\u00e4\u00b7ter", "ein\u00b7mal", "(", "Gott", "mag", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "$(", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "wann das geschieht), zur\u00fcck, sind Leid und Wirrsal aus:", "tokens": ["wann", "das", "ge\u00b7schieht", ")", ",", "zu\u00b7r\u00fcck", ",", "sind", "Leid", "und", "Wirr\u00b7sal", "aus", ":"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVFIN", "$(", "$,", "PTKVZ", "$,", "VAFIN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "dann, Meier, wollen wir dich gerne missen!", "tokens": ["dann", ",", "Mei\u00b7er", ",", "wol\u00b7len", "wir", "dich", "ger\u00b7ne", "mis\u00b7sen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dann bleib zu Haus!", "tokens": ["Dann", "bleib", "zu", "Haus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}