{"textgrid.poem.42606": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "3. Die Drossel", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbich will ja nicht zum Garten gehn,", "tokens": ["\u00bb", "ich", "will", "ja", "nicht", "zum", "Gar\u00b7ten", "gehn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADV", "PTKNEG", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will liegen sommerlang,", "tokens": ["Will", "lie\u00b7gen", "som\u00b7mer\u00b7lang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "H\u00f6rt ich die lust'ge Drossel nur,", "tokens": ["H\u00f6rt", "ich", "die", "lust'\u00b7ge", "Dros\u00b7sel", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die in dem Busche sang.\u00ab", "tokens": ["Die", "in", "dem", "Bu\u00b7sche", "sang", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Man f\u00e4ngt dem Kind die Drossel ein,", "tokens": ["Man", "f\u00e4ngt", "dem", "Kind", "die", "Dros\u00b7sel", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im K\u00e4fig sitzt sie dort,", "tokens": ["Im", "K\u00e4\u00b7fig", "sitzt", "sie", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch singen will sie nicht und h\u00e4ngt", "tokens": ["Doch", "sin\u00b7gen", "will", "sie", "nicht", "und", "h\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PTKNEG", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr K\u00f6pfchen immerfort.", "tokens": ["Ihr", "K\u00f6pf\u00b7chen", "im\u00b7mer\u00b7fort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Noch einmal blickt das Kind nach ihr", "tokens": ["Noch", "ein\u00b7mal", "blickt", "das", "Kind", "nach", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit bittendem Gesicht,", "tokens": ["Mit", "bit\u00b7ten\u00b7dem", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da schl\u00e4gt die Drossel sch\u00f6n und hell,", "tokens": ["Da", "schl\u00e4gt", "die", "Dros\u00b7sel", "sch\u00f6n", "und", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da gl\u00e4nzt sein Aug und bricht.", "tokens": ["Da", "gl\u00e4nzt", "sein", "Aug", "und", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}