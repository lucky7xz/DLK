{"dta.poem.11869": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du mit deinem Schatz willst einen Bretterkasten,", "tokens": ["Wenn", "du", "mit", "dei\u00b7nem", "Schatz", "willst", "ei\u00b7nen", "Bret\u00b7ter\u00b7kas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit Geheimnissen ein Frauenherz belasten;", "tokens": ["Und", "mit", "Ge\u00b7heim\u00b7nis\u00b7sen", "ein", "Frau\u00b7en\u00b7herz", "be\u00b7las\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Besorge, da\u00df ein Dieb den Kasten dir erbreche,", "tokens": ["Be\u00b7sor\u00b7ge", ",", "da\u00df", "ein", "Dieb", "den", "Kas\u00b7ten", "dir", "er\u00b7bre\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bef\u00fcrchte, da\u00df dein Lieb das Schweigen brech' und spreche.", "tokens": ["Be\u00b7f\u00fcrch\u00b7te", ",", "da\u00df", "dein", "Lieb", "das", "Schwei\u00b7gen", "brech'", "und", "spre\u00b7che", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum trage deinen Schatz bei dir in deinem Seckel,", "tokens": ["Drum", "tra\u00b7ge", "dei\u00b7nen", "Schatz", "bei", "dir", "in", "dei\u00b7nem", "Se\u00b7ckel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und deine Heimlichkeit bewahrt vom Busendeckel.", "tokens": ["Und", "dei\u00b7ne", "Heim\u00b7lich\u00b7keit", "be\u00b7wahrt", "vom", "Bu\u00b7sen\u00b7de\u00b7ckel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein Schatz ist sicher auch im Seckel nicht zu tragen,", "tokens": ["Ein", "Schatz", "ist", "si\u00b7cher", "auch", "im", "Se\u00b7ckel", "nicht", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADV", "APPRART", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch immer sicher ein Geheimnis nicht zu sagen.", "tokens": ["Doch", "im\u00b7mer", "si\u00b7cher", "ein", "Ge\u00b7heim\u00b7nis", "nicht", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}