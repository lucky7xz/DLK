{"textgrid.poem.66879": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nachtfalter, frischgeschminkten Angesichts,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nachtfalter, frischgeschminkten Angesichts,", "tokens": ["Nacht\u00b7fal\u00b7ter", ",", "frischge\u00b7schmink\u00b7ten", "An\u00b7ge\u00b7sichts", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schwirren im leichenfahlen Schein des Lichts.", "tokens": ["Schwir\u00b7ren", "im", "lei\u00b7chen\u00b7fah\u00b7len", "Schein", "des", "Lichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Mit hagerer Gestalt vor\u00fcberwehn", "tokens": ["Mit", "ha\u00b7ge\u00b7rer", "Ge\u00b7stalt", "vor\u00b7\u00fc\u00b7berw\u00b7ehn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Abgr\u00fcndler, die Schakalen \u00e4hnlich sehn.", "tokens": ["Ab\u00b7gr\u00fcnd\u00b7ler", ",", "die", "Scha\u00b7ka\u00b7len", "\u00e4hn\u00b7lich", "sehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ihr Blick, der hinterr\u00fccks dich noch zerfleischt,", "tokens": ["Ihr", "Blick", ",", "der", "hin\u00b7ter\u00b7r\u00fccks", "dich", "noch", "zer\u00b7fleischt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Durchschauert dich. In diesem Blicke kreischt", "tokens": ["Durch\u00b7schau\u00b7ert", "dich", ".", "In", "die\u00b7sem", "Bli\u00b7cke", "kreischt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein Ha\u00df, der, wenn der Wunsch die Tat gew\u00e4hrt,", "tokens": ["Ein", "Ha\u00df", ",", "der", ",", "wenn", "der", "Wunsch", "die", "Tat", "ge\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "KOUS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dir rachedurstig an die Gurgel f\u00e4hrt.", "tokens": ["Dir", "ra\u00b7che\u00b7durs\u00b7tig", "an", "die", "Gur\u00b7gel", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Du fragst und suchst: Woher fiel je ein Strahl", "tokens": ["Du", "fragst", "und", "suchst", ":", "Wo\u00b7her", "fiel", "je", "ein", "Strahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "PWAV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "In dieser Augenh\u00f6hlen H\u00f6llental? \u2013", "tokens": ["In", "die\u00b7ser", "Au\u00b7gen\u00b7h\u00f6h\u00b7len", "H\u00f6l\u00b7len\u00b7tal", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Vor Spiegelscheiben aufgeputzte Fraun,", "tokens": ["Vor", "Spie\u00b7gel\u00b7schei\u00b7ben", "auf\u00b7ge\u00b7putz\u00b7te", "Fraun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die schwatzend Ballkost\u00fcme sich beschaun.", "tokens": ["Die", "schwat\u00b7zend", "Ball\u00b7kos\u00b7t\u00fc\u00b7me", "sich", "be\u00b7schaun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ihr Herzenswunsch \u2013 mit Wahrung nobler Sitten \u2013", "tokens": ["Ihr", "Her\u00b7zens\u00b7wunsch", "\u2013", "mit", "Wah\u00b7rung", "nob\u00b7ler", "Sit\u00b7ten", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "APPR", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Durchbrochne Spitzen, Herzform ausgeschnitten.", "tokens": ["Durch\u00b7broch\u00b7ne", "Spit\u00b7zen", ",", "Herz\u00b7form", "aus\u00b7ge\u00b7schnit\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "\u00bbwozu hat der Gemahl sein Portemonnaie?", "tokens": ["\u00bb", "wo\u00b7zu", "hat", "der", "Ge\u00b7mahl", "sein", "Por\u00b7te\u00b7mon\u00b7nai\u00b7e", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Zwar manchmal sind die M\u00e4nner schrecklich z\u00e4h.", "tokens": ["Zwar", "manch\u00b7mal", "sind", "die", "M\u00e4n\u00b7ner", "schreck\u00b7lich", "z\u00e4h", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Man lebt nur einmal. Die Saison ist kurz,", "tokens": ["Man", "lebt", "nur", "ein\u00b7mal", ".", "Die", "Sai\u00b7son", "ist", "kurz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "$.", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Mi\u00df Duncan tanzt ja freilich halb im Schurz,", "tokens": ["Mi\u00df", "Dun\u00b7can", "tanzt", "ja", "frei\u00b7lich", "halb", "im", "Schurz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ADV", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Blo\u00dfbeinig ", "tokens": ["Blo\u00df\u00b7bei\u00b7nig"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.20": {"text": "Und k\u00f6nnten meterweis am Stoffe sparen ...\u00ab", "tokens": ["Und", "k\u00f6nn\u00b7ten", "me\u00b7ter\u00b7weis", "am", "Stof\u00b7fe", "spa\u00b7ren", "...", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VMFIN", "ADV", "APPRART", "NN", "VVINF", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Ein Dienstmann h\u00e4lt mir einen Zettel zu.", "tokens": ["Ein", "Dienst\u00b7mann", "h\u00e4lt", "mir", "ei\u00b7nen", "Zet\u00b7tel", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "\u00bbhochelegant! Der Fremden Rendezvous.", "tokens": ["\u00bb", "hoc\u00b7he\u00b7le\u00b7gant", "!", "Der", "Frem\u00b7den", "Ren\u00b7dez\u00b7vous", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$.", "ART", "NN", "NE", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.23": {"text": "Gleich um die Ecke rechts. Zum Venusgarten.", "tokens": ["Gleich", "um", "die", "E\u00b7cke", "rechts", ".", "Zum", "Ve\u00b7nus\u00b7gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "$.", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Zehn fesche Weiber da, um aufzuwarten.", "tokens": ["Zehn", "fe\u00b7sche", "Wei\u00b7ber", "da", ",", "um", "auf\u00b7zu\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "PTKVZ", "$,", "KOUI", "VVIZU", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.25": {"text": "Man am\u00fcsiert sich k\u00f6stlich bei Konzert.", "tokens": ["Man", "a\u00b7m\u00fc\u00b7siert", "sich", "k\u00f6st\u00b7lich", "bei", "Kon\u00b7zert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADJD", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Bumsvallera!\u00ab \u2013 Mein Beutel bleibt gesperrt", "tokens": ["Bums\u00b7val\u00b7le\u00b7ra", "!", "\u00ab", "\u2013", "Mein", "Beu\u00b7tel", "bleibt", "ge\u00b7sperrt"], "token_info": ["word", "punct", "punct", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "$(", "$(", "PPOSAT", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "F\u00fcr Aphroditens Taubenstra\u00dfenzwecke,", "tokens": ["F\u00fcr", "A\u00b7phro\u00b7di\u00b7tens", "Tau\u00b7ben\u00b7stra\u00b7\u00df\u00b7en\u00b7zwe\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,"], "meter": "-----+--+-+-", "measure": "iambic.tri.relaxed"}, "line.28": {"text": "Verschm\u00e4ht sind die Sirenen \u00bbum die Ecke\u00ab.", "tokens": ["Ver\u00b7schm\u00e4ht", "sind", "die", "Si\u00b7re\u00b7nen", "\u00bb", "um", "die", "E\u00b7cke", "\u00ab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$(", "APPR", "ART", "NN", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "So weiterstreifend durch die Menschenmasse", "tokens": ["So", "wei\u00b7ter\u00b7strei\u00b7fend", "durch", "die", "Men\u00b7schen\u00b7mas\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Seh ich auf einmal aus der Nebengasse", "tokens": ["Seh", "ich", "auf", "ein\u00b7mal", "aus", "der", "Ne\u00b7ben\u00b7gas\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Den \u00e4rmsten Kr\u00fcppel mit 'nem kreidebleichen", "tokens": ["Den", "\u00e4rms\u00b7ten", "Kr\u00fcp\u00b7pel", "mit", "'nem", "krei\u00b7de\u00b7blei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Gesicht Streichh\u00f6lzchen armausstreckend reichen ...", "tokens": ["Ge\u00b7sicht", "Streich\u00b7h\u00f6lz\u00b7chen", "ar\u00b7maus\u00b7stre\u00b7ckend", "rei\u00b7chen", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Halb noch ein J\u00fcngling, stiert auf einen Punkt", "tokens": ["Halb", "noch", "ein", "J\u00fcng\u00b7ling", ",", "stiert", "auf", "ei\u00b7nen", "Punkt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "So vor sich hin, die Beine abgestrunkt \u2013", "tokens": ["So", "vor", "sich", "hin", ",", "die", "Bei\u00b7ne", "ab\u00b7ge\u00b7strunkt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "PTKVZ", "$,", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Es steckt der Rumpf in einem Kinderkarren,", "tokens": ["Es", "steckt", "der", "Rumpf", "in", "ei\u00b7nem", "Kin\u00b7der\u00b7kar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Die Augen glanzlos, gl\u00fccklos, trostlos starren,", "tokens": ["Die", "Au\u00b7gen", "glanz\u00b7los", ",", "gl\u00fcc\u00b7klos", ",", "trost\u00b7los", "star\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ADJD", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Starren wie ein zerschossnes junges Wild,", "tokens": ["Star\u00b7ren", "wie", "ein", "zer\u00b7schoss\u00b7nes", "jun\u00b7ges", "Wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.38": {"text": "Verendend auf des J\u00e4gers Weidgefild.", "tokens": ["Ver\u00b7en\u00b7dend", "auf", "des", "J\u00e4\u00b7gers", "Weid\u00b7ge\u00b7fild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Starren auf einen Punkt in einem fort,", "tokens": ["Star\u00b7ren", "auf", "ei\u00b7nen", "Punkt", "in", "ei\u00b7nem", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "ART", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.40": {"text": "Das Pflaster schreit: \u00bbVerst\u00fcmmelt und verdorrt!\u00ab", "tokens": ["Das", "Pflas\u00b7ter", "schreit", ":", "\u00bb", "Ver\u00b7st\u00fcm\u00b7melt", "und", "ver\u00b7dorrt", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "VVPP", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Ich geh vorbei. Was h\u00e4lt die Hand zur\u00fcck?", "tokens": ["Ich", "geh", "vor\u00b7bei", ".", "Was", "h\u00e4lt", "die", "Hand", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PWS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Ich sch\u00e4me mich mit dem Zehnpfennigst\u00fcck ...", "tokens": ["Ich", "sch\u00e4\u00b7me", "mich", "mit", "dem", "Zehn\u00b7pfen\u00b7nig\u00b7st\u00fcck", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.43": {"text": "Und zwanzig Schritte weiter geb ich's aus", "tokens": ["Und", "zwan\u00b7zig", "Schrit\u00b7te", "wei\u00b7ter", "geb", "ich's", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "ADV", "VVFIN", "PIS", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "F\u00fcr einen kleinen, duftigen Veilchenstrau\u00df.", "tokens": ["F\u00fcr", "ei\u00b7nen", "klei\u00b7nen", ",", "duf\u00b7ti\u00b7gen", "Veil\u00b7chen\u00b7strau\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}