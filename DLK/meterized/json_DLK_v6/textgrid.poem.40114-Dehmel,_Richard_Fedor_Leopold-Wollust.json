{"textgrid.poem.40114": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Wollust", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In w\u00fcster Schmach Vergeudung heil'ger Glut", "tokens": ["In", "w\u00fcs\u00b7ter", "Schmach", "Ver\u00b7geu\u00b7dung", "heil'\u00b7ger", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ist Wollust, wenn sie pra\u00dft, \u2013 und leergepra\u00dft", "tokens": ["ist", "Wol\u00b7lust", ",", "wenn", "sie", "pra\u00dft", ",", "\u2013", "und", "leer\u00b7ge\u00b7pra\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["VAFIN", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "$(", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "bricht Schw\u00fcre sie, verleumdet, l\u00e4stert, ha\u00dft,", "tokens": ["bricht", "Schw\u00fc\u00b7re", "sie", ",", "ver\u00b7leum\u00b7det", ",", "l\u00e4s\u00b7tert", ",", "ha\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "$,", "VVPP", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "buhlt mit dem Grauen, bangt und giert nach Blut, \u2013", "tokens": ["buhlt", "mit", "dem", "Grau\u00b7en", ",", "bangt", "und", "giert", "nach", "Blut", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$,", "$("], "meter": "---+-+-+-+", "measure": "zehnsilber"}}, "stanza.2": {"line.1": {"text": "ges\u00e4ttigt kaum, von Ekel schon gehetzt, \u2013", "tokens": ["ge\u00b7s\u00e4t\u00b7tigt", "kaum", ",", "von", "E\u00b7kel", "schon", "ge\u00b7hetzt", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "ADV", "$,", "APPR", "NN", "ADV", "VVPP", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "sinnlose L\u00fcsternheit und, kaum verraucht,", "tokens": ["sinn\u00b7lo\u00b7se", "L\u00fcs\u00b7tern\u00b7heit", "und", ",", "kaum", "ver\u00b7raucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "$,", "ADV", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "sinnlose D\u00fcsterkeit, in Wut getaucht,", "tokens": ["sinn\u00b7lo\u00b7se", "D\u00fcs\u00b7ter\u00b7keit", ",", "in", "Wut", "ge\u00b7taucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "als h\u00e4tt' ein Tollkraut die Vernunft zerfetzt, \u2013", "tokens": ["als", "h\u00e4tt'", "ein", "Toll\u00b7kraut", "die", "Ver\u00b7nunft", "zer\u00b7fetzt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOKOM", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "ma\u00dflos im Rausch, im Taumel, in der Wahl, \u2013", "tokens": ["ma\u00df\u00b7los", "im", "Rausch", ",", "im", "Tau\u00b7mel", ",", "in", "der", "Wahl", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,", "APPRART", "NN", "$,", "APPR", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "im Wunsche Wahnsinn, Wahnsinn in der Brunst, \u2013", "tokens": ["im", "Wun\u00b7sche", "Wahn\u00b7sinn", ",", "Wahn\u00b7sinn", "in", "der", "Brunst", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "NN", "APPR", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "erd\u00fcrstet Ueppigkeit, genossen Dunst, \u2013", "tokens": ["er\u00b7d\u00fcrs\u00b7tet", "Uep\u00b7pig\u00b7keit", ",", "ge\u00b7nos\u00b7sen", "Dunst", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVPP", "NN", "$,", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "verz\u00fcckt vor Wonne, dann erdr\u00fcckt von Qual ...", "tokens": ["ver\u00b7z\u00fcckt", "vor", "Won\u00b7ne", ",", "dann", "er\u00b7dr\u00fcckt", "von", "Qual", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "ADV", "VVPP", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ach, Jeder ", "tokens": ["Ach", ",", "Je\u00b7der"], "token_info": ["word", "punct", "word"], "pos": ["ITJ", "$,", "PIS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "zu dieser H\u00f6lle diesen Himmelssteg!", "tokens": ["zu", "die\u00b7ser", "H\u00f6l\u00b7le", "die\u00b7sen", "Him\u00b7melss\u00b7teg", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}