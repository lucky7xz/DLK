{"textgrid.poem.66043": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Sonnenblumen", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Abend zwischen Traum und Wachen,", "tokens": ["Am", "A\u00b7bend", "zwi\u00b7schen", "Traum", "und", "Wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich dachte nicht grad an heilige Sachen,", "tokens": ["Ich", "dach\u00b7te", "nicht", "grad", "an", "hei\u00b7li\u00b7ge", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Vor mir der Nazarener stand.", "tokens": ["Vor", "mir", "der", "Na\u00b7za\u00b7re\u00b7ner", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Die sch\u00f6nen Gottesaugen lagen", "tokens": ["Die", "sch\u00f6\u00b7nen", "Got\u00b7tes\u00b7au\u00b7gen", "la\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auf mir wie zwei freundliche Fragen.", "tokens": ["Auf", "mir", "wie", "zwei", "freund\u00b7li\u00b7che", "Fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KOKOM", "CARD", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Hielt eine Blume in der Hand,", "tokens": ["Hielt", "ei\u00b7ne", "Blu\u00b7me", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hochstengelig ein goldener Stern", "tokens": ["Hochs\u00b7ten\u00b7ge\u00b7lig", "ein", "gol\u00b7de\u00b7ner", "Stern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Lehnt an der Schulter unserm Herrn,", "tokens": ["Lehnt", "an", "der", "Schul\u00b7ter", "un\u00b7serm", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wie frommer Maler Engelsgestalten", "tokens": ["Wie", "from\u00b7mer", "Ma\u00b7ler", "En\u00b7gels\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Ihre Friedenspalmen halten:", "tokens": ["Ih\u00b7re", "Frie\u00b7dens\u00b7pal\u00b7men", "hal\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Eine Sonnenblume, voll erschlossen,", "tokens": ["Ei\u00b7ne", "Son\u00b7nen\u00b7blu\u00b7me", ",", "voll", "er\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Von einem lieblichen Licht umflossen,", "tokens": ["Von", "ei\u00b7nem", "lieb\u00b7li\u00b7chen", "Licht", "um\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Hob sich von seinem blauen Kleid", "tokens": ["Hob", "sich", "von", "sei\u00b7nem", "blau\u00b7en", "Kleid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Als ein gl\u00e4nzendes Geschmeid.", "tokens": ["Als", "ein", "gl\u00e4n\u00b7zen\u00b7des", "Ge\u00b7schmeid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "So schwebte wie ein Nebel zart", "tokens": ["So", "schweb\u00b7te", "wie", "ein", "Ne\u00b7bel", "zart"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOKOM", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Vor mir die g\u00f6ttliche Gegenwart,", "tokens": ["Vor", "mir", "die", "g\u00f6tt\u00b7li\u00b7che", "Ge\u00b7gen\u00b7wart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Darauf ich holden Schreckens geblickt,", "tokens": ["Da\u00b7rauf", "ich", "hol\u00b7den", "Schre\u00b7ckens", "ge\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.18": {"text": "Bis ich dar\u00fcber eingenickt.", "tokens": ["Bis", "ich", "da\u00b7r\u00fc\u00b7ber", "ein\u00b7ge\u00b7nickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Am Morgen, nach gesundem Schlaf,", "tokens": ["Am", "Mor\u00b7gen", ",", "nach", "ge\u00b7sun\u00b7dem", "Schlaf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stand mir der Sinn ins Feld hinaus,", "tokens": ["Stand", "mir", "der", "Sinn", "ins", "Feld", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ich auf eine H\u00fctte traf,", "tokens": ["Wo", "ich", "auf", "ei\u00b7ne", "H\u00fct\u00b7te", "traf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein leicht gezimmert h\u00f6lzern Haus.", "tokens": ["Ein", "leicht", "ge\u00b7zim\u00b7mert", "h\u00f6l\u00b7zern", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum ragten als ein Schirm und Zaun,", "tokens": ["Drum", "rag\u00b7ten", "als", "ein", "Schirm", "und", "Zaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "KOKOM", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als ein golden Gegitter anzuschaun,", "tokens": ["Als", "ein", "gol\u00b7den", "Ge\u00b7git\u00b7ter", "an\u00b7zu\u00b7schaun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVIZU", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Hochs\u00e4ulig aufgereiht beisammen,", "tokens": ["Hoch\u00b7s\u00e4u\u00b7lig", "auf\u00b7ge\u00b7reiht", "bei\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sonnenblumen, zehn helle Flammen.", "tokens": ["Son\u00b7nen\u00b7blu\u00b7men", ",", "zehn", "hel\u00b7le", "Flam\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "CARD", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Das war ein dichterlicher Platz,", "tokens": ["Das", "war", "ein", "dich\u00b7ter\u00b7li\u00b7cher", "Platz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie nur am Wege hold versteckt", "tokens": ["Wie", "nur", "am", "We\u00b7ge", "hold", "ver\u00b7steckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Sonntagskind ihn einmal entdeckt.", "tokens": ["Ein", "Sonn\u00b7tags\u00b7kind", "ihn", "ein\u00b7mal", "ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ein W\u00e4sserlein lief mit s\u00fc\u00dfem Geschwatz", "tokens": ["Ein", "W\u00e4s\u00b7ser\u00b7lein", "lief", "mit", "s\u00fc\u00b7\u00dfem", "Ge\u00b7schwatz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Durch eine schattige Wiese hin,", "tokens": ["Durch", "ei\u00b7ne", "schat\u00b7ti\u00b7ge", "Wie\u00b7se", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sonst war die Stille hier K\u00f6nigin;", "tokens": ["Sonst", "war", "die", "Stil\u00b7le", "hier", "K\u00f6\u00b7ni\u00b7gin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ihr K\u00f6nig, der Frieden, sa\u00df auf der Bank", "tokens": ["Ihr", "K\u00f6\u00b7nig", ",", "der", "Frie\u00b7den", ",", "sa\u00df", "auf", "der", "Bank"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und putzte seine Krone blank.", "tokens": ["Und", "putz\u00b7te", "sei\u00b7ne", "Kro\u00b7ne", "blank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So oft ich dem H\u00e4uschen vor\u00fcbergeh,", "tokens": ["So", "oft", "ich", "dem", "H\u00e4usc\u00b7hen", "vor\u00b7\u00fc\u00b7ber\u00b7geh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ein blau Gewand ich vor mir seh.", "tokens": ["Ein", "blau", "Ge\u00b7wand", "ich", "vor", "mir", "seh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geht nicht, steht nicht, schwebt vielmehr", "tokens": ["Geht", "nicht", ",", "steht", "nicht", ",", "schwebt", "viel\u00b7mehr"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "VVFIN", "PTKNEG", "$,", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In einiger H\u00f6he vor mir her.", "tokens": ["In", "ei\u00b7ni\u00b7ger", "H\u00f6\u00b7he", "vor", "mir", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sch\u00f6ne Gottesaugen schlagen", "tokens": ["Sch\u00f6\u00b7ne", "Got\u00b7tes\u00b7au\u00b7gen", "schla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich nach mir auf mit freundlichem Fragen,", "tokens": ["Sich", "nach", "mir", "auf", "mit", "freund\u00b7li\u00b7chem", "Fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und von der Schulter unserm Herrn", "tokens": ["Und", "von", "der", "Schul\u00b7ter", "un\u00b7serm", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nickt schwankend der goldne Bl\u00e4tterstern,", "tokens": ["Nickt", "schwan\u00b7kend", "der", "gold\u00b7ne", "Bl\u00e4t\u00b7ters\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Die Sonnenblume, voll erbl\u00fcht,", "tokens": ["Die", "Son\u00b7nen\u00b7blu\u00b7me", ",", "voll", "er\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Von einem himmlischen Leuchten umgl\u00fcht.", "tokens": ["Von", "ei\u00b7nem", "himm\u00b7li\u00b7schen", "Leuch\u00b7ten", "um\u00b7gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "War nie diesen Blumen recht gut gewesen.", "tokens": ["War", "nie", "die\u00b7sen", "Blu\u00b7men", "recht", "gut", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PDAT", "NN", "ADV", "ADJD", "VAPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Schalt sie b\u00e4uerisch und gemein,", "tokens": ["Schalt", "sie", "b\u00e4u\u00b7e\u00b7risch", "und", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kamen mir vor wie K\u00fcchenbesen,", "tokens": ["Ka\u00b7men", "mir", "vor", "wie", "K\u00fc\u00b7chen\u00b7be\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KOKOM", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die gerne wollten Prinzessinnen sein.", "tokens": ["Die", "ger\u00b7ne", "woll\u00b7ten", "Prin\u00b7zes\u00b7sin\u00b7nen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VMFIN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Aber so l\u00e4\u00dft, was wir verachtet,", "tokens": ["A\u00b7ber", "so", "l\u00e4\u00dft", ",", "was", "wir", "ver\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Ehs dr\u00fcber getagt nur oder genachtet,", "tokens": ["Ehs", "dr\u00fc\u00b7ber", "ge\u00b7tagt", "nur", "o\u00b7der", "ge\u00b7nach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PAV", "VVPP", "ADV", "KON", "VVPP", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Oft pl\u00f6tzlich die schlichte H\u00fclle sinken", "tokens": ["Oft", "pl\u00f6tz\u00b7lich", "die", "schlich\u00b7te", "H\u00fcl\u00b7le", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und uns seine heimliche Sch\u00f6nheit trinken.", "tokens": ["Und", "uns", "sei\u00b7ne", "heim\u00b7li\u00b7che", "Sch\u00f6n\u00b7heit", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Besonders Poeten kommen oft", "tokens": ["Be\u00b7son\u00b7ders", "Po\u00b7et\u00b7en", "kom\u00b7men", "oft"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Zu solchen Gnaden unverhofft.", "tokens": ["Zu", "sol\u00b7chen", "Gna\u00b7den", "un\u00b7ver\u00b7hofft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}