{"textgrid.poem.45524": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "In Cruce Spes Mea", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stellst deine Hoffnung du aufs Kreuz?", "tokens": ["Stellst", "dei\u00b7ne", "Hoff\u00b7nung", "du", "aufs", "Kreuz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir wollen um ein Wort nicht balgen.", "tokens": ["Wir", "wol\u00b7len", "um", "ein", "Wort", "nicht", "bal\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein gleicher Wunsch lebt unsrerseits,", "tokens": ["Ein", "glei\u00b7cher", "Wunsch", "lebt", "uns\u00b7rer\u00b7seits", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir s\u00e4hen l\u00e4ngst dich gern am Galgen.", "tokens": ["Wir", "s\u00e4\u00b7hen", "l\u00e4ngst", "dich", "gern", "am", "Gal\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}