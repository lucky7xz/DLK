{"textgrid.poem.34804": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sprach der Herr am sechsten Tage:", "tokens": ["Sprach", "der", "Herr", "am", "sechs\u00b7ten", "Ta\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbhab am Ende nun vollbracht", "tokens": ["\u00bb", "hab", "am", "En\u00b7de", "nun", "voll\u00b7bracht"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VAFIN", "APPRART", "NN", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese gro\u00dfe, sch\u00f6ne Sch\u00f6pfung,", "tokens": ["Die\u00b7se", "gro\u00b7\u00dfe", ",", "sch\u00f6\u00b7ne", "Sch\u00f6p\u00b7fung", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und hab alles gut gemacht.", "tokens": ["Und", "hab", "al\u00b7les", "gut", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie die Sonne rosengoldig", "tokens": ["Wie", "die", "Son\u00b7ne", "ro\u00b7sen\u00b7gol\u00b7dig"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Meere widerstrahlt!", "tokens": ["In", "dem", "Mee\u00b7re", "wi\u00b7der\u00b7strahlt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die B\u00e4ume gr\u00fcn und gl\u00e4nzend!", "tokens": ["Wie", "die", "B\u00e4u\u00b7me", "gr\u00fcn", "und", "gl\u00e4n\u00b7zend", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist nicht alles wie gemalt?", "tokens": ["Ist", "nicht", "al\u00b7les", "wie", "ge\u00b7malt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PIS", "KOKOM", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sind nicht wei\u00df wie Alabaster", "tokens": ["Sind", "nicht", "wei\u00df", "wie", "A\u00b7la\u00b7bas\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "VVFIN", "KOKOM", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dort die L\u00e4mmchen auf der Flur?", "tokens": ["Dort", "die", "L\u00e4mm\u00b7chen", "auf", "der", "Flur", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist sie nicht so sch\u00f6n vollendet", "tokens": ["Ist", "sie", "nicht", "so", "sch\u00f6n", "voll\u00b7en\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und nat\u00fcrlich, die Natur?", "tokens": ["Und", "na\u00b7t\u00fcr\u00b7lich", ",", "die", "Na\u00b7tur", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Erd' und Himmel sind erf\u00fcllet", "tokens": ["Erd'", "und", "Him\u00b7mel", "sind", "er\u00b7f\u00fcl\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ganz von meiner Herrlichkeit,", "tokens": ["Ganz", "von", "mei\u00b7ner", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Mensch, er wird mich loben", "tokens": ["Und", "der", "Mensch", ",", "er", "wird", "mich", "lo\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis in alle Ewigkeit!\u00ab", "tokens": ["Bis", "in", "al\u00b7le", "E\u00b7wig\u00b7keit", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "APPR", "PIAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}