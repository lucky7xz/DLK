{"textgrid.poem.64691": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbkein Heil ist au\u00dfer mir!\u00ab Der Kirche, die das lehrt,", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbkein Heil ist au\u00dfer mir!\u00ab Der Kirche, die das lehrt,", "tokens": ["\u00bb", "kein", "Heil", "ist", "au\u00b7\u00dfer", "mir", "!", "\u00ab", "Der", "Kir\u00b7che", ",", "die", "das", "lehrt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "VAFIN", "APPR", "PPER", "$.", "$(", "ART", "NN", "$,", "PRELS", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Glaubt Mancher nicht so viel, der selbst zu ihr geh\u00f6rt.", "tokens": ["Glaubt", "Man\u00b7cher", "nicht", "so", "viel", ",", "der", "selbst", "zu", "ihr", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "ADV", "ADV", "$,", "PRELS", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u00bbnichts wu\u00dfte man vor uns von Wahrheit und von Tugend!\u00ab", "tokens": ["\u00bb", "nichts", "wu\u00df\u00b7te", "man", "vor", "uns", "von", "Wahr\u00b7heit", "und", "von", "Tu\u00b7gend", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PIS", "VVFIN", "PIS", "APPR", "PPER", "APPR", "NN", "KON", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Glaubt doch den Kritischen f\u00fcr jetzt die liebe Tugend. -", "tokens": ["Glaubt", "doch", "den", "Kri\u00b7ti\u00b7schen", "f\u00fcr", "jetzt", "die", "lie\u00b7be", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ADV", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u00bbnur die sind frei, die wir beherrschen und berauben!\u00ab", "tokens": ["\u00bb", "nur", "die", "sind", "frei", ",", "die", "wir", "be\u00b7herr\u00b7schen", "und", "be\u00b7rau\u00b7ben", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ART", "VAFIN", "ADJD", "$,", "PRELS", "PPER", "VVINF", "KON", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das soll die ganze Welt den Freiheitsbringern glauben.", "tokens": ["Das", "soll", "die", "gan\u00b7ze", "Welt", "den", "Frei\u00b7heits\u00b7brin\u00b7gern", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}