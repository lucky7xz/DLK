{"textgrid.poem.39900": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Sohn des Regenten", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor der letzten engen Zelle", "tokens": ["Vor", "der", "letz\u00b7ten", "en\u00b7gen", "Zel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In Sanct Genofevens Haus", "tokens": ["In", "Sanct", "Ge\u00b7no\u00b7fe\u00b7vens", "Haus"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Murmelt schwach die ferne Welle", "tokens": ["Mur\u00b7melt", "schwach", "die", "fer\u00b7ne", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der Weltstadt Lustgebraus.", "tokens": ["Von", "der", "Welt\u00b7stadt", "Lust\u00b7ge\u00b7braus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kein Gemach ist so voll B\u00e4ngni\u00df", "tokens": ["Kein", "Ge\u00b7mach", "ist", "so", "voll", "B\u00e4ng\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den G\u00e4\u00dfchen von Paris,", "tokens": ["In", "den", "G\u00e4\u00df\u00b7chen", "von", "Pa\u00b7ris", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So voll Schatten kein Gef\u00e4ngni\u00df,", "tokens": ["So", "voll", "Schat\u00b7ten", "kein", "Ge\u00b7f\u00e4ng\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keines M\u00f6rders Thurmverlie\u00df.", "tokens": ["Kei\u00b7nes", "M\u00f6r\u00b7ders", "Thurm\u00b7ver\u00b7lie\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hier wohnt Einer, m\u00fcd' von Plage,", "tokens": ["Hier", "wohnt", "Ei\u00b7ner", ",", "m\u00fcd'", "von", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "VMFIN", "APPR", "NN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Harmvoll, in geringer Tracht.", "tokens": ["Harm\u00b7voll", ",", "in", "ge\u00b7rin\u00b7ger", "Tracht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf dem Knie liegt er am Tage,", "tokens": ["Auf", "dem", "Knie", "liegt", "er", "am", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hart auf Stroh ruht er zu Nacht.", "tokens": ["Hart", "auf", "Stroh", "ruht", "er", "zu", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "--+---+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Und kein Holz am kalten Morgen", "tokens": ["Und", "kein", "Holz", "am", "kal\u00b7ten", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Knistert lindernd im Kamin,", "tokens": ["Knis\u00b7tert", "lin\u00b7dernd", "im", "Ka\u00b7min", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selbst die Bettler sind geborgen,", "tokens": ["Selbst", "die", "Bett\u00b7ler", "sind", "ge\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keinen sch\u00fcttelt Frost, wie ihn.", "tokens": ["Kei\u00b7nen", "sch\u00fct\u00b7telt", "Frost", ",", "wie", "ihn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "VVFIN", "NN", "$,", "PWAV", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Bei dem kargen Mittagsmahle", "tokens": ["Bei", "dem", "kar\u00b7gen", "Mit\u00b7tags\u00b7mah\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Speist das schwarze Brod ihn kaum,", "tokens": ["Speist", "das", "schwar\u00b7ze", "Brod", "ihn", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zum Wasser in der Schale", "tokens": ["Und", "zum", "Was\u00b7ser", "in", "der", "Scha\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mischt sich nie des Weines Schaum.", "tokens": ["Mischt", "sich", "nie", "des", "Wei\u00b7nes", "Schaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sechsmal nach dem Winterreife", "tokens": ["Sechs\u00b7mal", "nach", "dem", "Win\u00b7ter\u00b7rei\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat sein Fenster ihm gethaut,", "tokens": ["Hat", "sein", "Fens\u00b7ter", "ihm", "ge\u00b7thaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An dem schmalen Himmelsstreife", "tokens": ["An", "dem", "schma\u00b7len", "Him\u00b7mels\u00b7strei\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sechsmal ihm der Lenz geblaut.", "tokens": ["Sechs\u00b7mal", "ihm", "der", "Lenz", "ge\u00b7blaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Da erscheint in seiner Pforte", "tokens": ["Da", "er\u00b7scheint", "in", "sei\u00b7ner", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Goldbetre\u00dfter Diener Hauf',", "tokens": ["Gold\u00b7be\u00b7tre\u00df\u00b7ter", "Die\u00b7ner", "Hauf'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit ehrfurchtsvollem Worte", "tokens": ["Und", "mit", "ehr\u00b7furchts\u00b7vol\u00b7lem", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00f6rt er den Versenkten auf:", "tokens": ["St\u00f6rt", "er", "den", "Ver\u00b7senk\u00b7ten", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbgn\u00e4d'ger Herzog! drin im Schlosse", "tokens": ["\u00bb", "gn\u00e4d'\u00b7ger", "Her\u00b7zog", "!", "drin", "im", "Schlos\u00b7se"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADJA", "NN", "$.", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Harrt der Sohn in Liebe dein:", "tokens": ["Harrt", "der", "Sohn", "in", "Lie\u00b7be", "dein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollest seinem ersten Sprosse,", "tokens": ["Wol\u00b7lest", "sei\u00b7nem", "ers\u00b7ten", "Spros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deinem Enkel, Pate sein!\u00ab", "tokens": ["Dei\u00b7nem", "En\u00b7kel", ",", "Pa\u00b7te", "sein", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "VAINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und er hebt, gedenk der W\u00fcrde,", "tokens": ["Und", "er", "hebt", ",", "ge\u00b7denk", "der", "W\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von den Knieen sich empor,", "tokens": ["Von", "den", "Kni\u00b7e\u00b7en", "sich", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Schreitet mit der Purpurb\u00fcrde", "tokens": ["Schrei\u00b7tet", "mit", "der", "Pur\u00b7pur\u00b7b\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "In dem schimmernden Palaste", "tokens": ["In", "dem", "schim\u00b7mern\u00b7den", "Pa\u00b7las\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seiner V\u00e4ter weilt er stumm,", "tokens": ["Sei\u00b7ner", "V\u00e4\u00b7ter", "weilt", "er", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieht sich in dem eiteln Glaste", "tokens": ["Sieht", "sich", "in", "dem", "ei\u00b7teln", "Glas\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Grabentstiegner um;", "tokens": ["Wie", "ein", "Gra\u00b7bent\u00b7stieg\u00b7ner", "um", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wiegt den Enkel in den Armen,", "tokens": ["Wiegt", "den", "En\u00b7kel", "in", "den", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis das Taufbad ihn geweiht,", "tokens": ["Bis", "das", "Tauf\u00b7bad", "ihn", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4\u00dft mit Blicken voll Erbarmen", "tokens": ["L\u00e4\u00dft", "mit", "Bli\u00b7cken", "voll", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihn im Schoos der blinden Zeit.", "tokens": ["Ihn", "im", "Schoos", "der", "blin\u00b7den", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Wie er in der Halle wieder", "tokens": ["Wie", "er", "in", "der", "Hal\u00b7le", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einsam seinem Heiland lebt,", "tokens": ["Ein\u00b7sam", "sei\u00b7nem", "Hei\u00b7land", "lebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wirft er sich auf's Antlitz nieder,", "tokens": ["Wirft", "er", "sich", "auf's", "Ant\u00b7litz", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sein Innerstes erbebt:", "tokens": ["Da\u00df", "sein", "In\u00b7ners\u00b7tes", "er\u00b7bebt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "\u00bbeiner liegt vor dir von allen", "tokens": ["\u00bb", "ei\u00b7ner", "liegt", "vor", "dir", "von", "al\u00b7len"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PIS", "VVFIN", "APPR", "PPER", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kindern \u00fcppigen Geschlechts,", "tokens": ["Kin\u00b7dern", "\u00fcp\u00b7pi\u00b7gen", "Ge\u00b7schlechts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herr, o Herr! la\u00df dir gefallen", "tokens": ["Herr", ",", "o", "Herr", "!", "la\u00df", "dir", "ge\u00b7fal\u00b7len"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "FM", "NN", "$.", "VVIMP", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tiefste Bu\u00dfe deines Knechts!", "tokens": ["Tiefs\u00b7te", "Bu\u00b7\u00dfe", "dei\u00b7nes", "Knechts", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Was mein Vater wild ges\u00fcndigt,", "tokens": ["Was", "mein", "Va\u00b7ter", "wild", "ge\u00b7s\u00fcn\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat ihm nachgethan das Land.", "tokens": ["Hat", "ihm", "nach\u00b7ge\u00b7than", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Neuer Greuel ist verk\u00fcndigt;", "tokens": ["Neu\u00b7er", "Greu\u00b7el", "ist", "ver\u00b7k\u00fcn\u00b7digt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drum ersticke du den Brand!", "tokens": ["Drum", "er\u00b7sti\u00b7cke", "du", "den", "Brand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Wieder Einer ist geboren!", "tokens": ["Wie\u00b7der", "Ei\u00b7ner", "ist", "ge\u00b7bo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sei, o Herr, es nicht zum Fluch!", "tokens": ["Sei", ",", "o", "Herr", ",", "es", "nicht", "zum", "Fluch", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "FM", "NN", "$,", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist zum Retter der erkoren,", "tokens": ["Ist", "zum", "Ret\u00b7ter", "der", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ART", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieg' ich gern im Leichentuch!\u00ab", "tokens": ["Lieg'", "ich", "gern", "im", "Lei\u00b7chen\u00b7tuch", "!", "\u00ab"], "token_info": 