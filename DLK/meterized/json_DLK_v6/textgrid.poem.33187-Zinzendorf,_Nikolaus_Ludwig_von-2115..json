{"textgrid.poem.33187": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "2115.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm herab, Gott Heilger Geist! mache alle h\u00e4user, und die herzen allerseits, auf das gute weiser.", "tokens": ["Komm", "her\u00b7ab", ",", "Gott", "Heil\u00b7ger", "Geist", "!", "ma\u00b7che", "al\u00b7le", "h\u00e4u\u00b7ser", ",", "und", "die", "her\u00b7zen", "al\u00b7ler\u00b7seits", ",", "auf", "das", "gu\u00b7te", "wei\u00b7ser", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "NN", "ADJA", "NN", "$.", "VVFIN", "PIS", "ADJD", "$,", "KON", "ART", "NN", "ADV", "$,", "APPR", "ART", "ADJA", "ADJA", "$."], "meter": "+-+-+-+--+-+-+-+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Was du bisdaher gethan an den ledgen ch\u00f6ren, thue auch an frau und mann, deiner eh zu ehren.", "tokens": ["Was", "du", "bis\u00b7da\u00b7her", "ge\u00b7than", "an", "den", "led\u00b7gen", "ch\u00f6\u00b7ren", ",", "thue", "auch", "an", "frau", "und", "mann", ",", "dei\u00b7ner", "eh", "zu", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVPP", "APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,", "PPOSAT", "KOUS", "PTKZU", "VVINF", "$."], "meter": "--+-+-+--+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "La\u00df in ihrem angesicht Jesu licht erscheinen, da\u00df sich alles darnach richt in den chor-gemeinen.", "tokens": ["La\u00df", "in", "ih\u00b7rem", "an\u00b7ge\u00b7sicht", "Je\u00b7su", "licht", "er\u00b7schei\u00b7nen", ",", "da\u00df", "sich", "al\u00b7les", "dar\u00b7nach", "richt", "in", "den", "chor\u00b7ge\u00b7mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "NE", "NN", "VVINF", "$,", "KOUS", "PRF", "PIS", "PAV", "VVFIN", "APPR", "ART", "ADJA", "$."], "meter": "+-+-+--+-+-+-+-+--+---+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Gib uns einen l\u00f6wen-muth bey dem lammes-sinne so gehn alle dinge gut draussen und auch drinne.", "tokens": ["Gib", "uns", "ei\u00b7nen", "l\u00f6\u00b7wen\u00b7muth", "bey", "dem", "lam\u00b7mes\u00b7sin\u00b7ne", "so", "gehn", "al\u00b7le", "din\u00b7ge", "gut", "draus\u00b7sen", "und", "auch", "drin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "ART", "NN", "ADV", "VVFIN", "PIAT", "NN", "ADJD", "VVINF", "KON", "ADV", "VVFIN", "$."], "meter": "+-+-+--+-+-+--+--+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.5": {"line.1": {"text": "Lege deine rechte hand mit dem n\u00e4gel-loche auf die leute deiner hand, unter deinem joche,", "tokens": ["Le\u00b7ge", "dei\u00b7ne", "rech\u00b7te", "hand", "mit", "dem", "n\u00e4\u00b7gel\u00b7lo\u00b7che", "auf", "die", "leu\u00b7te", "dei\u00b7ner", "hand", ",", "un\u00b7ter", "dei\u00b7nem", "jo\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "APPR", "ART", "ADJA", "APPR", "ART", "NN", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+--+-+-+-+-+----+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.6": {"line.1": {"text": "Da\u00df sie nicht allein f\u00fcr sich gute sachen machen, sondern alles lediglich als des Lammes sachen.", "tokens": ["Da\u00df", "sie", "nicht", "al\u00b7lein", "f\u00fcr", "sich", "gu\u00b7te", "sa\u00b7chen", "ma\u00b7chen", ",", "son\u00b7dern", "al\u00b7les", "le\u00b7di\u00b7glich", "als", "des", "Lam\u00b7mes", "sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "APPR", "PRF", "ADJA", "VVINF", "VVINF", "$,", "KON", "PIS", "ADV", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-+-+--+-+-", "measure": "iambic.octa.plus"}}, "stanza.7": {"line.1": {"text": "La\u00df uns in gemeinschaft stehn in genad und segen, und als deine engel gehn alles durchzufegen.", "tokens": ["La\u00df", "uns", "in", "ge\u00b7mein\u00b7schaft", "stehn", "in", "ge\u00b7nad", "und", "se\u00b7gen", ",", "und", "als", "dei\u00b7ne", "en\u00b7gel", "gehn", "al\u00b7les", "durch\u00b7zu\u00b7fe\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJD", "VVFIN", "APPR", "NE", "KON", "VVFIN", "$,", "KON", "KOUS", "PPOSAT", "NN", "VVFIN", "PIS", "VVINF", "$."], "meter": "+-+----+-+-+-+-+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.8": {"line.1": {"text": "Und uns dir mit dankbarkeit f\u00fcr so vielen segen, Lamm! als deine kirchen-leut vor die f\u00fcsse legen:", "tokens": ["Und", "uns", "dir", "mit", "dank\u00b7bar\u00b7keit", "f\u00fcr", "so", "vie\u00b7len", "se\u00b7gen", ",", "Lamm", "!", "als", "dei\u00b7ne", "kir\u00b7chen\u00b7leut", "vor", "die", "f\u00fcs\u00b7se", "le\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "APPR", "NN", "APPR", "ADV", "PIAT", "NN", "$,", "NN", "$.", "KOUS", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-+--+-+-+-", "measure": "iambic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Kurz, dieselbe priester hand, die uns immer segnet, heb sich \u00fcber ort und land, wo man ihr begegnet.", "tokens": ["Kurz", ",", "die\u00b7sel\u00b7be", "pries\u00b7ter", "hand", ",", "die", "uns", "im\u00b7mer", "seg\u00b7net", ",", "heb", "sich", "\u00fc\u00b7ber", "ort", "und", "land", ",", "wo", "man", "ihr", "be\u00b7geg\u00b7net", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PDAT", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,", "PWAV", "PIS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.10": {"line.1": {"text": "Wenn wir in der k\u00fcnftgen zeit etwas machen wollen, das wir dir zur herzens-freud unternehmen sollen,", "tokens": ["Wenn", "wir", "in", "der", "k\u00fcnft\u00b7gen", "zeit", "et\u00b7was", "ma\u00b7chen", "wol\u00b7len", ",", "das", "wir", "dir", "zur", "her\u00b7zens\u00b7freud", "un\u00b7ter\u00b7neh\u00b7men", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "ADV", "VVINF", "VMFIN", "$,", "PRELS", "PPER", "PPER", "APPRART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+---+--+-+-+-+-+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.11": {"line.1": {"text": "Mache uns so klein gesinnt in den grossen sachen, als ob etwa so ein kind solte gr\u00fcbgen machen.", "tokens": ["Ma\u00b7che", "uns", "so", "klein", "ge\u00b7sinnt", "in", "den", "gros\u00b7sen", "sa\u00b7chen", ",", "als", "ob", "et\u00b7wa", "so", "ein", "kind", "sol\u00b7te", "gr\u00fcb\u00b7gen", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "VVPP", "APPR", "ART", "ADJA", "VVINF", "$,", "KOKOM", "KOUS", "ADV", "ADV", "ART", "NN", "VMFIN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-+--++--+-+-", "measure": "trochaic.octa.plus.relaxed"}}}}}