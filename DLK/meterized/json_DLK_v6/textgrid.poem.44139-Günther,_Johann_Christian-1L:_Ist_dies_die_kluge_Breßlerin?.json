{"textgrid.poem.44139": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ist dies die kluge Bre\u00dflerin?", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist dies die kluge Bre\u00dflerin?", "tokens": ["Ist", "dies", "die", "klu\u00b7ge", "Bre\u00df\u00b7le\u00b7rin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer will ein todtes Bild so nennen?", "tokens": ["Wer", "will", "ein", "tod\u00b7tes", "Bild", "so", "nen\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jedoch, begehrt ihr sie zu kennen,", "tokens": ["Je\u00b7doch", ",", "be\u00b7gehrt", "ihr", "sie", "zu", "ken\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So gebt ihr selbst die Feder hin.", "tokens": ["So", "gebt", "ihr", "selbst", "die", "Fe\u00b7der", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}