{"textgrid.poem.52452": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "34. Kranz f\u00fcr Berenices Bild", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bildner, w\u00e4hle zum Kranz der Holden bescheidenes Sinngr\u00fcn,", "tokens": ["Bild\u00b7ner", ",", "w\u00e4h\u00b7le", "zum", "Kranz", "der", "Hol\u00b7den", "be\u00b7schei\u00b7de\u00b7nes", "Sinn\u00b7gr\u00fcn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPRART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Weich, wie der Sanftmut Reis, stark, wie der Eppich der Treu'.", "tokens": ["Weich", ",", "wie", "der", "Sanft\u00b7mut", "Reis", ",", "stark", ",", "wie", "der", "Ep\u00b7pich", "der", "Treu'", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "NN", "$,", "ADJD", "$,", "PWAV", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "W\u00e4hle die zarte Ranke, die jeglichem buhlenden L\u00fcftchen", "tokens": ["W\u00e4h\u00b7le", "die", "zar\u00b7te", "Ran\u00b7ke", ",", "die", "jeg\u00b7li\u00b7chem", "buh\u00b7len\u00b7den", "L\u00fcft\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PIAT", "ADJA", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Ausbeugt, welche nicht rei\u00dft, wenn sie das Schicksal umst\u00fcrmt.", "tokens": ["Aus\u00b7beugt", ",", "wel\u00b7che", "nicht", "rei\u00dft", ",", "wenn", "sie", "das", "Schick\u0