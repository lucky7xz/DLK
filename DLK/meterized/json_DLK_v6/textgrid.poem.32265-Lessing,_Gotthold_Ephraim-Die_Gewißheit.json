{"textgrid.poem.32265": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Die Gewi\u00dfheit", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob ich morgen leben werde,", "tokens": ["Ob", "ich", "mor\u00b7gen", "le\u00b7ben", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00df ich freilich nicht:", "tokens": ["Wei\u00df", "ich", "frei\u00b7lich", "nicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Aber, wenn ich morgen lebe,", "tokens": ["A\u00b7ber", ",", "wenn", "ich", "mor\u00b7gen", "le\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich morgen trinken werde,", "tokens": ["Da\u00df", "ich", "mor\u00b7gen", "trin\u00b7ken", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wei\u00df ich ganz gewi\u00df.", "tokens": ["Wei\u00df", "ich", "ganz", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}