{"textgrid.poem.67033": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Adramelech", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Adramelechs Grimm erwachte, des H\u00f6llenbewohners:", "tokens": ["Ad\u00b7ra\u00b7me\u00b7lechs", "Grimm", "er\u00b7wach\u00b7te", ",", "des", "H\u00f6l\u00b7len\u00b7be\u00b7woh\u00b7ners", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$,", "ART", "NN", "$."], "meter": "+---+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "H\u00f6lle, sinke tiefer hinab, Adramelech w\u00fctet,", "tokens": ["H\u00f6l\u00b7le", ",", "sin\u00b7ke", "tie\u00b7fer", "hin\u00b7ab", ",", "Ad\u00b7ra\u00b7me\u00b7lech", "w\u00fc\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADJD", "PTKVZ", "$,", "NE", "VVFIN", "$,"], "meter": "+-+-+--++-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Staune, Satan du, verzweifle, K\u00f6nig der H\u00f6lle,", "tokens": ["Stau\u00b7ne", ",", "Sa\u00b7tan", "du", ",", "ver\u00b7zweif\u00b7le", ",", "K\u00f6\u00b7nig", "der", "H\u00f6l\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "NE", "$,", "VVFIN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Nur Adramelech bleibt gro\u00df \u2013 entdeck ich die gro\u00dfen Entw\u00fcrfe", "tokens": ["Nur", "Ad\u00b7ra\u00b7me\u00b7lech", "bleibt", "gro\u00df", "\u2013", "ent\u00b7deck", "ich", "die", "gro\u00b7\u00dfen", "Ent\u00b7w\u00fcr\u00b7fe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "VVFIN", "ADJD", "$(", "VVIMP", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Dann und meine Gedanken, die den Olympus beherrschen,", "tokens": ["Dann", "und", "mei\u00b7ne", "Ge\u00b7dan\u00b7ken", ",", "die", "den", "O\u00b7lym\u00b7pus", "be\u00b7herr\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "PPOSAT", "NN", "$,", "PRELS", "ART", "NE", "VVINF", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Seinen Rat vereiteln, wie werden die Schw\u00e4chere gaffen,", "tokens": ["Sei\u00b7nen", "Rat", "ver\u00b7ei\u00b7teln", ",", "wie", "wer\u00b7den", "die", "Schw\u00e4\u00b7che\u00b7re", "gaf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,", "PWAV", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.7": {"text": "Satan wird vom Thron mit neidischem Stolze herabschaun,", "tokens": ["Sa\u00b7tan", "wird", "vom", "Thron", "mit", "nei\u00b7di\u00b7schem", "Stol\u00b7ze", "her\u00b7ab\u00b7schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPRART", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Du Jehova sollst bald in deinem richtenden Grimme \u2013", "tokens": ["Du", "Je\u00b7ho\u00b7va", "sollst", "bald", "in", "dei\u00b7nem", "rich\u00b7ten\u00b7den", "Grim\u00b7me", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "VMFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Dieses dein Isra\u00ebl soll dein Rachedonner zerschmettern,", "tokens": ["Die\u00b7ses", "dein", "Is\u00b7ra\u00ebl", "soll", "dein", "Ra\u00b7che\u00b7don\u00b7ner", "zer\u00b7schmet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NE", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+---+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Oder Mein Geist ist hin \u2013 verloren des m\u00e4chtigsten Kr\u00e4fte.", "tokens": ["O\u00b7der", "Mein", "Geist", "ist", "hin", "\u2013", "ver\u00b7lo\u00b7ren", "des", "m\u00e4ch\u00b7tigs\u00b7ten", "Kr\u00e4f\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "$(", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.11": {"text": "So sprach er \u2013 und kehrte mit Wut zur H\u00f6lle zur\u00fccke.", "tokens": ["So", "sprach", "er", "\u2013", "und", "kehr\u00b7te", "mit", "Wut", "zur", "H\u00f6l\u00b7le", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KON", "VVFIN", "APPR", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Sein verschlagener Stolz versammelte alle Gestalten,", "tokens": ["Sein", "ver\u00b7schla\u00b7ge\u00b7ner", "Stolz", "ver\u00b7sam\u00b7mel\u00b7te", "al\u00b7le", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Alle Schrecken des Tods um sich her, um seines Regenten", "tokens": ["Al\u00b7le", "Schre\u00b7cken", "des", "Tods", "um", "sich", "her", ",", "um", "sei\u00b7nes", "Re\u00b7gen\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN", "APPR", "PRF", "PTKVZ", "$,", "KOUI", "PPOSAT", "NN"], "meter": "--+--+--+-+-+--", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Schreckenvolle Pracht an sich den Geistern zu zeigen.", "tokens": ["Schre\u00b7cken\u00b7vol\u00b7le", "Pracht", "an", "sich", "den", "Geis\u00b7tern", "zu", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PRF", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.15": {"text": "Und so fuhr er ein, die zitternde Geister der Pforte", "tokens": ["Und", "so", "fuhr", "er", "ein", ",", "die", "zit\u00b7tern\u00b7de", "Geis\u00b7ter", "der", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "\u00d6ffneten ihre knarrende Tore weit auf, mit Erstaunen", "tokens": ["\u00d6ff\u00b7ne\u00b7ten", "ih\u00b7re", "knar\u00b7ren\u00b7de", "To\u00b7re", "weit", "auf", ",", "mit", "Er\u00b7stau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ADJD", "PTKVZ", "$,", "APPR", "NN"], "meter": "+--+-+--+-+++-+-", "measure": "iambic.octa.plus.invert"}, "line.17": {"text": "Sahn sie seine schreckbare Wut, mit flammendem Zorne,", "tokens": ["Sahn", "sie", "sei\u00b7ne", "schreck\u00b7ba\u00b7re", "Wut", ",", "mit", "flam\u00b7men\u00b7dem", "Zor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.18": {"text": "Wie nur selten Satan ergrimmt, deckt' er die h\u00f6llische R\u00e4nke...", "tokens": ["Wie", "nur", "sel\u00b7ten", "Sa\u00b7tan", "er\u00b7grimmt", ",", "deckt'", "er", "die", "h\u00f6l\u00b7li\u00b7sche", "R\u00e4n\u00b7ke", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJA", "NN", "VVPP", "$,", "PRELS", "PPER", "ART", "ADJA", "NN", "$("], "meter": "--+-+--+---+--+-", "measure": "iambic.penta.relaxed"}}}}}