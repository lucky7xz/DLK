{"dta.poem.19081": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LxIII.  Auf des Baron Kittlitz Heyrath mit  \n der Gr\u00e4fin Henriette Henckeln.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Was Unterschiedne thun, kan unterschieden seyn.", "tokens": ["Was", "Un\u00b7ter\u00b7schied\u00b7ne", "thun", ",", "kan", "un\u00b7ter\u00b7schie\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVINF", "$,", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Alten freyten dort, und liessen sich auch freyn:", "tokens": ["Die", "Al\u00b7ten", "frey\u00b7ten", "dort", ",", "und", "lies\u00b7sen", "sich", "auch", "freyn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "KON", "VVFIN", "PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Engel freyen nicht, die Kinder GOttes freyen;", "tokens": ["Die", "En\u00b7gel", "frey\u00b7en", "nicht", ",", "die", "Kin\u00b7der", "Got\u00b7tes", "frey\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ist Gefahr dabey, so kan es auch gedeyhen.", "tokens": ["Und", "ist", "Ge\u00b7fahr", "da\u00b7bey", ",", "so", "kan", "es", "auch", "ge\u00b7dey\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "PAV", "$,", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer nicht recht theilen kan, dem klingt es furchterlich,", "tokens": ["Wer", "nicht", "recht", "thei\u00b7len", "kan", ",", "dem", "klingt", "es", "furch\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$,", "PRELS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So bald das Wort erschallt: Der, die verehlicht sich.", "tokens": ["So", "bald", "das", "Wort", "er\u00b7schallt", ":", "Der", ",", "die", "ver\u00b7eh\u00b7licht", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$.", "ART", "$,", "PRELS", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Mein Hertz! was dachtest du? was fiel dir dr\u00fcber ein?", "tokens": ["Mein", "Hertz", "!", "was", "dach\u00b7test", "du", "?", "was", "fiel", "dir", "dr\u00fc\u00b7ber", "ein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PWS", "VVFIN", "PPER", "$.", "PWS", "VVFIN", "PPER", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als erst die Rede gieng: Die ", "tokens": ["Als", "erst", "die", "Re\u00b7de", "gieng", ":", "Die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$.", "ART"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.11": {"text": "Der Baron ", "tokens": ["Der", "Ba\u00b7ron"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Er sucht nicht irrdische, er sucht vollkommne Gaben,", "tokens": ["Er", "sucht", "nicht", "irr\u00b7di\u00b7sche", ",", "er", "sucht", "voll\u00b7komm\u00b7ne", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJA", "$,", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er hat so unrecht nicht, (gedachtest du, mein Hertz;)", "tokens": ["Er", "hat", "so", "un\u00b7recht", "nicht", ",", "(", "ge\u00b7dach\u00b7test", "du", ",", "mein", "Hertz", ";)"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "emoticon"], "pos": ["PPER", "VAFIN", "ADV", "NN", "PTKNEG", "$,", "$(", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "All ein, das Freyen ist den Christen ausser Schertz:", "tokens": ["All", "ein", ",", "das", "Frey\u00b7en", "ist", "den", "Chris\u00b7ten", "aus\u00b7ser", "Schertz", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ART", "NN", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wenn Henriette sich doch ja nicht \u00fcbereilte,", "tokens": ["Wenn", "Hen\u00b7riet\u00b7te", "sich", "doch", "ja", "nicht", "\u00fc\u00b7be\u00b7reil\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PRF", "ADV", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.16": {"text": "Und (weil ihrs GOtt verg\u00f6nnt) sich noch mit niemand theilte.", "tokens": ["Und", "(", "weil", "ihrs", "Gott", "ver\u00b7g\u00f6nnt", ")", "sich", "noch", "mit", "nie\u00b7mand", "theil\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "KOUS", "PPOSAT", "NN", "VVPP", "$(", "PRF", "ADV", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wer wei\u00df, erhohlte sich die Uberlegungs-Kraft,)", "tokens": ["Wer", "wei\u00df", ",", "er\u00b7hohl\u00b7te", "sich", "die", "U\u00b7ber\u00b7le\u00b7gungs\u00b7Kraft", ",", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "$,", "VVFIN", "PRF", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ob dieser Ehestand nicht viel Gedeylichs schaft.", "tokens": ["Ob", "die\u00b7ser", "E\u00b7hes\u00b7tand", "nicht", "viel", "Ge\u00b7dey\u00b7lichs", "schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PTKNEG", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Hat nicht manch theures Weib dem Mann, der sie erwehlet,", "tokens": ["Hat", "nicht", "manch", "theu\u00b7res", "Weib", "dem", "Mann", ",", "der", "sie", "er\u00b7weh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PIAT", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "In JEsu Christi Grund noch tiefer eingepf\u00e4hlet?", "tokens": ["In", "Je\u00b7su", "Chris\u00b7ti", "Grund", "noch", "tie\u00b7fer", "ein\u00b7ge\u00b7pf\u00e4h\u00b7let", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Mein Dencken endigte sich mit Gebet und Flehn;", "tokens": ["Mein", "Den\u00b7cken", "en\u00b7dig\u00b7te", "sich", "mit", "Ge\u00b7bet", "und", "Flehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "NN", "KON", "VVINF", "$."], "meter": "-+----+-+--+", "measure": "iambic.tetra.chol"}, "line.22": {"text": "Wohlan, dahin all ein soll meine Bitte gehn,", "tokens": ["Wo\u00b7hlan", ",", "da\u00b7hin", "all", "ein", "soll", "mei\u00b7ne", "Bit\u00b7te", "gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PAV", "PIAT", "ART", "PIAT", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Er, seufzt ich: Theurer Freund und Br\u00e4utigam der Hertzen!", "tokens": ["Er", ",", "seufzt", "ich", ":", "Theu\u00b7rer", "Freund", "und", "Br\u00e4u\u00b7ti\u00b7gam", "der", "Hert\u00b7zen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "PPER", "$.", "ADJD", "NN", "KON", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Entz\u00fcnd in diesem Paar der reinen Liebe Kertzen!", "tokens": ["Ent\u00b7z\u00fcnd", "in", "die\u00b7sem", "Paar", "der", "rei\u00b7nen", "Lie\u00b7be", "Kert\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Entflamm ein iegliches mit deiner Freudigkeit!", "tokens": ["Ent\u00b7flamm", "ein", "ieg\u00b7li\u00b7ches", "mit", "dei\u00b7ner", "Freu\u00b7dig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Dein Eifer um das Hau\u00df des Vaters sey ihr Kleid!", "tokens": ["Dein", "Ei\u00b7fer", "um", "das", "Hau\u00df", "des", "Va\u00b7ters", "sey", "ihr", "Kleid", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Dein sanfter Liebes-Sinn sey ihr Geschmuck von innen!", "tokens": ["Dein", "sanf\u00b7ter", "Lie\u00b7bes\u00b7Sinn", "sey", "ihr", "Ge\u00b7schmuck", "von", "in\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So wird auch diesem Paar die Welt nichts abgewinnen.", "tokens": ["So", "wird", "auch", "die\u00b7sem", "Paar", "die", "Welt", "nichts", "ab\u00b7ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PDAT", "NN", "ART", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So st\u00fcrtzt vor diesesmal der aufgehaltne Flu\u00df", "tokens": ["So", "st\u00fcrtzt", "vor", "die\u00b7ses\u00b7mal", "der", "auf\u00b7ge\u00b7halt\u00b7ne", "Flu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der Sinnen aufs Papier, das ich ergreiffen mu\u00df,", "tokens": ["Der", "Sin\u00b7nen", "aufs", "Pa\u00b7pier", ",", "das", "ich", "er\u00b7greif\u00b7fen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Um meine Redlichkeit den beyden lieben H\u00e4usern,", "tokens": ["Um", "mei\u00b7ne", "Red\u00b7lich\u00b7keit", "den", "bey\u00b7den", "lie\u00b7ben", "H\u00e4u\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ART", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die GOtt verbunden hat, mit wenigen zu \u00e4usern.", "tokens": ["Die", "Gott", "ver\u00b7bun\u00b7den", "hat", ",", "mit", "we\u00b7ni\u00b7gen", "zu", "\u00e4u\u00b7sern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,", "APPR", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Lebt, Hochgeliebteste! und da ich noch nicht wei\u00df,", "tokens": ["Lebt", ",", "Hoch\u00b7ge\u00b7lieb\u00b7tes\u00b7te", "!", "und", "da", "ich", "noch", "nicht", "wei\u00df", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "$.", "KON", "KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ob Baron Kittlitz nicht auch Bruder Kittlitz hei\u00df;", "tokens": ["Ob", "Ba\u00b7ron", "Kitt\u00b7litz", "nicht", "auch", "Bru\u00b7der", "Kitt\u00b7litz", "hei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NE", "PTKNEG", "ADV", "NN", "NE", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So wag\u2019 ichs auf den HErrn ihn br\u00fcderlich zu segnen:", "tokens": ["So", "wag'", "ichs", "auf", "den", "Herrn", "ihn", "br\u00fc\u00b7der\u00b7lich", "zu", "seg\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Jhn m\u00fcsse alles das, was Br\u00fcder trift, begegnen!", "tokens": ["Jhn", "m\u00fcs\u00b7se", "al\u00b7les", "das", ",", "was", "Br\u00fc\u00b7der", "trift", ",", "be\u00b7geg\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PDS", "$,", "PWS", "NN", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Warum erwehlt er sich ein Weib zum Eh-Gemahl,", "tokens": ["Wa\u00b7rum", "er\u00b7wehlt", "er", "sich", "ein", "Weib", "zum", "Eh\u00b7Ge\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die aus der sonderlich erkauften Schwestern Zahl?", "tokens": ["Die", "aus", "der", "son\u00b7der\u00b7lich", "er\u00b7kauf\u00b7ten", "Schwes\u00b7tern", "Zahl", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJD", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wer Schwestern freyen will, der mu\u00df als Bruder leben;", "tokens": ["Wer", "Schwes\u00b7tern", "frey\u00b7en", "will", ",", "der", "mu\u00df", "als", "Bru\u00b7der", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVINF", "VMFIN", "$,", "ART", "VMFIN", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Sonst kan sich ihm kein Hertz, das JEsu ist, ergeben.", "tokens": ["Sonst", "kan", "sich", "ihm", "kein", "Hertz", ",", "das", "Je\u00b7su", "ist", ",", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "PPER", "PIAT", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Man hat nicht Sicherheit, wenn man ein JEsus-P", "tokens": ["Man", "hat", "nicht", "Si\u00b7cher\u00b7heit", ",", "wenn", "man", "ein", "Je\u00b7sus\u00b7\u00b7P"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PTKNEG", "NN", "$,", "KOUS", "PIS", "ART", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "In seine Arme nimmt, und nicht aus JEsu Hand:", "tokens": ["In", "sei\u00b7ne", "Ar\u00b7me", "nimmt", ",", "und", "nicht", "aus", "Je\u00b7su", "Hand", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "KON", "PTKNEG", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Der ihm die Fran geschenckt, der schenck ihm \u00fcberschw\u00e4nglich", "tokens": ["Der", "ihm", "die", "Fran", "ge\u00b7schenckt", ",", "der", "schenck", "ihm", "\u00fc\u00b7bersc\u00b7hw\u00e4ng\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$,", "PRELS", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Creutz, Schmach und Seligkeit, und alles unverg\u00e4nglich!", "tokens": ["Creutz", ",", "Schmach", "und", "Se\u00b7lig\u00b7keit", ",", "und", "al\u00b7les", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,", "KON", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}