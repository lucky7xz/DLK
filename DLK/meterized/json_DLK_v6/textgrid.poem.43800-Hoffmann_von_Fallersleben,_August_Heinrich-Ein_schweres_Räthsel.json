{"textgrid.poem.43800": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein schweres R\u00e4thsel", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf unsrer Wiese gehet was,", "tokens": ["Auf", "uns\u00b7rer", "Wie\u00b7se", "ge\u00b7het", "was", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Watet durch die S\u00fcmpfe,", "tokens": ["Wa\u00b7tet", "durch", "die", "S\u00fcmp\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Es hat ein wei\u00dfes J\u00e4cklein an,", "tokens": ["Es", "hat", "ein", "wei\u00b7\u00dfes", "J\u00e4ck\u00b7lein", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tr\u00e4gt auch rothe Str\u00fcmpfe,", "tokens": ["Tr\u00e4gt", "auch", "ro\u00b7the", "Str\u00fcmp\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "F\u00e4ngt die Fr\u00f6sche schnapp wapp wapp,", "tokens": ["F\u00e4ngt", "die", "Fr\u00f6\u00b7sche", "schnapp", "wapp", "wapp", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "ITJ", "ITJ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Klappert lustig klapper diklapp \u2013", "tokens": ["Klap\u00b7pert", "lus\u00b7tig", "klap\u00b7per", "di\u00b7klapp", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Wer kann das errathen?", "tokens": ["Wer", "kann", "das", "er\u00b7ra\u00b7then", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Ihr denkt, es ist der Klapperstorch,", "tokens": ["Ihr", "denkt", ",", "es", "ist", "der", "Klap\u00b7per\u00b7storch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Watet durch die S\u00fcmpfe,", "tokens": ["Wa\u00b7tet", "durch", "die", "S\u00fcmp\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Er hat ein wei\u00dfes J\u00e4cklein an,", "tokens": ["Er", "hat", "ein", "wei\u00b7\u00dfes", "J\u00e4ck\u00b7lein", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tr\u00e4gt auch rothe Str\u00fcmpfe,", "tokens": ["Tr\u00e4gt", "auch", "ro\u00b7the", "Str\u00fcmp\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "F\u00e4ngt die Fr\u00f6sche schnapp wapp wapp,", "tokens": ["F\u00e4ngt", "die", "Fr\u00f6\u00b7sche", "schnapp", "wapp", "wapp", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "ITJ", "ITJ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Klappert lustig klapper diklapp \u2013", "tokens": ["Klap\u00b7pert", "lus\u00b7tig", "klap\u00b7per", "di\u00b7klapp", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Nein, nein! 's ist eine St\u00f6rchin.", "tokens": ["Nein", ",", "nein", "!", "'s", "ist", "ei\u00b7ne", "St\u00f6r\u00b7chin", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}