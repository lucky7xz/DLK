{"dta.poem.4015": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Hoffnung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "O Hoffnung! holde! g\u00fctig gesch\u00e4ftige!", "tokens": ["O", "Hoff\u00b7nung", "!", "hol\u00b7de", "!", "g\u00fc\u00b7tig", "ge\u00b7sch\u00e4f\u00b7ti\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "$.", "ADJD", "ADJA", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die du das Haus der Trauernden nicht verschm\u00e4hst,", "tokens": ["Die", "du", "das", "Haus", "der", "Trau\u00b7ern\u00b7den", "nicht", "ver\u00b7schm\u00e4hst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und gerne dienend, Edle, zwischen", "tokens": ["Und", "ger\u00b7ne", "die\u00b7nend", ",", "Ed\u00b7le", ",", "zwi\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "ADV", "VVPP", "$,", "ADJA", "$,", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sterblichen waltest und Himmelsm\u00e4chten;", "tokens": ["Sterb\u00b7li\u00b7chen", "wal\u00b7test", "und", "Him\u00b7mels\u00b7m\u00e4ch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Wo bist du? wenig lebt' ich, doch athmet kalt", "tokens": ["Wo", "bist", "du", "?", "we\u00b7nig", "lebt'", "ich", ",", "doch", "ath\u00b7met", "kalt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "PIS", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mein Abend schon, und stille, den Schatten gleich,", "tokens": ["Mein", "A\u00b7bend", "schon", ",", "und", "stil\u00b7le", ",", "den", "Schat\u00b7ten", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,", "KON", "VVFIN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bin ich schon hier; und schon gesanglos", "tokens": ["Bin", "ich", "schon", "hier", ";", "und", "schon", "ge\u00b7sang\u00b7los"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "$.", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schlummert das schauernde Herz im Busen.", "tokens": ["Schlum\u00b7mert", "das", "schau\u00b7ern\u00b7de", "Herz", "im", "Bu\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}