{"textgrid.poem.25342": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Kirschbaumbl\u00fcten im lichtdurchschwemmten Garten", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Kirschbaumbl\u00fcten im lichtdurchschwemmten Garten", "tokens": ["Die", "Kirschbaum\u00b7bl\u00fc\u00b7ten", "im", "licht\u00b7durch\u00b7schwemm\u00b7ten", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sind wie Kandelaber von Millionen Kerzen,", "tokens": ["Sind", "wie", "Kan\u00b7de\u00b7la\u00b7ber", "von", "Mil\u00b7lion\u00b7en", "Ker\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "NN", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Die das Vollmondfeuer angesteckt. Die zarten Kissen", "tokens": ["Die", "das", "Voll\u00b7mond\u00b7feu\u00b7er", "an\u00b7ge\u00b7steckt", ".", "Die", "zar\u00b7ten", "Kis\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVPP", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Gr\u00fcngesprengten Rasens zwischen Krokusbeeten", "tokens": ["Gr\u00fcn\u00b7ge\u00b7spreng\u00b7ten", "Ra\u00b7sens", "zwi\u00b7schen", "Kro\u00b7kus\u00b7bee\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Sind besteckt mit wei\u00dfen Perlens\u00e4umen,", "tokens": ["Sind", "be\u00b7steckt", "mit", "wei\u00b7\u00dfen", "Per\u00b7len\u00b7s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und die k\u00fchle spiegelhelle Luft", "tokens": ["Und", "die", "k\u00fch\u00b7le", "spie\u00b7gel\u00b7hel\u00b7le", "Luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Ist ein feiner Schleier von gewebtem Silber,", "tokens": ["Ist", "ein", "fei\u00b7ner", "Schlei\u00b7er", "von", "ge\u00b7web\u00b7tem", "Sil\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Den die Lenznacht heimlich gl\u00fchend um die", "tokens": ["Den", "die", "Lenz\u00b7nacht", "heim\u00b7lich", "gl\u00fc\u00b7hend", "um", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADJD", "ADJD", "APPR", "ART"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Wei\u00dfe warme Nacktheit ihrer Glieder h\u00e4ngt.", "tokens": ["Wei\u00b7\u00dfe", "war\u00b7me", "Nackt\u00b7heit", "ih\u00b7rer", "Glie\u00b7der", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}