{"dta.poem.7116": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie eine Mutter ihrem Kinde,", "tokens": ["Wie", "ei\u00b7ne", "Mut\u00b7ter", "ih\u00b7rem", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Damit dasselbige gelinde", "tokens": ["Da\u00b7mit", "das\u00b7sel\u00b7bi\u00b7ge", "ge\u00b7lin\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PDAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und sanffte schlaffen mag, die Wiege zuzudecken,", "tokens": ["Und", "sanff\u00b7te", "schlaf\u00b7fen", "mag", ",", "die", "Wie\u00b7ge", "zu\u00b7zu\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVINF", "VMFIN", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Um eine Dunckelheit durch Kunst ihm zu erwecken,", "tokens": ["Um", "ei\u00b7ne", "Dun\u00b7ckel\u00b7heit", "durch", "Kunst", "ihm", "zu", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und eine kleine Nacht zu machen pflegt;", "tokens": ["Und", "ei\u00b7ne", "klei\u00b7ne", "Nacht", "zu", "ma\u00b7chen", "pflegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So ziehet GOTTES Huld, die Sorge f\u00fcr uns tr\u00e4get,", "tokens": ["So", "zie\u00b7het", "GoT\u00b7TES", "Huld", ",", "die", "Sor\u00b7ge", "f\u00fcr", "uns", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NE", "$,", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Um jedes Theil der Welt, wenn wir von M\u00fcdigkeit,", "tokens": ["Um", "je\u00b7des", "Theil", "der", "Welt", ",", "wenn", "wir", "von", "M\u00fc\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Von Arbeit und von M\u00fch gedr\u00fcckt sind, und geschw\u00e4cht,", "tokens": ["Von", "Ar\u00b7beit", "und", "von", "M\u00fch", "ge\u00b7dr\u00fcckt", "sind", ",", "und", "ge\u00b7schw\u00e4cht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NE", "VVPP", "VAFIN", "$,", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie ein Gewand, gewebt aus Dunckelheit,", "tokens": ["Wie", "ein", "Ge\u00b7wand", ",", "ge\u00b7webt", "aus", "Dun\u00b7ckel\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "An allen Orten vor. Wir sind recht eingeh\u00fcllet,", "tokens": ["An", "al\u00b7len", "Or\u00b7ten", "vor", ".", "Wir", "sind", "recht", "ein\u00b7ge\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$.", "PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "In d\u00fcsterer Finsterni\u00df, die alle Ding erf\u00fcllet,", "tokens": ["In", "d\u00fcs\u00b7te\u00b7rer", "Fins\u00b7ter\u00b7ni\u00df", ",", "die", "al\u00b7le", "Ding", "er\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Um alles das zu sehn uns zu verwehren,", "tokens": ["Um", "al\u00b7les", "das", "zu", "sehn", "uns", "zu", "ver\u00b7weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "PDS", "PTKZU", "VVINF", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Was uns gewi\u00df sonst w\u00fcrd\u2019 in unsrer Ruhe st\u00f6hren.", "tokens": ["Was", "uns", "ge\u00b7wi\u00df", "sonst", "w\u00fcrd'", "in", "uns\u00b7rer", "Ru\u00b7he", "st\u00f6h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Merckt, wie sich die Natur, zu unserm Nutz, bem\u00fcht!", "tokens": ["Merckt", ",", "wie", "sich", "die", "Na\u00b7tur", ",", "zu", "un\u00b7serm", "Nutz", ",", "be\u00b7m\u00fcht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PRF", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.15": {"text": "Merckt, wie sie gleichsam fast die Welt, der Welt entzieht!", "tokens": ["Merckt", ",", "wie", "sie", "gleich\u00b7sam", "fast", "die", "Welt", ",", "der", "Welt", "ent\u00b7zieht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "ADJD", "ADV", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sie hindert unser Aug\u2019, so sich nicht m\u00fcde sieht,", "tokens": ["Sie", "hin\u00b7dert", "un\u00b7ser", "Aug'", ",", "so", "sich", "nicht", "m\u00fc\u00b7de", "sieht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "PRF", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Da\u00df, ob sichs gleich nicht schlie\u00dft, blo\u00df durch die Dunckelheit", "tokens": ["Da\u00df", ",", "ob", "sichs", "gleich", "nicht", "schlie\u00dft", ",", "blo\u00df", "durch", "die", "Dun\u00b7ckel\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "PIS", "ADV", "PTKNEG", "VVFIN", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Es doch gehindert wird, was eigentlich zu schauen.", "tokens": ["Es", "doch", "ge\u00b7hin\u00b7dert", "wird", ",", "was", "ei\u00b7gent\u00b7lich", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "VAFIN", "$,", "PRELS", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}