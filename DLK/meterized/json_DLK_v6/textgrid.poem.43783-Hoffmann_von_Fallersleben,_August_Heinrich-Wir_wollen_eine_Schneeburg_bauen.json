{"textgrid.poem.43783": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wir wollen eine Schneeburg bauen", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und eh' es wieder beginnt zu thauen,", "tokens": ["Und", "eh'", "es", "wie\u00b7der", "be\u00b7ginnt", "zu", "thau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da wollen wir eine Schneeburg bauen", "tokens": ["Da", "wol\u00b7len", "wir", "ei\u00b7ne", "Schnee\u00b7burg", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mit hohem Wall und Pfosten und W\u00e4nden", "tokens": ["Mit", "ho\u00b7hem", "Wall", "und", "Pfos\u00b7ten", "und", "W\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gesch\u00fctzt nach allen Ecken und Enden.", "tokens": ["Ge\u00b7sch\u00fctzt", "nach", "al\u00b7len", "E\u00b7cken", "und", "En\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und haben wir uns geh\u00f6rig verschanzet,", "tokens": ["Und", "ha\u00b7ben", "wir", "uns", "ge\u00b7h\u00f6\u00b7rig", "ver\u00b7schan\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit B\u00e4llen reichlich verseh'n von innen,", "tokens": ["Mit", "B\u00e4l\u00b7len", "reich\u00b7lich", "ver\u00b7seh'n", "von", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dann wird die Fahne aufgepflanzet,", "tokens": ["Dann", "wird", "die", "Fah\u00b7ne", "auf\u00b7ge\u00b7pflan\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann m\u00f6gen die Feinde zu st\u00fcrmen beginnen.", "tokens": ["Dann", "m\u00f6\u00b7gen", "die", "Fein\u00b7de", "zu", "st\u00fcr\u00b7men", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Wir wollen uns tapfer von innen wehren,", "tokens": ["Wir", "wol\u00b7len", "uns", "tap\u00b7fer", "von", "in\u00b7nen", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und vertheidigen unsere Festung mit Ehren.", "tokens": ["Und", "ver\u00b7thei\u00b7di\u00b7gen", "un\u00b7se\u00b7re", "Fes\u00b7tung", "mit", "Eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Wir aber werden die Fahne schwingen", "tokens": ["Wir", "a\u00b7ber", "wer\u00b7den", "die", "Fah\u00b7ne", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und jubelnd unser Sturmlied singen", "tokens": ["Und", "ju\u00b7belnd", "un\u00b7ser", "Sturm\u00b7lied", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und siegend in eure Schneeburg dringen.", "tokens": ["Und", "sie\u00b7gend", "in", "eu\u00b7re", "Schnee\u00b7burg", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}}}}}