{"textgrid.poem.63517": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: So weich und warm", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So weich und warm", "tokens": ["So", "weich", "und", "warm"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Hegt dich kein Arm,", "tokens": ["Hegt", "dich", "kein", "Arm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wie dich der Mutter Arm umf\u00e4ngt.", "tokens": ["Wie", "dich", "der", "Mut\u00b7ter", "Arm", "um\u00b7f\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nie findest du", "tokens": ["Nie", "fin\u00b7dest", "du"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "So s\u00fc\u00dfe Ruh,", "tokens": ["So", "s\u00fc\u00b7\u00dfe", "Ruh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Als wenn dein Aug an ihrem h\u00e4ngt.", "tokens": ["Als", "wenn", "dein", "Aug", "an", "ih\u00b7rem", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und kehrt ergreist", "tokens": ["Und", "kehrt", "er\u00b7greist"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Dem m\u00fcden Geist", "tokens": ["Dem", "m\u00fc\u00b7den", "Geist"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Noch manch ein Jugendbild zur\u00fcck,", "tokens": ["Noch", "manch", "ein", "Ju\u00b7gend\u00b7bild", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es gr\u00fc\u00dft dich keins", "tokens": ["Es", "gr\u00fc\u00dft", "dich", "keins"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIAT"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "So milden Scheins,", "tokens": ["So", "mil\u00b7den", "Scheins", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Wie deiner Mutter Segensblick.", "tokens": ["Wie", "dei\u00b7ner", "Mut\u00b7ter", "Se\u00b7gens\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O f\u00fchrt dich nicht", "tokens": ["O", "f\u00fchrt", "dich", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Dies liebe Licht", "tokens": ["Dies", "lie\u00b7be", "Licht"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ins dunkle Leben treulich ein,", "tokens": ["Ins", "dunk\u00b7le", "Le\u00b7ben", "treu\u00b7lich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob auch die Welt", "tokens": ["Ob", "auch", "die", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sich dir gesellt,", "tokens": ["Sich", "dir", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "PPER", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Bist dennoch mutterseelenallein!", "tokens": ["Bist", "den\u00b7noch", "mut\u00b7ter\u00b7see\u00b7len\u00b7al\u00b7lein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}