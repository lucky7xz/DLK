{"dta.poem.18451": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen gro\u00df na\u00df und naseweisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein nasen-k\u00f6nig ging in einer engen ga\u00df/", "tokens": ["Ein", "na\u00b7sen\u00b7k\u00f6\u00b7nig", "ging", "in", "ei\u00b7ner", "en\u00b7gen", "ga\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein na\u00dfwei\u00df rief: Macht raum? der gro\u00df-naas h\u00f6rte das.", "tokens": ["Ein", "na\u00df\u00b7wei\u00df", "rief", ":", "Macht", "raum", "?", "der", "gro\u00df\u00b7naas", "h\u00f6r\u00b7te", "das", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$.", "NN", "PTKVZ", "$.", "ART", "ADJA", "VVFIN", "PDS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sprung an die eine seit/ standt still und sprach zum andern/", "tokens": ["Sprung", "an", "die", "ei\u00b7ne", "seit", "/", "standt", "still", "und", "sprach", "zum", "an\u00b7dern", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ART", "NN", "$(", "VVFIN", "ADJD", "KON", "VVFIN", "APPRART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So gehe du voran/ weil narren zollfrey wandern.", "tokens": ["So", "ge\u00b7he", "du", "vo\u00b7ran", "/", "weil", "nar\u00b7ren", "zoll\u00b7frey", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$(", "KOUS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}