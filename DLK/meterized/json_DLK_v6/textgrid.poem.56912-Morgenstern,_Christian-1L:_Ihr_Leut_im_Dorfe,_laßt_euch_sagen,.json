{"textgrid.poem.56912": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr Leut im Dorfe, la\u00dft euch sagen,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Leut im Dorfe, la\u00dft euch sagen,", "tokens": ["Ihr", "Leut", "im", "Dor\u00b7fe", ",", "la\u00dft", "euch", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die Glock am Turm ist elf.", "tokens": ["die", "Glock", "am", "Turm", "ist", "elf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht lang, so wird es wieder tagen,", "tokens": ["Nicht", "lang", ",", "so", "wird", "es", "wie\u00b7der", "ta\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "ADV", "VAFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "drum auf, und geht zu Bett!", "tokens": ["drum", "auf", ",", "und", "geht", "zu", "Bett", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Denn der nur gilt in der Gemeine,", "tokens": ["Denn", "der", "nur", "gilt", "in", "der", "Ge\u00b7mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "der r\u00fcstig wirkt und schafft,", "tokens": ["der", "r\u00fcs\u00b7tig", "wirkt", "und", "schafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "der sorgt getreulich f\u00fcr die Seinen,", "tokens": ["der", "sorgt", "ge\u00b7treu\u00b7lich", "f\u00fcr", "die", "Sei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ADJD", "APPR", "ART", "PPOSS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "bis ihn der Tod entrafft.", "tokens": ["bis", "ihn", "der", "Tod", "en\u00b7trafft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr Leut im Dorfe, la\u00dft euch sagen,", "tokens": ["Ihr", "Leut", "im", "Dor\u00b7fe", ",", "la\u00dft", "euch", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die Glock am Turm ist elf.", "tokens": ["die", "Glock", "am", "Turm", "ist", "elf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht lang, so wird es wieder tagen,", "tokens": ["Nicht", "lang", ",", "so", "wird", "es", "wie\u00b7der", "ta\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "ADV", "VAFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "drum auf, und geht zu Bett!", "tokens": ["drum", "auf", ",", "und", "geht", "zu", "Bett", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}