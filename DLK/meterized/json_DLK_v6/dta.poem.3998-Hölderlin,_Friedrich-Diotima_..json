{"dta.poem.3998": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Diotima .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Leuchtest Du wie vormals nieder,", "tokens": ["Leuch\u00b7test", "Du", "wie", "vor\u00b7mals", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Goldner Tag! und sprossen mir", "tokens": ["Gold\u00b7ner", "Tag", "!", "und", "spros\u00b7sen", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "KON", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Des Gesanges Blumen wieder", "tokens": ["Des", "Ge\u00b7san\u00b7ges", "Blu\u00b7men", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebenathmend auf zu Dir?", "tokens": ["Le\u00b7be\u00b7nath\u00b7mend", "auf", "zu", "Dir", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie so anders ist's geworden!", "tokens": ["Wie", "so", "an\u00b7ders", "ist's", "ge\u00b7wor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VAFIN", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Manches, was ich traurig mied,", "tokens": ["Man\u00b7ches", ",", "was", "ich", "trau\u00b7rig", "mied", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Stimmt in freundlichen Akkorden", "tokens": ["Stimmt", "in", "freund\u00b7li\u00b7chen", "Ak\u00b7kor\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nun in meiner Freude Lied,", "tokens": ["Nun", "in", "mei\u00b7ner", "Freu\u00b7de", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und mit jedem Stundenschlage", "tokens": ["Und", "mit", "je\u00b7dem", "Stun\u00b7den\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Werd' ich wunderbar gemahnt", "tokens": ["Werd'", "ich", "wun\u00b7der\u00b7bar", "ge\u00b7mahnt"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "An der Kindheit stille Tage,", "tokens": ["An", "der", "Kind\u00b7heit", "stil\u00b7le", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Seit ich sie, die Eine, fand.", "tokens": ["Seit", "ich", "sie", ",", "die", "Ei\u00b7ne", ",", "fand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "PRELS", "ART", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Diotima! edles Leben!", "tokens": ["Dio\u00b7ti\u00b7ma", "!", "ed\u00b7les", "Le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Schwester, heilig mir verwandt!", "tokens": ["Schwes\u00b7ter", ",", "hei\u00b7lig", "mir", "ver\u00b7wandt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eh' ich Dir die Hand gegeben,", "tokens": ["Eh'", "ich", "Dir", "die", "Hand", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hab' ich ferne Dich gekannt.", "tokens": ["Hab'", "ich", "fer\u00b7ne", "Dich", "ge\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Damals schon, da ich in Tr\u00e4umen,", "tokens": ["Da\u00b7mals", "schon", ",", "da", "ich", "in", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir entlokt vom heitern Tag,", "tokens": ["Mir", "ent\u00b7lokt", "vom", "hei\u00b7tern", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unter meines Gartens B\u00e4umen,", "tokens": ["Un\u00b7ter", "mei\u00b7nes", "Gar\u00b7tens", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ein zufriedner Knabe lag,", "tokens": ["Ein", "zu\u00b7fried\u00b7ner", "Kna\u00b7be", "lag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Da in leiser Lust und Sch\u00f6ne", "tokens": ["Da", "in", "lei\u00b7ser", "Lust", "und", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Meiner Seele Mai begann:", "tokens": ["Mei\u00b7ner", "See\u00b7le", "Mai", "be\u00b7gann", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "S\u00e4uselte, wie Zephyrst\u00f6ne,", "tokens": ["S\u00e4u\u00b7sel\u00b7te", ",", "wie", "Ze\u00b7phyr\u00b7st\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "G\u00f6ttliche! Dein Hauch mich an.", "tokens": ["G\u00f6tt\u00b7li\u00b7che", "!", "Dein", "Hauch", "mich", "an", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach! und da, wie eine Sage,", "tokens": ["Ach", "!", "und", "da", ",", "wie", "ei\u00b7ne", "Sa\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "ADV", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeder frohe Gott mir schwand,", "tokens": ["Je\u00b7der", "fro\u00b7he", "Gott", "mir", "schwand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da ich vor des Himmels Tage", "tokens": ["Da", "ich", "vor", "des", "Him\u00b7mels", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Darbend, wie ein Blinder, stand,", "tokens": ["Dar\u00b7bend", ",", "wie", "ein", "Blin\u00b7der", ",", "stand", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da die Last der Zeit mich beugte,", "tokens": ["Da", "die", "Last", "der", "Zeit", "mich", "beug\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Und mein Leben, kalt und bleich,", "tokens": ["Und", "mein", "Le\u00b7ben", ",", "kalt", "und", "bleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sehnend schon hinab sich neigte", "tokens": ["Seh\u00b7nend", "schon", "hin\u00b7ab", "sich", "neig\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "PRF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In der Todten stummes Reich:", "tokens": ["In", "der", "Tod\u00b7ten", "stum\u00b7mes", "Reich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "W\u00fcnscht' ich \u00f6fters noch, dem blinden", "tokens": ["W\u00fcnscht'", "ich", "\u00f6f\u00b7ters", "noch", ",", "dem", "blin\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wanderer, dies Eine mir,", "tokens": ["Wan\u00b7de\u00b7rer", ",", "dies", "Ei\u00b7ne", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "ART", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Meines Herzens Bild zu finden", "tokens": ["Mei\u00b7nes", "Her\u00b7zens", "Bild", "zu", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Bei den Schatten oder hier.", "tokens": ["Bei", "den", "Schat\u00b7ten", "o\u00b7der", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}