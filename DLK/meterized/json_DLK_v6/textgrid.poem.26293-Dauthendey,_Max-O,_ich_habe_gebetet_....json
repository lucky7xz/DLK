{"textgrid.poem.26293": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "O, ich habe gebetet ...", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O, ich habe gebetet unter dem n\u00e4chtlichen Baum,", "tokens": ["O", ",", "ich", "ha\u00b7be", "ge\u00b7be\u00b7tet", "un\u00b7ter", "dem", "n\u00e4cht\u00b7li\u00b7chen", "Baum", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "O, ich habe gebettelt um eine Gnade nur.", "tokens": ["O", ",", "ich", "ha\u00b7be", "ge\u00b7bet\u00b7telt", "um", "ei\u00b7ne", "Gna\u00b7de", "nur", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "VVPP", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "O, ich fragte beim Reichtum der Sterne im Raum.", "tokens": ["O", ",", "ich", "frag\u00b7te", "beim", "Reich\u00b7tum", "der", "Ster\u00b7ne", "im", "Raum", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "G\u00f6tter, lenket das Gl\u00fcck auf meine Spur!", "tokens": ["G\u00f6t\u00b7ter", ",", "len\u00b7ket", "das", "Gl\u00fcck", "auf", "mei\u00b7ne", "Spur", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Ging und wurde schweigend wortlos entlassen.", "tokens": ["Ging", "und", "wur\u00b7de", "schwei\u00b7gend", "wort\u00b7los", "ent\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VAFIN", "ADJD", "ADJD", "VVPP", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Ging und schwieg vor mich hin in den leeren Gassen.", "tokens": ["Ging", "und", "schwieg", "vor", "mich", "hin", "in", "den", "lee\u00b7ren", "Gas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}