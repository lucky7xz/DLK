{"dta.poem.5900": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lucretius fing endlich an zu singen:", "tokens": ["Lu\u00b7cre\u00b7ti\u00b7us", "fing", "end\u00b7lich", "an", "zu", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APZR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als wenn die ", "tokens": ["Als", "wenn", "die"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "KOUS", "ART"], "meter": "++-", "measure": "unknown.measure.di"}, "line.3": {"text": "Von unterschiedner Gr\u00f6\u00df\u2019, und mancherley Figur,", "tokens": ["Von", "un\u00b7ter\u00b7schied\u00b7ner", "Gr\u00f6\u00df'", ",", "und", "man\u00b7cher\u00b7ley", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von einer ew\u2019gen Daur, in steter Festigkeit,", "tokens": ["Von", "ei\u00b7ner", "ew'\u00b7gen", "Daur", ",", "in", "ste\u00b7ter", "Fes\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit stetem Unterscheid,", "tokens": ["Mit", "ste\u00b7tem", "Un\u00b7ter\u00b7scheid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In einem leeren Raum sich mischten und sich schw\u00fcngen.", "tokens": ["In", "ei\u00b7nem", "lee\u00b7ren", "Raum", "sich", "mischten", "und", "sich", "schw\u00fcn\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PRF", "VVFIN", "KON", "PRF", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}