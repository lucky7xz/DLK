{"textgrid.poem.38346": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "F f Fink", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Morgens fr\u00fch, des Abends spat, der Fink hat keine Ruh,", "tokens": ["Des", "Mor\u00b7gens", "fr\u00fch", ",", "des", "A\u00b7bends", "spat", ",", "der", "Fink", "hat", "kei\u00b7ne", "Ruh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "$,", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Die Musen er ins Gr\u00fcne lad't mit seinem Reit her zu,", "tokens": ["Die", "Mu\u00b7sen", "er", "ins", "Gr\u00fc\u00b7ne", "lad't", "mit", "sei\u00b7nem", "Reit", "her", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "APZR", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Fr\u00fch ist gar gut studieren, wenns k\u00fchl, still, ruhig ist,", "tokens": ["Fr\u00fch", "ist", "gar", "gut", "stu\u00b7die\u00b7ren", ",", "wenns", "k\u00fchl", ",", "still", ",", "ru\u00b7hig", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "ADJD", "VVINF", "$,", "KOUS", "ADJD", "$,", "ADJD", "$,", "ADJD", "VAFIN", "$,"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Steh auf und thu's probieren, du fauler", "tokens": ["Steh", "auf", "und", "thu's", "pro\u00b7bie\u00b7ren", ",", "du", "fau\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKVZ", "KON", "NE", "VVINF", "$,", "PPER", "ADJA"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Prinzipist,", "tokens": ["Prin\u00b7zi\u00b7pist", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Grammatist,", "tokens": ["Gram\u00b7ma\u00b7tist", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Syntaxist,", "tokens": ["Syn\u00b7ta\u00b7xist", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.4": {"text": "Humanist.", "tokens": ["Hu\u00b7ma\u00b7nist", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Fr\u00f6hlich der Fink im Fr\u00fchling singt, sa sa, sa sa hui Dieb,", "tokens": ["Fr\u00f6h\u00b7lich", "der", "Fink", "im", "Fr\u00fch\u00b7ling", "singt", ",", "sa", "sa", ",", "sa", "sa", "hui", "Dieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPRART", "NN", "VVFIN", "$,", "FM", "FM", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.2": {"text": "Im ganzen Wald sein Stimm erklingt, wenns Wetter nicht zu tr\u00fcb,", "tokens": ["Im", "gan\u00b7zen", "Wald", "sein", "Stimm", "er\u00b7klingt", ",", "wenns", "Wet\u00b7ter", "nicht", "zu", "tr\u00fcb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "NN", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Die Dieb will er verjagen, die rund heraus er schilt,", "tokens": ["Die", "Dieb", "will", "er", "ver\u00b7ja\u00b7gen", ",", "die", "rund", "he\u00b7raus", "er", "schilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,", "PRELS", "ADJD", "PTKVZ", "PPER", "VVFIN", "$,"], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Dem Sperling thut er sagen, da\u00df er viel Waizen stiehlt.", "tokens": ["Dem", "Sper\u00b7ling", "thut", "er", "sa\u00b7gen", ",", "da\u00df", "er", "viel", "Wai\u00b7zen", "stiehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}