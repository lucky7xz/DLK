{"textgrid.poem.46519": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[in mildem lauem Klima]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In mildem lauem Klima,", "tokens": ["In", "mil\u00b7dem", "lau\u00b7em", "Kli\u00b7ma", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie eines waltet unter", "tokens": ["Wie", "ei\u00b7nes", "wal\u00b7tet", "un\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Italiens Sonne, oder", "tokens": ["I\u00b7ta\u00b7li\u00b7ens", "Son\u00b7ne", ",", "o\u00b7der"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "NN", "$,", "KON"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Auf Raphaels Gem\u00e4lden,", "tokens": ["Auf", "Ra\u00b7phaels", "Ge\u00b7m\u00e4l\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Gedeihn nur solche Knospen", "tokens": ["Ge\u00b7deihn", "nur", "sol\u00b7che", "Knos\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von Sch\u00f6nheit, wie ihr waret,", "tokens": ["Von", "Sch\u00f6n\u00b7heit", ",", "wie", "ihr", "wa\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zu v\u00f6lliger Entwicklung,", "tokens": ["Zu", "v\u00f6l\u00b7li\u00b7ger", "Ent\u00b7wick\u00b7lung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ohn' Abbruch und Verkr\u00fcpplung.", "tokens": ["Ohn'", "Ab\u00b7bruch", "und", "Ver\u00b7kr\u00fcpp\u00b7lung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wie werdet ihr gedeihen,", "tokens": ["Wie", "wer\u00b7det", "ihr", "ge\u00b7dei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Dacht' ich hier voll Besorgni\u00df,", "tokens": ["Dacht'", "ich", "hier", "voll", "Be\u00b7sorg\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "In wildem rauhem Klima?", "tokens": ["In", "wil\u00b7dem", "rau\u00b7hem", "Kli\u00b7ma", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "In wildem rauhem Klima,", "tokens": ["In", "wil\u00b7dem", "rau\u00b7hem", "Kli\u00b7ma", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wie wird der reine Spiegel", "tokens": ["Wie", "wird", "der", "rei\u00b7ne", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Anmuth bald sich tr\u00fcben,", "tokens": ["Der", "An\u00b7muth", "bald", "sich", "tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Bl\u00fcthendrang der Knospen", "tokens": ["Der", "Bl\u00fc\u00b7then\u00b7drang", "der", "Knos\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sich im Aufbrechen stumpfen,", "tokens": ["Sich", "im", "Auf\u00b7bre\u00b7chen", "stump\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verschrumpfen und verdumpfen!", "tokens": ["Ver\u00b7schrump\u00b7fen", "und", "ver\u00b7dump\u00b7fen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Darum seid ihr, o Weh mir", "tokens": ["Da\u00b7rum", "seid", "ihr", ",", "o", "Weh", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "$,", "FM", "NN", "PPER"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Heil euch, ihr seid gegangen", "tokens": ["Heil", "euch", ",", "ihr", "seid", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PPER", "VAFIN", "VVPP"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "Und bl\u00fchet nun im Himmel,", "tokens": ["Und", "bl\u00fc\u00b7het", "nun", "im", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und bl\u00fcht in meinem Liede;", "tokens": ["Und", "bl\u00fcht", "in", "mei\u00b7nem", "Lie\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Ihr bl\u00fchet hier und dorten", "tokens": ["Ihr", "bl\u00fc\u00b7het", "hier", "und", "dor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "In mildem lauem Klima.", "tokens": ["In", "mil\u00b7dem", "lau\u00b7em", "Kli\u00b7ma", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}