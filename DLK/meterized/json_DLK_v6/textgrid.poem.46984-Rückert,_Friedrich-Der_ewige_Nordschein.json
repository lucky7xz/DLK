{"textgrid.poem.46984": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der ewige Nordschein", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Himmel ist ein Flammenrot,", "tokens": ["Am", "Him\u00b7mel", "ist", "ein", "Flam\u00b7men\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es ist nicht Abendr\u00f6te,", "tokens": ["Es", "ist", "nicht", "A\u00b7bend\u00b7r\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es ist auch nicht das Morgenrot,", "tokens": ["Es", "ist", "auch", "nicht", "das", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was ist's f\u00fcr eine R\u00f6te?", "tokens": ["Was", "ist's", "f\u00fcr", "ei\u00b7ne", "R\u00f6\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die tief herauf aus Norden bricht,", "tokens": ["Die", "tief", "her\u00b7auf", "aus", "Nor\u00b7den", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und fort und fort verlischet nicht,", "tokens": ["Und", "fort", "und", "fort", "ver\u00b7li\u00b7schet", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "KON", "PTKVZ", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie gestern so noch heute;", "tokens": ["Wie", "ge\u00b7stern", "so", "noch", "heu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ADV", "ADV", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Wer ist, der es mir deute?", "tokens": ["Wer", "ist", ",", "der", "es", "mir", "deu\u00b7te", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da sprach der Geist, der bei mir stand,", "tokens": ["Da", "sprach", "der", "Geist", ",", "der", "bei", "mir", "stand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und deutete, wo's spr\u00fchte,", "tokens": ["Und", "deu\u00b7te\u00b7te", ",", "wo's", "spr\u00fch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zum Himmel auf mit seiner Hand,", "tokens": ["Zum", "Him\u00b7mel", "auf", "mit", "sei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df dran der Finger gl\u00fchte;", "tokens": ["Da\u00df", "dran", "der", "Fin\u00b7ger", "gl\u00fch\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hast du vernommen von der Stadt,", "tokens": ["Hast", "du", "ver\u00b7nom\u00b7men", "von", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die sich gemacht zum Ph\u00f6nix hat,", "tokens": ["Die", "sich", "ge\u00b7macht", "zum", "Ph\u00f6\u00b7nix", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVPP", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Um aus der Flamme Wehen", "tokens": ["Um", "aus", "der", "Flam\u00b7me", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verj\u00fcngt hervorzugehen?", "tokens": ["Ver\u00b7j\u00fcngt", "her\u00b7vor\u00b7zu\u00b7ge\u00b7hen", "?"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein Jahr ist, seit sie ausgebrannt,", "tokens": ["Ein", "Jahr", "ist", ",", "seit", "sie", "aus\u00b7ge\u00b7brannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch steht des Scheines Helle", "tokens": ["Doch", "steht", "des", "Schei\u00b7nes", "Hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Noch leuchtend \u00fcber allem Land,", "tokens": ["Noch", "leuch\u00b7tend", "\u00fc\u00b7ber", "al\u00b7lem", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und auf derselben Stelle.", "tokens": ["Und", "auf", "der\u00b7sel\u00b7ben", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vergehn wird noch ein ander Jahr,", "tokens": ["Ver\u00b7gehn", "wird", "noch", "ein", "an\u00b7der", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und stehn der Schein wird immerdar,", "tokens": ["Und", "stehn", "der", "Schein", "wird", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vergehn noch viele Jahre,", "tokens": ["Ver\u00b7gehn", "noch", "vie\u00b7le", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und stehn der Schein, der klare.", "tokens": ["Und", "stehn", "der", "Schein", ",", "der", "kla\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Solang' als Gottes Odem weht", "tokens": ["So\u00b7lang'", "als", "Got\u00b7tes", "O\u00b7dem", "weht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Himmelsstr\u00f6me feuchten,", "tokens": ["Und", "Him\u00b7mels\u00b7str\u00f6\u00b7me", "feuch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wird dieser Schein, der nie vergeht,", "tokens": ["Wird", "die\u00b7ser", "Schein", ",", "der", "nie", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem, der ihn sehn kann, leuchten.", "tokens": ["Dem", ",", "der", "ihn", "sehn", "kann", ",", "leuch\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Weit \u00fcber Raum und \u00fcber Zeit,", "tokens": ["Weit", "\u00fc\u00b7ber", "Raum", "und", "\u00fc\u00b7ber", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Zeugnis seiner Herrlichkeit", "tokens": ["Ein", "Zeug\u00b7nis", "sei\u00b7ner", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wird Gott ihn lassen funkeln;", "tokens": ["Wird", "Gott", "ihn", "las\u00b7sen", "fun\u00b7keln", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wer will den Schein verdunkeln?", "tokens": ["Wer", "will", "den", "Schein", "ver\u00b7dun\u00b7keln", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}