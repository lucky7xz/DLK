{"textgrid.poem.28694": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gesundheit ist der beste Schmuck; den wirfft man \u00fcber Hauffen", "tokens": ["Ge\u00b7sund\u00b7heit", "ist", "der", "bes\u00b7te", "Schmuck", ";", "den", "wirfft", "man", "\u00fc\u00b7ber", "Hauf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$.", "ART", "VVFIN", "PIS", "APPR", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Durch Geilheit, Mutwill, M\u00fcssiggang, durch fressen und durch sauffen", "tokens": ["Durch", "Geil\u00b7heit", ",", "Mut\u00b7will", ",", "M\u00fcs\u00b7sig\u00b7gang", ",", "durch", "fres\u00b7sen", "und", "durch", "sauf\u00b7fen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "VMFIN", "$,", "NN", "$,", "APPR", "ADJA", "KON", "APPR", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Und meint, es sey dann au\u00dfgericht durch sch\u00f6ner Kleider kauffen.", "tokens": ["Und", "meint", ",", "es", "sey", "dann", "au\u00df\u00b7ge\u00b7richt", "durch", "sch\u00f6\u00b7ner", "Klei\u00b7der", "kauf\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "VVPP", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}