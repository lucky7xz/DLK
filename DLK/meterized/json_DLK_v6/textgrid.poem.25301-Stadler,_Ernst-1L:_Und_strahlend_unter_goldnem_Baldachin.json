{"textgrid.poem.25301": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und strahlend unter goldnem Baldachin", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und strahlend unter goldnem Baldachin", "tokens": ["Und", "strah\u00b7lend", "un\u00b7ter", "gold\u00b7nem", "Bal\u00b7da\u00b7ch\u00b7in"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "um starre Wipfel funkelnd hingebreitet", "tokens": ["um", "star\u00b7re", "Wip\u00b7fel", "fun\u00b7kelnd", "hin\u00b7ge\u00b7brei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und Kronen tragend gehn wir hin", "tokens": ["und", "Kro\u00b7nen", "tra\u00b7gend", "gehn", "wir", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADJD", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und fl\u00fcsternd gleitet", "tokens": ["und", "fl\u00fcs\u00b7ternd", "glei\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "dein s\u00fc\u00dfer Tritt ged\u00e4mpft im bunten Laub.", "tokens": ["dein", "s\u00fc\u00b7\u00dfer", "Tritt", "ge\u00b7d\u00e4mpft", "im", "bun\u00b7ten", "Laub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Aus wilden schwanken lachenden Girlanden", "tokens": ["Aus", "wil\u00b7den", "schwan\u00b7ken", "la\u00b7chen\u00b7den", "Gir\u00b7lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "rieselt's wie goldner Staub", "tokens": ["rie\u00b7selt's", "wie", "gold\u00b7ner", "Staub"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "und webt sich flie\u00dfend ein in den Gewanden", "tokens": ["und", "webt", "sich", "flie\u00b7\u00dfend", "ein", "in", "den", "Ge\u00b7wan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "und heftet wie Juwelen schwer", "tokens": ["und", "hef\u00b7tet", "wie", "Ju\u00b7we\u00b7len", "schwer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "sich dir ins Haar und jagt vom Licht gehetzt", "tokens": ["sich", "dir", "ins", "Haar", "und", "jagt", "vom", "Licht", "ge\u00b7hetzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "PPER", "APPRART", "NN", "KON", "VVFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "in grellen Wirbeln vor uns her", "tokens": ["in", "grel\u00b7len", "Wir\u00b7beln", "vor", "uns", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "PRF", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "und sinkt aufstiebend in das wirre Meer", "tokens": ["und", "sinkt", "auf\u00b7stie\u00b7bend", "in", "das", "wir\u00b7re", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "kr\u00e4uselnder Bl\u00e4tter die vom Abendduft genetzt", "tokens": ["kr\u00e4u\u00b7seln\u00b7der", "Bl\u00e4t\u00b7ter", "die", "vom", "A\u00b7bend\u00b7duft", "ge\u00b7netzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "APPRART", "NN", "VVPP"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.14": {"text": "wie goldgewirkte Teppiche sich spannen ...", "tokens": ["wie", "gold\u00b7ge\u00b7wirk\u00b7te", "Tep\u00b7pi\u00b7che", "sich", "span\u00b7nen", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PRF", "VVINF", "$("], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Nun lischt im fernsten Feld der letzte Laut.", "tokens": ["Nun", "lischt", "im", "ferns\u00b7ten", "Feld", "der", "letz\u00b7te", "Laut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vom Feuer leis umgl\u00fcht ragen die Tannen.", "tokens": ["Vom", "Feu\u00b7er", "leis", "um\u00b7gl\u00fcht", "ra\u00b7gen", "die", "Tan\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein feiner d\u00fcnner Nebel staut", "tokens": ["Ein", "fei\u00b7ner", "d\u00fcn\u00b7ner", "Ne\u00b7bel", "staut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und schlingt sich b\u00e4umend um zerm\u00fcrbte Reiser", "tokens": ["und", "schlingt", "sich", "b\u00e4u\u00b7mend", "um", "zer\u00b7m\u00fcrb\u00b7te", "Rei\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und irgendwo zerf\u00e4llt ein irres Rufen.", "tokens": ["und", "ir\u00b7gend\u00b7wo", "zer\u00b7f\u00e4llt", "ein", "ir\u00b7res", "Ru\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und deiner Schleppe Goldsaum knistert leiser", "tokens": ["Und", "dei\u00b7ner", "Schlep\u00b7pe", "Gold\u00b7saum", "knis\u00b7tert", "lei\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und atmend steigen wir auf steilen Stufen.", "tokens": ["und", "at\u00b7mend", "stei\u00b7gen", "wir", "auf", "stei\u00b7len", "Stu\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weit w\u00e4chst das Land von Schatten feucht umballt.", "tokens": ["Weit", "w\u00e4chst", "das", "Land", "von", "Schat\u00b7ten", "feucht", "um\u00b7ballt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Drohend aus Nebeln reckt sich Baum an Baum.", "tokens": ["Dro\u00b7hend", "aus", "Ne\u00b7beln", "reckt", "sich", "Baum", "an", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVFIN", "PRF", "NE", "APPR", "NE", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Und schwarz umf\u00e4ngt uns schon der gro\u00dfe Wald.", "tokens": ["Und", "schwarz", "um\u00b7f\u00e4ngt", "uns", "schon", "der", "gro\u00b7\u00dfe", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und dunkel tr\u00e4gt uns schon der gro\u00dfe Traum.", "tokens": ["Und", "dun\u00b7kel", "tr\u00e4gt", "uns", "schon", "der", "gro\u00b7\u00dfe", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}