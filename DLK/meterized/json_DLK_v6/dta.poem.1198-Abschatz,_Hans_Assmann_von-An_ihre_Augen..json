{"dta.poem.1198": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "An ihre Augen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin kein Adler nicht/ der deiner Sonnen Blincken/", "tokens": ["Ich", "bin", "kein", "Ad\u00b7ler", "nicht", "/", "der", "dei\u00b7ner", "Son\u00b7nen", "Blin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "PTKNEG", "$(", "ART", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der deiner Wangen Glantz kan schauen unverwandt.", "tokens": ["Der", "dei\u00b7ner", "Wan\u00b7gen", "Glantz", "kan", "schau\u00b7en", "un\u00b7ver\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VMFIN", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann deiner Augen Glutt in meinen widerstralt/", "tokens": ["Wann", "dei\u00b7ner", "Au\u00b7gen", "Glutt", "in", "mei\u00b7nen", "wi\u00b7der\u00b7stralt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihrer Flammen Schein auff meine Wangen mahlt/", "tokens": ["Und", "ih\u00b7rer", "Flam\u00b7men", "Schein", "auff", "mei\u00b7ne", "Wan\u00b7gen", "mahlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So m\u00fcssen sie besch\u00e4mt zur Erde niedersincken;", "tokens": ["So", "m\u00fcs\u00b7sen", "sie", "be\u00b7sch\u00e4mt", "zur", "Er\u00b7de", "nie\u00b7der\u00b7sin\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch aber will ich nicht der scheuen Eule gleichen/", "tokens": ["Doch", "a\u00b7ber", "will", "ich", "nicht", "der", "scheu\u00b7en", "Eu\u00b7le", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die vor des Tages Zier erwehlt die braune Nacht;", "tokens": ["Die", "vor", "des", "Ta\u00b7ges", "Zier", "er\u00b7wehlt", "die", "brau\u00b7ne", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich eile nach dem Feur/ das mich zu Asche macht", "tokens": ["Ich", "ei\u00b7le", "nach", "dem", "Feur", "/", "das", "mich", "zu", "A\u00b7sche", "macht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "PRELS", "PRF", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Verdirbt die M\u00fccke gleich durch selbst-gesuchten Brand/", "tokens": ["Ver\u00b7dirbt", "die", "M\u00fc\u00b7cke", "gleich", "durch", "selbst\u00b7ge\u00b7such\u00b7ten", "Brand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der edle Ph\u00f6nix wird doch eben so zur Leichen.", "tokens": ["Der", "ed\u00b7le", "Ph\u00f6\u00b7nix", "wird", "doch", "e\u00b7ben", "so", "zur", "Lei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}