{"textgrid.poem.67424": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ideen, wenn es nicht Phantome sind,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ideen, wenn es nicht Phantome sind,", "tokens": ["I\u00b7deen", ",", "wenn", "es", "nicht", "Phan\u00b7to\u00b7me", "sind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PTKNEG", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sie schleichen sich, in etwas schwerer Tracht,", "tokens": ["Sie", "schlei\u00b7chen", "sich", ",", "in", "et\u00b7was", "schwe\u00b7rer", "Tracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu Deiner Abendlampe. Mustre sie,", "tokens": ["Zu", "Dei\u00b7ner", "A\u00b7ben\u00b7dlam\u00b7pe", ".", "Must\u00b7re", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du Weise, Liebliche, und w\u00e4hle Dir,", "tokens": ["Du", "Wei\u00b7se", ",", "Lieb\u00b7li\u00b7che", ",", "und", "w\u00e4h\u00b7le", "Dir", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "$,", "KON", "VVIMP", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn ein' und andre sich das Gl\u00fcck verdient,", "tokens": ["Wenn", "ein'", "und", "and\u00b7re", "sich", "das", "Gl\u00fcck", "ver\u00b7dient", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "KON", "PIS", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zur stillen Freundin diese, jene zu", "tokens": ["Zur", "stil\u00b7len", "Freun\u00b7din", "die\u00b7se", ",", "je\u00b7ne", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PDAT", "$,", "PDS", "PTKZU"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Rathgeberinnen, Tr\u00f6sterinnen, und", "tokens": ["Rath\u00b7ge\u00b7be\u00b7rin\u00b7nen", ",", "Tr\u00f6s\u00b7te\u00b7rin\u00b7nen", ",", "und"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "KON"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Wozu Du ihren treuen Dienst sonst magst;", "tokens": ["Wo\u00b7zu", "Du", "ih\u00b7ren", "treu\u00b7en", "Dienst", "sonst", "magst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "ADJA", "NN", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die andern sende mit Protest zur\u00fcck,", "tokens": ["Die", "an\u00b7dern", "sen\u00b7de", "mit", "Pro\u00b7test", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wie oder la\u00df sie Schw\u00e4tzerinnen sein,", "tokens": ["Wie", "o\u00b7der", "la\u00df", "sie", "Schw\u00e4t\u00b7ze\u00b7rin\u00b7nen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KON", "VVIMP", "PPER", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die Dir die Stunden k\u00fcrzen. Wie es sei!", "tokens": ["Die", "Dir", "die", "Stun\u00b7den", "k\u00fcr\u00b7zen", ".", "Wie", "es", "sei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVINF", "$.", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Nur ihrem k\u00fchnen Autor bleibe hold", "tokens": ["Nur", "ih\u00b7rem", "k\u00fch\u00b7nen", "Au\u00b7tor", "blei\u00b7be", "hold"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und lebe, edle Weise, lebe wohl!", "tokens": ["Und", "le\u00b7be", ",", "ed\u00b7le", "Wei\u00b7se", ",", "le\u00b7be", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJA", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}