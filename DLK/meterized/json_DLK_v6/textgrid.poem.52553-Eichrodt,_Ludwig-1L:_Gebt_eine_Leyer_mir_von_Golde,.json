{"textgrid.poem.52553": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gebt eine Leyer mir von Golde,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gebt eine Leyer mir von Golde,", "tokens": ["Gebt", "ei\u00b7ne", "Le\u00b7yer", "mir", "von", "Gol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPER", "APPR", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Gebt mir des Zephyrs zarte Hand,", "tokens": ["Gebt", "mir", "des", "Ze\u00b7phyrs", "zar\u00b7te", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df mir ein Lied auf jene Holde", "tokens": ["Da\u00df", "mir", "ein", "Lied", "auf", "je\u00b7ne", "Hol\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gelinge, die mein Herz verstand!", "tokens": ["Ge\u00b7lin\u00b7ge", ",", "die", "mein", "Herz", "ver\u00b7stand", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein hoher Stern ging sie vor\u00fcber", "tokens": ["Ein", "ho\u00b7her", "Stern", "ging", "sie", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An meines Lebens D\u00fcsterheit,", "tokens": ["An", "mei\u00b7nes", "Le\u00b7bens", "D\u00fcs\u00b7ter\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und manche Wolke ward noch tr\u00fcber", "tokens": ["Und", "man\u00b7che", "Wol\u00b7ke", "ward", "noch", "tr\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seitdem als sonst in tr\u00fcber Zeit.", "tokens": ["Seit\u00b7dem", "als", "sonst", "in", "tr\u00fc\u00b7ber", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOKOM", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gef\u00e4llig Jedem, nie gefallsam,", "tokens": ["Ge\u00b7f\u00e4l\u00b7lig", "Je\u00b7dem", ",", "nie", "ge\u00b7fall\u00b7sam", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gef\u00e4hrlich immer meiner Ruh,", "tokens": ["Ge\u00b7f\u00e4hr\u00b7lich", "im\u00b7mer", "mei\u00b7ner", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schlo\u00df sie mit ihrer Anmuth Balsam", "tokens": ["Schlo\u00df", "sie", "mit", "ih\u00b7rer", "An\u00b7muth", "Bal\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zugleich der Seele Wunden zu.", "tokens": ["Zu\u00b7gleich", "der", "See\u00b7le", "Wun\u00b7den", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mit solchem Wuchse, reich an Jugend,", "tokens": ["Mit", "sol\u00b7chem", "Wuch\u00b7se", ",", "reich", "an", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit solchem Blick, in Gluthen mild,", "tokens": ["Mit", "sol\u00b7chem", "Blick", ",", "in", "Glu\u00b7then", "mild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "War sie der Sch\u00f6nheit und der Tugend", "tokens": ["War", "sie", "der", "Sch\u00f6n\u00b7heit", "und", "der", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verwirrend und erl\u00f6send Bild.", "tokens": ["Ver\u00b7wir\u00b7rend", "und", "er\u00b7l\u00f6\u00b7send", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Soll ich vergessen, soll gedenken", "tokens": ["Soll", "ich", "ver\u00b7ges\u00b7sen", ",", "soll", "ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "VVPP", "$,", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich ihrer oft mit s\u00fc\u00dfem Schmerz?", "tokens": ["Ich", "ih\u00b7rer", "oft", "mit", "s\u00fc\u00b7\u00dfem", "Schmerz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vergessen hei\u00dft, ich soll sie kr\u00e4nken,", "tokens": ["Ver\u00b7ges\u00b7sen", "hei\u00dft", ",", "ich", "soll", "sie", "kr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erinnerung bricht mir das Herz.", "tokens": ["E\u00b7rin\u00b7ne\u00b7rung", "bricht", "mir", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}