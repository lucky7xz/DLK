{"textgrid.poem.44496": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "In das Gutenberg-Album", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du lichte, schwarze Kunst!", "tokens": ["Du", "lich\u00b7te", ",", "schwar\u00b7ze", "Kunst", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ob Gutenbergs, ob Fausts,", "tokens": ["Ob", "Gu\u00b7ten\u00b7bergs", ",", "ob", "Fausts", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "War man mit Recht in Zweifel;", "tokens": ["War", "man", "mit", "Recht", "in", "Zwei\u00b7fel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn halb stammst du von Gott,", "tokens": ["Denn", "halb", "stammst", "du", "von", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und halb hat dich der Teufel.", "tokens": ["Und", "halb", "hat", "dich", "der", "Teu\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch la\u00dft, wie sehr besorgt,", "tokens": ["Doch", "la\u00dft", ",", "wie", "sehr", "be\u00b7sorgt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vom Feind euch nicht erschrecken,", "tokens": ["Vom", "Feind", "euch", "nicht", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gott hat ihm Macht geborgt,", "tokens": ["Gott", "hat", "ihm", "Macht", "ge\u00b7borgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er dient nur Gottes Zwecken.", "tokens": ["Er", "dient", "nur", "Got\u00b7tes", "Zwe\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Acker ist so weit,", "tokens": ["Der", "A\u00b7cker", "ist", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wer will ihn \u00fcberblicken,", "tokens": ["Wer", "will", "ihn", "\u00fc\u00b7berb\u00b7li\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Sichel h\u00e4lt die Zeit,", "tokens": ["Die", "Si\u00b7chel", "h\u00e4lt", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie wird ihn schon beschicken.", "tokens": ["Sie", "wird", "ihn", "schon", "be\u00b7schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wenn auch Unkraut w\u00e4chst,", "tokens": ["Und", "wenn", "auch", "Un\u00b7kraut", "w\u00e4chst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So h\u00fctet euch vor J\u00e4ten,", "tokens": ["So", "h\u00fc\u00b7tet", "euch", "vor", "J\u00e4\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr k\u00f6nntet im Bem\u00fchn", "tokens": ["Ihr", "k\u00f6nn\u00b7tet", "im", "Be\u00b7m\u00fchn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die gute Saat zertreten.", "tokens": ["Die", "gu\u00b7te", "Saat", "zer\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}