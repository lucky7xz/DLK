{"textgrid.poem.61212": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mich will's bed\u00fcnken fast gleich einem Schwanke,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich will's bed\u00fcnken fast gleich einem Schwanke,", "tokens": ["Mich", "will's", "be\u00b7d\u00fcn\u00b7ken", "fast", "gleich", "ei\u00b7nem", "Schwan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df dieses Inselreich, das kleine, schwache,", "tokens": ["Da\u00df", "die\u00b7ses", "In\u00b7sel\u00b7reich", ",", "das", "klei\u00b7ne", ",", "schwa\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aufb\u00e4umend wie ein zorn'ger Meeresdrache,", "tokens": ["Auf\u00b7b\u00e4u\u00b7mend", "wie", "ein", "zorn'\u00b7ger", "Mee\u00b7res\u00b7dra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich wider uns erhebt zu grimmem Zanke.", "tokens": ["Sich", "wi\u00b7der", "uns", "er\u00b7hebt", "zu", "grim\u00b7mem", "Zan\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn ", "tokens": ["Denn"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "F\u00fcr solchen Trotz es da in blut'ger Lache,", "tokens": ["F\u00fcr", "sol\u00b7chen", "Trotz", "es", "da", "in", "blut'\u00b7ger", "La\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es sei denn, da\u00df vor unsrer starken Rache", "tokens": ["Es", "sei", "denn", ",", "da\u00df", "vor", "uns\u00b7rer", "star\u00b7ken", "Ra\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KOUS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Slaw' es wolle schirmen oder Franke.", "tokens": ["Der", "Sla\u00b7w'", "es", "wol\u00b7le", "schir\u00b7men", "o\u00b7der", "Fran\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VMFIN", "VVINF", "KON", "NN", "$."], "meter": "-++-+-+-+-+-", "measure": "unknown.measure.hexa"}}, "stanza.3": {"line.1": {"text": "Doch w\u00e4r' es so, und spie' aus seinen Kreisen", "tokens": ["Doch", "w\u00e4r'", "es", "so", ",", "und", "spie'", "aus", "sei\u00b7nen", "Krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Eispol Scharen her wie Sand am Meere,", "tokens": ["Der", "Eis\u00b7pol", "Scha\u00b7ren", "her", "wie", "Sand", "am", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "KOKOM", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und br\u00fcllte Frankreich, seinen Ruhm zu speisen:", "tokens": ["Und", "br\u00fcll\u00b7te", "Fran\u00b7kreich", ",", "sei\u00b7nen", "Ruhm", "zu", "spei\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Auf dann, mein Volk, die Herzen hoch, die Speere!", "tokens": ["Auf", "dann", ",", "mein", "Volk", ",", "die", "Her\u00b7zen", "hoch", ",", "die", "Spee\u00b7re", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "ADJD", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann g\u00e4lt' es erst, im Kampf uns zu erweisen,", "tokens": ["Dann", "g\u00e4lt'", "es", "erst", ",", "im", "Kampf", "uns", "zu", "er\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "APPRART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im ein'gen Riesenkampf um Deutschlands Ehre.", "tokens": ["Im", "ein'\u00b7gen", "Rie\u00b7sen\u00b7kampf", "um", "Deutschlands", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}