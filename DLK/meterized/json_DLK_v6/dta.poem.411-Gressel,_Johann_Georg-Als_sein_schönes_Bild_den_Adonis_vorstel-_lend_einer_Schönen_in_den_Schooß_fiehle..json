{"dta.poem.411": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sein sch\u00f6nes Bild/ den  Adonis  vorstel-  \n lend/ einer Sch\u00f6nen in den Schoo\u00df fiehle.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Weil sich das Artige dem Sch\u00f6nen zugesellet/", "tokens": ["Weil", "sich", "das", "Ar\u00b7ti\u00b7ge", "dem", "Sch\u00f6\u00b7nen", "zu\u00b7ge\u00b7sel\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So mu\u00df ein sch\u00f6nes Kind auch hier Besitzer seyn/", "tokens": ["So", "mu\u00df", "ein", "sch\u00f6\u00b7nes", "Kind", "auch", "hier", "Be\u00b7sit\u00b7zer", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "ADV", "ADV", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Spruch der ist gemacht/ das Urtheil ist gef\u00e4llet/", "tokens": ["Der", "Spruch", "der", "ist", "ge\u00b7macht", "/", "das", "Ur\u00b7theil", "ist", "ge\u00b7f\u00e4l\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "VVPP", "$(", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es stellet sich dis Bild bey ihrer Sch\u00f6nheit ein.", "tokens": ["Es", "stel\u00b7let", "sich", "dis", "Bild", "bey", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PDS", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es will ein Sclave seyn von den so sch\u00f6nen H\u00e4nden", "tokens": ["Es", "will", "ein", "Scla\u00b7ve", "seyn", "von", "den", "so", "sch\u00f6\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "APPR", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gleich wie mein Geist auch schon die sch\u00f6nen Fesseln k\u00fc\u00dft.", "tokens": ["Gleich", "wie", "mein", "Geist", "auch", "schon", "die", "sch\u00f6\u00b7nen", "Fes\u00b7seln", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPOSAT", "NN", "ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "L\u00e4\u00dft sie nun/ sch\u00f6nstes Kind/ sich nicht das Bild entwenden/", "tokens": ["L\u00e4\u00dft", "sie", "nun", "/", "sch\u00f6ns\u00b7tes", "Kind", "/", "sich", "nicht", "das", "Bild", "ent\u00b7wen\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$(", "ADJA", "NN", "$(", "PRF", "PTKNEG", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wei\u00df ich/ da\u00df noch Trost vor mich zu hoffen ist.", "tokens": ["So", "wei\u00df", "ich", "/", "da\u00df", "noch", "Trost", "vor", "mich", "zu", "hof\u00b7fen", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "ADV", "NN", "APPR", "PPER", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}