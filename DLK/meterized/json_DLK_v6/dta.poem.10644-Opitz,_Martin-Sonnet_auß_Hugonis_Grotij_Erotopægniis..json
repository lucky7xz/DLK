{"dta.poem.10644": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet au\u00df Hugonis Grotij  \n  Erotop\u00e6gniis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.71", "no:0.28"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Lieb/ so offte mir meinarme krancke Sinnen", "tokens": ["Mein", "Lieb", "/", "so", "off\u00b7te", "mir", "mein\u00b7ar\u00b7me", "kran\u00b7cke", "Sin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Himlisches vnd mein betr\u00fcbtes Angesicht.", "tokens": ["Dein", "Him\u00b7li\u00b7sches", "vnd", "mein", "be\u00b7tr\u00fcb\u00b7tes", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Einbildet/ wundert mich da\u00df deiner Augen Licht", "tokens": ["Ein\u00b7bil\u00b7det", "/", "wun\u00b7dert", "mich", "da\u00df", "dei\u00b7ner", "Au\u00b7gen", "Licht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$(", "VVFIN", "PPER", "KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich nicht erweichen lest/ durch meiner Augen rinnen/", "tokens": ["Sich", "nicht", "er\u00b7wei\u00b7chen", "lest", "/", "durch", "mei\u00b7ner", "Au\u00b7gen", "rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "VVINF", "VVFIN", "$(", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wann aber ich nicht kan dein Huld vnd Gunst gewinnen/", "tokens": ["Wann", "a\u00b7ber", "ich", "nicht", "kan", "dein", "Huld", "vnd", "Gunst", "ge\u00b7win\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PTKNEG", "VMFIN", "PPOSAT", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Warumb ergeussestu den grossen Zorn auch nicht/", "tokens": ["Wa\u00b7rumb", "er\u00b7geus\u00b7se\u00b7stu", "den", "gros\u00b7sen", "Zorn", "auch", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "ADV", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df ich durch seine Macht werdt endtlich hingericht/", "tokens": ["Da\u00df", "ich", "durch", "sei\u00b7ne", "Macht", "werdt", "endt\u00b7lich", "hin\u00b7ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Auff da\u00df mein Augen auch dich nicht mehr sehen k\u00fcnnen?", "tokens": ["Auff", "da\u00df", "mein", "Au\u00b7gen", "auch", "dich", "nicht", "mehr", "se\u00b7hen", "k\u00fcn\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPOSAT", "NN", "ADV", "PPER", "PTKNEG", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch gleichwol ob du schon mein Augen w\u00fcrdest blenden/", "tokens": ["Doch", "gleich\u00b7wol", "ob", "du", "schon", "mein", "Au\u00b7gen", "w\u00fcr\u00b7dest", "blen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ob mein Gesichte gleich verdunckelt w\u00fcrde gantz/", "tokens": ["Ob", "mein", "Ge\u00b7sich\u00b7te", "gleich", "ver\u00b7dun\u00b7ckelt", "w\u00fcr\u00b7de", "gantz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVPP", "VAFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Durch deiner Sonnen Liecht/ durch deiner \u00e4uglein glantz/", "tokens": ["Durch", "dei\u00b7ner", "Son\u00b7nen", "Liecht", "/", "durch", "dei\u00b7ner", "\u00e4ug\u00b7lein", "glantz", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$(", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So k\u00f6ntestu doch nicht dich g\u00e4ntzlich von mir wenden/", "tokens": ["So", "k\u00f6n\u00b7tes\u00b7tu", "doch", "nicht", "dich", "g\u00e4ntz\u00b7lich", "von", "mir", "wen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PTKNEG", "PPER", "ADJD", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dieweil mein trawrig Hertz/ ob ich schon were blindt/", "tokens": ["Die\u00b7weil", "mein", "traw\u00b7rig", "Hertz", "/", "ob", "ich", "schon", "we\u00b7re", "blindt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJD", "NN", "$(", "KOUS", "PPER", "ADV", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dich allezeit in sich fest einverleibet findt.", "tokens": ["Dich", "al\u00b7le\u00b7zeit", "in", "sich", "fest", "ein\u00b7ver\u00b7lei\u00b7bet", "findt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PRF", "ADJD", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}