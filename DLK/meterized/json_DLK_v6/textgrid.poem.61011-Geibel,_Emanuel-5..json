{"textgrid.poem.61011": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gebt mir vom Becher nur den Schaum,", "tokens": ["Gebt", "mir", "vom", "Be\u00b7cher", "nur", "den", "Schaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den leichten Schaum der Reben!", "tokens": ["Den", "leich\u00b7ten", "Schaum", "der", "Re\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gebt nur einen fl\u00fcchtigen Liebestraum", "tokens": ["Gebt", "nur", "ei\u00b7nen", "fl\u00fcch\u00b7ti\u00b7gen", "Lie\u00b7best\u00b7raum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Mir f\u00fcr dies fl\u00fcchtige Leben!", "tokens": ["Mir", "f\u00fcr", "dies", "fl\u00fcch\u00b7ti\u00b7ge", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PDS", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Den vollen Zug, das sichre Gut,", "tokens": ["Den", "vol\u00b7len", "Zug", ",", "das", "sich\u00b7re", "Gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich g\u00f6nn' es jedem andern,", "tokens": ["Ich", "g\u00f6nn'", "es", "je\u00b7dem", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "4der fest am eignen Herde ruht;", "tokens": ["fest", "am", "eig\u00b7nen", "Her\u00b7de", "ruht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich aber mu\u00df schweifen und wandern.", "tokens": ["Ich", "a\u00b7ber", "mu\u00df", "schwei\u00b7fen", "und", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "VVINF", "KON", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Mu\u00df schweifen und wandern hin und her", "tokens": ["Mu\u00df", "schwei\u00b7fen", "und", "wan\u00b7dern", "hin", "und", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "KON", "VVFIN", "PTKVZ", "KON", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf allen Pfaden und Wegen,", "tokens": ["Auf", "al\u00b7len", "Pfa\u00b7den", "und", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wohl \u00fcber die Lande, wohl \u00fcber das Meer,", "tokens": ["Wohl", "\u00fc\u00b7ber", "die", "Lan\u00b7de", ",", "wohl", "\u00fc\u00b7ber", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Dem ewigen Lenz entgegen.", "tokens": ["Dem", "e\u00b7wi\u00b7gen", "Lenz", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und wo ein Blick mir freundlich gl\u00e4nzt,", "tokens": ["Und", "wo", "ein", "Blick", "mir", "freund\u00b7lich", "gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wo auf meiner Reise", "tokens": ["Und", "wo", "auf", "mei\u00b7ner", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Gastfreund mir den Wein kredenzt,", "tokens": ["Ein", "Gast\u00b7freund", "mir", "den", "Wein", "kre\u00b7denzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Da sing' ich die alte Weise:", "tokens": ["Da", "sing'", "ich", "die", "al\u00b7te", "Wei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbgebt mir vom Becher nur den Schaum,", "tokens": ["\u00bb", "gebt", "mir", "vom", "Be\u00b7cher", "nur", "den", "Schaum", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPRART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den leichten Schaum der Reben,", "tokens": ["Den", "leich\u00b7ten", "Schaum", "der", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gebt nur einen fl\u00fcchtigen Liebestraum", "tokens": ["Gebt", "nur", "ei\u00b7nen", "fl\u00fcch\u00b7ti\u00b7gen", "Lie\u00b7best\u00b7raum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Mir f\u00fcr dies fl\u00fcchtige Leben!\u00ab", "tokens": ["Mir", "f\u00fcr", "dies", "fl\u00fcch\u00b7ti\u00b7ge", "Le\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPR", "PDS", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}