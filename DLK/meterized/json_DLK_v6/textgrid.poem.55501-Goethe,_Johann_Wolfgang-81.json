{"textgrid.poem.55501": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "81", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Reichen Beifall hattest du erworben,", "tokens": ["Rei\u00b7chen", "Bei\u00b7fall", "hat\u00b7test", "du", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Allgemeine Neigung rein erzielt;", "tokens": ["All\u00b7ge\u00b7mei\u00b7ne", "Nei\u00b7gung", "rein", "er\u00b7zielt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Viel Personen sind in dir gestorben,", "tokens": ["Viel", "Per\u00b7so\u00b7nen", "sind", "in", "dir", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und du hast sie alle gut gespielt.", "tokens": ["Und", "du", "hast", "sie", "al\u00b7le", "gut", "ge\u00b7spielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "PIS", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}