{"dta.poem.8297": {"metadata": {"author": {"name": "Milton, John", "birth": "N.A.", "death": "N.A."}, "title": "Das  \n Verlohrne Paradies.  \n Achter Gesang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1763", "urn": "urn:nbn:de:kobv:b4-200905199734", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Raphael endigte hier. Doch seine bezaubernde Stimme", "tokens": ["Ra\u00b7phael", "en\u00b7dig\u00b7te", "hier", ".", "Doch", "sei\u00b7ne", "be\u00b7zau\u00b7bern\u00b7de", "Stim\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "$.", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Klang noch immer in ", "tokens": ["Klang", "noch", "im\u00b7mer", "in"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Jhn zu h\u00f6ren; und sa\u00df, auf ihn die Gedanken geheftet;", "tokens": ["Jhn", "zu", "h\u00f6\u00b7ren", ";", "und", "sa\u00df", ",", "auf", "ihn", "die", "Ge\u00b7dan\u00b7ken", "ge\u00b7hef\u00b7tet", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$.", "KON", "VVFIN", "$,", "APPR", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Bis er zuletzt, wie von neuem erwacht, erkenntlich versetzte.", "tokens": ["Bis", "er", "zu\u00b7letzt", ",", "wie", "von", "neu\u00b7em", "er\u00b7wacht", ",", "er\u00b7kennt\u00b7lich", "ver\u00b7setz\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "PWAV", "APPR", "ADJA", "VVPP", "$,", "ADJD", "VVFIN", "$."], "meter": "---+--+--+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Welchen Dank, und welche Vergeltung kann ich dir erstatten,", "tokens": ["Wel\u00b7chen", "Dank", ",", "und", "wel\u00b7che", "Ver\u00b7gel\u00b7tung", "kann", "ich", "dir", "er\u00b7stat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "$,", "KON", "PWAT", "NN", "VMFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Gro\u00dfer g\u00f6ttlicher Lehrer so fremder erhabner Geschichte!", "tokens": ["Gro\u00b7\u00dfer", "g\u00f6tt\u00b7li\u00b7cher", "Leh\u00b7rer", "so", "frem\u00b7der", "er\u00b7hab\u00b7ner", "Ge\u00b7schich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "ADV", "ADJA", "ADJA", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Da du den Durst nach Wissenschaft mir so reichlich gestillt hast,", "tokens": ["Da", "du", "den", "Durst", "nach", "Wis\u00b7sen\u00b7schaft", "mir", "so", "reich\u00b7lich", "ge\u00b7stillt", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "PPER", "ADV", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und so geneigt, wie ein Freund, zu mir dich heruntergelassen,", "tokens": ["Und", "so", "ge\u00b7neigt", ",", "wie", "ein", "Freund", ",", "zu", "mir", "dich", "her\u00b7un\u00b7ter\u00b7ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,", "PWAV", "ART", "NN", "$,", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Dinge mir zu erz\u00e4hlen, die ich sonst nimmer erfahren,", "tokens": ["Din\u00b7ge", "mir", "zu", "er\u00b7z\u00e4h\u00b7len", ",", "die", "ich", "sonst", "nim\u00b7mer", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.6": {"text": "Preis und Ehre daf\u00fcr dem gro\u00dfen allm\u00e4chtigen Sch\u00f6pfer,", "tokens": ["Preis", "und", "Eh\u00b7re", "da\u00b7f\u00fcr", "dem", "gro\u00b7\u00dfen", "all\u00b7m\u00e4ch\u00b7ti\u00b7gen", "Sch\u00f6p\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PAV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.7": {"text": "Wie sichs geb\u00fchrt. Doch bleibt mir inde\u00df ein Zweifel zur\u00fccke,", "tokens": ["Wie", "sichs", "ge\u00b7b\u00fchrt", ".", "Doch", "bleibt", "mir", "in\u00b7de\u00df", "ein", "Zwei\u00b7fel", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVPP", "$.", "KON", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Den du allein mir benehmen kannst. Wenn voller Entz\u00fcckung", "tokens": ["Den", "du", "al\u00b7lein", "mir", "be\u00b7neh\u00b7men", "kannst", ".", "Wenn", "vol\u00b7ler", "Ent\u00b7z\u00fc\u00b7ckung"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "PPER", "VVINF", "VMFIN", "$.", "KOUS", "ADJA", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Ich den herrlichen Bau von Himmel und Erde ", "tokens": ["Ich", "den", "herr\u00b7li\u00b7chen", "Bau", "von", "Him\u00b7mel", "und", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Und dagegen die Erde beschau, wie ein Punkt nur, ein Sandkorn,", "tokens": ["Und", "da\u00b7ge\u00b7gen", "die", "Er\u00b7de", "be\u00b7schau", ",", "wie", "ein", "Punkt", "nur", ",", "ein", "Sand\u00b7korn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "NN", "PTKVZ", "$,", "PWAV", "ART", "NN", "ADV", "$,", "ART", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.11": {"text": "Und ein St\u00e4ubchen, verglichen mit jenem stralenden Himmel,", "tokens": ["Und", "ein", "St\u00e4ub\u00b7chen", ",", "ver\u00b7gli\u00b7chen", "mit", "je\u00b7nem", "stra\u00b7len\u00b7den", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "VVPP", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.12": {"text": "Und den unz\u00e4hligen Sternen, die unbegreifliche Kreise", "tokens": ["Und", "den", "un\u00b7z\u00e4h\u00b7li\u00b7gen", "Ster\u00b7nen", ",", "die", "un\u00b7be\u00b7greif\u00b7li\u00b7che", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "--++--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Durchzulaufen scheinen, wie die\u00df aus ihrer Entfernung,", "tokens": ["Durch\u00b7zu\u00b7lau\u00b7fen", "schei\u00b7nen", ",", "wie", "die\u00df", "aus", "ih\u00b7rer", "Ent\u00b7fer\u00b7nung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "PDS", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Blo\u00df, so scheint es, Tag und Nacht der dunkelen Erde", "tokens": ["Blo\u00df", ",", "so", "scheint", "es", ",", "Tag", "und", "Nacht", "der", "dun\u00b7ke\u00b7len", "Er\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "$,", "NN", "KON", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.15": {"text": "Dienstbar zu leuchten; da sonst nach ihrem gewaltigen Umfang,", "tokens": ["Dienst\u00b7bar", "zu", "leuch\u00b7ten", ";", "da", "sonst", "nach", "ih\u00b7rem", "ge\u00b7wal\u00b7ti\u00b7gen", "Um\u00b7fang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$.", "ADV", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+-+-+", "measure": "dactylic.di.plus"}, "line.16": {"text": "Fast kein anderer Nutzen erscheint; wenn ich dieses erw\u00e4ge:", "tokens": ["Fast", "kein", "an\u00b7de\u00b7rer", "Nut\u00b7zen", "er\u00b7scheint", ";", "wenn", "ich", "die\u00b7ses", "er\u00b7w\u00e4\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVFIN", "$.", "KOUS", "PPER", "PDS", "VVFIN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.17": {"text": "Wundert mich oft, wie die weise Natur, die alles so sparsam", "tokens": ["Wun\u00b7dert", "mich", "oft", ",", "wie", "die", "wei\u00b7se", "Na\u00b7tur", ",", "die", "al\u00b7les", "so", "spar\u00b7sam"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "PRELS", "PIS", "ADV", "ADJD"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.18": {"text": "Und mit verschwendrischer Hand so viele gr\u00f6\u00dfere K\u00f6rper", "tokens": ["Und", "mit", "ver\u00b7schwend\u00b7ri\u00b7scher", "Hand", "so", "vie\u00b7le", "gr\u00f6\u00b7\u00dfe\u00b7re", "K\u00f6r\u00b7per"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Blo\u00df zu diesem Endzweck gemacht, so wie es uns vork\u00f6mmt,", "tokens": ["Blo\u00df", "zu", "die\u00b7sem", "End\u00b7zweck", "ge\u00b7macht", ",", "so", "wie", "es", "uns", "vor\u00b7k\u00f6mmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "VVPP", "$,", "ADV", "KOKOM", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "Und so schnelle Bewegung, die Tag und Nacht wiederhohlt wird,", "tokens": ["Und", "so", "schnel\u00b7le", "Be\u00b7we\u00b7gung", ",", "die", "Tag", "und", "Nacht", "wie\u00b7der\u00b7hohlt", "wird", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$,", "ART", "NN", "KON", "NN", "VVPP", "VAFIN", "$,"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.21": {"text": "Jhrer Laufbahn bestimmt, inde\u00df die ruhende Erde,", "tokens": ["Ih\u00b7rer", "Lauf\u00b7bahn", "be\u00b7stimmt", ",", "in\u00b7de\u00df", "die", "ru\u00b7hen\u00b7de", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Von viel edlern Gestirnen, als wie sie selbst ist, bedient wird;", "tokens": ["Von", "viel", "ed\u00b7lern", "Ge\u00b7stir\u00b7nen", ",", "als", "wie", "sie", "selbst", "ist", ",", "be\u00b7dient", "wird", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,", "KOUS", "PWAV", "PPER", "ADV", "VAFIN", "$,", "VVPP", "VAFIN", "$."], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.23": {"text": "Ohne die mindste Bewegung ihr Ziel erreichet, und ruhig", "tokens": ["Oh\u00b7ne", "die", "minds\u00b7te", "Be\u00b7we\u00b7gung", "ihr", "Ziel", "er\u00b7rei\u00b7chet", ",", "und", "ru\u00b7hig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$,", "KON", "ADJD"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Jhren Tribut empf\u00e4ngt von Licht und von W\u00e4rme, der t\u00e4glich", "tokens": ["Ih\u00b7ren", "Tri\u00b7but", "emp\u00b7f\u00e4ngt", "von", "Licht", "und", "von", "W\u00e4r\u00b7me", ",", "der", "t\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$,", "PRELS", "ADJD"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.25": {"text": "Jhr mit einer Eile, mit einer Geschwindigkeit zustr\u00f6mt,", "tokens": ["Ihr", "mit", "ei\u00b7ner", "Ei\u00b7le", ",", "mit", "ei\u00b7ner", "Ge\u00b7schwin\u00b7dig\u00b7keit", "zu\u00b7str\u00f6mt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.26": {"text": "Selber der Phantasie die geh\u00f6rigen Zahlen ermangeln.", "tokens": ["Sel\u00b7ber", "der", "Phan\u00b7ta\u00b7sie", "die", "ge\u00b7h\u00f6\u00b7ri\u00b7gen", "Zah\u00b7len", "er\u00b7man\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}}, "stanza.3": {"line.1": {"text": "Also sagte der erste Vater. An seiner Geb\u00e4rde", "tokens": ["Al\u00b7so", "sag\u00b7te", "der", "ers\u00b7te", "Va\u00b7ter", ".", "An", "sei\u00b7ner", "Ge\u00b7b\u00e4r\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sah man, da\u00df sich sein Geist in ernste verborgne Gedanken", "tokens": ["Sah", "man", ",", "da\u00df", "sich", "sein", "Geist", "in", "erns\u00b7te", "ver\u00b7borg\u00b7ne", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "PRF", "PPOSAT", "NN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Still nachsinnend vertieste. Da ", "tokens": ["Still", "nach\u00b7sin\u00b7nend", "ver\u00b7ties\u00b7te", ".", "Da"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADJD", "VVPP", "VVFIN", "$.", "KOUS"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Jhm im Gesicht sa\u00df, auf, mit majest\u00e4tischer Demuth,", "tokens": ["Jhm", "im", "Ge\u00b7sicht", "sa\u00df", ",", "auf", ",", "mit", "ma\u00b7jes\u00b7t\u00e4\u00b7ti\u00b7scher", "De\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", 