{"dta.poem.9194": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1.  die Freyheit.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Wol dem/ der seinen jungen jahren", "tokens": ["Wol", "dem", "/", "der", "sei\u00b7nen", "jun\u00b7gen", "jah\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$(", "ART", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die s\u00fcsse freyheit g\u00f6nnen kan!", "tokens": ["Die", "s\u00fcs\u00b7se", "frey\u00b7heit", "g\u00f6n\u00b7nen", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er mag die zarten kr\u00e4ffte spahren/", "tokens": ["Er", "mag", "die", "zar\u00b7ten", "kr\u00e4ff\u00b7te", "spah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und legt die stunden besser an/", "tokens": ["Und", "legt", "die", "stun\u00b7den", "bes\u00b7ser", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als einer/ der sich tag und nacht", "tokens": ["Als", "ei\u00b7ner", "/", "der", "sich", "tag", "und", "nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "$(", "PRELS", "PRF", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit liebes-grillen m\u00fcde macht.", "tokens": ["Mit", "lie\u00b7bes\u00b7gril\u00b7len", "m\u00fc\u00b7de", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}