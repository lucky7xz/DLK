{"textgrid.poem.26067": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Du und ich", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du und ich!", "tokens": ["Du", "und", "ich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Wunschlose Seligkeit", "tokens": ["Wun\u00b7schlo\u00b7se", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Str\u00f6mt deine N\u00e4he \u00fcber mich.", "tokens": ["Str\u00f6mt", "dei\u00b7ne", "N\u00e4\u00b7he", "\u00fc\u00b7ber", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Alltag wird zur Sonntagszeit,", "tokens": ["Der", "All\u00b7tag", "wird", "zur", "Sonn\u00b7tags\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Unsterblich schlingt das Leben sich", "tokens": ["U\u00b7nsterb\u00b7lich", "schlingt", "das", "Le\u00b7ben", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PRF"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Um uns. Und Menscheng\u00f6ttlichkeit", "tokens": ["Um", "uns", ".", "Und", "Men\u00b7schen\u00b7g\u00f6tt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KOUI", "PPER", "$.", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fchl' ich bei dir durch dich.", "tokens": ["F\u00fchl'", "ich", "bei", "dir", "durch", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was einst gewesen, wei\u00df ich kaum.", "tokens": ["Was", "einst", "ge\u00b7we\u00b7sen", ",", "wei\u00df", "ich", "kaum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAPP", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die enge Welt wird weiter Raum.", "tokens": ["Die", "en\u00b7ge", "Welt", "wird", "wei\u00b7ter", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Holz wird Eisen, Eisen Holz", "tokens": ["Und", "Holz", "wird", "Ei\u00b7sen", ",", "Ei\u00b7sen", "Holz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "VAFIN", "NN", "$,", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Stolz wird Demut, Demut Stolz.", "tokens": ["Und", "Stolz", "wird", "De\u00b7mut", ",", "De\u00b7mut", "Stolz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "NN", "$,", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gar wunderbare Weisen", "tokens": ["Gar", "wun\u00b7der\u00b7ba\u00b7re", "Wei\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Singt dann bei seinem Kreisen", "tokens": ["Singt", "dann", "bei", "sei\u00b7nem", "Krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mein Blut im Paradies f\u00fcr mich.", "tokens": ["Mein", "Blut", "im", "Pa\u00b7ra\u00b7dies", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es haben alle W\u00fcnsche Ruh', \u2013", "tokens": ["Es", "ha\u00b7ben", "al\u00b7le", "W\u00fcn\u00b7sche", "Ruh'", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ich wei\u00df nicht mehr, wer bist dann du.", "tokens": ["Ich", "wei\u00df", "nicht", "mehr", ",", "wer", "bist", "dann", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "PWS", "VAFIN", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ich wei\u00df nicht mehr, wer bin dann ich.", "tokens": ["Ich", "wei\u00df", "nicht", "mehr", ",", "wer", "bin", "dann", "ich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "PWS", "VAFIN", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}