{"textgrid.poem.48707": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "8. Als der kunstreiche Johan Rudolf Stadeler aus der Schweiz auf dem Meidan zu Ispahan in Persien j\u00e4mmerlich niederges\u00e4belt ward", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein tapfrer Christenmut, du werter Schweizer du,", "tokens": ["Dein", "tapf\u00b7rer", "Chris\u00b7ten\u00b7mut", ",", "du", "wer\u00b7ter", "Schwei\u00b7zer", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ist ewig lobenswert: denn da du kuntest leben,", "tokens": ["ist", "e\u00b7wig", "lo\u00b7bens\u00b7wert", ":", "denn", "da", "du", "kun\u00b7test", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVFIN", "$.", "KON", "KOUS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "hast du dich willig hin in deinen Tod gegeben.", "tokens": ["hast", "du", "dich", "wil\u00b7lig", "hin", "in", "dei\u00b7nen", "Tod", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was deinen Leib bringt umb, das ist ein kurzes Nu.", "tokens": ["Was", "dei\u00b7nen", "Leib", "bringt", "umb", ",", "das", "ist", "ein", "kur\u00b7zes", "Nu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,", "PDS", "VAFIN", "ART", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Seele flog darvon, ihr kam kein S\u00e4bel zu.", "tokens": ["Die", "See\u00b7le", "flog", "dar\u00b7von", ",", "ihr", "kam", "kein", "S\u00e4\u00b7bel", "zu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$,", "PPER", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun siehst du um dich her die Serafinen schweben,", "tokens": ["Nun", "siehst", "du", "um", "dich", "her", "die", "Se\u00b7ra\u00b7fi\u00b7nen", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PRF", "APZR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "schaust auf di\u00df gro\u00dfe Nichts, um welches wir so streben,", "tokens": ["schaust", "auf", "di\u00df", "gro\u00b7\u00dfe", "Nichts", ",", "um", "wel\u00b7ches", "wir", "so", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDS", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "lachst deine M\u00f6rder aus und jauchzest in der Ruh.", "tokens": ["lachst", "dei\u00b7ne", "M\u00f6r\u00b7der", "aus", "und", "jauch\u00b7zest", "in", "der", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Hier ist dein M\u00e4rterkranz, du Redlicher, du Treuer,", "tokens": ["Hier", "ist", "dein", "M\u00e4r\u00b7ter\u00b7kranz", ",", "du", "Red\u00b7li\u00b7cher", ",", "du", "Treu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "den nim mit in dein Grab. Wir wollen deinen Preis", "tokens": ["den", "nim", "mit", "in", "dein", "Grab", ".", "Wir", "wol\u00b7len", "dei\u00b7nen", "Preis"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "APPR", "PPOSAT", "NN", "$.", "PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "durch die erl\u00f6ste Welt bei Allen machen teuer.", "tokens": ["durch", "die", "er\u00b7l\u00f6s\u00b7te", "Welt", "bei", "Al\u00b7len", "ma\u00b7chen", "teu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NE", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dein Vaterland sol sein der Erden weiter Kreis!", "tokens": ["Dein", "Va\u00b7ter\u00b7land", "sol", "sein", "der", "Er\u00b7den", "wei\u00b7ter", "Kreis", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPOSAT", "ART", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer so wie du verdirbt, der bleibet unverdorben,", "tokens": ["Wer", "so", "wie", "du", "ver\u00b7dirbt", ",", "der", "blei\u00b7bet", "un\u00b7ver\u00b7dor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "KOKOM", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "lebt, wenn er nicht mehr lebt, und stirbet ungestorben.", "tokens": ["lebt", ",", "wenn", "er", "nicht", "mehr", "lebt", ",", "und", "stir\u00b7bet", "un\u00b7ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}