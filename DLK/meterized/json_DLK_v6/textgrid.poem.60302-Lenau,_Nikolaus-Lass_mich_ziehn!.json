{"textgrid.poem.60302": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Lass mich ziehn!", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin kein Freund von Sterbensehen;", "tokens": ["Ich", "bin", "kein", "Freund", "von", "Ster\u00b7ben\u00b7se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn deine Liebe soll vergehen,", "tokens": ["Wenn", "dei\u00b7ne", "Lie\u00b7be", "soll", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So sterbe sie allein; ich will", "tokens": ["So", "ster\u00b7be", "sie", "al\u00b7lein", ";", "ich", "will"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit meiner sein allein und still.", "tokens": ["Mit", "mei\u00b7ner", "sein", "al\u00b7lein", "und", "still", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VAINF", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ged\u00e4chtnis wei\u00df getreu von Jahren", "tokens": ["Ge\u00b7d\u00e4cht\u00b7nis", "wei\u00df", "ge\u00b7treu", "von", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Liebeszeichen zu bewahren;", "tokens": ["Die", "Lie\u00b7bes\u00b7zei\u00b7chen", "zu", "be\u00b7wah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn eins dir nach dem andern weicht,", "tokens": ["Wenn", "eins", "dir", "nach", "dem", "an\u00b7dern", "weicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seh ich, wie Tod dein Herz beschleicht.", "tokens": ["Seh", "ich", ",", "wie", "Tod", "dein", "Herz", "be\u00b7schleicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du merkst es nicht, viel ist geblieben;", "tokens": ["Du", "merkst", "es", "nicht", ",", "viel", "ist", "ge\u00b7blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "O Gott! es war ein reiches Lieben;", "tokens": ["O", "Gott", "!", "es", "war", "ein", "rei\u00b7ches", "Lie\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Viel hat der Tod zu knicken doch,", "tokens": ["Viel", "hat", "der", "Tod", "zu", "kni\u00b7cken", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKZU", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis alles aus; er knickt es noch.", "tokens": ["Bis", "al\u00b7les", "aus", ";", "er", "knickt", "es", "noch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du merkst es nicht; mein sind die Schmerzen;", "tokens": ["Du", "merkst", "es", "nicht", ";", "mein", "sind", "die", "Schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PPOSAT", "VAFIN", "ART", "NN", "$."], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Doch leichter wird es deinem Herzen,", "tokens": ["Doch", "leich\u00b7ter", "wird", "es", "dei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da du von mir dich scheidest los,", "tokens": ["Da", "du", "von", "mir", "dich", "schei\u00b7dest", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn Lieben ist ein banges Los.", "tokens": ["Denn", "Lie\u00b7ben", "ist", "ein", "ban\u00b7ges", "Los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie Tod sich mag mit Liebe messen,", "tokens": ["Wie", "Tod", "sich", "mag", "mit", "Lie\u00b7be", "mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bei dir, die ich nicht kann vergessen,", "tokens": ["Bei", "dir", ",", "die", "ich", "nicht", "kann", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "PPER", "PTKNEG", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Will ichs nicht schaun, wenn ichs auch seh", "tokens": ["Will", "ichs", "nicht", "schaun", ",", "wenn", "ichs", "auch", "seh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "PTKNEG", "VVINF", "$,", "KOUS", "PIS", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Schmerze, da\u00df allein ich steh.", "tokens": ["Im", "Schmer\u00b7ze", ",", "da\u00df", "al\u00b7lein", "ich", "steh", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Gut ists, vors Aug die H\u00e4nde schlagen,", "tokens": ["Gut", "ists", ",", "vors", "Aug", "die", "H\u00e4n\u00b7de", "schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist nicht ein Anblick zu ertragen;", "tokens": ["Ist", "nicht", "ein", "An\u00b7blick", "zu", "er\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O k\u00f6nnte so das Herz dem Licht", "tokens": ["O", "k\u00f6nn\u00b7te", "so", "das", "Herz", "dem", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entfliehn beim Anblick, der es bricht!", "tokens": ["Ent\u00b7fliehn", "beim", "An\u00b7blick", ",", "der", "es", "bricht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich glaub es nicht, da\u00df deiner Seele,", "tokens": ["Ich", "glaub", "es", "nicht", ",", "da\u00df", "dei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der sch\u00f6nsten, ewge Liebe fehle;", "tokens": ["Der", "sch\u00f6ns\u00b7ten", ",", "ew\u00b7ge", "Lie\u00b7be", "feh\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch traur ich, bis die Gruft mich deckt,", "tokens": ["Doch", "traur", "ich", ",", "bis", "die", "Gruft", "mich", "deckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df meine Lieb sie nicht geweckt.", "tokens": ["Da\u00df", "mei\u00b7ne", "Lieb", "sie", "nicht", "ge\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}