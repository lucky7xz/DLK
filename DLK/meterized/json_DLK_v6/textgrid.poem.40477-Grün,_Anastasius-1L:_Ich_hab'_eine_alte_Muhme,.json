{"textgrid.poem.40477": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich hab' eine alte Muhme,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' eine alte Muhme,", "tokens": ["Ich", "hab'", "ei\u00b7ne", "al\u00b7te", "Muh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Die ein altes B\u00fcchlein hat,", "tokens": ["Die", "ein", "al\u00b7tes", "B\u00fcch\u00b7lein", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Es liegt in dem alten Buche", "tokens": ["Es", "liegt", "in", "dem", "al\u00b7ten", "Bu\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein altes, d\u00fcrres Blatt.", "tokens": ["Ein", "al\u00b7tes", ",", "d\u00fcr\u00b7res", "Blatt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So d\u00fcrr sind wohl auch die H\u00e4nde,", "tokens": ["So", "d\u00fcrr", "sind", "wohl", "auch", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+----+-", "measure": "dactylic.init"}, "line.2": {"text": "Die einst im Lenz ihr's gepfl\u00fcckt.", "tokens": ["Die", "einst", "im", "Lenz", "ih\u00b7r's", "ge\u00b7pfl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was mag doch die Alte haben?", "tokens": ["Was", "mag", "doch", "die", "Al\u00b7te", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie weint, so oft sie's erblickt.", "tokens": ["Sie", "weint", ",", "so", "oft", "sie's", "er\u00b7blickt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}