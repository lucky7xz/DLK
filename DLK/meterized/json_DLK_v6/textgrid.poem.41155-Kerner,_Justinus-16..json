{"textgrid.poem.41155": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Geistin hier in schwarzer Tracht", "tokens": ["Die", "Geis\u00b7tin", "hier", "in", "schwar\u00b7zer", "Tracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schwebt aus der Burg jedwede Nacht,", "tokens": ["Schwebt", "aus", "der", "Burg", "jed\u00b7we\u00b7de", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sobald t\u00f6nt zw\u00f6lf des Turmes Glocke;", "tokens": ["So\u00b7bald", "t\u00f6nt", "zw\u00f6lf", "des", "Tur\u00b7mes", "Glo\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf ihrem langen schwarzen Rocke", "tokens": ["Auf", "ih\u00b7rem", "lan\u00b7gen", "schwar\u00b7zen", "Ro\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sich bildet ein Gerippe dann,", "tokens": ["Sich", "bil\u00b7det", "ein", "Ge\u00b7rip\u00b7pe", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Totengeripp' von ihrem Mann;", "tokens": ["Das", "To\u00b7ten\u00b7ge\u00b7ripp'", "von", "ih\u00b7rem", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wie Phosphor leuchtet's durch die Nacht,", "tokens": ["Wie", "Phos\u00b7phor", "leuch\u00b7tet's", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie hat durch Gift ihn umgebracht.", "tokens": ["Sie", "hat", "durch", "Gift", "ihn", "um\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Schwebt sie durch meines Gartens Hecke,", "tokens": ["Schwebt", "sie", "durch", "mei\u00b7nes", "Gar\u00b7tens", "He\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ich morgens stets mit Schau'r entdecke,", "tokens": ["Ich", "mor\u00b7gens", "stets", "mit", "Schau'r", "ent\u00b7de\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Da\u00df rings von meinen Blumen allen", "tokens": ["Da\u00df", "rings", "von", "mei\u00b7nen", "Blu\u00b7men", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die Bl\u00e4tter liegen abgefallen.", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "lie\u00b7gen", "ab\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}