{"textgrid.poem.35059": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "In die Berge", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon weicht das Flache hinter mir;", "tokens": ["Schon", "weicht", "das", "Fla\u00b7che", "hin\u00b7ter", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ebene beginnt, zu steigen.", "tokens": ["Die", "E\u00b7be\u00b7ne", "be\u00b7ginnt", ",", "zu", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So naht das Herz, Jehovah, dir,", "tokens": ["So", "naht", "das", "Herz", ",", "Je\u00b7ho\u00b7vah", ",", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "NE", "$,", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn hinter ihm die Zweifel weichen.", "tokens": ["Wenn", "hin\u00b7ter", "ihm", "die", "Zwei\u00b7fel", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es ist, als ob am Horizont", "tokens": ["Es", "ist", ",", "als", "ob", "am", "Ho\u00b7ri\u00b7zont"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich Bergesspitzen leuchten s\u00e4he.", "tokens": ["Ich", "Ber\u00b7ges\u00b7spit\u00b7zen", "leuch\u00b7ten", "s\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So reinigt, l\u00e4utert, w\u00e4rmt und sonnt", "tokens": ["So", "rei\u00b7nigt", ",", "l\u00e4u\u00b7tert", ",", "w\u00e4rmt", "und", "sonnt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "XY"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seele sich in Himmelsn\u00e4he.", "tokens": ["Die", "See\u00b7le", "sich", "in", "Him\u00b7mels\u00b7n\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hinauf, hinauf! Ich raste nicht;", "tokens": ["Hin\u00b7auf", ",", "hin\u00b7auf", "!", "Ich", "ras\u00b7te", "nicht", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$.", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich darf und mag nicht unten bleiben.", "tokens": ["Ich", "darf", "und", "mag", "nicht", "un\u00b7ten", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein fr\u00f6mmstes, herzlichstes Gedicht", "tokens": ["Mein", "fr\u00f6mms\u00b7tes", ",", "herz\u00b7lichs\u00b7tes", "Ge\u00b7dicht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will ich beim Gl\u00fchn der Alpen schreiben.", "tokens": ["Will", "ich", "beim", "Gl\u00fchn", "der", "Al\u00b7pen", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dann werde ich es heimlich, still,", "tokens": ["Dann", "wer\u00b7de", "ich", "es", "heim\u00b7lich", ",", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In einem Kirchlein niederlegen;", "tokens": ["In", "ei\u00b7nem", "Kirch\u00b7lein", "nie\u00b7der\u00b7le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vielleicht gereicht's, so Gott es will,", "tokens": ["Viel\u00b7leicht", "ge\u00b7reicht's", ",", "so", "Gott", "es", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "NN", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem, der es findet, dann zum Segen!", "tokens": ["Dem", ",", "der", "es", "fin\u00b7det", ",", "dann", "zum", "Se\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}