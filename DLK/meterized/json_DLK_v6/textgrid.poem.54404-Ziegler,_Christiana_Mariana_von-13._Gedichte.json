{"textgrid.poem.54404": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "13. Gedichte", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In was f\u00fcr Einbildung und wunderlichem Wahn", "tokens": ["In", "was", "f\u00fcr", "Ein\u00b7bil\u00b7dung", "und", "wun\u00b7der\u00b7li\u00b7chem", "Wahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Steckt doch nicht oftermals ein blinder Curtisan;", "tokens": ["Steckt", "doch", "nicht", "of\u00b7ter\u00b7mals", "ein", "blin\u00b7der", "Cur\u00b7ti\u00b7san", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der, weil der Liebesgott ihn k\u00f6rnet und ihm heuchelt,", "tokens": ["Der", ",", "weil", "der", "Lie\u00b7bes\u00b7gott", "ihn", "k\u00f6r\u00b7net", "und", "ihm", "heu\u00b7chelt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich mehr als allzustark mit Gegenliebe schmeichelt?", "tokens": ["Sich", "mehr", "als", "all\u00b7zu\u00b7stark", "mit", "Ge\u00b7gen\u00b7lie\u00b7be", "schmei\u00b7chelt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIS", "KOKOM", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ob gleich die so er liebt, ihn nur mit Worten speist,", "tokens": ["Ob", "gleich", "die", "so", "er", "liebt", ",", "ihn", "nur", "mit", "Wor\u00b7ten", "speist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADV", "PPER", "VVFIN", "$,", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und seinem Umgang sich so oft sie kann, entrei\u00dft,", "tokens": ["Und", "sei\u00b7nem", "Um\u00b7gang", "sich", "so", "oft", "sie", "kann", ",", "ent\u00b7rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PRF", "ADV", "ADV", "PPER", "VMFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So wird ihn jener doch gar leicht beschwatzen k\u00f6nnen:", "tokens": ["So", "wird", "ihn", "je\u00b7ner", "doch", "gar", "leicht", "be\u00b7schwat\u00b7zen", "k\u00f6n\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDS", "ADV", "ADV", "ADJD", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als s\u00e4h er dieser Herz gleich einem Schorstein brennen.", "tokens": ["Als", "s\u00e4h", "er", "die\u00b7ser", "Herz", "gleich", "ei\u00b7nem", "Schor\u00b7stein", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PDAT", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er leget jedes Wort, das doch die Unschuld spricht,", "tokens": ["Er", "le\u00b7get", "je\u00b7des", "Wort", ",", "das", "doch", "die", "Un\u00b7schuld", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und alles, was nur blo\u00df aus H\u00f6flichkeit geschicht,", "tokens": ["Und", "al\u00b7les", ",", "was", "nur", "blo\u00df", "aus", "H\u00f6f\u00b7lich\u00b7keit", "ge\u00b7schicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "ADV", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Zu seinem Vortheil aus; und r\u00fchmet sich der Ehre,", "tokens": ["Zu", "sei\u00b7nem", "Vor\u00b7theil", "aus", ";", "und", "r\u00fch\u00b7met", "sich", "der", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$.", "KON", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Als wenn er Hahn allein im Liebeskorbe w\u00e4re,", "tokens": ["Als", "wenn", "er", "Hahn", "al\u00b7lein", "im", "Lie\u00b7bes\u00b7kor\u00b7be", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NE", "ADV", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Den seine G\u00f6ttin doch, die sich nicht leicht verstellt,", "tokens": ["Den", "sei\u00b7ne", "G\u00f6t\u00b7tin", "doch", ",", "die", "sich", "nicht", "leicht", "ver\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "$,", "PRELS", "PRF", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So fest als Herz und Sinn vor ihn verschlossen h\u00e4lt.", "tokens": ["So", "fest", "als", "Herz", "und", "Sinn", "vor", "ihn", "ver\u00b7schlos\u00b7sen", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NN", "KON", "NN", "APPR", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So gehts dem Coridon, den Selimene hetzet,", "tokens": ["So", "gehts", "dem", "Co\u00b7ri\u00b7don", ",", "den", "Se\u00b7li\u00b7me\u00b7ne", "het\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Weil er sich einen Wurm in seinen Kopf gesetzet.", "tokens": ["Weil", "er", "sich", "ei\u00b7nen", "Wurm", "in", "sei\u00b7nen", "Kopf", "ge\u00b7set\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Er bildet sich gewi\u00df und recht unfehlbar ein", "tokens": ["Er", "bil\u00b7det", "sich", "ge\u00b7wi\u00df", "und", "recht", "un\u00b7fehl\u00b7bar", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "KON", "ADJD", "ADJD", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Als m\u00fcst ihm selbige gar sehr gewogen seyn.", "tokens": ["Als", "m\u00fcst", "ihm", "sel\u00b7bi\u00b7ge", "gar", "sehr", "ge\u00b7wo\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ADJA", "ADV", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ihr Herze, tr\u00e4umet er, sey ihm vor andern allen", "tokens": ["Ihr", "Her\u00b7ze", ",", "tr\u00e4u\u00b7met", "er", ",", "sey", "ihm", "vor", "an\u00b7dern", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "APPR", "PIS", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Aus grosser Z\u00e4rtlichkeit im Lieben zugefallen.", "tokens": ["Aus", "gros\u00b7ser", "Z\u00e4rt\u00b7lich\u00b7keit", "im", "Lie\u00b7ben", "zu\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Held! der du Garn und Netz so schlau den Herzen schlingst", "tokens": ["Held", "!", "der", "du", "Garn", "und", "Netz", "so", "schlau", "den", "Her\u00b7zen", "schlingst"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "NE", "NE", "KON", "NN", "ADV", "ADJD", "ART", "NN", "VVFIN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.22": {"text": "Und, eh man es gemeynt, der Chloris Geist bezwingst,", "tokens": ["Und", ",", "eh", "man", "es", "ge\u00b7meynt", ",", "der", "Chlo\u00b7ris", "Geist", "be\u00b7zwingst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PIS", "PPER", "VVPP", "$,", "ART", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Lehr uns doch deine Kunst die Sch\u00f6nen zu bemeistern,", "tokens": ["Lehr", "uns", "doch", "dei\u00b7ne", "Kunst", "die", "Sch\u00f6\u00b7nen", "zu", "be\u00b7meis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPOSAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Denn diese Zauberey stammt sicherlich von Geistern,", "tokens": ["Denn", "die\u00b7se", "Zau\u00b7be\u00b7rey", "stammt", "si\u00b7cher\u00b7lich", "von", "Geis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und nicht von Menschen her; ihr Witz reicht hier nicht zu.", "tokens": ["Und", "nicht", "von", "Men\u00b7schen", "her", ";", "ihr", "Witz", "reicht", "hier", "nicht", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "PTKVZ", "$.", "PPOSAT", "NN", "VVFIN", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wer kann so meisterlich und so geschwind, als du,", "tokens": ["Wer", "kann", "so", "meis\u00b7ter\u00b7lich", "und", "so", "ge\u00b7schwind", ",", "als", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,", "KOUS", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die Nymphen, welche doch die st\u00e4rksten Waffen tragen,", "tokens": ["Die", "Nym\u00b7phen", ",", "wel\u00b7che", "doch", "die", "st\u00e4rks\u00b7ten", "Waf\u00b7fen", "tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und vor die Freyheit stehn, in Band und Ketten schlagen?", "tokens": ["Und", "vor", "die", "Frey\u00b7heit", "stehn", ",", "in", "Band", "und", "Ket\u00b7ten", "schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$,", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wird deiner Chloris Gunst, die ihr doch niemals feil", "tokens": ["Wird", "dei\u00b7ner", "Chlo\u00b7ris", "Gunst", ",", "die", "ihr", "doch", "nie\u00b7mals", "feil"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NE", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Bey klugen Freyern war, so schleunig dir zu theil?", "tokens": ["Bey", "klu\u00b7gen", "Frey\u00b7ern", "war", ",", "so", "schleu\u00b7nig", "dir", "zu", "theil", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "$,", "ADV", "ADJD", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "O Thore! glaub es nicht; man weis es alles besser,", "tokens": ["O", "Tho\u00b7re", "!", "glaub", "es", "nicht", ";", "man", "weis", "es", "al\u00b7les", "bes\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "$.", "PIS", "PTKVZ", "PPER", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Du baust, wie mich bed\u00fcnkt, nur deine Hoffnungsschl\u00f6sser,", "tokens": ["Du", "baust", ",", "wie", "mich", "be\u00b7d\u00fcnkt", ",", "nur", "dei\u00b7ne", "Hoff\u00b7nungs\u00b7schl\u00f6s\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Aus Luft und leren Wind. Betrogner Coridon", "tokens": ["Aus", "Luft", "und", "le\u00b7ren", "Wind", ".", "Be\u00b7trog\u00b7ner", "Co\u00b7ri\u00b7don"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "KON", "VVFIN", "NN", "$.", "NN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Du tr\u00e4gst der Sch\u00f6nen Gunst, nicht wie du glaubst davon.", "tokens": ["Du", "tr\u00e4gst", "der", "Sch\u00f6\u00b7nen", "Gunst", ",", "nicht", "wie", "du", "glaubst", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,", "PTKNEG", "PWAV", "PPER", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Denn weil dies schlaue Kind, das dich vollkommen kennet,", "tokens": ["Denn", "weil", "dies", "schlau\u00b7e", "Kind", ",", "das", "dich", "voll\u00b7kom\u00b7men", "ken\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PDS", "ADJA", "NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und einer andern gern solch Leckerbi\u00dfchen g\u00f6nnet,", "tokens": ["Und", "ei\u00b7ner", "an\u00b7dern", "gern", "solch", "Le\u00b7cker\u00b7bi\u00df\u00b7chen", "g\u00f6n\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Bey deinem widrigen und albern Liebesspiel", "tokens": ["Bey", "dei\u00b7nem", "wid\u00b7ri\u00b7gen", "und", "al\u00b7bern", "Lie\u00b7bes\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Dich mit der Margaris in Umgang bringen will;", "tokens": ["Dich", "mit", "der", "Mar\u00b7ga\u00b7ris", "in", "Um\u00b7gang", "brin\u00b7gen", "will", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NE", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.39": {"text": "So suchet sie sonst nichts, als den verliebten Affen", "tokens": ["So", "su\u00b7chet", "sie", "sonst", "nichts", ",", "als", "den", "ver\u00b7lieb\u00b7ten", "Af\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIS", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "Den sie an dir gehabt, sich von dem Hals zu schaffen.", "tokens": ["Den", "sie", "an", "dir", "ge\u00b7habt", ",", "sich", "von", "dem", "Hals", "zu", "schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VAPP", "$,", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.41": {"text": "Doch, nein, ich irre wohl, denn jeder macht den Schlu\u00df,", "tokens": ["Doch", ",", "nein", ",", "ich", "ir\u00b7re", "wohl", ",", "denn", "je\u00b7der", "macht", "den", "Schlu\u00df", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PTKANT", "$,", "PPER", "VVFIN", "ADV", "$,", "KON", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Da\u00df Selimene dich vortrefflich lieben mu\u00df.", "tokens": ["Da\u00df", "Se\u00b7li\u00b7me\u00b7ne", "dich", "vor\u00b7treff\u00b7lich", "lie\u00b7ben", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Sie will dich doch nicht leer von sich zur\u00fccke senden;", "tokens": ["Sie", "will", "dich", "doch", "nicht", "leer", "von", "sich", "zu\u00b7r\u00fc\u00b7cke", "sen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "ADJD", "APPR", "PRF", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Die andern haben nichts, du tr\u00e4gst den Korb in H\u00e4nden.", "tokens": ["Die", "an\u00b7dern", "ha\u00b7ben", "nichts", ",", "du", "tr\u00e4gst", "den", "Korb", "in", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PIS", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}