{"dta.poem.241": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als zwey Verliebte durch den dritten ver-  \n st\u00f6hret wurden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.57", "af:0.42"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Amariane die Ge\u00fcbte", "tokens": ["A\u00b7ma\u00b7ri\u00b7a\u00b7ne", "die", "Ge\u00b7\u00fcb\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NE", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lag in dem weichen Feder-Grab/", "tokens": ["Lag", "in", "dem", "wei\u00b7chen", "Fe\u00b7der\u00b7Grab", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ihr ", "tokens": ["Als", "ihr"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Vergn\u00fcgte Lust zu kosten gab/", "tokens": ["Ver\u00b7gn\u00fcg\u00b7te", "Lust", "zu", "kos\u00b7ten", "gab", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach! sprach sie/ m\u00f6cht\u2019 ich so stets leben/", "tokens": ["Ach", "!", "sprach", "sie", "/", "m\u00f6cht'", "ich", "so", "stets", "le\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$(", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und mir mein Licht ", "tokens": ["Und", "mir", "mein", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Sie schlo\u00df ihn in die weichen Arme/", "tokens": ["Sie", "schlo\u00df", "ihn", "in", "die", "wei\u00b7chen", "Ar\u00b7me", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und druckte ihn an ihre Brust;", "tokens": ["Und", "druck\u00b7te", "ihn", "an", "ih\u00b7re", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sprach: wenn ich also erwarme/", "tokens": ["Sie", "sprach", ":", "wenn", "ich", "al\u00b7so", "er\u00b7war\u00b7me", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wird mir solche Lust bewust/", "tokens": ["So", "wird", "mir", "sol\u00b7che", "Lust", "be\u00b7wust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die gegen alle S\u00fcssigkeiten", "tokens": ["Die", "ge\u00b7gen", "al\u00b7le", "S\u00fcs\u00b7sig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kan um die Ober-Herrschafft streiten.", "tokens": ["Kan", "um", "die", "O\u00b7ber\u00b7Herr\u00b7schafft", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hier hielt sie Augenblicklich innen/", "tokens": ["Hier", "hielt", "sie", "Au\u00b7gen\u00b7blick\u00b7lich", "in\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Zunge lie\u00df das Reden seyn/", "tokens": ["Die", "Zun\u00b7ge", "lie\u00df", "das", "Re\u00b7den", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch warff die Anmuht ihre Sinnen", "tokens": ["Auch", "warff", "die", "An\u00b7muht", "ih\u00b7re", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In eine s\u00fcsse Ohnmacht ein/", "tokens": ["In", "ei\u00b7ne", "s\u00fcs\u00b7se", "Ohn\u00b7macht", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie starb/ doch muste ihr das sterben", "tokens": ["Sie", "starb", "/", "doch", "mus\u00b7te", "ihr", "das", "ster\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "ADV", "VMFIN", "PPER", "PDS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die allers\u00fc\u00dfte Lust erwerben.", "tokens": ["Die", "al\u00b7ler\u00b7s\u00fc\u00df\u00b7te", "Lust", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie lag und r\u00fchrte nicht die Glieder/", "tokens": ["Sie", "lag", "und", "r\u00fchr\u00b7te", "nicht", "die", "Glie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur da\u00df der Athen hefftig ging/", "tokens": ["Nur", "da\u00df", "der", "A\u00b7then", "heff\u00b7tig", "ging", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch kahm das Leben endlich wieder/", "tokens": ["Doch", "kahm", "das", "Le\u00b7ben", "end\u00b7lich", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "ADV", "ADV", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Worauf sie an zu reden fing:", "tokens": ["Wo\u00b7rauf", "sie", "an", "zu", "re\u00b7den", "fing", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach s\u00fcsser Tod! in welch\u2019 Erg\u00f6tzen/", "tokens": ["Ach", "s\u00fcs\u00b7ser", "Tod", "!", "in", "welch'", "Er\u00b7g\u00f6t\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$.", "APPR", "PWAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kanst du doch meine Seele setzen.", "tokens": ["Kanst", "du", "doch", "mei\u00b7ne", "See\u00b7le", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nichts kan auf diesem Rund der Erden/", "tokens": ["Nichts", "kan", "auf", "die\u00b7sem", "Rund", "der", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PDAT", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das mit dir zu vergleichen sey/", "tokens": ["Das", "mit", "dir", "zu", "ver\u00b7glei\u00b7chen", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "PTKZU", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch keine Kunst erfunden werden;", "tokens": ["Durch", "kei\u00b7ne", "Kunst", "er\u00b7fun\u00b7den", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Helari stimmte auch mit bey/", "tokens": ["He\u00b7la\u00b7ri", "stimm\u00b7te", "auch", "mit", "bey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sprach: Mein Kind/ ach dis Vergn\u00fcgen/", "tokens": ["Und", "sprach", ":", "Mein", "Kind", "/", "ach", "dis", "Ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPOSAT", "NN", "$(", "XY", "PDS", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kan alle ander Lust besiegen.", "tokens": ["Kan", "al\u00b7le", "an\u00b7der", "Lust", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Kaum als die Worte ausgesprochen/", "tokens": ["Kaum", "als", "die", "Wor\u00b7te", "aus\u00b7ge\u00b7spro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da sanck ", "tokens": ["Da", "san\u00b7ck"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Sie sprach: Jhr Augen seyd gebrochen/", "tokens": ["Sie", "sprach", ":", "Ihr", "Au\u00b7gen", "seyd", "ge\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie! da\u00df ich noch am Leben bin/", "tokens": ["Wie", "!", "da\u00df", "ich", "noch", "am", "Le\u00b7ben", "bin", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KOUS", "PPER", "ADV", "APPRART", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach Tod komm! la\u00df es doch geschehen/", "tokens": ["Ach", "Tod", "komm", "!", "la\u00df", "es", "doch", "ge\u00b7sche\u00b7hen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVFIN", "$.", "VVIMP", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich mit ihm mag untergehen.", "tokens": ["Da\u00df", "ich", "mit", "ihm", "mag", "un\u00b7ter\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "So gleich ersturben ihr die Geister/", "tokens": ["So", "gleich", "er\u00b7stur\u00b7ben", "ihr", "die", "Geis\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Zunge blieb erstarret stehn.", "tokens": ["Die", "Zun\u00b7ge", "blieb", "er\u00b7star\u00b7ret", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Ohmacht wurde von ihr Meister/", "tokens": ["Die", "Oh\u00b7macht", "wur\u00b7de", "von", "ihr", "Meis\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es war um alle Krafft geschehn.", "tokens": ["Es", "war", "um", "al\u00b7le", "Krafft", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch ihr Vergn\u00fcgen zu vermehren/", "tokens": ["Doch", "ihr", "Ver\u00b7gn\u00fc\u00b7gen", "zu", "ver\u00b7meh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Must\u2019 ihnen der Geist wiederkehren.", "tokens": ["Must'", "ih\u00b7nen", "der", "Geist", "wie\u00b7der\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Sie k\u00fc\u00dften sich/ und lachten beyde/", "tokens": ["Sie", "k\u00fc\u00df\u00b7ten", "sich", "/", "und", "lach\u00b7ten", "bey\u00b7de", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$(", "KON", "VVFIN", "PIS", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als jemand in die Kammer kahm/", "tokens": ["Als", "je\u00b7mand", "in", "die", "Kam\u00b7mer", "kahm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Worauf zu ihrem grossem Leide", "tokens": ["Wo\u00b7rauf", "zu", "ih\u00b7rem", "gros\u00b7sem", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gleich ihre Lust ein Ende nahm/", "tokens": ["Gleich", "ih\u00b7re", "Lust", "ein", "En\u00b7de", "nahm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In welcher man nach vielen Stunden", "tokens": ["In", "wel\u00b7cher", "man", "nach", "vie\u00b7len", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PIS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie h\u00e4tte sonsten noch gefunden.", "tokens": ["Sie", "h\u00e4t\u00b7te", "sons\u00b7ten", "noch", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}