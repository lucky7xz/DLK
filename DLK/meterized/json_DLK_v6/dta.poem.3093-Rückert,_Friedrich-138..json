{"dta.poem.3093": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "138.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du glaubst, was ich nicht glaub', und glaubst nicht, was ich glaube;", "tokens": ["Du", "glaubst", ",", "was", "ich", "nicht", "glaub'", ",", "und", "glaubst", "nicht", ",", "was", "ich", "glau\u00b7be", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "PTKNEG", "VVFIN", "$,", "KON", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erlaub mein Glauben mir, wie ich dir deins erlaube.", "tokens": ["Er\u00b7laub", "mein", "Glau\u00b7ben", "mir", ",", "wie", "ich", "dir", "deins", "er\u00b7lau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PPER", "$,", "PWAV", "PPER", "PPER", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer noch nichts glaubt, ist leicht zum Glauben zu bekehren,", "tokens": ["Wer", "noch", "nichts", "glaubt", ",", "ist", "leicht", "zum", "Glau\u00b7ben", "zu", "be\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PIS", "VVFIN", "$,", "VAFIN", "ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie die Gef\u00e4\u00dfe leicht zu f\u00fcllen sind, die leeren.", "tokens": ["Wie", "die", "Ge\u00b7f\u00e4\u00b7\u00dfe", "leicht", "zu", "f\u00fcl\u00b7len", "sind", ",", "die", "lee\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "PTKZU", "VVINF", "VAFIN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch dem, der etwas glaubt, f\u00e4llt andres glauben schwer;", "tokens": ["Doch", "dem", ",", "der", "et\u00b7was", "glaubt", ",", "f\u00e4llt", "and\u00b7res", "glau\u00b7ben", "schwer", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "PIS", "VVFIN", "$,", "VVFIN", "PIS", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gibt er es einmal auf, so glaubt er gar nichts mehr.", "tokens": ["Gibt", "er", "es", "ein\u00b7mal", "auf", ",", "so", "glaubt", "er", "gar", "nichts", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "ADV", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}