{"dta.poem.285": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er liebet sie allein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein himmlisches Bild/ euch lieb ich allein;", "tokens": ["Mein", "himm\u00b7li\u00b7sches", "Bild", "/", "euch", "lieb", "ich", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPER", "ADJD", "PPER", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mu\u00df ich gleich zu weilen bey andern auch seyn/", "tokens": ["Mu\u00df", "ich", "gleich", "zu", "wei\u00b7len", "bey", "an\u00b7dern", "auch", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKZU", "VVFIN", "APPR", "PIS", "ADV", "VAINF", "$("], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Doch bleib ich/ mein Leben/", "tokens": ["Doch", "bleib", "ich", "/", "mein", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "PPOSAT", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Euch eintzig ergeben/", "tokens": ["Euch", "eint\u00b7zig", "er\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Euch will ich mich weyhn.", "tokens": ["Euch", "will", "ich", "mich", "weyhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Jhr habet mein Hertz", "tokens": ["Ihr", "ha\u00b7bet", "mein", "Hertz"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Mir l\u00e4ngsten geraubet/", "tokens": ["Mir", "l\u00e4ngs\u00b7ten", "ge\u00b7rau\u00b7bet", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Wie! da\u00df ihr denn glauhet", "tokens": ["Wie", "!", "da\u00df", "ihr", "denn", "glau\u00b7het"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "KOUS", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Ich lieb\u2019 euch aus Schertz;", "tokens": ["Ich", "lieb'", "euch", "aus", "Schertz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Da ich doch mein Licht", "tokens": ["Da", "ich", "doch", "mein", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.11": {"text": "Euch eintzig verpflicht.", "tokens": ["Euch", "eint\u00b7zig", "ver\u00b7pflicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Verbannet das Dencken/", "tokens": ["Ver\u00b7ban\u00b7net", "das", "Den\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Das euch nur will kr\u00e4ncken/", "tokens": ["Das", "euch", "nur", "will", "kr\u00e4n\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Und lebet in Lust/", "tokens": ["Und", "le\u00b7bet", "in", "Lust", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Euch bleib ich ergeben", "tokens": ["Euch", "bleib", "ich", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "So lange das Leben", "tokens": ["So", "lan\u00b7ge", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Mir nur ist bewust.", "tokens": ["Mir", "nur", "ist", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Die Sonne erschw\u00e4rtzt dein himmlischer Schein", "tokens": ["Die", "Son\u00b7ne", "er\u00b7schw\u00e4rtzt", "dein", "himm\u00b7li\u00b7scher", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der lieblichen Augen/ die feuriger seyn/", "tokens": ["Der", "lieb\u00b7li\u00b7chen", "Au\u00b7gen", "/", "die", "feu\u00b7ri\u00b7ger", "seyn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJD", "VAINF", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Als alle die Flammen/", "tokens": ["Als", "al\u00b7le", "die", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Der Erden kan leyhn.", "tokens": ["Der", "Er\u00b7den", "kan", "leyhn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Ein eintziger Blick", "tokens": ["Ein", "eint\u00b7zi\u00b7ger", "Blick"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Erquicket mein Hertze.", "tokens": ["Er\u00b7quic\u00b7ket", "mein", "Hert\u00b7ze", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und bringet nach Schmertze", "tokens": ["Und", "brin\u00b7get", "nach", "Schmert\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Zur\u00fccke mein Gl\u00fcck.", "tokens": ["Zu\u00b7r\u00fc\u00b7cke", "mein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Ich werde beseelt", "tokens": ["Ich", "wer\u00b7de", "be\u00b7seelt"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Wenn Kummer mich qu\u00e4hlt", "tokens": ["Wenn", "Kum\u00b7mer", "mich", "qu\u00e4hlt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Durch liebliche Blicke/", "tokens": ["Durch", "lieb\u00b7li\u00b7che", "Bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Und k\u00fcsse mein Gl\u00fccke", "tokens": ["Und", "k\u00fcs\u00b7se", "mein", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "In deinem Gesicht/", "tokens": ["In", "dei\u00b7nem", "Ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Es lassen die Augen/", "tokens": ["Es", "las\u00b7sen", "die", "Au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Stets Freundlichkeit saugen", "tokens": ["Stets", "Freund\u00b7lich\u00b7keit", "sau\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Und Anmuth mein Licht.", "tokens": ["Und", "An\u00b7muth", "mein", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}