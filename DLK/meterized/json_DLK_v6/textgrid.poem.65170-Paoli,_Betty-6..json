{"textgrid.poem.65170": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbheut gilt es alles einzusetzen!", "tokens": ["\u00bb", "heut", "gilt", "es", "al\u00b7les", "ein\u00b7zu\u00b7set\u00b7zen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu fechten, k\u00fchner noch als je!", "tokens": ["Zu", "fech\u00b7ten", ",", "k\u00fch\u00b7ner", "noch", "als", "je", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADJA", "ADV", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Scharte gilt es auszuwetzen", "tokens": ["Die", "Schar\u00b7te", "gilt", "es", "aus\u00b7zu\u00b7wet\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Ungl\u00fcckstages von Tess\u00e9!", "tokens": ["Des", "Un\u00b7gl\u00fccks\u00b7ta\u00b7ges", "von", "Tess\u00e9", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Das Blut, das dort umsonst geflossen,", "tokens": ["Das", "Blut", ",", "das", "dort", "um\u00b7sonst", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Um Rache schreit's zum Himmel auf!", "tokens": ["Um", "Ra\u00b7che", "schreit's", "zum", "Him\u00b7mel", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Heran, ihr Freunde und Genossen!", "tokens": ["He\u00b7ran", ",", "ihr", "Freun\u00b7de", "und", "Ge\u00b7nos\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nehmt heut die Stadt im Sturmeslauf!", "tokens": ["Nehmt", "heut", "die", "Stadt", "im", "Stur\u00b7mes\u00b7lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mag immerhin von ihren Zinnen", "tokens": ["Mag", "im\u00b7mer\u00b7hin", "von", "ih\u00b7ren", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Tricolore trotzig weh'n!", "tokens": ["Die", "Tri\u00b7co\u00b7lo\u00b7re", "trot\u00b7zig", "weh'n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hausten tausend Teufel drinnen,", "tokens": ["Und", "haus\u00b7ten", "tau\u00b7send", "Teu\u00b7fel", "drin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "CARD", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie k\u00f6nnten nicht vor euch besteh'n!", "tokens": ["Sie", "k\u00f6nn\u00b7ten", "nicht", "vor", "euch", "be\u00b7steh'n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wei\u00df: des Sieges Engel winket", "tokens": ["Ich", "wei\u00df", ":", "des", "Sie\u00b7ges", "En\u00b7gel", "win\u00b7ket"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Euch l\u00e4chelnd zu, verhei\u00dfungsfroh!", "tokens": ["Euch", "l\u00e4\u00b7chelnd", "zu", ",", "ver\u00b7hei\u00b7\u00dfungs\u00b7froh", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und was euch jetzt ein Wunder d\u00fcnket,", "tokens": ["Und", "was", "euch", "jetzt", "ein", "Wun\u00b7der", "d\u00fcn\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gescheh'n wird's, denn Gott will's so!\u00ab", "tokens": ["Ge\u00b7scheh'n", "wird's", ",", "denn", "Gott", "will's", "so", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "$,", "KON", "NN", "VMFIN", "ADV", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Voll Andacht lauscht das Volk den Worten,", "tokens": ["Voll", "An\u00b7dacht", "lauscht", "das", "Volk", "den", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Agra's Bischof zu ihm spricht;", "tokens": ["Die", "A\u00b7gra's", "Bi\u00b7schof", "zu", "ihm", "spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie k\u00fcssen seiner Stola Borten,", "tokens": ["Sie", "k\u00fcs\u00b7sen", "sei\u00b7ner", "Sto\u00b7la", "Bor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Herz ergl\u00fcht in Zuversicht!", "tokens": ["Ihr", "Herz", "er\u00b7gl\u00fcht", "in", "Zu\u00b7ver\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Kelch des Opfers in den H\u00e4nden,", "tokens": ["Den", "Kelch", "des", "Op\u00b7fers", "in", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erteilt der Kirche F\u00fcrst und Sohn", "tokens": ["Er\u00b7teilt", "der", "Kir\u00b7che", "F\u00fcrst", "und", "Sohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem Heere ihre Gnadenspenden,", "tokens": ["Dem", "Hee\u00b7re", "ih\u00b7re", "Gna\u00b7den\u00b7spen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Trost der Absolution. \u2013", "tokens": ["Den", "Trost", "der", "Ab\u00b7so\u00b7lu\u00b7ti\u00b7on", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Jetzt tritt Lescure vor, von den Seinen", "tokens": ["Jetzt", "tritt", "Le\u00b7scu\u00b7re", "vor", ",", "von", "den", "Sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "PTKVZ", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Begr\u00fc\u00dft mit einem Jubelschrei:", "tokens": ["Be\u00b7gr\u00fc\u00dft", "mit", "ei\u00b7nem", "Ju\u00b7bel\u00b7schrei", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbschlimm steht's mit uns! man sollt' es meinen!", "tokens": ["\u00bb", "schlimm", "steht's", "mit", "uns", "!", "man", "sollt'", "es", "mei\u00b7nen", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "APPR", "PPER", "$.", "PIS", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir haben Pulver nicht noch Blei.", "tokens": ["Wir", "ha\u00b7ben", "Pul\u00b7ver", "nicht", "noch", "Blei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "PTKNEG", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch, da\u00df mit Knitteln und mit Spie\u00dfen", "tokens": ["Doch", ",", "da\u00df", "mit", "Knit\u00b7teln", "und", "mit", "Spie\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Man Batterien nehmen kann,", "tokens": ["Man", "Bat\u00b7te\u00b7ri\u00b7en", "neh\u00b7men", "kann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das habt ihr selber mir bewiesen!", "tokens": ["Das", "habt", "ihr", "sel\u00b7ber", "mir", "be\u00b7wie\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "An's Werk denn, Kinder! drauf und dran!", "tokens": ["An's", "Werk", "denn", ",", "Kin\u00b7der", "!", "drauf", "und", "dran", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "NN", "$.", "PTKVZ", "KON", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mir nach!\u00ab \u2013 Mit Rauch und Knall und Blitze", "tokens": ["Mir", "nach", "!", "\u00ab", "\u2013", "Mit", "Rauch", "und", "Knall", "und", "Blit\u00b7ze"], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$.", "$(", "$(", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kommt ihm entgegen Schu\u00df auf Schu\u00df!", "tokens": ["Kommt", "ihm", "ent\u00b7ge\u00b7gen", "Schu\u00df", "auf", "Schu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Kugeln streifen seine M\u00fctze,", "tokens": ["Die", "Ku\u00b7geln", "strei\u00b7fen", "sei\u00b7ne", "M\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und rei\u00dfen ihm den Sporn vom Fu\u00df.", "tokens": ["Und", "rei\u00b7\u00dfen", "ihm", "den", "Sporn", "vom", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbla\u00dft euch den Bettel nicht verdrie\u00dfen!\u00ab", "tokens": ["\u00bb", "la\u00dft", "euch", "den", "Bet\u00b7tel", "nicht", "ver\u00b7drie\u00b7\u00dfen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$.", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Mit heller Stimme ruft's der Graf,", "tokens": ["Mit", "hel\u00b7ler", "Stim\u00b7me", "ruft's", "der", "Graf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbseht nur, wie schlecht die Blauen schie\u00dfen!", "tokens": ["\u00bb", "seht", "nur", ",", "wie", "schlecht", "die", "Blau\u00b7en", "schie\u00b7\u00dfen", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$,", "PWAV", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht ", "tokens": ["Nicht"], "token_info": ["word"], "pos": ["PTKNEG"], "meter": "+", "measure": "single.up"}}, "stanza.6": {"line.1": {"text": "Er sprengt voran auf seinem Schimmel,", "tokens": ["Er", "sprengt", "vo\u00b7ran", "auf", "sei\u00b7nem", "Schim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bei der Kanonen Donnerkrach!", "tokens": ["Bei", "der", "Ka\u00b7no\u00b7nen", "Don\u00b7ner\u00b7krach", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ging's gerade in den Himmel,", "tokens": ["Als", "ging's", "ge\u00b7ra\u00b7de", "in", "den", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So dr\u00e4ngen ihm die Bauern nach,", "tokens": ["So", "dr\u00e4n\u00b7gen", "ihm", "die", "Bau\u00b7ern", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch pl\u00f6tzlich stocken ihre Schritte!", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "sto\u00b7cken", "ih\u00b7re", "Schrit\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie knien nieder zum Gebet", "tokens": ["Sie", "kni\u00b7en", "nie\u00b7der", "zum", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Vor einem Kreuz, das in der Mitte", "tokens": ["Vor", "ei\u00b7nem", "Kreuz", ",", "das", "in", "der", "Mit\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des Wegs am gr\u00fcnen Raine steht.", "tokens": ["Des", "Wegs", "am", "gr\u00fc\u00b7nen", "Rai\u00b7ne", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dem Hagel trotzend der Geschosse,", "tokens": ["Dem", "Ha\u00b7gel", "trot\u00b7zend", "der", "Ge\u00b7schos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weih'n sie ihm ihrer Andacht Zoll.", "tokens": ["Weih'n", "sie", "ihm", "ih\u00b7rer", "An\u00b7dacht", "Zoll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hinjagt auf schaumbedecktem Rosse", "tokens": ["Hin\u00b7jagt", "auf", "schaum\u00b7be\u00b7deck\u00b7tem", "Ros\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Herr von Boissy: \u00bbWie? seid ihr toll?", "tokens": ["Herr", "von", "Bois\u00b7sy", ":", "\u00bb", "Wie", "?", "seid", "ihr", "toll", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$.", "$(", "PWAV", "$.", "VAFIN", "PPER", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "H\u00f6rt ihr den Ruf nicht der Trompeten?", "tokens": ["H\u00f6rt", "ihr", "den", "Ruf", "nicht", "der", "Trom\u00b7pe\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Schlacht beginnt! Was ficht euch an?\u00ab", "tokens": ["Die", "Schlacht", "be\u00b7ginnt", "!