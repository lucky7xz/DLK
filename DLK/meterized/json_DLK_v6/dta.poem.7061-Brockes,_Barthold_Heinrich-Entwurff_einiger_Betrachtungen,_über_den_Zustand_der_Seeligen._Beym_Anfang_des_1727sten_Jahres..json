{"dta.poem.7061": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Noch sind verschiedene, die meinen,", "tokens": ["Noch", "sind", "ver\u00b7schie\u00b7de\u00b7ne", ",", "die", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "$,", "PRELS", "PPOSAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als wenn die Seel in einem sanfften Schlummer,", "tokens": ["Als", "wenn", "die", "Seel", "in", "ei\u00b7nem", "sanff\u00b7ten", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ohn alle Noth, Verdrie\u00dflichkeit und Kummer,", "tokens": ["Ohn", "al\u00b7le", "Noth", ",", "Ver\u00b7drie\u00df\u00b7lich\u00b7keit", "und", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bi\u00df Leib und Seele sich aufs neu vereinen,", "tokens": ["Bi\u00df", "Leib", "und", "See\u00b7le", "sich", "aufs", "neu", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PRF", "APPRART", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch ohn Empfindlichkeit f\u00fcr Lust und Wonne bleibe;", "tokens": ["Doch", "ohn", "Emp\u00b7find\u00b7lich\u00b7keit", "f\u00fcr", "Lust", "und", "Won\u00b7ne", "blei\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Vor langer Zeit hierinn der Meinung wol gewesen.", "tokens": ["Vor", "lan\u00b7ger", "Zeit", "hie\u00b7rinn", "der", "Mei\u00b7nung", "wol", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ART", "NN", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denselben aber beyzustimmen,", "tokens": ["Den\u00b7sel\u00b7ben", "a\u00b7ber", "bey\u00b7zu\u00b7stim\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Vertr\u00e4get mein Begriff von GOTTES Liebe nicht.", "tokens": ["Ver\u00b7tr\u00e4\u00b7get", "mein", "Be\u00b7griff", "von", "GoT\u00b7TES", "Lie\u00b7be", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NE", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mich deucht es ist der Menschen-Pflicht,", "tokens": ["Mich", "deucht", "es", "ist", "der", "Men\u00b7schen\u00b7Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Von GOTT das Herrlichste, das Gr\u00f6sste zu gedencken:", "tokens": ["Von", "GoTT", "das", "Herr\u00b7lichs\u00b7te", ",", "das", "Gr\u00f6ss\u00b7te", "zu", "ge\u00b7den\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nun ist es ja der GOttheit w\u00fcrdiger", "tokens": ["Nun", "ist", "es", "ja", "der", "Got\u00b7theit", "w\u00fcr\u00b7di\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Zu glauben, da\u00df, da GOTT der HERR", "tokens": ["Zu", "glau\u00b7ben", ",", "da\u00df", ",", "da", "GoTT", "der", "HeRR"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "$,", "KOUS", "NE", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Aus Gnaden uns bestimmt, die Seeligkeit zu schencken,", "tokens": ["Aus", "Gna\u00b7den", "uns", "be\u00b7stimmt", ",", "die", "See\u00b7lig\u00b7keit", "zu", "schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Er unserm besten Theil dieselbige ja nicht", "tokens": ["Er", "un\u00b7serm", "bes\u00b7ten", "Theil", "die\u00b7sel\u00b7bi\u00b7ge", "ja", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "PDS", "ADV", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So lange vorenthalten werde,", "tokens": ["So", "lan\u00b7ge", "vor\u00b7ent\u00b7hal\u00b7ten", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Bis da\u00df der morsche Leib dereinsten aus der Erde,", "tokens": ["Bis", "da\u00df", "der", "mor\u00b7sche", "Leib", "de\u00b7reins\u00b7ten", "aus", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wer wei\u00df nach wie viel Jahren,", "tokens": ["Wer", "wei\u00df", "nach", "wie", "viel", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "KOKOM", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Erstehen wird und sich mit seiner Seele paaren.", "tokens": ["Er\u00b7ste\u00b7hen", "wird", "und", "sich", "mit", "sei\u00b7ner", "See\u00b7le", "paa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "KON", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Es scheinet \u00fcber dem, da\u00df diese Lehre", "tokens": ["Es", "schei\u00b7net", "\u00fc\u00b7ber", "dem", ",", "da\u00df", "die\u00b7se", "Leh\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "$,", "KOUS", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Auch einem Sterbenden mehr Trost gewehre.", "tokens": ["Auch", "ei\u00b7nem", "Ster\u00b7ben\u00b7den", "mehr", "Trost", "ge\u00b7weh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Ist es nicht glaublicher,", "tokens": ["Ist", "es", "nicht", "glaub\u00b7li\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.23": {"text": "Da\u00df GOTT, der HERR,", "tokens": ["Da\u00df", "GoTT", ",", "der", "HeRR", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.24": {"text": "Der alles Gute will und kan,", "tokens": ["Der", "al\u00b7les", "Gu\u00b7te", "will", "und", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VMFIN", "KON", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Sogleich von unserm Sterben an,", "tokens": ["Sog\u00b7leich", "von", "un\u00b7serm", "Ster\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Der Seele von dem ew\u2019gen Leben", "tokens": ["Der", "See\u00b7le", "von", "dem", "ew'\u00b7gen", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Werd\u2019 einen Anfang schon und s\u00fcssen Vorschmack geben?", "tokens": ["Werd'", "ei\u00b7nen", "An\u00b7fang", "schon", "und", "s\u00fcs\u00b7sen", "Vor\u00b7schmack", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der aber doch vermuthlich nicht", "tokens": ["Der", "a\u00b7ber", "doch", "ver\u00b7muth\u00b7lich", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADV", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Der gr\u00f6ssern Herrlichkeit, der Seeligkeit, dem Licht", "tokens": ["Der", "gr\u00f6s\u00b7sern", "Herr\u00b7lich\u00b7keit", ",", "der", "See\u00b7lig\u00b7keit", ",", "dem", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wird zu vergleichen seyn, so sie erlangen werden", "tokens": ["Wird", "zu", "ver\u00b7glei\u00b7chen", "seyn", ",", "so", "sie", "er\u00b7lan\u00b7gen", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKZU", "VVINF", "VAINF", "$,", "ADV", "PPER", "VVINF", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wenn dermaleinst die C\u00f6rper aus der Erden", "tokens": ["Wenn", "der\u00b7ma\u00b7leinst", "die", "C\u00f6r\u00b7per", "aus", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "An jenem Tage sich zu ihnen wieder f\u00fcgen,", "tokens": ["An", "je\u00b7nem", "Ta\u00b7ge", "sich", "zu", "ih\u00b7nen", "wie\u00b7der", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PRF", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und sie sodann im seeligen Vergn\u00fcgen,", "tokens": ["Und", "sie", "so\u00b7dann", "im", "see\u00b7li\u00b7gen", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Die GOttheit selbst zu sehn, vielleicht gew\u00fcrdigt werden.", "tokens": ["Die", "Got\u00b7theit", "selbst", "zu", "sehn", ",", "viel\u00b7leicht", "ge\u00b7w\u00fcr\u00b7digt", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$,", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wer wei\u00df, ob GOTT sie nicht, in den Saphirnen H\u00f6h\u2019n,", "tokens": ["Wer", "wei\u00df", ",", "ob", "GoTT", "sie", "nicht", ",", "in", "den", "Sa\u00b7phir\u00b7nen", "H\u00f6h'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "NE", "PPER", "PTKNEG", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der Sonn\u2019 und Weldten Meng\u2019, als Seiner Wercke Pracht,", "tokens": ["Der", "Sonn'", "und", "Weld\u00b7ten", "Meng'", ",", "als", "Sei\u00b7ner", "Wer\u00b7cke", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NE", "$,", "KOUS", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Um in den Wercken Jhn zu sehen, f\u00e4hig macht.", "tokens": ["Um", "in", "den", "Wer\u00b7cken", "Jhn", "zu", "se\u00b7hen", ",", "f\u00e4\u00b7hig", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Doch stell\u2019 ichs GOTT anheim, und halte die Gedancken", "tokens": ["Doch", "stell'", "ichs", "GoTT", "an\u00b7heim", ",", "und", "hal\u00b7te", "die", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "NE", "NE", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Von seinen Ordnungen in Demuhts-vollen Schraneken.", "tokens": ["Von", "sei\u00b7nen", "Ord\u00b7nun\u00b7gen", "in", "Dem\u00b7uhts\u00b7vol\u00b7len", "Schra\u00b7ne\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}}}}}