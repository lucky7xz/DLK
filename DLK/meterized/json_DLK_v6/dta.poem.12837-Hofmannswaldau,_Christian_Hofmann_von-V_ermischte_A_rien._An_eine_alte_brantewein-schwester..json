{"dta.poem.12837": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "V ermischte  A rien.  \n An eine alte brantewein-schwester.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Alte mutter! sch\u00e4mt euch doch,", "tokens": ["Al\u00b7te", "mut\u00b7ter", "!", "sch\u00e4mt", "euch", "doch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ihr eure alte sachen", "tokens": ["Da\u00df", "ihr", "eu\u00b7re", "al\u00b7te", "sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Annoch wolt gemeine machen,", "tokens": ["An\u00b7noch", "wolt", "ge\u00b7mei\u00b7ne", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und zieht noch am liebes-joch.", "tokens": ["Und", "zieht", "noch", "am", "lie\u00b7bes\u00b7joch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ach mischt ihr euch, alte! noch unter die nymphen,", "tokens": ["Ach", "mischt", "ihr", "euch", ",", "al\u00b7te", "!", "noch", "un\u00b7ter", "die", "nym\u00b7phen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "PPER", "$,", "ADJA", "$.", "ADV", "APPR", "ART", "ADJA", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Als wolte der nebel die sterne beschimpffen.", "tokens": ["Als", "wol\u00b7te", "der", "ne\u00b7bel", "die", "ster\u00b7ne", "be\u00b7schimpf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "ART", "NN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Mu\u00df das nicht po\u00dfierlich stehn,", "tokens": ["Mu\u00df", "das", "nicht", "po\u00df\u00b7ier\u00b7lich", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die tochter mit der mutter,", "tokens": ["Wenn", "die", "toch\u00b7ter", "mit", "der", "mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alter speck und stinck\u2019chte hutter,", "tokens": ["Al\u00b7ter", "speck", "und", "stin\u00b7ck'ch\u00b7te", "hut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Beyde courtesiren gehn;", "tokens": ["Bey\u00b7de", "cour\u00b7te\u00b7si\u00b7ren", "gehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So lernen die jungen in zeiten von alten", "tokens": ["So", "ler\u00b7nen", "die", "jun\u00b7gen", "in", "zei\u00b7ten", "von", "al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "APPR", "NN", "APPR", "ADJA"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Hinf\u00fchro die g\u00fcther mit nutzen verwalten.", "tokens": ["Hin\u00b7f\u00fch\u00b7ro", "die", "g\u00fc\u00b7ther", "mit", "nut\u00b7zen", "ver\u00b7wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Niemals k\u00f6nnt ihr n\u00fcchtern seyn,", "tokens": ["Nie\u00b7mals", "k\u00f6nnt", "ihr", "n\u00fcch\u00b7tern", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn ihr tragt die bisgen heller", "tokens": ["Denn", "ihr", "tragt", "die", "bis\u00b7gen", "hel\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach taback und in den keller,", "tokens": ["Nach", "ta\u00b7back", "und", "in", "den", "kel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der liebe brantewein,", "tokens": ["Und", "der", "lie\u00b7be", "bran\u00b7te\u00b7wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der mu\u00df euch so morgens als abends ergetzen,", "tokens": ["Der", "mu\u00df", "euch", "so", "mor\u00b7gens", "als", "a\u00b7bends", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ADV", "KOKOM", "ADV", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und solt er euch lunge und leber verletzen.", "tokens": ["Und", "solt", "er", "euch", "lun\u00b7ge", "und", "le\u00b7ber", "ver\u00b7let\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "VVFIN", "KON", "ADJD", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Nun wohlan! bekehrt euch doch!", "tokens": ["Nun", "wo\u00b7hlan", "!", "be\u00b7kehrt", "euch", "doch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Heute habt ihr zeit zur bu\u00dfe;", "tokens": ["Heu\u00b7te", "habt", "ihr", "zeit", "zur", "bu\u00b7\u00dfe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Morgen ists schon nicht mehr mu\u00dfe,", "tokens": ["Mor\u00b7gen", "ists", "schon", "nicht", "mehr", "mu\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PTKNEG", "ADV", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn ihr sterbt wol heute noch:", "tokens": ["Denn", "ihr", "sterbt", "wol", "heu\u00b7te", "noch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Drum schickt euch nur immer zum seeligen ende,", "tokens": ["Drum", "schickt", "euch", "nur", "im\u00b7mer", "zum", "see\u00b7li\u00b7gen", "en\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Befehlet die seele in g\u00f6ttliche h\u00e4nde.", "tokens": ["Be\u00b7feh\u00b7let", "die", "see\u00b7le", "in", "g\u00f6tt\u00b7li\u00b7che", "h\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}