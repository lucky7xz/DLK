{"textgrid.poem.40263": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Damals, Seele, ja; ich war ein Kind \u2013", "tokens": ["Da\u00b7mals", ",", "See\u00b7le", ",", "ja", ";", "ich", "war", "ein", "Kind", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "ADV", "$.", "PPER", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "und das alte Forsthaus dumpf und eng.", "tokens": ["und", "das", "al\u00b7te", "Forst\u00b7haus", "dumpf", "und", "eng", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und in hellen und in dunkeln N\u00e4chten,", "tokens": ["Und", "in", "hel\u00b7len", "und", "in", "dun\u00b7keln", "N\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "wenn ich so am Kammerfenster stand", "tokens": ["wenn", "ich", "so", "am", "Kam\u00b7mer\u00b7fens\u00b7ter", "stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "und die gro\u00dfen Eichen schwarz erschauern h\u00f6rte,", "tokens": ["und", "die", "gro\u00b7\u00dfen", "Ei\u00b7chen", "schwarz", "er\u00b7schau\u00b7ern", "h\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "wurde mir das Dach noch dumpfer.", "tokens": ["wur\u00b7de", "mir", "das", "Dach", "noch", "dum\u00b7pfer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Denn immer sah ich,", "tokens": ["Denn", "im\u00b7mer", "sah", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "dr\u00fcben,", "tokens": ["dr\u00fc\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "dr\u00fcben fern,", "tokens": ["dr\u00fc\u00b7ben", "fern", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "wo aus der Waldnacht um die Felder", "tokens": ["wo", "aus", "der", "Wald\u00b7nacht", "um", "die", "Fel\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "die Eine hohe Kiefer in den Himmel horchte,", "tokens": ["die", "Ei\u00b7ne", "ho\u00b7he", "Kie\u00b7fer", "in", "den", "Him\u00b7mel", "horch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "immer ruhte dann da dr\u00fcben", "tokens": ["im\u00b7mer", "ruh\u00b7te", "dann", "da", "dr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "durch die Wolken", "tokens": ["durch", "die", "Wol\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "jener weitgewobne Schimmerkreis.", "tokens": ["je\u00b7ner", "weit\u00b7ge\u00b7wob\u00b7ne", "Schim\u00b7mer\u00b7kreis", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Und in bleichen N\u00e4chten", "tokens": ["Und", "in", "blei\u00b7chen", "N\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "war er bla\u00df und flehend", "tokens": ["war", "er", "bla\u00df", "und", "fle\u00b7hend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "wie ein Heiligenschein,", "tokens": ["wie", "ein", "Hei\u00b7li\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.18": {"text": "aber in den grauen", "tokens": ["a\u00b7ber", "in", "den", "grau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "tr\u00f6stlich blau und schirmend", "tokens": ["tr\u00f6st\u00b7lich", "blau", "und", "schir\u00b7mend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "KON", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.20": {"text": "wie der Glanz von einem klaren Stahlschild", "tokens": ["wie", "der", "Glanz", "von", "ei\u00b7nem", "kla\u00b7ren", "Stahl\u00b7schild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "oder mild und gelb wie Kronengold;", "tokens": ["o\u00b7der", "mild", "und", "gelb", "wie", "Kro\u00b7nen\u00b7gold", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "KOKOM", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.22": {"text": "und ich wollte K\u00f6nig werden.", "tokens": ["und", "ich", "woll\u00b7te", "K\u00f6\u00b7nig", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "NN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Meine Mutter aber sagte mir's,", "tokens": ["Mei\u00b7ne", "Mut\u00b7ter", "a\u00b7ber", "sag\u00b7te", "mir's", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.24": {"text": "dort lag Berlin ...", "tokens": ["dort", "lag", "Ber\u00b7lin", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.25": {"text": "Damals wu\u00dft'ich nicht, warum mir bangte,", "tokens": ["Da\u00b7mals", "wu\u00dft'\u00b7ich", "nicht", ",", "wa\u00b7rum", "mir", "bang\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "als sie mir die Stirne k\u00fc\u00dfte.", "tokens": ["als", "sie", "mir", "die", "Stir\u00b7ne", "k\u00fc\u00df\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Dort lag die Lichtstadt", "tokens": ["Dort", "lag", "die", "Licht\u00b7stadt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.28": {"text": "und straalte ...", "tokens": ["und", "straal\u00b7te", "..."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$("], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Heute ist auch Nacht;", "tokens": ["Heu\u00b7te", "ist", "auch", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "der Mond will in mein Fenster,", "tokens": ["der", "Mond", "will", "in", "mein", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "und ich sehe \u00fcber tausend D\u00e4cher.", "tokens": ["und", "ich", "se\u00b7he", "\u00fc\u00b7ber", "tau\u00b7send", "D\u00e4\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "CARD", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Im schweren, weichen Schnee", "tokens": ["Im", "schwe\u00b7ren", ",", "wei\u00b7chen", "Schnee"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "ruhn und horchen mit verhaltnem Atem", "tokens": ["ruhn", "und", "hor\u00b7chen", "mit", "ver\u00b7halt\u00b7nem", "A\u00b7tem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVINF", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "die Schatten der Stadt.", "tokens": ["die", "Schat\u00b7ten", "der", "Stadt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Bis in den blauen Silberschein der Ferne", "tokens": ["Bis", "in", "den", "blau\u00b7en", "Sil\u00b7ber\u00b7schein", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "schwillt in langen Falten", "tokens": ["schwillt", "in", "lan\u00b7gen", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "wei\u00df und zart die sanfte Decke hin,", "tokens": ["wei\u00df", "und", "zart", "die", "sanf\u00b7te", "De\u00b7cke", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "wie \u00fcber die Kissen", "tokens": ["wie", "\u00fc\u00b7ber", "die", "Kis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "eines T\u00e4uflings.", "tokens": ["ei\u00b7nes", "T\u00e4uf\u00b7lings", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Die aber, die darunter schlafen \u2013", "tokens": ["Die", "a\u00b7ber", ",", "die", "da\u00b7run\u00b7ter", "schla\u00b7fen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "PRELS", "PAV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "und wachen? \u2013 \u2013", "tokens": ["und", "wa\u00b7chen", "?", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVINF", "$.", "$(", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Schwarz und scharf", "tokens": ["Schwarz", "und", "scharf"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "stechen die T\u00fcrme,", "tokens": ["ste\u00b7chen", "die", "T\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Kirche neben Kirche,", "tokens": ["Kir\u00b7che", "ne\u00b7ben", "Kir\u00b7che", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "in den k\u00fchlen Himmel;", "tokens": ["in", "den", "k\u00fch\u00b7len", "Him\u00b7mel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "stahlspitz flittert ein Glanz", "tokens": ["stahl\u00b7spitz", "flit\u00b7tert", "ein", "Glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "um die finsterhohe Kuppelkrone", "tokens": ["um", "die", "fins\u00b7ter\u00b7ho\u00b7he", "Kup\u00b7pel\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "jenes Palastes,", "tokens": ["je\u00b7nes", "Pa\u00b7las\u00b7tes", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.21": {"text": "und \u00fcber einem dicken Schlote", "tokens": ["und", "\u00fc\u00b7ber", "ei\u00b7nem", "di\u00b7cken", "Schlo\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "stockt ein Schild von Qualm.", "tokens": ["stockt", "ein", "Schild", "von", "Qualm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.23": {"text": "Jetzt, unten an der Ecke dr\u00fcben,", "tokens": ["Jetzt", ",", "un\u00b7ten", "an", "der", "E\u00b7cke", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "wo eine Gaslaterne", "tokens": ["wo", "ei\u00b7ne", "Gas\u00b7la\u00b7ter\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "tr\u00fcbgelb mit dem Mondlicht k\u00e4mpft,", "tokens": ["tr\u00fcb\u00b7gelb", "mit", "dem", "Mond\u00b7licht", "k\u00e4mpft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}, "line.26": {"text": "schimpft ein frierender Schutzmann", "tokens": ["schimpft", "ein", "frie\u00b7ren\u00b7der", "Schutz\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.27": {"text": "ein betrunknes Stra\u00dfenm\u00e4dchen aus.", "tokens": ["ein", "be\u00b7trunk\u00b7nes", "Stra\u00b7\u00dfen\u00b7m\u00e4d\u00b7chen", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.28": {"text": "Seele, ja:", "tokens": ["See\u00b7le", ",", "ja", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.29": {"text": "da liegt Berlin ...", "tokens": ["da", "liegt", "Ber\u00b7lin", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$("], "meter": "+-+-", "measure": "trochaic.di"}}}}}