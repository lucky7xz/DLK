{"textgrid.poem.36973": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir singen unsre Lieder, so lang' das Schwert noch ruht;", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir singen unsre Lieder, so lang' das Schwert noch ruht;", "tokens": ["Wir", "sin\u00b7gen", "uns\u00b7re", "Lie\u00b7der", ",", "so", "lang'", "das", "Schwert", "noch", "ruht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mit Melodieen wecken wir unsrer Br\u00fcder Muth;", "tokens": ["Mit", "Me\u00b7lo\u00b7di\u00b7e\u00b7en", "we\u00b7cken", "wir", "uns\u00b7rer", "Br\u00fc\u00b7der", "Muth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wir singen f\u00fcr die Freiheit, ob auch der Feind uns droht;", "tokens": ["Wir", "sin\u00b7gen", "f\u00fcr", "die", "Frei\u00b7heit", ",", "ob", "auch", "der", "Feind", "uns", "droht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "KOUS", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wir singen und wir sterben den sch\u00f6nen Liedertod.", "tokens": ["Wir", "sin\u00b7gen", "und", "wir", "ster\u00b7ben", "den", "sch\u00f6\u00b7nen", "Lie\u00b7der\u00b7tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Gott hauchte uns das Leben der Lieder in die Brust;", "tokens": ["Gott", "hauch\u00b7te", "uns", "das", "Le\u00b7ben", "der", "Lie\u00b7der", "in", "die", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Lied werden unsre Schmerzen und Lied ist unsre Lust;", "tokens": ["Lied", "wer\u00b7den", "uns\u00b7re", "Schmer\u00b7zen", "und", "Lied", "ist", "uns\u00b7re", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "So k\u00e4mpfen wir und singen nach g\u00f6ttlichem Gebot:", "tokens": ["So", "k\u00e4mp\u00b7fen", "wir", "und", "sin\u00b7gen", "nach", "g\u00f6tt\u00b7li\u00b7chem", "Ge\u00b7bot", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wir singen und wir sterben den sch\u00f6nen Liedertod.", "tokens": ["Wir", "sin\u00b7gen", "und", "wir", "ster\u00b7ben", "den", "sch\u00f6\u00b7nen", "Lie\u00b7der\u00b7tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein jeder Tropfen Blutes ein rosenrothes Lied,", "tokens": ["Ein", "je\u00b7der", "Trop\u00b7fen", "Blu\u00b7tes", "ein", "ro\u00b7sen\u00b7ro\u00b7thes", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das, aus dem eignen quellend, in alle Herzen zieht,", "tokens": ["Das", ",", "aus", "dem", "eig\u00b7nen", "quel\u00b7lend", ",", "in", "al\u00b7le", "Her\u00b7zen", "zieht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "APPR", "ART", "ADJA", "VVPP", "$,", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So opfern wir das Leben f\u00fcr unser Aller Noth,", "tokens": ["So", "op\u00b7fern", "wir", "das", "Le\u00b7ben", "f\u00fcr", "un\u00b7ser", "Al\u00b7ler", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wir singen und wir sterben den sch\u00f6nen Liedertod.", "tokens": ["Wir", "sin\u00b7gen", "und", "wir", "ster\u00b7ben", "den", "sch\u00f6\u00b7nen", "Lie\u00b7der\u00b7tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Und will's der Feind nich anders, so sind wir wohlbewehrt,", "tokens": ["Und", "will's", "der", "Feind", "nich", "an\u00b7ders", ",", "so", "sind", "wir", "wohl\u00b7be\u00b7wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PTKNEG", "ADV", "$,", "ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+----+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "So ziehen wir und schwingen f\u00fcr unser Wort das Schwert!", "tokens": ["So", "zie\u00b7hen", "wir", "und", "schwin\u00b7gen", "f\u00fcr", "un\u00b7ser", "Wort", "das", "Schwert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Doch singen wir im Kampfe, da\u00df rings Begeisterung loht.", "tokens": ["Doch", "sin\u00b7gen", "wir", "im", "Kamp\u00b7fe", ",", "da\u00df", "rings", "Be\u00b7geis\u00b7te\u00b7rung", "loht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$,", "KOUS", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wir singen und wir sterben den sch\u00f6nen Liedertod.", "tokens": ["Wir", "sin\u00b7gen", "und", "wir", "ster\u00b7ben", "den", "sch\u00f6\u00b7nen", "Lie\u00b7der\u00b7tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Und wie die Lerchen jubeln entgegen wir dem Tag,", "tokens": ["Und", "wie", "die", "Ler\u00b7chen", "ju\u00b7beln", "ent\u00b7ge\u00b7gen", "wir", "dem", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVINF", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df Sonne nicht mehr leuchtet auf unsres Volkes Schmach;", "tokens": ["Da\u00df", "Son\u00b7ne", "nicht", "mehr", "leuch\u00b7tet", "auf", "uns\u00b7res", "Vol\u00b7kes", "Schmach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKNEG", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das letzte Lied der Freiheit! in ihrem Morgenroth,", "tokens": ["Das", "letz\u00b7te", "Lied", "der", "Frei\u00b7heit", "!", "in", "ih\u00b7rem", "Mor\u00b7gen\u00b7roth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So singen wir und sterben den sch\u00f6nen Liedertod.", "tokens": ["So", "sin\u00b7gen", "wir", "und", "ster\u00b7ben", "den", "sch\u00f6\u00b7nen", "Lie\u00b7der\u00b7tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}