{"textgrid.poem.44896": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es spukt! Ein Doppelg\u00e4nger, ein Gespenst!", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es spukt! Ein Doppelg\u00e4nger, ein Gespenst!", "tokens": ["Es", "spukt", "!", "Ein", "Dop\u00b7pel\u00b7g\u00e4n\u00b7ger", ",", "ein", "Ge\u00b7spenst", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Fern lebend in Paris, nicht minder doch darum", "tokens": ["Fern", "le\u00b7bend", "in", "Pa\u00b7ris", ",", "nicht", "min\u00b7der", "doch", "da\u00b7rum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "NE", "$,", "PTKNEG", "ADV", "ADV", "PAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geht Paganini leibhaft unter uns herum,", "tokens": ["Geht", "Pa\u00b7ga\u00b7ni\u00b7ni", "leib\u00b7haft", "un\u00b7ter", "uns", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dennoch kein Gespenst! Kein Geist! nur K\u00f6rper.", "tokens": ["Und", "den\u00b7noch", "kein", "Ge\u00b7spenst", "!", "Kein", "Geist", "!", "nur", "K\u00f6r\u00b7per", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "$.", "PIAT", "NN", "$.", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}