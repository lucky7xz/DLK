{"textgrid.poem.65795": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "69", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nichts fehlt den \u00e4u\u00dfern Gaben, die dem Licht", "tokens": ["Nichts", "fehlt", "den", "\u00e4u\u00b7\u00dfern", "Ga\u00b7ben", ",", "die", "dem", "Licht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du zeigst, das Menschenwitz verbessern k\u00f6nnte;", "tokens": ["Du", "zeigst", ",", "das", "Men\u00b7schen\u00b7witz", "ver\u00b7bes\u00b7sern", "k\u00f6nn\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie aller Mund, durch den die Seele spricht,", "tokens": ["Wie", "al\u00b7ler", "Mund", ",", "durch", "den", "die", "See\u00b7le", "spricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "APPR", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ja selbst dein Feind dies wahre Lob dir g\u00f6nnte.", "tokens": ["Ja", "selbst", "dein", "Feind", "dies", "wah\u00b7re", "Lob", "dir", "g\u00f6nn\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "PPOSAT", "NN", "PDS", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So wird f\u00fcr \u00e4u\u00dfern Reiz dir \u00e4u\u00df'rer Lohn;", "tokens": ["So", "wird", "f\u00fcr", "\u00e4u\u00b7\u00dfern", "Reiz", "dir", "\u00e4u\u00df'\u00b7rer", "Lohn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch eben jener Mund, der, was dein eigen", "tokens": ["Doch", "e\u00b7ben", "je\u00b7ner", "Mund", ",", "der", ",", "was", "dein", "ei\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "PDAT", "NN", "$,", "PRELS", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dir gab, zerst\u00f6rt dies Lob aus anderm Ton,", "tokens": ["Dir", "gab", ",", "zer\u00b7st\u00f6rt", "dies", "Lob", "aus", "an\u00b7derm", "Ton", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PDS", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und sp\u00fcret weiter als die Augen reichen.", "tokens": ["Und", "sp\u00fc\u00b7ret", "wei\u00b7ter", "als", "die", "Au\u00b7gen", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "In deiner Seele Sch\u00f6nheit tauchen sie;", "tokens": ["In", "dei\u00b7ner", "See\u00b7le", "Sch\u00f6n\u00b7heit", "tau\u00b7chen", "sie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die mi\u00dft Vermutung ab nach deinen Taten:", "tokens": ["Die", "mi\u00dft", "Ver\u00b7mu\u00b7tung", "ab", "nach", "dei\u00b7nen", "Ta\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Kargm\u00fctig, augeng\u00fctig hauchen sie", "tokens": ["Karg\u00b7m\u00fc\u00b7tig", ",", "au\u00b7gen\u00b7g\u00fc\u00b7tig", "hau\u00b7chen", "sie"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Auf deinen Blumenflor des Unkrauts geilen Schwaden.", "tokens": ["Auf", "dei\u00b7nen", "Blu\u00b7men\u00b7flor", "des", "Un\u00b7krauts", "gei\u00b7len", "Schwa\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch da\u00df dein Duft nicht gleicht dem Augenschein,", "tokens": ["Doch", "da\u00df", "dein", "Duft", "nicht", "gleicht", "dem", "Au\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PTKNEG", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Daran ist schuld: du machst dich selbst gemein.", "tokens": ["Da\u00b7ran", "ist", "schuld", ":", "du", "machst", "dich", "selbst", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADJD", "$.", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}