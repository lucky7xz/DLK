{"textgrid.poem.40292": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Venus Primitiva", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O da\u00df der Ku\u00df doch ewig dauern m\u00f6chte,", "tokens": ["O", "da\u00df", "der", "Ku\u00df", "doch", "e\u00b7wig", "dau\u00b7ern", "m\u00f6ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "NN", "ADV", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u2013 starr stand, wie Binsen starr, der Schwarm der G\u00e4ste;", "tokens": ["\u2013", "starr", "stand", ",", "wie", "Bin\u00b7sen", "starr", ",", "der", "Schwarm", "der", "G\u00e4s\u00b7te", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "$,", "PWAV", "NN", "ADJD", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "der Ku\u00df doch ewig, den ich auf die Rechte,", "tokens": ["der", "Ku\u00df", "doch", "e\u00b7wig", ",", "den", "ich", "auf", "die", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "tanztaumelnd dir auf Hals und Br\u00fcste pre\u00dfte!", "tokens": ["tanz\u00b7tau\u00b7melnd", "dir", "auf", "Hals", "und", "Br\u00fcs\u00b7te", "pre\u00df\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nein, l\u00e4nger duld'ich nicht dies leere Sehnen,", "tokens": ["Nein", ",", "l\u00e4n\u00b7ger", "duld'\u00b7ich", "nicht", "dies", "lee\u00b7re", "Seh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "ADJD", "PTKNEG", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ich ", "tokens": ["ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "die liebekranken Glieder N\u00e4chtens dehnen;", "tokens": ["die", "lie\u00b7be\u00b7kran\u00b7ken", "Glie\u00b7der", "N\u00e4ch\u00b7tens", "deh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbo komm, du Weib!\u00ab entbreit'ich meine Arme ...", "tokens": ["\u00bb", "o", "komm", ",", "du", "Weib", "!", "\u00ab", "ent\u00b7breit'\u00b7ich", "mei\u00b7ne", "Ar\u00b7me", "..."], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VVFIN", "$,", "PPER", "NN", "$.", "$(", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Oh komm! noch f\u00fchlt dich zitternd jeder Sinn,", "tokens": ["Oh", "komm", "!", "noch", "f\u00fchlt", "dich", "zit\u00b7ternd", "je\u00b7der", "Sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "vom hei\u00dfen Duft berauscht aus deinem Kleide,", "tokens": ["vom", "hei\u00b7\u00dfen", "Duft", "be\u00b7rauscht", "aus", "dei\u00b7nem", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "f\u00fchlt wogend gl\u00fchn, du Flammenk\u00f6nigin,", "tokens": ["f\u00fchlt", "wo\u00b7gend", "gl\u00fchn", ",", "du", "Flam\u00b7men\u00b7k\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVINF", "$,", "PPER", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "im Aschenflor um dich die Kupferseide.", "tokens": ["im", "A\u00b7schen\u00b7flor", "um", "dich", "die", "Kup\u00b7fer\u00b7sei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gie\u00df aus in mich die Schale deiner Glut!", "tokens": ["Gie\u00df", "aus", "in", "mich", "die", "Scha\u00b7le", "dei\u00b7ner", "Glut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "PRF", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ich ", "tokens": ["ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "vor dieses Feuerregens milder Brut,", "tokens": ["vor", "die\u00b7ses", "Feu\u00b7er\u00b7re\u00b7gens", "mil\u00b7der", "Brut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "vor diesen Weh'n, die w\u00fchlend in mir brauen.", "tokens": ["vor", "die\u00b7sen", "Weh'n", ",", "die", "w\u00fch\u00b7lend", "in", "mir", "brau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Es schie\u00dft die Saat aus ihrem dunklen Schoo\u00df,", "tokens": ["Es", "schie\u00dft", "die", "Saat", "aus", "ih\u00b7rem", "dunk\u00b7len", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "die lange schmachtend lag in spr\u00f6der H\u00fclle;", "tokens": ["die", "lan\u00b7ge", "schmach\u00b7tend", "lag", "in", "spr\u00f6\u00b7der", "H\u00fcl\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ich will mich lauter bl\u00fchn, lauter und los", "tokens": ["ich", "will", "mich", "lau\u00b7ter", "bl\u00fchn", ",", "lau\u00b7ter", "und", "los"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$,", "ADJD", "KON", "PTKVZ"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "aus meiner dumpfen Brunst zu Frucht und F\u00fclle!", "tokens": ["aus", "mei\u00b7ner", "dum\u00b7pfen", "Brunst", "zu", "Frucht", "und", "F\u00fcl\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Satt werden will ich meiner scheuen Lust:", "tokens": ["Satt", "wer\u00b7den", "will", "ich", "mei\u00b7ner", "scheu\u00b7en", "Lust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAINF", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "oh komm, du Weib! nimm ", "tokens": ["oh", "komm", ",", "du", "Weib", "!", "nimm"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADJD", "VVFIN", "$,", "PPER", "NN", "$.", "XY"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "die Furcht, die Sehnsucht dieser jungen Brust;", "tokens": ["die", "Furcht", ",", "die", "Sehn\u00b7sucht", "die\u00b7ser", "jun\u00b7gen", "Brust", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "noch trank ich nie den Rausch eurer Pokale ...", "tokens": ["noch", "trank", "ich", "nie", "den", "Rausch", "eu\u00b7rer", "Po\u00b7ka\u00b7le", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Auf Nelkend\u00fcften kommt die Nacht gezogen,", "tokens": ["Auf", "Nel\u00b7ken\u00b7d\u00fcf\u00b7ten", "kommt", "die", "Nacht", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "o k\u00e4mst auch Du so s\u00fc\u00df und so verstohlen:", "tokens": ["o", "k\u00e4mst", "auch", "Du", "so", "s\u00fc\u00df", "und", "so", "ver\u00b7stoh\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "VVFIN", "ADV", "PPER", "ADV", "ADJD", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "so mondeswei\u00df dich in die Sammetwogen,", "tokens": ["so", "mon\u00b7des\u00b7wei\u00df", "dich", "in", "die", "Sam\u00b7met\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "den Purpurflaum der schw\u00e4rzlichen Violen,", "tokens": ["den", "Pur\u00b7pur\u00b7flaum", "der", "schw\u00e4rz\u00b7li\u00b7chen", "Vi\u00b7o\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "die ich dir streun will, an mich her zu betten,", "tokens": ["die", "ich", "dir", "streun", "will", ",", "an", "mich", "her", "zu", "bet\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "PPER", "VVINF", "VMFIN", "$,", "APPR", "PRF", "APZR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "da\u00df alle meine M\u00e4chte an des Weibes", "tokens": ["da\u00df", "al\u00b7le", "mei\u00b7ne", "M\u00e4ch\u00b7te", "an", "des", "Wei\u00b7bes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "enth\u00fcllten G\u00f6ttlichkeiten sich entketten,", "tokens": ["ent\u00b7h\u00fcll\u00b7ten", "G\u00f6tt\u00b7lich\u00b7kei\u00b7ten", "sich", "ent\u00b7ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "versink'ich \u2013 in den Teppich \u2013 deines \u2013 Leibes!", "tokens": ["ver\u00b7sink'\u00b7ich", "\u2013", "in", "den", "Tep\u00b7pich", "\u2013", "dei\u00b7nes", "\u2013", "Lei\u00b7bes", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$(", "APPR", "ART", "NN", "$(", "PPOSAT", "$(", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}