{"dta.poem.13039": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ees trachtet jede fast nach diesem Ehren-Rang:", "tokens": ["\u201e", "es", "trach\u00b7tet", "je\u00b7de", "fast", "nach", "die\u00b7sem", "Eh\u00b7ren\u00b7Rang", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PIAT", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "270\u201dHat die Begierde nicht der Eigen-Liebe Klang,", "tokens": ["\"", "Hat", "die", "Be\u00b7gier\u00b7de", "nicht", "der", "Ei\u00b7gen\u00b7Lie\u00b7be", "Klang", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ART", "NN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201edie man sonst billig ha\u00dft? allein es will geb\u00fchren,", "tokens": ["\u201e", "die", "man", "sonst", "bil\u00b7lig", "ha\u00dft", "?", "al\u00b7lein", "es", "will", "ge\u00b7b\u00fch\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PIS", "ADV", "ADJD", "VVFIN", "$.", "ADV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eda\u00df man von dem Verdienst und Ruhm nichts soll verliehren,", "tokens": ["\u201e", "da\u00df", "man", "von", "dem", "Ver\u00b7dienst", "und", "Ruhm", "nichts", "soll", "ver\u00b7lieh\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PIS", "APPR", "ART", "NN", "KON", "NN", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eman hie\u00df es ungerecht. Jedoch ich la\u00df\u2019 es seyn:", "tokens": ["\u201e", "man", "hie\u00df", "es", "un\u00b7ge\u00b7recht", ".", "Je\u00b7doch", "ich", "la\u00df'", "es", "seyn", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VVFIN", "PPER", "ADJD", "$.", "ADV", "PPER", "VVFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ees schliesse dieses Fries nur einen Nahmen ein,", "tokens": ["\u201e", "es", "schlies\u00b7se", "die\u00b7ses", "Fries", "nur", "ei\u00b7nen", "Nah\u00b7men", "ein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PDAT", "NN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "275\u201dDem alle Tugenden den Vorzug geben sollen;", "tokens": ["\"", "Dem", "al\u00b7le", "Tu\u00b7gen\u00b7den", "den", "Vor\u00b7zug", "ge\u00b7ben", "sol\u00b7len", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PIAT", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ewer wird den Unterscheid, bedenckt es! leiden wollen?", "tokens": ["\u201e", "wer", "wird", "den", "Un\u00b7ter\u00b7scheid", ",", "be\u00b7denckt", "es", "!", "lei\u00b7den", "wol\u00b7len", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "ART", "NN", "$,", "VVFIN", "PPER", "$.", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}