{"textgrid.poem.46975": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Deutschlands Heldenleib", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu welch hohem Heldenleibe", "tokens": ["Zu", "welch", "ho\u00b7hem", "Hel\u00b7den\u00b7lei\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einer Riesin voller Mark", "tokens": ["Ei\u00b7ner", "Rie\u00b7sin", "vol\u00b7ler", "Mark"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nntest du aus schwachem Weibe", "tokens": ["K\u00f6nn\u00b7test", "du", "aus", "schwa\u00b7chem", "Wei\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wachsen, Deutschland, gro\u00df und stark!", "tokens": ["Wach\u00b7sen", ",", "Deutschland", ",", "gro\u00df", "und", "stark", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Da vom Moder der Verwesung,", "tokens": ["Da", "vom", "Mo\u00b7der", "der", "Ver\u00b7we\u00b7sung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo du lagest schwer und tief,", "tokens": ["Wo", "du", "la\u00b7gest", "schwer", "und", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott zu pl\u00f6tzlicher Genesung", "tokens": ["Gott", "zu", "pl\u00f6tz\u00b7li\u00b7cher", "Ge\u00b7ne\u00b7sung"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.4": {"text": "Dich des neuen Lebens rief!", "tokens": ["Dich", "des", "neu\u00b7en", "Le\u00b7bens", "rief", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn nur auf dem Bau der Glieder", "tokens": ["Wenn", "nur", "auf", "dem", "Bau", "der", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleich ein kriegerisches Haupt", "tokens": ["Gleich", "ein", "krie\u00b7ge\u00b7ri\u00b7sches", "Haupt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oben wollte wachsen wieder,", "tokens": ["O\u00b7ben", "woll\u00b7te", "wach\u00b7sen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das man dir im Schlaf geraubt!", "tokens": ["Das", "man", "dir", "im", "Schlaf", "ge\u00b7raubt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Wenn nur Glieder nicht, die kleinen,", "tokens": ["Wenn", "nur", "Glie\u00b7der", "nicht", ",", "die", "klei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "PTKNEG", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Statt ein Leib zu sein vereint,", "tokens": ["Statt", "ein", "Leib", "zu", "sein", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKZU", "VAINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selber Leiber wollten scheinen,", "tokens": ["Sel\u00b7ber", "Lei\u00b7ber", "woll\u00b7ten", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder gar dem Ganzen feind!", "tokens": ["O\u00b7der", "gar", "dem", "Gan\u00b7zen", "feind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Zu welch hohem Heldenleibe", "tokens": ["Zu", "welch", "ho\u00b7hem", "Hel\u00b7den\u00b7lei\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einer Riesin voller Mark", "tokens": ["Ei\u00b7ner", "Rie\u00b7sin", "vol\u00b7ler", "Mark"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nntest du aus schwachem Weibe", "tokens": ["K\u00f6nn\u00b7test", "du", "aus", "schwa\u00b7chem", "Wei\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wachsen, Deutschland, gro\u00df und stark!", "tokens": ["Wach\u00b7sen", ",", "Deutschland", ",", "gro\u00df", "und", "stark", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}