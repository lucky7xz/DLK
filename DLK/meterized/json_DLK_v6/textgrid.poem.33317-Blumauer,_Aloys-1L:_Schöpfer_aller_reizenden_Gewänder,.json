{"textgrid.poem.33317": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sch\u00f6pfer aller reizenden Gew\u00e4nder,", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6pfer aller reizenden Gew\u00e4nder,", "tokens": ["Sch\u00f6p\u00b7fer", "al\u00b7ler", "rei\u00b7zen\u00b7den", "Ge\u00b7w\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ew'ger Forscher auf der Sch\u00f6nheit Spur,", "tokens": ["Ew'\u00b7ger", "For\u00b7scher", "auf", "der", "Sch\u00f6n\u00b7heit", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Unersch\u00f6pflich neuer Reizespender,", "tokens": ["Un\u00b7er\u00b7sch\u00f6pf\u00b7lich", "neu\u00b7er", "Rei\u00b7ze\u00b7spen\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "B\u00e4ndiger der h\u00e4\u00dflichen Natur!", "tokens": ["B\u00e4n\u00b7di\u00b7ger", "der", "h\u00e4\u00df\u00b7li\u00b7chen", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Du nur bist's, in dessen Sch\u00f6pfer-H\u00e4nden", "tokens": ["Du", "nur", "bist's", ",", "in", "des\u00b7sen", "Sch\u00f6p\u00b7fer\u00b7H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "NE", "$,", "APPR", "PRELAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "All' der Reiz und Zauber residirt,", "tokens": ["All'", "der", "Reiz", "und", "Zau\u00b7ber", "re\u00b7si\u00b7dirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Der, wohin wir nur das Auge wenden,", "tokens": ["Der", ",", "wo\u00b7hin", "wir", "nur", "das", "Au\u00b7ge", "wen\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Unsern Blick so m\u00e4chtig fascinirt.", "tokens": ["Un\u00b7sern", "Blick", "so", "m\u00e4ch\u00b7tig", "fas\u00b7ci\u00b7nirt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Du bist's, der selbst mit Pygmalions St\u00e4rke", "tokens": ["Du", "bist's", ",", "der", "selbst", "mit", "Pyg\u00b7ma\u00b7li\u00b7ons", "St\u00e4r\u00b7ke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NE", "$,", "PRELS", "ADV", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Rohen Formen Reiz und Anmuth giebt,", "tokens": ["Ro\u00b7hen", "For\u00b7men", "Reiz", "und", "An\u00b7muth", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aber sich in seiner H\u00e4nde Werke", "tokens": ["A\u00b7ber", "sich", "in", "sei\u00b7ner", "H\u00e4n\u00b7de", "Wer\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nicht, wie jener, auch zugleich \u2013 verliebt.", "tokens": ["Nicht", ",", "wie", "je\u00b7ner", ",", "auch", "zu\u00b7gleich", "\u2013", "ver\u00b7liebt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "PDS", "$,", "ADV", "ADV", "$(", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Der die sch\u00f6nsten Manns- und Weibsgestalten,", "tokens": ["Der", "die", "sch\u00f6ns\u00b7ten", "Manns", "und", "Weibs\u00b7ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "TRUNC", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ja den ganzen Reiz der sch\u00f6nen Welt,", "tokens": ["Ja", "den", "gan\u00b7zen", "Reiz", "der", "sch\u00f6\u00b7nen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Oft in wenig neuen Modefalten", "tokens": ["Oft", "in", "we\u00b7nig", "neu\u00b7en", "Mo\u00b7de\u00b7fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zwischen zwei allm\u00e4cht'gen Fingern h\u00e4lt.", "tokens": ["Zwi\u00b7schen", "zwei", "all\u00b7m\u00e4cht'\u00b7gen", "Fin\u00b7gern", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Der dann bald die magersten Skelette", "tokens": ["Der", "dann", "bald", "die", "ma\u00b7gers\u00b7ten", "Ske\u00b7let\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Mit der Sch\u00f6nheit vollem Reiz umh\u00e4ngt,", "tokens": ["Mit", "der", "Sch\u00f6n\u00b7heit", "vol\u00b7lem", "Reiz", "um\u00b7h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bald den Wuchs der plump'sten Dindonnete", "tokens": ["Bald", "den", "Wuchs", "der", "plump'\u00b7sten", "Din\u00b7don\u00b7ne\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+-++-+-+", "measure": "anapaest.init"}, "line.4": {"text": "In die reizendsten Konturen zw\u00e4ngt.", "tokens": ["In", "die", "rei\u00b7zends\u00b7ten", "Kon\u00b7tu\u00b7ren", "zw\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-++--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Der des schiefsten Afterwuchses Schande", "tokens": ["Der", "des", "schiefs\u00b7ten", "Af\u00b7ter\u00b7wuch\u00b7ses", "Schan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deckt mit einem sch\u00f6n taillirten Rock,", "tokens": ["Deckt", "mit", "ei\u00b7nem", "sch\u00f6n", "tail\u00b7lir\u00b7ten", "Rock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und daher im w\u00f6rtlichen Verstande", "tokens": ["Und", "da\u00b7her", "im", "w\u00f6rt\u00b7li\u00b7chen", "Ver\u00b7stan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "APPRART", "ADJA", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Sich Merkure schnitzt aus einem Stock.", "tokens": ["Sich", "Mer\u00b7ku\u00b7re", "schnitzt", "aus", "ei\u00b7nem", "Stock", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Ja, auf dessen sch\u00f6pferisches Werde", "tokens": ["Ja", ",", "auf", "des\u00b7sen", "sch\u00f6p\u00b7fe\u00b7ri\u00b7sches", "Wer\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "PRELAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hier sich eine volle H\u00fcfte bl\u00e4ht,", "tokens": ["Hier", "sich", "ei\u00b7ne", "vol\u00b7le", "H\u00fcf\u00b7te", "bl\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sich ein Bauch da willig senkt zur Erde,", "tokens": ["Sich", "ein", "Bauch", "da", "wil\u00b7lig", "senkt", "zur", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADV", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dort ein nie geseh'ner Stei\u00df entsteht.", "tokens": ["Dort", "ein", "nie", "ge\u00b7seh'\u00b7ner", "Stei\u00df", "ent\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Unter dessen Hand hier eine Wade", "tokens": ["Un\u00b7ter", "des\u00b7sen", "Hand", "hier", "ei\u00b7ne", "Wa\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aufschwillt, dort ein H\u00f6cker sich verliert,", "tokens": ["Auf\u00b7schwillt", ",", "dort", "ein", "H\u00f6\u00b7cker", "sich", "ver\u00b7liert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hier ein s\u00e4belkrummes Bein gerade,", "tokens": ["Hier", "ein", "s\u00e4\u00b7bel\u00b7krum\u00b7mes", "Bein", "ge\u00b7ra\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und dort voll ein leerer Busen wird.", "tokens": ["Und", "dort", "voll", "ein", "lee\u00b7rer", "Bu\u00b7sen", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}}, "stanza.9": {"line.1": {"text": "Der bald kleiner macht, und bald vergr\u00f6ssert,", "tokens": ["Der", "bald", "klei\u00b7ner", "macht", ",", "und", "bald", "ver\u00b7gr\u00f6s\u00b7sert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "$,", "KON", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und dadurch oft mit dem kleinsten Schnitt", "tokens": ["Und", "da\u00b7durch", "oft", "mit", "dem", "kleins\u00b7ten", "Schnitt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Die er an den Menschenkindern sieht.", "tokens": ["Die", "er", "an", "den", "Men\u00b7schen\u00b7kin\u00b7dern", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Sag, o Lied! Wer ist der Wunderth\u00e4ter,", "tokens": ["Sag", ",", "o", "Lied", "!", "Wer", "ist", "der", "Wun\u00b7der\u00b7th\u00e4\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$.", "PWS", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der mit seinem m\u00e4cht'gen Talisman,", "tokens": ["Der", "mit", "sei\u00b7nem", "m\u00e4cht'\u00b7gen", "Ta\u00b7lis\u00b7man", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Als des hohen Sch\u00f6pfers Stellvertreter,", "tokens": ["Als", "des", "ho\u00b7hen", "Sch\u00f6p\u00b7fers", "Stell\u00b7ver\u00b7tre\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Alle diese Wunder wirken kann?", "tokens": ["Al\u00b7le", "die\u00b7se", "Wun\u00b7der", "wir\u00b7ken", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PDAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "O wer glaubte wohl, da\u00df eine Scheere", "tokens": ["O", "wer", "glaub\u00b7te", "wohl", ",", "da\u00df", "ei\u00b7ne", "Schee\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "PWS", "VVFIN", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "All' die Wunderkraft in sich verschlie\u00dft,", "tokens": ["All'", "die", "Wun\u00b7der\u00b7kraft", "in", "sich", "ver\u00b7schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und der Meister, dem hievon die Ehre", "tokens": ["Und", "der", "Meis\u00b7ter", ",", "dem", "hie\u00b7von", "die", "Eh\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PAV", "ART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Einzig zugeh\u00f6rt \u2013 ein Schneider ist!", "tokens": ["Ein\u00b7zig", "zu\u00b7ge\u00b7h\u00f6rt", "\u2013", "ein", "Schnei\u00b7der", "ist", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$(", "ART", "NE", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}