{"dta.poem.1125": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "11. Die Sch\u00f6ne Stumme.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Du/ die du bist versteckt in ewig stillem Schweigen/ ", "tokens": ["Du", "/", "die", "du", "bist", "ver\u00b7steckt", "in", "e\u00b7wig", "stil\u00b7lem", "Schwei\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PRELS", "PPER", "VAFIN", "VVPP", "APPR", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bist stumm/ und machst dein Lob durch hundert Zeugen", "tokens": ["Bist", "stumm", "/", "und", "machst", "dein", "Lob", "durch", "hun\u00b7dert", "Zeu\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$(", "KON", "VVFIN", "PPOSAT", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "kund/ ", "tokens": ["kund", "/"], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Zwingst tausend Hertzen/ die von deiner Liebe wund/", "tokens": ["Zwingst", "tau\u00b7send", "Hert\u00b7zen", "/", "die", "von", "dei\u00b7ner", "Lie\u00b7be", "wund", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$(", "ART", "APPR", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des sch\u00f6nen Mundes Ruhm zum Sternen machen steigen.", "tokens": ["Des", "sch\u00f6\u00b7nen", "Mun\u00b7des", "Ruhm", "zum", "Ster\u00b7nen", "ma\u00b7chen", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "APPRART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Rosen sicht man auch Corallne Lippen zeigen/", "tokens": ["Die", "Ro\u00b7sen", "sicht", "man", "auch", "Co\u00b7rall\u00b7ne", "Lip\u00b7pen", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Liljen g\u00f6ldne Zung\u2019/ und dennoch schweigt ihr Mund.", "tokens": ["Die", "Lil\u00b7jen", "g\u00f6ld\u00b7ne", "Zung'", "/", "und", "den\u00b7noch", "schweigt", "ihr", "Mund", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "KON", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Durch Schweigen birget man des schlauen Hertzens Grund.", "tokens": ["Durch", "Schwei\u00b7gen", "bir\u00b7get", "man", "des", "schlau\u00b7en", "Hert\u00b7zens", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das kluge Schweigen ist dem Witz/ der Demuth eigen.", "tokens": ["Das", "klu\u00b7ge", "Schwei\u00b7gen", "ist", "dem", "Witz", "/", "der", "De\u00b7muth", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$(", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein leer Versprechen stellt den Magen nicht zur Ruh;", "tokens": ["Ein", "leer", "Ver\u00b7spre\u00b7chen", "stellt", "den", "Ma\u00b7gen", "nicht", "zur", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Liebe l\u00e4sset sich mit Worten nicht vergn\u00fcgen.", "tokens": ["Die", "Lie\u00b7be", "l\u00e4s\u00b7set", "sich", "mit", "Wor\u00b7ten", "nicht", "ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn deiner Sch\u00f6nheit noch die Sprache w\u00fcchse zu/", "tokens": ["Wenn", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "noch", "die", "Spra\u00b7che", "w\u00fcch\u00b7se", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ART", "NN", "VVFIN", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ihr w\u00fcrden allzuviel von dir gefesselt liegen.", "tokens": ["Ihr", "w\u00fcr\u00b7den", "all\u00b7zu\u00b7viel", "von", "dir", "ge\u00b7fes\u00b7selt", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "APPR", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "W\u00e4rn auch so stumm/ wie du/ die falschen Wasser-Frauen/", "tokens": ["W\u00e4rn", "auch", "so", "stumm", "/", "wie", "du", "/", "die", "fal\u00b7schen", "Was\u00b7ser\u00b7Frau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADJD", "$(", "PWAV", "PPER", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So w\u00fcrde man sie sch\u00f6n und nicht betr\u00fcglich schauen.", "tokens": ["So", "w\u00fcr\u00b7de", "man", "sie", "sch\u00f6n", "und", "nicht", "be\u00b7tr\u00fcg\u00b7lich", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PPER", "ADJD", "KON", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}