{"textgrid.poem.37522": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An Karl Arnold Kortum", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier sitz ich auf dem Meilenstein", "tokens": ["Hier", "sitz", "ich", "auf", "dem", "Mei\u00b7len\u00b7stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sehe froh-verwundert,", "tokens": ["Und", "se\u00b7he", "froh\u00b7ver\u00b7wun\u00b7dert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie Du auf Deinem R\u00f6\u00dflein fein", "tokens": ["Wie", "Du", "auf", "Dei\u00b7nem", "R\u00f6\u00df\u00b7lein", "fein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hertrabst durch das Jahrhundert.", "tokens": ["Her\u00b7trabst", "durch", "das", "Jahr\u00b7hun\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Jetzt bist Du da. \u2013 Ich zieh den Hut,", "tokens": ["Jetzt", "bist", "Du", "da", ".", "\u2013", "Ich", "zieh", "den", "Hut", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du ziehst den vollen S\u00e4ckel", "tokens": ["Du", "ziehst", "den", "vol\u00b7len", "S\u00e4\u00b7ckel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wirfst die Batzen wohlgemut", "tokens": ["Und", "wirfst", "die", "Bat\u00b7zen", "wohl\u00b7ge\u00b7mut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In meinen alten Deckel.", "tokens": ["In", "mei\u00b7nen", "al\u00b7ten", "De\u00b7ckel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das R\u00f6\u00dflein sch\u00fcttelt mit dem Kopf,", "tokens": ["Das", "R\u00f6\u00df\u00b7lein", "sch\u00fct\u00b7telt", "mit", "dem", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es sitzt so stramm der Reiter;", "tokens": ["Es", "sitzt", "so", "stramm", "der", "Rei\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie lustig wackelt ihm der Zopf!", "tokens": ["Wie", "lus\u00b7tig", "wa\u00b7ckelt", "ihm", "der", "Zopf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zack zack! So geht es weiter.", "tokens": ["Zack", "zack", "!", "So", "geht", "es", "wei\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}