{"dta.poem.5344": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Garten-Andacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein jedes Blatt, ein jedes Kraut,", "tokens": ["Ein", "je\u00b7des", "Blatt", ",", "ein", "je\u00b7des", "Kraut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das man in diesem Garten schaut,", "tokens": ["Das", "man", "in", "die\u00b7sem", "Gar\u00b7ten", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein\u2019 jede Blum\u2019, ein\u2019 jede Bl\u00fcthe", "tokens": ["Ein'", "je\u00b7de", "Blum'", ",", "ein'", "je\u00b7de", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "$,", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind Proben von des Sch\u00f6pfers G\u00fcte,", "tokens": ["Sind", "Pro\u00b7ben", "von", "des", "Sch\u00f6p\u00b7fers", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sind in der That ein G\u00f6ttliches Geschencke.", "tokens": ["Sind", "in", "der", "That", "ein", "G\u00f6tt\u00b7li\u00b7ches", "Ge\u00b7schen\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da ich in der bewachsenen Allee,", "tokens": ["Da", "ich", "in", "der", "be\u00b7wach\u00b7se\u00b7nen", "Al\u00b7lee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da ich auf vieler B\u00e4ume Cronen", "tokens": ["Da", "ich", "auf", "vie\u00b7ler", "B\u00e4u\u00b7me", "Cro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So viele viele Millionen", "tokens": ["So", "vie\u00b7le", "vie\u00b7le", "Mil\u00b7lion\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIAT", "PIAT", "NN"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.9": {"text": "Derselben nun besitz\u2019 und seh;", "tokens": ["Der\u00b7sel\u00b7ben", "nun", "be\u00b7sitz'", "und", "seh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So gieb, o HErr, da\u00df ich es oft bedencke,", "tokens": ["So", "gieb", ",", "o", "Herr", ",", "da\u00df", "ich", "es", "oft", "be\u00b7den\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "$,", "FM", "NN", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Da\u00df ich mich oft an ihrer Lieblichkeit,", "tokens": ["Da\u00df", "ich", "mich", "oft", "an", "ih\u00b7rer", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "An ihrer Farben-Pracht, an der Vollkommenheit", "tokens": ["An", "ih\u00b7rer", "Fa\u00b7rben\u00b7Pracht", ",", "an", "der", "Voll\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Bildungen ergetz\u2019, in ihnen Dein mich freue,", "tokens": ["Der", "Bil\u00b7dun\u00b7gen", "er\u00b7getz'", ",", "in", "ih\u00b7nen", "Dein", "mich", "freu\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "PPER", "PPOSAT", "PPER", "VVFIN", "$,"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.14": {"text": "Und allezeit in meiner Brust,", "tokens": ["Und", "al\u00b7le\u00b7zeit", "in", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Durch die darob versp\u00fchrte Lust,", "tokens": ["Durch", "die", "da\u00b7rob", "ver\u00b7sp\u00fchr\u00b7te", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Durch ein GOtt Lob! Dein Lob erneue!", "tokens": ["Durch", "ein", "Gott", "Lob", "!", "Dein", "Lob", "er\u00b7neu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$.", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}}}}}