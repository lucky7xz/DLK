{"textgrid.poem.59813": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mirza Schaffy! Du m\u00fc\u00dftest blind sein,", "tokens": ["Mir\u00b7za", "Schaf\u00b7fy", "!", "Du", "m\u00fc\u00df\u00b7test", "blind", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPER", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Von Herzen ein Greis, von Glauben ein Kind sein,", "tokens": ["Von", "Her\u00b7zen", "ein", "Greis", ",", "von", "Glau\u00b7ben", "ein", "Kind", "sein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,", "APPR", "NN", "ART", "NN", "VAINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wolltest du dich in deinem Tun und Dichten", "tokens": ["Woll\u00b7test", "du", "dich", "in", "dei\u00b7nem", "Tun", "und", "Dich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach Gauben und Satzung der Toren richten!", "tokens": ["Nach", "Gau\u00b7ben", "und", "Sat\u00b7zung", "der", "To\u00b7ren", "rich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}