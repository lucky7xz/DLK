{"dta.poem.1016": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Zu traurigem behuf               ", "tokens": ["Zu", "trau\u00b7ri\u00b7gem", "be\u00b7huf"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Erweckte sturm die flur               ", "tokens": ["Er\u00b7weck\u00b7te", "sturm", "die", "flur"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus finstrem tag entfuhr               ", "tokens": ["Aus", "fins\u00b7trem", "tag", "ent\u00b7fuhr"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein todesvogel-ruf             ", "tokens": ["Ein", "to\u00b7des\u00b7vo\u00b7gel\u00b7ruf"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Kaum zeigt der h&#252;gelrund               ", "tokens": ["Kaum", "zeigt", "der", "h", "&#252;", "gel\u00b7rund"], "token_info": ["word", "word", "word", "word", "XML_entity", "word"], "pos": ["ADV", "VVFIN", "ART", "VVFIN", "$(", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der grauen stunden flucht               ", "tokens": ["Der", "grau\u00b7en", "stun\u00b7den", "flucht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein baum tiefh&#228;ngend sucht               ", "tokens": ["Ein", "baum", "tiefh", "&#228;", "ngend", "sucht"], "token_info": ["word", "word", "word", "XML_entity", "word", "word"], "pos": ["ART", "ADV", "ADJD", "$(", "ADJD", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Nach halmen &#252;berm grund             ", "tokens": ["Nach", "hal\u00b7men", "&#252;", "berm", "grund"], "token_info": ["word", "word", "XML_entity", "word", "word"], "pos": ["APPR", "ADJA", "$(", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Schon taucht die w&#252;stenei               ", "tokens": ["Schon", "taucht", "die", "w", "&#252;", "ste\u00b7nei"], "token_info": ["word", "word", "word", "word", "XML_entity", "word"], "pos": ["ADV", "VVFIN", "ART", "NE", "$(", "XY"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zur&#252;ck zum dunklen schacht               ", "tokens": ["Zur", "&#252;", "ck", "zum", "dunk\u00b7len", "schacht"], "token_info": ["word", "XML_entity", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ein ton von qual und nacht               ", "tokens": ["Ein", "ton", "von", "qual", "und", "nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bricht wie ein lezter schrei.             ", "tokens": ["Bricht", "wie", "ein", "lez\u00b7ter", "schrei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}