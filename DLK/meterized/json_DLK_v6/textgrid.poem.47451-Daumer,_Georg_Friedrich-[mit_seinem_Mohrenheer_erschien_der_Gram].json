{"textgrid.poem.47451": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[mit seinem Mohrenheer erschien der Gram]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit seinem Mohrenheer erschien der Gram,", "tokens": ["Mit", "sei\u00b7nem", "Moh\u00b7ren\u00b7heer", "er\u00b7schien", "der", "Gram", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Blut vergie\u00dfend grausam und verrucht;", "tokens": ["Mein", "Blut", "ver\u00b7gie\u00b7\u00dfend", "grau\u00b7sam", "und", "ver\u00b7rucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein lichtgeborner, wei\u00dfer Engel kam", "tokens": ["Ein", "licht\u00b7ge\u00b7bor\u00b7ner", ",", "wei\u00b7\u00dfer", "En\u00b7gel", "kam"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und schlug den Unhold pl\u00f6tzlich in die Flucht.", "tokens": ["Und", "schlug", "den", "Un\u00b7hold", "pl\u00f6tz\u00b7lich", "in", "die", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}