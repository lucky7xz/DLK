{"textgrid.poem.65929": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Die G\u00f6ttin beim Volk", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O bleib beim Volk, geliebte Aphrodite!", "tokens": ["O", "bleib", "beim", "Volk", ",", "ge\u00b7lieb\u00b7te", "A\u00b7phro\u00b7di\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur Vorstadt und Geh\u00f6ften schickst du Tauben:", "tokens": ["Zur", "Vor\u00b7stadt", "und", "Ge\u00b7h\u00f6f\u00b7ten", "schickst", "du", "Tau\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn stadtw\u00e4rts Wagen mit Berauschten stauben,", "tokens": ["Wenn", "stadt\u00b7w\u00e4rts", "Wa\u00b7gen", "mit", "Be\u00b7rauschten", "stau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bef\u00fcgst du sie in freudvolle Gebiete.", "tokens": ["Be\u00b7f\u00fcgst", "du", "sie", "in", "freud\u00b7vol\u00b7le", "Ge\u00b7bie\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Du bietest M\u00e4dchen an, auf schnelle Miete,", "tokens": ["Du", "bie\u00b7test", "M\u00e4d\u00b7chen", "an", ",", "auf", "schnel\u00b7le", "Mie\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bes\u00e4nftigst St\u00fcrmische in dichten Lauben,", "tokens": ["Be\u00b7s\u00e4nf\u00b7tigst", "St\u00fcr\u00b7mi\u00b7sche", "in", "dich\u00b7ten", "Lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sich Verwegne keine Jungfrau rauben:", "tokens": ["Da\u00df", "sich", "Ver\u00b7weg\u00b7ne", "kei\u00b7ne", "Jung\u00b7frau", "rau\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erfreut sei, wer vor Schenkels\u00e4ulen kniete!", "tokens": ["Er\u00b7freut", "sei", ",", "wer", "vor", "Schen\u00b7kel\u00b7s\u00e4u\u00b7len", "knie\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PWS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du bist beim Volk: drum lebt es nun zufrieden.", "tokens": ["Du", "bist", "beim", "Volk", ":", "drum", "lebt", "es", "nun", "zu\u00b7frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$.", "PAV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer staunte nicht, wie du dein Reich erweiterst,", "tokens": ["Wer", "staun\u00b7te", "nicht", ",", "wie", "du", "dein", "Reich", "er\u00b7wei\u00b7terst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In allen Gauen ist dir Gunst beschieden!", "tokens": ["In", "al\u00b7len", "Gau\u00b7en", "ist", "dir", "Gunst", "be\u00b7schie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Geliebte G\u00f6ttin, da du uns erheiterst,", "tokens": ["Ge\u00b7lieb\u00b7te", "G\u00f6t\u00b7tin", ",", "da", "du", "uns", "er\u00b7hei\u00b7terst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entschweb mir nicht, verweile sanft hienieden:", "tokens": ["Ent\u00b7schweb", "mir", "nicht", ",", "ver\u00b7wei\u00b7le", "sanft", "hien\u00b7ie\u00b7den", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$,", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Herbeigeschiffte, bleib, da\u00df du nicht scheiterst!", "tokens": ["Her\u00b7bei\u00b7ge\u00b7schiff\u00b7te", ",", "bleib", ",", "da\u00df", "du", "nicht", "schei\u00b7terst", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}