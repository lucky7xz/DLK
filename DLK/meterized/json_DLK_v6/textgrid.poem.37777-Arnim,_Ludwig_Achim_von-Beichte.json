{"textgrid.poem.37777": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Beichte", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sonntag winkt mit stillen Blicken", "tokens": ["Der", "Sonn\u00b7tag", "winkt", "mit", "stil\u00b7len", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schm\u00fcckt ein jedes Blumenbeet,", "tokens": ["Und", "schm\u00fcckt", "ein", "je\u00b7des", "Blu\u00b7men\u00b7beet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der G\u00e4rtner will ein Str\u00e4u\u00dflein pfl\u00fccken,", "tokens": ["Der", "G\u00e4rt\u00b7ner", "will", "ein", "Str\u00e4u\u00df\u00b7lein", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil seine Frau zur Kirche geht.", "tokens": ["Weil", "sei\u00b7ne", "Frau", "zur", "Kir\u00b7che", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und kann sich immer nicht entschlie\u00dfen,", "tokens": ["Und", "kann", "sich", "im\u00b7mer", "nicht", "ent\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo er sein Messer brauchen soll,", "tokens": ["Wo", "er", "sein", "Mes\u00b7ser", "brau\u00b7chen", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Blumen sich im Thau noch k\u00fcssen", "tokens": ["Die", "Blu\u00b7men", "sich", "im", "Thau", "noch", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "APPRART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Herz am Herzen h\u00e4ngt so voll.", "tokens": ["Und", "Herz", "am", "Her\u00b7zen", "h\u00e4ngt", "so", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da kommt sein junges Weib gegangen,", "tokens": ["Da", "kommt", "sein", "jun\u00b7ges", "Weib", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr schwarz Gebetbuch in der Hand,", "tokens": ["Ihr", "schwarz", "Ge\u00b7bet\u00b7buch", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Blick gesenkt im frommen Bangen,", "tokens": ["Ihr", "Blick", "ge\u00b7senkt", "im", "from\u00b7men", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Laube hat sie sich gewandt;", "tokens": ["Zur", "Lau\u00b7be", "hat", "sie", "sich", "ge\u00b7wandt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie heimlich gl\u00fcht die Geisblattlaube,", "tokens": ["Wie", "heim\u00b7lich", "gl\u00fcht", "die", "Geis\u00b7blatt\u00b7lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Schatten ist ein duftig Bad,", "tokens": ["Ihr", "Schat\u00b7ten", "ist", "ein", "duf\u00b7tig", "Bad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJD", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und drinnen girrt die Turteltaube", "tokens": ["Und", "drin\u00b7nen", "girrt", "die", "Tur\u00b7tel\u00b7tau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Nelken gl\u00e4nzen an dem Pfad.", "tokens": ["Und", "Nel\u00b7ken", "gl\u00e4n\u00b7zen", "an", "dem", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da spricht die Frau mit bangen Sorgen:", "tokens": ["Da", "spricht", "die", "Frau", "mit", "ban\u00b7gen", "Sor\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vergessen ist die S\u00fcndenschuld,", "tokens": ["Ver\u00b7ges\u00b7sen", "ist", "die", "S\u00fcn\u00b7den\u00b7schuld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was wollt ich beichten heute Morgen,", "tokens": ["Was", "wollt", "ich", "beich\u00b7ten", "heu\u00b7te", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach Gott, hab nur mit mir Geduld.", "tokens": ["Ach", "Gott", ",", "hab", "nur", "mit", "mir", "Ge\u00b7duld", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "VAFIN", "ADV", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach h\u00e4tte ich nur eine Stunde,", "tokens": ["Ach", "h\u00e4t\u00b7te", "ich", "nur", "ei\u00b7ne", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mir fielen wieder S\u00fcnden ein,", "tokens": ["Mir", "fie\u00b7len", "wie\u00b7der", "S\u00fcn\u00b7den", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Aus welchem b\u00f6sen S\u00fcndengrunde", "tokens": ["Aus", "wel\u00b7chem", "b\u00f6\u00b7sen", "S\u00fcn\u00b7den\u00b7grun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mag ich wohl so verge\u00dflich sein.", "tokens": ["Mag", "ich", "wohl", "so", "ver\u00b7ge\u00df\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der G\u00e4rtner hat sich nicht verstecket,", "tokens": ["Der", "G\u00e4rt\u00b7ner", "hat", "sich", "nicht", "ver\u00b7ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch ist er nicht von ihr gesehn,", "tokens": ["Doch", "ist", "er", "nicht", "von", "ihr", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Reben haben ihn gedecket,", "tokens": ["Die", "Re\u00b7ben", "ha\u00b7ben", "ihn", "ge\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er staunet still, wie sie so sch\u00f6n;", "tokens": ["Er", "stau\u00b7net", "still", ",", "wie", "sie", "so", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PWAV", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es kniet sein Weib am B\u00e4nklein nieder", "tokens": ["Es", "kniet", "sein", "Weib", "am", "B\u00e4n\u00b7klein", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und deckt das holde Angesicht,", "tokens": ["Und", "deckt", "das", "hol\u00b7de", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und steht dann auf und saget wieder:", "tokens": ["Und", "steht", "dann", "auf", "und", "sa\u00b7get", "wie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was ich ges\u00fcndigt, wei\u00df ich nicht.", "tokens": ["Was", "ich", "ge\u00b7s\u00fcn\u00b7digt", ",", "wei\u00df", "ich", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Mann will eben zu ihr springen,", "tokens": ["Der", "Mann", "will", "e\u00b7ben", "zu", "ihr", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ihr in Kraft von Lieb und Lust,", "tokens": ["Und", "ihr", "in", "Kraft", "von", "Lieb", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vergebung f\u00fcr die S\u00fcnde bringen,", "tokens": ["Ver\u00b7ge\u00b7bung", "f\u00fcr", "die", "S\u00fcn\u00b7de", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ihrem Herzen unbewu\u00dft,", "tokens": ["Die", "ih\u00b7rem", "Her\u00b7zen", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da h\u00f6rt er eine Harfe klingen,", "tokens": ["Da", "h\u00f6rt", "er", "ei\u00b7ne", "Har\u00b7fe", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sieht eine Frau mit gr\u00fcnem Hut,", "tokens": ["Sieht", "ei\u00b7ne", "Frau", "mit", "gr\u00fc\u00b7nem", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die ihr will s\u00fc\u00dfe Fr\u00fcchte bringen,", "tokens": ["Die", "ihr", "will", "s\u00fc\u00b7\u00dfe", "Fr\u00fcch\u00b7te", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VMFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Frau sagt wahr und ist ihr gut.", "tokens": ["Die", "Frau", "sagt", "wahr", "und", "ist", "ihr", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie k\u00fc\u00dft die Hand des sch\u00f6nen Weibes", "tokens": ["Sie", "k\u00fc\u00dft", "die", "Hand", "des", "sch\u00f6\u00b7nen", "Wei\u00b7bes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und rufet mit Verwundrung aus;", "tokens": ["Und", "ru\u00b7fet", "mit", "Ver\u00b7wund\u00b7rung", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbdu bist gesegnet Deines Leibes,", "tokens": ["\u00bb", "du", "bist", "ge\u00b7seg\u00b7net", "Dei\u00b7nes", "Lei\u00b7bes", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Segen kommt nun in Dein Haus!\u00ab", "tokens": ["Und", "Se\u00b7gen", "kommt", "nun", "in", "Dein", "Haus", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Besch\u00e4mt will es die Frau nicht glauben,", "tokens": ["Be\u00b7sch\u00e4mt", "will", "es", "die", "Frau", "nicht", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und klagt wie schwer zu Muthe ihr,", "tokens": ["Und", "klagt", "wie", "schwer", "zu", "Mu\u00b7the", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ADJD", "APPR", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Tyrola spricht: \u00bbEh reif die Trauben,", "tokens": ["Ty\u00b7ro\u00b7la", "spricht", ":", "\u00bb", "Eh", "reif", "die", "Trau\u00b7ben", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "$(", "NN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die jetzt so hart, dann glaubst Du mir.\u00ab", "tokens": ["Die", "jetzt", "so", "hart", ",", "dann", "glaubst", "Du", "mir", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ihr glaubt die Frau und heil'ge Blicke", "tokens": ["Ihr", "glaubt", "die", "Frau", "und", "heil'\u00b7ge", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Perlen sie umkr\u00e4nzen sch\u00f6n,", "tokens": ["Wie", "Per\u00b7len", "sie", "um\u00b7kr\u00e4n\u00b7zen", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tyrola singt von ihrem Gl\u00fccke", "tokens": ["Ty\u00b7ro\u00b7la", "singt", "von", "ih\u00b7rem", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu ihrer Harfe Vollget\u00f6n;", "tokens": ["Zu", "ih\u00b7rer", "Har\u00b7fe", "Voll\u00b7ge\u00b7t\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was sie gedr\u00fcckt war keine S\u00fcnde,", "tokens": ["Was", "sie", "ge\u00b7dr\u00fcckt", "war", "kei\u00b7ne", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es war die ungewohnte Lust,", "tokens": ["Es", "war", "die", "un\u00b7ge\u00b7wohn\u00b7te", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df sie den Dank zu Gott verk\u00fcnde,", "tokens": ["Da\u00df", "sie", "den", "Dank", "zu", "Gott", "ver\u00b7k\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Erhebt Gesang die freud'ge Brust.", "tokens": ["Er\u00b7hebt", "Ge\u00b7sang", "die", "freu\u00b7d'\u00b7ge", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "In wessen Herz die S\u00fcnde schweiget,", "tokens": ["In", "wes\u00b7sen", "Herz", "die", "S\u00fcn\u00b7de", "schwei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da klingt des Herren Lobgesang,", "tokens": ["Da", "klingt", "des", "Her\u00b7ren", "Lob\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Dasein sich so freundlich zeiget,", "tokens": ["Das", "Da\u00b7sein", "sich", "so", "freund\u00b7lich", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn neue Hoffnung es durchdrang,", "tokens": ["Wenn", "neu\u00b7e", "Hoff\u00b7nung", "es", "durch\u00b7drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie fleht, da\u00df sie der Herr durchdringe", "tokens": ["Sie", "fleht", ",", "da\u00df", "sie", "der", "Herr", "durch\u00b7drin\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit seines Geistes Gegenwart,", "tokens": ["Mit", "sei\u00b7nes", "Geis\u00b7tes", "Ge\u00b7gen\u00b7wart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df fr\u00fch ihr Kind den Geist empfinge,", "tokens": ["Da\u00df", "fr\u00fch", "ihr", "Kind", "den", "Geist", "emp\u00b7fin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn es noch bildsam, rein und zart.", "tokens": ["Wenn", "es", "noch", "bild\u00b7sam", ",", "rein", "und", "zart", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Da kann der G\u00e4rtner sich nicht halten,", "tokens": ["Da", "kann", "der", "G\u00e4rt\u00b7ner", "sich", "nicht", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er stimmt in's fromme Lied mit ein,", "tokens": ["Er", "stimmt", "in's", "from\u00b7me", "Lied", "mit", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "APPR", "ART", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mu\u00df die H\u00e4nde betend falten:", "tokens": ["Und", "mu\u00df", "die", "H\u00e4n\u00b7de", "be\u00b7tend", "fal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So mu\u00df sich eine Kirche weihn!", "tokens": ["So", "mu\u00df", "sich", "ei\u00b7ne", "Kir\u00b7che", "weihn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und er gelobt, an dieser Stelle,", "tokens": ["Und", "er", "ge\u00b7lobt", ",", "an", "die\u00b7ser", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zum Angedenken dieser Gunst,", "tokens": ["Zum", "An\u00b7ge\u00b7den\u00b7ken", "die\u00b7ser", "Gunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Will er erbauen die Kapelle", "tokens": ["Will", "er", "er\u00b7bau\u00b7en", "die", "Ka\u00b7pel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit hocherfahrner Bildner Kunst.", "tokens": ["Mit", "ho\