s": ["Den", "Ar\u00b7men", "gieb", "zum", "Freu\u00b7den\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df Gott, der Herr, Dein Kind bewahret", "tokens": ["Da\u00df", "Gott", ",", "der", "Herr", ",", "Dein", "Kind", "be\u00b7wah\u00b7ret"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "ART", "NN", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und f\u00fchrt es leicht zum Sonnenstrahl.\u00ab", "tokens": ["Und", "f\u00fchrt", "es", "leicht", "zum", "Son\u00b7nen\u00b7strahl", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}