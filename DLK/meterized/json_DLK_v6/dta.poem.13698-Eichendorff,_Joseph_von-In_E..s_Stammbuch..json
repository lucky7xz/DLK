{"dta.poem.13698": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "In E..s Stammbuch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "In klaren Ebenmaa\u00dfen sch\u00f6n gefugt", "tokens": ["In", "kla\u00b7ren", "E\u00b7ben\u00b7maa\u00b7\u00dfen", "sch\u00f6n", "ge\u00b7fugt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gleich dem Pallaste freundlich sich erhebend,", "tokens": ["Gleich", "dem", "Pal\u00b7las\u00b7te", "freund\u00b7lich", "sich", "er\u00b7he\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Stark wie die Burg, die von dem Feld dort lugt,", "tokens": ["Stark", "wie", "die", "Burg", ",", "die", "von", "dem", "Feld", "dort", "lugt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "In ernster H\u00f6h der alten Freiheit lebend,", "tokens": ["In", "erns\u00b7ter", "H\u00f6h", "der", "al\u00b7ten", "Frei\u00b7heit", "le\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gleich jenem Thurm stets nach dem H\u00f6chsten strebend,", "tokens": ["Gleich", "je\u00b7nem", "Thurm", "stets", "nach", "dem", "H\u00f6chs\u00b7ten", "stre\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Schlo\u00df, Burg und was da irdisch, \u00fcberfl\u00fcgelnd \u2014", "tokens": ["Schlo\u00df", ",", "Burg", "und", "was", "da", "ir\u00b7disch", ",", "\u00fc\u00b7berf\u00b7l\u00fc\u00b7gelnd"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "PWS", "ADV", "ADJD", "$,", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dabei, still wie die See dort, im Gem\u00fcth", "tokens": ["Da\u00b7bei", ",", "still", "wie", "die", "See", "dort", ",", "im", "Ge\u00b7m\u00fcth"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "$,", "ADJD", "KOKOM", "ART", "NN", "ADV", "$,", "APPRART", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Des Himmels Blau und was auf Erden bl\u00fcht,", "tokens": ["Des", "Him\u00b7mels", "Blau", "und", "was", "auf", "Er\u00b7den", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "PWS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "In frommer Klarheit ewig heiter spiegelnd;", "tokens": ["In", "from\u00b7mer", "Klar\u00b7heit", "e\u00b7wig", "hei\u00b7ter", "spie\u00b7gelnd", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Vor allem dann fern \u00fcber Strom und Land", "tokens": ["Vor", "al\u00b7lem", "dann", "fern", "\u00fc\u00b7ber", "Strom", "und", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "ADV", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Den alten Freunden treulich zugewandt!", "tokens": ["Den", "al\u00b7ten", "Freun\u00b7den", "treu\u00b7lich", "zu\u00b7ge\u00b7wandt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}