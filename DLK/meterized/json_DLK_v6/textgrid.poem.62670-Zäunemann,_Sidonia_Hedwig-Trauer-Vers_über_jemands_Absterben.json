{"textgrid.poem.62670": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Trauer-Vers \u00fcber jemands Absterben", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun danket alle Gott,", "tokens": ["Nun", "dan\u00b7ket", "al\u00b7le", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Weil dieser Mann erblichen,", "tokens": ["Weil", "die\u00b7ser", "Mann", "er\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der uns mit vielem Spott", "tokens": ["Der", "uns", "mit", "vie\u00b7lem", "Spott"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PIS", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und L\u00e4stern \u00fcberstrichen.", "tokens": ["Und", "L\u00e4s\u00b7tern", "\u00fc\u00b7bers\u00b7tri\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nun ist der Feinde Rott", "tokens": ["Nun", "ist", "der", "Fein\u00b7de", "Rott"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Geringer, samt der Quaal,", "tokens": ["Ge\u00b7rin\u00b7ger", ",", "samt", "der", "Qua\u00b7al", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Drum sing ich noch einmahl:", "tokens": ["Drum", "sing", "ich", "noch", "ein\u00b7mahl", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Nun danket alle Gott.", "tokens": ["Nun", "dan\u00b7ket", "al\u00b7le", "Gott", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}