{"dta.poem.7842": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "Das zerrissene Wiesenthal.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.85", "nl:0.14"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Florian gieng/ in den Tagen/", "tokens": ["Flo\u00b7ri\u00b7an", "gieng", "/", "in", "den", "Ta\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$(", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "durch das gr\u00fcne Pegnitzthal/", "tokens": ["durch", "das", "gr\u00fc\u00b7ne", "Peg\u00b7nitz\u00b7thal", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "als ihr Zelt im Wiesensaal", "tokens": ["als", "ihr", "Zelt", "im", "Wie\u00b7sen\u00b7saal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Flora wieder aufgeschlagen/", "tokens": ["Flo\u00b7ra", "wie\u00b7der", "auf\u00b7ge\u00b7schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "und ihr Buhle Zefyrus", "tokens": ["und", "ihr", "Buh\u00b7le", "Ze\u00b7fy\u00b7rus"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NE"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "ihr nun gabe Ku\u00df \u00fcm Ku\u00df.", "tokens": ["ihr", "nun", "ga\u00b7be", "Ku\u00df", "\u00fcm", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich will (sprach er) nicht beneiden", "tokens": ["Ich", "will", "(", "sprach", "er", ")", "nicht", "be\u00b7nei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "$(", "VVFIN", "PPER", "$(", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "deine Wollust/ G\u00f6ttlichs Paar!", "tokens": ["dei\u00b7ne", "Wol\u00b7lust", "/", "G\u00f6tt\u00b7lichs", "Paar", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber ich/ mu\u00df dieses Jahr", "tokens": ["A\u00b7ber", "ich", "/", "mu\u00df", "die\u00b7ses", "Jahr"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$(", "VMFIN", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "diese s\u00fcsse Freuden meiden:", "tokens": ["die\u00b7se", "s\u00fcs\u00b7se", "Freu\u00b7den", "mei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sylvien ihr Rosenmund/", "tokens": ["Syl\u00b7vi\u00b7en", "ihr", "Ro\u00b7sen\u00b7mund", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "will mir nimmer seyn vergunt.", "tokens": ["will", "mir", "nim\u00b7mer", "seyn", "ver\u00b7gunt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VAINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Seufzend gieng er fort am Rande", "tokens": ["Seuf\u00b7zend", "gieng", "er", "fort", "am", "Ran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ach! die sch\u00f6ne Sch\u00e4ferey", "tokens": ["ach", "!", "die", "sch\u00f6\u00b7ne", "Sch\u00e4\u00b7fe\u00b7rey"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "war nun eine W\u00fcsteney/", "tokens": ["war", "nun", "ei\u00b7ne", "W\u00fcs\u00b7te\u00b7ney", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "die er halb zerrissen fande", "tokens": ["die", "er", "halb", "zer\u00b7ris\u00b7sen", "fan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRELS", "PPER", "ADJD", "VVPP", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "von der angeschwemmten Flut/", "tokens": ["von", "der", "an\u00b7ge\u00b7schwemm\u00b7ten", "Flut", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "von der n\u00e4chsten Wasserwut.", "tokens": ["von", "der", "n\u00e4chs\u00b7ten", "Was\u00b7ser\u00b7wut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wo sonst (sagt\u2019 er) gienge grasen", "tokens": ["Wo", "sonst", "(", "sagt'", "er", ")", "gien\u00b7ge", "gra\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "$(", "VVFIN", "PPER", "$(", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "unser krauses Wollenvieh;", "tokens": ["un\u00b7ser", "krau\u00b7ses", "Wol\u00b7len\u00b7vieh", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wo die Sonne in der fr\u00fch", "tokens": ["wo", "die", "Son\u00b7ne", "in", "der", "fr\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Perlen leckte von den Rasen:", "tokens": ["Per\u00b7len", "leck\u00b7te", "von", "den", "Ra\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "da begr\u00e4bt itz und der Sand", "tokens": ["da", "be\u00b7gr\u00e4bt", "itz", "und", "der", "Sand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "KON", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "das gemahlte Blumenland.", "tokens": ["das", "ge\u00b7mahl\u00b7te", "Blu\u00b7men\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo zuvor die Flut gedrehet", "tokens": ["Wo", "zu\u00b7vor", "die", "Flut", "ge\u00b7dre\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ein bemostes Wasserrad/", "tokens": ["ein", "be\u00b7mos\u00b7tes", "Was\u00b7ser\u00b7rad", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "das die Wiesen f\u00fchrt\u2019 ins Bad/", "tokens": ["das", "die", "Wie\u00b7sen", "f\u00fchrt'", "ins", "Bad", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "APPRART", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "und den frischen Klee ges\u00e4et:", "tokens": ["und", "den", "fri\u00b7schen", "Klee", "ge\u00b7s\u00e4et", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "da steht itzt/ im nassen Rock/", "tokens": ["da", "steht", "itzt", "/", "im", "nas\u00b7sen", "Rock", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$(", "APPRART", "ADJA", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "ein unn\u00fctzer halber Stock.", "tokens": ["ein", "un\u00b7n\u00fct\u00b7zer", "hal\u00b7ber", "Stock", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Unter jenen Kirschenb\u00e4umen/", "tokens": ["Un\u00b7ter", "je\u00b7nen", "Kir\u00b7schen\u00b7b\u00e4u\u00b7men", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "sa\u00df ich oft in k\u00fchler Ruh.", "tokens": ["sa\u00df", "ich", "oft", "in", "k\u00fch\u00b7ler", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "dorten th\u00e4t ich/ was ich thu.", "tokens": ["dor\u00b7ten", "th\u00e4t", "ich", "/", "was", "ich", "thu", "."], "token_info": ["word", 