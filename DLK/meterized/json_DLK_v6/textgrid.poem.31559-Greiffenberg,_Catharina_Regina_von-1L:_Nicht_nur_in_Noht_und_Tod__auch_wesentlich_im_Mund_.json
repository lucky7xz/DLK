{"textgrid.poem.31559": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht nur in Noht und Tod / auch wesentlich im Mund /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht nur in Noht und Tod / auch wesentlich im Mund /", "tokens": ["Nicht", "nur", "in", "Noht", "und", "Tod", "/", "auch", "we\u00b7sent\u00b7lich", "im", "Mund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "KON", "NN", "$(", "ADV", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "gibt sich mein Liebster mir / mit S\u00fcssigkeit zuf\u00fchlen.", "tokens": ["gibt", "sich", "mein", "Liebs\u00b7ter", "mir", "/", "mit", "S\u00fcs\u00b7sig\u00b7keit", "zu\u00b7f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "PPER", "$(", "APPR", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Sein Lieb-erhitztes Blut solt meine Herz-Hitz stillen.", "tokens": ["Sein", "Lie\u00b7ber\u00b7hitz\u00b7tes", "Blut", "solt", "mei\u00b7ne", "Her\u00b7zHitz", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich k\u00fcss' / und iss' ihn gar vor Lieb' in meinen Schlund.", "tokens": ["Ich", "k\u00fcss'", "/", "und", "iss'", "ihn", "gar", "vor", "Lieb'", "in", "mei\u00b7nen", "Schlund", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "VVIMP", "PPER", "ADV", "APPR", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nicht n\u00e4her er sich ja mit mir verm\u00e4hlen kund.", "tokens": ["Nicht", "n\u00e4\u00b7her", "er", "sich", "ja", "mit", "mir", "ver\u00b7m\u00e4h\u00b7len", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PPER", "PRF", "ADV", "APPR", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er gibt sein eignes Herz / mir meine Gier zu stillen:", "tokens": ["Er", "gibt", "sein", "eig\u00b7nes", "Herz", "/", "mir", "mei\u00b7ne", "Gier", "zu", "stil\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$(", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "vermacht sich uns selbselbst in seinem letzten Willen /", "tokens": ["ver\u00b7macht", "sich", "uns", "selb\u00b7selbst", "in", "sei\u00b7nem", "letz\u00b7ten", "Wil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "der auch sein Erster war noch vor der Erden Grund.", "tokens": ["der", "auch", "sein", "Ers\u00b7ter", "war", "noch", "vor", "der", "Er\u00b7den", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VAFIN", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach schick ein Flammen-Heer / die Andacht zuentz\u00fcnden.", "tokens": ["Ach", "schick", "ein", "Flam\u00b7men\u00b7Heer", "/", "die", "An\u00b7dacht", "zu\u00b7ent\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ART", "NN", "$(", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Herz-Bereiter komm / dein Geist mach dir die Bahn:", "tokens": ["Der", "Her\u00b7zBe\u00b7rei\u00b7ter", "komm", "/", "dein", "Geist", "mach", "dir", "die", "Bahn", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "er schick die Tugenden / als Seelen-Zier / voran /", "tokens": ["er", "schick", "die", "Tu\u00b7gen\u00b7den", "/", "als", "See\u00b7len\u00b7Zier", "/", "vo\u00b7ran", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "$(", "KOUS", "NN", "$(", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "auf da\u00df du kanst dein Haus rein und bereitet finden.", "tokens": ["auf", "da\u00df", "du", "kanst", "dein", "Haus", "rein", "und", "be\u00b7rei\u00b7tet", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VMFIN", "PPOSAT", "NN", "ADJD", "KON", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Gott / der du dich mir gibst! gib / da\u00df ich w\u00fcrdiglich /", "tokens": ["Gott", "/", "der", "du", "dich", "mir", "gibst", "!", "gib", "/", "da\u00df", "ich", "w\u00fcr\u00b7dig\u00b7lich", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PRELS", "PPER", "PRF", "PPER", "VVFIN", "$.", "VVIMP", "$(", "KOUS", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "und da\u00df mit deiner Gnad' ich k\u00f6nn' entfahen dich.", "tokens": ["und", "da\u00df", "mit", "dei\u00b7ner", "Gnad'", "ich", "k\u00f6nn'", "ent\u00b7fa\u00b7hen", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN", "PPER", "VMFIN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}