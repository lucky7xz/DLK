{"textgrid.poem.31494": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: HeRR Jesu! meine Seel / die vor schon ganz dein eigen", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "HeRR Jesu! meine Seel / die vor schon ganz dein eigen", "tokens": ["HeRR", "Je\u00b7su", "!", "mei\u00b7ne", "Seel", "/", "die", "vor", "schon", "ganz", "dein", "ei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$.", "PPOSAT", "NN", "$(", "ART", "APPR", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "samt aller Kr\u00e4fften Krafft / schenk' ich aufs neue dir.", "tokens": ["samt", "al\u00b7ler", "Kr\u00e4ff\u00b7ten", "Krafft", "/", "schenk'", "ich", "aufs", "neu\u00b7e", "dir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$(", "VVFIN", "PPER", "APPRART", "ADJA", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ich halte nicht vor mein / was dir nicht dient an mir.", "tokens": ["ich", "hal\u00b7te", "nicht", "vor", "mein", "/", "was", "dir", "nicht", "dient", "an", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "$(", "PWS", "PPER", "PTKNEG", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wollst deine Gnaden Str\u00f6m' in meine Glieder neigen /", "tokens": ["wollst", "dei\u00b7ne", "Gna\u00b7den", "Str\u00f6m'", "in", "mei\u00b7ne", "Glie\u00b7der", "nei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "der Allmacht Wunder-Pracht / in meiner Schwachheit zeigen.", "tokens": ["der", "All\u00b7macht", "Wun\u00b7der\u00b7Pracht", "/", "in", "mei\u00b7ner", "Schwach\u00b7heit", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ach allerf\u00fcller! f\u00fcll die Wunder-Ehr-Begier!", "tokens": ["Ach", "al\u00b7ler\u00b7f\u00fcl\u00b7ler", "!", "f\u00fcll", "die", "Wun\u00b7der\u00b7Ehr\u00b7Be\u00b7gier", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "dein' Herrschungs Kron alsdann erlangt mehr Strahlen Zier /", "tokens": ["dein'", "Herr\u00b7schungs", "Kron", "als\u00b7dann", "er\u00b7langt", "mehr", "Strah\u00b7len", "Zier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "ADV", "VVFIN", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "wann durch der Stummen Mund du machst die Redner schweigen.", "tokens": ["wann", "durch", "der", "Stum\u00b7men", "Mund", "du", "machst", "die", "Red\u00b7ner", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "PPER", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "La\u00df deinen wei\u00dfen Geist mein Lippen-Lauten schlagen /", "tokens": ["La\u00df", "dei\u00b7nen", "wei\u00b7\u00dfen", "Geist", "mein", "Lip\u00b7pen\u00b7Lau\u00b7ten", "schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "da\u00df solcher wollaut auch die Leuenherzen zwing.", "tokens": ["da\u00df", "sol\u00b7cher", "wol\u00b7laut", "auch", "die", "Leu\u00b7en\u00b7her\u00b7zen", "zwing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "la\u00df' aus der Irthums H\u00f6l die Glaubens Todten jagen /", "tokens": ["la\u00df'", "aus", "der", "Ir\u00b7thums", "H\u00f6l", "die", "Glau\u00b7bens", "Tod\u00b7ten", "ja\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "NN", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "da\u00df das dreyk\u00f6pfig Thier um seine Beut ich bring.", "tokens": ["da\u00df", "das", "drey\u00b7k\u00f6p\u00b7fig", "Thier", "um", "sei\u00b7ne", "Beut", "ich", "bring", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "CARD", "NN", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es ist mein ganzes Thun / zu deiner Ehr gericht /", "tokens": ["Es", "ist", "mein", "gan\u00b7zes", "Thun", "/", "zu", "dei\u00b7ner", "Ehr", "ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$(", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "und ist mir \u00e4usserst Ernst / nicht ein erdichts Gedicht.", "tokens": ["und", "ist", "mir", "\u00e4us\u00b7serst", "Ernst", "/", "nicht", "ein", "er\u00b7dichts", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "NE", "$(", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}