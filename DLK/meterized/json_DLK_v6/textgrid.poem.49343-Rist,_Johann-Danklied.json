{"textgrid.poem.49343": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Danklied", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie gro\u00df ist deine Lieb', o Herr,", "tokens": ["Wie", "gro\u00df", "ist", "dei\u00b7ne", "Lieb'", ",", "o", "Herr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du freundlicher, du g\u00fctiger,", "tokens": ["Du", "freund\u00b7li\u00b7cher", ",", "du", "g\u00fc\u00b7ti\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer kan dir gnugsam danken?", "tokens": ["Wer", "kan", "dir", "gnug\u00b7sam", "dan\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich bin vol Ungerechtigkeit,", "tokens": ["Ich", "bin", "vol", "Un\u00b7ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch liebest du zur jeden Zeit", "tokens": ["Noch", "lie\u00b7best", "du", "zur", "je\u00b7den", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mich S\u00fcndigen, mich Kranken,", "tokens": ["Mich", "S\u00fcn\u00b7di\u00b7gen", ",", "mich", "Kran\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da gleichwol meine Missethat", "tokens": ["Da", "gleich\u00b7wol", "mei\u00b7ne", "Mis\u00b7se\u00b7that"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Erschrecklich dich beleidigt hat!", "tokens": ["Er\u00b7schreck\u00b7lich", "dich", "be\u00b7lei\u00b7digt", "hat", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Du hast uns deinen werten Sohn,", "tokens": ["Du", "hast", "uns", "dei\u00b7nen", "wer\u00b7ten", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Das Licht der Welt, den Gnadenthron,", "tokens": ["Das", "Licht", "der", "Welt", ",", "den", "Gna\u00b7dent\u00b7hron", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So v\u00e4terlich gegeben", "tokens": ["So", "v\u00e4\u00b7ter\u00b7lich", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und dieses gro\u00dfe Sacrament,", "tokens": ["Und", "die\u00b7ses", "gro\u00b7\u00dfe", "Sa\u00b7cra\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Das man des Herren Nachtmal nennt,", "tokens": ["Das", "man", "des", "Her\u00b7ren", "Nacht\u00b7mal", "nennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Geschenket uns daneben,", "tokens": ["Ge\u00b7schen\u00b7ket", "uns", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Auf da\u00df es solt ein Merkmal sein,", "tokens": ["Auf", "da\u00df", "es", "solt", "ein", "Merk\u00b7mal", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Da\u00df alles unser ist, was dein.", "tokens": ["Da\u00df", "al\u00b7les", "un\u00b7ser", "ist", ",", "was", "dein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VAFIN", "$,", "PRELS", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Vernunft zwar kan es nimmer recht", "tokens": ["Ver\u00b7nunft", "zwar", "kan", "es", "nim\u00b7mer", "recht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VMFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Begreifen, noch die Zunge schlecht", "tokens": ["Be\u00b7grei\u00b7fen", ",", "noch", "die", "Zun\u00b7ge", "schlecht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Die\u00df gro\u00dfe Werk erz\u00e4hlen,", "tokens": ["Die\u00df", "gro\u00b7\u00dfe", "Werk", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Doch bitt ich, nimm itz gn\u00e4dig an,", "tokens": ["Doch", "bitt", "ich", ",", "nimm", "itz", "gn\u00e4\u00b7dig", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "VVIMP", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Die\u00df Kleine, das ich geben kan,", "tokens": ["Die\u00df", "Klei\u00b7ne", ",", "das", "ich", "ge\u00b7ben", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "La\u00df meinen Wunsch nicht fehlen;", "tokens": ["La\u00df", "mei\u00b7nen", "Wunsch", "nicht", "feh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Hilf, da\u00df ich dir mein Leben lang", "tokens": ["Hilf", ",", "da\u00df", "ich", "dir", "mein", "Le\u00b7ben", "lang"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "PPER", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Von Herzen sage Lob und Dank.", "tokens": ["Von", "Her\u00b7zen", "sa\u00b7ge", "Lob", "und", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Du hast so wol erquicket mich,", "tokens": ["Du", "hast", "so", "wol", "er\u00b7quic\u00b7ket", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Daf\u00fcr preis' ich dich ewiglich;", "tokens": ["Da\u00b7f\u00fcr", "preis'", "ich", "dich", "e\u00b7wig\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.27": {"text": "Ach, st\u00e4rke meinen Glauben!", "tokens": ["Ach", ",", "st\u00e4r\u00b7ke", "mei\u00b7nen", "Glau\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "La\u00df auch die Liebe feurig sein,", "tokens": ["La\u00df", "auch", "die", "Lie\u00b7be", "feu\u00b7rig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Verh\u00fcte, da\u00df noch Not, noch Pein", "tokens": ["Ver\u00b7h\u00fc\u00b7te", ",", "da\u00df", "noch", "Not", ",", "noch", "Pein"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "KOUS", "ADV", "NN", "$,", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Mir die Gedult wegrauben.", "tokens": ["Mir", "die", "Ge\u00b7dult", "we\u00b7grau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVIZU", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.31": {"text": "Herr, gib mir ein gehorsams Her,", "tokens": ["Herr", ",", "gib", "mir", "ein", "ge\u00b7hor\u00b7sams", "Her", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Das sich nicht kehre hinterw\u00e4rts.", "tokens": ["Das", "sich", "nicht", "keh\u00b7re", "hin\u00b7ter\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "PTKNEG", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Verleihe, da\u00df ich meine S\u00fcnd',", "tokens": ["Ver\u00b7lei\u00b7he", ",", "da\u00df", "ich", "mei\u00b7ne", "S\u00fcnd'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Als welch' ich stets in mir noch find',", "tokens": ["Als", "welch'", "ich", "stets", "in", "mir", "noch", "find'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAT", "PPER", "ADV", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Herzinniglich bereue,", "tokens": ["Her\u00b7zin\u00b7nig\u00b7lich", "be\u00b7re\u00b7ue", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.36": {"text": "Da\u00df ich der Lieb' und Sanftmut vol", "tokens": ["Da\u00df", "ich", "der", "Lieb'", "und", "Sanft\u00b7mut", "vol"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Stets wandlen m\u00f6g', und herzlich wol", "tokens": ["Stets", "wand\u00b7len", "m\u00f6g'", ",", "und", "herz\u00b7lich", "wol"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVINF", "VMFIN", "$,", "KON", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "In Jesu mich erfreue,", "tokens": ["In", "Je\u00b7su", "mich", "er\u00b7freu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Und wenn mich gar nichts tr\u00f6sten kan,", "tokens": ["Und", "wenn", "mich", "gar", "nichts", "tr\u00f6s\u00b7ten", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "So tritt du selbst mit Trost heran.", "tokens": ["So", "tritt", "du", "selbst", "mit", "Trost", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Herr, habe du zu Tag und Nacht", "tokens": ["Herr", ",", "ha\u00b7be", "du", "zu", "Tag", "und", "Nacht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VAFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Auf mein Gebet und Seufzen Acht,", "tokens": ["Auf", "mein", "Ge\u00b7bet", "und", "Seuf\u00b7zen", "Acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "La\u00df mich die S\u00fcnde meiden;", "tokens": ["La\u00df", "mich", "die", "S\u00fcn\u00b7de", "mei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Beh\u00fcte mich f\u00fcr Satans List,", "tokens": ["Be\u00b7h\u00fc\u00b7te", "mich", "f\u00fcr", "Sa\u00b7tans", "List", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Der mir so sehr geh\u00e4ssig ist,", "tokens": ["Der", "mir", "so", "sehr", "ge\u00b7h\u00e4s\u00b7sig", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Ja, mich von dir wil scheiden,", "tokens": ["Ja", ",", "mich", "von", "dir", "wil", "schei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRF", "APPR", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Halt' ihn, auch selber mich, im Zaum,", "tokens": ["Halt'", "ihn", ",", "auch", "sel\u00b7ber", "mich", ",", "im", "Zaum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADV", "ADV", "PPER", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Da\u00df ich ihm lasse keinen Raum.", "tokens": ["Da\u00df", "ich", "ihm", "las\u00b7se", "kei\u00b7nen", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Verleih, o Vater, da\u00df allein", "tokens": ["Ver\u00b7leih", ",", "o", "Va\u00b7ter", ",", "da\u00df", "al\u00b7lein"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "FM", "NN", "$,", "KOUS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Mein Schutz und Wohnung m\u00f6ge sein", "tokens": ["Mein", "Schutz", "und", "Woh\u00b7nung", "m\u00f6\u00b7ge", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "VMFIN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "In Jesu Blut und Wunden;", "tokens": ["In", "Je\u00b7su", "Blut", "und", "Wun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "Hilf, da\u00df sein Leiden, Angst und Tod", "tokens": ["Hilf", ",", "da\u00df", "sein", "Lei\u00b7den", ",", "Angst", "und", "Tod"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPOSAT", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Von mir in meiner letzten Not", "tokens": ["Von", "mir", "in", "mei\u00b7ner", "letz\u00b7ten", "Not"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "So kr\u00e4ftig werd' empfunden,", "tokens": ["So", "kr\u00e4f\u00b7tig", "werd'", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.55": {"text": "Da\u00df ich aus diesem Jammerthal", "tokens": ["Da\u00df", "ich", "aus", "die\u00b7sem", "Jam\u00b7mer\u00b7thal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "Bald fahr' in deinen Freudensal.", "tokens": ["Bald", "fahr'", "in", "dei\u00b7nen", "Freu\u00b7den\u00b7sal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "Dem Vater sei Lob, Ehr' und Preis,", "tokens": ["Dem", "Va\u00b7ter", "sei", "Lob", ",", "Ehr'", "und", "Preis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "Und dir, Herr Jesu, gleicherweis,", "tokens": ["Und", "dir", ",", "Herr", "Je\u00b7su", ",", "glei\u00b7cher\u00b7weis", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "NE", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.59": {"text": "Als auch dem Geist der Gnaden;", "tokens": ["Als", "auch", "dem", "Geist", "der", "Gna\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.60": {"text": "Du heilige Dreifaltigkeit,", "tokens": ["Du", "hei\u00b7li\u00b7ge", "Drei\u00b7fal\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.61": {"text": "Verh\u00fcte, da\u00df in dieser Zeit", "tokens": ["Ver\u00b7h\u00fc\u00b7te", ",", "da\u00df", "in", "die\u00b7ser", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.62": {"text": "Kein Feind mir m\u00fcge schaden,", "tokens": ["Kein", "Feind", "mir", "m\u00fc\u00b7ge", "scha\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.63": {"text": "Drauf f\u00fchre mich aus dieser Welt", "tokens": ["Drauf", "f\u00fch\u00b7re", "mich", "aus", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PRF", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.64": {"text": "Zum Himmel, wenn es dir gef\u00e4llt.", "tokens": ["Zum", "Him\u00b7mel", ",", "wenn", "es", "dir", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}