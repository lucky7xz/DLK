{"dta.poem.526": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf ihre Jugend.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Die Rose riechet sch\u00f6n/ die noch nicht auffgebl\u00fcht/", "tokens": ["Die", "Ro\u00b7se", "rie\u00b7chet", "sch\u00f6n", "/", "die", "noch", "nicht", "auff\u00b7ge\u00b7bl\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$(", "ART", "ADV", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die jungen Tauben sind zur Kost die allerbesten/", "tokens": ["Die", "jun\u00b7gen", "Tau\u00b7ben", "sind", "zur", "Kost", "die", "al\u00b7ler\u00b7bes\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPRART", "NN", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Am liebsten auf dem Tisch man junge M\u00f6hren sieht/", "tokens": ["Am", "liebs\u00b7ten", "auf", "dem", "Tisch", "man", "jun\u00b7ge", "M\u00f6h\u00b7ren", "sieht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "ART", "NN", "PIS", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die jungen H\u00fcner sind ein Labsahl denen G\u00e4sten.", "tokens": ["Die", "jun\u00b7gen", "H\u00fc\u00b7ner", "sind", "ein", "Lab\u00b7sahl", "de\u00b7nen", "G\u00e4s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "PDS", "NN", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Der frische Pomerantz den alten \u00fcbersteigt/", "tokens": ["Der", "fri\u00b7sche", "Po\u00b7me\u00b7rantz", "den", "al\u00b7ten", "\u00fc\u00b7bers\u00b7teigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Am ", "tokens": ["Am"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Ein junges Pferd mehr Muth als wie ein altes zeigt/", "tokens": ["Ein", "jun\u00b7ges", "Pferd", "mehr", "Muth", "als", "wie", "ein", "al\u00b7tes", "zeigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "KOUS", "KOKOM", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der alten Krafft zerf\u00e4llt bey heurigen Citronen.", "tokens": ["Der", "al\u00b7ten", "Krafft", "zer\u00b7f\u00e4llt", "bey", "heu\u00b7ri\u00b7gen", "Cit\u00b7ro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Geht nun dem Alterthum die frische Jugend f\u00fcr?", "tokens": ["Geht", "nun", "dem", "Al\u00b7ter\u00b7thum", "die", "fri\u00b7sche", "Ju\u00b7gend", "f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So kanst du ", "tokens": ["So", "kanst", "du"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Der M\u00e4dgen Jugend reicht die herrlichste ", "tokens": ["Der", "M\u00e4d\u00b7gen", "Ju\u00b7gend", "reicht", "die", "herr\u00b7lichs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Weil eine junge Schoo\u00df Lust und Vergn\u00fcgung kr\u00f6hnen.", "tokens": ["Weil", "ei\u00b7ne", "jun\u00b7ge", "Schoo\u00df", "Lust", "und", "Ver\u00b7gn\u00fc\u00b7gung", "kr\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}