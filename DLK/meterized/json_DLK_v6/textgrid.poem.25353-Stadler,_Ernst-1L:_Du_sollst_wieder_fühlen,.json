{"textgrid.poem.25353": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du sollst wieder f\u00fchlen,", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sollst wieder f\u00fchlen,", "tokens": ["Du", "sollst", "wie\u00b7der", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "da\u00df alle stark und jungen Kr\u00e4fte dich umschweifen,", "tokens": ["da\u00df", "al\u00b7le", "stark", "und", "jun\u00b7gen", "Kr\u00e4f\u00b7te", "dich", "um\u00b7schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "KON", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df nichts stille steht,", "tokens": ["Da\u00df", "nichts", "stil\u00b7le", "steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJA", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "da\u00df Gold des Himmels um dich kreist", "tokens": ["da\u00df", "Gold", "des", "Him\u00b7mels", "um", "dich", "kreist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "NN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und Sterne dich umwehn,", "tokens": ["und", "Ster\u00b7ne", "dich", "um\u00b7wehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df Sonne und Abend niederf\u00e4llt", "tokens": ["Da\u00df", "Son\u00b7ne", "und", "A\u00b7bend", "nie\u00b7der\u00b7f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "und Winde \u00fcber blaue Meeressteppen gehn,", "tokens": ["und", "Win\u00b7de", "\u00fc\u00b7ber", "blau\u00b7e", "Mee\u00b7ress\u00b7tep\u00b7pen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du sollst durch Sturz und Bruch der Wolken", "tokens": ["Du", "sollst", "durch", "Sturz", "und", "Bruch", "der", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "wilder in die hellgest\u00fcrmten Himmel greifen.", "tokens": ["wil\u00b7der", "in", "die", "hell\u00b7ge\u00b7st\u00fcrm\u00b7ten", "Him\u00b7mel", "grei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Meintest du,", "tokens": ["Mein\u00b7test", "du", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "die sanften Hafenlichter k\u00f6nnten deine Segel halten,", "tokens": ["die", "sanf\u00b7ten", "Ha\u00b7fen\u00b7lich\u00b7ter", "k\u00f6nn\u00b7ten", "dei\u00b7ne", "Se\u00b7gel", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Die sich bl\u00e4hen wie junge Br\u00fcste,", "tokens": ["Die", "sich", "bl\u00e4\u00b7hen", "wie", "jun\u00b7ge", "Br\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVINF", "KOKOM", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "ungeb\u00e4rdig dr\u00e4ngend unter d\u00fcnner Linnen Hut?", "tokens": ["un\u00b7ge\u00b7b\u00e4r\u00b7dig", "dr\u00e4n\u00b7gend", "un\u00b7ter", "d\u00fcn\u00b7ner", "Lin\u00b7nen", "Hut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "Horch, im Dunkel, geisterhafte Liebesstimme,", "tokens": ["Horch", ",", "im", "Dun\u00b7kel", ",", "geis\u00b7ter\u00b7haf\u00b7te", "Lie\u00b7bess\u00b7tim\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "str\u00f6mt und lallt dein Blut \u2013", "tokens": ["str\u00f6mt", "und", "lallt", "dein", "Blut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und du wolltest deine H\u00e4nde m\u00fcde zur Ergebung falten?", "tokens": ["Und", "du", "woll\u00b7test", "dei\u00b7ne", "H\u00e4n\u00b7de", "m\u00fc\u00b7de", "zur", "Er\u00b7ge\u00b7bung", "fal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "NN", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "--+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "F\u00fchle:", "tokens": ["F\u00fch\u00b7le", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Licht und Regen deines Traumes sind zergangen,", "tokens": ["Licht", "und", "Re\u00b7gen", "dei\u00b7nes", "Trau\u00b7mes", "sind", "zer\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Welt ist aufgerissen,", "tokens": ["Welt", "ist", "auf\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Abgrund zieht und Himmelsbl\u00e4ue loht,", "tokens": ["Ab\u00b7grund", "zieht", "und", "Him\u00b7mels\u00b7bl\u00e4u\u00b7e", "loht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Sturm ist los", "tokens": ["Sturm", "ist", "los"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "und weht dein Herz in schmelzendes Umfangen,", "tokens": ["und", "weht", "dein", "Herz", "in", "schmel\u00b7zen\u00b7des", "Um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Bis es grenzenlos zusammensinkt", "tokens": ["Bis", "es", "gren\u00b7zen\u00b7los", "zu\u00b7sam\u00b7men\u00b7sinkt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "im Schrei von Lust und Gl\u00fcck und Tod.", "tokens": ["im", "Schrei", "von", "Lust", "und", "Gl\u00fcck", "und", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}