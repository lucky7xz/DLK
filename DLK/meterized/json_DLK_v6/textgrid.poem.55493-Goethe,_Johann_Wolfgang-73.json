{"textgrid.poem.55493": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "73", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Gestalten gehn vor\u00fcber,", "tokens": ["Die", "Ge\u00b7stal\u00b7ten", "gehn", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Masken scheinen sie zu sein;", "tokens": ["Mas\u00b7ken", "schei\u00b7nen", "sie", "zu", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch sie sind uns beiden lieber,", "tokens": ["Doch", "sie", "sind", "uns", "bei\u00b7den", "lie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "PIAT", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uns vom edelsten Verein.", "tokens": ["Uns", "vom", "e\u00b7dels\u00b7ten", "Ver\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+---+", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Sie sind wahr, denn wohl vernommen", "tokens": ["Sie", "sind", "wahr", ",", "denn", "wohl", "ver\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "KON", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haben wir sie selbst gef\u00fchlt", "tokens": ["Ha\u00b7ben", "wir", "sie", "selbst", "ge\u00b7f\u00fchlt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, wie es vielleicht gekommen,", "tokens": ["Und", ",", "wie", "es", "viel\u00b7leicht", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie zum Teile mitgespielt.", "tokens": ["Sie", "zum", "Tei\u00b7le", "mit\u00b7ge\u00b7spielt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Denke nun zum vielten Male,", "tokens": ["Den\u00b7ke", "nun", "zum", "viel\u00b7ten", "Ma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was, nach sternenheller Nacht,", "tokens": ["Was", ",", "nach", "ster\u00b7nen\u00b7hel\u00b7ler", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Holder Tag im hohen Saale", "tokens": ["Hol\u00b7der", "Tag", "im", "ho\u00b7hen", "Saa\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wunderf\u00e4ltig dargebracht.", "tokens": ["Wun\u00b7der\u00b7f\u00e4l\u00b7tig", "dar\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}