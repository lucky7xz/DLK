{"textgrid.poem.62567": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "And\u00e4chtige Weyhnachts-Gedanken", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willkommen allerliebstes Kind", "tokens": ["Will\u00b7kom\u00b7men", "al\u00b7ler\u00b7liebs\u00b7tes", "Kind"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Herr der Potentaten!", "tokens": ["Du", "Herr", "der", "Po\u00b7ten\u00b7ta\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O Gl\u00fcck! da\u00df man dich jetzo findt.", "tokens": ["O", "Gl\u00fcck", "!", "da\u00df", "man", "dich", "jet\u00b7zo", "findt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "KOUS", "PIS", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo bist du hingerathen?", "tokens": ["Wo", "bist", "du", "hin\u00b7ge\u00b7ra\u00b7then", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du k\u00f6mmst auf dieses Jammerthal,", "tokens": ["Du", "k\u00f6mmst", "auf", "die\u00b7ses", "Jam\u00b7mer\u00b7thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verl\u00e4\u00dft den sch\u00f6nen Himmels-Saal,", "tokens": ["Ver\u00b7l\u00e4\u00dft", "den", "sch\u00f6\u00b7nen", "Him\u00b7mels\u00b7Saal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Erwehlst der Menschen Orden,", "tokens": ["Er\u00b7wehlst", "der", "Men\u00b7schen", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und bist ein Kind geworden!", "tokens": ["Und", "bist", "ein", "Kind", "ge\u00b7wor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Jedoch ich wundre mich nicht mehr", "tokens": ["Je\u00b7doch", "ich", "wund\u00b7re", "mich", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df du den Thron verlassen,", "tokens": ["Da\u00df", "du", "den", "Thron", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df du die allergr\u00f6\u00dfte Ehr,", "tokens": ["Da\u00df", "du", "die", "al\u00b7ler\u00b7gr\u00f6\u00df\u00b7te", "Ehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf eine Zeit wilst hassen,", "tokens": ["Auf", "ei\u00b7ne", "Zeit", "wilst", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Herr, deine Liebe hats gemacht,", "tokens": ["Herr", ",", "dei\u00b7ne", "Lie\u00b7be", "hats", "ge\u00b7macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die hat dich auf die Welt gebracht,", "tokens": ["Die", "hat", "dich", "auf", "die", "Welt", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Uns dadurch von den Ketten", "tokens": ["Uns", "da\u00b7durch", "von", "den", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PAV", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Der Finsterni\u00df zu retten.", "tokens": ["Der", "Fins\u00b7ter\u00b7ni\u00df", "zu", "ret\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Allein, o Herr der ganzen Welt", "tokens": ["Al\u00b7lein", ",", "o", "Herr", "der", "gan\u00b7zen", "Welt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "FM", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und aller Herrlichkeiten!", "tokens": ["Und", "al\u00b7ler", "Herr\u00b7lich\u00b7kei\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie hast du dich so sehr verstellt,", "tokens": ["Wie", "hast", "du", "dich", "so", "sehr", "ver\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Legst allen Pracht zur Seiten;", "tokens": ["Legst", "al\u00b7len", "Pracht", "zur", "Sei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nimmst einen Stall zur Wohnung ein,", "tokens": ["Nimmst", "ei\u00b7nen", "Stall", "zur", "Woh\u00b7nung", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo Ochsen und wo Esel seyn,", "tokens": ["Wo", "Och\u00b7sen", "und", "wo", "E\u00b7sel", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "PWAV", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du wilst an statt der Wiegen,", "tokens": ["Du", "wilst", "an", "statt", "der", "Wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In einer Krippe liegen.", "tokens": ["In", "ei\u00b7ner", "Krip\u00b7pe", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Um meinet willen bist du arm,", "tokens": ["Um", "mei\u00b7net", "wil\u00b7len", "bist", "du", "arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "VVFIN", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sehr gering erschienen;", "tokens": ["Und", "sehr", "ge\u00b7ring", "er\u00b7schie\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch deine K\u00e4lte macht mich warm,", "tokens": ["Doch", "dei\u00b7ne", "K\u00e4l\u00b7te", "macht", "mich", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du k\u00f6mmst, nun mir zu dienen.", "tokens": ["Du", "k\u00f6mmst", ",", "nun", "mir", "zu", "die\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dein Elend machet mich recht gro\u00df,", "tokens": ["Dein", "E\u00b7lend", "ma\u00b7chet", "mich", "recht", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erwirbet mir des Vaters Schoo\u00df:", "tokens": ["Er\u00b7wir\u00b7bet", "mir", "des", "Va\u00b7ters", "Schoo\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Weil deine Niedrigkeiten,", "tokens": ["Weil", "dei\u00b7ne", "Nied\u00b7rig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mir lauter Gl\u00fcck bereiten.", "tokens": ["Mir", "lau\u00b7ter", "Gl\u00fcck", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie? soll das Stroh dein Lager seyn?", "tokens": ["Wie", "?", "soll", "das", "Stroh", "dein", "La\u00b7ger", "seyn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VMFIN", "ART", "NN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df dir den Tausch belieben,", "tokens": ["La\u00df", "dir", "den", "Tausch", "be\u00b7lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Komm nimm davor mein Herze ein,", "tokens": ["Komm", "nimm", "da\u00b7vor", "mein", "Her\u00b7ze", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PAV", "PPOSAT", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich hab es dir verschrieben.", "tokens": ["Ich", "hab", "es", "dir", "ver\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach! schenke mir dein Angesicht,", "tokens": ["Ach", "!", "schen\u00b7ke", "mir", "dein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zieh ein, verschm\u00e4h mein Bitten nicht,", "tokens": ["Zieh", "ein", ",", "ver\u00b7schm\u00e4h", "mein", "Bit\u00b7ten", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bleib doch nicht drausen stehen;", "tokens": ["Bleib", "doch", "nicht", "drau\u00b7sen", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Ich mu\u00df dich bey mir sehen.", "tokens": ["Ich", "mu\u00df", "dich", "bey", "mir", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Glaube soll die Windel seyn,", "tokens": ["Der", "Glau\u00b7be", "soll", "die", "Win\u00b7del", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Darein will ich dich winden,", "tokens": ["Da\u00b7rein", "will", "ich", "dich", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es soll der b\u00f6se Heuchel-Schein", "tokens": ["Es", "soll", "der", "b\u00f6\u00b7se", "Heu\u00b7chel\u00b7Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich nicht mit mir verbinden.", "tokens": ["Sich", "nicht", "mit", "mir", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nimm an mein Herz, bereit es zu,", "tokens": ["Nimm", "an", "mein", "Herz", ",", "be\u00b7reit", "es", "zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf da\u00df du deine sanfte Ruh", "tokens": ["Auf", "da\u00df", "du", "dei\u00b7ne", "sanf\u00b7te", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Darinnen m\u00f6gest halten,", "tokens": ["Da\u00b7rin\u00b7nen", "m\u00f6\u00b7gest", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und nur nach Willen schalten.", "tokens": ["Und", "nur", "nach", "Wil\u00b7len", "schal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O Jesu! allerliebstes Kind,", "tokens": ["O", "Je\u00b7su", "!", "al\u00b7ler\u00b7liebs\u00b7tes", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erh\u00f6r mein herzlich Bethen,", "tokens": ["Er\u00b7h\u00f6r", "mein", "herz\u00b7lich", "Be\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gieb, da\u00df ich Gnade vor dir find,", "tokens": ["Gieb", ",", "da\u00df", "ich", "Gna\u00b7de", "vor", "dir", "find", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Geist wird mich vertreten.", "tokens": ["Dein", "Geist", "wird", "mich", "ver\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Erh\u00f6re doch mein heises Flehn,", "tokens": ["Er\u00b7h\u00f6\u00b7re", "doch", "mein", "hei\u00b7ses", "Flehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und la\u00df es alsobald geschehn,", "tokens": ["Und", "la\u00df", "es", "al\u00b7so\u00b7bald", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So hab ich, was mir n\u00fctzet,", "tokens": ["So", "hab", "ich", ",", "was", "mir", "n\u00fct\u00b7zet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und vor den Tod besch\u00fctzet.", "tokens": ["Und", "vor", "den", "Tod", "be\u00b7sch\u00fct\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}