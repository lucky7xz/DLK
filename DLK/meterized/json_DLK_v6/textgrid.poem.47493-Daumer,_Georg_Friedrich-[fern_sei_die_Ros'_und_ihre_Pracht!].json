{"textgrid.poem.47493": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[fern sei die Ros' und ihre Pracht!]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fern sei die Ros' und ihre Pracht!", "tokens": ["Fern", "sei", "die", "Ros'", "und", "ih\u00b7re", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Rosenm\u00fcndchen ist genug;", "tokens": ["Ein", "Ro\u00b7sen\u00b7m\u00fcnd\u00b7chen", "ist", "ge\u00b7nug", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fern sei der Bund mit Gl\u00fcck und Macht!", "tokens": ["Fern", "sei", "der", "Bund", "mit", "Gl\u00fcck", "und", "Macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Koseb\u00fcndchen ist genug.", "tokens": ["Ein", "Ko\u00b7se\u00b7b\u00fcnd\u00b7chen", "ist", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach schicke mich nach Eden nicht", "tokens": ["Ach", "schi\u00b7cke", "mich", "nach", "E\u00b7den", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PRF", "APPR", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus deiner Kammer, s\u00fc\u00dfes Kind!", "tokens": ["Aus", "dei\u00b7ner", "Kam\u00b7mer", ",", "s\u00fc\u00b7\u00dfes", "Kind", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein R\u00e4umchen hier, zu s\u00fcndigen", "tokens": ["Ein", "R\u00e4um\u00b7chen", "hier", ",", "zu", "s\u00fcn\u00b7di\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein trautes S\u00fcndchen, ist genug.", "tokens": ["Ein", "trau\u00b7tes", "S\u00fcnd\u00b7chen", ",", "ist", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mir wurde kein erhabner Geist,", "tokens": ["Mir", "wur\u00b7de", "kein", "er\u00b7hab\u00b7ner", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den gro\u00dfer Dinge Fund begl\u00fcckt;", "tokens": ["Den", "gro\u00b7\u00dfer", "Din\u00b7ge", "Fund", "be\u00b7gl\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch find' ich einer Schenke Th\u00fcr',", "tokens": ["Doch", "find'", "ich", "ei\u00b7ner", "Schen\u00b7ke", "Th\u00fcr'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O dieses F\u00fcndchen ist genug.", "tokens": ["O", "die\u00b7ses", "F\u00fcnd\u00b7chen", "ist", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zu ewig ist die Ewigkeit", "tokens": ["Zu", "e\u00b7wig", "ist", "die", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr meine schwache Phantasie;", "tokens": ["F\u00fcr", "mei\u00b7ne", "schwa\u00b7che", "Phan\u00b7ta\u00b7sie", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An einer warmen Wogebrust", "tokens": ["An", "ei\u00b7ner", "war\u00b7men", "Wo\u00b7ge\u00b7brust"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Wonnest\u00fcndchen ist genug.", "tokens": ["Ein", "Won\u00b7ne\u00b7st\u00fcnd\u00b7chen", "ist", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Aus welchem Grunde bin ich hier?", "tokens": ["Aus", "wel\u00b7chem", "Grun\u00b7de", "bin", "ich", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sei's ohne weitern, sei es nur,", "tokens": ["Sei's", "oh\u00b7ne", "wei\u00b7tern", ",", "sei", "es", "nur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "$,", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu k\u00fcssen deiner F\u00fc\u00dfe Staub!", "tokens": ["Zu", "k\u00fcs\u00b7sen", "dei\u00b7ner", "F\u00fc\u00b7\u00dfe", "Staub", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn dieses Gr\u00fcndchen ist genug.", "tokens": ["Denn", "die\u00b7ses", "Gr\u00fcnd\u00b7chen", "ist", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}