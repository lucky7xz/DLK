{"dta.poem.19204": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "51.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Erd' in ihrem Bau ist gar nicht eingerichtet", "tokens": ["Die", "Erd'", "in", "ih\u00b7rem", "Bau", "ist", "gar", "nicht", "ein\u00b7ge\u00b7rich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "ADV", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Paradis zu seyn, wie Fantasie es dichtet,", "tokens": ["Ein", "Pa\u00b7ra\u00b7dis", "zu", "seyn", ",", "wie", "Fan\u00b7ta\u00b7sie", "es", "dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VAINF", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ganz ungeeignet, von Unsterblichen bewohnt", "tokens": ["Ganz", "un\u00b7ge\u00b7eig\u00b7net", ",", "von", "U\u00b7nsterb\u00b7li\u00b7chen", "be\u00b7wohnt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "APPR", "NN", "VVPP"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu seyn, da \u00fcberall auf ihr Zerst\u00f6rung thront,", "tokens": ["Zu", "seyn", ",", "da", "\u00fc\u00b7be\u00b7rall", "auf", "ihr", "Zer\u00b7st\u00f6\u00b7rung", "thront", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VAINF", "$,", "KOUS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ihr ew'ges Leben nur auf ewiger Zerst\u00f6rung,", "tokens": ["Ihr", "ew'\u00b7ges", "Le\u00b7ben", "nur", "auf", "e\u00b7wi\u00b7ger", "Zer\u00b7st\u00f6\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr ew'ger Friede ruht auf ewiger Emp\u00f6rung;", "tokens": ["Ihr", "ew'\u00b7ger", "Frie\u00b7de", "ruht", "auf", "e\u00b7wi\u00b7ger", "Em\u00b7p\u00f6\u00b7rung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Darum unsterblich kann nur das Geschlecht allein", "tokens": ["Da\u00b7rum", "uns\u00b7terb\u00b7lich", "kann", "nur", "das", "Ge\u00b7schlecht", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADJD", "VMFIN", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Anbeginn, wie es noch ist, gewesen seyn,", "tokens": ["Von", "An\u00b7be\u00b7ginn", ",", "wie", "es", "noch", "ist", ",", "ge\u00b7we\u00b7sen", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$,", "VAPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Kein Einzelner, der, selbst unsterblich, das Verderben", "tokens": ["Kein", "Ein\u00b7zel\u00b7ner", ",", "der", ",", "selbst", "uns\u00b7terb\u00b7lich", ",", "das", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PRELS", "$,", "ADV", "ADJD", "$,", "ART", "NN"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Nicht h\u00e4tte k\u00f6nnen sehn ringsum, ohn' auch zu sterben,", "tokens": ["Nicht", "h\u00e4t\u00b7te", "k\u00f6n\u00b7nen", "sehn", "ring\u00b7sum", ",", "ohn'", "auch", "zu", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "VMFIN", "VVINF", "ADV", "$,", "KOUI", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}}, "stanza.6": {"line.1": {"text": "Nicht h\u00e4tte k\u00f6nnen sehn die Pflanzen j\u00e4hrlich bl\u00fchn", "tokens": ["Nicht", "h\u00e4t\u00b7te", "k\u00f6n\u00b7nen", "sehn", "die", "Pflan\u00b7zen", "j\u00e4hr\u00b7lich", "bl\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VAFIN", "VMFIN", "VVINF", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und welken, ohne mit in Sehnsucht zu vergl\u00fchn,", "tokens": ["Und", "wel\u00b7ken", ",", "oh\u00b7ne", "mit", "in", "Sehn\u00b7sucht", "zu", "ver\u00b7gl\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "KOUI", "APPR", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Nicht zittern sehn die Erd' und ihre Berge splittern,", "tokens": ["Nicht", "zit\u00b7tern", "sehn", "die", "Erd'", "und", "ih\u00b7re", "Ber\u00b7ge", "split\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VVFIN", "ART", "NN", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ohn' uranf\u00e4nglichem Granit gleich zu verwittern.", "tokens": ["Ohn'", "u\u00b7ran\u00b7f\u00e4ng\u00b7li\u00b7chem", "Gra\u00b7nit", "gleich", "zu", "ver\u00b7wit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-++-+-", "measure": "iambic.hexa.relaxed"}}}}}