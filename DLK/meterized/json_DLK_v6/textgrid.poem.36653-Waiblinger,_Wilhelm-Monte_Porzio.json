{"textgrid.poem.36653": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Monte Porzio", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stehst du auf Tusculums H\u00f6h' in der aufgegrabenen Vorwelt,", "tokens": ["Stehst", "du", "auf", "Tus\u00b7cu\u00b7lums", "H\u00f6h'", "in", "der", "auf\u00b7ge\u00b7gra\u00b7be\u00b7nen", "Vor\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Suchst du in Tr\u00fcmmern und Schutt Cicero's Villa hier auf,", "tokens": ["Suchst", "du", "in", "Tr\u00fcm\u00b7mern", "und", "Schutt", "Ci\u00b7ce\u00b7ro's", "Vil\u00b7la", "hier", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "NE", "NE", "ADV", "PTKVZ", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Dann zum Rebenh\u00fcgel hinab ins lustige D\u00f6rfchen,", "tokens": ["Dann", "zum", "Re\u00b7ben\u00b7h\u00fc\u00b7gel", "hin\u00b7ab", "ins", "lus\u00b7ti\u00b7ge", "D\u00f6rf\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und wie Cicero bald macht dich Freund Bacchus beredt.", "tokens": ["Und", "wie", "Ci\u00b7ce\u00b7ro", "bald", "macht", "dich", "Freund", "Bac\u00b7chus", "be\u00b7redt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "ADV", "VVFIN", "PPER", "NN", "NE", "ADJD", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}