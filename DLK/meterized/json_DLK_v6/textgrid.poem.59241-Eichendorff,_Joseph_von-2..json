{"textgrid.poem.59241": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir sind so tief betr\u00fcbt, wenn wir auch scherzen,", "tokens": ["Wir", "sind", "so", "tief", "be\u00b7tr\u00fcbt", ",", "wenn", "wir", "auch", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$,", "KOUS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die armen Menschen m\u00fchn sich ab und reisen,", "tokens": ["Die", "ar\u00b7men", "Men\u00b7schen", "m\u00fchn", "sich", "ab", "und", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PRF", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Welt zieht ernst und streng in ihren Gleisen,", "tokens": ["Die", "Welt", "zieht", "ernst", "und", "streng", "in", "ih\u00b7ren", "Glei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein feuchter Wind verl\u00f6scht die lust'gen Kerzen.", "tokens": ["Ein", "feuch\u00b7ter", "Wind", "ver\u00b7l\u00f6scht", "die", "lust'\u00b7gen", "Ker\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du hast so sch\u00f6ne Worte tief im Herzen,", "tokens": ["Du", "hast", "so", "sch\u00f6\u00b7ne", "Wor\u00b7te", "tief", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du wei\u00dft so wunderbare, alte Weisen,", "tokens": ["Du", "wei\u00dft", "so", "wun\u00b7der\u00b7ba\u00b7re", ",", "al\u00b7te", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wie die Stern am Firmamente kreisen,", "tokens": ["Und", "wie", "die", "Stern", "am", "Fir\u00b7ma\u00b7men\u00b7te", "krei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ziehn durch die Brust dir ewig Lust und Schmerzen.", "tokens": ["Ziehn", "durch", "die", "Brust", "dir", "e\u00b7wig", "Lust", "und", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So la\u00df dein' Stimme hell im Wald erscheinen!", "tokens": ["So", "la\u00df", "dein'", "Stim\u00b7me", "hell", "im", "Wald", "er\u00b7schei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPOSAT", "NN", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Waldhorn fromm wird auf und nieder wehen,", "tokens": ["Das", "Wald\u00b7horn", "fromm", "wird", "auf", "und", "nie\u00b7der", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "APPR", "KON", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Wasser gehn und einsam Rehe weiden.", "tokens": ["Die", "Was\u00b7ser", "gehn", "und", "ein\u00b7sam", "Re\u00b7he", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wir wollen stille sitzen und nicht weinen,", "tokens": ["Wir", "wol\u00b7len", "stil\u00b7le", "sit\u00b7zen", "und", "nicht", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "VVINF", "KON", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir wollen in den Rhein hinuntersehen,", "tokens": ["Wir", "wol\u00b7len", "in", "den", "Rhein", "hin\u00b7un\u00b7ter\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und, wird es finster, nicht von sammen scheiden.", "tokens": ["Und", ",", "wird", "es", "fins\u00b7ter", ",", "nicht", "von", "sam\u00b7men", "schei\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "PPER", "ADJD", "$,", "PTKNEG", "APPR", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}