{"textgrid.poem.35679": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Trinkt, Br\u00fcder, der Reben", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trinkt, Br\u00fcder, der Reben", "tokens": ["Trinkt", ",", "Br\u00fc\u00b7der", ",", "der", "Re\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Entflammten Saft!", "tokens": ["Ent\u00b7flamm\u00b7ten", "Saft", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Er w\u00fcrzet das Leben,", "tokens": ["Er", "w\u00fcr\u00b7zet", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und schenkt uns Kraft!", "tokens": ["Und", "schenkt", "uns", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die Wassertrinker, die keuchen,", "tokens": ["Die", "Was\u00b7ser\u00b7trin\u00b7ker", ",", "die", "keu\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Sehn wie Gespenster und Leichen;", "tokens": ["Sehn", "wie", "Ge\u00b7spens\u00b7ter", "und", "Lei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Und werden mit m\u00fcrrischem Gram bestraft!", "tokens": ["Und", "wer\u00b7den", "mit", "m\u00fcr\u00b7ri\u00b7schem", "Gram", "be\u00b7straft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Schleicht heute nicht blasser", "tokens": ["Schleicht", "heu\u00b7te", "nicht", "blas\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "PTKNEG", "ADJA"], "meter": "+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Der Mond dahin?", "tokens": ["Der", "Mond", "da\u00b7hin", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Er trank zu viel Wasser;", "tokens": ["Er", "trank", "zu", "viel", "Was\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das bleichet ihn!", "tokens": ["Das", "blei\u00b7chet", "ihn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "H\u00e4tt' er Burgunder zu trinken;", "tokens": ["H\u00e4tt'", "er", "Bur\u00b7gun\u00b7der", "zu", "trin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Er w\u00fcrd' euch trefflicher blinken,", "tokens": ["Er", "w\u00fcrd'", "euch", "treff\u00b7li\u00b7cher", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Er w\u00fcrde wie unsere Wangen gl\u00fchn!", "tokens": ["Er", "w\u00fcr\u00b7de", "wie", "un\u00b7se\u00b7re", "Wan\u00b7gen", "gl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Was quaken die Fr\u00f6sche", "tokens": ["Was", "qua\u00b7ken", "die", "Fr\u00f6\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "In jenem Sumpf?", "tokens": ["In", "je\u00b7nem", "Sumpf", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wird nicht ihr Gew\u00e4sche", "tokens": ["Wird", "nicht", "ihr", "Ge\u00b7w\u00e4\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Vom Wasser dumpf?", "tokens": ["Vom", "Was\u00b7ser", "dumpf", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "La\u00dft sie im Rebensaft schwimmen;", "tokens": ["La\u00dft", "sie", "im", "Re\u00b7ben\u00b7saft", "schwim\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ich schw\u00f6r's, in unsere Stimmen", "tokens": ["Ich", "schw\u00f6r's", ",", "in", "un\u00b7se\u00b7re", "Stim\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "T\u00f6nt gellend dem Bacchus auch ihr Triumph!", "tokens": ["T\u00f6nt", "gel\u00b7lend", "dem", "Bac\u00b7chus", "auch", "ihr", "Tri\u00b7umph", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ART", "NE", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}