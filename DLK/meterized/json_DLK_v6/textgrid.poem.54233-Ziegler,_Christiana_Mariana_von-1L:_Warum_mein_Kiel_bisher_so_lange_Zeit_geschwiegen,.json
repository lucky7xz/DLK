{"textgrid.poem.54233": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Warum mein Kiel bisher so lange Zeit geschwiegen,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum mein Kiel bisher so lange Zeit geschwiegen,", "tokens": ["Wa\u00b7rum", "mein", "Kiel", "bis\u00b7her", "so", "lan\u00b7ge", "Zeit", "ge\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Macht, weil du selber ihm das Schweigen auferlegt.", "tokens": ["Macht", ",", "weil", "du", "sel\u00b7ber", "ihm", "das", "Schwei\u00b7gen", "auf\u00b7er\u00b7legt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "Die Muse scheint bey mir schon halb entselt zu liegen,", "tokens": ["Die", "Mu\u00b7se", "scheint", "bey", "mir", "schon", "halb", "ent\u00b7selt", "zu", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "ADV", "ADJD", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein Scheide-Brief hat ihr ein Schrecken eingepr\u00e4gt.", "tokens": ["Dein", "Schei\u00b7de\u00b7Brief", "hat", "ihr", "ein", "Schre\u00b7cken", "ein\u00b7ge\u00b7pr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du woltest, schriebst du n\u00e4chst, den Helicon verlassen,", "tokens": ["Du", "wol\u00b7test", ",", "schriebst", "du", "n\u00e4chst", ",", "den", "He\u00b7li\u00b7con", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VVFIN", "PPER", "VVFIN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des Schicksals Grausamkeit erzw\u00e4nge solchen Schlu\u00df.", "tokens": ["Des", "Schick\u00b7sals", "Grau\u00b7sam\u00b7keit", "er\u00b7zw\u00e4n\u00b7ge", "sol\u00b7chen", "Schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Geliebte B-- wilst du dich selber hassen?", "tokens": ["Ge\u00b7lieb\u00b7te", "B\u00b7", "wilst", "du", "dich", "sel\u00b7ber", "has\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "TRUNC", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "meter.parsing.error", "measure": "measure.parsing.error"}, "line.8": {"text": "Verbanne Harm und Gram, verjage den Verdru\u00df.", "tokens": ["Ver\u00b7ban\u00b7ne", "Harm", "und", "Gram", ",", "ver\u00b7ja\u00b7ge", "den", "Ver\u00b7dru\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Himmel wird nicht stets mit Blitz und Donner dr\u00e4uen,", "tokens": ["Der", "Him\u00b7mel", "wird", "nicht", "stets", "mit", "Blitz", "und", "Don\u00b7ner", "dr\u00e4u\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich seh den Freuden-Stern bereits von ferne stehn,", "tokens": ["Ich", "seh", "den", "Freu\u00b7den\u00b7S\u00b7tern", "be\u00b7reits", "von", "fer\u00b7ne", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und dieser wird dein Hertz gantz unverhofft erfreuen,", "tokens": ["Und", "die\u00b7ser", "wird", "dein", "Hertz", "gantz", "un\u00b7ver\u00b7hofft", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dein Fu\u00df soll k\u00fcnfftig hin auf Ros- und Nelcken gehn.", "tokens": ["Dein", "Fu\u00df", "soll", "k\u00fcnff\u00b7tig", "hin", "auf", "Ros", "und", "Nel\u00b7cken", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJD", "ADV", "APPR", "TRUNC", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was wird die Welt von dir wohl vor ein Urtheil f\u00e4llen?", "tokens": ["Was", "wird", "die", "Welt", "von", "dir", "wohl", "vor", "ein", "Ur\u00b7theil", "f\u00e4l\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "APPR", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Du weist, sie liebet dich und deine Dichter-Kunst;", "tokens": ["Du", "weist", ",", "sie", "lie\u00b7bet", "dich", "und", "dei\u00b7ne", "Dich\u00b7ter\u00b7Kunst", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "La\u00df dich bey deinen Schmertz zufrieden wieder stellen,", "tokens": ["La\u00df", "dich", "bey", "dei\u00b7nen", "Schmertz", "zu\u00b7frie\u00b7den", "wie\u00b7der", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Vertreibe durch Vernunfft der Schwermuth gifftge Dunst.", "tokens": ["Ver\u00b7trei\u00b7be", "durch", "Ver\u00b7nunfft", "der", "Schwer\u00b7muth", "gifft\u00b7ge", "Dunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der Schlag ist alzu hart, klagst du, der mich betroffen", "tokens": ["Der", "Schlag", "ist", "al\u00b7zu", "hart", ",", "klagst", "du", ",", "der", "mich", "be\u00b7trof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$,", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Erst meiner Augen-Lust, jetzt mein geliebtes Kind,", "tokens": ["Erst", "mei\u00b7ner", "Au\u00b7gen\u00b7Lust", ",", "jetzt", "mein", "ge\u00b7lieb\u00b7tes", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Bey letztern kan ich zwar noch auf Genesen hoffen,", "tokens": ["Bey", "letz\u00b7tern", "kan", "ich", "zwar", "noch", "auf", "Ge\u00b7ne\u00b7sen", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VMFIN", "PPER", "ADV", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wiewohl mir di\u00df noch nicht die Wunden gantz verbind.", "tokens": ["Wie\u00b7wohl", "mir", "di\u00df", "noch", "nicht", "die", "Wun\u00b7den", "gantz", "ver\u00b7bind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "ADV", "PTKNEG", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Di\u00df ziehet meine Hand vom S\u00e4yten-Spiel zur\u00fccke,", "tokens": ["Di\u00df", "zie\u00b7het", "mei\u00b7ne", "Hand", "vom", "S\u00e4y\u00b7ten\u00b7Spiel", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Sonst hie\u00df Calliope mein liebster Zeitvertreib,", "tokens": ["Sonst", "hie\u00df", "Cal\u00b7li\u00b7o\u00b7pe", "mein", "liebs\u00b7ter", "Zeit\u00b7ver\u00b7treib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Jetzt aber geb ich ihr recht E\u00dfig-saure Blicke,", "tokens": ["Jetzt", "a\u00b7ber", "geb", "ich", "ihr", "recht", "E\u00b7\u00dfig\u00b7sau\u00b7re", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Warum? Ich bin ein kranck und Jammer-volles Weib.", "tokens": ["Wa\u00b7rum", "?", "Ich", "bin", "ein", "kranck", "und", "Jam\u00b7mer\u00b7vol\u00b7les", "Weib", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "VAFIN", "ART", "ADJD", "KON", "NE", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Dein Klagen ist gerecht, wer kan dich widerlegen?", "tokens": ["Dein", "Kla\u00b7gen", "ist", "ge\u00b7recht", ",", "wer", "kan", "dich", "wi\u00b7der\u00b7le\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PWS", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Doch sey auch wiederum zu heilen dich bem\u00fcht;", "tokens": ["Doch", "sey", "auch", "wie\u00b7de\u00b7rum", "zu", "hei\u00b7len", "dich", "be\u00b7m\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "PTKZU", "VVINF", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Was wilst du deinen Schmertz noch l\u00e4nger n\u00e4hren, hegen?", "tokens": ["Was", "wilst", "du", "dei\u00b7nen", "Schmertz", "noch", "l\u00e4n\u00b7ger", "n\u00e4h\u00b7ren", ",", "he\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der als ein M\u00f6rder dich zuletzt gar nieder zieht.", "tokens": ["Der", "als", "ein", "M\u00f6r\u00b7der", "dich", "zu\u00b7letzt", "gar", "nie\u00b7der", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "ART", "NN", "PPER", "ADV", "ADV", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wer kan des Schicksals Schlu\u00df durch Klag- und Thr\u00e4nen zwingen?", "tokens": ["Wer", "kan", "des", "Schick\u00b7sals", "Schlu\u00df", "durch", "Klag", "und", "Thr\u00e4\u00b7nen", "zwin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "NN", "APPR", "TRUNC", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Vielmehr verdoppelt dir der Kumer deine Pein,", "tokens": ["Viel\u00b7mehr", "ver\u00b7dop\u00b7pelt", "dir", "der", "Ku\u00b7mer", "dei\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Auf! und erhohle dich ein m\u00e4nnlich Lied zu singen,", "tokens": ["Auf", "!", "und", "er\u00b7hoh\u00b7le", "dich", "ein", "m\u00e4nn\u00b7lich", "Lied", "zu", "sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "KON", "VVFIN", "PRF", "ART", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Di\u00df wird die Panace vor deine Schwermuth seyn.", "tokens": ["Di\u00df", "wird", "die", "Pa\u00b7na\u00b7ce", "vor", "dei\u00b7ne", "Schwer\u00b7muth", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Zwar scheints, als woltest du nicht mehr an Reime dencken,", "tokens": ["Zwar", "scheints", ",", "als", "wol\u00b7test", "du", "nicht", "mehr", "an", "Rei\u00b7me", "den\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "VMFIN", "PPER", "PTKNEG", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Denn meine Muse heist f\u00fcr dich vielleicht zu schlecht,", "tokens": ["Denn", "mei\u00b7ne", "Mu\u00b7se", "heist", "f\u00fcr", "dich", "viel\u00b7leicht", "zu", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Drum soltst du wohl damit was w\u00fcrdigers beschencken,", "tokens": ["Drum", "soltst", "du", "wohl", "da\u00b7mit", "was", "w\u00fcr\u00b7di\u00b7gers", "be\u00b7schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "PAV", "PWS", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Du hast, ich sag es selbst, in diesen St\u00fccke recht.", "tokens": ["Du", "hast", ",", "ich", "sag", "es", "selbst", ",", "in", "die\u00b7sen", "St\u00fc\u00b7cke", "recht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "APPR", "PDAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Kein schlechtes Epheu-Kraut soll sich zu Cedern wagen,", "tokens": ["Kein", "schlech\u00b7tes", "E\u00b7pheu\u00b7Kraut", "soll", "sich", "zu", "Ce\u00b7dern", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Dieweil es selbigen gar zu ver\u00e4chtlich scheint;", "tokens": ["Die\u00b7weil", "es", "sel\u00b7bi\u00b7gen", "gar", "zu", "ver\u00b7\u00e4cht\u00b7lich", "scheint", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Allein mein Kiel erk\u00fchnt sich nicht dich auszufragen,", "tokens": ["Al\u00b7lein", "mein", "Kiel", "er\u00b7k\u00fchnt", "sich", "nicht", "dich", "aus\u00b7zu\u00b7fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PRF", "PTKNEG", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ob deine Feder es noch gut mit meiner meynt.", "tokens": ["Ob", "dei\u00b7ne", "Fe\u00b7der", "es", "noch", "gut", "mit", "mei\u00b7ner", "meynt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Deswegen la\u00df ich mich nicht in dem Singen schrecken,", "tokens": ["Des\u00b7we\u00b7gen", "la\u00df", "ich", "mich", "nicht", "in", "dem", "Sin\u00b7gen", "schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Wenn mir dein netter Kiel nur was in Prosa schreibt,", "tokens": ["Wenn", "mir", "dein", "net\u00b7ter", "Kiel", "nur", "was", "in", "Pro\u00b7sa", "schreibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "ADV", "PWS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "So will ich doch die Hand darnach mit Freuden strecken,", "tokens": ["So", "will", "ich", "doch", "die", "Hand", "dar\u00b7nach", "mit", "Freu\u00b7den", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "PAV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Ob deiner W\u00f6rter Pracht gleich ungereimet bleibt.", "tokens": ["Ob", "dei\u00b7ner", "W\u00f6r\u00b7ter", "Pracht", "gleich", "un\u00b7ge\u00b7rei\u00b7met", "bleibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Die Sylben werden doch in Ohr und Hertze dringen,", "tokens": ["Die", "Syl\u00b7ben", "wer\u00b7den", "doch", "in", "Ohr", "und", "Hert\u00b7ze", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "NN", "KON", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Ich wei\u00df die S\u00fc\u00dfigkeit erquicket meinen Sinn.", "tokens": ["Ich", "wei\u00df", "die", "S\u00fc\u00b7\u00dfig\u00b7keit", "er\u00b7quic\u00b7ket", "mei\u00b7nen", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Mein Schmaterwerck will dich gar nicht zum Dichten zwingen,", "tokens": ["Mein", "Schma\u00b7ter\u00b7werck", "will", "dich", "gar", "nicht", "zum", "Dich\u00b7ten", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADV", "PTKNEG", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Dieweil ich gegen dir noch gar zu kindisch bin.", "tokens": ["Die\u00b7weil", "ich", "ge\u00b7gen", "dir", "noch", "gar", "zu", "kin\u00b7disch", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "ADV", "PTKA", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Apollo wird nicht leicht vor mich den Lorber winden,", "tokens": ["A\u00b7pol\u00b7lo", "wird", "nicht", "leicht", "vor", "mich", "den", "Lor\u00b7ber", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PTKNEG", "ADJD", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Er sieht mi