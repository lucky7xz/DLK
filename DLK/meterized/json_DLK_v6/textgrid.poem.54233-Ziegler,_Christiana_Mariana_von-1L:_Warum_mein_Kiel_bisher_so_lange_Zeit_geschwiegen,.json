", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "APPR", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Und Ph\u00f6bus lie\u00df schon l\u00e4ngst in alle Welt erschallen,", "tokens": ["Und", "Ph\u00f6\u00b7bus", "lie\u00df", "schon", "l\u00e4ngst", "in", "al\u00b7le", "Welt", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ADV", "ADV", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Was deine kluge Hand vor nette Verse macht.", "tokens": ["Was", "dei\u00b7ne", "klu\u00b7ge", "Hand", "vor", "net\u00b7te", "Ver\u00b7se", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Man wird von deinen Geist und ungemeinen Gaben,", "tokens": ["Man", "wird", "von", "dei\u00b7nen", "Geist", "und", "un\u00b7ge\u00b7mei\u00b7nen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Betrachtet man genau Kunst und Gelehrsamkeit,", "tokens": ["Be\u00b7trach\u00b7tet", "man", "ge\u00b7nau", "Kunst", "und", "Ge\u00b7lehr\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Wohl schwerlich in der Welt mehr solche Damen haben,", "tokens": ["Wohl", "schwer\u00