{"textgrid.poem.34522": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "[im Thal der Freude weiss ich ein stilles Haus]", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Thal der Freude weiss ich ein stilles Haus", "tokens": ["Im", "Thal", "der", "Freu\u00b7de", "weiss", "ich", "ein", "stil\u00b7les", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "am dunklen, bergquellrauschenden Tannenwald.", "tokens": ["am", "dunk\u00b7len", ",", "berg\u00b7quell\u00b7rau\u00b7schen\u00b7den", "Tan\u00b7nen\u00b7wald", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es t\u00f6nt ein Lied durch meine N\u00e4chte,", "tokens": ["Es", "t\u00f6nt", "ein", "Lied", "durch", "mei\u00b7ne", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "das mir im Traume der Quell gesungen.", "tokens": ["das", "mir", "im", "Trau\u00b7me", "der", "Quell", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Im Thal der Freude weiss ich ein stilles Haus.", "tokens": ["Im", "Thal", "der", "Freu\u00b7de", "weiss", "ich", "ein", "stil\u00b7les", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie hiess das Lied? Ich sinne vergebens nur.", "tokens": ["Wie", "hiess", "das", "Lied", "?", "Ich", "sin\u00b7ne", "ver\u00b7ge\u00b7bens", "nur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Fern im Gebirg, durch Felsenspalten", "tokens": ["Fern", "im", "Ge\u00b7birg", ",", "durch", "Fel\u00b7sen\u00b7spal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "APPR", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "rieselt es heimlich \u2013 im Thal der Freude.", "tokens": ["rie\u00b7selt", "es", "heim\u00b7lich", "\u2013", "im", "Thal", "der", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$(", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}