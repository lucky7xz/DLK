{"textgrid.poem.39718": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wild verwirrt sind mir die Sinne;", "genre": "verse", "period": "N.A.", "pub_year": 1805, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wild verwirrt sind mir die Sinne;", "tokens": ["Wild", "ver\u00b7wirrt", "sind", "mir", "die", "Sin\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steig herauf, eh ich beginne,", "tokens": ["Steig", "her\u00b7auf", ",", "eh", "ich", "be\u00b7gin\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich am Schlangenhaar zu greifen,", "tokens": ["Dich", "am", "Schlan\u00b7gen\u00b7haar", "zu", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcthend an den Tag zu schleifen.", "tokens": ["W\u00fct\u00b7hend", "an", "den", "Tag", "zu", "schlei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weh wenn du mit Antwort geizest,", "tokens": ["Weh", "wenn", "du", "mit", "Ant\u00b7wort", "gei\u00b7zest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwer mir so die Seele reizest,", "tokens": ["Schwer", "mir", "so", "die", "See\u00b7le", "rei\u00b7zest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wisse das ich dann dich finde,", "tokens": ["Wis\u00b7se", "das", "ich", "dann", "dich", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "W\u00e4rst du in der H\u00f6llenschlunde.", "tokens": ["W\u00e4rst", "du", "in", "der", "H\u00f6l\u00b7len\u00b7schlun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Peitsche dich mit Flammenruthen,", "tokens": ["Peit\u00b7sche", "dich", "mit", "Flam\u00b7men\u00b7rut\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Bade dich in H\u00f6lengluthen;", "tokens": ["Ba\u00b7de", "dich", "in", "H\u00f6\u00b7len\u00b7glu\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Dich soll die Sommersgluth verzehren", "tokens": ["Dich", "soll", "die", "Som\u00b7mers\u00b7gluth", "ver\u00b7zeh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und Todesangst dein Leben n\u00e4hren.", "tokens": ["Und", "To\u00b7de\u00b7sangst", "dein", "Le\u00b7ben", "n\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Kein Mitleid soll dir Trost gew\u00e4hren,", "tokens": ["Kein", "Mit\u00b7leid", "soll", "dir", "Trost", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Schmerz, Qual erzeugen und geb\u00e4hren,", "tokens": ["Schmerz", ",", "Qual", "er\u00b7zeu\u00b7gen", "und", "ge\u00b7b\u00e4h\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVINF", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Drum steig herauf ehe gewaltig", "tokens": ["Drum", "steig", "her\u00b7auf", "e\u00b7he", "ge\u00b7wal\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "KOUS", "ADJD"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Dich ergreifet meines Zornes", "tokens": ["Dich", "er\u00b7grei\u00b7fet", "mei\u00b7nes", "Zor\u00b7nes"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "M\u00e4chtige Rechte!", "tokens": ["M\u00e4ch\u00b7ti\u00b7ge", "Rech\u00b7te", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Wer ruft so mit m\u00e4chtiger", "tokens": ["Wer", "ruft", "so", "mit", "m\u00e4ch\u00b7ti\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ADJA"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Stimme in den Abgrund,", "tokens": ["Stim\u00b7me", "in", "den", "Ab\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df der Hela", "tokens": ["Da\u00df", "der", "He\u00b7la"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Burgen erbeben", "tokens": ["Bur\u00b7gen", "er\u00b7be\u00b7ben"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und wiederhallen", "tokens": ["Und", "wie\u00b7der\u00b7hal\u00b7len"], "token_info": ["word", "word"], "pos": ["KON", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Die dunklen Kl\u00fcfte", "tokens": ["Die", "dunk\u00b7len", "Kl\u00fcf\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Vom gr\u00e4\u00dflichen Nachhall?", "tokens": ["Vom", "gr\u00e4\u00df\u00b7li\u00b7chen", "Nach\u00b7hall", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}