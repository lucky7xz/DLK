{"textgrid.poem.44738": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Persa", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie seitw\u00e4rts schielend den Kopf er h\u00e4ngt,", "tokens": ["Wie", "seit\u00b7w\u00e4rts", "schie\u00b7lend", "den", "Kopf", "er", "h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aschfarb erbleichen die Wangen;", "tokens": ["Aschfarb", "er\u00b7blei\u00b7chen", "die", "Wan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "So blickt, au\u00dfer einem, der Diebe f\u00e4ngt,", "tokens": ["So", "blickt", ",", "au\u00b7\u00dfer", "ei\u00b7nem", ",", "der", "Die\u00b7be", "f\u00e4ngt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ART", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Nur der Dieb, der selbst wird gefangen.", "tokens": ["Nur", "der", "Dieb", ",", "der", "selbst", "wird", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADV", "VAFIN", "VVPP", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}