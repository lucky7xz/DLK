{"textgrid.poem.39166": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich war entschlummert einst am Rasenb\u00fchle,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich war entschlummert einst am Rasenb\u00fchle,", "tokens": ["Ich", "war", "ent\u00b7schlum\u00b7mert", "einst", "am", "Ra\u00b7sen\u00b7b\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um mich des Lenzes w\u00fcrz'ges Duftgemische", "tokens": ["Um", "mich", "des", "Len\u00b7zes", "w\u00fcrz'\u00b7ges", "Duft\u00b7ge\u00b7mi\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PRF", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und in mir selbst des Lenzes duft'ge K\u00fchle.", "tokens": ["Und", "in", "mir", "selbst", "des", "Len\u00b7zes", "duft'\u00b7ge", "K\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da tr\u00e4umt' ich Liebestr\u00e4ume, zauberische,", "tokens": ["Da", "tr\u00e4umt'", "ich", "Lie\u00b7be\u00b7str\u00e4u\u00b7me", ",", "zau\u00b7be\u00b7ri\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Heldentr\u00e4ume stolz und ungeheuer,", "tokens": ["Und", "Hel\u00b7den\u00b7tr\u00e4u\u00b7me", "stolz", "und", "un\u00b7ge\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Freiheitstr\u00e4ume, mut'ge, jugendfrische.", "tokens": ["Und", "Frei\u00b7heits\u00b7tr\u00e4u\u00b7me", ",", "mut'\u00b7ge", ",", "ju\u00b7gend\u00b7fri\u00b7sche", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "VVFIN", "$,", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch als verglomm des Morgens Purpurfeuer,", "tokens": ["Doch", "als", "ver\u00b7glomm", "des", "Mor\u00b7gens", "Pur\u00b7pur\u00b7feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADJD", "ART", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da war verstummt das buhlerische Kosen,", "tokens": ["Da", "war", "ver\u00b7stummt", "das", "buh\u00b7le\u00b7ri\u00b7sche", "Ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Winter war genaht, der Flockenstreuer.", "tokens": ["Der", "Win\u00b7ter", "war", "ge\u00b7naht", ",", "der", "Flo\u00b7cken\u00b7streu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und um mein Haupt in Windeswirbeltosen", "tokens": ["Und", "um", "mein", "Haupt", "in", "Win\u00b7des\u00b7wir\u00b7bel\u00b7to\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Flog sparsam nur das Schneegelock des Greisen;", "tokens": ["Flog", "spar\u00b7sam", "nur", "das", "Schnee\u00b7ge\u00b7lock", "des", "Grei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich war verwelkt, wie Lenz und Baum und Rosen,", "tokens": ["Ich", "war", "ver\u00b7welkt", ",", "wie", "Lenz", "und", "Baum", "und", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PWAV", "NN", "KON", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein matter Nachhall sch\u00f6ner Fr\u00fchlingsweisen.", "tokens": ["Ein", "mat\u00b7ter", "Nach\u00b7hall", "sch\u00f6\u00b7ner", "Fr\u00fch\u00b7lings\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}