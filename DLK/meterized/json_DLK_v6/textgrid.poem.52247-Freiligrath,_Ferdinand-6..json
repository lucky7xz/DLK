{"textgrid.poem.52247": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch \u2013 wenn zuerst in Meer- und Pulvernebel", "tokens": ["Doch", "\u2013", "wenn", "zu\u00b7erst", "in", "Meer", "und", "Pul\u00b7ver\u00b7ne\u00b7bel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$(", "KOUS", "ADV", "APPR", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir also schwimmend Volk an Volk gerungen;", "tokens": ["Wir", "al\u00b7so", "schwim\u00b7mend", "Volk", "an", "Volk", "ge\u00b7run\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn eine Seeschlacht Lorbeern uns geschlungen", "tokens": ["Wenn", "ei\u00b7ne", "See\u00b7schlacht", "Lor\u00b7beern", "uns", "ge\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um unsre Lunten und um unsre S\u00e4bel:", "tokens": ["Um", "uns\u00b7re", "Lun\u00b7ten", "und", "um", "uns\u00b7re", "S\u00e4\u00b7bel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "KON", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann seid gedenk! An Schiffen sitzen Schn\u00e4bel!", "tokens": ["Dann", "seid", "ge\u00b7denk", "!", "An", "Schif\u00b7fen", "sit\u00b7zen", "Schn\u00e4\u00b7bel", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$.", "APPR", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Drauf, ihr Matrosen und Kaj\u00fctenjungen!", "tokens": ["Drauf", ",", "ihr", "Mat\u00b7ro\u00b7sen", "und", "Ka\u00b7j\u00fc\u00b7ten\u00b7jun\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den wucht'gen Hammer und das Beil geschwungen!", "tokens": ["Den", "wucht'\u00b7gen", "Ham\u00b7mer", "und", "das", "Beil", "ge\u00b7schwun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Schn\u00e4bel ab! und bringt sie heim als Hebel!", "tokens": ["Die", "Schn\u00e4\u00b7bel", "ab", "!", "und", "bringt", "sie", "heim", "als", "He\u00b7bel", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "KON", "VVFIN", "PPER", "PTKVZ", "KOKOM", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Als Hebel? \u2013 Ja! \u2013 Ihr, die mit heiterm Sp\u00e4hen", "tokens": ["Als", "He\u00b7bel", "?", "\u2013", "Ja", "!", "\u2013", "Ihr", ",", "die", "mit", "hei\u00b7term", "Sp\u00e4\u00b7hen"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "$.", "$(", "PTKANT", "$.", "$(", "PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am Strand ihr jauchztet unsrer frischen K\u00fchne", "tokens": ["Am", "Strand", "ihr", "jauchz\u00b7tet", "uns\u00b7rer", "fri\u00b7schen", "K\u00fch\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und l\u00e4chelnd ansaht unser salzig Rennen:", "tokens": ["Und", "l\u00e4\u00b7chelnd", "an\u00b7saht", "un\u00b7ser", "sal\u00b7zig", "Ren\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr Bannerherrn, wohin mit den Troph\u00e4en? \u2013", "tokens": ["Ihr", "Ban\u00b7ner\u00b7herrn", ",", "wo\u00b7hin", "mit", "den", "Trop\u00b7h\u00e4\u00b7en", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sorgt f\u00fcr ein ", "tokens": ["Sorgt", "f\u00fcr", "ein"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Da\u00df wir, wie Rom, das Beste schm\u00fccken k\u00f6nnen!", "tokens": ["Da\u00df", "wir", ",", "wie", "Rom", ",", "das", "Bes\u00b7te", "schm\u00fc\u00b7cken", "k\u00f6n\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "NE", "$,", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}