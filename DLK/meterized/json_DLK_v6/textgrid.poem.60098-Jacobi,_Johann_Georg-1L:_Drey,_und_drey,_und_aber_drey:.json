{"textgrid.poem.60098": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Drey, und drey, und aber drey:", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drey, und drey, und aber drey:", "tokens": ["Drey", ",", "und", "drey", ",", "und", "a\u00b7ber", "drey", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "$,", "KON", "CARD", "$,", "KON", "ADV", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Strom zur\u00fcck! Fels herbey!", "tokens": ["Strom", "zu\u00b7r\u00fcck", "!", "Fels", "her\u00b7bey", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "NE", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Drey, und drey, und aber drey:", "tokens": ["Drey", ",", "und", "drey", ",", "und", "a\u00b7ber", "drey", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "$,", "KON", "CARD", "$,", "KON", "ADV", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mond herunter! Herauf die Todten!", "tokens": ["Mond", "her\u00b7un\u00b7ter", "!", "Her\u00b7auf", "die", "Tod\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "NN", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "K\u00f6nntest du l\u00f6sen die heiligen Knoten,", "tokens": ["K\u00f6nn\u00b7test", "du", "l\u00f6\u00b7sen", "die", "hei\u00b7li\u00b7gen", "Kno\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Wehe, wehe, wehe dir!", "tokens": ["We\u00b7he", ",", "we\u00b7he", ",", "we\u00b7he", "dir", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00fcrdest wandeln Mensch und Thier;", "tokens": ["W\u00fcr\u00b7dest", "wan\u00b7deln", "Mensch", "und", "Thier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit dem Otterngez\u00fcchte scherzen,", "tokens": ["Mit", "dem", "Ot\u00b7tern\u00b7ge\u00b7z\u00fcch\u00b7te", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Aber nicht wandeln Menschen-Herzen.", "tokens": ["A\u00b7ber", "nicht", "wan\u00b7deln", "Men\u00b7schen\u00b7Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Herzen bewegt kein Zauberspruch;", "tokens": ["Her\u00b7zen", "be\u00b7wegt", "kein", "Zau\u00b7ber\u00b7spruch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Willst du das, so mache Versuch,", "tokens": ["Willst", "du", "das", ",", "so", "ma\u00b7che", "Ver\u00b7such", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDS", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Da\u00df dir g\u00fcnstig ein holderes Drey,", "tokens": ["Da\u00df", "dir", "g\u00fcns\u00b7tig", "ein", "hol\u00b7de\u00b7res", "Drey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.13": {"text": "G\u00fcnstig jede der Grazien sey!", "tokens": ["G\u00fcns\u00b7tig", "je\u00b7de", "der", "Gra\u00b7zi\u00b7en", "sey", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ART", "NN", "VAFIN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}}}}