{"textgrid.poem.43500": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: O wie freu'n wir uns,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie freu'n wir uns,", "tokens": ["O", "wie", "freu'n", "wir", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wenn ein Fr\u00fchlingstag", "tokens": ["Wenn", "ein", "Fr\u00fch\u00b7lings\u00b7tag"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Endlich heiter lacht", "tokens": ["End\u00b7lich", "hei\u00b7ter", "lacht"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ueber Feld und Hag!", "tokens": ["Ue\u00b7ber", "Feld", "und", "Hag", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn ein Falter froh", "tokens": ["Wenn", "ein", "Fal\u00b7ter", "froh"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Durch die Luft sich schwingt", "tokens": ["Durch", "die", "Luft", "sich", "schwingt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und ein Bl\u00fcmchen still", "tokens": ["Und", "ein", "Bl\u00fcm\u00b7chen", "still"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PTKVZ"], "meter": "--+-+", "measure": "anapaest.init"}, "line.4": {"text": "Aus der Knospe springt;", "tokens": ["Aus", "der", "Knos\u00b7pe", "springt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn der letzte Schnee", "tokens": ["Wenn", "der", "letz\u00b7te", "Schnee"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Rieselt hin als Quell", "tokens": ["Rie\u00b7selt", "hin", "als", "Quell"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KOUS", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Durch die gr\u00fcnen Au'n", "tokens": ["Durch", "die", "gr\u00fc\u00b7nen", "Au'n"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Rein und silberhell;", "tokens": ["Rein", "und", "sil\u00b7ber\u00b7hell", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Wenn zum ersten Mal", "tokens": ["Wenn", "zum", "ers\u00b7ten", "Mal"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Uns mit frohem Schall", "tokens": ["Uns", "mit", "fro\u00b7hem", "Schall"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Aus dem jungen Laub", "tokens": ["Aus", "dem", "jun\u00b7gen", "Laub"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.4": {"text": "Gr\u00fc\u00dft die Nachtigall \u2013", "tokens": ["Gr\u00fc\u00dft", "die", "Nach\u00b7ti\u00b7gall", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Unser Herz geht auf", "tokens": ["Un\u00b7ser", "Herz", "geht", "auf"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wie das Bl\u00fcmelein", "tokens": ["Wie", "das", "Bl\u00fc\u00b7me\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und es freuet sich", "tokens": ["Und", "es", "freu\u00b7et", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Auch am Sonnenschein.", "tokens": ["Auch", "am", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Freue du dich auch", "tokens": ["Freu\u00b7e", "du", "dich", "auch"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wie der Fr\u00fchlingstag,", "tokens": ["Wie", "der", "Fr\u00fch\u00b7lings\u00b7tag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der da heiter lacht", "tokens": ["Der", "da", "hei\u00b7ter", "lacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ueber Feld und Hag!", "tokens": ["Ue\u00b7ber", "Feld", "und", "Hag", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}