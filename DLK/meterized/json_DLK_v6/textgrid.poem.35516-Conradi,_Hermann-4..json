{"textgrid.poem.35516": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Flut ist nun verbrandet,", "tokens": ["Die", "Flut", "ist", "nun", "ver\u00b7bran\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Sturm ist nun verdr\u00f6hnt \u2013", "tokens": ["Der", "Sturm", "ist", "nun", "ver\u00b7dr\u00f6hnt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich aber bin gelandet,", "tokens": ["Ich", "a\u00b7ber", "bin", "ge\u00b7lan\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Und mir den Kummer ebbt,", "tokens": ["Und", "mir", "den", "Kum\u00b7mer", "ebbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den ich durch Staub und Schlachtendampf", "tokens": ["Den", "ich", "durch", "Staub", "und", "Schlach\u00b7ten\u00b7dampf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Tag\u00fcber mitgeschleppt.", "tokens": ["Ta\u00b7g\u00fc\u00b7ber", "mit\u00b7ge\u00b7schleppt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es hat die Wunderaugen", "tokens": ["Es", "hat", "die", "Wun\u00b7der\u00b7au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Nacht erschlossen weit,", "tokens": ["Die", "Nacht", "er\u00b7schlos\u00b7sen", "weit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und meine Blicke saugen", "tokens": ["Und", "mei\u00b7ne", "Bli\u00b7cke", "sau\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich in die Ewigkeit.", "tokens": ["Sich", "in", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mir ist, als h\u00f6rt' ich schlagen", "tokens": ["Mir", "ist", ",", "als", "h\u00f6rt'", "ich", "schla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In mir das Herz der Welt,", "tokens": ["In", "mir", "das", "Herz", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Als w\u00e4r' ich, ird'scher Grenzen bar,", "tokens": ["Als", "w\u00e4r'", "ich", ",", "ird'\u00b7scher", "Gren\u00b7zen", "bar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dem ", "tokens": ["Dem"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Wie d\u00fcnkt mich Menschentrachten", "tokens": ["Wie", "d\u00fcnkt", "mich", "Men\u00b7schen\u00b7trach\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So zwerghaft nun und klein!", "tokens": ["So", "zwerg\u00b7haft", "nun", "und", "klein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein gro\u00dfes Weltverachten", "tokens": ["Ein", "gro\u00b7\u00dfes", "Welt\u00b7ver\u00b7ach\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zieht in die Brust mir ein!", "tokens": ["Zieht", "in", "die", "Brust", "mir", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was mich im Tagesschwall bewegt,", "tokens": ["Was", "mich", "im", "Ta\u00b7ges\u00b7schwall", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zerf\u00e4llt nun und versinkt!", "tokens": ["Zer\u00b7f\u00e4llt", "nun", "und", "ver\u00b7sinkt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Flut ist nun verbrandet,", "tokens": ["Die", "Flut", "ist", "nun", "ver\u00b7bran\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Sturm ist nun verdr\u00f6hnt,", "tokens": ["Der", "Sturm", "ist", "nun", "ver\u00b7dr\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich aber bin gelandet,", "tokens": ["Ich", "a\u00b7ber", "bin", "ge\u00b7lan\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "In goldner Flut entquillt sie", "tokens": ["In", "gold\u00b7ner", "Flut", "ent\u00b7quillt", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dem Universums-Kern,", "tokens": ["Dem", "U\u00b7ni\u00b7ver\u00b7sums\u00b7Kern", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und ihren Schleier spannt sie aus", "tokens": ["Und", "ih\u00b7ren", "Schlei\u00b7er", "spannt", "sie", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}