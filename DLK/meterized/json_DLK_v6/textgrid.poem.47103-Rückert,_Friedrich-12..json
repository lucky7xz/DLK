{"textgrid.poem.47103": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Beseligt sein und selig tief empfinden,", "tokens": ["Be\u00b7se\u00b7ligt", "sein", "und", "se\u00b7lig", "tief", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "KON", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie du, beseliget, beseligest;", "tokens": ["Wie", "du", ",", "be\u00b7se\u00b7li\u00b7get", ",", "be\u00b7se\u00b7li\u00b7gest", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Herz, la\u00df dir das Bewu\u00dftsein nie entwinden,", "tokens": ["Herz", ",", "la\u00df", "dir", "das", "Be\u00b7wu\u00df\u00b7tsein", "nie", "ent\u00b7win\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fest halt' es, wie im Arm die Liebste, fest!", "tokens": ["Fest", "halt'", "es", ",", "wie", "im", "Arm", "die", "Liebs\u00b7te", ",", "fest", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PWAV", "APPRART", "NN", "ART", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}