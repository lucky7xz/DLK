{"textgrid.poem.35835": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Voll Wehmuth denke ich an Dich,", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Voll Wehmuth denke ich an Dich,", "tokens": ["Voll", "Weh\u00b7muth", "den\u00b7ke", "ich", "an", "Dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Du von mir geschieden,", "tokens": ["Die", "Du", "von", "mir", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und meine Seel' umw\u00f6lket sich", "tokens": ["Und", "mei\u00b7ne", "Seel'", "um\u00b7w\u00f6l\u00b7ket", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sehnt sich weg hienieden.", "tokens": ["Und", "sehnt", "sich", "weg", "hien\u00b7ie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O, da\u00df auch mich der stille Tod", "tokens": ["O", ",", "da\u00df", "auch", "mich", "der", "stil\u00b7le", "Tod"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "ADV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Stengel nur entpfl\u00fcckte!", "tokens": ["Dem", "Sten\u00b7gel", "nur", "ent\u00b7pfl\u00fcck\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "O, da\u00df ich bald das Morgenroth", "tokens": ["O", ",", "da\u00df", "ich", "bald", "das", "Mor\u00b7gen\u00b7roth"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des letzten Tags erblickte!", "tokens": ["Des", "letz\u00b7ten", "Tags", "er\u00b7blick\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Freuden, so die Welt mir beut,", "tokens": ["Der", "Freu\u00b7den", ",", "so", "die", "Welt", "mir", "beut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist l\u00e4ngst mein Herz entw\u00f6hnet", "tokens": ["Ist", "l\u00e4ngst", "mein", "Herz", "ent\u00b7w\u00f6h\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und hat von ihrer Eitelkeit", "tokens": ["Und", "hat", "von", "ih\u00b7rer", "Ei\u00b7tel\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich schmerzlich weg gesehnet.", "tokens": ["Sich", "schmerz\u00b7lich", "weg", "ge\u00b7seh\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O, m\u00f6cht' ich Deine Krone sehn", "tokens": ["O", ",", "m\u00f6cht'", "ich", "Dei\u00b7ne", "Kro\u00b7ne", "sehn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und h\u00f6ren Deine Weisen!", "tokens": ["Und", "h\u00f6\u00b7ren", "Dei\u00b7ne", "Wei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "O, d\u00fcrft' ich Dir zur Seite stehn,", "tokens": ["O", ",", "d\u00fcrft'", "ich", "Dir", "zur", "Sei\u00b7te", "stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "PPER", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit Dir Gott ewig preisen! \u2013", "tokens": ["Mit", "Dir", "Gott", "e\u00b7wig", "prei\u00b7sen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "NN", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mein Vater! hilf mir diesen Schmerz,", "tokens": ["Mein", "Va\u00b7ter", "!", "hilf", "mir", "die\u00b7sen", "Schmerz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVIMP", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den tiefen Schmerz mir tragen,", "tokens": ["Den", "tie\u00b7fen", "Schmerz", "mir", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis endlich heilt dies wunde Herz,", "tokens": ["Bis", "end\u00b7lich", "heilt", "dies", "wun\u00b7de", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis mir Dein Licht will tagen.", "tokens": ["Bis", "mir", "Dein", "Licht", "will", "ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du wei\u00dft, Herr! da\u00df kein Erdengut", "tokens": ["Du", "wei\u00dft", ",", "Herr", "!", "da\u00df", "kein", "Er\u00b7den\u00b7gut"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "$.", "KOUS", "PIAT", "NN"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Den Jammer mir kann stillen;", "tokens": ["Den", "Jam\u00b7mer", "mir", "kann", "stil\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Du wollst mit Deiner Liebe Gluth", "tokens": ["Du", "wollst", "mit", "Dei\u00b7ner", "Lie\u00b7be", "Gluth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein sehnend Herz erf\u00fcllen!", "tokens": ["Mein", "seh\u00b7nend", "Herz", "er\u00b7f\u00fcl\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbso walle deinen Pfad hinab \u2013", "tokens": ["\u00bb", "so", "wal\u00b7le", "dei\u00b7nen", "Pfad", "hin\u00b7ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Br\u00fcdern sollst du dienen;", "tokens": ["Den", "Br\u00fc\u00b7dern", "sollst", "du", "die\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann wird dereinst dein Pilgerstab", "tokens": ["Dann", "wird", "de\u00b7reinst", "dein", "Pil\u00b7ger\u00b7stab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Palme dir ergr\u00fcnen.\u00ab", "tokens": ["Zur", "Pal\u00b7me", "dir", "er\u00b7gr\u00fc\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ja, Vater! was Du mir erw\u00e4hlt,", "tokens": ["Ja", ",", "Va\u00b7ter", "!", "was", "Du", "mir", "er\u00b7w\u00e4hlt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$.", "PWS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das hab' auch ich beschlossen.", "tokens": ["Das", "hab'", "auch", "ich", "be\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wenn Deine Kraft das Herz mir st\u00e4hlt,", "tokens": ["Wenn", "Dei\u00b7ne", "Kraft", "das", "Herz", "mir", "st\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So leid' ich unverdrossen.", "tokens": ["So", "leid'", "ich", "un\u00b7ver\u00b7dros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}