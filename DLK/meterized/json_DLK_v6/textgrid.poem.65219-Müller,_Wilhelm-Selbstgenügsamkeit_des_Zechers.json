{"textgrid.poem.65219": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Selbstgen\u00fcgsamkeit des Zechers", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich trinke guten Wein,", "tokens": ["Wenn", "ich", "trin\u00b7ke", "gu\u00b7ten", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00e4llt es mir mit nichten ein,", "tokens": ["F\u00e4llt", "es", "mir", "mit", "nich\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PIS", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber dieser Erde Schranken", "tokens": ["\u00dc\u00b7ber", "die\u00b7ser", "Er\u00b7de", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aufzuschwingen die Gedanken,", "tokens": ["Auf\u00b7zu\u00b7schwin\u00b7gen", "die", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und zu schaun in blaue Fernen", "tokens": ["Und", "zu", "schaun", "in", "blau\u00b7e", "Fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKZU", "VVINF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nach des ew'gen Ruhmes Sternen.", "tokens": ["Nach", "des", "ew'\u00b7gen", "Ruh\u00b7mes", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn ich trinke guten Wein,", "tokens": ["Wenn", "ich", "trin\u00b7ke", "gu\u00b7ten", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Will ich nicht im Himmel sein.", "tokens": ["Will", "ich", "nicht", "im", "Him\u00b7mel", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPRART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wi\u00dft ihr von dem Phaeton,", "tokens": ["Wi\u00dft", "ihr", "von", "dem", "Phae\u00b7ton", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ph\u00f6bus naseweisem Sohn,", "tokens": ["Ph\u00f6\u00b7bus", "na\u00b7se\u00b7wei\u00b7sem", "Sohn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der auf seines Vaters Wagen", "tokens": ["Der", "auf", "sei\u00b7nes", "Va\u00b7ters", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollte durch den Himmel jagen?", "tokens": ["Woll\u00b7te", "durch", "den", "Him\u00b7mel", "ja\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jupiter mit seinem Blitze", "tokens": ["Ju\u00b7pi\u00b7ter", "mit", "sei\u00b7nem", "Blit\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schmettert' ihn vom Kutschersitze", "tokens": ["Schmet\u00b7tert'", "ihn", "vom", "Kut\u00b7scher\u00b7sit\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "H\u00e4uptlings in den Po hinab,", "tokens": ["H\u00e4upt\u00b7lings", "in", "den", "Po", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und das ", "tokens": ["Und", "das"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Anders ging es nicht dem Kind,", "tokens": ["An\u00b7ders", "ging", "es", "nicht", "dem", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das aus Kreta's Labyrinth", "tokens": ["Das", "aus", "Kre\u00b7ta's", "La\u00b7by\u00b7rinth"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "NE", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Wollt' auf seinen eitlen Schwingen", "tokens": ["Wollt'", "auf", "sei\u00b7nen", "eit\u00b7len", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Grad' empor zur Sonne dringen.", "tokens": ["Grad'", "em\u00b7por", "zur", "Son\u00b7ne", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bald zerschmolz das Wachsgefieder,", "tokens": ["Bald", "zer\u00b7schmolz", "das", "Wachs\u00b7ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Vogel st\u00fcrzte nieder:", "tokens": ["Und", "der", "Vo\u00b7gel", "st\u00fcrz\u00b7te", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "VVFIN", "PTKVZ", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "In des ", "tokens": ["In", "des"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "B\u00fc\u00dft' er seinen tollen Muth.", "tokens": ["B\u00fc\u00dft'", "er", "sei\u00b7nen", "tol\u00b7len", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Phaeton und Ikarus,", "tokens": ["Phae\u00b7ton", "und", "I\u00b7ka\u00b7rus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.2": {"text": "Du im Meer, und du im Flu\u00df,", "tokens": ["Du", "im", "Meer", ",", "und", "du", "im", "Flu\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "KON", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4ttet ihr h\u00fcbsch Wein getrunken,", "tokens": ["H\u00e4t\u00b7tet", "ihr", "h\u00fcbsch", "Wein", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimmer w\u00e4ret ihr gesunken", "tokens": ["Nim\u00b7mer", "w\u00e4\u00b7ret", "ihr", "ge\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Von dem hohen Himmelsbogen", "tokens": ["Von", "dem", "ho\u00b7hen", "Him\u00b7mels\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In die tiefen Wasserwogen:", "tokens": ["In", "die", "tie\u00b7fen", "Was\u00b7ser\u00b7wo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die da trinken guten Wein,", "tokens": ["Die", "da", "trin\u00b7ken", "gu\u00b7ten", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wollen nicht im Himmel sein.", "tokens": ["Wol\u00b7len", "nicht", "im", "Him\u00b7mel", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPRART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn ich trinke guten Wein,", "tokens": ["Wenn", "ich", "trin\u00b7ke", "gu\u00b7ten", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00e4llt mir oft eur Schicksal ein,", "tokens": ["F\u00e4llt", "mir", "oft", "eur", "Schick\u00b7sal", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich blick' als frommer Zecher", "tokens": ["Und", "ich", "blick'", "als", "from\u00b7mer", "Ze\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nieder in den engen Becher,", "tokens": ["Nie\u00b7der", "in", "den", "en\u00b7gen", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht empor nach Ehrensternen,", "tokens": ["Nicht", "em\u00b7por", "nach", "Eh\u00b7rens\u00b7ter\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKVZ", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht hinaus in blaue Fernen:", "tokens": ["Nicht", "hin\u00b7aus", "in", "blau\u00b7e", "Fer\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn ich trinke guten Wein,", "tokens": ["Wenn", "ich", "trin\u00b7ke", "gu\u00b7ten", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mein' ich, was ich will, zu sein.", "tokens": ["Mein'", "ich", ",", "was", "ich", "will", ",", "zu", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWS", "PPER", "VMFIN", "$,", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}