{"textgrid.poem.63725": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "N\u00e4chstenliebe", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach Sapo di Sorrento wollt' ich heut.", "tokens": ["Nach", "Sa\u00b7po", "di", "Sor\u00b7ren\u00b7to", "wollt'", "ich", "heut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "NE", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die Stra\u00dfe geht bergan, dazu die Glut", "tokens": ["Die", "Stra\u00b7\u00dfe", "geht", "ber\u00b7gan", ",", "da\u00b7zu", "die", "Glut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PAV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des fr\u00fchen Sommers. Langsam schritt ich fort", "tokens": ["Des", "fr\u00fc\u00b7hen", "Som\u00b7mers", ".", "Lang\u00b7sam", "schritt", "ich", "fort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ADJD", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und trocknet' h\u00e4ufig an der Stirn den Schwei\u00df.", "tokens": ["Und", "trock\u00b7net'", "h\u00e4u\u00b7fig", "an", "der", "Stirn", "den", "Schwei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wer jetzt ein W\u00e4glein h\u00e4tt'!", "tokens": ["Wer", "jetzt", "ein", "W\u00e4g\u00b7lein", "h\u00e4tt'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da, hinter mir,", "tokens": ["Da", ",", "hin\u00b7ter", "mir", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wie durch ein Zauberwort herangelockt,", "tokens": ["Wie", "durch", "ein", "Zau\u00b7ber\u00b7wort", "her\u00b7an\u00b7ge\u00b7lockt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Trapp! trapp! ein Hufgeklapper, ein Ger\u00e4usch", "tokens": ["Trapp", "!", "trapp", "!", "ein", "Huf\u00b7ge\u00b7klap\u00b7per", ",", "ein", "Ge\u00b7r\u00e4usch"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$.", "ADJD", "$.", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Von R\u00e4dern, atemlos in toller Fahrt.", "tokens": ["Von", "R\u00e4\u00b7dern", ",", "a\u00b7tem\u00b7los", "in", "tol\u00b7ler", "Fahrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Denn ohne Pause lie\u00df vom Kutschenbock", "tokens": ["Denn", "oh\u00b7ne", "Pau\u00b7se", "lie\u00df", "vom", "Kut\u00b7schen\u00b7bock"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der Wagenlenker auf sein armes Ro\u00df", "tokens": ["Der", "Wa\u00b7gen\u00b7len\u00b7ker", "auf", "sein", "ar\u00b7mes", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die Peitsche niedersausen. ", "tokens": ["Die", "Peit\u00b7sche", "nie\u00b7der\u00b7sau\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Nur ein magrer Klepper war's,", "tokens": ["Nur", "ein", "mag\u00b7rer", "Klep\u00b7per", "wa\u00b7r's", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.14": {"text": "Dem man die Rippen z\u00e4hlen konnt' im Fell,", "tokens": ["Dem", "man", "die", "Rip\u00b7pen", "z\u00e4h\u00b7len", "konnt'", "im", "Fell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "NN", "VVINF", "VMFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Krummbeinig, Geifer um das offne Maul,", "tokens": ["Krumm\u00b7bei\u00b7nig", ",", "Gei\u00b7fer", "um", "das", "off\u00b7ne", "Maul", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.16": {"text": "Und keuchend schwer den steilen Berg hinan.", "tokens": ["Und", "keu\u00b7chend", "schwer", "den", "stei\u00b7len", "Berg", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Im Wagen aber sa\u00df zur\u00fcckgelehnt", "tokens": ["Im", "Wa\u00b7gen", "a\u00b7ber", "sa\u00df", "zu\u00b7r\u00fcck\u00b7ge\u00b7lehnt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Ein dicker Priester \u2013 nein, ein Pfaffe nur,", "tokens": ["Ein", "di\u00b7cker", "Pries\u00b7ter", "\u2013", "nein", ",", "ein", "Pfaf\u00b7fe", "nur", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PTKANT", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und blinzelt' mit den muntren \u00c4uglein sehr", "tokens": ["Und", "blin\u00b7zelt'", "mit", "den", "mun\u00b7tren", "\u00c4ug\u00b7lein", "sehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Vergn\u00fcgt umher ob seiner raschen Fahrt.", "tokens": ["Ver\u00b7gn\u00fcgt", "um\u00b7her", "ob", "sei\u00b7ner", "ra\u00b7schen", "Fahrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KOUS", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Mir schwoll das Herz vor Grimm. Ich stand und rief", "tokens": ["Mir", "schwoll", "das", "Herz", "vor", "Grimm", ".", "Ich", "stand", "und", "rief"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ART", "NN", "APPR", "NE", "$.", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Dem Burschen zu: Wahnsinniger! Siehst du nicht,", "tokens": ["Dem", "Bur\u00b7schen", "zu", ":", "Wahn\u00b7sin\u00b7ni\u00b7ger", "!", "Siehst", "du", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Da\u00df deinem Gaul die Zunge lechzend schon", "tokens": ["Da\u00df", "dei\u00b7nem", "Gaul", "die", "Zun\u00b7ge", "lech\u00b7zend", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "ADJD", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Zum Hals heraush\u00e4ngt, und du schl\u00e4gst ihn noch?", "tokens": ["Zum", "Hals", "her\u00b7aus\u00b7h\u00e4ngt", ",", "und", "du", "schl\u00e4gst", "ihn", "noch", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Erbarmt dich nicht der stummen Kreatur?", "tokens": ["Er\u00b7barmt", "dich", "nicht", "der", "stum\u00b7men", "Kre\u00b7a\u00b7tur", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch er, die Z\u00e4hne fletschend mir zum Hohn:", "tokens": ["Doch", "er", ",", "die", "Z\u00e4h\u00b7ne", "flet\u00b7schend", "mir", "zum", "Hohn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "VVPP", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was wollt Ihr, Herr? 's ist ein Bestie nur.", "tokens": ["Was", "wollt", "Ihr", ",", "Herr", "?", "'s", "ist", "ein", "Be\u00b7stie", "nur", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$,", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Bestie du selbst! \u2013 Und zu dem Priester \u2013 nein,", "tokens": ["Be\u00b7stie", "du", "selbst", "!", "\u2013", "Und", "zu", "dem", "Pries\u00b7ter", "\u2013", "nein", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$.", "$(", "KON", "APPR", "ART", "NN", "$(", "PTKANT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem Pfaffen, hilfeflehend blickt' ich hin.", "tokens": ["Dem", "Pfaf\u00b7fen", ",", "hil\u00b7fe\u00b7fle\u00b7hend", "blickt'", "ich", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der aber zuckte nur die Achseln, schob", "tokens": ["Der", "a\u00b7ber", "zuck\u00b7te", "nur", "die", "Ach\u00b7seln", ",", "schob"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADV", "VVFIN", "ADV", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Unterlippe vor und wiegte lachend", "tokens": ["Die", "Un\u00b7ter\u00b7lip\u00b7pe", "vor", "und", "wieg\u00b7te", "la\u00b7chend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Den Kopf, als wollt' er sagen: Nehmt es nicht", "tokens": ["Den", "Kopf", ",", "als", "wollt'", "er", "sa\u00b7gen", ":", "Nehmt", "es", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "VMFIN", "PPER", "VVINF", "$.", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So tragisch! Denn f\u00fcrwahr, der Bursch hat recht.", "tokens": ["So", "tra\u00b7gisch", "!", "Denn", "f\u00fcr\u00b7wahr", ",", "der", "Bursch", "hat", "recht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "KON", "ADV", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und vor\u00fcber fliegt", "tokens": ["Und", "vor\u00b7\u00fc\u00b7ber", "fliegt"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "Das edle Paar. Ich stehe tief emp\u00f6rt", "tokens": ["Das", "ed\u00b7le", "Paar", ".", "Ich", "ste\u00b7he", "tief", "em\u00b7p\u00f6rt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und kummervoll. Wie? Keine Seele h\u00e4tt's?", "tokens": ["Und", "kum\u00b7mer\u00b7voll", ".", "Wie", "?", "Kei\u00b7ne", "See\u00b7le", "h\u00e4tt's", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "PWAV", "$.", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Jawohl, so niedrig keine, wie du selbst!", "tokens": ["Ja\u00b7wohl", ",", "so", "nied\u00b7rig", "kei\u00b7ne", ",", "wie", "du", "selbst", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADJD", "PIAT", "$,", "PWAV", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und w\u00e4r' kein Christ? So einer nicht, wie Ihr,", "tokens": ["Und", "w\u00e4r'", "kein", "Christ", "?", "So", "ei\u00b7ner", "nicht", ",", "wie", "Ihr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "$.", "ADV", "ART", "PTKNEG", "$,", "PWAV", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Hochw\u00fcrd'ger Herr! Doch wenn am j\u00fcngsten Tag", "tokens": ["Hoch\u00b7w\u00fcrd'\u00b7ger", "Herr", "!", "Doch", "wenn", "am", "j\u00fcng\u00b7sten", "Tag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "KON", "KOUS", "APPRART", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.15": {"text": "Die Wiederbringung aller Ding erfolgt", "tokens": ["Die", "Wie\u00b7der\u00b7brin\u00b7gung", "al\u00b7ler", "Ding", "er\u00b7folgt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und dieses arme Pferdchen neben Euch", "tokens": ["Und", "die\u00b7ses", "ar\u00b7me", "Pferd\u00b7chen", "ne\u00b7ben", "Euch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Vor unser aller Richter steht, mich d\u00fcnkt,", "tokens": ["Vor", "un\u00b7ser", "al\u00b7ler", "Rich\u00b7ter", "steht", ",", "mich", "d\u00fcnkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "PIAT", "NN", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Der milde Jesus wird mit d\u00fcstrer Stirn", "tokens": ["Der", "mil\u00b7de", "Je\u00b7sus", "wird", "mit", "d\u00fcst\u00b7rer", "Stirn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Zu Euch sich wenden: Hier im Himmel ist", "tokens": ["Zu", "Euch", "sich", "wen\u00b7den", ":", "Hier", "im", "Him\u00b7mel", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PRF", "VVINF", "$.", "ADV", "APPRART", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Kein Platz f\u00fcr die, so kalt und heuchlerisch", "tokens": ["Kein", "Platz", "f\u00fcr", "die", ",", "so", "kalt", "und", "heuch\u00b7le\u00b7risch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "ART", "$,", "ADV", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Sich meines heiligen Namens angema\u00dft.", "tokens": ["Sich", "mei\u00b7nes", "hei\u00b7li\u00b7gen", "Na\u00b7mens", "an\u00b7ge\u00b7ma\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Lehrt' ich Euch nicht, da\u00df der Gerechte sich", "tokens": ["Lehrt'", "ich", "Euch", "nicht", ",", "da\u00df", "der", "Ge\u00b7rech\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "$,", "KOUS", "ART", "NN", "PRF"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.23": {"text": "Auch seines Viehs erbarmt? Und sollt Ihr nicht", "tokens": ["Auch", "sei\u00b7nes", "Viehs", "er\u00b7barmt", "?", "Und", "sollt", "Ihr", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$.", "KON", "VMFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Die Wesen alle, die mein Vater schuf,", "tokens": ["Die", "We\u00b7sen", "al\u00b7l