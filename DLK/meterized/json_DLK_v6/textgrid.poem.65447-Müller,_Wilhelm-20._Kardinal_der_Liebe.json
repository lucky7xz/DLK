{"textgrid.poem.65447": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "20. Kardinal der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bittre Pomeranzenschale schneidest du in meinen Wein,", "tokens": ["Bitt\u00b7re", "Po\u00b7me\u00b7ran\u00b7zen\u00b7scha\u00b7le", "schnei\u00b7dest", "du", "in", "mei\u00b7nen", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Aber mit dem andern H\u00e4ndchen wirfst du Zucker auch hinein.", "tokens": ["A\u00b7ber", "mit", "dem", "an\u00b7dern", "H\u00e4nd\u00b7chen", "wirfst", "du", "Zu\u00b7cker", "auch", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VAFIN", "PPER", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Spricht zu mir dein spr\u00f6des M\u00fcndchen bittre Worte manches Mal,", "tokens": ["Spricht", "zu", "mir", "dein", "spr\u00f6\u00b7des", "M\u00fcnd\u00b7chen", "bitt\u00b7re", "Wor\u00b7te", "man\u00b7ches", "Mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Giebt mir s\u00fc\u00dfen Trost dein Auge, und mein Herz trinkt Kardinal.", "tokens": ["Giebt", "mir", "s\u00fc\u00b7\u00dfen", "Trost", "dein", "Au\u00b7ge", ",", "und", "mein", "Herz", "trinkt", "Kar\u00b7di\u00b7nal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "NE", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}