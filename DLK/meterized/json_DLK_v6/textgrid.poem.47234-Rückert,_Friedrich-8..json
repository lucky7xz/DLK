{"textgrid.poem.47234": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drei Kiele kenn' ich, die gewaltig sind!", "tokens": ["Drei", "Kie\u00b7le", "kenn'", "ich", ",", "die", "ge\u00b7wal\u00b7tig", "sind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "$,", "PRELS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der erste Kiel ist, den die V\u00f6gel spannen,", "tokens": ["Der", "ers\u00b7te", "Kiel", "ist", ",", "den", "die", "V\u00f6\u00b7gel", "span\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Womit sie \u00fcber Berg und Thal von dannen", "tokens": ["Wo\u00b7mit", "sie", "\u00fc\u00b7ber", "Berg", "und", "Thal", "von", "dan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN", "KON", "NN", "APPR", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ziehn, hingeschaukelt auf des Himmels Wind.", "tokens": ["Ziehn", ",", "hin\u00b7ge\u00b7schau\u00b7kelt", "auf", "des", "Him\u00b7mels", "Wind", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der zweite Kiel, nicht weniger geschwind,", "tokens": ["Der", "zwei\u00b7te", "Kiel", ",", "nicht", "we\u00b7ni\u00b7ger", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist der, womit ein Wunderbau von Tannen", "tokens": ["Ist", "der", ",", "wo\u00b7mit", "ein", "Wun\u00b7der\u00b7bau", "von", "Tan\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "$,", "PWAV", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ger\u00fcstet ist, worauf sich zum Tyrannen", "tokens": ["Ge\u00b7r\u00fcs\u00b7tet", "ist", ",", "wo\u00b7rauf", "sich", "zum", "Ty\u00b7ran\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "$,", "PWAV", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Meeres macht das k\u00fchne Menschenkind.", "tokens": ["Des", "Mee\u00b7res", "macht", "das", "k\u00fch\u00b7ne", "Men\u00b7schen\u00b7kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der dritte Kiel ist aber, der gewaltig", "tokens": ["Der", "drit\u00b7te", "Kiel", "ist", "a\u00b7ber", ",", "der", "ge\u00b7wal\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor allen ist; wohin kein Vogel fliegt,", "tokens": ["Vor", "al\u00b7len", "ist", ";", "wo\u00b7hin", "kein", "Vo\u00b7gel", "fliegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "$.", "PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Schiff, da geht sein Fu\u00dftritt doppelspaltig.", "tokens": ["Kein", "Schiff", ",", "da", "geht", "sein", "Fu\u00df\u00b7tritt", "dop\u00b7pel\u00b7spal\u00b7tig", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er ist's, der den Gedanken selbst besiegt,", "tokens": ["Er", "ist's", ",", "der", "den", "Ge\u00b7dan\u00b7ken", "selbst", "be\u00b7siegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den unsichtbaren Riesen vielgestaltig,", "tokens": ["Den", "un\u00b7sicht\u00b7ba\u00b7ren", "Rie\u00b7sen", "viel\u00b7ge\u00b7stal\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df er gebannt auf zarten Bl\u00e4ttern liegt.", "tokens": ["Da\u00df", "er", "ge\u00b7bannt", "auf", "zar\u00b7ten", "Bl\u00e4t\u00b7tern", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}