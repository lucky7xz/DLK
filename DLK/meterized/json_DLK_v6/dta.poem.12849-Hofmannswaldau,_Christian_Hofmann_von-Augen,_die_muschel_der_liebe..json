{"dta.poem.12849": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Augen, die muschel der liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Florettens augen sind der liebe muschel-hau\u00df,", "tokens": ["Flo\u00b7ret\u00b7tens", "au\u00b7gen", "sind", "der", "lie\u00b7be", "mu\u00b7schel\u00b7hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von dannen stieg sie n\u00e4chst in meinem hertzen aus.", "tokens": ["Von", "dan\u00b7nen", "stieg", "sie", "n\u00e4chst", "in", "mei\u00b7nem", "hert\u00b7zen", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}