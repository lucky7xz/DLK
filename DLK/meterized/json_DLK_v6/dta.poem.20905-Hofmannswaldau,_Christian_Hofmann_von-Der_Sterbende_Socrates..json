{"dta.poem.20905": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Der Klo\u00df ist rund nach meinem D\u00fcncken/", "tokens": ["Der", "Klo\u00df", "ist", "rund", "nach", "mei\u00b7nem", "D\u00fcn\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Himmel schliessen ihn recht ein/", "tokens": ["Die", "Him\u00b7mel", "schlies\u00b7sen", "ihn", "recht", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er mu\u00df stets unbeweglich seyn/", "tokens": ["Er", "mu\u00df", "stets", "un\u00b7be\u00b7weg\u00b7lich", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein gleicher Hang l\u00e4st ihn nicht sincken.", "tokens": ["Sein", "glei\u00b7cher", "Hang", "l\u00e4st", "ihn", "nicht", "sin\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Himmel so gleich um ihn gehn/", "tokens": ["Die", "Him\u00b7mel", "so", "gleich", "um", "ihn", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "APPR", "PPER", "VVINF", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Die lassen ihn schnur-gleiche stehn;", "tokens": ["Die", "las\u00b7sen", "ihn", "schnur\u00b7glei\u00b7che", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und weil er auf ihn selbst kan sitzen/", "tokens": ["Und", "weil", "er", "auf", "ihn", "selbst", "kan", "sit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und gleich getheilt Gewichte hat/", "tokens": ["Und", "gleich", "ge\u00b7theilt", "Ge\u00b7wich\u00b7te", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So h\u00e4lt er sich ohn andre St\u00fctzen/", "tokens": ["So", "h\u00e4lt", "er", "sich", "ohn", "and\u00b7re", "St\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Als des Gerichtes Wunderthat.", "tokens": ["Als", "des", "Ge\u00b7rich\u00b7tes", "Wun\u00b7der\u00b7that", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}