{"textgrid.poem.42264": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Der versetzte Himmel", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Licht und Lust des Himmels zu erschauen,", "tokens": ["Licht", "und", "Lust", "des", "Him\u00b7mels", "zu", "er\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo hinan des Frommen W\u00fcnsche schweben,", "tokens": ["Wo", "hi\u00b7nan", "des", "From\u00b7men", "W\u00fcn\u00b7sche", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mu\u00df dein Blick sich \u00fcber dich erheben,", "tokens": ["Mu\u00df", "dein", "Blick", "sich", "\u00fc\u00b7ber", "dich", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie des Betenden voll Gottvertrauen.", "tokens": ["Wie", "des", "Be\u00b7ten\u00b7den", "voll", "Gott\u00b7ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Unter dir ist Todesnacht und Grauen.", "tokens": ["Un\u00b7ter", "dir", "ist", "To\u00b7des\u00b7nacht", "und", "Grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "W\u00fcrde dir ein Blick hinab gegeben,", "tokens": ["W\u00fcr\u00b7de", "dir", "ein", "Blick", "hin\u00b7ab", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "So gewahrtest du mit Angst und Beben", "tokens": ["So", "ge\u00b7wahr\u00b7test", "du", "mit", "Angst", "und", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Das Gebiet der H\u00f6ll' und Satans Klauen.", "tokens": ["Das", "Ge\u00b7biet", "der", "H\u00f6ll'", "und", "Sa\u00b7tans", "Klau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Also spricht gemeiner Menschenglaube.", "tokens": ["Al\u00b7so", "spricht", "ge\u00b7mei\u00b7ner", "Men\u00b7schen\u00b7glau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aber wann aus meines Armes Wiege", "tokens": ["A\u00b7ber", "wann", "aus", "mei\u00b7nes", "Ar\u00b7mes", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Molly's Blick empor nach meinem schmachtet:", "tokens": ["Mol\u00b7ly's", "Blick", "em\u00b7por", "nach", "mei\u00b7nem", "schmach\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Wei\u00df ich, da\u00df im Auge meiner Taube", "tokens": ["Wei\u00df", "ich", ",", "da\u00df", "im", "Au\u00b7ge", "mei\u00b7ner", "Tau\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aller Himmelsseligkeit Gen\u00fcge", "tokens": ["Al\u00b7ler", "Him\u00b7mels\u00b7se\u00b7lig\u00b7keit", "Ge\u00b7n\u00fc\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Unter mir der trunkne Blick betrachtet.", "tokens": ["Un\u00b7ter", "mir", "der", "trunk\u00b7ne", "Blick", "be\u00b7trach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}