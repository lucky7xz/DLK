{"textgrid.poem.59824": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jenem Tage zum Ged\u00e4chtnis", "tokens": ["Je\u00b7nem", "Ta\u00b7ge", "zum", "Ge\u00b7d\u00e4cht\u00b7nis"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sei ein langer Trunk gemacht,", "tokens": ["Sei", "ein", "lan\u00b7ger", "Trunk", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo vom Bethaus in die Schenke", "tokens": ["Wo", "vom", "Be\u00b7thaus", "in", "die", "Schen\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Ich den ersten Sprung gemacht!", "tokens": ["Ich", "den", "ers\u00b7ten", "Sprung", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "War verdummt in blinder Demut,", "tokens": ["War", "ver\u00b7dummt", "in", "blin\u00b7der", "De\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War gealtert wie ein Greis \u2013", "tokens": ["War", "ge\u00b7al\u00b7tert", "wie", "ein", "Greis", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KOKOM", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber Wein, Gesang und Liebe", "tokens": ["A\u00b7ber", "Wein", ",", "Ge\u00b7sang", "und", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat mich wieder jung gemacht!", "tokens": ["Hat", "mich", "wie\u00b7der", "jung", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Trink, Mirza-Schaffy, berausche", "tokens": ["Trink", ",", "Mir\u00b7za\u00b7Schaffy", ",", "be\u00b7rau\u00b7sche"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NE", "$,", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich in Liebe, Sang und Wein!", "tokens": ["Dich", "in", "Lie\u00b7be", ",", "Sang", "und", "Wein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur im Rausch sind deine Lieder", "tokens": ["Nur", "im", "Rausch", "sind", "dei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So voll Glut und Schwung gemacht!", "tokens": ["So", "voll", "Glut", "und", "Schwung", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "KON", "NN", "VVPP", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}