{"textgrid.poem.41510": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Gru\u00df an Wilhelm Junkmann", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein L\u00e4mpchen zuckt, sein Docht verglimmt,", "tokens": ["Mein", "L\u00e4mp\u00b7chen", "zuckt", ",", "sein", "Docht", "ver\u00b7glimmt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Funken knistern im Kamine,", "tokens": ["Die", "Fun\u00b7ken", "knis\u00b7tern", "im", "Ka\u00b7mi\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie eine Nebeldecke schwimmt", "tokens": ["Wie", "ei\u00b7ne", "Ne\u00b7bel\u00b7de\u00b7cke", "schwimmt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es an des Saales hoher B\u00fchne;", "tokens": ["Es", "an", "des", "Saa\u00b7les", "ho\u00b7her", "B\u00fch\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Im Schneegest\u00f6ber schl\u00e4ft die Luft,", "tokens": ["Im", "Schnee\u00b7ge\u00b7st\u00f6\u00b7ber", "schl\u00e4ft", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am Scheite ist das Harz entglommen,", "tokens": ["Am", "Schei\u00b7te", "ist", "das", "Harz", "ent\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mich d\u00fcnkt, als sp\u00fcr' ich einen Duft", "tokens": ["Mich", "d\u00fcnkt", ",", "als", "sp\u00fcr'", "ich", "ei\u00b7nen", "Duft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie Weihrauch an der Gruft des Frommen.", "tokens": ["Wie", "Weih\u00b7rauch", "an", "der", "Gruft", "des", "From\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dies ist die Stunde, das Gemach,", "tokens": ["Dies", "ist", "die", "Stun\u00b7de", ",", "das", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo sich Gedanken m\u00f6gen wiegen,", "tokens": ["Wo", "sich", "Ge\u00b7dan\u00b7ken", "m\u00f6\u00b7gen", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verklungne Laute hallen nach,", "tokens": ["Ver\u00b7klung\u00b7ne", "Lau\u00b7te", "hal\u00b7len", "nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es d\u00e4mmert in verloschnen Z\u00fcgen;", "tokens": ["Es", "d\u00e4m\u00b7mert", "in", "ver\u00b7lo\u00b7schnen", "Z\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Im Hirne summt es, wie ein Lied", "tokens": ["Im", "Hir\u00b7ne", "summt", "es", ",", "wie", "ein", "Lied"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das mit den Flocken m\u00f6chte steigen,", "tokens": ["Das", "mit", "den", "Flo\u00b7cken", "m\u00f6ch\u00b7te", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und, fl\u00fcsternd wie der Hauch im Ried,", "tokens": ["Und", ",", "fl\u00fcs\u00b7ternd", "wie", "der", "Hauch", "im", "Ried", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVPP", "KOKOM", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "An eines Freundes Locke neigen.", "tokens": ["An", "ei\u00b7nes", "Freun\u00b7des", "Lo\u00b7cke", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schon seh ich ihn, im gelben Licht,", "tokens": ["Schon", "seh", "ich", "ihn", ",", "im", "gel\u00b7ben", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das seines Ofens Flamme spielet,", "tokens": ["Das", "sei\u00b7nes", "O\u00b7fens", "Flam\u00b7me", "spie\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er selbst ein wunderlich Gedicht,", "tokens": ["Er", "selbst", "ein", "wun\u00b7der\u00b7lich", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Begriffen schwer, doch leicht gef\u00fchlet.", "tokens": ["Be\u00b7grif\u00b7fen", "schwer", ",", "doch", "leicht", "ge\u00b7f\u00fch\u00b7let", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich seh ihn, wie, die Stirn gest\u00fctzt,", "tokens": ["Ich", "seh", "ihn", ",", "wie", ",", "die", "Stirn", "ge\u00b7st\u00fctzt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er leise l\u00e4chelt in Gedanken;", "tokens": ["Er", "lei\u00b7se", "l\u00e4\u00b7chelt", "in", "Ge\u00b7dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wo weilen sie? wo bl\u00fchen itzt", "tokens": ["Wo", "wei\u00b7len", "sie", "?", "wo", "bl\u00fc\u00b7hen", "itzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$.", "PWAV", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und treiben diese zarten Ranken?", "tokens": ["Und", "trei\u00b7ben", "die\u00b7se", "zar\u00b7ten", "Ran\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Baun sie im schlichten Heidekraut", "tokens": ["Baun", "sie", "im", "schlich\u00b7ten", "Hei\u00b7de\u00b7kraut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Nestchen sich aus Immortellen?", "tokens": ["Ihr", "Nest\u00b7chen", "sich", "aus", "Im\u00b7mor\u00b7tel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sind mit der Flocke sie getaut", "tokens": ["Sind", "mit", "der", "Flo\u00b7cke", "sie", "ge\u00b7taut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Tr\u00e4ne, wo die Gr\u00e4ber schwellen?", "tokens": ["Als", "Tr\u00e4\u00b7ne", ",", "wo", "die", "Gr\u00e4\u00b7ber", "schwel\u00b7len", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vielleicht in fernes fernes Land", "tokens": ["Viel\u00b7leicht", "in", "fer\u00b7nes", "fer\u00b7nes", "Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Nachtigallen fortgezogen,", "tokens": ["Wie", "Nach\u00b7ti\u00b7gal\u00b7len", "fort\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Oder am heil'gen Meeresstrand,", "tokens": ["O\u00b7der", "am", "heil'\u00b7gen", "Mee\u00b7res\u00b7strand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gleich der Morgana auf den Wogen.", "tokens": ["Gleich", "der", "Mor\u00b7ga\u00b7na", "auf", "den", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NE", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Ihm hat Begeistrung, ein Orkan,", "tokens": ["Ihm", "hat", "Be\u00b7geis\u00b7trung", ",", "ein", "Or\u00b7kan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Des Lebens Zedern nicht gebeuget,", "tokens": ["Des", "Le\u00b7bens", "Ze\u00b7dern", "nicht", "ge\u00b7beu\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht sah er sie als Flamme nahn,", "tokens": ["Nicht", "sah", "er", "sie", "als", "Flam\u00b7me", "nahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "PPER", "KOUS", "NN", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die lodernd durch den Urwald steiget;", "tokens": ["Die", "lo\u00b7dernd", "durch", "den", "Ur\u00b7wald", "stei\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nein, als entschlief der Morgenwind,", "tokens": ["Nein", ",", "als", "ent\u00b7schlief", "der", "Mor\u00b7gen\u00b7wind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am Strauche summten fromme Bienen,", "tokens": ["Am", "Strau\u00b7che", "summ\u00b7ten", "from\u00b7me", "Bie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da ist der Herr im S\u00e4useln lind", "tokens": ["Da", "ist", "der", "Herr", "im", "S\u00e4u\u00b7seln", "lind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gleich dem Elias ihm erschienen.", "tokens": ["Gleich", "dem", "E\u00b7lias", "ihm", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Und wie er sitzt, so vorgebeugt,", "tokens": ["Und", "wie", "er", "sitzt", ",", "so", "vor\u00b7ge\u00b7beugt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die hohe Stirn vom Schein umflossen,", "tokens": ["Die", "ho\u00b7he", "Stirn", "vom", "Schein", "um\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Ohr wie fremden T\u00f6nen neigt,", "tokens": ["Das", "Ohr", "wie", "frem\u00b7den", "T\u00f6\u00b7nen", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und l\u00e4chelt geistigen Genossen,", "tokens": ["Und", "l\u00e4\u00b7chelt", "geis\u00b7ti\u00b7gen", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein lichter Blitz in seinem Aug',", "tokens": ["Ein", "lich\u00b7ter", "Blitz", "in", "sei\u00b7nem", "Aug'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie ein verirrter Strahl aus Eden, \u2013", "tokens": ["Wie", "ein", "ver\u00b7irr\u00b7ter", "Strahl", "aus", "E\u00b7den", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da m\u00f6cht' ich leise, leise auch", "tokens": ["Da", "m\u00f6cht'", "ich", "lei\u00b7se", ",", "lei\u00b7se", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "$,", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als \u00c4olsharfe zu ihm reden.", "tokens": ["Als", "\u00c4\u00b7ols\u00b7har\u00b7fe", "zu", "ihm", "re\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}