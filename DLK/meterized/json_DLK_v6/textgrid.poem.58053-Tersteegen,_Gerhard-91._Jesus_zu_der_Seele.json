{"textgrid.poem.58053": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "91. Jesus zu der Seele", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stets gehst du in dir selbst mit deinen Lasten tragen;", "tokens": ["Stets", "gehst", "du", "in", "dir", "selbst", "mit", "dei\u00b7nen", "Las\u00b7ten", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bin ich doch immer nah, kannst du es mir nicht sagen?", "tokens": ["Bin", "ich", "doch", "im\u00b7mer", "nah", ",", "kannst", "du", "es", "mir", "nicht", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "VMFIN", "PPER", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Willst du dir helfen selbst, du arme Kreatur?", "tokens": ["Willst", "du", "dir", "hel\u00b7fen", "selbst", ",", "du", "ar\u00b7me", "Kre\u00b7a\u00b7tur", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVFIN", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich helf' dir ja so gern; sei offenherzig nur!", "tokens": ["Ich", "hel\u00b7f'", "dir", "ja", "so", "gern", ";", "sei", "of\u00b7fen\u00b7her\u00b7zig", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$.", "VAFIN", "ADJD", "ADV", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}