{"textgrid.poem.56282": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Die Mutter und das Kind", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ward zu solchem Jammer", "tokens": ["Wie", "ward", "zu", "sol\u00b7chem", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der stolzen Mutter Lust?", "tokens": ["Der", "stol\u00b7zen", "Mut\u00b7ter", "Lust", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie weint in \u00f6der Kammer,", "tokens": ["Sie", "weint", "in", "\u00f6\u00b7der", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Kind an ihrer Brust;", "tokens": ["Kein", "Kind", "an", "ih\u00b7rer", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Kind gebettet haben", "tokens": ["Das", "Kind", "ge\u00b7bet\u00b7tet", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie in den schwarzen Schrein,", "tokens": ["Sie", "in", "den", "schwar\u00b7zen", "Schrein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und tief den Schrein vergraben,", "tokens": ["Und", "tief", "den", "Schrein", "ver\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als m\u00fc\u00dft es also sein.", "tokens": ["Als", "m\u00fc\u00dft", "es", "al\u00b7so", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie da die Erde fallend", "tokens": ["Wie", "da", "die", "Er\u00b7de", "fal\u00b7lend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf den versenkten Sarg", "tokens": ["Auf", "den", "ver\u00b7senk\u00b7ten", "Sarg"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihn dumpf und schaurig schallend", "tokens": ["Ihn", "dumpf", "und", "schau\u00b7rig", "schal\u00b7lend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor ihren Augen barg,", "tokens": ["Vor", "ih\u00b7ren", "Au\u00b7gen", "barg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hat Tr\u00e4nen sie gefunden,", "tokens": ["Hat", "Tr\u00e4\u00b7nen", "sie", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die nicht zu hemmen sind,", "tokens": ["Die", "nicht", "zu", "hem\u00b7men", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie weint zu allen Stunden", "tokens": ["Sie", "weint", "zu", "al\u00b7len", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Um ihr geliebtes Kind.", "tokens": ["Um", "ihr", "ge\u00b7lieb\u00b7tes", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wann andrer Lust und Sorgen", "tokens": ["Wann", "an\u00b7drer", "Lust", "und", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der laute Tag bescheint,", "tokens": ["Der", "lau\u00b7te", "Tag", "be\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weilt schweigsam sie verborgen", "tokens": ["Weilt", "schweig\u00b7sam", "sie", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In finstrer Klaus und weint;", "tokens": ["In", "finst\u00b7rer", "Klaus", "und", "weint", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wann andrer Schmerzen lindert", "tokens": ["Wann", "an\u00b7drer", "Schmer\u00b7zen", "lin\u00b7dert"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Nacht, und alles ruht,", "tokens": ["Die", "Nacht", ",", "und", "al\u00b7les", "ruht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vergie\u00dft sie ungehindert", "tokens": ["Ver\u00b7gie\u00dft", "sie", "un\u00b7ge\u00b7hin\u00b7dert"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Tr\u00e4nen bittre Flut.", "tokens": ["Der", "Tr\u00e4\u00b7nen", "bitt\u00b7re", "Flut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie einst sie unter Tr\u00e4nen", "tokens": ["Wie", "einst", "sie", "un\u00b7ter", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die stumme Mitternacht", "tokens": ["Die", "stum\u00b7me", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In hoffnungslosem Sehnen", "tokens": ["In", "hoff\u00b7nungs\u00b7lo\u00b7sem", "Seh\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verst\u00f6rt herangewacht,", "tokens": ["Ver\u00b7st\u00f6rt", "her\u00b7an\u00b7ge\u00b7wacht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sieht wunderbarer Weise", "tokens": ["Sieht", "wun\u00b7der\u00b7ba\u00b7rer", "Wei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Kindlein sie sich nahn,", "tokens": ["Das", "Kin\u00b7dlein", "sie", "sich", "nahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PRF", "ADJA", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es tritt so leise, leise,", "tokens": ["Es", "tritt", "so", "lei\u00b7se", ",", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es sieht sie trauernd an.", "tokens": ["Es", "sieht", "sie", "trau\u00b7ernd", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O Mutter, in der Erden", "tokens": ["O", "Mut\u00b7ter", ",", "in", "der", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gewinn ich keine Rast,", "tokens": ["Ge\u00b7winn", "ich", "kei\u00b7ne", "Rast", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie sollt ich ruhig werden,", "tokens": ["Wie", "sollt", "ich", "ru\u00b7hig", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn du geweinet hast?", "tokens": ["Wenn", "du", "ge\u00b7wei\u00b7net", "hast", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Tr\u00e4nen f\u00fchl ich rinnen", "tokens": ["Die", "Tr\u00e4\u00b7nen", "f\u00fchl", "ich", "rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu mir ohn Unterla\u00df,", "tokens": ["Zu", "mir", "ohn", "Un\u00b7ter\u00b7la\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein Hemdlein und das Linnen,", "tokens": ["Mein", "Hemd\u00b7lein", "und", "das", "Lin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sie sind davon so na\u00df.", "tokens": ["Sie", "sind", "da\u00b7von", "so", "na\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O Mutter, la\u00df dein L\u00e4cheln", "tokens": ["O", "Mut\u00b7ter", ",", "la\u00df", "dein", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVIMP", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinab ins feuchte Haus", "tokens": ["Hin\u00b7ab", "ins", "feuch\u00b7te", "Haus"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir laue L\u00fcfte f\u00e4cheln,", "tokens": ["Mir", "lau\u00b7e", "L\u00fcf\u00b7te", "f\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann trocknet's wieder aus,", "tokens": ["Dann", "trock\u00b7net's", "wie\u00b7der", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und scheinet deinem Kinde", "tokens": ["Und", "schei\u00b7net", "dei\u00b7nem", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein Auge wieder klar,", "tokens": ["Dein", "Au\u00b7ge", "wie\u00b7der", "klar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Umbl\u00fchn es Ros und Winde,", "tokens": ["Um\u00b7bl\u00fchn", "es", "Ros", "und", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie sonst es oben war.", "tokens": ["Wie", "sonst", "es", "o\u00b7ben", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O weine nicht! sei munter!", "tokens": ["O", "wei\u00b7ne", "nicht", "!", "sei", "mun\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$.", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was helfen Tr\u00e4nen dir?", "tokens": ["Was", "hel\u00b7fen", "Tr\u00e4\u00b7nen", "dir", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Komm lieber doch hinunter", "tokens": ["Komm", "lie\u00b7ber", "doch", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und lege dich zu mir;", "tokens": ["Und", "le\u00b7ge", "dich", "zu", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da magst du leise kosen", "tokens": ["Da", "magst", "du", "lei\u00b7se", "ko\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit deinem Kindelein,", "tokens": ["Mit", "dei\u00b7nem", "Kin\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du liegst auf weichen Rosen", "tokens": ["Du", "liegst", "auf", "wei\u00b7chen", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und schl\u00e4fst so ruhig ein.", "tokens": ["Und", "schl\u00e4fst", "so", "ru\u00b7hig", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie hat aus s\u00fc\u00dfem Munde", "tokens": ["Sie", "hat", "aus", "s\u00fc\u00b7\u00dfem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Warnung wohl geh\u00f6rt,", "tokens": ["Die", "War\u00b7nung", "wohl", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat von dieser Stunde", "tokens": ["Sie", "hat", "von", "die\u00b7ser", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu weinen aufgeh\u00f6rt.", "tokens": ["Zu", "wei\u00b7nen", "auf\u00b7ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wohl bleichten ihre Wangen,", "tokens": ["Wohl", "bleich\u00b7ten", "ih\u00b7re", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch blieb ihr Auge klar;", "tokens": ["Doch", "blieb", "ihr", "Au\u00b7ge", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie ist hinab gegangen,", "tokens": ["Sie", "ist", "hin\u00b7ab", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wo schon ihr Liebling war.", "tokens": ["Wo", "schon", "ihr", "Lieb\u00b7ling", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}