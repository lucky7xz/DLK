{"textgrid.poem.52934": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo Du einst, im Arm die Harfe, gingest Deine Dichterpfade,", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo Du einst, im Arm die Harfe, gingest Deine Dichterpfade,", "tokens": ["Wo", "Du", "einst", ",", "im", "Arm", "die", "Har\u00b7fe", ",", "gin\u00b7gest", "Dei\u00b7ne", "Dich\u00b7ter\u00b7pfa\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "$,", "APPRART", "NN", "ART", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Durch die Kaiserstadt und l\u00e4ngs der Donau lustigem Gestade,", "tokens": ["Durch", "die", "Kai\u00b7ser\u00b7stadt", "und", "l\u00e4ngs", "der", "Do\u00b7nau", "lus\u00b7ti\u00b7gem", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "ART", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Bin ich j\u00fcngst Dir nachgeschritten, treulich und mit frommem Fu\u00df,", "tokens": ["Bin", "ich", "j\u00fcngst", "Dir", "nach\u00b7ge\u00b7schrit\u00b7ten", ",", "treu\u00b7lich", "und", "mit", "from\u00b7mem", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "PPER", "VVFIN", "$,", "ADJD", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Dich im Munde, Dich im Herzen, edler Anastasius!", "tokens": ["Dich", "im", "Mun\u00b7de", ",", "Dich", "im", "Her\u00b7zen", ",", "ed\u00b7ler", "A\u00b7nas\u00b7ta\u00b7si\u00b7us", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "PPER", "APPRART", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "War mir doch, als ob die Welle gr\u00fc\u00dfend Deinen Namen rauschte,", "tokens": ["War", "mir", "doch", ",", "als", "ob", "die", "Wel\u00b7le", "gr\u00fc\u00b7\u00dfend", "Dei\u00b7nen", "Na\u00b7men", "rauschte", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "KOKOM", "KOUS", "ART", "NN", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ob Dein Auge, gro\u00df und feurig, aus dem Gr\u00fcn der Reben lauschte,", "tokens": ["Ob", "Dein", "Au\u00b7ge", ",", "gro\u00df", "und", "feu\u00b7rig", ",", "aus", "dem", "Gr\u00fcn", "der", "Re\u00b7ben", "lauschte", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Um den Kalenberg ergo\u00df sich und den Felsen Leopold", "tokens": ["Um", "den", "Ka\u00b7len\u00b7berg", "er\u00b7go\u00df", "sich", "und", "den", "Fel\u00b7sen", "Leo\u00b7pold"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "VVFIN", "PRF", "KON", "ART", "NN", "NE"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Deiner Dichtung lichter Nimbus und der Abendsonne Gold.", "tokens": ["Dei\u00b7ner", "Dich\u00b7tung", "lich\u00b7ter", "Nim\u00b7bus", "und", "der", "A\u00b7bend\u00b7son\u00b7ne", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NE", "KON", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Ja, es waren diese B\u00e4ume, die um Deine Stirn ges\u00e4uselt,", "tokens": ["Ja", ",", "es", "wa\u00b7ren", "die\u00b7se", "B\u00e4u\u00b7me", ",", "die", "um", "Dei\u00b7ne", "Stirn", "ge\u00b7s\u00e4u\u00b7selt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PDAT", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Hier am S\u00f6ller hat der Nachtwind Deine Locken k\u00fchl durchkr\u00e4uselt,", "tokens": ["Hier", "am", "S\u00f6l\u00b7ler", "hat", "der", "Nacht\u00b7wind", "Dei\u00b7ne", "Lo\u00b7cken", "k\u00fchl", "durch\u00b7kr\u00e4u\u00b7selt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "ART", "NN", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Dort hast Du geruht im Grase, ewiger Gedanken voll,", "tokens": ["Dort", "hast", "Du", "ge\u00b7ruht", "im", "Gra\u00b7se", ",", "e\u00b7wi\u00b7ger", "Ge\u00b7dan\u00b7ken", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "APPRART", "NN", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Als das hohe Lied vom Fr\u00fchling gl\u00fchend Deiner Brust entquoll.", "tokens": ["Als", "das", "ho\u00b7he", "Lied", "vom", "Fr\u00fch\u00b7ling", "gl\u00fc\u00b7hend", "Dei\u00b7ner", "Brust", "ent\u00b7quoll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPRART", "NN", "ADJD", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Aber, Wunder! wo Du gingest, \u00fcber Dornen und Gebeine,", "tokens": ["A\u00b7ber", ",", "Wun\u00b7der", "!", "wo", "Du", "gin\u00b7gest", ",", "\u00fc\u00b7ber", "Dor\u00b7nen", "und", "Ge\u00b7bei\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$.", "PWAV", "PPER", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Keimten unter Deinen Schritten Blumen aus dem d\u00fcrren Steine,", "tokens": ["Ke\u00b7im\u00b7ten", "un\u00b7ter", "Dei\u00b7nen", "Schrit\u00b7ten", "Blu\u00b7men", "aus", "dem", "d\u00fcr\u00b7ren", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPOSAT", "NN", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.3": {"text": "Und Dein Blut, die Spur des Weges, das auf leere Bl\u00e4tter flo\u00df,", "tokens": ["Und", "Dein", "Blut", ",", "die", "Spur", "des", "We\u00b7ges", ",", "das", "auf", "lee\u00b7re", "Bl\u00e4t\u00b7ter", "flo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Sieh, wie es in rote Rosen \u00fcberall befruchtend spro\u00df!", "tokens": ["Sieh", ",", "wie", "es", "in", "ro\u00b7te", "Ro\u00b7sen", "\u00fc\u00b7be\u00b7rall", "be\u00b7fruch\u00b7tend", "spro\u00df", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "APPR", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "+---+-+-+-+-+-+", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Das ist wahrer Dichtersegen: auch den Schutt in Brot verwandeln,", "tokens": ["Das", "ist", "wah\u00b7rer", "Dich\u00b7ter\u00b7se\u00b7gen", ":", "auch", "den", "Schutt", "in", "Brot", "ver\u00b7wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$.", "ADV", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Brunnen zaubern aus dem Felsen, und, wo andre reimen, handeln;", "tokens": ["Brun\u00b7nen", "zau\u00b7bern", "aus", "dem", "Fel\u00b7sen", ",", "und", ",", "wo", "and\u00b7re", "rei\u00b7men", ",", "han\u00b7deln", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "$,", "PWAV", "PIS", "VVINF", "$,", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ein Poet in Werk und Worten tatest Du wie keiner tat,", "tokens": ["Ein", "Po\u00b7et", "in", "Werk", "und", "Wor\u00b7ten", "ta\u00b7test", "Du", "wie", "kei\u00b7ner", "tat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "PPER", "KOKOM", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Daf\u00fcr reift auch rings im Lande tausendf\u00e4ltig Deine Saat!", "tokens": ["Da\u00b7f\u00fcr", "reift", "auch", "rings", "im", "Lan\u00b7de", "tau\u00b7send\u00b7f\u00e4l\u00b7tig", "Dei\u00b7ne", "Saat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ADV", "APPRART", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Und da\u00df unter Korn und Blumen auch die Schlange Dir nicht fehle,", "tokens": ["Und", "da\u00df", "un\u00b7ter", "Korn", "und", "Blu\u00b7men", "auch", "die", "Schlan\u00b7ge", "Dir", "nicht", "feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "NN", "KON", "NN", "ADV", "ART", "NN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Zischt nun heimlich die Verleumdung um die offne Dichterseele,", "tokens": ["Zischt", "nun", "heim\u00b7lich", "die", "Ver\u00b7leum\u00b7dung", "um", "die", "off\u00b7ne", "Dich\u00b7ter\u00b7see\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Der Verdacht mit Lauerblicken schleicht er um Dein sichres Haus,", "tokens": ["Der", "Ver\u00b7dacht", "mit", "Lau\u00b7er\u00b7bli\u00b7cken", "schleicht", "er", "um", "Dein", "sich\u00b7res", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und weil Du in Liebe schlummerst, schreit er Dich f\u00fcr \u2013 scheintot aus.", "tokens": ["Und", "weil", "Du", "in", "Lie\u00b7be", "schlum\u00b7merst", ",", "schreit", "er", "Dich", "f\u00fcr", "\u2013", "schein\u00b7tot", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,", "KOUS", "PPER", "PRF", "APPR", "$(", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.7": {"line.1": {"text": "Tritt ihn nieder, letzter Ritter, diesen schadenfrohen Drachen,", "tokens": ["Tritt", "ihn", "nie\u00b7der", ",", "letz\u00b7ter", "Rit\u00b7ter", ",", "die\u00b7sen", "scha\u00b7den\u00b7fro\u00b7hen", "Dra\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ADJA", "NN", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Komm, da\u00df wir die elke L\u00fcge durch ein Lied zu Schande machen,", "tokens": ["Komm", ",", "da\u00df", "wir", "die", "el\u00b7ke", "L\u00fc\u00b7ge", "durch", "ein", "Lied", "zu", "Schan\u00b7de", "ma\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Sag es, da\u00df Du nimmer treulos uns und Dir gewesen bist,", "tokens": ["Sag", "es", ",", "da\u00df", "Du", "nim\u00b7mer", "treu\u00b7los", "uns", "und", "Dir", "ge\u00b7we\u00b7sen", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "ADV", "ADJD", "PPER", "KON", "PPER", "VAPP", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Da\u00df Dein Dichterschild so rein noch, wie Dein Grafenwappen ist!", "tokens": ["Da\u00df", "Dein", "Dich\u00b7ter\u00b7schild", "so", "rein", "noch", ",", "wie", "Dein", "Gra\u00b7fen\u00b7wap\u00b7pen", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "ADV", "$,", "PWAV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "O sie will es nie begreifen, ihre Prosa und Gemeinheit,", "tokens": ["O", "sie", "will", "es", "nie", "be\u00b7grei\u00b7fen", ",", "ih\u00b7re", "Pro\u00b7sa", "und", "Ge\u00b7mein\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df ein Geist wie Du, ein Name b\u00fcrgt f\u00fcr der Gesinnung Reinheit,", "tokens": ["Da\u00df", "ein", "Geist", "wie", "Du", ",", "ein", "Na\u00b7me", "b\u00fcrgt", "f\u00fcr", "der", "Ge\u00b7sin\u00b7nung", "Rein\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KOKOM", "PPER", "$,", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Nur das Schlechte glaubt sie willig, und wo wer zu wanken droht,", "tokens": ["Nur", "das", "Schlech\u00b7te", "glaubt", "sie", "wil\u00b7lig", ",", "und", "wo", "wer", "zu", "wan\u00b7ken", "droht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,", "KON", "PWAV", "PWS", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Zerrt sie ihn mit frechem Jubel zu sich nieder in den Kot.", "tokens": ["Zerrt", "sie", "ihn", "mit", "fre\u00b7chem", "Ju\u00b7bel", "zu", "sich", "nie\u00b7der", "in", "den", "Kot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "APPR", "PRF", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Du erliege nicht und weiche ihren Stein- und Hagel-W\u00fcrfen,", "tokens": ["Du", "er\u00b7lie\u00b7ge", "nicht", "und", "wei\u00b7che", "ih\u00b7ren", "Stein", "und", "Ha\u00b7gel\u00b7W\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KON", "ADJA", "PPOSAT", "TRUNC", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wisse, da\u00df Dir alle trauen, die sich selbst noch trauen d\u00fcrfen.", "tokens": ["Wis\u00b7se", ",", "da\u00df", "Dir", "al\u00b7le", "trau\u00b7en", ",", "die", "sich", "selbst", "noch", "trau\u00b7en", "d\u00fcr\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PIS", "VVINF", "$,", "PRELS", "PRF", "ADV", "ADV", "VVINF", "VMINF", "$."], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Aber weh, wenn erst der Dichter an dem Dichter zweifeln mu\u00df \u2013\u2013", "tokens": ["A\u00b7ber", "weh", ",", "wenn", "erst", "der", "Dich\u00b7ter", "an", "dem", "D