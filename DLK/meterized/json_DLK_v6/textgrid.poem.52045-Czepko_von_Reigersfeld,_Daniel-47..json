{"textgrid.poem.52045": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es m\u00fc\u00dfen Rosen dir auff deiner Taffel bl\u00fchn,", "tokens": ["Es", "m\u00fc\u00b7\u00dfen", "Ro\u00b7sen", "dir", "auff", "dei\u00b7ner", "Taf\u00b7fel", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die der gelehrte Koch mit Speisen wei\u00df zu zieren:", "tokens": ["Die", "der", "ge\u00b7lehr\u00b7te", "Koch", "mit", "Spei\u00b7sen", "wei\u00df", "zu", "zie\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man sieht sechs sch\u00f6ne Ro\u00df an deiner Kutschen ziehn,", "tokens": ["Man", "sieht", "sechs", "sch\u00f6\u00b7ne", "Ro\u00df", "an", "dei\u00b7ner", "Kut\u00b7schen", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "CARD", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und so viel Bey Pferd auch an Reuters H\u00e4nden f\u00fchren:", "tokens": ["Und", "so", "viel", "Bey", "Pferd", "auch", "an", "Reu\u00b7ters", "H\u00e4n\u00b7den", "f\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "NN", "ADV", "APPR", "NE", "NN", "VVINF", "$."], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Knaben dienen dir in gleicher Lieberey;", "tokens": ["Die", "Kna\u00b7ben", "die\u00b7nen", "dir", "in", "glei\u00b7cher", "Lie\u00b7be\u00b7rey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man h\u00f6rt dir die Trompet und auch die Laute klingen:", "tokens": ["Man", "h\u00f6rt", "dir", "die", "Trom\u00b7pet", "und", "auch", "die", "Lau\u00b7te", "klin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "NN", "KON", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Rhein l\u00e4st Baccharach, die Tonau ihr Tockay,", "tokens": ["Der", "Rhein", "l\u00e4st", "Bac\u00b7cha\u00b7rach", ",", "die", "To\u00b7nau", "ihr", "Toc\u00b7kay", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "NE", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wann du es nur befiehlst in deine Schaale springen.", "tokens": ["Wann", "du", "es", "nur", "be\u00b7fiehlst", "in", "dei\u00b7ne", "Schaa\u00b7le", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es sieht ja Herrisch aus: doch niemand wils verstehn,", "tokens": ["Es", "sieht", "ja", "Her\u00b7risch", "aus", ":", "doch", "nie\u00b7mand", "wils", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "PTKVZ", "$.", "ADV", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann soltu es vollf\u00fchrn, so must du sachter prahlen,", "tokens": ["Dann", "sol\u00b7tu", "es", "voll\u00b7f\u00fchrn", ",", "so", "must", "du", "sach\u00b7ter", "prah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es d\u00fcrfft' in kurtzer Zeit der bleiche Schuldner gehn,", "tokens": ["Es", "d\u00fcrfft'", "in", "kurt\u00b7zer", "Zeit", "der", "blei\u00b7che", "Schuld\u00b7ner", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und bey dem Richter schreyn; Er sol mich auch noch zahlen!", "tokens": ["Und", "bey", "dem", "Rich\u00b7ter", "schreyn", ";", "Er", "sol", "mich", "auch", "noch", "zah\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAINF", "$.", "PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}