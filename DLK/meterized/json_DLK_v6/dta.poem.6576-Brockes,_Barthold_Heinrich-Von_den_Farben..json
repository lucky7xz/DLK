{"dta.poem.6576": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Farben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man sieht es sonderlich, wenn C\u00f6rper ferne liegen,", "tokens": ["Man", "sieht", "es", "son\u00b7der\u00b7lich", ",", "wenn", "C\u00f6r\u00b7per", "fer\u00b7ne", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df starcke Farben stets die schw\u00e4cheren besiegen.", "tokens": ["Da\u00df", "star\u00b7cke", "Far\u00b7ben", "stets", "die", "schw\u00e4\u00b7che\u00b7ren", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn wir ein sch\u00f6nes Feld von fern erblicken,", "tokens": ["Wenn", "wir", "ein", "sch\u00f6\u00b7nes", "Feld", "von", "fern", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das von so mancher Art viel tausend Blumen schm\u00fccken,", "tokens": ["Das", "von", "so", "man\u00b7cher", "Art", "viel", "tau\u00b7send", "Blu\u00b7men", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADV", "PIAT", "NN", "ADV", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und etwan roth und gelb mit weissen sich verbinden;", "tokens": ["Und", "et\u00b7wan", "roth", "und", "gelb", "mit", "weis\u00b7sen", "sich", "ver\u00b7bin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "KON", "ADJD", "APPR", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wird roth und gelb von weiten gantz verschwinden.", "tokens": ["Wird", "roth", "und", "gelb", "von", "wei\u00b7ten", "gantz", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "APPR", "ADJA", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es ist das ebne Feld nicht anders anzusehn,", "tokens": ["Es", "ist", "das", "eb\u00b7ne", "Feld", "nicht", "an\u00b7ders", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PTKNEG", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als wenn auf gr\u00fcnem Grund, von Silber, Bluhmen stehn.", "tokens": ["Als", "wenn", "auf", "gr\u00fc\u00b7nem", "Grund", ",", "von", "Sil\u00b7ber", ",", "Bluh\u00b7men", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPR", "ADJA", "NN", "$,", "APPR", "NN", "$,", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}