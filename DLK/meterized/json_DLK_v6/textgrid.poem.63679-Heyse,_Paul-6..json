{"textgrid.poem.63679": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich konnte nie die Gl\u00fccklichen beneiden,", "tokens": ["Ich", "konn\u00b7te", "nie", "die", "Gl\u00fcck\u00b7li\u00b7chen", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor denen offen, wie am hellen Tag", "tokens": ["Vor", "de\u00b7nen", "of\u00b7fen", ",", "wie", "am", "hel\u00b7len", "Tag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ADJD", "$,", "PWAV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Blumenbeet, das Jenseitsr\u00e4tsel lag,", "tokens": ["Ein", "Blu\u00b7men\u00b7beet", ",", "das", "Jen\u00b7seits\u00b7r\u00e4t\u00b7sel", "lag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie Kinder gl\u00e4ubig sich an M\u00e4rchen weiden.", "tokens": ["Wie", "Kin\u00b7der", "gl\u00e4u\u00b7big", "sich", "an", "M\u00e4r\u00b7chen", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Fr\u00fch lernt' ich, ob mit Schmerz auch, mich bescheiden,", "tokens": ["Fr\u00fch", "lernt'", "ich", ",", "ob", "mit", "Schmerz", "auch", ",", "mich", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "KOUS", "APPR", "NN", "ADV", "$,", "PPER", "VVPP", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Da\u00df kein beschr\u00e4nkter Geist erfassen mag", "tokens": ["Da\u00df", "kein", "be\u00b7schr\u00e4nk\u00b7ter", "Geist", "er\u00b7fas\u00b7sen", "mag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Unendliches, und weder dreist noch zag", "tokens": ["Un\u00b7end\u00b7li\u00b7ches", ",", "und", "we\u00b7der", "dreist", "noch", "zag"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "$,", "KON", "KON", "ADV", "ADV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gab ich mich drein, mein dunkles Los zu leiden.", "tokens": ["Gab", "ich", "mich", "drein", ",", "mein", "dunk\u00b7les", "Los", "zu", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch, mu\u00dft' ich auf die Freuden auch verzichten,", "tokens": ["Doch", ",", "mu\u00dft'", "ich", "auf", "die", "Freu\u00b7den", "auch", "ver\u00b7zich\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VMFIN", "PPER", "APPR", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die jener Himmelstrost den Frommen beut,", "tokens": ["Die", "je\u00b7ner", "Him\u00b7mel\u00b7strost", "den", "From\u00b7men", "beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Unfromm und trostlos war ich drum mitnichten.", "tokens": ["Un\u00b7fromm", "und", "trost\u00b7los", "war", "ich", "drum", "mit\u00b7nich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "PPER", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Birgt doch ein Ew'ges jede Spanne Zeit,", "tokens": ["Birgt", "doch", "ein", "Ew'\u00b7ges", "je\u00b7de", "Span\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo Wahres wir erkennen, Sch\u00f6nes dichten,", "tokens": ["Wo", "Wah\u00b7res", "wir", "er\u00b7ken\u00b7nen", ",", "Sch\u00f6\u00b7nes", "dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "VVINF", "$,", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wer da liebt, der f\u00fchlt Unendlichkeit.", "tokens": ["Und", "wer", "da", "liebt", ",", "der", "f\u00fchlt", "Un\u00b7end\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "$,", "PRELS", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}