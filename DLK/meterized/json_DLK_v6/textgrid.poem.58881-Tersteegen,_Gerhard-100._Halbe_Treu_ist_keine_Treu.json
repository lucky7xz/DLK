{"textgrid.poem.58881": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "100. Halbe Treu ist keine Treu", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer sich nur halb an Gott will geben,", "tokens": ["Wer", "sich", "nur", "halb", "an", "Gott", "will", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "ADJD", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der f\u00fchrt ein rechtes Jammerleben;", "tokens": ["Der", "f\u00fchrt", "ein", "rech\u00b7tes", "Jam\u00b7mer\u00b7le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Brich durch, es koste, was es will,", "tokens": ["Brich", "durch", ",", "es", "kos\u00b7te", ",", "was", "es", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "PPER", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sonst wird dein armes Herz nicht still!", "tokens": ["Sonst", "wird", "dein", "ar\u00b7mes", "Herz", "nicht", "still", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}