{"textgrid.poem.38666": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Alpenreise", "genre": "verse", "period": "N.A.", "pub_year": 1791, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00df athmen die Bl\u00fcten am st\u00fcrzenden Bach,", "tokens": ["S\u00fc\u00df", "ath\u00b7men", "die", "Bl\u00fc\u00b7ten", "am", "st\u00fcr\u00b7zen\u00b7den", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Hoch l\u00e4chelt am H\u00fcgel manch friedliches Dach,", "tokens": ["Hoch", "l\u00e4\u00b7chelt", "am", "H\u00fc\u00b7gel", "manch", "fried\u00b7li\u00b7ches", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPRART", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Umkreist von gr\u00fcnen Gehegen,", "tokens": ["Um\u00b7kreist", "von", "gr\u00fc\u00b7nen", "Ge\u00b7he\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dem Wandrer entgegen.", "tokens": ["Dem", "Wand\u00b7rer", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Die L\u00fcfte wehn reiner, die Unterwelt flieht,", "tokens": ["Die", "L\u00fcf\u00b7te", "wehn", "rei\u00b7ner", ",", "die", "Un\u00b7ter\u00b7welt", "flieht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Pfade sind schattig, der Cytisus bl\u00fcht.", "tokens": ["Die", "Pfa\u00b7de", "sind", "schat\u00b7tig", ",", "der", "Cy\u00b7ti\u00b7sus", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NE", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wie mild ergeu\u00dft sich die Frische", "tokens": ["Wie", "mild", "er\u00b7geu\u00dft", "sich", "die", "Fri\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Balsamgeb\u00fcsche!", "tokens": ["Der", "Bal\u00b7sam\u00b7ge\u00b7b\u00fc\u00b7sche", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie schimmert das Gr\u00fcn der arkadischen Flur!", "tokens": ["Wie", "schim\u00b7mert", "das", "Gr\u00fcn", "der", "ar\u00b7ka\u00b7di\u00b7schen", "Flur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie gl\u00e4nzen die Th\u00e4ler von Gold und Azur!", "tokens": ["Wie", "gl\u00e4n\u00b7zen", "die", "Th\u00e4\u00b7ler", "von", "Gold", "und", "A\u00b7zur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wie blinkt im wolligen Kleide", "tokens": ["Wie", "blinkt", "im", "wol\u00b7li\u00b7gen", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die silberne Weide!", "tokens": ["Die", "sil\u00b7ber\u00b7ne", "Wei\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie funkelt der B\u00e4che m\u00e4andrische Flut!", "tokens": ["Wie", "fun\u00b7kelt", "der", "B\u00e4\u00b7che", "m\u00e4\u00b7an\u00b7dri\u00b7sche", "Flut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie d\u00e4mmern die H\u00fcgel von Heerden umruht!", "tokens": ["Wie", "d\u00e4m\u00b7mern", "die", "H\u00fc\u00b7gel", "von", "Heer\u00b7den", "um\u00b7ruht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wie gl\u00fchn, in blendender Reihe,", "tokens": ["Wie", "gl\u00fchn", ",", "in", "blen\u00b7den\u00b7der", "Rei\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Berg' in der Bl\u00e4ue!", "tokens": ["Die", "Ber\u00b7g'", "in", "der", "Bl\u00e4u\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dem Tempe des Friedens von Heerden bewallt,", "tokens": ["Dem", "Tem\u00b7pe", "des", "Frie\u00b7dens", "von", "Heer\u00b7den", "be\u00b7wallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Entwinden die steinigen Pfade sich bald,", "tokens": ["Ent\u00b7win\u00b7den", "die", "stei\u00b7ni\u00b7gen", "Pfa\u00b7de", "sich", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PRF", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der Schlund am Felsen wird enger,", "tokens": ["Der", "Schlund", "am", "Fel\u00b7sen", "wird", "en\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die D\u00fcsternis b\u00e4nger.", "tokens": ["Die", "D\u00fcs\u00b7ter\u00b7nis", "b\u00e4n\u00b7ger", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Nun sterben die Laute beseelter Natur;", "tokens": ["Nun", "ster\u00b7ben", "die", "Lau\u00b7te", "be\u00b7seel\u00b7ter", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Dumpftosend umsch\u00e4umen Gew\u00e4sser mich nur,", "tokens": ["Dumpf\u00b7to\u00b7send", "um\u00b7sch\u00e4u\u00b7men", "Ge\u00b7w\u00e4s\u00b7ser", "mich", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "PPER", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die hoch an schwarzen Geh\u00f6lzen", "tokens": ["Die", "hoch", "an", "schwar\u00b7zen", "Ge\u00b7h\u00f6l\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dem Gletscher entschmelzen.", "tokens": ["Dem", "Glet\u00b7scher", "ent\u00b7schmel\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Wo Felsen den w\u00fctenden Stromfall umdr\u00e4un,", "tokens": ["Wo", "Fel\u00b7sen", "den", "w\u00fc\u00b7ten\u00b7den", "Strom\u00b7fall", "um\u00b7dr\u00e4un", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da wandl' ich im Schauer der Wildni\u00df allein,", "tokens": ["Da", "wandl'", "ich", "im", "Schau\u00b7er", "der", "Wild\u00b7ni\u00df", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und seh' mit traurigem Sinnen", "tokens": ["Und", "seh'", "mit", "trau\u00b7ri\u00b7gem", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Fluten verrinnen.", "tokens": ["Die", "Flu\u00b7ten", "ver\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Hier wandelte nimmer der Odem des Mais;", "tokens": ["Hier", "wan\u00b7del\u00b7te", "nim\u00b7mer", "der", "O\u00b7dem", "des", "Mais", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Hier wiegt sich kein Vogel auf duftendem Reis;", "tokens": ["Hier", "wiegt", "sich", "kein", "Vo\u00b7gel", "auf", "duf\u00b7ten\u00b7dem", "Reis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PIAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Nur Moos' und Flechten entgr\u00fcnen", "tokens": ["Nur", "Moos'", "und", "Flech\u00b7ten", "ent\u00b7gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den wilden Ruinen.", "tokens": ["Den", "wil\u00b7den", "Ru\u00b7i\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+----", "measure": "dactylic.init"}}, "stanza.9": {"line.1": {"text": "Wie Hesper vom Purpur des Abends umwallt,", "tokens": ["Wie", "Hes\u00b7per", "vom", "Pur\u00b7pur", "des", "A\u00b7bends", "um\u00b7wallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "O Freundin, so l\u00e4chelt mir deine Gestalt,", "tokens": ["O", "Freun\u00b7din", ",", "so", "l\u00e4\u00b7chelt", "mir", "dei\u00b7ne", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und hellt mit mondlicher Milde", "tokens": ["Und", "hellt", "mit", "mond\u00b7li\u00b7cher", "Mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Des Todes Gefilde.", "tokens": ["Des", "To\u00b7des", "Ge\u00b7fil\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "O Freundin! ich denke mit Lust und mit Weh", "tokens": ["O", "Freun\u00b7din", "!", "ich", "den\u00b7ke", "mit", "Lust", "und", "mit", "Weh"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Des H\u00fcgels, wo wir, unter Eichen, am See,", "tokens": ["Des", "H\u00fc\u00b7gels", ",", "wo", "wir", ",", "un\u00b7ter", "Ei\u00b7chen", ",", "am", "See", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "$,", "APPR", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Im Geist' all unsern Vertrauten", "tokens": ["Im", "Geist'", "all", "un\u00b7sern", "Ver\u00b7trau\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PIAT", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein H\u00fcttchen erbauten.", "tokens": ["Ein", "H\u00fctt\u00b7chen", "er\u00b7bau\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Noch t\u00f6net, wie leiser Harmonikaklang,", "tokens": ["Noch", "t\u00f6\u00b7net", ",", "wie", "lei\u00b7ser", "Har\u00b7mo\u00b7nik\u00b7a\u00b7klang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "ADJD", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mir tief in der Seele dein s\u00fcsser Gesang.", "tokens": ["Mir", "tief", "in", "der", "See\u00b7le", "dein", "s\u00fcs\u00b7ser", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Du r\u00fchrst im Grazienschleier", "tokens": ["Du", "r\u00fchrst", "im", "Gra\u00b7zi\u00b7en\u00b7schlei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die lesbische Leier.", "tokens": ["Die", "les\u00b7bi\u00b7sche", "Lei\u00b7er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "Hell schwebt noch, im abendlich duftigen Flor,", "tokens": ["Hell", "schwebt", "noch", ",", "im", "a\u00b7bend\u00b7lich", "duf\u00b7ti\u00b7gen", "Flor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "APPRART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Das Eiland der friedlichen Saone mir vor,", "tokens": ["Das", "Ei\u00b7land", "der", "fried\u00b7li\u00b7chen", "Sao\u00b7ne", "mir", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wo j\u00fcngst wir unter Syringen", "tokens": ["Wo", "j\u00fcngst", "wir", "un\u00b7ter", "Sy\u00b7rin\u