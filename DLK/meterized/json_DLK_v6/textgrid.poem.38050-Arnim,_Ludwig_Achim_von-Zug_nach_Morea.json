{"textgrid.poem.38050": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Zug nach Morea", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was haben die Urner und Zuger gethan,", "tokens": ["Was", "ha\u00b7ben", "die", "Ur\u00b7ner", "und", "Zu\u00b7ger", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie wollen ein Zug gen Morea han,", "tokens": ["Sie", "wol\u00b7len", "ein", "Zug", "gen", "Mo\u00b7rea", "han", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "NE", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Gen Morea wollens dingen,", "tokens": ["Gen", "Mo\u00b7rea", "wol\u00b7lens", "din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie wollen dingen achttausend Mann,", "tokens": ["Sie", "wol\u00b7len", "din\u00b7gen", "acht\u00b7tau\u00b7send", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "CARD", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wider den T\u00fcrken wollens kriegen,", "tokens": ["Wi\u00b7der", "den", "T\u00fcr\u00b7ken", "wol\u00b7lens", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Sie zogen durchs freye Amt hinab,", "tokens": ["Sie", "zo\u00b7gen", "durchs", "frey\u00b7e", "Amt", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie fanden gar manchen jungen Soldat,", "tokens": ["Sie", "fan\u00b7den", "gar", "man\u00b7chen", "jun\u00b7gen", "Sol\u00b7dat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie liessens all roth bekleiden, sie f\u00fchren \u00fcber den Zuger-See,", "tokens": ["Sie", "lies\u00b7sens", "all", "roth", "be\u00b7klei\u00b7den", ",", "sie", "f\u00fch\u00b7ren", "\u00fc\u00b7ber", "den", "Zu\u00b7ger\u00b7See", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIAT", "ADJD", "VVINF", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+----+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sie lugen umher und das thut weh:", "tokens": ["Sie", "lu\u00b7gen", "um\u00b7her", "und", "das", "thut", "weh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PDS", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "He der Krieg m\u00f6gt manchem verleiden.", "tokens": ["He", "der", "Krieg", "m\u00f6gt", "man\u00b7chem", "ver\u00b7lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie zogen zu Zug wohl aus der Stadt,", "tokens": ["Sie", "zo\u00b7gen", "zu", "Zug", "wohl", "aus", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und ein gut Gesell zum andern sprach:", "tokens": ["Und", "ein", "gut", "Ge\u00b7sell", "zum", "an\u00b7dern", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "APPRART", "PIS", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbich habs gar eben gerechnet,", "tokens": ["\u00bb", "ich", "habs", "gar", "e\u00b7ben", "ge\u00b7rech\u00b7net", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "NE", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wir m\u00fcssen ziehn dem T\u00fcrken zu,", "tokens": ["Wir", "m\u00fcs\u00b7sen", "ziehn", "dem", "T\u00fcr\u00b7ken", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "He ich mein, mein Herz m\u00fcst brechen!\u00ab", "tokens": ["He", "ich", "mein", ",", "mein", "Herz", "m\u00fcst", "bre\u00b7chen", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PPER", "PPOSAT", "$,", "PPOSAT", "NN", "VMFIN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie sie aus den Schiffen heraus dann steigen,", "tokens": ["Wie", "sie", "aus", "den", "Schif\u00b7fen", "he\u00b7raus", "dann", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "APZR", "ADV", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Hauptleut thun ihnen die H\u00e4nd all reichen,", "tokens": ["Die", "Haupt\u00b7leut", "thun", "ih\u00b7nen", "die", "H\u00e4nd", "all", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "PIAT", "ADJA", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sie th\u00e4tens in Glieder stellen,", "tokens": ["Sie", "th\u00e4\u00b7tens", "in", "Glie\u00b7der", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der ein Hauptmann zum andern sprach:", "tokens": ["Der", "ein", "Haupt\u00b7mann", "zum", "an\u00b7dern", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "PIS", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "\u00bbhe wie han wir die bravsten Gesellen.\u00ab", "tokens": ["\u00bb", "he", "wie", "han", "wir", "die", "bravs\u00b7ten", "Ge\u00b7sel\u00b7len", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "KOKOM", "VAFIN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.5": {"line.1": {"text": "Der ein Hauptmann zum andern seit:", "tokens": ["Der", "ein", "Haupt\u00b7mann", "zum", "an\u00b7dern", "seit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "\u00bbheut Nacht wend wir noch gen Uri hinein, wir m\u00fcssen tapfer laufen.\u00ab", "tokens": ["\u00bb", "heut", "Nacht", "wend", "wir", "noch", "gen", "U\u00b7ri", "hin\u00b7ein", ",", "wir", "m\u00fcs\u00b7sen", "tap\u00b7fer", "lau\u00b7fen", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "NN", "KOUS", "PPER", "ADV", "APPR", "NE", "PTKVZ", "$,", "PPER", "VMFIN", "ADJD", "VVINF", "$.", "$("], "meter": "-+--+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Der ein Hauptmann zum andern sprach,", "tokens": ["Der", "ein", "Haupt\u00b7mann", "zum", "an\u00b7dern", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "PIS", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wie ihm gefiele diese Sach:", "tokens": ["Wie", "ihm", "ge\u00b7fie\u00b7le", "die\u00b7se", "Sach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "He die Gesellen wollen wir verkaufen.", "tokens": ["He", "die", "Ge\u00b7sel\u00b7len", "wol\u00b7len", "wir", "ver\u00b7kau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Sie zogen \u00fcber den Gotthard auf,", "tokens": ["Sie", "zo\u00b7gen", "\u00fc\u00b7ber", "den", "Got\u00b7thard", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NE", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die jungen Soldaten schreien \u00fcberlaut,", "tokens": ["Die", "jun\u00b7gen", "Sol\u00b7da\u00b7ten", "schrei\u00b7en", "\u00fc\u00b7berl\u00b7aut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es wolt sie all schier gereuen,", "tokens": ["Es", "wolt", "sie", "all", "schier", "ge\u00b7reu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der ein gut Gesell zum andern sprach:", "tokens": ["Der", "ein", "gut", "Ge\u00b7sell", "zum", "an\u00b7dern", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "NN", "APPRART", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "He keim Hauptmann ist mehr zu trauen.", "tokens": ["He", "keim", "Haupt\u00b7mann", "ist", "mehr", "zu", "trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "VAFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Sie fahren \u00fcber den langen See,", "tokens": ["Sie", "fah\u00b7ren", "\u00fc\u00b7ber", "den", "lan\u00b7gen", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie sehen das Vaterland nimmermehr,", "tokens": ["Sie", "se\u00b7hen", "das", "Va\u00b7ter\u00b7land", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sie th\u00e4ten schier all weinen,", "tokens": ["Sie", "th\u00e4\u00b7ten", "schier", "all", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der ein gut Gesell zum andern sprach:", "tokens": ["Der", "ein", "gut", "Ge\u00b7sell", "zum", "an\u00b7dern", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "NN", "APPRART", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "He w\u00e4ren wir nunmehr daheime.", "tokens": ["He", "w\u00e4\u00b7ren", "wir", "nun\u00b7mehr", "da\u00b7hei\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und wie sie kamen zu der Meerstangen,", "tokens": ["Und", "wie", "sie", "ka\u00b7men", "zu", "der", "Meer\u00b7stan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Es thut die Schweizersoldaten plangen:", "tokens": ["Es", "thut", "die", "Schwei\u00b7zer\u00b7sol\u00b7da\u00b7ten", "plan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbwie weit m\u00fcnd wir von hinnen,", "tokens": ["\u00bb", "wie", "weit", "m\u00fcnd", "wir", "von", "hin\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADJD", "VMFIN", "PPER", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wann ich denke an mein Vaterland,", "tokens": ["Wann", "ich", "den\u00b7ke", "an", "mein", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "He mein Herz m\u00f6cht mir zerspringen!\u00ab", "tokens": ["He", "mein", "Herz", "m\u00f6cht", "mir", "zer\u00b7sprin\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Sie reisen eine weite Rei\u00df,", "tokens": ["Sie", "rei\u00b7sen", "ei\u00b7ne", "wei\u00b7te", "Rei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der ein gut Gesell zum andern schreit:", "tokens": ["Der", "ein", "gut", "Ge\u00b7sell", "zum", "an\u00b7dern", "schreit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie weit m\u00fcnd wir noch reisen?", "tokens": ["Wie", "weit", "m\u00fcnd", "wir", "noch", "rei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Hauptmann zu den Soldaten sprach:", "tokens": ["Der", "Haupt\u00b7mann", "zu", "den", "Sol\u00b7da\u00b7ten", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "He Venedig will ich bald zeigen.", "tokens": ["He", "Ve\u00b7ne\u00b7dig", "will", "ich", "bald", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Der Wachmeister ist ein munterer Mann,", "tokens": ["Der", "Wach\u00b7meis\u00b7ter", "ist", "ein", "mun\u00b7te\u00b7rer", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er hat die bravsten Soldaten g'han,", "tokens": ["Er", "hat", "die", "bravs\u00b7ten", "Sol\u00b7da\u00b7ten", "g'han", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zu Venedig war er der erste,", "tokens": ["Zu", "Ve\u00b7ne\u00b7dig", "war", "er", "der", "ers\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPER", "ART", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sie haben ihm geben viel Gut und Geld:", "tokens": ["Sie", "ha\u00b7ben", "ihm", "ge\u00b7ben", "viel", "Gut", "und", "Geld", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "ADV", "ADJD", "KON", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "He ein guldene Ketten vest.", "tokens": ["He", "ein", "gul\u00b7de\u00b7ne", "Ket\u00b7ten", "vest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.11"