{"textgrid.poem.63143": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Hiob", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und war kein Elend, das ihn nicht befiel,", "tokens": ["Und", "war", "kein", "E\u00b7lend", ",", "das", "ihn", "nicht", "be\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und keine Seuchen, die ihn nicht best\u00fcrzten.", "tokens": ["Und", "kei\u00b7ne", "Seu\u00b7chen", ",", "die", "ihn", "nicht", "be\u00b7st\u00fcrz\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es faulte sein Getreide schon am Stiel,", "tokens": ["Es", "faul\u00b7te", "sein", "Ge\u00b7trei\u00b7de", "schon", "am", "Stiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Riff zerspellte seines Schiffes Kiel,", "tokens": ["Ein", "Riff", "zer\u00b7spell\u00b7te", "sei\u00b7nes", "Schif\u00b7fes", "Kiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Tr\u00e4nen einzig seinen Abend w\u00fcrzten.", "tokens": ["Und", "Tr\u00e4\u00b7nen", "ein\u00b7zig", "sei\u00b7nen", "A\u00b7bend", "w\u00fcrz\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sein Haus verbrannte. Seine Mutter ward", "tokens": ["Sein", "Haus", "ver\u00b7brann\u00b7te", ".", "Sei\u00b7ne", "Mut\u00b7ter", "ward"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von den Nomaden vor der Stadt gesch\u00e4ndet.", "tokens": ["Von", "den", "No\u00b7ma\u00b7den", "vor", "der", "Stadt", "ge\u00b7sch\u00e4n\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Sohn erh\u00e4ngte sich am ersten Bart.", "tokens": ["Ein", "Sohn", "er\u00b7h\u00e4ng\u00b7te", "sich", "am", "ers\u00b7ten", "Bart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sein einziger Bruder hatte sich geschart", "tokens": ["Sein", "ein\u00b7zi\u00b7ger", "Bru\u00b7der", "hat\u00b7te", "sich", "ge\u00b7schart"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PRF", "VVPP"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Der R\u00e4uberbande, die sein Vieh entwendet.", "tokens": ["Der", "R\u00e4u\u00b7berb\u00b7an\u00b7de", ",", "die", "sein", "Vieh", "ent\u00b7wen\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und die die Bitternis vers\u00fcsste: sie,", "tokens": ["Und", "die", "die", "Bit\u00b7ter\u00b7nis", "ver\u00b7s\u00fcss\u00b7te", ":", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "VVFIN", "$.", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Frau aus Ebenholz und aus Granaten:", "tokens": ["Die", "Frau", "aus", "E\u00b7ben\u00b7holz", "und", "aus", "Gra\u00b7na\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr zweiter Sohn in Br\u00fcnsten spiesste sie.", "tokens": ["Ihr", "zwei\u00b7ter", "Sohn", "in", "Br\u00fcns\u00b7ten", "spiess\u00b7te", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit ihren letzten Blicken gr\u00fcsste sie", "tokens": ["Mit", "ih\u00b7ren", "letz\u00b7ten", "Bli\u00b7cken", "gr\u00fcs\u00b7ste", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den Gatten \u2013 welche wild um Rache baten.", "tokens": ["Den", "Gat\u00b7ten", "\u2013", "wel\u00b7che", "wild", "um", "Ra\u00b7che", "ba\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er aber kannte Rache nicht noch Hass,", "tokens": ["Er", "a\u00b7ber", "kann\u00b7te", "Ra\u00b7che", "nicht", "noch", "Hass", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "NN", "PTKNEG", "ADV", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So sehr der Schmerz sein Ackerland verwildert,", "tokens": ["So", "sehr", "der", "Schmerz", "sein", "A\u00b7cker\u00b7land", "ver\u00b7wil\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So unersch\u00f6pflich tief sein Tr\u00e4nenfass.", "tokens": ["So", "un\u00b7er\u00b7sch\u00f6pf\u00b7lich", "tief", "sein", "Tr\u00e4\u00b7nen\u00b7fass", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er sang mit seinem frommen Pilgerbass", "tokens": ["Er", "sang", "mit", "sei\u00b7nem", "from\u00b7men", "Pil\u00b7ger\u00b7bass"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dem Leben zu, dass sich um ihn bebildert.", "tokens": ["Dem", "Le\u00b7ben", "zu", ",", "dass", "sich", "um", "ihn", "be\u00b7bil\u00b7dert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KOUS", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und hast du, Herr, wie Marmor mich zerschlagen,", "tokens": ["Und", "hast", "du", ",", "Herr", ",", "wie", "Mar\u00b7mor", "mich", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "$,", "NN", "$,", "PWAV", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und g\u00f6nntest du mir nicht die kleinste Tat:", "tokens": ["Und", "g\u00f6nn\u00b7test", "du", "mir", "nicht", "die", "kleins\u00b7te", "Tat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie darf ich gegen deine Einsicht wagen", "tokens": ["Wie", "darf", "ich", "ge\u00b7gen", "dei\u00b7ne", "Ein\u00b7sicht", "wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch nur die j\u00e4mmerlichste meiner Klagen?", "tokens": ["Auch", "nur", "die", "j\u00e4m\u00b7mer\u00b7lichs\u00b7te", "mei\u00b7ner", "Kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du bist der M\u00e4her und ich bin die Mahd.", "tokens": ["Du", "bist", "der", "M\u00e4\u00b7her", "und", "ich", "bin", "die", "Mahd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und sendest du auch Blitze, mich zu blenden,", "tokens": ["Und", "sen\u00b7dest", "du", "auch", "Blit\u00b7ze", ",", "mich", "zu", "blen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und machst du lahm den Leib, die Seele taub,", "tokens": ["Und", "machst", "du", "lahm", "den", "Leib", ",", "die", "See\u00b7le", "taub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und reisst du mir die Finger von den H\u00e4nden:", "tokens": ["Und", "reisst", "du", "mir", "die", "Fin\u00b7ger", "von", "den", "H\u00e4n\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich preise dennoch meiner Mutter Lenden", "tokens": ["Ich", "prei\u00b7se", "den\u00b7noch", "mei\u00b7ner", "Mut\u00b7ter", "Len\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und werde nimmer eines Unmuts Raub.", "tokens": ["Und", "wer\u00b7de", "nim\u00b7mer", "ei\u00b7nes", "Un\u00b7muts", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Dass einen Fr\u00fchling ich im Licht erlebte,", "tokens": ["Dass", "ei\u00b7nen", "Fr\u00fch\u00b7ling", "ich", "im", "Licht", "er\u00b7leb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dass mir die Mutter s\u00fcsse Kuchen buk,", "tokens": ["Dass", "mir", "die", "Mut\u00b7ter", "s\u00fcs\u00b7se", "Ku\u00b7chen", "buk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dass ich als J\u00fcngling sch\u00f6n in T\u00e4nzen schwebte,", "tokens": ["Dass", "ich", "als", "J\u00fcng\u00b7ling", "sch\u00f6n", "in", "T\u00e4n\u00b7zen", "schweb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dass ich am Teppich der Gedanken webte,", "tokens": ["Dass", "ich", "am", "Tep\u00b7pich", "der", "Ge\u00b7dan\u00b7ken", "web\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "War dies nicht Gl\u00fcck und goldnes Gl\u00fcck genug?", "tokens": ["War", "dies", "nicht", "Gl\u00fcck", "und", "gold\u00b7nes", "Gl\u00fcck", "ge\u00b7nug", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "NN", "KON", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Dass ich nur einmal durft mein Weib umarmen,", "tokens": ["Dass", "ich", "nur", "ein\u00b7mal", "durft", "mein", "Weib", "um\u00b7ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dass ich nur einmal in die Sonne sah:", "tokens": ["Dass", "ich", "nur", "ein\u00b7mal", "in", "die", "Son\u00b7ne", "sah", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dies ist soviel schon meines Gotts Erbarmen,", "tokens": ["Dies", "ist", "so\u00b7viel", "schon", "mei\u00b7nes", "Gotts", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dass ich der Reichste unter allen Armen \u2013", "tokens": ["Dass", "ich", "der", "Reichs\u00b7te", "un\u00b7ter", "al\u00b7len", "Ar\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Lob sei und Preis dem Herrn. Hallelujah!", "tokens": ["Lob", "sei", "und", "Preis", "dem", "Herrn", ".", "Hal\u00b7le\u00b7lu\u00b7jah", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "KON", "NN", "ART", "NN", "$.", "ITJ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}