{"textgrid.poem.31572": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Herzenherrscher du! gebrauche dieses Jahr!", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Herzenherrscher du! gebrauche dieses Jahr!", "tokens": ["Du", "Her\u00b7zen\u00b7herr\u00b7scher", "du", "!", "ge\u00b7brau\u00b7che", "die\u00b7ses", "Jahr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPER", "$.", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mein Herz zum S\u00e4itenspiel / den Mund zur Lob-Posaunen /", "tokens": ["mein", "Herz", "zum", "S\u00e4i\u00b7ten\u00b7spiel", "/", "den", "Mund", "zur", "Lob\u00b7Po\u00b7sau\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$(", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die Zung zum Cimbelwerk; auf da\u00df man mit erstaunen", "tokens": ["die", "Zung", "zum", "Cim\u00b7bel\u00b7werk", ";", "auf", "da\u00df", "man", "mit", "er\u00b7stau\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "$.", "APPR", "KOUS", "PIS", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "dein Lob erschallen h\u00f6r / da\u00df ich es offenbar.", "tokens": ["dein", "Lob", "er\u00b7schal\u00b7len", "h\u00f6r", "/", "da\u00df", "ich", "es", "of\u00b7fen\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VVFIN", "$(", "KOUS", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mach deine Herrlichkeit in meiner Finstre klar.", "tokens": ["Mach", "dei\u00b7ne", "Herr\u00b7lich\u00b7keit", "in", "mei\u00b7ner", "Finst\u00b7re", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "wollst dein' Vnendlichkeit mit meinem Kiel umzaunen.", "tokens": ["wollst", "dein'", "Vnend\u00b7lich\u00b7keit", "mit", "mei\u00b7nem", "Kiel", "um\u00b7zau\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVIZU", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "bekehr das harte Herz mit deiner Wort-Carthaunen /", "tokens": ["be\u00b7kehr", "das", "har\u00b7te", "Herz", "mit", "dei\u00b7ner", "Wor\u00b7tCart\u00b7hau\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "da\u00df deines s\u00fcssen Triebs ich fr\u00f6lich werd gewar.", "tokens": ["da\u00df", "dei\u00b7nes", "s\u00fcs\u00b7sen", "Triebs", "ich", "fr\u00f6\u00b7lich", "werd", "ge\u00b7war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPER", "ADJD", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "La\u00df / nach dem Splitterstrau\u00df / ein Labe-L\u00fcfftlein wehn.", "tokens": ["La\u00df", "/", "nach", "dem", "Split\u00b7ter\u00b7strau\u00df", "/", "ein", "La\u00b7be\u00b7L\u00fcfft\u00b7lein", "wehn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "APPR", "ART", "NN", "$(", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Erhalt aus Gnaden / die sich dir zu F\u00fcssen legt /", "tokens": ["Er\u00b7halt", "aus", "Gna\u00b7den", "/", "die", "sich", "dir", "zu", "F\u00fcs\u00b7sen", "legt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$(", "PRELS", "PRF", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "aus Demut / Lieb und Furcht / la\u00df mich im Glauben stehn", "tokens": ["aus", "De\u00b7mut", "/", "Lieb", "und", "Furcht", "/", "la\u00df", "mich", "im", "Glau\u00b7ben", "stehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$(", "NN", "KON", "NN", "$(", "VVIMP", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "ger\u00fcst mit Geist und Krafft / wann schon die Sach' ausschl\u00e4gt", "tokens": ["ge\u00b7r\u00fcst", "mit", "Geist", "und", "Krafft", "/", "wann", "schon", "die", "Sach'", "aus\u00b7schl\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$(", "PWAV", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "zum Knall und Fall / gib mir mit Lust hinein zu gehn /", "tokens": ["zum", "Knall", "und", "Fall", "/", "gib", "mir", "mit", "Lust", "hin\u00b7ein", "zu", "gehn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$(", "VVIMP", "PPER", "APPR", "NN", "APZR", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "zu seh'n was deine Gnad vor neue Wunder hegt.", "tokens": ["zu", "seh'n", "was", "dei\u00b7ne", "Gnad", "vor", "neu\u00b7e", "Wun\u00b7der", "hegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PRELS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}