{"textgrid.poem.38661": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Bonnet", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wolken erheben sich nie bis zur Stirn dieses heiligen Greises,", "tokens": ["Wol\u00b7ken", "er\u00b7he\u00b7ben", "sich", "nie", "bis", "zur", "Stirn", "die\u00b7ses", "hei\u00b7li\u00b7gen", "Grei\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADV", "APPR", "APPRART", "NN", "PDAT", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Und in Aurorens Gestalt winkt ihm der l\u00e4chelnde Tod.", "tokens": ["Und", "in", "Au\u00b7ro\u00b7rens", "Ge\u00b7stalt", "winkt", "ihm", "der", "l\u00e4\u00b7cheln\u00b7de", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "---+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So wie der Hirt auf den Alpen in sonniger Heitre den Donner", "tokens": ["So", "wie", "der", "Hirt", "auf", "den", "Al\u00b7pen", "in", "son\u00b7ni\u00b7ger", "Heit\u00b7re", "den", "Don\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "H\u00f6rt aus umnachtetem Thal, h\u00f6rt er der Leidenschaft Sturm.", "tokens": ["H\u00f6rt", "aus", "um\u00b7nach\u00b7te\u00b7tem", "Thal", ",", "h\u00f6rt", "er", "der", "Lei\u00b7den\u00b7schaft", "Sturm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ADJA", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}}}}}