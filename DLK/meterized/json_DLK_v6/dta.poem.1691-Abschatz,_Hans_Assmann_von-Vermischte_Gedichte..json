{"dta.poem.1691": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Lieb und Zorn erhizt die Jugend/", "tokens": ["Lieb", "und", "Zorn", "er\u00b7hizt", "die", "Ju\u00b7gend", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Toller Wahn und Eitelkeit", "tokens": ["Tol\u00b7ler", "Wahn", "und", "Ei\u00b7tel\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stihlt der Menschen beste Zeit/", "tokens": ["Stihlt", "der", "Men\u00b7schen", "bes\u00b7te", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mittel-Stra\u00dfe f\u00fchrt zur Tugend.", "tokens": ["Mit\u00b7tel\u00b7Stra\u00b7\u00dfe", "f\u00fchrt", "zur", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kohlen die am meisten rauchen", "tokens": ["Koh\u00b7len", "die", "am", "meis\u00b7ten", "rau\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "APPRART", "PIS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind am mindsten zu gebrauchen.", "tokens": ["Sind", "am", "minds\u00b7ten", "zu", "ge\u00b7brau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Liebe/ wann die Zeit verhanden", "tokens": ["Lie\u00b7be", "/", "wann", "die", "Zeit", "ver\u00b7han\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$(", "PWAV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und Vernunfft den Weg geweist.", "tokens": ["Und", "Ver\u00b7nunfft", "den", "Weg", "ge\u00b7weist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Z\u00fcrne/ wanns die Ehre heist/", "tokens": ["Z\u00fcr\u00b7ne", "/", "wanns", "die", "Eh\u00b7re", "heist", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PWAV", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Da\u00df du nicht bestehst mit Schanden.", "tokens": ["Da\u00df", "du", "nicht", "be\u00b7stehst", "mit", "Schan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Rede/ was du kanst bekennen;", "tokens": ["Re\u00b7de", "/", "was", "du", "kanst", "be\u00b7ken\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWS", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Gleiche zu ist gutt zu rennen.", "tokens": ["Glei\u00b7che", "zu", "ist", "gutt", "zu", "ren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "VAFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}