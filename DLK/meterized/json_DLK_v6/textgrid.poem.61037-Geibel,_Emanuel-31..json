{"textgrid.poem.61037": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Wald, im hellen Sonnenschein,", "tokens": ["Im", "Wald", ",", "im", "hel\u00b7len", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn alle Knospen springen,", "tokens": ["Wenn", "al\u00b7le", "Knos\u00b7pen", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da mag ich gerne mittendrein", "tokens": ["Da", "mag", "ich", "ger\u00b7ne", "mit\u00b7ten\u00b7drein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eins singen.", "tokens": ["Eins", "sin\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Wie mir zu Mut in Leid und Lust,", "tokens": ["Wie", "mir", "zu", "Mut", "in", "Leid", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Wachen und im Tr\u00e4umen,", "tokens": ["Im", "Wa\u00b7chen", "und", "im", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das stimm' ich an aus voller Brust", "tokens": ["Das", "stimm'", "ich", "an", "aus", "vol\u00b7ler", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den B\u00e4umen.", "tokens": ["Den", "B\u00e4u\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Und sie verstehen mich gar fein,", "tokens": ["Und", "sie", "ver\u00b7ste\u00b7hen", "mich", "gar", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Bl\u00e4tter alle lauschen,", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "al\u00b7le", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und fall'n am rechten Orte ein", "tokens": ["Und", "fall'n", "am", "rech\u00b7ten", "Or\u00b7te", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Rauschen.", "tokens": ["Mit", "Rau\u00b7schen", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Und weiter wandelt Schall und Hall", "tokens": ["Und", "wei\u00b7ter", "wan\u00b7delt", "Schall", "und", "Hall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Wipfeln, Fels und B\u00fcschen,", "tokens": ["In", "Wip\u00b7feln", ",", "Fels", "und", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hell schmettert auch Frau Nachtigall", "tokens": ["Hell", "schmet\u00b7tert", "auch", "Frau", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dazwischen.", "tokens": ["Da\u00b7zwi\u00b7schen", "."], "token_info": ["word", "punct"], "pos": ["PAV", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Da f\u00fchlt die Brust am eignen Klang,", "tokens": ["Da", "f\u00fchlt", "die", "Brust", "am", "eig\u00b7nen", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie darf sich was erk\u00fchnen -", "tokens": ["Sie", "darf", "sich", "was", "er\u00b7k\u00fch\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "PIS", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O frische Luft! Gesang! Gesang", "tokens": ["O", "fri\u00b7sche", "Luft", "!", "Ge\u00b7sang", "!", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "ADJA", "NN", "$.", "NN", "$.", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Gr\u00fcnen!", "tokens": ["Im", "Gr\u00fc\u00b7nen", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}