{"dta.poem.6909": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber die Jdeen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was denckt man, wenn man fragt, wie doch auf eine Seele", "tokens": ["Was", "denckt", "man", ",", "wenn", "man", "fragt", ",", "wie", "doch", "auf", "ei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PIS", "$,", "KOUS", "PIS", "VVFIN", "$,", "PWAV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein C\u00f6rper w\u00fcrcken kan?", "tokens": ["Ein", "C\u00f6r\u00b7per", "w\u00fcr\u00b7cken", "kan", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch welche Art von Druck kan man", "tokens": ["Durch", "wel\u00b7che", "Art", "von", "Druck", "kan", "man"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "APPR", "NN", "VMFIN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vereinen, und sich \u00fcberf\u00fchren,", "tokens": ["Ver\u00b7ei\u00b7nen", ",", "und", "sich", "\u00fc\u00b7berf\u00b7\u00fch\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df einen Geist ein C\u00f6rper k\u00f6nne r\u00fchren?", "tokens": ["Da\u00df", "ei\u00b7nen", "Geist", "ein", "C\u00f6r\u00b7per", "k\u00f6n\u00b7ne", "r\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Es liegt, wenn wir die Wahrheit sagen wollen,", "tokens": ["Es", "liegt", ",", "wenn", "wir", "die", "Wahr\u00b7heit", "sa\u00b7gen", "wol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Jm Dencken blo\u00df allein,", "tokens": ["Jm", "Den\u00b7cken", "blo\u00df", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und in Vernehmungen, die nie erzeuget seyn", "tokens": ["Und", "in", "Ver\u00b7neh\u00b7mun\u00b7gen", ",", "die", "nie", "er\u00b7zeu\u00b7get", "seyn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "PRELS", "ADV", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Von eines C\u00f6rpers Stoff, und welche billig sollen", "tokens": ["Von", "ei\u00b7nes", "C\u00f6r\u00b7pers", "Stoff", ",", "und", "wel\u00b7che", "bil\u00b7lig", "sol\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "PRELS", "ADJD", "VMFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vom Vorwurff unterschieden werden,", "tokens": ["Vom", "Vor\u00b7wurff", "un\u00b7ter\u00b7schie\u00b7den", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Mit dem die Seelen sie verbinden.", "tokens": ["Mit", "dem", "die", "See\u00b7len", "sie", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}