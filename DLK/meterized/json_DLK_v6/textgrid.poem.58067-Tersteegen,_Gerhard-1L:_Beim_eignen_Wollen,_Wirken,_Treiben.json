{"textgrid.poem.58067": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Beim eignen Wollen, Wirken, Treiben", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Beim eignen Wollen, Wirken, Treiben", "tokens": ["Beim", "eig\u00b7nen", "Wol\u00b7len", ",", "Wir\u00b7ken", ",", "Trei\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wirst du derselbe immer bleiben;", "tokens": ["Wirst", "du", "der\u00b7sel\u00b7be", "im\u00b7mer", "blei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDAT", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wer abgeschieden, willenlos,", "tokens": ["Wer", "ab\u00b7ge\u00b7schie\u00b7den", ",", "wil\u00b7len\u00b7los", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVPP", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gelassen liegt in Gottes Scho\u00df", "tokens": ["Ge\u00b7las\u00b7sen", "liegt", "in", "Got\u00b7tes", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und so von eignen Werken ruht,", "tokens": ["Und", "so", "von", "eig\u00b7nen", "Wer\u00b7ken", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor Gott die beste Arbeit tut.", "tokens": ["Vor", "Gott", "die", "bes\u00b7te", "Ar\u00b7beit", "tut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}