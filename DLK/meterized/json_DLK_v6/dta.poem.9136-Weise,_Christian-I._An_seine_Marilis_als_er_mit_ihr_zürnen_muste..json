{"dta.poem.9136": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n An seine Marilis/ als er mit ihr z\u00fcrnen muste.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin schon satt und sage", "tokens": ["Ich", "bin", "schon", "satt", "und", "sa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df mich der hunger treibt/", "tokens": ["Da\u00df", "mich", "der", "hun\u00b7ger", "treibt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich seh mein gl\u00fcck/ und klage", "tokens": ["Ich", "seh", "mein", "gl\u00fcck", "/", "und", "kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df solches aussen bleibt/", "tokens": ["Da\u00df", "sol\u00b7ches", "aus\u00b7sen", "bleibt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVIZU", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich such und fliehe meine pflicht/", "tokens": ["Ich", "such", "und", "flie\u00b7he", "mei\u00b7ne", "pflicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich wolte gern und will doch nicht.", "tokens": ["Ich", "wol\u00b7te", "gern", "und", "will", "doch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "KON", "VMFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Ich bin verliebt/ und leide", "tokens": ["Ich", "bin", "ver\u00b7liebt", "/", "und", "lei\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die pein der einsamkeit/", "tokens": ["Die", "pein", "der", "ein\u00b7sam\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sehne mich und meide", "tokens": ["Ich", "seh\u00b7ne", "mich", "und", "mei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch die gelegenheit/", "tokens": ["Doch", "die", "ge\u00b7le\u00b7gen\u00b7heit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$("], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "Die gegenwart beliebet mir/", "tokens": ["Die", "ge\u00b7gen\u00b7wart", "be\u00b7lie\u00b7bet", "mir", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und gleichwol geh ich nicht zu ihr.", "tokens": ["Und", "gleich\u00b7wol", "geh", "ich", "nicht", "zu", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Die liebes-grillen wancken", "tokens": ["Die", "lie\u00b7bes\u00b7gril\u00b7len", "wan\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Jm hertzen hin und her/", "tokens": ["Jm", "hert\u00b7zen", "hin", "und", "her", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und machen die gedancken", "tokens": ["Und", "ma\u00b7chen", "die", "ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit tausend sorgen schwer/", "tokens": ["Mit", "tau\u00b7send", "sor\u00b7gen", "schwer", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und schliesse doch vor meiner ruh", "tokens": ["Und", "schlies\u00b7se", "doch", "vor", "mei\u00b7ner", "ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den kopff/ das hertz/ und alles zu.", "tokens": ["Den", "kopff", "/", "das", "hertz", "/", "und", "al\u00b7les", "zu", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "KON", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Ich bin ein patiente/", "tokens": ["Ich", "bin", "ein", "pa\u00b7ti\u00b7en\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und lauffe vor der cur/", "tokens": ["Und", "lauf\u00b7fe", "vor", "der", "cur", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als wann mein r\u00fccken brennte/", "tokens": ["Als", "wann", "mein", "r\u00fc\u00b7cken", "brenn\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PPOSAT", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich suche zwar die spur/", "tokens": ["Ich", "su\u00b7che", "zwar", "die", "spur", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch wann ich ihrer inne bin/", "tokens": ["Doch", "wann", "ich", "ih\u00b7rer", "in\u00b7ne", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPOSAT", "PTKVZ", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So will der schwere leib nicht hin.", "tokens": ["So", "will", "der", "schwe\u00b7re", "leib", "nicht", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Ich schlaff und mu\u00df doch wachen/", "tokens": ["Ich", "schlaff", "und", "mu\u00df", "doch", "wa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich red und schweige doch/", "tokens": ["Ich", "red", "und", "schwei\u00b7ge", "doch", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich klag und mu\u00df auch lachen/", "tokens": ["Ich", "klag", "und", "mu\u00df", "auch", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich sterb und lebe noch/", "tokens": ["Ich", "sterb", "und", "le\u00b7be", "noch", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und dieses w\u00e4hrt schon lange zeit/", "tokens": ["Und", "die\u00b7ses", "w\u00e4hrt", "schon", "lan\u00b7ge", "zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bey tag und nacht ohn unterscheid.", "tokens": ["Bey", "tag", "und", "nacht", "ohn", "un\u00b7ter\u00b7scheid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Du allerliebste seele/", "tokens": ["Du", "al\u00b7ler\u00b7liebs\u00b7te", "see\u00b7le", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dir ist allein bewust/", "tokens": ["Dir", "ist", "al\u00b7lein", "be\u00b7wust", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich dein hertz erwehle", "tokens": ["Da\u00df", "ich", "dein", "hertz", "er\u00b7weh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu meiner zarten lust/", "tokens": ["Zu", "mei\u00b7ner", "zar\u00b7ten", "lust", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und da\u00df ich liebe/ weil das licht", "tokens": ["Und", "da\u00df", "ich", "lie\u00b7be", "/", "weil", "das", "licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$(", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des lebens mir den schein verspricht.", "tokens": ["Des", "le\u00b7bens", "mir", "den", "schein", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ART", "ADJD", "VVPP", "$."], "meter": "-----+-+", "measure": "unknown.measure.di"}}, "stanza.7": {"line.1": {"text": "7. Jtzt mu\u00df ich mich verstellen/", "tokens": ["Jtzt", "mu\u00df", "ich", "mich", "ver\u00b7stel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein kind/ du wirst von mir", "tokens": ["Mein", "kind", "/", "du", "wirst", "von", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PPER", "VAFIN", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein schlimmes urtheil f\u00e4llen/", "tokens": ["Kein", "schlim\u00b7mes", "ur\u00b7theil", "f\u00e4l\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann ich verspreche dir/", "tokens": ["Dann", "ich", "ver\u00b7spre\u00b7che", "dir", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein hertz soll unbefleckt und rein/", "tokens": ["Mein", "hertz", "soll", "un\u00b7be\u00b7fleckt", "und", "rein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gar bald bey deinem wieder seyn.", "tokens": ["Gar", "bald", "bey", "dei\u00b7nem", "wie\u00b7der", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "8. Wie mu\u00df ich mich bem\u00fchen/", "tokens": ["Wie", "mu\u00df", "ich", "mich", "be\u00b7m\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich es enden kan/", "tokens": ["Da\u00df", "ich", "es", "en\u00b7den", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann die magneten ziehen", "tokens": ["Dann", "die", "mag\u00b7ne\u00b7ten", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mein schwaches eisen an.", "tokens": ["Mein", "schwa\u00b7ches", "ei\u00b7sen", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und achten meine krafft nicht viel/", "tokens": ["Und", "ach\u00b7ten", "mei\u00b7ne", "krafft", "nicht", "viel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wann ich mich widersetzen wil.", "tokens": ["Wann", "ich", "mich", "wi\u00b7der\u00b7set\u00b7zen", "wil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "9. Doch halt ich mich zur\u00fccke", "tokens": ["Doch", "halt", "ich", "mich", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So lang ich halten kan/", "tokens": ["So", "lang", "ich", "hal\u00b7ten", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schaue deine blicke", "tokens": ["Und", "schau\u00b7e", "dei\u00b7ne", "bli\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur in gedancken an/", "tokens": ["Nur", "in", "ge\u00b7dan\u00b7cken", "an", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und suche die zufriedenheit", "tokens": ["Und", "su\u00b7che", "die", "zu\u00b7frie\u00b7den\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In der entfernten freundlichkeit.", "tokens": ["In", "der", "ent\u00b7fern\u00b7ten", "freund\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "10. So will ich dich probiren/", "tokens": ["So", "will", "ich", "dich", "pro\u00b7bi\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es steht die wieder frey", "tokens": ["Es", "steht", "die", "wie\u00b7der", "frey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich etwas rum zu f\u00fchren/", "tokens": ["Mich", "et\u00b7was", "rum", "zu", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch da wird meine treu", "tokens": ["Doch", "da", "wird", "mei\u00b7ne", "treu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In allen proben voller Schein", "tokens": ["In", "al\u00b7len", "pro\u00b7ben", "vol\u00b7ler", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie feines gold und silber seyn.", "tokens": ["Wie", "fei\u00b7nes", "gold", "und", "sil\u00b7ber", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}