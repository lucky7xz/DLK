{"dta.poem.2750": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Citronen/  \n  Bey Beerdigung Hn. S. S.   M. D.   ausge-  \n streuet/ den 14. Octobr. 1680.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich liefre deinem Grab/ mein Freund/ zu letzt Citronen/", "tokens": ["Ich", "lief\u00b7re", "dei\u00b7nem", "Grab", "/", "mein", "Freund", "/", "zu", "letzt", "Cit\u00b7ro\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "APPR", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer sucht bey tieffem Herbst der Tulipanen Zier?", "tokens": ["Wer", "sucht", "bey", "tief\u00b7fem", "Herbst", "der", "Tu\u00b7li\u00b7pa\u00b7nen", "Zier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du Gart- und Blumen Herr/ bist w\u00fcrdig da zu wohnen", "tokens": ["Du", "Gar\u00b7t", "und", "Blu\u00b7men", "Herr", "/", "bist", "w\u00fcr\u00b7dig", "da", "zu", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "TRUNC", "KON", "NN", "NN", "$(", "VAFIN", "ADJD", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Wo aus den Gr\u00e4bern auch ein Fr\u00fchling sprost\u2019 herf\u00fcr.", "tokens": ["Wo", "aus", "den", "Gr\u00e4\u00b7bern", "auch", "ein", "Fr\u00fch\u00b7ling", "sprost'", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Selbst Flora geht best\u00fcrtzt in einem Trauer-Kleide", "tokens": ["Selbst", "Flo\u00b7ra", "geht", "be\u00b7st\u00fcrtzt", "in", "ei\u00b7nem", "Trau\u00b7e\u00b7rKlei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie wei\u00df noch was du hast f\u00fcr Nutzen ihr geschafft.", "tokens": ["Sie", "wei\u00df", "noch", "was", "du", "hast", "f\u00fcr", "Nut\u00b7zen", "ihr", "ge\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PWS", "PPER", "VAFIN", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Lust-Gefilde stehn als wie bereifft im Leyde", "tokens": ["Die", "Lust\u00b7Ge\u00b7fil\u00b7de", "stehn", "als", "wie", "be\u00b7re\u00b7ifft", "im", "Ley\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KON", "PWAV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Da\u00df ihr Erhalter ist so zeitlich weggerafft.", "tokens": ["Da\u00df", "ihr", "Er\u00b7hal\u00b7ter", "ist", "so", "zeit\u00b7lich", "weg\u00b7ge\u00b7rafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was aber klaget doch das heil\u2019 ge Volck der Musen?", "tokens": ["Was", "a\u00b7ber", "kla\u00b7get", "doch", "das", "heil'", "ge", "Volck", "der", "Mu\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ADV", "ART", "ADJA", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Merckt Ph\u00f6bus sein Altar von alten Priestern blo\u00df?", "tokens": ["Merckt", "Ph\u00f6\u00b7bus", "sein", "Al\u00b7tar", "von", "al\u00b7ten", "Pries\u00b7tern", "blo\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PPOSAT", "NN", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie/ oder seh ich schon mit Fr\u00fcchten Hesperthusen", "tokens": ["Wie", "/", "o\u00b7der", "seh", "ich", "schon", "mit", "Fr\u00fcch\u00b7ten", "Hes\u00b7per\u00b7thu\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$(", "KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "NE"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der drey gedritten Schaar anbiethen Kampff und Lo\u00df?", "tokens": ["Der", "drey", "ge\u00b7drit\u00b7ten", "Schaar", "an\u00b7bie\u00b7then", "Kampff", "und", "Lo\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ADJA", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ach ja/ was sollen sie doch Sterbe-Lieder singen?", "tokens": ["Ach", "ja", "/", "was", "sol\u00b7len", "sie", "doch", "Ster\u00b7be\u00b7Lie\u00b7der", "sin\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$(", "PWS", "VMFIN", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und Hippocrene sich mit Thr\u00e4nen schwellen an?", "tokens": ["Und", "Hip\u00b7po\u00b7cre\u00b7ne", "sich", "mit", "Thr\u00e4\u00b7nen", "schwel\u00b7len", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PRF", "APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sie wollen dir vielmehr Triumph-Ges\u00e4nge bringen/", "tokens": ["Sie", "wol\u00b7len", "dir", "viel\u00b7mehr", "Tri\u00b7um\u00b7ph\u00b7Ge\u00b7s\u00e4n\u00b7ge", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "NN", "VVINF", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Da\u00df unter deinen Fu\u00df ist Noth und Tod gethan.", "tokens": ["Da\u00df", "un\u00b7ter", "dei\u00b7nen", "Fu\u00df", "ist", "Noth", "und", "Tod", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Nur/ weil du sie geehrt und mit erhitzten Sinnen", "tokens": ["Nur", "/", "weil", "du", "sie", "ge\u00b7ehrt", "und", "mit", "er\u00b7hitz\u00b7ten", "Sin\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$(", "KOUS", "PPER", "PPER", "VVPP", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Den K\u00fcnsten zugethan/ die Geister ausge\u00fcbt/", "tokens": ["Den", "K\u00fcns\u00b7ten", "zu\u00b7ge\u00b7than", "/", "die", "Geis\u00b7ter", "aus\u00b7ge\u00b7\u00fcbt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So dencken gleichfals auch die keuschen Castalinnen", "tokens": ["So", "den\u00b7cken", "gleich\u00b7fals", "auch", "die", "keu\u00b7schen", "Cas\u00b7ta\u00b7lin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wie ihre Schuldigkeit ein w\u00fcrcklich Zeugn\u00fc\u00df gibt.", "tokens": ["Wie", "ih\u00b7re", "Schul\u00b7dig\u00b7keit", "ein", "w\u00fcrck\u00b7lich", "Zeug\u00b7n\u00fc\u00df", "gibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Denn da\u00df du viel gesehn/ gelesen und ersahren/", "tokens": ["Denn", "da\u00df", "du", "viel", "ge\u00b7sehn", "/", "ge\u00b7le\u00b7sen", "und", "er\u00b7sah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "$(", "VVPP", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Viel hohe Schulen hast mit grossem Ruhm besucht/", "tokens": ["Viel", "ho\u00b7he", "Schu\u00b7len", "hast", "mit", "gros\u00b7sem", "Ruhm", "be\u00b7sucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Bl\u00fcthen angelegt von den begr\u00fcnten Jahren", "tokens": ["Die", "Bl\u00fc\u00b7then", "an\u00b7ge\u00b7legt", "von", "den", "be\u00b7gr\u00fcn\u00b7ten", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Geno\u00df gemeine Stadt nicht sonder Nutz und Frucht.", "tokens": ["Ge\u00b7no\u00df", "ge\u00b7mei\u00b7ne", "Stadt", "nicht", "son\u00b7der", "Nutz", "und", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PTKNEG", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Allein jetzt sinnen sie dein Grabmal aus zuziehren/", "tokens": ["Al\u00b7lein", "jetzt", "sin\u00b7nen", "sie", "dein", "Grab\u00b7mal", "aus", "zu\u00b7zieh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da drey Hesperides entgegen ihnen stehn;", "tokens": ["Da", "drey", "Hes\u00b7pe\u00b7ri\u00b7des", "ent\u00b7ge\u00b7gen", "ih\u00b7nen", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und melden da\u00df die Pflicht f\u00fcr sie nur will geb\u00fchren", "tokens": ["Und", "mel\u00b7den", "da\u00df", "die", "Pflicht", "f\u00fcr", "sie", "nur", "will", "ge\u00b7b\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOUS", "ART", "NN", "APPR", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Da\u00df Blumen vom Parna\u00df dein Lob doch nicht erh\u00f6hn.", "tokens": ["Da\u00df", "Blu\u00b7men", "vom", "Par\u00b7na\u00df", "dein", "Lob", "doch", "nicht", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "PPOSAT", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Ja diese schencken dir so sch\u00f6n\u2019 als g\u00fcldne Fr\u00fcchte/", "tokens": ["Ja", "die\u00b7se", "schen\u00b7cken", "dir", "so", "sch\u00f6n'", "als", "g\u00fcld\u00b7ne", "Fr\u00fcch\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PDS", "VVFIN", "PPER", "ADV", "ADJD", "KOKOM", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wo sind die Nymfen her? wo ist ihr Vaterland?", "tokens": ["Wo", "sind", "die", "Nym\u00b7fen", "her", "?", "wo", "ist", "ihr", "Va\u00b7ter\u00b7land", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PTKVZ", "$.", "PWAV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "G\u00f6ttinnen die ihr nie besuchet mein Gedichte", "tokens": ["G\u00f6t\u00b7tin\u00b7nen", "die", "ihr", "nie", "be\u00b7su\u00b7chet", "mein", "Ge\u00b7dich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "PPER", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.32": {"text": "Gebt zu/ da\u00df euch mein Reim macht etwas mehr bekandt.", "tokens": ["Gebt", "zu", "/", "da\u00df", "euch", "mein", "Reim", "macht", "et\u00b7was", "mehr", "be\u00b7kandt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "$(", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Prangt Hesperthusa nicht mit Schalen von Pomrantzen?", "tokens": ["Prangt", "Hes\u00b7per\u00b7thu\u00b7sa", "nicht", "mit", "Scha\u00b7len", "von", "Pom\u00b7rant\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKNEG", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ist mit Limonien nicht Arethusa reich?", "tokens": ["Ist", "mit", "Li\u00b7mo\u00b7ni\u00b7en", "nicht", "A\u00b7ret\u00b7hu\u00b7sa", "reich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NE", "PTKNEG", "NN", "ADJD", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.35": {"text": "Die Aegle will hier blo\u00df Citronen B\u00e4ume pflantzen/", "tokens": ["Die", "A\u00b7e\u00b7gle", "will", "hier", "blo\u00df", "Cit\u00b7ro\u00b7nen", "B\u00e4u\u00b7me", "pflant\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "ADJA", "NN", "VVINF", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "Du werther Podalir zu schm\u00fccken deine Leich\u2019.", "tokens": ["Du", "wert\u00b7her", "Po\u00b7da\u00b7lir", "zu", "schm\u00fc\u00b7cken", "dei\u00b7ne", "Leich'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Es sey Hesperien der Garten nun gepriesen/", "tokens": ["Es", "sey", "Hes\u00b7pe\u00b7ri\u00b7en", "der", "Gar\u00b7ten", "nun", "ge\u00b7prie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Da wo sich Lybien in so viel Kr\u00fcmmen senckt:", "tokens": ["Da", "wo", "sich", "Ly\u00b7bi\u00b7en", "in", "so", "viel", "Kr\u00fcm\u00b7men", "senckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PRF", "NE", "APPR", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Es sey das Mohrenland/ der Tingitaner Wiesen/", "tokens": ["Es", "sey", "das", "Moh\u00b7ren\u00b7land", "/", "der", "Tin\u00b7gi\u00b7ta\u00b7ner", "Wie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Des Gartens Kostbarkeit zum Eigenthum geschenckt;", "tokens": ["Des", "Gar\u00b7tens", "Kost\u00b7bar\u00b7keit", "zum", "Ei\u00b7gen\u00b7thum", "ge\u00b7schenckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Und Atlas mag allda den Himmel unterst\u00fctzen/", "tokens": ["Und", "At\u00b7las", "mag", "all\u00b7da", "den", "Him\u00b7mel", "un\u00b7ter\u00b7st\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VMFIN", "PAV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Damit dem Fabelwerck die Schmincke gar nicht fehlt:", "tokens": ["Da\u00b7mit", "dem", "Fa\u00b7bel\u00b7werck", "