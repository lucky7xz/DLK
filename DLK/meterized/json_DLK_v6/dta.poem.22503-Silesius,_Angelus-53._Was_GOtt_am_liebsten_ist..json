{"dta.poem.22503": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "53. Was GOtt am liebsten ist.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Nichts ist da\u00df GOtt so sehr als eine Jungfrau liebt/", "tokens": ["Nichts", "ist", "da\u00df", "Gott", "so", "sehr", "als", "ei\u00b7ne", "Jung\u00b7frau", "liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "KOUS", "NN", "ADV", "ADV", "KOUS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er auch jhr sich selbst zur Frucht un\u0303 Kind ergiebt:", "tokens": ["Da\u00df", "er", "auch", "jhr", "sich", "selbst", "zur", "Frucht", "u\u00f1", "Kind", "er\u00b7giebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "PRF", "ADV", "APPRART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wilstu sein Liebstes seyn noch hier auf dieser Erden/", "tokens": ["Wils\u00b7tu", "sein", "Liebs\u00b7tes", "seyn", "noch", "hier", "auf", "die\u00b7ser", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPOSAT", "ADV", "ADV", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So darffstu anders nichts als eine Jungfrau werden.", "tokens": ["So", "darffs\u00b7tu", "an\u00b7ders", "nichts", "als", "ei\u00b7ne", "Jung\u00b7frau", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "w