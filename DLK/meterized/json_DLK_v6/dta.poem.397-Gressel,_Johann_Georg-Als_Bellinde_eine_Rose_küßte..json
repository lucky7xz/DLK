{"dta.poem.397": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als  Bellinde  eine Rose k\u00fc\u00dfte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Bellinde schauet zwar die Rosen ihrer Wangen", "tokens": ["Bel\u00b7lin\u00b7de", "schau\u00b7et", "zwar", "die", "Ro\u00b7sen", "ih\u00b7rer", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch k\u00fc\u00dft ihr Rosen-Mund der Rosen Purpur-Pracht.", "tokens": ["Doch", "k\u00fc\u00dft", "ihr", "Ro\u00b7sen\u00b7Mund", "der", "Ro\u00b7sen", "Pur\u00b7pur\u00b7Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Wangen Rose kan den Ku\u00df nicht selbst empfangen/", "tokens": ["Der", "Wan\u00b7gen", "Ro\u00b7se", "kan", "den", "Ku\u00df", "nicht", "selbst", "emp\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VMFIN", "ART", "NN", "PTKNEG", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So hat ihr den Geschmack die Rose kund gemacht.", "tokens": ["So", "hat", "ihr", "den", "Ge\u00b7schmack", "die", "Ro\u00b7se", "kund", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}