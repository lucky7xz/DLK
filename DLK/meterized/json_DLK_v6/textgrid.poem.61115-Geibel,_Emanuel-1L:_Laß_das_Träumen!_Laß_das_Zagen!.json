{"textgrid.poem.61115": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: La\u00df das Tr\u00e4umen! La\u00df das Zagen!", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df das Tr\u00e4umen! La\u00df das Zagen!", "tokens": ["La\u00df", "das", "Tr\u00e4u\u00b7men", "!", "La\u00df", "das", "Za\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$.", "VVIMP", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unerm\u00fcdet wandre fort!", "tokens": ["Un\u00b7er\u00b7m\u00fc\u00b7det", "wand\u00b7re", "fort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will die Kraft dir schier versagen,", "tokens": ["Will", "die", "Kraft", "dir", "schier", "ver\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vorw\u00e4rts ist das rechte Wort.", "tokens": ["Vor\u00b7w\u00e4rts", "ist", "das", "rech\u00b7te", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Darfst nicht weilen, wenn die Stunde", "tokens": ["Darfst", "nicht", "wei\u00b7len", ",", "wenn", "die", "Stun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "PTKNEG", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rosen dir entgegenbringt,", "tokens": ["Ro\u00b7sen", "dir", "ent\u00b7ge\u00b7gen\u00b7bringt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn dir aus des Meeres Grunde", "tokens": ["Wenn", "dir", "aus", "des", "Mee\u00b7res", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Sirene lockend singt.", "tokens": ["Die", "Si\u00b7re\u00b7ne", "lo\u00b7ckend", "singt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Vorw\u00e4rts! vorw\u00e4rts! Im Gesange", "tokens": ["Vor\u00b7w\u00e4rts", "!", "vor\u00b7w\u00e4rts", "!", "Im", "Ge\u00b7san\u00b7ge"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "$.", "PTKVZ", "$.", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ringe mit dem Schmerz der Welt,", "tokens": ["Rin\u00b7ge", "mit", "dem", "Schmerz", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis auf deine hei\u00dfe Wange", "tokens": ["Bis", "auf", "dei\u00b7ne", "hei\u00b7\u00dfe", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Goldner Strahl von oben f\u00e4llt;", "tokens": ["Gold\u00b7ner", "Strahl", "von", "o\u00b7ben", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bis der Kranz, der dichtbelaubte,", "tokens": ["Bis", "der", "Kranz", ",", "der", "dicht\u00b7be\u00b7laub\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schattig deine Stirn umwebt,", "tokens": ["Schat\u00b7tig", "dei\u00b7ne", "Stirn", "um\u00b7webt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis verkl\u00e4rend \u00fcberm Haupte", "tokens": ["Bis", "ver\u00b7kl\u00e4\u00b7rend", "\u00fc\u00b7berm", "Haup\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "VVPP", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir des Geistes Flamme schwebt.", "tokens": ["Dir", "des", "Geis\u00b7tes", "Flam\u00b7me", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Vorw\u00e4rts drum durch Feindes Zinnen,", "tokens": ["Vor\u00b7w\u00e4rts", "drum", "durch", "Fein\u00b7des", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vorw\u00e4rts durch des Todes Pein!", "tokens": ["Vor\u00b7w\u00e4rts", "durch", "des", "To\u00b7des", "Pein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer den Himmel will gewinnen,", "tokens": ["Wer", "den", "Him\u00b7mel", "will", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df ein rechter K\u00e4mpfer sein.", "tokens": ["Mu\u00df", "ein", "rech\u00b7ter", "K\u00e4mp\u00b7fer", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}