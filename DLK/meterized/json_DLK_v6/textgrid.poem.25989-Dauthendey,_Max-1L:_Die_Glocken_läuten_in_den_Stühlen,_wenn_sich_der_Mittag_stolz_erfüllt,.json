{"textgrid.poem.25989": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Glocken l\u00e4uten in den St\u00fchlen, wenn sich der Mittag stolz erf\u00fcllt,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Glocken l\u00e4uten in den St\u00fchlen, wenn sich der Mittag stolz erf\u00fcllt,", "tokens": ["Die", "Glo\u00b7cken", "l\u00e4u\u00b7ten", "in", "den", "St\u00fch\u00b7len", ",", "wenn", "sich", "der", "Mit\u00b7tag", "stolz", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "KOUS", "PRF", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "So l\u00e4utet jubelnd mir mein Blut, wenn ich dich k\u00fcsse und die Sehnsucht stirbt.", "tokens": ["So", "l\u00e4u\u00b7tet", "ju\u00b7belnd", "mir", "mein", "Blut", ",", "wenn", "ich", "dich", "k\u00fcs\u00b7se", "und", "die", "Sehn\u00b7sucht", "stirbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Ich war wie die erfrorenen B\u00e4ume armselig und blind vor der Sonne,", "tokens": ["Ich", "war", "wie", "die", "er\u00b7fro\u00b7re\u00b7nen", "B\u00e4u\u00b7me", "arm\u00b7se\u00b7lig", "und", "blind", "vor", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Doch als unsere Blicke sich kreuzten, rauchte mein Herz.", "tokens": ["Doch", "als", "un\u00b7se\u00b7re", "Bli\u00b7cke", "sich", "kreuz\u00b7ten", ",", "rauch\u00b7te", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "--+--+--+-+--+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Wie ein Stahl steckt mir dein Blick in der Brust,", "tokens": ["Wie", "ein", "Stahl", "steckt", "mir", "dein", "Blick", "in", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ziehst du ihn aus, mu\u00df ich verbluten und sterben.", "tokens": ["Ziehst", "du", "ihn", "aus", ",", "mu\u00df", "ich", "ver\u00b7blu\u00b7ten", "und", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$,", "VMFIN", "PPER", "VVFIN", "KON", "VVINF", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}}}}}