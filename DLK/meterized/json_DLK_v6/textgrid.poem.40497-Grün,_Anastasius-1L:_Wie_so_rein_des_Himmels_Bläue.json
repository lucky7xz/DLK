{"textgrid.poem.40497": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie so rein des Himmels Bl\u00e4ue", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie so rein des Himmels Bl\u00e4ue", "tokens": ["Wie", "so", "rein", "des", "Him\u00b7mels", "Bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber meinem Haupte gl\u00e4nzt,", "tokens": ["Ue\u00b7ber", "mei\u00b7nem", "Haup\u00b7te", "gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fest und licht wie ew'ge Treue,", "tokens": ["Fest", "und", "licht", "wie", "ew'\u00b7ge", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KOKOM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wandellos und unbegrenzt!", "tokens": ["Wan\u00b7del\u00b7los", "und", "un\u00b7be\u00b7grenzt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gleich dem ew'gen Frieden schimmert", "tokens": ["Gleich", "dem", "ew'\u00b7gen", "Frie\u00b7den", "schim\u00b7mert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruhig, klar und gr\u00fcn das Meer;", "tokens": ["Ru\u00b7hig", ",", "klar", "und", "gr\u00fcn", "das", "Meer", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die heil'ge Liebe flimmert", "tokens": ["Wie", "die", "heil'\u00b7ge", "Lie\u00b7be", "flim\u00b7mert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hell die Sonne dr\u00fcber her.", "tokens": ["Hell", "die", "Son\u00b7ne", "dr\u00fc\u00b7ber", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PAV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Frei und leicht auf freien Wogen", "tokens": ["Frei", "und", "leicht", "auf", "frei\u00b7en", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zog das Schiff die eb'ne Bahn,", "tokens": ["Zog", "das", "Schiff", "die", "eb'\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stolz die wei\u00dfen Segel flogen", "tokens": ["Stolz", "die", "wei\u00b7\u00dfen", "Se\u00b7gel", "flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der Freiheit Siegesfahn'.", "tokens": ["Wie", "der", "Frei\u00b7heit", "Sie\u00b7ges\u00b7fahn'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sonne, Meer und Himmelsbl\u00e4ue,", "tokens": ["Son\u00b7ne", ",", "Meer", "und", "Him\u00b7mels\u00b7bl\u00e4u\u00b7e", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nichts ums Schiff sonst ringsumher!", "tokens": ["Nichts", "ums", "Schiff", "sonst", "rings\u00b7um\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPRART", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebe, Freiheit, Fried' und Treue!", "tokens": ["Lie\u00b7be", ",", "Frei\u00b7heit", ",", "Fried'", "und", "Treu\u00b7e", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ei, was willst du denn noch mehr?", "tokens": ["Ei", ",", "was", "willst", "du", "denn", "noch", "mehr", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VMFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach, wenn nur der Wind vom Lande", "tokens": ["Ach", ",", "wenn", "nur", "der", "Wind", "vom", "Lan\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "ADV", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir ein gr\u00fcnes Blatt allein,", "tokens": ["Mir", "ein", "gr\u00fc\u00b7nes", "Blatt", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Bl\u00fcthe nur vom Strande", "tokens": ["Ei\u00b7ne", "Bl\u00fc\u00b7the", "nur", "vom", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wehte in das Schiff hinein!", "tokens": ["Weh\u00b7te", "in", "das", "Schiff", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}