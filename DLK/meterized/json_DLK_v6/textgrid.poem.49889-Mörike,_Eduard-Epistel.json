{"textgrid.poem.49889": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Epistel", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sich dein neuer Poet in unserem Kreise gefalle?", "tokens": ["Wie", "sich", "dein", "neu\u00b7er", "Po\u00b7et", "in", "un\u00b7se\u00b7rem", "Krei\u00b7se", "ge\u00b7fal\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Nicht zum besten. Er meint, man verst\u00fcnd ihn eben auch hier nicht.", "tokens": ["Nicht", "zum", "bes\u00b7ten", ".", "Er", "meint", ",", "man", "ver\u00b7st\u00fcnd", "ihn", "e\u00b7ben", "auch", "hier", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "ADJA", "$.", "PPER", "VVFIN", "$,", "PIS", "VVFIN", "PPER", "ADV", "ADV", "ADV", "PTKNEG", "$."], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Jetzo hat er ein griechisches Epos, h\u00f6r ich, die Argonauten,", "tokens": ["Jet\u00b7zo", "hat", "er", "ein", "grie\u00b7chi\u00b7sches", "E\u00b7pos", ",", "h\u00f6r", "ich", ",", "die", "Ar\u00b7go\u00b7nau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "heroische Form, auf dem Ambo\u00df. Segn' es der Gott ihm,", "tokens": ["he\u00b7ro\u00b7i\u00b7sche", "Form", ",", "auf", "dem", "Am\u00b7bo\u00df", ".", "Segn'", "es", "der", "Gott", "ihm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ART", "NN", "$.", "NN", "PPER", "ART", "NN", "PPER", "$,"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Aber zu lesen begehr ich es nicht. Glaub mir, das ist auch so", "tokens": ["A\u00b7ber", "zu", "le\u00b7sen", "be\u00b7gehr", "ich", "es", "nicht", ".", "Glaub", "mir", ",", "das", "ist", "auch", "so"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKZU", "VVINF", "VVFIN", "PPER", "PPER", "PTKNEG", "$.", "NN", "PPER", "$,", "PDS", "VAFIN", "ADV", "ADV"], "meter": "+--+--+--++--+-+", "measure": "dactylic.tri.plus"}, "line.6": {"text": "Eins von den sauren Genies, dergleichen wir mehrere kennen.", "tokens": ["Eins", "von", "den", "sau\u00b7ren", "Ge\u00b7nies", ",", "derg\u00b7lei\u00b7chen", "wir", "meh\u00b7re\u00b7re", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,", "PIS", "PPER", "PIAT", "VVINF", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.7": {"text": "Wortkarg streicht er den Schnurrbart sich, wie verstimmt und befangen,", "tokens": ["Wort\u00b7karg", "streicht", "er", "den", "Schnurr\u00b7bart", "sich", ",", "wie", "ver\u00b7stimmt", "und", "be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "PRF", "$,", "PWAV", "VVPP", "KON", "VVPP", "$,"], "meter": "--+--++-+-+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Wenn man des Trefflichsten irgend gedenkt von den Alten und Neuen;", "tokens": ["Wenn", "man", "des", "Treff\u00b7lichs\u00b7ten", "ir\u00b7gend", "ge\u00b7denkt", "von", "den", "Al\u00b7ten", "und", "Neu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ADV", "VVFIN", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.9": {"text": "Oder er m\u00e4kelt daran mit kleinlichem Tadel, von fern erst,", "tokens": ["O\u00b7der", "er", "m\u00e4\u00b7kelt", "da\u00b7ran", "mit", "klein\u00b7li\u00b7chem", "Ta\u00b7del", ",", "von", "fern", "erst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PAV", "APPR", "ADJA", "NN", "$,", "APPR", "ADJD", "ADV", "$,"], "meter": "+--+--+-+--+-+-+", "measure": "dactylic.di.plus"}, "line.10": {"text": "Bis er, hitziger werdend im Streit, Ma\u00dfloses daherschwatzt", "tokens": ["Bis", "er", ",", "hit\u00b7zi\u00b7ger", "wer\u00b7dend", "im", "Streit", ",", "Ma\u00df\u00b7lo\u00b7ses", "da\u00b7her\u00b7schwatzt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ADJD", "ADJD", "APPRART", "NN", "$,", "NN", "VVFIN"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.11": {"text": "Und wie ein st\u00e4tischer Esel hinausschl\u00e4gt, wo es auch hintrifft.", "tokens": ["Und", "wie", "ein", "st\u00e4\u00b7ti\u00b7scher", "E\u00b7sel", "hin\u00b7aus\u00b7schl\u00e4gt", ",", "wo", "es", "auch", "hin\u00b7trifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$,", "PWAV", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.12": {"text": "Das sind schlimme Symptome. \u2013 Vernimm ein homerisches Gleichnis", "tokens": ["Das", "sind", "schlim\u00b7me", "Symp\u00b7to\u00b7me", ".", "\u2013", "Ver\u00b7nimm", "ein", "ho\u00b7me\u00b7ri\u00b7sches", "Gleich\u00b7nis"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$.", "$(", "VVIMP", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.13": {"text": "(pflegten wir doch vormals in parodischer Laune zuweilen", "tokens": ["(", "pfleg\u00b7ten", "wir", "doch", "vor\u00b7mals", "in", "pa\u00b7ro\u00b7di\u00b7scher", "Lau\u00b7ne", "zu\u00b7wei\u00b7len"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Stundenlang nach der Weise des g\u00f6ttlichen Alten zu reden):", "tokens": ["Stun\u00b7den\u00b7lang", "nach", "der", "Wei\u00b7se", "des", "g\u00f6tt\u00b7li\u00b7chen", "Al\u00b7ten", "zu", "re\u00b7den", ")", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$(", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.15": {"text": "Gleichwie die gelbliche Birne zur Herbstzeit, wenn sie gereifet", "tokens": ["Gleich\u00b7wie", "die", "gelb\u00b7li\u00b7che", "Bir\u00b7ne", "zur", "Herbst\u00b7zeit", ",", "wenn", "sie", "ge\u00b7rei\u00b7fet"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "NN", "$,", "KOUS", "PPER", "VVPP"], "meter": "+--+--+--+--+-+-", "measure": "dactylic.tetra.plus"}, "line.16": {"text": "Fiel vom Ast und im Fall von der dornigen Hecke verwundet", "tokens": ["Fiel", "vom", "Ast", "und", "im", "Fall", "von", "der", "dor\u00b7ni\u00b7gen", "He\u00b7cke", "ver\u00b7wun\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "KON", "APPRART", "NN", "APPR", "ART", "ADJA", "NN", "VVPP"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.17": {"text": "Liegt am Boden, alsbald mit schw\u00e4rmenden Wespen bedeckt ist,", "tokens": ["Liegt", "am", "Bo\u00b7den", ",", "als\u00b7bald", "mit", "schw\u00e4r\u00b7men\u00b7den", "Wes\u00b7pen", "be\u00b7deckt", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "ADV", "APPR", "ADJA", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Welche sie rings aush\u00f6hlen, die gierigen Kiefer bewegend:", "tokens": ["Wel\u00b7che", "sie", "rings", "aus\u00b7h\u00f6h\u00b7len", ",", "die", "gie\u00b7ri\u00b7gen", "Kie\u00b7fer", "be\u00b7we\u00b7gend", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVINF", "$,", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.19": {"text": "Also strotzet sein Herz von wilden Gedanken der Ehrsucht", "tokens": ["Al\u00b7so", "strot\u00b7zet", "sein", "Herz", "von", "wil\u00b7den", "Ge\u00b7dan\u00b7ken", "der", "Ehr\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "Und des verzehrenden Neids. Ihn blendete v\u00f6llig ein D\u00e4mon.", "tokens": ["Und", "des", "ver\u00b7zeh\u00b7ren\u00b7den", "Neids", ".", "Ihn", "blen\u00b7de\u00b7te", "v\u00f6l\u00b7lig", "ein", "D\u00e4\u00b7mon", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "---+--+-+--+--+-", "measure": "iambic.penta.relaxed"}}}}}