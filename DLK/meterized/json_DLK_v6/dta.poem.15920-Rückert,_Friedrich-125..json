{"dta.poem.15920": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "125.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zufrieden mu\u00dft du seyn, zufrieden mit der Welt,", "tokens": ["Zu\u00b7frie\u00b7den", "mu\u00dft", "du", "seyn", ",", "zu\u00b7frie\u00b7den", "mit", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VAINF", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es halten so mit ihr, wie sie mit dir es h\u00e4lt.", "tokens": ["Es", "hal\u00b7ten", "so", "mit", "ihr", ",", "wie", "sie", "mit", "dir", "es", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$,", "PWAV", "PPER", "APPR", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zufrieden mu\u00dft du seyn mit dem, was Gott beschieden,", "tokens": ["Zu\u00b7frie\u00b7den", "mu\u00dft", "du", "seyn", "mit", "dem", ",", "was", "Gott", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VAINF", "APPR", "ART", "$,", "PRELS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Besonders aber mu\u00dft du seyn mit dir zufrieden.", "tokens": ["Be\u00b7son\u00b7ders", "a\u00b7ber", "mu\u00dft", "du", "seyn", "mit", "dir", "zu\u00b7frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VAINF", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer nie zufrieden ist mit dem, was er vollbracht,", "tokens": ["Wer", "nie", "zu\u00b7frie\u00b7den", "ist", "mit", "dem", ",", "was", "er", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VAFIN", "APPR", "ART", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist es auch nicht mit Gott, der so ihn hat gemacht.", "tokens": ["Ist", "es", "auch", "nicht", "mit", "Gott", ",", "der", "so", "ihn", "hat", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "APPR", "NN", "$,", "PRELS", "ADV", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}