{"dta.poem.10547": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Ilse .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin die Prinzessin Ilse,", "tokens": ["Ich", "bin", "die", "Prin\u00b7zes\u00b7sin", "Il\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und wohne im Ilsenstein;", "tokens": ["Und", "woh\u00b7ne", "im", "Il\u00b7sen\u00b7stein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Komm mit nach meinem Schlosse,", "tokens": ["Komm", "mit", "nach", "mei\u00b7nem", "Schlos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir wollen selig seyn.", "tokens": ["Wir", "wol\u00b7len", "se\u00b7lig", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dein Haupt will ich benetzen", "tokens": ["Dein", "Haupt", "will", "ich", "be\u00b7net\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit meiner klaren Well',", "tokens": ["Mit", "mei\u00b7ner", "kla\u00b7ren", "Well'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du sollst deine Schmerzen vergessen,", "tokens": ["Du", "sollst", "dei\u00b7ne", "Schmer\u00b7zen", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Du sorgenkranker Gesell!", "tokens": ["Du", "sor\u00b7gen\u00b7kran\u00b7ker", "Ge\u00b7sell", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "In meinen wei\u00dfen Armen,", "tokens": ["In", "mei\u00b7nen", "wei\u00b7\u00dfen", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An meiner wei\u00dfen Brust,", "tokens": ["An", "mei\u00b7ner", "wei\u00b7\u00dfen", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sollst du liegen und tr\u00e4umen", "tokens": ["Da", "sollst", "du", "lie\u00b7gen", "und", "tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "KON", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von alter M\u00e4hrchenlust.", "tokens": ["Von", "al\u00b7ter", "M\u00e4hr\u00b7chen\u00b7lust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich will dich k\u00fcssen und herzen", "tokens": ["Ich", "will", "dich", "k\u00fcs\u00b7sen", "und", "her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "KON", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie ich geherzt und gek\u00fc\u00dft", "tokens": ["Wie", "ich", "ge\u00b7herzt", "und", "ge\u00b7k\u00fc\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVPP", "KON", "VVPP"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Den lieben Kaiser Heinrich,", "tokens": ["Den", "lie\u00b7ben", "Kai\u00b7ser", "Hein\u00b7rich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Der nun gestorben ist.", "tokens": ["Der", "nun", "ge\u00b7stor\u00b7ben", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es bleiben todt die Todten,", "tokens": ["Es", "blei\u00b7ben", "todt", "die", "Tod\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und nur der Lebendige liebt;", "tokens": ["Und", "nur", "der", "Le\u00b7ben\u00b7di\u00b7ge", "liebt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ich bin sch\u00f6n und bl\u00fchend,", "tokens": ["Und", "ich", "bin", "sch\u00f6n", "und", "bl\u00fc\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein lachendes Herze bebt.", "tokens": ["Mein", "la\u00b7chen\u00b7des", "Her\u00b7ze", "bebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Und bebt mein Herz dort unten,", "tokens": ["Und", "bebt", "mein", "Herz", "dort", "un\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So klingt mein kristallenes Schlo\u00df,", "tokens": ["So", "klingt", "mein", "kris\u00b7tal\u00b7le\u00b7nes", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Es tanzen die Fr\u00e4ulein und Ritter,", "tokens": ["Es", "tan\u00b7zen", "die", "Fr\u00e4u\u00b7lein", "und", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Es jubelt der Knappentro\u00df.", "tokens": ["Es", "ju\u00b7belt", "der", "Knap\u00b7pen\u00b7tro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Es rauschen die seidenen Schleppen,", "tokens": ["Es", "rau\u00b7schen", "die", "sei\u00b7de\u00b7nen", "Schlep\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Es klirren die Eisenspor'n,", "tokens": ["Es", "klir\u00b7ren", "die", "Ei\u00b7sen\u00b7spor'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Zwerge trompeten und pauken,", "tokens": ["Die", "Zwer\u00b7ge", "trom\u00b7pe\u00b7ten", "und", "pau\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und fiedeln und blasen das Horn.", "tokens": ["Und", "fie\u00b7deln", "und", "bla\u00b7sen", "das", "Horn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.8": {"line.1": {"text": "Doch dich soll mein Arm umschlingen", "tokens": ["Doch", "dich", "soll", "mein", "Arm", "um\u00b7schlin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Wie er Kaiser Heinrich umschlang;", "tokens": ["Wie", "er", "Kai\u00b7ser", "Hein\u00b7rich", "um\u00b7schlang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "NE", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Ich hielt ihm zu die Ohren,", "tokens": ["Ich", "hielt", "ihm", "zu", "die", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn die Trompet' erklang.", "tokens": ["Wenn", "die", "Trom\u00b7pet'", "er\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}}}}