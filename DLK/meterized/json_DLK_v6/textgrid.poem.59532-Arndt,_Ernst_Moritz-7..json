{"textgrid.poem.59532": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott, deine Kindlein treten", "tokens": ["Gott", ",", "dei\u00b7ne", "Kin\u00b7dlein", "tre\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVINF"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Freuden zu dir hin,", "tokens": ["Mit", "Freu\u00b7den", "zu", "dir", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie stammeln und sie beten;", "tokens": ["Sie", "stam\u00b7meln", "und", "sie", "be\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du kennst der Worte Sinn:", "tokens": ["Du", "kennst", "der", "Wor\u00b7te", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was aus dem Borne quillet,", "tokens": ["Was", "aus", "dem", "Bor\u00b7ne", "quil\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der nimmermehr versiegt,", "tokens": ["Der", "nim\u00b7mer\u00b7mehr", "ver\u00b7siegt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was ihnen selbst verh\u00fcllet", "tokens": ["Was", "ih\u00b7nen", "selbst", "ver\u00b7h\u00fcl\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im tiefsten Herzen liegt,", "tokens": ["Im", "tiefs\u00b7ten", "Her\u00b7zen", "liegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das lockst du hoch nach oben", "tokens": ["Das", "lockst", "du", "hoch", "nach", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In seliger Begier,", "tokens": ["In", "se\u00b7li\u00b7ger", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Milde dein zu loben", "tokens": ["Die", "Mil\u00b7de", "dein", "zu", "lo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und G\u00fcte f\u00fcr und f\u00fcr.", "tokens": ["Und", "G\u00fc\u00b7te", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O du, der in den H\u00f6hen", "tokens": ["O", "du", ",", "der", "in", "den", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und in den Tiefen wohnt,", "tokens": ["Und", "in", "den", "Tie\u00b7fen", "wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df kindlich uns verstehen,", "tokens": ["La\u00df", "kind\u00b7lich", "uns", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was \u00fcberschwenglich lohnt.", "tokens": ["Was", "\u00fc\u00b7bersc\u00b7hweng\u00b7lich", "lohnt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Gib fromme Kinderworte,", "tokens": ["Gib", "from\u00b7me", "Kin\u00b7der\u00b7wor\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gib s\u00fc\u00dfen Kinderwahn!", "tokens": ["Gib", "s\u00fc\u00b7\u00dfen", "Kin\u00b7der\u00b7wahn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So wird uns nur die Pforte", "tokens": ["So", "wird", "uns", "nur", "die", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Himmel aufgetan.", "tokens": ["Der", "Him\u00b7mel", "auf\u00b7ge\u00b7tan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}