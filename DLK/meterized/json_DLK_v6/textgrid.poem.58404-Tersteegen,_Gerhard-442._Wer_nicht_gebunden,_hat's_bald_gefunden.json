{"textgrid.poem.58404": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "442. Wer nicht gebunden, hat's bald gefunden", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Geist ist ein Gewicht, das stets zum Grund sich neigt,", "tokens": ["Dein", "Geist", "ist", "ein", "Ge\u00b7wicht", ",", "das", "stets", "zum", "Grund", "sich", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein Mittelpunkt ist Gott, der ihn ber\u00fchrt und zeucht;", "tokens": ["Sein", "Mit\u00b7tel\u00b7punkt", "ist", "Gott", ",", "der", "ihn", "be\u00b7r\u00fchrt", "und", "zeucht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "PRELS", "PPER", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "La\u00df los die Kreatur, die ihn so leicht kann binden,", "tokens": ["La\u00df", "los", "die", "Kre\u00b7a\u00b7tur", ",", "die", "ihn", "so", "leicht", "kann", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So sinkt er selbst zu Grund, da Gott in dir zu finden!", "tokens": ["So", "sinkt", "er", "selbst", "zu", "Grund", ",", "da", "Gott", "in", "dir", "zu", "fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,", "KOUS", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}