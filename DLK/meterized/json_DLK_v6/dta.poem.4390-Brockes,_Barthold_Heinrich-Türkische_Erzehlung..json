{"dta.poem.4390": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "T\u00fcrkische Erzehlung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ward ein Sultan in Egypten, der immer sein Ge-", "tokens": ["Es", "ward", "ein", "Sul\u00b7tan", "in", "E\u00b7gyp\u00b7ten", ",", "der", "im\u00b7mer", "sein", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NE", "$,", "PRELS", "ADV", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "sp\u00f6tt getrieben", "tokens": ["sp\u00f6tt", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word"], "pos": ["ADJD", "VVPP"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Mit dem, was Mahomet von sich (er w\u00e4r einst aus dem", "tokens": ["Mit", "dem", ",", "was", "Ma\u00b7ho\u00b7met", "von", "sich", "(", "er", "w\u00e4r", "einst", "aus", "dem"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "$,", "PRELS", "NE", "APPR", "PRF", "$(", "PPER", "VAFIN", "ADV", "APPR", "ART"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Bett genommen,", "tokens": ["Bett", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Und, nach fast hundert tausend Reden mit Gabriel, zur\u00fcck", "tokens": ["Und", ",", "nach", "fast", "hun\u00b7dert", "tau\u00b7send", "Re\u00b7den", "mit", "Ga\u00b7bri\u00b7el", ",", "zu\u00b7r\u00fcck"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "APPR", "ADV", "CARD", "CARD", "NN", "APPR", "NE", "$,", "PTKVZ"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.6": {"text": "gekommen,", "tokens": ["ge\u00b7kom\u00b7men", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Da er sein Bett noch warm gefunden) in seinen Alcoran", "tokens": ["Da", "er", "sein", "Bett", "noch", "warm", "ge\u00b7fun\u00b7den", ")", "in", "sei\u00b7nen", "Al\u00b7co\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$(", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "geschrieben,", "tokens": ["ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Von einem Heiligen einmahl, nebst seiner Hofstadt, hinge-", "tokens": ["Von", "ei\u00b7nem", "Hei\u00b7li\u00b7gen", "ein\u00b7mahl", ",", "nebst", "sei\u00b7ner", "Hof\u00b7stadt", ",", "hin\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "stellt", "tokens": ["stellt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Bey einem grossen Wasser-K\u00fcfen. Mit Bitt\u2019, er m\u00f6gte", "tokens": ["Bey", "ei\u00b7nem", "gros\u00b7sen", "Was\u00b7ser\u00b7K\u00fc\u00b7fen", ".", "Mit", "Bitt'", ",", "er", "m\u00f6g\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "APPR", "NN", "$,", "PPER", "VMFIN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "sich bem\u00fchen,", "tokens": ["sich", "be\u00b7m\u00fc\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Sein Haupt in dieses Na\u00df zu tunken, und alsobald her-", "tokens": ["Sein", "Haupt", "in", "die\u00b7ses", "Na\u00df", "zu", "tun\u00b7ken", ",", "und", "al\u00b7so\u00b7bald", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$,", "KON", "ADV", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "aus zu ziehen.", "tokens": ["aus", "zu", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Er thate kaum, was jener bate, so fand er sich bey einem", "tokens": ["Er", "tha\u00b7te", "kaum", ",", "was", "je\u00b7ner", "ba\u00b7te", ",", "so", "fand", "er", "sich", "bey", "ei\u00b7nem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PDS", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PRF", "APPR", "ART"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Zelt,", "tokens": ["Zelt", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Am Ufer eines grossen Meeres, an eines steilen Berges", "tokens": ["Am", "U\u00b7fer", "ei\u00b7nes", "gros\u00b7sen", "Mee\u00b7res", ",", "an", "ei\u00b7nes", "stei\u00b7len", "Ber\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "Fu\u00df.", "tokens": ["Fu\u00df", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Nun mogt\u2019 er, wie er wollte, fluchen auf seinen Heiligen,", "tokens": ["Nun", "mogt'", "er", ",", "wie", "er", "woll\u00b7te", ",", "flu\u00b7chen", "auf", "sei\u00b7nen", "Hei\u00b7li\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "er mu\u00df,", "tokens": ["er", "mu\u00df", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.21": {"text": "Um Kost und Nahrung zu gewinnen, sich unges\u00e4umt", "tokens": ["Um", "Kost", "und", "Nah\u00b7rung", "zu", "ge\u00b7win\u00b7nen", ",", "sich", "un\u00b7ge\u00b7s\u00e4umt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUI", "NN", "KON", "NN", "PTKZU", "VVINF", "$,", "PRF", "ADJD"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "sogleich bequehmen,", "tokens": ["sog\u00b7leich", "be\u00b7queh\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Die Zuflucht zu verschiednen Leuten, die er daselbsten fand,", "tokens": ["Die", "Zu\u00b7flucht", "zu", "ver\u00b7schied\u00b7nen", "Leu\u00b7ten", ",", "die", "er", "da\u00b7selbs\u00b7ten", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,", "PRELS", "PPER", "PDS", "VVFIN", "$,"], "meter": "--+--+-+-+--+-+", "measure": "anapaest.di.plus"}, "line.24": {"text": "zu nehmen,", "tokens": ["zu", "neh\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Die f\u00fchren ihn nach einer Stadt, so von dem Ort nicht", "tokens": ["Die", "f\u00fch\u00b7ren", "ihn", "nach", "ei\u00b7ner", "Stadt", ",", "so", "von", "dem", "Ort", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "weit entlegen,", "tokens": ["weit", "ent\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Woselbst, nach manchen fremden F\u00e4llen, die gar zu lang", "tokens": ["Wo\u00b7selbst", ",", "nach", "man\u00b7chen", "frem\u00b7den", "F\u00e4l\u00b7len", ",", "die", "gar", "zu", "lang"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "PIAT", "ADJA", "NN", "$,", "PRELS", "ADV", "PTKA", "ADJD"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "sind zu erzehlen,", "tokens": ["sind", "zu", "er\u00b7zeh\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.29": {"text": "Er sich mit einer reichen Frau beschlo\u00df geruhig zu ver-", "tokens": ["Er", "sich", "mit", "ei\u00b7ner", "rei\u00b7chen", "Frau", "be\u00b7schlo\u00df", "ge\u00b7ru\u00b7hig", "zu", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PRF", "APPR", "ART", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.30": {"text": "m\u00e4hlen.", "tokens": ["m\u00e4h\u00b7len", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.31": {"text": "Er zeugte mit ihr vierzehn Kinder. Zuletzt war ihm das", "tokens": ["Er", "zeug\u00b7te", "mit", "ihr", "vier\u00b7zehn", "Kin\u00b7der", ".", "Zu\u00b7letzt", "war", "ihm", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "CARD", "NN", "$.", "ADV", "VAFIN", "PPER", "ART"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.32": {"text": "Gl\u00fcck zugegen,", "tokens": ["Gl\u00fcck", "zu\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.33": {"text": "So da\u00df er all sein Gut verlohr, und sich aus Noht ent-", "tokens": ["So", "da\u00df", "er", "all", "sein", "Gut", "ver\u00b7lohr", ",", "und", "sich", "aus", "Noht", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PIAT", "PPOSAT", "NN", "VVFIN", "$,", "KON", "PRF", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "schliessen mu\u00dfte,", "tokens": ["schlies\u00b7sen", "mu\u00df\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.35": {"text": "Ein armer Arbeits-Mann zu werden, weil er sich nicht zu", "tokens": ["Ein", "ar\u00b7mer", "Ar\u00b7beits\u00b7Mann", "zu", "wer\u00b7den", ",", "weil", "er", "sich", "nicht", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VAINF", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "PTKZU"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.36": {"text": "helfen wu\u00dfte.", "tokens": ["hel\u00b7fen", "wu\u00df\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.37": {"text": "Bis er, voll Traurigkeit und Schwermuht, einst an des", "tokens": ["Bis", "er", ",", "voll", "Trau\u00b7rig\u00b7keit", "und", "Schwer\u00b7muht", ",", "einst", "an", "des"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ADJD", "NN", "KON", "NN", "$,", "ADV", "APPR", "ART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "Meeres Ufer ging,", "tokens": ["Mee\u00b7res", "U\u00b7fer", "ging", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.39": {"text": "Und seinen ungl\u00fccksel\u2019gen Stand, und alle fremde F\u00e4ll\u2019,", "tokens": ["Und", "sei\u00b7nen", "un\u00b7gl\u00fcck\u00b7sel'\u00b7gen", "Stand", ",", "und", "al\u00b7le", "frem\u00b7de", "F\u00e4ll'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,", "KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.40": {"text": "erwog,", "tokens": ["er\u00b7wog", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.41": {"text": "Auch, voller Reu, nach ihren S\u00e4tzen, im Meer sich an", "tokens": ["Auch", ",", "vol\u00b7ler", "Reu", ",", "nach", "ih\u00b7ren", "S\u00e4t\u00b7zen", ",", "im", "Meer", "sich", "an"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADJA", "NE", "$,", "APPR", "PPOSAT", "NN", "$,", "APPRART", "NN", "PRF", "APPR"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.42": {"text": "zu waschen fing.", "tokens": ["zu", "wa\u00b7schen", "fing", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.43": {"text": "Kaum aber, da\u00df er, nach dem Bade, sich wieder aus dem", "tokens": ["Kaum", "a\u00b7ber", ",", "da\u00df", "er", ",", "nach", "dem", "Ba\u00b7de", ",", "sich", "wie\u00b7der", "aus", "dem"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "$,", "APPR", "ART", "NN", "$,", "PRF", "ADV", "APPR", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.44": {"text": "Wasser zog;", "tokens": ["Was\u00b7ser", "zog", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.45": {"text": "So fand er sich bey seiner K\u00fcfe, beym Lehrer und der", "tokens": ["So", "fand", "er", "sich", "bey", "sei\u00b7ner", "K\u00fc\u00b7fe", ",", "beym", "Leh\u00b7rer", "und", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,", "APPRART", "NN", "KON", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.46": {"text": "Hofstadt, stehn.", "tokens": ["Hof\u00b7stadt", ",", "stehn", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.47": {"text": "Er fing, wie leichtlich zu erachten, ob allem, was mit ihm", "tokens": ["Er", "fing", ",", "wie", "leicht\u00b7lich", "zu", "e\u00b7rach\u00b7ten", ",", "ob", "al\u00b7lem", ",", "was", "mit", "ihm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADJD", "PTKZU", "VVINF", "$,", "KOUS", "PIS", "$,", "PRELS", "APPR", "PPER"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.48": {"text": "geschehn,", "tokens": ["ge\u00b7schehn", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.49": {"text": "Mit vieler Streng- und Heftigkeit, auf seinen Heil\u2019gen an", "tokens": ["Mit", "vie\u00b7ler", "Streng", "und", "Hef\u00b7tig\u00b7keit", ",", "auf", "sei\u00b7nen", "Heil'\u00b7gen", "an"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "TRUNC", "KON", "NN", "$,", "APPR", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.50": {"text": "zu schm\u00e4hlen,", "tokens": ["zu", "schm\u00e4h\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.51": {"text": "Da\u00df er ihn so gequ\u00e4let h\u00e4tte, und zwar so eine lange", "tokens": ["Da\u00df", "er", "ihn", "so", "ge\u00b7qu\u00e4\u00b7let", "h\u00e4t\u00b7te", ",", "und", "zwar", "so", "ei\u00b7ne", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVPP", "VAFIN", "$,", "KON", "ADV", "ADV", "ART", "ADJA"], "meter": "--+--+-+--+-+-+-", "measure": "anapaest.di.plus"}, "line.52": {"text": "Zeit.", "tokens": ["Zeit", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.53": {"text": "Allein, wie wundert er sich nicht auf aller seiner Leut\u2019", "tokens": ["Al\u00b7lein", ",", "wie", "wun\u00b7dert", "er", "sich", "nicht", "auf", "al\u00b7ler", "sei\u00b7ner", "Leut