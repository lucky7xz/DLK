{"textgrid.poem.52079": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich liebe dich, da\u00df ich dich m\u00f6cht' aus Liebe fre\u00dfen,", "tokens": ["Ich", "lie\u00b7be", "dich", ",", "da\u00df", "ich", "dich", "m\u00f6cht'", "aus", "Lie\u00b7be", "fre\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sprach der von P\u00fcschel nechst, mein Thomasen, zu dir,", "tokens": ["Sprach", "der", "von", "P\u00fc\u00b7schel", "nechst", ",", "mein", "Tho\u00b7ma\u00b7sen", ",", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "NE", "VVFIN", "$,", "PPOSAT", "NN", "$,", "APPR", "PPER", "$,"], "meter": "+--+-+-+---+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ich fieng dar\u00fcber an, weil ich dabey gese\u00dfen:", "tokens": ["Ich", "fi\u00b7eng", "da\u00b7r\u00fc\u00b7ber", "an", ",", "weil", "ich", "da\u00b7bey", "ge\u00b7se\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PTKVZ", "$,", "KOUS", "PPER", "PAV", "VVPP", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er ist ein Schlesier, Herr Haubtmann, seht euch f\u00fcr.", "tokens": ["Er", "ist", "ein", "Schle\u00b7sier", ",", "Herr", "Haubt\u00b7mann", ",", "seht", "euch", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "NN", "NN", "$,", "VVFIN", "PPER", "APPR", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}