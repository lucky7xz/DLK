{"textgrid.poem.52223": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Ein Patriot", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hasardspiel? \u2013 Pfui \u2013 da\u00df mich der Herr bewahre!", "tokens": ["Ha\u00b7sard\u00b7spiel", "?", "\u2013", "Pfui", "\u2013", "da\u00df", "mich", "der", "Herr", "be\u00b7wah\u00b7re", "!"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "$(", "NE", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hol' es der Teufel \u2013 ja, das sag' ich frisch!", "tokens": ["Hol'", "es", "der", "Teu\u00b7fel", "\u2013", "ja", ",", "das", "sag'", "ich", "frisch", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$(", "PTKANT", "$,", "PDS", "VVFIN", "PPER", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ich werde morgen meine sechzig Jahre,", "tokens": ["Ich", "wer\u00b7de", "mor\u00b7gen", "mei\u00b7ne", "sech\u00b7zig", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und trat noch niemals an den gr\u00fcnen Tisch!", "tokens": ["Und", "trat", "noch", "nie\u00b7mals", "an", "den", "gr\u00fc\u00b7nen", "Tisch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "H\u00e4tt' ich's getan \u2013 bei Gott, ich m\u00fc\u00dft' err\u00f6ten!", "tokens": ["H\u00e4tt'", "ich's", "ge\u00b7tan", "\u2013", "bei", "Gott", ",", "ich", "m\u00fc\u00dft'", "er\u00b7r\u00f6\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVPP", "$(", "APPR", "NN", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "O, dies Roulett, ich hass' es und verfem's!", "tokens": ["O", ",", "dies", "Rou\u00b7lett", ",", "ich", "hass'", "es", "und", "ver\u00b7fem's", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PDS", "NN", "$,", "PPER", "VVFIN", "PPER", "KON", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich bin ein Chirst \u2013 und schlag' ein Kreuz vor K\u00f6then!", "tokens": ["Ich", "bin", "ein", "Chirst", "\u2013", "und", "schlag'", "ein", "Kreuz", "vor", "K\u00f6\u00b7then", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "KON", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich bin ein Mann \u2013 und pfeife was auf Ems!", "tokens": ["Ich", "bin", "ein", "Mann", "\u2013", "und", "pfei\u00b7fe", "was", "auf", "Ems", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "KON", "VVFIN", "PIS", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nein, was ich liebe, ist ein ehrlich Lotto;", "tokens": ["Nein", ",", "was", "ich", "lie\u00b7be", ",", "ist", "ein", "ehr\u00b7lich", "Lot\u00b7to", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "ART", "ADJD", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Mensch mu\u00df spielen \u2013 ja, das r\u00e4um' ich ein!", "tokens": ["Der", "Mensch", "mu\u00df", "spie\u00b7len", "\u2013", "ja", ",", "das", "r\u00e4um'", "ich", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$(", "PTKANT", "$,", "PDS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbwagen gewinnt!\u00ab ist des Jahrhunderts Motto \u2013", "tokens": ["\u00bb", "wa\u00b7gen", "ge\u00b7winnt", "!", "\u00ab", "ist", "des", "Jahr\u00b7hun\u00b7derts", "Mot\u00b7to", "\u2013"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "VVFIN", "$.", "$(", "VAFIN", "ART", "NN", "NE", "$("], "meter": "+--+--++-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Drum m\u00fcssen halt auch Lotterien sein!", "tokens": ["Drum", "m\u00fcs\u00b7sen", "halt", "auch", "Lot\u00b7te\u00b7ri\u00b7en", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "VVFIN", "ADV", "NE", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die sind moralisch! Hoch ein Hasardieren,", "tokens": ["Die", "sind", "mo\u00b7ra\u00b7lisch", "!", "Hoch", "ein", "Ha\u00b7sar\u00b7die\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$.", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem Flor des Volkes gilt als h\u00f6chstes Ziel!", "tokens": ["Dem", "Flor", "des", "Vol\u00b7kes", "gilt", "als", "h\u00f6chs\u00b7tes", "Ziel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wer wird sein Geld an Benazet verlieren,", "tokens": ["Wer", "wird", "sein", "Geld", "an", "Be\u00b7na\u00b7zet", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wenn Staaten rufen: \u00bbMachen Sie Ihr Spiel!\u00ab", "tokens": ["Wenn", "Staa\u00b7ten", "ru\u00b7fen", ":", "\u00bb", "Ma\u00b7chen", "Sie", "Ihr", "Spiel", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NN", "VVINF", "$.", "$(", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein hehrer Ruf! Er ging mir nicht verloren!", "tokens": ["Ein", "heh\u00b7rer", "Ruf", "!", "Er", "ging", "mir", "nicht", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seit drei\u00dfig Jahren setz' ich p\u00fcnktlich ein!", "tokens": ["Seit", "drei\u00b7\u00dfig", "Jah\u00b7ren", "setz'", "ich", "p\u00fcnkt\u00b7lich", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch nur im Lande \u2013 sei es euch geschworen!", "tokens": ["Doch", "nur", "im", "Lan\u00b7de", "\u2013", "sei", "es", "euch", "ge\u00b7schwo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$(", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich schmeichle mir, ein Patriot zu sein!", "tokens": ["Ich", "schmeich\u00b7le", "mir", ",", "ein", "Pat\u00b7ri\u00b7ot", "zu", "sein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nein, ich vertrug der Heimat keinen Heller,", "tokens": ["Nein", ",", "ich", "ver\u00b7trug", "der", "Hei\u00b7mat", "kei\u00b7nen", "Hel\u00b7ler", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nie war ich Hamburg, nie den D\u00e4nen gr\u00fcn!", "tokens": ["Nie", "war", "ich", "Ham\u00b7burg", ",", "nie", "den", "D\u00e4\u00b7nen", "gr\u00fcn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "$,", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nie fing zu Frankfurt mich ein Vogelsteller", "tokens": ["Nie", "fing", "zu", "Frank\u00b7furt", "mich", "ein", "Vo\u00b7gel\u00b7stel\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NE", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit unsoliden G\u00fcterlotterien!", "tokens": ["Mit", "un\u00b7so\u00b7li\u00b7den", "G\u00fc\u00b7ter\u00b7lot\u00b7te\u00b7ri\u00b7en", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich blieb daheim \u2013 drum ward ich auch gesegnet!", "tokens": ["Ich", "blieb", "da\u00b7heim", "\u2013", "drum", "ward", "ich", "auch", "ge\u00b7seg\u00b7net", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "PAV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Versteht mich recht: leer wurde meine Truh'!", "tokens": ["Ver\u00b7steht", "mich", "recht", ":", "leer", "wur\u00b7de", "mei\u00b7ne", "Truh'", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$.", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht hat Fortuna mich mit Gold beregnet \u2013", "tokens": ["Nicht", "hat", "For\u00b7tu\u00b7na", "mich", "mit", "Gold", "be\u00b7reg\u00b7net", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "NN", "PRF", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch warf ihr Rad den Bettelstab mir zu!", "tokens": ["Doch", "warf", "ihr", "Rad", "den", "Bet\u00b7tel\u00b7stab", "mir", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mein siechend Weib und meine Rangen klagen;", "tokens": ["Mein", "sie\u00b7chend", "Weib", "und", "mei\u00b7ne", "Ran\u00b7gen", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "CARD", "NN", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was heulen sie? \u2013 ich glaube gar, um Brot.", "tokens": ["Was", "heu\u00b7len", "sie", "?", "\u2013", "ich", "glau\u00b7be", "gar", ",", "um", "Brot", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "$(", "PPER", "VVFIN", "ADV", "$,", "KOUI", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Beschr\u00e4nktes Volk! was will der Bettel sagen?", "tokens": ["Be\u00b7schr\u00e4nk\u00b7tes", "Volk", "!", "was", "will", "der", "Bet\u00b7tel", "sa\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PWS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich gab's dem Staat \u2013 ich bin ein Patriot!", "tokens": ["Ich", "gab's", "dem", "Staat", "\u2013", "ich", "bin", "ein", "Pat\u00b7ri\u00b7ot", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Was ich verlor, hat manchen armen Teufel", "tokens": ["Was", "ich", "ver\u00b7lor", ",", "hat", "man\u00b7chen", "ar\u00b7men", "Teu\u00b7fel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vielleicht gerettet \u2013 Gott wei\u00df, wo im Land.", "tokens": ["Viel\u00b7leicht", "ge\u00b7ret\u00b7tet", "\u2013", "Gott", "wei\u00df", ",", "wo", "im", "Land", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$(", "NN", "VVFIN", "$,", "PWAV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo nicht \u2013 ei nun, so ward es ohne Zweifel", "tokens": ["Wo", "nicht", "\u2013", "ei", "nun", ",", "so", "ward", "es", "oh\u00b7ne", "Zwei\u00b7fel"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "$(", "ITJ", "ADV", "$,", "ADV", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zur Volksbegl\u00fcckung sonsten angewandt!", "tokens": ["Zur", "Volks\u00b7be\u00b7gl\u00fc\u00b7ckung", "sons\u00b7ten", "an\u00b7ge\u00b7wandt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O, wirkte jeder so mit Ernst wie ich,", "tokens": ["O", ",", "wirk\u00b7te", "je\u00b7der", "so", "mit", "Ernst", "wie", "ich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PIS", "ADV", "APPR", "NE", "PWAV", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie m\u00fc\u00dfte da das Vaterland florieren,", "tokens": ["Wie", "m\u00fc\u00df\u00b7te", "da", "das", "Va\u00b7ter\u00b7land", "flo\u00b7rie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie mehrte da des Volkes Wohlstand sich!", "tokens": ["Wie", "mehr\u00b7te", "da", "des", "Vol\u00b7kes", "Wohl\u00b7stand", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "NN", "PRF", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.6": {"line.1": {"text": "Ich \u2013 nun ich tat nach meinen schwachen Kr\u00e4ften!", "tokens": ["Ich", "\u2013", "nun", "ich", "tat", "nach", "mei\u00b7nen", "schwa\u00b7chen", "Kr\u00e4f\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "ADV", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und \u2013 zum Roulettisch sah mich niemand gehn!", "tokens": ["Und", "\u2013", "zum", "Rou\u00b7let\u00b7tisch", "sah", "mich", "nie\u00b7mand", "gehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "APPRART", "NN", "VVFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wird man kein Kreuz mir auf den Kittel heften?", "tokens": ["Wird", "man", "kein", "Kreuz", "mir", "auf", "den", "Kit\u00b7tel", "hef\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PIAT", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es w\u00fcrde gut zu meinem Hauskreuz stehn!", "tokens": ["Es", "w\u00fcr\u00b7de", "gut", "zu", "mei\u00b7nem", "Haus\u00b7kreuz", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auch zu dem Tannenkreuz auf meinem k\u00fchlen", "tokens": ["Auch", "zu", "dem", "Tan\u00b7nen\u00b7kreuz", "auf", "mei\u00b7nem", "k\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Grabh\u00fcgel bald, hart an des K