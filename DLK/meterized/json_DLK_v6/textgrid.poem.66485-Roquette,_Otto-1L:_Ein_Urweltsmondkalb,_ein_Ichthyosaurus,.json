{"textgrid.poem.66485": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Urweltsmondkalb, ein Ichthyosaurus,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Urweltsmondkalb, ein Ichthyosaurus,", "tokens": ["Ein", "Ur\u00b7welts\u00b7mond\u00b7kalb", ",", "ein", "Ich\u00b7thy\u00b7o\u00b7sau\u00b7rus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Ungeheu'r vom Stamme der Fossilien,", "tokens": ["Ein", "Un\u00b7ge\u00b7heu'r", "vom", "Stam\u00b7me", "der", "Fos\u00b7si\u00b7li\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und w\u00e4re das Skelet des Minotaurus", "tokens": ["Und", "w\u00e4\u00b7re", "das", "Ske\u00b7let", "des", "Mi\u00b7no\u00b7tau\u00b7rus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Zu finden, oder andrer Mischfamilien", "tokens": ["Zu", "fin\u00b7den", ",", "o\u00b7der", "an\u00b7drer", "Mischfa\u00b7mi\u00b7li\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Entschlaf'ne Vettern, etwa ein Centaurus,", "tokens": ["Ent\u00b7schlaf'\u00b7ne", "Vet\u00b7tern", ",", "et\u00b7wa", "ein", "Cen\u00b7tau\u00b7rus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auch wohl die Haut herkulischer Reptilien:", "tokens": ["Auch", "wohl", "die", "Haut", "her\u00b7ku\u00b7li\u00b7scher", "Rep\u00b7ti\u00b7li\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Das Alles wird und w\u00fcrde Modesache", "tokens": ["Das", "Al\u00b7les", "wird", "und", "w\u00fcr\u00b7de", "Mo\u00b7de\u00b7sa\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "VAFIN", "KON", "VAFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "F\u00fcr Freund' und G\u00f6nner vom Gelehrtenfache.", "tokens": ["F\u00fcr", "Freund'", "und", "G\u00f6n\u00b7ner", "vom", "Ge\u00b7lehr\u00b7ten\u00b7fa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Giebt's in der Wissenschaft gelehrte Moden,", "tokens": ["Giebt's", "in", "der", "Wis\u00b7sen\u00b7schaft", "ge\u00b7lehr\u00b7te", "Mo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "So bringt in unerm\u00fcdlichem Erneuen", "tokens": ["So", "bringt", "in", "un\u00b7er\u00b7m\u00fcd\u00b7li\u00b7chem", "Er\u00b7neu\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie Jahr und Tag auf all und jedem Boden.", "tokens": ["Sie", "Jahr", "und", "Tag", "auf", "all", "und", "je\u00b7dem", "Bo\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "APPR", "PIAT", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An stetem Wechsel will die Welt sich freuen,", "tokens": ["An", "ste\u00b7tem", "Wech\u00b7sel", "will", "die", "Welt", "sich", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Und eilt, das Aeltre nur hinwegzuroden,", "tokens": ["Und", "eilt", ",", "das", "A\u00b7elt\u00b7re", "nur", "hin\u00b7weg\u00b7zu\u00b7ro\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Um Aller\u00e4ltestes zu wiederk\u00e4uen.", "tokens": ["Um", "Al\u00b7le\u00b7r\u00e4l\u00b7tes\u00b7tes", "zu", "wie\u00b7der\u00b7k\u00e4u\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Was fremd erscheint in g\u00fcnst'ger Zeitenstunde,", "tokens": ["Was", "fremd", "er\u00b7scheint", "in", "g\u00fcnst'\u00b7ger", "Zei\u00b7ten\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Macht auf der Neugier Stra\u00dfen schnell die Runde.", "tokens": ["Macht", "auf", "der", "Neu\u00b7gier", "Stra\u00b7\u00dfen", "schnell", "die", "Run\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was alles ausersehn zum Modemachen,", "tokens": ["Was", "al\u00b7les", "au\u00b7ser\u00b7sehn", "zum", "Mo\u00b7de\u00b7ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVIZU", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wie das Modemachen gar gemacht wird,", "tokens": ["Und", "wie", "das", "Mo\u00b7de\u00b7ma\u00b7chen", "gar", "ge\u00b7macht", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O lustig Schauspiel! Oft zuerst mit Lachen", "tokens": ["O", "lus\u00b7tig", "Schau\u00b7spiel", "!", "Oft", "zu\u00b7erst", "mit", "La\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "NN", "$.", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Empf\u00e4ngt man, was Gemeingut \u00fcber Nacht wird,", "tokens": ["Emp\u00b7f\u00e4ngt", "man", ",", "was", "Ge\u00b7mein\u00b7gut", "\u00fc\u00b7ber", "Nacht", "wird", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PRELS", "ADJD", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bis pl\u00f6tzlich es, nach kurzem Glanz und Prachen", "tokens": ["Bis", "pl\u00f6tz\u00b7lich", "es", ",", "nach", "kur\u00b7zem", "Glanz", "und", "Pra\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "PPER", "$,", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Geworfen in des Tr\u00f6dels tiefsten Schacht wird.", "tokens": ["Ge\u00b7wor\u00b7fen", "in", "des", "Tr\u00f6\u00b7dels", "tiefs\u00b7ten", "Schacht", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auch auf des Ruhmes Bahn die Tageswunder", "tokens": ["Auch", "auf", "des", "Ruh\u00b7mes", "Bahn", "die", "Ta\u00b7ges\u00b7wun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Man hebt, verw\u00f6hnt sie, wirft sie weg als Plunder.", "tokens": ["Man", "hebt", ",", "ver\u00b7w\u00f6hnt", "sie", ",", "wirft", "sie", "weg", "als", "Plun\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es ist das Loos des K\u00fcnstlers auch und Dichters,", "tokens": ["Es", "ist", "das", "Loos", "des", "K\u00fcnst\u00b7lers", "auch", "und", "Dich\u00b7ters", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des besten oft. Er theilt das Gunstempfangen", "tokens": ["Des", "bes\u00b7ten", "oft", ".", "Er", "theilt", "das", "Guns\u00b7temp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit jedem Tro\u00dfknecht seichtesten Gelichters,", "tokens": ["Mit", "je\u00b7dem", "Tro\u00df\u00b7knecht", "seich\u00b7tes\u00b7ten", "Ge\u00b7lich\u00b7ters", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der schnell zur Hand f\u00fcr jegliches Verlangen,", "tokens": ["Der", "schnell", "zur", "Hand", "f\u00fcr", "jeg\u00b7li\u00b7ches", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und keck verlacht den Spruch des ernsten Richters.", "tokens": ["Und", "keck", "ver\u00b7lacht", "den", "Spruch", "des", "erns\u00b7ten", "Rich\u00b7ters", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Beste, dem sie heut noch Hymnen sangen,", "tokens": ["Der", "Bes\u00b7te", ",", "dem", "sie", "heut", "noch", "Hym\u00b7nen", "san\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "H\u00f6rt morgen jauchzen die entz\u00fcckten Horden,", "tokens": ["H\u00f6rt", "mor\u00b7gen", "jauch\u00b7zen", "die", "ent\u00b7z\u00fcck\u00b7ten", "Hor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein Popanz ist ihr Liebling schnell geworden.", "tokens": ["Ein", "Po\u00b7panz", "ist", "ihr", "Lieb\u00b7ling", "schnell", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das nie Veraltende war selten modisch.", "tokens": ["Das", "nie", "Ver\u00b7al\u00b7ten\u00b7de", "war", "sel\u00b7ten", "mo\u00b7disch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "NN", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Wenn auch begr\u00fc\u00dft von Vielen, und verstanden,", "tokens": ["Wenn", "auch", "be\u00b7gr\u00fc\u00dft", "von", "Vie\u00b7len", ",", "und", "ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVPP", "APPR", "NN", "$,", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geht's durch die Welt doch einsam, episodisch,", "tokens": ["Geht's", "durch", "die", "Welt", "doch", "ein\u00b7sam", ",", "e\u00b7pi\u00b7so\u00b7disch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADV", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo Wirrwarr sich und Ungeschmack verbanden.", "tokens": ["Wo", "Wirr\u00b7warr", "sich", "und", "Un\u00b7ge\u00b7schmack", "ver\u00b7ban\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es lautet nur dem engsten Kreis melodisch,", "tokens": ["Es", "lau\u00b7tet", "nur", "dem", "engs\u00b7ten", "Kreis", "me\u00b7lo\u00b7disch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Klang, in dem die Besten nur sich fanden.", "tokens": ["Ein", "Klang", ",", "in", "dem", "die", "Bes\u00b7ten", "nur", "sich", "fan\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Welt verlangt Trompeten, Paukendr\u00f6hnen,", "tokens": ["Die", "Welt", "ver\u00b7langt", "Trom\u00b7pe\u00b7ten", ",", "Pau\u00b7ken\u00b7dr\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Um gl\u00e4ubig zu genie\u00dfen und zu kr\u00f6nen.", "tokens": ["Um", "gl\u00e4u\u00b7big", "zu", "ge\u00b7nie\u00b7\u00dfen", "und", "zu", "kr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Was niemals Mode war, das Sch\u00f6ne, Gute,", "tokens": ["Was", "nie\u00b7mals", "Mo\u00b7de", "war", ",", "das", "Sch\u00f6\u00b7ne", ",", "Gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADV", "NN", "VAFIN", "$,", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ging ruhig wachsend doch durch alle Zeiten,", "tokens": ["Ging", "ru\u00b7hig", "wach\u00b7send", "doch", "durch", "al\u00b7le", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Inde\u00df im Mummenschanz der Weltminute,", "tokens": ["In\u00b7de\u00df", "im", "Mum\u00b7men\u00b7schanz", "der", "Welt\u00b7mi\u00b7nu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die fl\u00fccht'gen Bildungen vor\u00fcber gleiten,", "tokens": ["Die", "fl\u00fccht'\u00b7gen", "Bil\u00b7dun\u00b7gen", "vor\u00b7\u00fc\u00b7ber", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und schwinden nach geb\u00fc\u00dftem Uebermuthe.", "tokens": ["Und", "schwin\u00b7den", "nach", "ge\u00b7b\u00fc\u00df\u00b7tem", "Ue\u00b7ber\u00b7mu\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was unzerst\u00f6rbar lebt f\u00fcr Ewigkeiten,", "tokens": ["Was", "un\u00b7zer\u00b7st\u00f6r\u00b7bar", "lebt", "f\u00fcr", "E\u00b7wig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Erscheint, wie wenn nach bunten Flackerfeuern", "tokens": ["Er\u00b7scheint", ",", "wie", "wenn", "nach", "bun\u00b7ten", "Fla\u00b7cker\u00b7feu\u00b7ern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOKOM", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sich S