{"dta.poem.179": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er vergleicht die  Lesbia  mit einem Garten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Entweiche Flora nur mit deiner Blumen-Pracht/", "tokens": ["Ent\u00b7wei\u00b7che", "Flo\u00b7ra", "nur", "mit", "dei\u00b7ner", "Blu\u00b7men\u00b7Pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verh\u00fcll das Lust-Gefild der sch\u00f6nen ", "tokens": ["Ver\u00b7h\u00fcll", "das", "Lust\u00b7Ge\u00b7fild", "der", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gib dem beliebten Schmuck der Nelcken gute Nacht/", "tokens": ["Gib", "dem", "be\u00b7lieb\u00b7ten", "Schmuck", "der", "Nel\u00b7cken", "gu\u00b7te", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verla\u00df die Prahlerey der stoltzen K\u00e4yser-Cronen.", "tokens": ["Ver\u00b7la\u00df", "die", "Prah\u00b7le\u00b7rey", "der", "stolt\u00b7zen", "K\u00e4y\u00b7ser\u00b7Cro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hilff der verla\u00dfnen Lilg beperlte Thr\u00e4nen streun/", "tokens": ["Hilff", "der", "ver\u00b7la\u00df\u00b7nen", "Lilg", "be\u00b7perl\u00b7te", "Thr\u00e4\u00b7nen", "streun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auf den geb\u00fcckten Klee/ entf\u00e4rbt euch ihr ", "tokens": ["Auf", "den", "ge\u00b7b\u00fcck\u00b7ten", "Klee", "/", "ent\u00b7f\u00e4rbt", "euch", "ihr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "VVFIN", "PPER", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Jhr ", "tokens": ["Ihr"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Jhr gr\u00fcnen Matten sterbt in dunckeln Finsternissen.", "tokens": ["Ihr", "gr\u00fc\u00b7nen", "Mat\u00b7ten", "sterbt", "in", "dun\u00b7ckeln", "Fins\u00b7ter\u00b7nis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du edles Blat ", "tokens": ["Du", "ed\u00b7les", "Blat"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Jhr gr\u00fcnen Lauben welckt/ ihr blauen Regen-Bogen", "tokens": ["Ihr", "gr\u00fc\u00b7nen", "Lau\u00b7ben", "welckt", "/", "ihr", "blau\u00b7en", "Re\u00b7gen\u00b7Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$(", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "H\u00fclt euren Indich bey/ la\u00df nur dein Bl\u00fchen seyn", "tokens": ["H\u00fclt", "eu\u00b7ren", "In\u00b7dich", "bey", "/", "la\u00df", "nur", "dein", "Bl\u00fc\u00b7hen", "seyn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "$(", "VVIMP", "ADV", "PPOSAT", "NN", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Du safftger ", "tokens": ["Du", "safft\u00b7ger"], "token_info": ["word", "word"], "pos": ["PPER", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Seht meine ", "tokens": ["Seht", "mei\u00b7ne"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Es mu\u00df vor ihrer Pracht eur schlechtes Ansehn weichen/", "tokens": ["Es", "mu\u00df", "vor", "ih\u00b7rer", "Pracht", "eur", "schlech\u00b7tes", "An\u00b7sehn", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sie hegt mehr Lustbarkeit als ", "tokens": ["Sie", "hegt", "mehr", "Lust\u00b7bar\u00b7keit", "als"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "KOKOM"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.17": {"text": "Weil ihr Rubinen Mund die Rosen \u00fcbersteigt", "tokens": ["Weil", "ihr", "Ru\u00b7bi\u00b7nen", "Mund", "die", "Ro\u00b7sen", "\u00fc\u00b7bers\u00b7teigt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Vor ihren Wangen mu\u00df die Nelcke sich entf\u00e4rben/", "tokens": ["Vor", "ih\u00b7ren", "Wan\u00b7gen", "mu\u00df", "die", "Nel\u00b7cke", "sich", "ent\u00b7f\u00e4r\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "ART", "NN", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die Kayser-Crone sich vor ihren Haupte neigt/", "tokens": ["Die", "Kay\u00b7ser\u00b7Cro\u00b7ne", "sich", "vor", "ih\u00b7ren", "Haup\u00b7te", "neigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und vor den Busen mu\u00df die stoltze Lilge sterben.", "tokens": ["Und", "vor", "den", "Bu\u00b7sen", "mu\u00df", "die", "stolt\u00b7ze", "Lil\u00b7ge", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Stirn-", "tokens": ["Die", "Stirn"], "token_info": ["word", "word"], "pos": ["ART", "TRUNC"], "meter": "-+", "measure": "iambic.single"}, "line.22": {"text": "Jhr Athen thut weit mehr als ", "tokens": ["Ihr", "A\u00b7then", "thut", "weit", "mehr", "als"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "PIAT", "KOKOM"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Die blauen Adern sind mit Turckis ausgeputzt/", "tokens": ["Die", "blau\u00b7en", "A\u00b7dern", "sind", "mit", "Tur\u00b7ckis", "aus\u00b7ge\u00b7putzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die Tulpe mu\u00df entschw\u00e4rtzt vor ihren Wartzen weichen.", "tokens": ["Die", "Tul\u00b7pe", "mu\u00df", "ent\u00b7schw\u00e4rtzt", "vor", "ih\u00b7ren", "Wart\u00b7zen", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVPP", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die blaue ", "tokens": ["Die", "blau\u00b7e"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.26": {"text": "Denn/ der beliebte Glantz den Indich \u00fcbersteiget;", "tokens": ["Denn", "/", "der", "be\u00b7lieb\u00b7te", "Glantz", "den", "In\u00b7dich", "\u00fc\u00b7bers\u00b7tei\u00b7get", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ein blauer Anmuths-Strahl spielt aus dem Angesicht", "tokens": ["Ein", "blau\u00b7er", "An\u00b7muths\u00b7Strahl", "spielt", "aus", "dem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der dem bemannten Geist die Liebes-Fackeln zeiget.", "tokens": ["Der", "dem", "be\u00b7mann\u00b7ten", "Geist", "die", "Lie\u00b7bes\u00b7Fa\u00b7ckeln", "zei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die rund-gew\u00f6lbte Schoo\u00df die Lauben \u00fcbertrifft/", "tokens": ["Die", "run\u00b7dge\u00b7w\u00f6lb\u00b7te", "Schoo\u00df", "die", "Lau\u00b7ben", "\u00fc\u00b7bert\u00b7rifft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der Eingang sticht hinweg das bl\u00fchn der ", "tokens": ["Der", "Ein\u00b7gang", "sticht", "hin\u00b7weg", "das", "bl\u00fchn", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PDS", "VVFIN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Die Gegend des Geb\u00fcschs ein feurig Kitzeln stifft/", "tokens": ["Die", "Ge\u00b7gend", "des", "Ge\u00b7b\u00fcschs", "ein", "feu\u00b7rig", "Kit\u00b7zeln", "stifft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "ADJD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Demselben/ der da bricht der L\u00fcste-Zucker-Rosen", "tokens": ["Dem\u00b7sel\u00b7ben", "/", "der", "da", "bricht", "der", "L\u00fcs\u00b7te\u00b7Zu\u00b7cke\u00b7rRo\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDAT", "$(", "ART", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Dem Garten fehlet nichts/ die Anmuth f\u00e4llt ihm bey/", "tokens": ["Dem", "Gar\u00b7ten", "feh\u00b7let", "nichts", "/", "die", "An\u00b7muth", "f\u00e4llt", "ihm", "bey", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "$(", "ART", "NN", "VVFIN", "PPER", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.35": {"text": "Die Huldinnen sind stets in dieser W\u00fcsteney/", "tokens": ["Die", "Hul\u00b7din\u00b7nen", "sind", "stets", "in", "die\u00b7ser", "W\u00fcs\u00b7te\u00b7ney", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PDAT", "NN", "$("], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.36": {"text": "Auch ", "tokens": ["Auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.37": {"text": "Nur eines fehlt ihm noch/ ein G\u00e4rtner/ der sich kan", "tokens": ["Nur", "ei\u00b7nes", "fehlt", "ihm", "noch", "/", "ein", "G\u00e4rt\u00b7ner", "/", "der", "sich", "kan"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "ADV", "$(", "ART", "NN", "$(", "PRELS", "PRF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Mit fetter Perlen-Milch zu s\u00e4emen ihn befleissen/", "tokens": ["Mit", "fet\u00b7ter", "Per\u00b7len\u00b7Milch", "zu", "s\u00e4e\u00b7men", "ihn", "be\u00b7fleis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Nimmt mich nun ", "tokens": ["Nimmt", "mich", "nun"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.40": {"text": "So kan ich ", "tokens": ["So", "kan", "ich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}}}}}