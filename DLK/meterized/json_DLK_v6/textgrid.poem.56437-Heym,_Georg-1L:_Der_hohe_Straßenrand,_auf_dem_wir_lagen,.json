{"textgrid.poem.56437": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der hohe Stra\u00dfenrand, auf dem wir lagen,", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der hohe Stra\u00dfenrand, auf dem wir lagen,", "tokens": ["Der", "ho\u00b7he", "Stra\u00b7\u00dfen\u00b7rand", ",", "auf", "dem", "wir", "la\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "War wei\u00df von Staub. Wir sahen in der Enge", "tokens": ["War", "wei\u00df", "von", "Staub", ".", "Wir", "sa\u00b7hen", "in", "der", "En\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVFIN", "APPR", "NN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Unz\u00e4hlig: Menschenstr\u00f6me und Gedr\u00e4nge,", "tokens": ["Un\u00b7z\u00e4h\u00b7lig", ":", "Men\u00b7schen\u00b7str\u00f6\u00b7me", "und", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und sahn die Weltstadt fern im Abend ragen.", "tokens": ["Und", "sahn", "die", "Welt\u00b7stadt", "fern", "im", "A\u00b7bend", "ra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die vollen Kremser fuhren durch die Menge,", "tokens": ["Die", "vol\u00b7len", "Krem\u00b7ser", "fuh\u00b7ren", "durch", "die", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Papierne F\u00e4hnchen waren drangeschlagen.", "tokens": ["Pa\u00b7pier\u00b7ne", "F\u00e4hn\u00b7chen", "wa\u00b7ren", "dran\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Omnibusse, voll Verdeck und Wagen.", "tokens": ["Die", "Om\u00b7ni\u00b7bus\u00b7se", ",", "voll", "Ver\u00b7deck", "und", "Wa\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Automobile, Rauch und Huppenkl\u00e4nge.", "tokens": ["Au\u00b7to\u00b7mo\u00b7bi\u00b7le", ",", "Rauch", "und", "Hup\u00b7pen\u00b7kl\u00e4n\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Dem Riesensteinmeer zu. Doch westlich sahn", "tokens": ["Dem", "Rie\u00b7sen\u00b7stein\u00b7meer", "zu", ".", "Doch", "west\u00b7lich", "sahn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wir an der langen Stra\u00dfe Baum an Baum,", "tokens": ["Wir", "an", "der", "lan\u00b7gen", "Stra\u00b7\u00dfe", "Baum", "an", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der bl\u00e4tterlosen Kronen Filigran.", "tokens": ["Der", "bl\u00e4t\u00b7ter\u00b7lo\u00b7sen", "Kro\u00b7nen", "Fi\u00b7li\u00b7gran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Sonnenball hing gro\u00df am Himmelssaum", "tokens": ["Der", "Son\u00b7nen\u00b7ball", "hing", "gro\u00df", "am", "Him\u00b7mels\u00b7saum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und rote Strahlen scho\u00df des Abends Bahn.", "tokens": ["Und", "ro\u00b7te", "Strah\u00b7len", "scho\u00df", "des", "A\u00b7bends", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf allen K\u00f6pfen lag des Lichtes Traum.", "tokens": ["Auf", "al\u00b7len", "K\u00f6p\u00b7fen", "lag", "des", "Lich\u00b7tes", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}