{"textgrid.poem.37549": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Anfang", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pegasus, du alter Renner,", "tokens": ["Pe\u00b7ga\u00b7sus", ",", "du", "al\u00b7ter", "Ren\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trag mich mal nach Afrika,", "tokens": ["Trag", "mich", "mal", "nach", "Af\u00b7ri\u00b7ka", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alldieweil so schwarze M\u00e4nner", "tokens": ["All\u00b7die\u00b7weil", "so", "schwar\u00b7ze", "M\u00e4n\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und so bunte V\u00f6gel da.", "tokens": ["Und", "so", "bun\u00b7te", "V\u00f6\u00b7gel", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kleider sind da wenig Sitte;", "tokens": ["Klei\u00b7der", "sind", "da", "we\u00b7nig", "Sit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6chstens tr\u00e4gt man einen Hut,", "tokens": ["H\u00f6chs\u00b7tens", "tr\u00e4gt", "man", "ei\u00b7nen", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch wohl einen Schurz der Mitte;", "tokens": ["Auch", "wohl", "ei\u00b7nen", "Schurz", "der", "Mit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Man ist schwarz und damit gut. \u2013", "tokens": ["Man", "ist", "schwarz", "und", "da\u00b7mit", "gut", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "KON", "PAV", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann ist freilich jeder bange,", "tokens": ["Dann", "ist", "frei\u00b7lich", "je\u00b7der", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Selbst der Affengreis entfleucht,", "tokens": ["Selbst", "der", "Af\u00b7fen\u00b7greis", "ent\u00b7fleucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die lange Brillenschlange", "tokens": ["Wenn", "die", "lan\u00b7ge", "Bril\u00b7len\u00b7schlan\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zischend von der Palme kreucht.", "tokens": ["Zi\u00b7schend", "von", "der", "Pal\u00b7me", "kreucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kr\u00f6ten fallen auf den R\u00fccken,", "tokens": ["Kr\u00f6\u00b7ten", "fal\u00b7len", "auf", "den", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00c4ngstlich wird das Bein bewegt;", "tokens": ["\u00c4ngst\u00b7lich", "wird", "das", "Bein", "be\u00b7wegt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Strau\u00df mu\u00df heftig dr\u00fccken,", "tokens": ["Und", "der", "Strau\u00df", "mu\u00df", "hef\u00b7tig", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis das gro\u00dfe Ei gelegt.", "tokens": ["Bis", "das", "gro\u00b7\u00dfe", "Ei", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Krokodile weinen Tr\u00e4nen,", "tokens": ["Kro\u00b7ko\u00b7di\u00b7le", "wei\u00b7nen", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Geier sehen kreischend zu;", "tokens": ["Gei\u00b7er", "se\u00b7hen", "krei\u00b7schend", "zu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sehr gemein sind die Hy\u00e4nen;", "tokens": ["Sehr", "ge\u00b7mein", "sind", "die", "Hy\u00b7\u00e4\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sch\u00e4big ist der Marabu.", "tokens": ["Sch\u00e4\u00b7big", "ist", "der", "Ma\u00b7ra\u00b7bu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nur die Affen, voller Schnacken,", "tokens": ["Nur", "die", "Af\u00b7fen", ",", "vol\u00b7ler", "Schna\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haben Vor- und Hinterhand;", "tokens": ["Ha\u00b7ben", "Vor", "und", "Hin\u00b7ter\u00b7hand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "TRUNC", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Emsig mummeln ihre Backen;", "tokens": ["Em\u00b7sig", "mum\u00b7meln", "ih\u00b7re", "Ba\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gerne hockt man beieinand.", "tokens": ["Ger\u00b7ne", "hockt", "man", "bei\u00b7ei\u00b7nand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Papa schaut in eine Stelle,", "tokens": ["Pa\u00b7pa", "schaut", "in", "ei\u00b7ne", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Onkel kratzt sich sehr geschwind,", "tokens": ["On\u00b7kel", "kratzt", "sich", "sehr", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tante kann es grad so schnelle,", "tokens": ["Tan\u00b7te", "kann", "es", "grad", "so", "schnel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mama untersucht das Kind.", "tokens": ["Ma\u00b7ma", "un\u00b7ter\u00b7sucht", "das", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Fipps \u2013 so wollen wir es nennen. \u2013", "tokens": ["Fipps", "\u2013", "so", "wol\u00b7len", "wir", "es", "nen\u00b7nen", ".", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$(", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber wie er sich betrug,", "tokens": ["A\u00b7ber", "wie", "er", "sich", "be\u00b7trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn wir ihn genauer kennen,", "tokens": ["Wenn", "wir", "ihn", "ge\u00b7nau\u00b7er", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, das ist betr\u00fcbt genug.", "tokens": ["Ach", ",", "das", "ist", "be\u00b7tr\u00fcbt", "ge\u00b7nug", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDS", "VAFIN", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Selten zeigt er sich best\u00e4ndig,", "tokens": ["Sel\u00b7ten", "zeigt", "er", "sich", "be\u00b7st\u00e4n\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einmal hilft er aus der Not;", "tokens": ["Ein\u00b7mal", "hilft", "er", "aus", "der", "Not", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Anfangs ist er recht lebendig,", "tokens": ["An\u00b7fangs", "ist", "er", "recht", "le\u00b7ben\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und am Schlusse ist er tot.", "tokens": ["Und", "am", "Schlus\u00b7se", "ist", "er", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}