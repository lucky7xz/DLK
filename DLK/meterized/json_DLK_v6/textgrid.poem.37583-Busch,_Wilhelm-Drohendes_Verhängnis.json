{"textgrid.poem.37583": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Drohendes Verh\u00e4ngnis", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es machen sich die Fliegen", "tokens": ["Es", "ma\u00b7chen", "sich", "die", "Flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein luftig Tanzvergn\u00fcgen.", "tokens": ["Ein", "luf\u00b7tig", "Tanz\u00b7ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Frosch der denkt: Nur munter!", "tokens": ["Der", "Frosch", "der", "denkt", ":", "Nur", "mun\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "$.", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr kommt schon noch herunter!", "tokens": ["Ihr", "kommt", "schon", "noch", "her\u00b7un\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}