{"textgrid.poem.64178": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Sir Roger de Montremy", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war Sir Roger de Montremy, zog singend durch die Gauen", "tokens": ["Das", "war", "Sir", "Ro\u00b7ger", "de", "Mon\u00b7tre\u00b7my", ",", "zog", "sin\u00b7gend", "durch", "die", "Gau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "NN", "NE", "NE", "NE", "$,", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Und wo er kam, da l\u00e4chelten sie, wo er schied, da fluchten die Frauen.", "tokens": ["Und", "wo", "er", "kam", ",", "da", "l\u00e4\u00b7chel\u00b7ten", "sie", ",", "wo", "er", "schied", ",", "da", "fluch\u00b7ten", "die", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+---+-+-+--+-", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Denn er trug an seiner linken Hand einen Ring von rotem Achate,", "tokens": ["Denn", "er", "trug", "an", "sei\u00b7ner", "lin\u00b7ken", "Hand", "ei\u00b7nen", "Ring", "von", "ro\u00b7tem", "A\u00b7cha\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+-+--+-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Den gab ihm einst aus Feenland Claribelle, seine Pate.", "tokens": ["Den", "gab", "ihm", "einst", "aus", "Fe\u00b7en\u00b7land", "Cla\u00b7ri\u00b7bel\u00b7le", ",", "sei\u00b7ne", "Pa\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "NN", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Und drehte das Gold er am Finger sacht, so zuckte sie, die er erkoren,", "tokens": ["Und", "dreh\u00b7te", "das", "Gold", "er", "am", "Fin\u00b7ger", "sacht", ",", "so", "zuck\u00b7te", "sie", ",", "die", "er", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+--+--+-+-+--+--+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und dr\u00fcckte er an den Stein mit Macht, \u2013 war mit Seel' und Leib sie verloren.", "tokens": ["Und", "dr\u00fcck\u00b7te", "er", "an", "den", "Stein", "mit", "Macht", ",", "\u2013", "war", "mit", "Seel'", "und", "Leib", "sie", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "NN", "$,", "$(", "VAFIN", "APPR", "NN", "KON", "NN", "PPER", "VVPP", "$."], "meter": "-+--+-+-+--+-+--+-", "measure": "iambic.septa.relaxed"}}, "stanza.4": {"line.1": {"text": "Und es konnte zur Rache kein Eh'gemahl, kein tapferer Bruder taugen,", "tokens": ["Und", "es", "konn\u00b7te", "zur", "Ra\u00b7che", "kein", "Eh'\u00b7ge\u00b7mahl", ",", "kein", "tap\u00b7fe\u00b7rer", "Bru\u00b7der", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPRART", "NN", "PIAT", "NN", "$,", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+-+--+-+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Denn die M\u00e4nner sanken vor seinem Stahl wie die Frauen vor seinen Augen.", "tokens": ["Denn", "die", "M\u00e4n\u00b7ner", "san\u00b7ken", "vor", "sei\u00b7nem", "Stahl", "wie", "die", "Frau\u00b7en", "vor", "sei\u00b7nen", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "KOKOM", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "--+-+--+-+--+--+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.5": {"line.1": {"text": "So ging er durch Frankreich und Burgund nach England \u00fcber die Wogen.", "tokens": ["So", "ging", "er", "durch", "Fran\u00b7kreich", "und", "Bur\u00b7gund", "nach", "En\u00b7gland", "\u00fc\u00b7ber", "die", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "KON", "NN", "APPR", "NE", "APPR", "ART", "NN", "$."], "meter": "-+---+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Heut war sein \u00fcberm\u00fctiger Mund von unb\u00e4ndigem Stolz umzogen.", "tokens": ["Heut", "war", "sein", "\u00fc\u00b7berm\u00b7\u00fc\u00b7ti\u00b7ger", "Mund", "von", "un\u00b7b\u00e4n\u00b7di\u00b7gem", "Stolz", "um\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+--+-++--+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.6": {"line.1": {"text": "Denn die sch\u00f6ne K\u00f6nigin Eleanor, das begehrteste Weib auf Erden,", "tokens": ["Denn", "die", "sch\u00f6\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", "E\u00b7lea\u00b7nor", ",", "das", "be\u00b7gehr\u00b7tes\u00b7te", "Weib", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NE", "$,", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Nach Teviot-Hall ihn heut nacht beschwor, da sollte viel Gl\u00fcck ihm werden.", "tokens": ["Nach", "Te\u00b7viot\u00b7Hall", "ihn", "heut", "nacht", "be\u00b7schwor", ",", "da", "soll\u00b7te", "viel", "Gl\u00fcck", "ihm", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADV", "NN", "VVFIN", "$,", "KOUS", "VMFIN", "PIAT", "NN", "PPER", "VAINF", "$."], "meter": "+--+--+-+-+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Sie hatte geschrieben: \u00bbSir Montremy, o komm, es gilt mein Leben,", "tokens": ["Sie", "hat\u00b7te", "ge\u00b7schrie\u00b7ben", ":", "\u00bb", "Sir", "Mon\u00b7tre\u00b7my", ",", "o", "komm", ",", "es", "gilt", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "$(", "NN", "NE", "$,", "FM", "VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ich will die Bretagne, die Normandie und mich selber will ich dir geben.\u00ab", "tokens": ["Ich", "will", "die", "Bre\u00b7tag\u00b7ne", ",", "die", "Nor\u00b7man\u00b7die", "und", "mich", "sel\u00b7ber", "will", "ich", "dir", "ge\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "ART", "NN", "KON", "PPER", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$.", "$("], "meter": "-+-+---+-+--+-+--+-", "measure": "iambic.septa.relaxed"}}, "stanza.8": {"line.1": {"text": "Und Sir Roger ritt im Abendlicht, wo des Teviot Fluten rauschen:", "tokens": ["Und", "Sir", "Ro\u00b7ger", "ritt", "im", "A\u00b7bend\u00b7licht", ",", "wo", "des", "Te\u00b7vi\u00b7ot", "Flu\u00b7ten", "rau\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NE", "VVFIN", "APPRART", "NN", "$,", "PWAV", "ART", "NN", "NN", "VVINF", "$."], "meter": "--+-+-+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Sein Stolz war gro\u00df: \u2013 er wollt' jetzt nicht mit Gott im Himmel tauschen.", "tokens": ["Sein", "Stolz", "war", "gro\u00df", ":", "\u2013", "er", "wollt'", "jetzt", "nicht", "mit", "Gott", "im", "Him\u00b7mel", "tau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$.", "$(", "PPER", "VMFIN", "ADV", "PTKNEG", "APPR", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.9": {"line.1": {"text": "Und als er kam, wo die F\u00e4hren sind, die Wandrer \u00fcberzufahren,", "tokens": ["Und", "als", "er", "kam", ",", "wo", "die", "F\u00e4h\u00b7ren", "sind", ",", "die", "Wand\u00b7rer", "\u00fc\u00b7ber\u00b7zu\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "VAFIN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Da sa\u00df am Steg das Schifferkind von noch nicht siebzehn Jahren \u2013", "tokens": ["Da", "sa\u00df", "am", "Steg", "das", "Schif\u00b7fer\u00b7kind", "von", "noch", "nicht", "sieb\u00b7zehn", "Jah\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "APPR", "ADV", "PTKNEG", "CARD", "NN", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.10": {"line.1": {"text": "Ein blaues R\u00f6cklein \u2013 ein Hemdchen wei\u00df, drauf zwei gelbe Z\u00f6pfe fielen,", "tokens": ["Ein", "blau\u00b7es", "R\u00f6ck\u00b7lein", "\u2013", "ein", "Hemd\u00b7chen", "wei\u00df", ",", "drauf", "zwei", "gel\u00b7be", "Z\u00f6p\u00b7fe", "fie\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "VVFIN", "$,", "PAV", "CARD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "\u00dcber die nackten Zehen leis lie\u00df sie rinnend die Wellen spielen. \u2013", "tokens": ["\u00dc\u00b7ber", "die", "nack\u00b7ten", "Ze\u00b7hen", "leis", "lie\u00df", "sie", "rin\u00b7nend", "die", "Wel\u00b7len", "spie\u00b7len", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "VVFIN", "PPER", "ADJD", "ART", "NN", "VVINF", "$.", "$("], "meter": "+--+-+-+--+--+-+-", "measure": "iambic.septa.invert"}}, "stanza.11": {"line.1": {"text": "Er stieg vom Ro\u00df, er rief sie an \u2013 ihr Blick hat ihn getroffen,", "tokens": ["Er", "stieg", "vom", "Ro\u00df", ",", "er", "rief", "sie", "an", "\u2013", "ihr", "Blick", "hat", "ihn", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$(", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Ein einziger Blick: \u2013 da fa\u00dft' es ihn an, als s\u00e4h' er den Himmel offen.", "tokens": ["Ein", "ein\u00b7zi\u00b7ger", "Blick", ":", "\u2013", "da", "fa\u00dft'", "es", "ihn", "an", ",", "als", "s\u00e4h'", "er", "den", "Him\u00b7mel", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "KOUS", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+-+--+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.12": {"line.1": {"text": "Und es kam wie Tau nach Sonnenbrand ihm \u00fcber die Seele gezogen", "tokens": ["Und", "es", "kam", "wie", "Tau", "nach", "Son\u00b7nen\u00b7brand", "ihm", "\u00fc\u00b7ber", "die", "See\u00b7le", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "NN", "APPR", "NN", "PPER", "APPR", "ART", "NN", "VVPP"], "meter": "--+-+-+-+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Und er streifte den Ring von der linken Hand, warf weit ihn weg in die Wogen.", "tokens": ["Und", "er", "streif\u00b7te", "den", "Ring", "von", "der", "lin\u00b7ken", "Hand", ",", "warf", "weit", "ihn", "weg", "in", "die", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "ADJD", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "--+--+--+-+-+-+--+-", "measure": "anapaest.tri.plus"}}, "stanza.13": {"line.1": {"text": "Und er sank vor dem Kind verstummt aufs Knie, in den Scho\u00df hat sein Haupt sie genommen: \u2013", "tokens": ["Und", "er", "sank", "vor", "dem", "Kind", "ver\u00b7stummt", "aufs", "Knie", ",", "in", "den", "Scho\u00df", "hat", "sein", "Haupt", "sie", "ge\u00b7nom\u00b7men", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "VVFIN", "APPRART", "NN", "$,", "APPR", "ART", "NN", "VAFIN", "PPOSAT", "NN", "PPER", "VVPP", "$.", "$("], "meter": "--+--+-+-+--+--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Seither hat von Roger de Montremy kein Mensch mehr Kunde bekommen.", "tokens": ["Sei\u00b7ther", "hat", "von", "Ro\u00b7ger", "de", "Mon\u00b7tre\u00b7my", "kein", "Mensch", "mehr", "Kun\u00b7de", "be\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "APPR", "NE", "NE", "NE", "PIAT", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+--+-", "measure": "trochaic.octa.plus.relaxed"}}}}}