{"dta.poem.207": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der  Ahalibama  Klage \u00fcber den Tod ihres  \n geliebten  Eliesers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Elieser stirbt! mein bester Schatz ist todt!", "tokens": ["Mein", "E\u00b7lie\u00b7ser", "stirbt", "!", "mein", "bes\u00b7ter", "Schatz", "ist", "todt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Mein ander Hertz ist hin/ ja selbst mein eigen Leben!", "tokens": ["Mein", "an\u00b7der", "Hertz", "ist", "hin", "/", "ja", "selbst", "mein", "ei\u00b7gen", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VAFIN", "ADV", "$(", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ahalibama auf! ermann dich in der Noht/", "tokens": ["A\u00b7ha\u00b7li\u00b7ba\u00b7ma", "auf", "!", "er\u00b7mann", "dich", "in", "der", "Noht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "VVFIN", "PRF", "APPR", "ART", "NN", "$("], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und la\u00df den krancken Geist in freyen L\u00fcfften schweben:", "tokens": ["Und", "la\u00df", "den", "kran\u00b7cken", "Geist", "in", "frey\u00b7en", "L\u00fcff\u00b7ten", "schwe\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Folg\u2019 ", "tokens": ["Fol\u00b7g'"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Schlie\u00dft Augen/ schlie\u00dft euch zu! erstarrt ihr matten Glieder!", "tokens": ["Schlie\u00dft", "Au\u00b7gen", "/", "schlie\u00dft", "euch", "zu", "!", "er\u00b7starrt", "ihr", "mat\u00b7ten", "Glie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$(", "VVFIN", "PPER", "PTKVZ", "$.", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Brich Hertze/ brich entzwey! bring aus dem Ungest\u00fchm", "tokens": ["Brich", "Hert\u00b7ze", "/", "brich", "ent\u00b7zwey", "!", "bring", "aus", "dem", "Un\u00b7ge\u00b7st\u00fchm"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$(", "VVIMP", "PTKVZ", "$.", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mich Tod! ach s\u00fcsser Tod! zu meinem Schatze wieder.", "tokens": ["Mich", "Tod", "!", "ach", "s\u00fcs\u00b7ser", "Tod", "!", "zu", "mei\u00b7nem", "Schat\u00b7ze", "wie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "XY", "ADJA", "NN", "$.", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Schlag Schmertze meinen Geist wie Tulp\u2019 und Nelck\u2019 entzwey!", "tokens": ["Schlag", "Schmert\u00b7ze", "mei\u00b7nen", "Geist", "wie", "Tul\u00b7p'", "und", "Nelck'", "ent\u00b7zwey", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPOSAT", "NN", "KOKOM", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Bringt mir den Becher her/ der meinen Schatz vergeben/", "tokens": ["Bringt", "mir", "den", "Be\u00b7cher", "her", "/", "der", "mei\u00b7nen", "Schatz", "ver\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$(", "ART", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mach ", "tokens": ["Mach"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Und g\u00f6nne uns die Ruh im ", "tokens": ["Und", "g\u00f6n\u00b7ne", "uns", "die", "Ruh", "im"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPRART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Ach! aber ach umsonst! mein ", "tokens": ["Ach", "!", "a\u00b7ber", "ach", "um\u00b7sonst", "!", "mein"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "$.", "ADV", "ADV", "ADV", "$.", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Ich soll um seinen Tod mich nicht zu tode kr\u00e4ncken/", "tokens": ["Ich", "soll", "um", "sei\u00b7nen", "Tod", "mich", "nicht", "zu", "to\u00b7de", "kr\u00e4n\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "PPER", "PTKNEG", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Verh\u00e4ngni\u00df qu\u00e4hlst du mich auf einmahl nicht zu viel?", "tokens": ["Ver\u00b7h\u00e4ng\u00b7ni\u00df", "qu\u00e4hlst", "du", "mich", "auf", "ein\u00b7mahl", "nicht", "zu", "viel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "APPR", "ADV", "PTKNEG", "PTKA", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Gefangen mich zu sehn/ den Schatz ins Grab zu sencken/", "tokens": ["Ge\u00b7fan\u00b7gen", "mich", "zu", "sehn", "/", "den", "Schatz", "ins", "Grab", "zu", "sen\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKZU", "VVINF", "$(", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Den Schatz/ der mir sehr offt die Lebens-Zeit vers\u00fc\u00dft/", "tokens": ["Den", "Schatz", "/", "der", "mir", "sehr", "offt", "die", "Le\u00b7bens\u00b7Zeit", "ver\u00b7s\u00fc\u00dft", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "ADV", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ach ", "tokens": ["Ach"], "token_info": ["word"], "pos": ["ITJ"], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Wie/ da\u00df der Moder schon die sch\u00f6nen Glieder fri\u00dft/", "tokens": ["Wie", "/", "da\u00df", "der", "Mo\u00b7der", "schon", "die", "sch\u00f6\u00b7nen", "Glie\u00b7der", "fri\u00dft", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "KOUS", "ART", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und deine ", "tokens": ["Und", "dei\u00b7ne"], "token_info": ["word", "word"], "pos": ["KON", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Der offt gew\u00fcnschte Tod/ der wird mir jetzt versagt/", "tokens": ["Der", "offt", "ge\u00b7w\u00fcnschte", "Tod", "/", "der", "wird", "mir", "jetzt", "ver\u00b7sagt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$(", "ART", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Da dessen Stachel mir am allers\u00fc\u00dften scheinet/", "tokens": ["Da", "des\u00b7sen", "Sta\u00b7chel", "mir", "am", "al\u00b7ler\u00b7s\u00fc\u00df\u00b7ten", "schei\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELAT", "NN", "PPER", "APPRART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wer ist der deinen Tod im Hertzen nicht beklagt/", "tokens": ["Wer", "ist", "der", "dei\u00b7nen", "Tod", "im", "Hert\u00b7zen", "nicht", "be\u00b7klagt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "PPOSAT", "NN", "APPRART", "NN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und mir Gequ\u00e4hlten gleich viel heisse Z\u00e4hren weinet?", "tokens": ["Und", "mir", "Ge\u00b7qu\u00e4hl\u00b7ten", "gleich", "viel", "heis\u00b7se", "Z\u00e4h\u00b7ren", "wei\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "ADV", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Doch der dein Vater hei\u00dft/ der dir das Leben gab/", "tokens": ["Doch", "der", "dein", "Va\u00b7ter", "hei\u00dft", "/", "der", "dir", "das", "Le\u00b7ben", "gab", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPOSAT", "NN", "VVFIN", "$(", "PRELS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Das Felsen-harte Hertz empfindet kleine Schmertzen/", "tokens": ["Das", "Fel\u00b7sen\u00b7har\u00b7te", "Hertz", "emp\u00b7fin\u00b7det", "klei\u00b7ne", "Schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Es st\u00fcrtzt um kleine Ehe den besten Sohn ins Grab/", "tokens": ["Es", "st\u00fcrtzt", "um", "klei\u00b7ne", "E\u00b7he", "den", "bes\u00b7ten", "Sohn", "ins", "Grab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Und will ums K\u00f6nigs Gunst der Kinder Heyl verschertzen.", "tokens": ["Und", "will", "ums", "K\u00f6\u00b7nigs", "Gunst", "der", "Kin\u00b7der", "Heyl", "ver\u00b7schert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPRART", "NN", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ach! da\u00df der Donner dich nicht l\u00e4ngst zerschlagen hat", "tokens": ["Ach", "!", "da\u00df", "der", "Don\u00b7ner", "dich", "nicht", "l\u00e4ngst", "zer\u00b7schla\u00b7gen", "hat"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KOUS", "ART", "NN", "PPER", "PTKNEG", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Du wilder W\u00fcterich! du Muster grauser V\u00e4ter/", "tokens": ["Du", "wil\u00b7der", "W\u00fc\u00b7te\u00b7rich", "!", "du", "Mus\u00b7ter", "grau\u00b7ser", "V\u00e4\u00b7ter", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "PPER", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ha ", "tokens": ["Ha"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.32": {"text": "Pfuy sch\u00e4me dich ins Hertz verdammeter Verr\u00e4hter!", "tokens": ["Pfuy", "sch\u00e4\u00b7me", "dich", "ins", "Hertz", "ver\u00b7dam\u00b7me\u00b7ter", "Ver\u00b7r\u00e4h\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ach ", "tokens": ["Ach"], "token_info": ["word"], "pos": ["ITJ"], "meter": "+", "measure": "single.up"}, "line.34": {"text": "Ahalibama stirbt wenn sie den Fall erweget/", "tokens": ["A\u00b7ha\u00b7li\u00b7ba\u00b7ma", "stirbt", "wenn", "sie", "den", "Fall", "er\u00b7we\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Mein Hertze steigt heraus aus der erstorbnen Brust/", "tokens": ["Mein", "Hert\u00b7ze", "steigt", "he\u00b7raus", "aus", "der", "ers\u00b7torb\u00b7nen", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VVFIN", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die Geister wein\u2019 ich weg/ die mein Gehirne heget.", "tokens": ["Die", "Geis\u00b7ter", "wein'", "ich", "weg", "/", "die", "mein", "Ge\u00b7hir\u00b7ne", "he\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Dein letzteres Gebot das Sterben mir verbeut/", "tokens": ["Dein", "letz\u00b7te\u00b7res", "Ge\u00b7bot", "das", "Ster\u00b7ben", "mir", "ver\u00b7beut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Dis geh\u2019 ich endlich ein/ allein die alten G\u00f6tter", "tokens": ["Dis", "geh'", "ich", "end\u00b7lich", "ein", "/", "al\u00b7lein", "die", "al\u00b7ten", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ART", "$(", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "La\u00df ich nicht um den Tand gelehrter Seltenheit/", "tokens": ["La\u00df", "ich", "nicht", "um", "den", "Tand", "ge\u00b7lehr\u00b7ter", "Sel\u00b7ten\u00b7heit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "ART", "NN", "ADJA", "NN", "$("], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.40": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.41": {"text": "Ich lebe/ aber ach! zu meinen gr\u00f6\u00dften Schmertz/", "tokens": ["Ich", "le\u00b7be", "/", "a\u00b7ber", "ach", "!", "zu", "mei\u00b7nen", "gr\u00f6\u00df\u00b7ten", "Schmertz", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADV", "ADV", "$.", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Mein bester Schatz ist todt/ mein Leben ist verblichen/", "tokens": ["Mein", "bes\u00b7ter", "Schatz", "ist", "todt", "/", "mein", "Le\u00b7ben", "ist", "ver\u00b7bli\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$(", "PPOSAT", "NN", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ich wei\u00df nicht wie mir ist? ich lebe ohne Hertz/", "tokens": ["Ich", "wei\u00df", "nicht", "wie", "mir", "ist", "?", "ich", "le\u00b7be", "oh\u00b7ne", "Hertz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWAV", "PPER", "VAFIN", "$.", "PPER", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Mit deinem Tod/ mein Schatz/ sind Hertz und Geist entwichen.", "tokens": ["Mit", "dei\u00b7nem", "Tod", "/", "mein", "Schatz", "/", "sind", "Hertz", "und", "Geist", "ent\u00b7wi\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "VAFIN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Ich lebe in der Lust von aller Lust entfernt/", "tokens": ["Ich", "le\u00b7be", "in", "der", "Lust", "von", 