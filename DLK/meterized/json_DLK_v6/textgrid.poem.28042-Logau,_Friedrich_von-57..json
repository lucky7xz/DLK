{"textgrid.poem.28042": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "57.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Portia gibt Antwort drum,", "tokens": ["Por\u00b7tia", "gibt", "Ant\u00b7wort", "drum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "PAV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Da\u00df sie nicht den Mann kan achten;", "tokens": ["Da\u00df", "sie", "nicht", "den", "Mann", "kan", "ach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer mit vielen gehet um,", "tokens": ["Wer", "mit", "vie\u00b7len", "ge\u00b7het", "um", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kan auff eines nicht nur trachten.", "tokens": ["Kan", "auff", "ei\u00b7nes", "nicht", "nur", "trach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}