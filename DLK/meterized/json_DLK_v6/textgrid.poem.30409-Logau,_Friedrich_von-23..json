{"textgrid.poem.30409": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eh Jungfer mocht und Junggeselle sich weiland bey den Sacis paaren,", "tokens": ["Eh", "Jung\u00b7fer", "mocht", "und", "Jung\u00b7ge\u00b7sel\u00b7le", "sich", "wei\u00b7land", "bey", "den", "Sa\u00b7cis", "paa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "KON", "NN", "PRF", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Must eines vor de\u00df andren St\u00e4rcke durch einen sondren Kampff erfahren;", "tokens": ["Must", "ei\u00b7nes", "vor", "de\u00df", "an\u00b7dren", "St\u00e4r\u00b7cke", "durch", "ei\u00b7nen", "sond\u00b7ren", "Kampff", "er\u00b7fah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Wer \u00fcberwand, war Herr im Hause. Bey uns begehren nicht au\u00df St\u00e4rcke", "tokens": ["Wer", "\u00fc\u00b7ber\u00b7wand", ",", "war", "Herr", "im", "Hau\u00b7se", ".", "Bey", "uns", "be\u00b7geh\u00b7ren", "nicht", "au\u00df", "St\u00e4r\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "VAFIN", "NN", "APPRART", "NN", "$.", "APPR", "PPER", "VVFIN", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Die Weiber Vorzug, Herrschafft, Ehre, vielmehr dieweil sie schwache Wercke.", "tokens": ["Die", "Wei\u00b7ber", "Vor\u00b7zug", ",", "Herr\u00b7schafft", ",", "Eh\u00b7re", ",", "viel\u00b7mehr", "die\u00b7weil", "sie", "schwa\u00b7che", "Wer\u00b7cke", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "$,", "NN", "$,", "ADV", "KOUS", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+------+-+-", "measure": "unknown.measure.hexa"}}}}}