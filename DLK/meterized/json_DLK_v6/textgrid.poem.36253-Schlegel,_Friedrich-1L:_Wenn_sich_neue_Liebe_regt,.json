{"textgrid.poem.36253": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn sich neue Liebe regt,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn sich neue Liebe regt,", "tokens": ["Wenn", "sich", "neu\u00b7e", "Lie\u00b7be", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles die Gef\u00fchle wagen,", "tokens": ["Al\u00b7les", "die", "Ge\u00b7f\u00fch\u00b7le", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die man, ach! so gerne hegt,", "tokens": ["Die", "man", ",", "ach", "!", "so", "ger\u00b7ne", "hegt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "ITJ", "$.", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df mich f\u00fchlen, doch nicht sagen,", "tokens": ["La\u00df", "mich", "f\u00fch\u00b7len", ",", "doch", "nicht", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "$,", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die Seele sich bewegt.", "tokens": ["Wie", "die", "See\u00b7le", "sich", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird sie jemals sich beschr\u00e4nken?", "tokens": ["Wird", "sie", "je\u00b7mals", "sich", "be\u00b7schr\u00e4n\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sich in Lust und Leid zu senken,", "tokens": ["Sich", "in", "Lust", "und", "Leid", "zu", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kann sie nimmer sich entw\u00f6hnen!", "tokens": ["Kann", "sie", "nim\u00b7mer", "sich", "ent\u00b7w\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Doch was soll das eitle Denken?", "tokens": ["Doch", "was", "soll", "das", "eit\u00b7le", "Den\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "S\u00fc\u00dfe Liebe denkt in T\u00f6nen.", "tokens": ["S\u00fc\u00b7\u00dfe", "Lie\u00b7be", "denkt", "in", "T\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn die Nachtigallen schlagen,", "tokens": ["Wenn", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hell die gr\u00fcne Farbe brennt,", "tokens": ["Hell", "die", "gr\u00fc\u00b7ne", "Far\u00b7be", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will ich, was die Blumen sagen", "tokens": ["Will", "ich", ",", "was", "die", "Blu\u00b7men", "sa\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Auge nur erkennt,", "tokens": ["Und", "das", "Au\u00b7ge", "nur", "er\u00b7kennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Leise kaum mich selbst befragen.", "tokens": ["Lei\u00b7se", "kaum", "mich", "selbst", "be\u00b7fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn ich wandl' auf stiller Flur,", "tokens": ["Wenn", "ich", "wandl'", "auf", "stil\u00b7ler", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Still verfolgend die Natur,", "tokens": ["Still", "ver\u00b7fol\u00b7gend", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und sie f\u00fchlend denken lerne,", "tokens": ["Und", "sie", "f\u00fch\u00b7lend", "den\u00b7ken", "ler\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Folg' ich den Gef\u00fchlen nur,", "tokens": ["Fol\u00b7g'", "ich", "den", "Ge\u00b7f\u00fch\u00b7len", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.10": {"text": "Denn Gedanken stehn zu ferne.", "tokens": ["Denn", "Ge\u00b7dan\u00b7ken", "stehn", "zu", "fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "PTKZU", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer es je im Herzen wagte,", "tokens": ["Wer", "es", "je", "im", "Her\u00b7zen", "wag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu dem \u00c4ther zu entfliehen,", "tokens": ["Zu", "dem", "\u00c4\u00b7ther", "zu", "ent\u00b7flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Den der Himmel uns versagte,", "tokens": ["Den", "der", "Him\u00b7mel", "uns", "ver\u00b7sag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denkt in leisen Fantasien,", "tokens": ["Denkt", "in", "lei\u00b7sen", "Fan\u00b7ta\u00b7si\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Was er nie in Worten sagte.", "tokens": ["Was", "er", "nie", "in", "Wor\u00b7ten", "sag\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Worten ist es nicht gegeben,", "tokens": ["Wor\u00b7ten", "ist", "es", "nicht", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Unsre Seele zu beleben;", "tokens": ["Uns\u00b7re", "See\u00b7le", "zu", "be\u00b7le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nah' sich ahnden schon das Ferne,", "tokens": ["Nah'", "sich", "ahn\u00b7den", "schon", "das", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "L\u00e4chelnd weinen, lieben, leben", "tokens": ["L\u00e4\u00b7chelnd", "wei\u00b7nen", ",", "lie\u00b7ben", ",", "le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADJD", "VVINF", "$,", "VVFIN", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Nur in T\u00f6nen mag sie gerne.", "tokens": ["Nur", "in", "T\u00f6\u00b7nen", "mag", "sie", "ger\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn sich s\u00fc\u00df Musik ergossen,", "tokens": ["Wenn", "sich", "s\u00fc\u00df", "Mu\u00b7sik", "er\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJD", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Darf es der Gesang nur wagen,", "tokens": ["Darf", "es", "der", "Ge\u00b7sang", "nur", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in Wohllaut hingegossen", "tokens": ["Und", "in", "Wohl\u00b7laut", "hin\u00b7ge\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leise zu der Laute sagen,", "tokens": ["Lei\u00b7se", "zu", "der", "Lau\u00b7te", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df im Wohllaut wir zerflossen.", "tokens": ["Da\u00df", "im", "Wohl\u00b7laut", "wir", "zer\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "--++--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wenn man den Gesang nur kennte,", "tokens": ["Wenn", "man", "den", "Ge\u00b7sang", "nur", "kenn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ihn den Schmerzen nicht mi\u00dfg\u00f6nnte,", "tokens": ["Ihn", "den", "Schmer\u00b7zen", "nicht", "mi\u00df\u00b7g\u00f6nn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "W\u00fcrden sie sich leicht vers\u00f6hnen,", "tokens": ["W\u00fcr\u00b7den", "sie", "sich", "leicht", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und die sch\u00f6ne Liebe k\u00f6nnte", "tokens": ["Und", "die", "sch\u00f6\u00b7ne", "Lie\u00b7be", "k\u00f6nn\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Alles, was sie will, versch\u00f6nen.", "tokens": ["Al\u00b7les", ",", "was", "sie", "will", ",", "ver\u00b7sch\u00f6\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "VMFIN", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}