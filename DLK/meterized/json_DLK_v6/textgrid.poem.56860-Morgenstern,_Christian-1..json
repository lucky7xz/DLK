{"textgrid.poem.56860": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich stand, ein Berg,", "tokens": ["Ich", "stand", ",", "ein", "Berg", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "still und einsam.", "tokens": ["still", "und", "ein\u00b7sam", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Da kamst ", "tokens": ["Da", "kamst"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "und zerschmolzest", "tokens": ["und", "zer\u00b7schmol\u00b7zest"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "das Erz meiner Adern!", "tokens": ["das", "Erz", "mei\u00b7ner", "A\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Nun bricht es vulkanisch heraus,", "tokens": ["Nun", "bricht", "es", "vul\u00b7ka\u00b7nisch", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ein Schrecken dem Wandrer,", "tokens": ["ein", "Schre\u00b7cken", "dem", "Wand\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "ein Schrecken mir