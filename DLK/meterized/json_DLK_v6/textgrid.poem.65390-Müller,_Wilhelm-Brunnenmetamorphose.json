{"textgrid.poem.65390": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Brunnenmetamorphose", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Wunder! Wie die kalten Erdenquellen", "tokens": ["O", "Wun\u00b7der", "!", "Wie", "die", "kal\u00b7ten", "Er\u00b7den\u00b7quel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von hei\u00dfer Gluth durchdrungen \u00fcberschwellen!", "tokens": ["Von", "hei\u00b7\u00dfer", "Gluth", "durch\u00b7drun\u00b7gen", "\u00fc\u00b7bersc\u00b7hwel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich trinke, Feuer flie\u00dft durch meine Glieder,", "tokens": ["Ich", "trin\u00b7ke", ",", "Feu\u00b7er", "flie\u00dft", "durch", "mei\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und meinen Becher setz' ich staunend nieder.", "tokens": ["Und", "mei\u00b7nen", "Be\u00b7cher", "setz'", "ich", "stau\u00b7nend", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich ahn' es wohl! Es sind die Wunderlippen,", "tokens": ["Ich", "ahn'", "es", "wohl", "!", "Es", "sind", "die", "Wun\u00b7der\u00b7lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die heut' zuerst aus diesem Sprudel nippen.", "tokens": ["Die", "heut'", "zu\u00b7erst", "aus", "die\u00b7sem", "Spru\u00b7del", "nip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie haben ihm den Erdenstoff genommen", "tokens": ["Sie", "ha\u00b7ben", "ihm", "den", "Er\u00b7den\u00b7stoff", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ihn mit ihrer Himmelskraft durchglommen.", "tokens": ["Und", "ihn", "mit", "ih\u00b7rer", "Him\u00b7mels\u00b7kraft", "durch\u00b7glom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So will ich trinken und nicht mehr mich h\u00e4rmen,", "tokens": ["So", "will", "ich", "trin\u00b7ken", "und", "nicht", "mehr", "mich", "h\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "KON", "PTKNEG", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob mich das Wasser k\u00fchlen mag, ob w\u00e4rmen,", "tokens": ["Ob", "mich", "das", "Was\u00b7ser", "k\u00fch\u00b7len", "mag", ",", "ob", "w\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN", "$,", "KOUS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und vorbereitend mich der Quelle nahen,", "tokens": ["Und", "vor\u00b7be\u00b7rei\u00b7tend", "mich", "der", "Quel\u00b7le", "na\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "PPER", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus der die Brunnen jetzt ihr Heil empfahen.", "tokens": ["Aus", "der", "die", "Brun\u00b7nen", "jetzt", "ihr", "Heil", "em\u00b7pfa\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}