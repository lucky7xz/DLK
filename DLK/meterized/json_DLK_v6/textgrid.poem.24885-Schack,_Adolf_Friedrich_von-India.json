{"textgrid.poem.24885": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "India", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft, wenn der Lebenstag mit dumpfer Schw\u00fcle", "tokens": ["Oft", ",", "wenn", "der", "Le\u00b7bens\u00b7tag", "mit", "dum\u00b7pfer", "Schw\u00fc\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf meinem Haupte dr\u00fcckt,", "tokens": ["Auf", "mei\u00b7nem", "Haup\u00b7te", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Eil' ich zu dir, da\u00df frische D\u00e4mmerk\u00fchle", "tokens": ["Eil'", "ich", "zu", "dir", ",", "da\u00df", "fri\u00b7sche", "D\u00e4m\u00b7mer\u00b7k\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PPER", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die m\u00fcde Stirn erquickt.", "tokens": ["Die", "m\u00fc\u00b7de", "Stirn", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Vom Glanz der Erdenjugend noch umflossen,", "tokens": ["Vom", "Glanz", "der", "Er\u00b7den\u00b7ju\u00b7gend", "noch", "um\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Fr\u00fchrot \u00fcbergl\u00fcht,", "tokens": ["Vom", "Fr\u00fch\u00b7rot", "\u00fc\u00b7berg\u00b7l\u00fcht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist, reich in Duft und Farbenpracht erschlossen,", "tokens": ["Ist", ",", "reich", "in", "Duft", "und", "Far\u00b7ben\u00b7pracht", "er\u00b7schlos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADJD", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Garten aufgebl\u00fcht.", "tokens": ["Dein", "Gar\u00b7ten", "auf\u00b7ge\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hoch von des Himalaya eis'ger Klippe,", "tokens": ["Hoch", "von", "des", "Hi\u00b7ma\u00b7la\u00b7ya", "eis'\u00b7ger", "Klip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Dem \u00e4ltsten G\u00f6tterdom,", "tokens": ["Dem", "\u00e4lts\u00b7ten", "G\u00f6t\u00b7ter\u00b7dom", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "St\u00fcrzt sich, ein Gott, Begeistrung auf der Lippe,", "tokens": ["St\u00fcrzt", "sich", ",", "ein", "Gott", ",", "Be\u00b7geis\u00b7trung", "auf", "der", "Lip\u00b7pe", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "ART", "NN", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Herab der Gangesstrom;", "tokens": ["Her\u00b7ab", "der", "Gan\u00b7ges\u00b7strom", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und Tempel, die das Weltgeheimnis h\u00fcten,", "tokens": ["Und", "Tem\u00b7pel", ",", "die", "das", "Welt\u00b7ge\u00b7heim\u00b7nis", "h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Stehn l\u00e4ngs der Flut gereiht;", "tokens": ["Stehn", "l\u00e4ngs", "der", "Flut", "ge\u00b7reiht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Im heil'gen Kelche ihrer Lotosbl\u00fcten", "tokens": ["Im", "heil'\u00b7gen", "Kel\u00b7che", "ih\u00b7rer", "Lo\u00b7tos\u00b7bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schl\u00e4ft die Unsterblichkeit.", "tokens": ["Schl\u00e4ft", "die", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Dort unter deiner Pflanzenwelt Titanen", "tokens": ["Dort", "un\u00b7ter", "dei\u00b7ner", "Pflan\u00b7zen\u00b7welt", "Ti\u00b7ta\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sitz' ich in Waldesnacht,", "tokens": ["Sitz'", "ich", "in", "Wal\u00b7des\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo tiefer noch das Ranken der Lianen", "tokens": ["Wo", "tie\u00b7fer", "noch", "das", "Ran\u00b7ken", "der", "Li\u00b7a\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das ernste Dunkel macht,", "tokens": ["Das", "erns\u00b7te", "Dun\u00b7kel", "macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wo von den Felsen, die vor Alter wanken,", "tokens": ["Wo", "von", "den", "Fel\u00b7sen", ",", "die", "vor", "Al\u00b7ter", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In den Granit gehaun,", "tokens": ["In", "den", "Gra\u00b7nit", "ge\u00b7haun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Auf mich herab die riesigen Gedanken", "tokens": ["Auf", "mich", "her\u00b7ab", "die", "rie\u00b7si\u00b7gen", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vergangner Tage schaun.", "tokens": ["Ver\u00b7gang\u00b7ner", "Ta\u00b7ge", "schaun", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Baniane steigt, das Kind der Tropen,", "tokens": ["Die", "Ba\u00b7ni\u00b7a\u00b7ne", "steigt", ",", "das", "Kind", "der", "Tro\u00b7pen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Breit\u00e4stig himmelauf;", "tokens": ["Brei\u00b7t\u00e4s\u00b7tig", "him\u00b7me\u00b7lauf", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durchs Dickicht fliehen schlanke Antilopen", "tokens": ["Durchs", "Di\u00b7ckicht", "flie\u00b7hen", "schlan\u00b7ke", "An\u00b7ti\u00b7lo\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVINF", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dahin in scheuem Lauf.", "tokens": ["Da\u00b7hin", "in", "scheu\u00b7em", "Lauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und zu mir, Lilien um die Stirn gewunden,", "tokens": ["Und", "zu", "mir", ",", "Li\u00b7li\u00b7en", "um", "die", "Stirn", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "NN", "APPR", "ART", "NN", "VAPP", "$,"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Das Auge gottbeseelt,", "tokens": ["Das", "Au\u00b7ge", "gott\u00b7be\u00b7seelt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gesellt Vyasa sich, der mir die Kunden", "tokens": ["Ge\u00b7sellt", "Vy\u00b7a\u00b7sa", "sich", ",", "der", "mir", "die", "Kun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "NE", "PRF", "$,", "PRELS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von alter Zeit erz\u00e4hlt,", "tokens": ["Von", "al\u00b7ter", "Zeit", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Indessen oben in den Palmenb\u00e4umen,", "tokens": ["In\u00b7des\u00b7sen", "o\u00b7ben", "in", "den", "Pal\u00b7men\u00b7b\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie sie der Windhauch schwingt,", "tokens": ["Wie", "sie", "der", "Wind\u00b7hauch", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Geist der Urzeit von den Wundertr\u00e4umen", "tokens": ["Ein", "Geist", "der", "Ur\u00b7zeit", "von", "den", "Wun\u00b7der\u00b7tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der ersten Weltnacht singt.", "tokens": ["Der", "ers\u00b7ten", "Welt\u00b7nacht", "singt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}