{"textgrid.poem.65896": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Kore", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Tochter Demeters, in weichen Schleiern,", "tokens": ["Die", "Toch\u00b7ter", "De\u00b7me\u00b7ters", ",", "in", "wei\u00b7chen", "Schlei\u00b7ern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erblickt ein Nymphenpaar zum Blumenspiel:", "tokens": ["Er\u00b7blickt", "ein", "Nym\u00b7phen\u00b7paar", "zum", "Blu\u00b7men\u00b7spiel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach, das entsternt den zarten Kranz vom Stiel,", "tokens": ["Ach", ",", "das", "ents\u00b7ternt", "den", "zar\u00b7ten", "Kranz", "vom", "Stiel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDS", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Befragt sein Schicksal froh um Hochzeitsfeiern.", "tokens": ["Be\u00b7fragt", "sein", "Schick\u00b7sal", "froh", "um", "Hoch\u00b7zeits\u00b7fei\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Oft spiegeln sich die Reizenden in Weihern,", "tokens": ["Oft", "spie\u00b7geln", "sich", "die", "Rei\u00b7zen\u00b7den", "in", "Wei\u00b7hern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Gang durch Wald dahin wird heitres Ziel;", "tokens": ["Ein", "Gang", "durch", "Wald", "da\u00b7hin", "wird", "heit\u00b7res", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PAV", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Man lacht zum Wasser, beugt sich im Profil", "tokens": ["Man", "lacht", "zum", "Was\u00b7ser", ",", "beugt", "sich", "im", "Pro\u00b7fil"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und denkt, ergl\u00fcht: bald herrsch ich \u00fcber Freiern!", "tokens": ["Und", "denkt", ",", "er\u00b7gl\u00fcht", ":", "bald", "herrsch", "ich", "\u00fc\u00b7ber", "Frei\u00b7ern", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "$.", "ADV", "ADJD", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Knabe h\u00fcpft verkleidet in den Reigen", "tokens": ["Ein", "Kna\u00b7be", "h\u00fcpft", "ver\u00b7klei\u00b7det", "in", "den", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der schlanken M\u00e4dchen und vergn\u00fcgt sich mit:", "tokens": ["Der", "schlan\u00b7ken", "M\u00e4d\u00b7chen", "und", "ver\u00b7gn\u00fcgt", "sich", "mit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er zeigt verj\u00fcngter Sitte Sich-Verneigen,", "tokens": ["Er", "zeigt", "ver\u00b7j\u00fcng\u00b7ter", "Sit\u00b7te", "Sich\u00b7Ver\u00b7nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Jonierinnen leichten Spitzen-Schritt:", "tokens": ["Der", "Jo\u00b7ni\u00b7e\u00b7rin\u00b7nen", "leich\u00b7ten", "Spit\u00b7zen\u00b7Schritt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Der Tanz ist anders. Seine Anmut eigen.", "tokens": ["Der", "Tanz", "ist", "an\u00b7ders", ".", "Sei\u00b7ne", "An\u00b7mut", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$.", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er singt dazu von Wein und Pantherritt.", "tokens": ["Er", "singt", "da\u00b7zu", "von", "Wein", "und", "Pan\u00b7ther\u00b7ritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}