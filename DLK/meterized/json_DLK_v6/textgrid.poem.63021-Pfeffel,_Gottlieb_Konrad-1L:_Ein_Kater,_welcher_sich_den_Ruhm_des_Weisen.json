{"textgrid.poem.63021": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Kater, welcher sich den Ruhm des Weisen", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Kater, welcher sich den Ruhm des Weisen", "tokens": ["Ein", "Ka\u00b7ter", ",", "wel\u00b7cher", "sich", "den", "Ruhm", "des", "Wei\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bey seinem Volk erwarb, fiel in Melancholie", "tokens": ["Bey", "sei\u00b7nem", "Volk", "er\u00b7warb", ",", "fiel", "in", "Me\u00b7lan\u00b7cho\u00b7lie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sah durchs Mikroskop der d\u00fcstern Phantasie,", "tokens": ["Und", "sah", "durchs", "Mik\u00b7ros\u00b7kop", "der", "d\u00fcs\u00b7tern", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo er nur gieng und stand ein Heer von Flederm\u00e4usen.", "tokens": ["Wo", "er", "nur", "gieng", "und", "stand", "ein", "Heer", "von", "Fle\u00b7der\u00b7m\u00e4u\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Umsonst bem\u00fchte sich der Freunde treues Chor,", "tokens": ["Um\u00b7sonst", "be\u00b7m\u00fch\u00b7te", "sich", "der", "Freun\u00b7de", "treu\u00b7es", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von Aerzten unterst\u00fctzt, den Irrwahn zu besiegen,", "tokens": ["Von", "A\u00b7erz\u00b7ten", "un\u00b7ter\u00b7st\u00fctzt", ",", "den", "Irr\u00b7wahn", "zu", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Er sah die Thiere nach wie vor", "tokens": ["Er", "sah", "die", "Thie\u00b7re", "nach", "wie", "vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "KOKOM", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Um seinen Kopf, wie Jesuiten, fliegen:", "tokens": ["Um", "sei\u00b7nen", "Kopf", ",", "wie", "Je\u00b7su\u00b7i\u00b7ten", ",", "flie\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "$,", "PWAV", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Oft scho\u00df er als ein Pfeil auf die verw\u00fcnschte Brut", "tokens": ["Oft", "scho\u00df", "er", "als", "ein", "Pfeil", "auf", "die", "ver\u00b7w\u00fcnschte", "Brut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Und haschte nichts; allein statt sie zu d\u00e4mpfen", "tokens": ["Und", "haschte", "nichts", ";", "al\u00b7lein", "statt", "sie", "zu", "d\u00e4mp\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "$.", "ADV", "APPR", "PPER", "PTKZU", "VVINF"], "meter": "--+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Vermehrte das nur seine Wuth.", "tokens": ["Ver\u00b7mehr\u00b7te", "das", "nur", "sei\u00b7ne", "Wuth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Oft schlo\u00df er, matt von den erhitzten K\u00e4mpfen,", "tokens": ["Oft", "schlo\u00df", "er", ",", "matt", "von", "den", "er\u00b7hitz\u00b7ten", "K\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUI", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die Augen zu. Vergebens schlo\u00df er sie:", "tokens": ["Die", "Au\u00b7gen", "zu", ".", "Ver\u00b7ge\u00b7bens", "schlo\u00df", "er", "sie", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Auch dann noch sah er Flederm\u00e4use.", "tokens": ["Auch", "dann", "noch", "sah", "er", "Fle\u00b7der\u00b7m\u00e4u\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "So trieb ers lang. Einst jagt ihn die Manie", "tokens": ["So", "trieb", "ers", "lang", ".", "Einst", "jagt", "ihn", "die", "Ma\u00b7nie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "$.", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "In eines Kirchthurms Uhrgeh\u00e4use;", "tokens": ["In", "ei\u00b7nes", "Kirch\u00b7thurms", "Uhr\u00b7ge\u00b7h\u00e4u\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Hier fieng er wirklich eine Fledermaus.", "tokens": ["Hier", "fi\u00b7eng", "er", "wirk\u00b7lich", "ei\u00b7ne", "Fle\u00b7der\u00b7maus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Da sieht mans, rief nun Murner aus,", "tokens": ["Da", "sieht", "mans", ",", "rief", "nun", "Mur\u00b7ner", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "VVFIN", "ADV", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Die Narren wollten mir die Wahrheit disputieren,", "tokens": ["Die", "Nar\u00b7ren", "woll\u00b7ten", "mir", "die", "Wahr\u00b7heit", "dis\u00b7pu\u00b7tie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nun kann ich sie handgreiflich \u00fcberf\u00fchren.", "tokens": ["Nun", "kann", "ich", "sie", "hand\u00b7greif\u00b7lich", "\u00fc\u00b7berf\u00b7\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}