{"textgrid.poem.25293": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sp\u00fcrst du es herbsten, Wacholder?", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sp\u00fcrst du es herbsten, Wacholder?", "tokens": ["Sp\u00fcrst", "du", "es", "herbs\u00b7ten", ",", "Wa\u00b7chol\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVFIN", "$,", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Tiefdunkel gr\u00fcnen die Erlen/", "tokens": ["Tief\u00b7dun\u00b7kel", "gr\u00fc\u00b7nen", "die", "Er\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch Sonne k\u00fc\u00dft immer holder.", "tokens": ["Doch", "Son\u00b7ne", "k\u00fc\u00dft", "im\u00b7mer", "hol\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Schwebt dorten nicht wei\u00df Gespinnst?", "tokens": ["Schwebt", "dor\u00b7ten", "nicht", "wei\u00df", "Ge\u00b7spinnst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "VVFIN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ach, Silberhaar, schweifende Wehmut", "tokens": ["Ach", ",", "Sil\u00b7ber\u00b7haar", ",", "schwei\u00b7fen\u00b7de", "Weh\u00b7mut"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "NN", "$,", "ADJA", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Ist all meines Sommers Gewinnst.", "tokens": ["Ist", "all", "mei\u00b7nes", "Som\u00b7mers", "Ge\u00b7winnst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Wacholder, dir bleiben die Nadeln.", "tokens": ["Wa\u00b7chol\u00b7der", ",", "dir", "blei\u00b7ben", "die", "Na\u00b7deln", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Laubherzlein m\u00f6gen welken,", "tokens": ["Laub\u00b7herz\u00b7lein", "m\u00f6\u00b7gen", "wel\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Uns beide soll Winterleid adeln.", "tokens": ["Uns", "bei\u00b7de", "soll", "Win\u00b7ter\u00b7leid", "a\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VMFIN", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Geistender Nebel auf Mooren.", "tokens": ["Geis\u00b7ten\u00b7der", "Ne\u00b7bel", "auf", "Moo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Du Welt hast heimliche Schl\u00fcfte;", "tokens": ["Du", "Welt", "hast", "heim\u00b7li\u00b7che", "Schl\u00fcf\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wohin ging Jugend verloren?", "tokens": ["Wo\u00b7hin", "ging", "Ju\u00b7gend", "ver\u00b7lo\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Mu\u00df Moder denn alles beerben?", "tokens": ["Mu\u00df", "Mo\u00b7der", "denn", "al\u00b7les", "be\u00b7er\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "PIS", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Hin rieseln die Augenblicke;", "tokens": ["Hin", "rie\u00b7seln", "die", "Au\u00b7gen\u00b7bli\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ach, alles Leben ein Sterben.", "tokens": ["Ach", ",", "al\u00b7les", "Le\u00b7ben", "ein", "Ster\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Der Himmel mattrotes Gold.", "tokens": ["Der", "Him\u00b7mel", "mat\u00b7tro\u00b7tes", "Gold", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "O bliebe doch eine Treue", "tokens": ["O", "blie\u00b7be", "doch", "ei\u00b7ne", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dem Begrabenen ewig hold!", "tokens": ["Dem", "Be\u00b7gra\u00b7be\u00b7nen", "e\u00b7wig", "hold", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Abendfunken verglimmen.", "tokens": ["A\u00b7bend\u00b7fun\u00b7ken", "ver\u00b7glim\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Wie Fl\u00f6tenseufzer will endlos", "tokens": ["Wie", "Fl\u00f6\u00b7ten\u00b7seuf\u00b7zer", "will", "end\u00b7los"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "VMFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Sehnen ins Weite schwimmen.", "tokens": ["Mein", "Seh\u00b7nen", "ins", "Wei\u00b7te", "schwim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Horch, s\u00e4useln nicht Friedhofs Cypressen?", "tokens": ["Horch", ",", "s\u00e4u\u00b7seln", "nicht", "Fried\u00b7hofs", "Cyp\u00b7res\u00b7sen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PTKNEG", "NE", "NE", "$."], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich wei\u00df eine Seele der Seelen,", "tokens": ["Ich", "wei\u00df", "ei\u00b7ne", "See\u00b7le", "der", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die kann kein St\u00e4ubchen vergessen.", "tokens": ["Die", "kann", "kein", "St\u00e4ub\u00b7chen", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}