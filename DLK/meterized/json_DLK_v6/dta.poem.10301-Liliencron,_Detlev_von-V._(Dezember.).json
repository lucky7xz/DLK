{"dta.poem.10301": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "V.  \n (Dezember.)", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Von Norwegs Felsen klingt es zu mir her,", "tokens": ["Von", "Nor\u00b7wegs", "Fel\u00b7sen", "klingt", "es", "zu", "mir", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Lied so r\u00fchrend und im Klang so leise,", "tokens": ["Ein", "Lied", "so", "r\u00fch\u00b7rend", "und", "im", "Klang", "so", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Sommerwellgesp\u00fcl dieselbe Weise;", "tokens": ["Wie", "Som\u00b7mer\u00b7well\u00b7ge\u00b7sp\u00fcl", "die\u00b7sel\u00b7be", "Wei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein armer Geistgetr\u00fcbter singt so schwer.", "tokens": ["Ein", "ar\u00b7mer", "Geist\u00b7ge\u00b7tr\u00fcb\u00b7ter", "singt", "so", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein junger blonder K\u00f6nig steht am Speer,", "tokens": ["Ein", "jun\u00b7ger", "blon\u00b7der", "K\u00f6\u00b7nig", "steht", "am", "Speer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf rotem Vorsprungriff, um ihn im Kreise", "tokens": ["Auf", "ro\u00b7tem", "Vor\u00b7sprun\u00b7griff", ",", "um", "ihn", "im", "Krei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUI", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kauern, das Haupt zur Erde, hundert Greise;", "tokens": ["Kau\u00b7ern", ",", "das", "Haupt", "zur", "Er\u00b7de", ",", "hun\u00b7dert", "Grei\u00b7se", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "APPRART", "NN", "$,", "CARD", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Er singt das Lied und schaut hinaus ins Meer.", "tokens": ["Er", "singt", "das", "Lied", "und", "schaut", "hin\u00b7aus", "ins", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Lautlose Stille rings. Von Zeit zu Zeit", "tokens": ["Laut\u00b7lo\u00b7se", "Stil\u00b7le", "rings", ".", "Von", "Zeit", "zu", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "$.", "APPR", "NN", "APPR", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Tutet das heisere Horn der K\u00fcstenwachen,", "tokens": ["Tu\u00b7tet", "das", "hei\u00b7se\u00b7re", "Horn", "der", "K\u00fcs\u00b7ten\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Der Rabe macht entsetzt die Fl\u00fcgel breit.", "tokens": ["Der", "Ra\u00b7be", "macht", "ent\u00b7setzt", "die", "Fl\u00fc\u00b7gel", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Weit, weit antwortet wo der Fischernachen,", "tokens": ["Weit", ",", "weit", "ant\u00b7wor\u00b7tet", "wo", "der", "Fi\u00b7scher\u00b7na\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVFIN", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der sich im Nebel schwer vom Eis befreit,", "tokens": ["Der", "sich", "im", "Ne\u00b7bel", "schwer", "vom", "Eis", "be\u00b7freit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schollen, die knirschen und ihn w\u00fcst umkrachen.", "tokens": ["Schol\u00b7len", ",", "die", "knir\u00b7schen", "und", "ihn", "w\u00fcst", "um\u00b7kra\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVFIN", "KON", "PPER", "VVFIN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}