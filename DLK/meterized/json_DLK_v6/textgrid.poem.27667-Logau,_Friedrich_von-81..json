{"textgrid.poem.27667": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "81.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Balearen Brauch ist zwar zu uns nicht kommen:", "tokens": ["Der", "Ba\u00b7le\u00b7a\u00b7ren", "Brauch", "ist", "zwar", "zu", "uns", "nicht", "kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "APPR", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df durch die G\u00e4ste vor der Braut wird abgenommen,", "tokens": ["Da\u00df", "durch", "die", "G\u00e4s\u00b7te", "vor", "der", "Braut", "wird", "ab\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was sonst der Br\u00e4utgam nimmt. Doch h\u00f6rt man, mancher mag", "tokens": ["Was", "sonst", "der", "Br\u00e4ut\u00b7gam", "nimmt", ".", "Doch", "h\u00f6rt", "man", ",", "man\u00b7cher", "mag"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ADV", "ART", "NE", "VVFIN", "$.", "KON", "VVFIN", "PIS", "$,", "PIS", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Thun vor, was erst man dort th\u00e4t auff den Hochzeit-Tag.", "tokens": ["Thun", "vor", ",", "was", "erst", "man", "dort", "th\u00e4t", "auff", "den", "Hoch\u00b7zeit\u00b7Tag", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN