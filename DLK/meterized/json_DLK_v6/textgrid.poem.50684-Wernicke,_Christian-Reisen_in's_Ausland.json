{"textgrid.poem.50684": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Reisen in's Ausland", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Krato reisen wollt' und von uns Abschied nahm,", "tokens": ["Als", "Kra\u00b7to", "rei\u00b7sen", "wollt'", "und", "von", "uns", "Ab\u00b7schied", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVINF", "VMFIN", "KON", "APPR", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da war er noch zu jung f\u00fcr einen weiten Ritt;", "tokens": ["Da", "war", "er", "noch", "zu", "jung", "f\u00fcr", "ei\u00b7nen", "wei\u00b7ten", "Ritt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum bracht' er, als er wiederkam,", "tokens": ["Drum", "bracht'", "er", ",", "als", "er", "wie\u00b7der\u00b7kam", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus fremden L\u00e4ndern nichts, als ihre Thorheit, mit.", "tokens": ["Aus", "frem\u00b7den", "L\u00e4n\u00b7dern", "nichts", ",", "als", "ih\u00b7re", "Thor\u00b7heit", ",", "mit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PIS", "$,", "KOUS", "PPOSAT", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So war er ", "tokens": ["So", "war", "er"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}}}}