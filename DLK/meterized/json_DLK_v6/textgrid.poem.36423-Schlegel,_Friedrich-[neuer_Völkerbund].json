{"textgrid.poem.36423": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[neuer V\u00f6lkerbund]", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vier gro\u00dfe V\u00f6lker sind sich fest verbunden;", "tokens": ["Vier", "gro\u00b7\u00dfe", "V\u00f6l\u00b7ker", "sind", "sich", "fest", "ver\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VAFIN", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Rauben gleichgesinnt, an Art verschieden.", "tokens": ["Zum", "Rau\u00b7ben", "gleich\u00b7ge\u00b7sinnt", ",", "an", "Art", "ver\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "All duldend, stiehlt durch List das ein' in Frieden;", "tokens": ["All", "dul\u00b7dend", ",", "stiehlt", "durch", "List", "das", "ein'", "in", "Frie\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "VVFIN", "APPR", "NN", "ART", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Erdkreis hielt des zweiten Mond umwunden;", "tokens": ["Den", "Erd\u00b7kreis", "hielt", "des", "zwei\u00b7ten", "Mond", "um\u00b7wun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das dritte war schon aus der Welt verschwunden;", "tokens": ["Das", "drit\u00b7te", "war", "schon", "aus", "der", "Welt", "ver\u00b7schwun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das viert' ist, wie es oft sich selbst beschieden,", "tokens": ["Das", "viert'", "ist", ",", "wie", "es", "oft", "sich", "selbst", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "$,", "PWAV", "PPER", "ADV", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das kl\u00fcgst' und gr\u00f6\u00dfte aller die hienieden.", "tokens": ["Das", "kl\u00fcgst'", "und", "gr\u00f6\u00df\u00b7te", "al\u00b7ler", "die", "hien\u00b7ie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "ADJA", "PIAT", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie wird ihr Wert auf rechter Wag' erfunden?", "tokens": ["Wie", "wird", "ihr", "Wert", "auf", "rech\u00b7ter", "Wag'", "er\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mit Recht m\u00fcssen zuerst die T\u00fcrken thronen,", "tokens": ["Mit", "Recht", "m\u00fcs\u00b7sen", "zu\u00b7erst", "die", "T\u00fcr\u00b7ken", "thro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ungl\u00e4ubig zwar, doch treu der Ehre Glauben;", "tokens": ["Un\u00b7gl\u00e4u\u00b7big", "zwar", ",", "doch", "treu", "der", "Eh\u00b7re", "Glau\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "ADV", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drauf folgen die barbarischen Polacken;", "tokens": ["Drauf", "fol\u00b7gen", "die", "bar\u00b7ba\u00b7ri\u00b7schen", "Po\u00b7la\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Gleich sind an Wert dann die zwei Nationen,", "tokens": ["Gleich", "sind", "an", "Wert", "dann", "die", "zwei", "Na\u00b7ti\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NE", "ADV", "ART", "CARD", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Franzosen, die frech l\u00fcgend alles rauben,", "tokens": ["Fran\u00b7zo\u00b7sen", ",", "die", "frech", "l\u00fc\u00b7gend", "al\u00b7les", "rau\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "ADJD", "PIS", "VVINF", "$,"], "meter": "-+-+++-+-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Der Jud', der alle fa\u00dft mit Wuchers Zacken.", "tokens": ["Der", "Jud'", ",", "der", "al\u00b7le", "fa\u00dft", "mit", "Wu\u00b7chers", "Za\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}