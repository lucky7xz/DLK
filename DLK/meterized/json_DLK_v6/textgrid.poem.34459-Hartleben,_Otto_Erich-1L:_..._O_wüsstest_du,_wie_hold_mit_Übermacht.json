{"textgrid.poem.34459": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: ... O w\u00fcsstest du, wie hold mit \u00dcbermacht", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "... O w\u00fcsstest du, wie hold mit \u00dcbermacht", "tokens": ["...", "O", "w\u00fcss\u00b7test", "du", ",", "wie", "hold", "mit", "\u00dc\u00b7ber\u00b7macht"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "NE", "VVFIN", "PPER", "$,", "PWAV", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "das Zucken jeder Fiber dich durchw\u00fchlt,", "tokens": ["das", "Zu\u00b7cken", "je\u00b7der", "Fi\u00b7ber", "dich", "durch\u00b7w\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "wenn meine Lippen sprachlos Wonne fl\u00fcstern", "tokens": ["wenn", "mei\u00b7ne", "Lip\u00b7pen", "sprach\u00b7los", "Won\u00b7ne", "fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "in deinen Leib ... O w\u00fcsstest du, wie wild", "tokens": ["in", "dei\u00b7nen", "Leib", "...", "O", "w\u00fcss\u00b7test", "du", ",", "wie", "wild"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$(", "NE", "VVFIN", "PPER", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "im Taumel deine Glieder beben lernen,", "tokens": ["im", "Tau\u00b7mel", "dei\u00b7ne", "Glie\u00b7der", "be\u00b7ben", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "als wollten sie dem Leben sich entwinden", "tokens": ["als", "woll\u00b7ten", "sie", "dem", "Le\u00b7ben", "sich", "ent\u00b7win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "ART", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "und ewig gl\u00fchn in Wollustfieberflammen ...", "tokens": ["und", "e\u00b7wig", "gl\u00fchn", "in", "Wol\u00b7lust\u00b7fie\u00b7ber\u00b7flam\u00b7men", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "O w\u00fcsstest dus! \u2013 Es ist ein Wunder, ja!", "tokens": ["O", "w\u00fcss\u00b7test", "dus", "!", "\u2013", "Es", "ist", "ein", "Wun\u00b7der", ",", "ja", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "$.", "$(", "PPER", "VAFIN", "ART", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und wer da zweifelt, wird es nimmer finden,", "tokens": ["Und", "wer", "da", "zwei\u00b7felt", ",", "wird", "es", "nim\u00b7mer", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "doch glaube nur, ach, lehne dich zur\u00fcck,", "tokens": ["doch", "glau\u00b7be", "nur", ",", "ach", ",", "leh\u00b7ne", "dich", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "ITJ", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "gieb \u00fcber deine Glieder mir Gewalt \u2013", "tokens": ["gieb", "\u00fc\u00b7ber", "dei\u00b7ne", "Glie\u00b7der", "mir", "Ge\u00b7walt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "PPER", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "und wie dem Tr\u00fcben, dem die Sonne langsam", "tokens": ["und", "wie", "dem", "Tr\u00fc\u00b7ben", ",", "dem", "die", "Son\u00b7ne", "lang\u00b7sam"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "PRELS", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "aufschliesst das Herz, bis sie ihn warm durchstr\u00f6mt,", "tokens": ["auf\u00b7schliesst", "das", "Herz", ",", "bis", "sie", "ihn", "warm", "durch\u00b7str\u00f6mt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "so wird auch dir ein unaussprechlich Gl\u00fcck,", "tokens": ["so", "wird", "auch", "dir", "ein", "un\u00b7aus\u00b7sprech\u00b7lich", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "berauschend ein Geheimnis sich enth\u00fcllen.", "tokens": ["be\u00b7rau\u00b7schend", "ein", "Ge\u00b7heim\u00b7nis", "sich", "ent\u00b7h\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}