{"textgrid.poem.49639": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Trauerklage", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Krieg ist \u00bbheilig\u00ab, der Krieg ist gut,", "tokens": ["Der", "Krieg", "ist", "\u00bb", "hei\u00b7lig", "\u00ab", ",", "der", "Krieg", "ist", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$(", "ADJD", "$(", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Fl\u00f6sse dabei nicht auch blaues Blut.", "tokens": ["Fl\u00f6s\u00b7se", "da\u00b7bei", "nicht", "auch", "blau\u00b7es", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "PTKNEG", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Fielen nur Gemeine und Bauern,", "tokens": ["Fie\u00b7len", "nur", "Ge\u00b7mei\u00b7ne", "und", "Bau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "W\u00e4re die Sache nicht sehr zu bedauern.", "tokens": ["W\u00e4\u00b7re", "die", "Sa\u00b7che", "nicht", "sehr", "zu", "be\u00b7dau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Aber \u2013 hier kommen des Mitleids T\u00f6ne \u2013", "tokens": ["A\u00b7ber", "\u2013", "hier", "kom\u00b7men", "des", "Mit\u00b7leids", "T\u00f6\u00b7ne", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ADV", "VVFIN", "ART", "NN", "NN", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Aus den besten Familien fallen die S\u00f6hne.", "tokens": ["Aus", "den", "bes\u00b7ten", "Fa\u00b7mi\u00b7li\u00b7en", "fal\u00b7len", "die", "S\u00f6h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "In der Schlacht f\u00e4llt mancher, f\u00e4llt allerhand", "tokens": ["In", "der", "Schlacht", "f\u00e4llt", "man\u00b7cher", ",", "f\u00e4llt", "al\u00b7ler\u00b7hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIS", "$,", "VVFIN", "PIAT"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Mit Gott f\u00fcr K\u00f6nig und Vaterland.", "tokens": ["Mit", "Gott", "f\u00fcr", "K\u00f6\u00b7nig", "und", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der gr\u00f6\u00dfere Teil davon ist schofel,", "tokens": ["Der", "gr\u00f6\u00b7\u00dfe\u00b7re", "Teil", "da\u00b7von", "ist", "scho\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Arbeiter, Bauern und sonstiger Pofel.", "tokens": ["Ar\u00b7bei\u00b7ter", ",", "Bau\u00b7ern", "und", "sons\u00b7ti\u00b7ger", "Po\u00b7fel", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Blieben doch nur in ihrer Sch\u00f6ne,", "tokens": ["Blie\u00b7ben", "doch", "nur", "in", "ih\u00b7rer", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Aus den besten Familien erhalten die S\u00f6hne!", "tokens": ["Aus", "den", "bes\u00b7ten", "Fa\u00b7mi\u00b7li\u00b7en", "er\u00b7hal\u00b7ten", "die", "S\u00f6h\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Burenmutter, was weinst du so laut?", "tokens": ["Bu\u00b7ren\u00b7mut\u00b7ter", ",", "was", "weinst", "du", "so", "laut", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+++-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Und du, arme verlassene Braut?", "tokens": ["Und", "du", ",", "ar\u00b7me", "ver\u00b7las\u00b7se\u00b7ne", "Braut", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADJA", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Weil euch die R\u00e4uber den Liebsten erschossen?", "tokens": ["Weil", "euch", "die", "R\u00e4u\u00b7ber", "den", "Liebs\u00b7ten", "er\u00b7schos\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ach, da sind andere Tr\u00e4nen geflossen!", "tokens": ["Ach", ",", "da", "sind", "an\u00b7de\u00b7re", "Tr\u00e4\u00b7nen", "ge\u00b7flos\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Von wirklichen Ladies um ihre Bar\u00f6ne!", "tokens": ["Von", "wirk\u00b7li\u00b7chen", "La\u00b7dies", "um", "ih\u00b7re", "Ba\u00b7r\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Aus den besten Familien fielen die S\u00f6hne.", "tokens": ["Aus", "den", "bes\u00b7ten", "Fa\u00b7mi\u00b7li\u00b7en", "fie\u00b7len", "die", "S\u00f6h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+-+---+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und fallen noch weiter. Geht ins Gericht", "tokens": ["Und", "fal\u00b7len", "noch", "wei\u00b7ter", ".", "Geht", "ins", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "$.", "VVFIN", "APPRART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit ihnen, Buren, und schont sie nicht!", "tokens": ["Mit", "ih\u00b7nen", ",", "Bu\u00b7ren", ",", "und", "schont", "sie", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NN", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihr braven, tapferen Bauernkerle", "tokens": ["Ihr", "bra\u00b7ven", ",", "tap\u00b7fe\u00b7ren", "Bau\u00b7ern\u00b7ker\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Erschie\u00dft noch manche Gesellschaftsperle,", "tokens": ["Er\u00b7schie\u00dft", "noch", "man\u00b7che", "Ge\u00b7sell\u00b7schafts\u00b7per\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die nichts tut und w\u00e4chst wie die Lilie,", "tokens": ["Die", "nichts", "tut", "und", "w\u00e4chst", "wie", "die", "Li\u00b7lie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "KON", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Den Sohn aus der guten Familie!", "tokens": ["Den", "Sohn", "aus", "der", "gu\u00b7ten", "Fa\u00b7mi\u00b7lie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}