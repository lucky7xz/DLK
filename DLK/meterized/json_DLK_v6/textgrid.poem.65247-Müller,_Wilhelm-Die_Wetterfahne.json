{"textgrid.poem.65247": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Wetterfahne", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Wind spielt mit der Wetterfahne", "tokens": ["Der", "Wind", "spielt", "mit", "der", "Wet\u00b7ter\u00b7fah\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf meines sch\u00f6nen Liebchens Haus.", "tokens": ["Auf", "mei\u00b7nes", "sch\u00f6\u00b7nen", "Lieb\u00b7chens", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da dacht' ich schon in meinem Wahne,", "tokens": ["Da", "dacht'", "ich", "schon", "in", "mei\u00b7nem", "Wah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie pfiff' den armen Fl\u00fcchtling aus.", "tokens": ["Sie", "pfiff'", "den", "ar\u00b7men", "Fl\u00fccht\u00b7ling", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er h\u00e4tt' es ehr bemerken sollen,", "tokens": ["Er", "h\u00e4tt'", "es", "ehr", "be\u00b7mer\u00b7ken", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Hauses aufgestecktes Schild,", "tokens": ["Des", "Hau\u00b7ses", "auf\u00b7ge\u00b7steck\u00b7tes", "Schild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So h\u00e4tt' er nimmer suchen wollen", "tokens": ["So", "h\u00e4tt'", "er", "nim\u00b7mer", "su\u00b7chen", "wol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Haus ein treues Frauenbild.", "tokens": ["Im", "Haus", "ein", "treu\u00b7es", "Frau\u00b7en\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Wind spielt drinnen mit den Herzen,", "tokens": ["Der", "Wind", "spielt", "drin\u00b7nen", "mit", "den", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie auf dem Dach, nur nicht so laut.", "tokens": ["Wie", "auf", "dem", "Dach", ",", "nur", "nicht", "so", "laut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "ADV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was fragen sie nach meinen Schmerzen?", "tokens": ["Was", "fra\u00b7gen", "sie", "nach", "mei\u00b7nen", "Schmer\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Kind ist eine reiche Braut.", "tokens": ["Ihr", "Kind", "ist", "ei\u00b7ne", "rei\u00b7che", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}