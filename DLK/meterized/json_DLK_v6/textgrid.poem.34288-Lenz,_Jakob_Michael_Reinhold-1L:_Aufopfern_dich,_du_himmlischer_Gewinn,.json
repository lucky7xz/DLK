{"textgrid.poem.34288": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aufopfern dich, du himmlischer Gewinn,", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aufopfern dich, du himmlischer Gewinn,", "tokens": ["Auf\u00b7op\u00b7fern", "dich", ",", "du", "himm\u00b7li\u00b7scher", "Ge\u00b7winn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dich Engel! einer Bulerin? \u2013", "tokens": ["Dich", "En\u00b7gel", "!", "ei\u00b7ner", "Bu\u00b7le\u00b7rin", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "NE", "$.", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nein nimmer, nimmer! m\u00f6ge mit Err\u00f6then", "tokens": ["Nein", "nim\u00b7mer", ",", "nim\u00b7mer", "!", "m\u00f6\u00b7ge", "mit", "Er\u00b7r\u00f6\u00b7then"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "ADV", "$,", "ADV", "$.", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Schutzgeist auf mich niedersehn,", "tokens": ["Mein", "Schutz\u00b7geist", "auf", "mich", "nie\u00b7der\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mich verderben sehn in meinen N\u00f6then,", "tokens": ["Und", "mich", "ver\u00b7der\u00b7ben", "sehn", "in", "mei\u00b7nen", "N\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "F\u00e4nd ich dich jemals minder sch\u00f6n.", "tokens": ["F\u00e4nd", "ich", "dich", "je\u00b7mals", "min\u00b7der", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und will mein Herz f\u00fcr andern Reiz entbrennen,", "tokens": ["Und", "will", "mein", "Herz", "f\u00fcr", "an\u00b7dern", "Reiz", "ent\u00b7bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "So st\u00fcrm die Leidenschaft wie heut die Larv' ihr ab", "tokens": ["So", "st\u00fcrm", "die", "Lei\u00b7den\u00b7schaft", "wie", "heut", "die", "La\u00b7rv'", "ihr", "ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "KOKOM", "ADV", "ART", "NN", "PPER", "PTKVZ"], "meter": "-+-+---+----+", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Und sto\u00df mich einen ", "tokens": ["Und", "sto\u00df", "mich", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART"], "meter": "-+-+-", "measure": "iambic.di"}}}}}