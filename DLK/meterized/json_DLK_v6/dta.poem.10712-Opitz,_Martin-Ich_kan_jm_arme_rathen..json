{"dta.poem.10712": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Ich kan jm arme rathen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.71", "sv:0.28"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Herr Gottfrid/ h\u00f6ret doch/ wie r\u00fchmet jhre thaten", "tokens": ["Herr", "Gott\u00b7frid", "/", "h\u00f6\u00b7ret", "doch", "/", "wie", "r\u00fch\u00b7met", "jhre", "tha\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$(", "VVFIN", "ADV", "$(", "PWAV", "VVFIN", "PPOSAT", "VVFIN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Jungfraw braut/ vnd spricht: ich kan jm arme Rahten.", "tokens": ["Die", "Jung\u00b7fraw", "braut", "/", "vnd", "spricht", ":", "ich", "kan", "jm", "ar\u00b7me", "Rah\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "KON", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geht rathet jhr jhr auch in ewren armen sein/", "tokens": ["Geht", "ra\u00b7thet", "jhr", "jhr", "auch", "in", "ew\u00b7ren", "ar\u00b7men", "sein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "PPER", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wird euch beiderseits gar wohl gerahten sein.", "tokens": ["So", "wird", "euch", "bei\u00b7der\u00b7seits", "gar", "wohl", "ge\u00b7rah\u00b7ten", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}