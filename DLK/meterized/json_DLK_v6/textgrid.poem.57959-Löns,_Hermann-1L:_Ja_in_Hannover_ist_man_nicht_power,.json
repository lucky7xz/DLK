{"textgrid.poem.57959": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja in Hannover ist man nicht power,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja in Hannover ist man nicht power,", "tokens": ["Ja", "in", "Han\u00b7no\u00b7ver", "ist", "man", "nicht", "po\u00b7wer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "NE", "VAFIN", "PIS", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da sch\u00e4tzt und ehrt man noch die Kunst,", "tokens": ["Da", "sch\u00e4tzt", "und", "ehrt", "man", "noch", "die", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PIS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da lohnt sogar sich schon die Lyrik,", "tokens": ["Da", "lohnt", "so\u00b7gar", "sich", "schon", "die", "Ly\u00b7rik", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PRF", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der Dichter dichtet nicht umsunst.", "tokens": ["Der", "Dich\u00b7ter", "dich\u00b7tet", "nicht", "um\u00b7sunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bist sechzig Jahr du dort geworden,", "tokens": ["Bist", "sech\u00b7zig", "Jahr", "du", "dort", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "NN", "PPER", "ADV", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dann, Dichter, wird dein Schicksal hold,", "tokens": ["Dann", ",", "Dich\u00b7ter", ",", "wird", "dein", "Schick\u00b7sal", "hold", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die st\u00e4dtischen Kollegien spenden", "tokens": ["Die", "st\u00e4d\u00b7ti\u00b7schen", "Kol\u00b7le\u00b7gi\u00b7en", "spen\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dir einen hohen Ehrensold.", "tokens": ["Dir", "ei\u00b7nen", "ho\u00b7hen", "Eh\u00b7ren\u00b7sold", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In bar und nicht in Naturalien,", "tokens": ["In", "bar", "und", "nicht", "in", "Na\u00b7tu\u00b7ra\u00b7li\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "PTKNEG", "APPR", "NE", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zum Beispiel Autos, ihn man zahlt,", "tokens": ["Zum", "Bei\u00b7spiel", "Au\u00b7tos", ",", "ihn", "man", "zahlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie andren Leuten; ist's ein Wunder,", "tokens": ["Wie", "an\u00b7dren", "Leu\u00b7ten", ";", "ist's", "ein", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$.", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df des Poeten Antlitz strahlt?", "tokens": ["Da\u00df", "des", "Po\u00b7et\u00b7en", "Ant\u00b7litz", "strahlt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der andre aber nimmt das Auto", "tokens": ["Der", "and\u00b7re", "a\u00b7ber", "nimmt", "das", "Au\u00b7to"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und denkt dabei in seinem Sinn:", "tokens": ["Und", "denkt", "da\u00b7bei", "in", "sei\u00b7nem", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbwas danke ich blo\u00df meinem Sch\u00f6pfer,", "tokens": ["\u00bb", "was", "dan\u00b7ke", "ich", "blo\u00df", "mei\u00b7nem", "Sch\u00f6p\u00b7fer", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich nicht auch ein Dichter bin!\u00ab", "tokens": ["Da\u00df", "ich", "nicht", "auch", "ein", "Dich\u00b7ter", "bin", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ART", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}