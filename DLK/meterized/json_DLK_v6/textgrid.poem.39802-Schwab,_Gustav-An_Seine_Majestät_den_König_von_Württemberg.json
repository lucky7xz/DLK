{"textgrid.poem.39802": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "An Seine Majest\u00e4t den K\u00f6nig von W\u00fcrttemberg", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst noch in den gro\u00dfen Alten,", "tokens": ["J\u00fcngst", "noch", "in", "den", "gro\u00b7\u00dfen", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deren Bl\u00e4tter wir entfalten,", "tokens": ["De\u00b7ren", "Bl\u00e4t\u00b7ter", "wir", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lasen wir von Freiheit viel,", "tokens": ["La\u00b7sen", "wir", "von", "Frei\u00b7heit", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von Gesetz und B\u00fcrgertugend,", "tokens": ["Von", "Ge\u00b7setz", "und", "B\u00fcr\u00b7ger\u00b7tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und von einer kr\u00e4ft'gen Jugend,", "tokens": ["Und", "von", "ei\u00b7ner", "kr\u00e4ft'\u00b7gen", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Welche rang nach solchem Ziel.", "tokens": ["Wel\u00b7che", "rang", "nach", "sol\u00b7chem", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da erz\u00e4hlten uns die V\u00e4ter,", "tokens": ["Da", "er\u00b7z\u00e4hl\u00b7ten", "uns", "die", "V\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du und Deines Volks Vertreter", "tokens": ["Du", "und", "Dei\u00b7nes", "Volks", "Ver\u00b7tre\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "KON", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seien eins zu gleichem Bund,", "tokens": ["Sei\u00b7en", "eins", "zu", "glei\u00b7chem", "Bund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wir, die Bl\u00fcte des Geschlechtes,", "tokens": ["Wir", ",", "die", "Bl\u00fc\u00b7te", "des", "Ge\u00b7schlech\u00b7tes", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sollen wurzeln in des Rechtes", "tokens": ["Sol\u00b7len", "wur\u00b7zeln", "in", "des", "Rech\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Neu von dir geschaffnem Grund.", "tokens": ["Neu", "von", "dir", "ge\u00b7schaff\u00b7nem", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Unsre jungen Herzen brannten,", "tokens": ["Uns\u00b7re", "jun\u00b7gen", "Her\u00b7zen", "brann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre Lippen jubelnd nannten,", "tokens": ["Uns\u00b7re", "Lip\u00b7pen", "ju\u00b7belnd", "nann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsre Augen suchten dich.", "tokens": ["Uns\u00b7re", "Au\u00b7gen", "such\u00b7ten", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich, nach der fernen Reise,", "tokens": ["End\u00b7lich", ",", "nach", "der", "fer\u00b7nen", "Rei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter deines Volkes Preise", "tokens": ["Un\u00b7ter", "dei\u00b7nes", "Vol\u00b7kes", "Prei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zeigt dein theures Antlitz sich.", "tokens": ["Zeigt", "dein", "theu\u00b7res", "Ant\u00b7litz", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Blick' auf deine j\u00fcngsten S\u00f6hne!", "tokens": ["Blick'", "auf", "dei\u00b7ne", "j\u00fcng\u00b7sten", "S\u00f6h\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der \u00e4ltern Br\u00fcder T\u00f6ne,", "tokens": ["In", "der", "\u00e4l\u00b7tern", "Br\u00fc\u00b7der", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vater, stimmen sie mit ein.", "tokens": ["Va\u00b7ter", ",", "stim\u00b7men", "sie", "mit", "ein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df sie jauchzen, denn sie ahnen,", "tokens": ["La\u00df", "sie", "jauch\u00b7zen", ",", "denn", "sie", "ah\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "KON", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was es hei\u00dft, die Unterthanen", "tokens": ["Was", "es", "hei\u00dft", ",", "die", "Un\u00b7ter\u00b7tha\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eines solchen F\u00fcrsten sein.", "tokens": ["Ei\u00b7nes", "sol\u00b7chen", "F\u00fcrs\u00b7ten", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PIAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}