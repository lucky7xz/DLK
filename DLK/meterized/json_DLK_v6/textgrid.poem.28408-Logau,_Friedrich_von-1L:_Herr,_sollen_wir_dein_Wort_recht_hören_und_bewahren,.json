{"textgrid.poem.28408": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr, sollen wir dein Wort recht h\u00f6ren und bewahren,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, sollen wir dein Wort recht h\u00f6ren und bewahren,", "tokens": ["Herr", ",", "sol\u00b7len", "wir", "dein", "Wort", "recht", "h\u00f6\u00b7ren", "und", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "PPOSAT", "NN", "ADJD", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O, so bewahr es auch f\u00fcr b\u00f6ser Geister Schaaren,", "tokens": ["O", ",", "so", "be\u00b7wahr", "es", "auch", "f\u00fcr", "b\u00f6\u00b7ser", "Geis\u00b7ter", "Schaa\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADJD", "PPER", "ADV", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als Feinden deines Reichs! erhalte, was wir halten,", "tokens": ["Als", "Fein\u00b7den", "dei\u00b7nes", "Reichs", "!", "er\u00b7hal\u00b7te", ",", "was", "wir", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "$.", "VVFIN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "La\u00df nicht das Wort von uns und uns vom Worte spalten!", "tokens": ["La\u00df", "nicht", "das", "Wort", "von", "uns", "und", "uns", "vom", "Wor\u00b7te", "spal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "ART", "NN", "APPR", "PPER", "KON", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}