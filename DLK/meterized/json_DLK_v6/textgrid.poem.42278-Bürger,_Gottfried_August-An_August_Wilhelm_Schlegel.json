{"textgrid.poem.42278": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "An August Wilhelm Schlegel", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kraft der Laute, die ich r\u00fchmlich schlug,", "tokens": ["Kraft", "der", "Lau\u00b7te", ",", "die", "ich", "r\u00fchm\u00b7lich", "schlug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Kraft der Zweige, die mein Haupt umwinden,", "tokens": ["Kraft", "der", "Zwei\u00b7ge", ",", "die", "mein", "Haupt", "um\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Darf ich dir ein hohes Wort verk\u00fcnden,", "tokens": ["Darf", "ich", "dir", "ein", "ho\u00b7hes", "Wort", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Das ich l\u00e4ngst in meinem Busen trug.", "tokens": ["Das", "ich", "l\u00e4ngst", "in", "mei\u00b7nem", "Bu\u00b7sen", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Junger Aar! Dein k\u00f6niglicher Flug", "tokens": ["Jun\u00b7ger", "Aar", "!", "Dein", "k\u00f6\u00b7nig\u00b7li\u00b7cher", "Flug"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wird den Druck der Wolken \u00fcberwinden,", "tokens": ["Wird", "den", "Druck", "der", "Wol\u00b7ken", "\u00fc\u00b7berw\u00b7in\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wird die Bahn zum Sonnentempel finden,", "tokens": ["Wird", "die", "Bahn", "zum", "Son\u00b7nen\u00b7tem\u00b7pel", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Oder Ph\u00f6bus Wort in mir ist Lug.", "tokens": ["O\u00b7der", "Ph\u00f6\u00b7bus", "Wort", "in", "mir", "ist", "Lug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "APPR", "PPER", "VAFIN", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6n und laut ist deines Fittichs T\u00f6nen,", "tokens": ["Sch\u00f6n", "und", "laut", "ist", "dei\u00b7nes", "Fit\u00b7tichs", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie das Erz, das zu Dodona klang,", "tokens": ["Wie", "das", "Erz", ",", "das", "zu", "Do\u00b7do\u00b7na", "klang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Leicht und stark dein Aufflug sonder Zwang.", "tokens": ["Leicht", "und", "stark", "dein", "Auf\u00b7flug", "son\u00b7der", "Zwang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Dich zum Dienst des Sonnengotts zu kr\u00f6nen,", "tokens": ["Dich", "zum", "Dienst", "des", "Son\u00b7nen\u00b7gotts", "zu", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hielt' ich nicht den eignen Kranz zu wert;", "tokens": ["Hielt'", "ich", "nicht", "den", "eig\u00b7nen", "Kranz", "zu", "wert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "PTKA", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch \u2013 dir ist ein besserer beschert.", "tokens": ["Doch", "\u2013", "dir", "ist", "ein", "bes\u00b7se\u00b7rer", "be\u00b7schert", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PPER", "VAFIN", "ART", "ADJD", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}