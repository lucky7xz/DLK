{"dta.poem.1406": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tauff- und Pathen-W\u00fcnsche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Des Menschen Hertze lebt im Blut/", "tokens": ["Des", "Men\u00b7schen", "Hert\u00b7ze", "lebt", "im", "Blut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Blutt h\u00e4lt uns kaum gezeugt umschlossen:", "tokens": ["Blutt", "h\u00e4lt", "uns", "kaum", "ge\u00b7zeugt", "um\u00b7schlos\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Menschen Seele k\u00f6mmt zu gutt/", "tokens": ["Des", "Men\u00b7schen", "See\u00b7le", "k\u00f6mmt", "zu", "gutt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Blutt/ das GOtt f\u00fcr uns vergossen.", "tokens": ["Das", "Blutt", "/", "das", "Gott", "f\u00fcr", "uns", "ver\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "$(", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In Christus Lieb- und Blutt-gef\u00e4rbten Wunden", "tokens": ["In", "Chris\u00b7tus", "Lie\u00b7b", "und", "Blut\u00b7tge\u00b7f\u00e4rb\u00b7ten", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "TRUNC", "KON", "NN", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wird unser Heyl und Seligkeit gefunden;", "tokens": ["Wird", "un\u00b7ser", "Heyl", "und", "Se\u00b7lig\u00b7keit", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der/ der sich selbsten f\u00fcrgestellt", "tokens": ["Der", "/", "der", "sich", "selbs\u00b7ten", "f\u00fcr\u00b7ge\u00b7stellt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$(", "PRELS", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Weg/ die Warheit und das Leben/", "tokens": ["Den", "Weg", "/", "die", "War\u00b7heit", "und", "das", "Le\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wird dich begleiten durch die Welt/", "tokens": ["Wird", "dich", "be\u00b7glei\u00b7ten", "durch", "die", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und nach der Welt gen Himmel heben:", "tokens": ["Und", "nach", "der", "Welt", "gen", "Him\u00b7mel", "he\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Di\u00df/ Pathe/ tr\u00f6ste dich in gutt und b\u00f6sen Stunden!", "tokens": ["Di\u00df", "/", "Pa\u00b7the", "/", "tr\u00f6s\u00b7te", "dich", "in", "gutt", "und", "b\u00f6\u00b7sen", "Stun\u00b7den", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$(", "NE", "$(", "VVFIN", "PRF", "APPR", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}