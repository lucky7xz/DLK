{"textgrid.poem.60989": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Das sterbende Kind", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie doch so still dir am Herzen", "tokens": ["Wie", "doch", "so", "still", "dir", "am", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "ADJD", "PPER", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ruhet das Kind!", "tokens": ["Ru\u00b7het", "das", "Kind", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wei\u00df nicht, wie Mutterschmerzen", "tokens": ["Wei\u00df", "nicht", ",", "wie", "Mut\u00b7ter\u00b7schmer\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So herbe sind.", "tokens": ["So", "her\u00b7be", "sind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Auf Stirn und Lippen und Wangen", "tokens": ["Auf", "Stirn", "und", "Lip\u00b7pen", "und", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ist schon vergangen", "tokens": ["Ist", "schon", "ver\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Das s\u00fc\u00dfe Rot;", "tokens": ["Das", "s\u00fc\u00b7\u00dfe", "Rot", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Und dennoch heimlicherweise", "tokens": ["Und", "den\u00b7noch", "heim\u00b7li\u00b7cher\u00b7wei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADJA"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "L\u00e4chelt es leise -", "tokens": ["L\u00e4\u00b7chelt", "es", "lei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Leise", "tokens": ["Lei\u00b7se"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "K\u00fcsset der Tod.", "tokens": ["K\u00fcs\u00b7set", "der", "Tod", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}