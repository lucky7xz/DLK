{"dta.poem.2644": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nach so viel Angst und Weh/ nach so geh\u00e4ufften Schmertz&#275;", "tokens": ["Nach", "so", "viel", "Angst", "und", "Weh", "/", "nach", "so", "ge\u00b7h\u00e4uff\u00b7ten", "Schmertz", "&#275;"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "XML_entity"], "pos": ["APPR", "ADV", "PIAT", "NN", "KON", "NN", "$(", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Kommt deine Friedenfarth/ ", "tokens": ["Kommt", "dei\u00b7ne", "Frie\u00b7den\u00b7farth", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du tr\u00e4gst wie Simeon den Heiland in dem Hertzen/", "tokens": ["Du", "tr\u00e4gst", "wie", "Si\u00b7me\u00b7on", "den", "Hei\u00b7land", "in", "dem", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NE", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df er auff solcher Bahn dir Licht und F\u00fchrer sey.", "tokens": ["Da\u00df", "er", "auff", "sol\u00b7cher", "Bahn", "dir", "Licht", "und", "F\u00fch\u00b7rer", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "PPER", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und solte nicht das Licht der ausgebreiten Heiden/", "tokens": ["Und", "sol\u00b7te", "nicht", "das", "Licht", "der", "aus\u00b7ge\u00b7brei\u00b7ten", "Hei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Glantz der Ewigkeit auch hier dein Leitstern stehn?", "tokens": ["Der", "Glantz", "der", "E\u00b7wig\u00b7keit", "auch", "hier", "dein", "Leits\u00b7tern", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df du mit Freuden kanst von dieser Welt abscheiden/", "tokens": ["Da\u00df", "du", "mit", "Freu\u00b7den", "kanst", "von", "die\u00b7ser", "Welt", "ab\u00b7schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VMFIN", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und durch das w\u00fcste Reich der Todes-Schatten gehn.", "tokens": ["Und", "durch", "das", "w\u00fcs\u00b7te", "Reich", "der", "To\u00b7des\u00b7Schat\u00b7ten", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der abgematte Grei\u00df stund gantz entzuckt in Wonne/", "tokens": ["Der", "ab\u00b7ge\u00b7mat\u00b7te", "Grei\u00df", "stund", "gantz", "ent\u00b7zuckt", "in", "Won\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als er das Kindlein sah/ den Herrscher aller Welt/", "tokens": ["Als", "er", "das", "Kin\u00b7dlein", "sah", "/", "den", "Herr\u00b7scher", "al\u00b7ler", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$(", "ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sein Glaubens-Aug\u2019 empfand die wahre Lebens-Sonne/", "tokens": ["Sein", "Glau\u00b7bens\u00b7Aug'", "emp\u00b7fand", "die", "wah\u00b7re", "Le\u00b7bens\u00b7Son\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die mit Gerechtigkeit und Trost ins Hertze f\u00e4llt.", "tokens": ["Die", "mit", "Ge\u00b7rech\u00b7tig\u00b7keit", "und", "Trost", "ins", "Hert\u00b7ze", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "APPRART", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So trug der Simeon den Himmel auff den Armen/", "tokens": ["So", "trug", "der", "Si\u00b7me\u00b7on", "den", "Him\u00b7mel", "auff", "den", "Ar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Weil/ wer an JEsum glaubt/ den Himmel finden kan/", "tokens": ["Weil", "/", "wer", "an", "Je\u00b7sum", "glaubt", "/", "den", "Him\u00b7mel", "fin\u00b7den", "kan", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "PWS", "APPR", "NE", "VVFIN", "$(", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und rieff: HErr nun ists Zeit/ ich preise dein Erbarmen/", "tokens": ["Und", "rieff", ":", "Herr", "nun", "ists", "Zeit", "/", "ich", "prei\u00b7se", "dein", "Er\u00b7bar\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NN", "ADV", "VAFIN", "NN", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Entla\u00df mich meines Diensts/ o GOtt/ es ist gethan.", "tokens": ["Ent\u00b7la\u00df", "mich", "mei\u00b7nes", "Diensts", "/", "o", "Gott", "/", "es", "ist", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$(", "FM", "NN", "$(", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Nun ich den Tag erlebt/ so wil ich gerne sterben/", "tokens": ["Nun", "ich", "den", "Tag", "er\u00b7lebt", "/", "so", "wil", "ich", "ger\u00b7ne", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "VVPP", "$(", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Rath zu Jerusalem/ Welt/ Ehre/ gute Nacht!", "tokens": ["Rath", "zu", "Je\u00b7ru\u00b7sa\u00b7lem", "/", "Welt", "/", "Eh\u00b7re", "/", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$(", "NN", "$(", "NN", "$(", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Mein w\u00fcnschen ist erf\u00fcllt/ ich kan den Himmel erben/", "tokens": ["Mein", "w\u00fcn\u00b7schen", "ist", "er\u00b7f\u00fcllt", "/", "ich", "kan", "den", "Him\u00b7mel", "er\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VAFIN", "VVPP", "$(", "PPER", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und mein Verlangen ist zum h\u00f6chsten Zweck gebracht.", "tokens": ["Und", "mein", "Ver\u00b7lan\u00b7gen", "ist", "zum", "h\u00f6chs\u00b7ten", "Zweck", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Auch du hast/ ", "tokens": ["Auch", "du", "hast", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.22": {"text": "Mit Seufftzen und mit Flehn umb Rettung angesucht:", "tokens": ["Mit", "Seufft\u00b7zen", "und", "mit", "Flehn", "umb", "Ret\u00b7tung", "an\u00b7ge\u00b7sucht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Nun; die Erl\u00f6sungs-Zeit kommt eben an dem Tage/", "tokens": ["Nun", ";", "die", "Er\u00b7l\u00f6\u00b7sungs\u00b7Zeit", "kommt", "e\u00b7ben", "an", "dem", "Ta\u00b7ge", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df du wie Simeon geneust des Glaubens Frucht.", "tokens": ["Da\u00df", "du", "wie", "Si\u00b7me\u00b7on", "ge\u00b7neust", "des", "Glau\u00b7bens", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "NE", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der hatte GOtt gedient/ und fuhr dahin in Friede/", "tokens": ["Der", "hat\u00b7te", "Gott", "ge\u00b7dient", "/", "und", "fuhr", "da\u00b7hin", "in", "Frie\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "VVPP", "$(", "KON", "VVFIN", "PAV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ein H\u00f6rer seines Worts/ als wie sein Name heist/", "tokens": ["Ein", "H\u00f6\u00b7rer", "sei\u00b7nes", "Worts", "/", "als", "wie", "sein", "Na\u00b7me", "heist", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "KOUS", "KOKOM", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Von Jahr und Kr\u00e4fften zwar/ doch nicht vom Glauben m\u00fcde", "tokens": ["Von", "Jahr", "und", "Kr\u00e4ff\u00b7ten", "zwar", "/", "doch", "nicht", "vom", "Glau\u00b7ben", "m\u00fc\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "$(", "ADV", "PTKNEG", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Als dessen Beystand war/ und blieb des HErren Geist.", "tokens": ["Als", "des\u00b7sen", "Beys\u00b7tand", "war", "/", "und", "blieb", "des", "Her\u00b7ren", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELAT", "NN", "VAFIN", "$(", "KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ach soll uns Sterblichen die letzte Fahrt gel\u00fccken/", "tokens": ["Ach", "soll", "uns", "Sterb\u00b7li\u00b7chen", "die", "letz\u00b7te", "Fahrt", "ge\u00b7l\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VMFIN", "PPER", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.30": {"text": "Soll man die Bitterkeit des Todes schmecken nicht/", "tokens": ["Soll", "man", "die", "Bit\u00b7ter\u00b7keit", "des", "To\u00b7des", "schme\u00b7cken", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "NN", "ART", "NN", "VVFIN", "PTKNEG", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.31": {"text": "So m\u00fcssen wir uns stets mit Glaub und Hoffnung schicken/", "tokens": ["So", "m\u00fcs\u00b7sen", "wir", "uns", "stets", "mit", "Glaub", "und", "Hoff\u00b7nung", "schi\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Versiegelt mit dem Geist/ da\u00df uns nie Krafft gebricht.", "tokens": ["Ver\u00b7sie\u00b7gelt", "mit", "dem", "Geist", "/", "da\u00df", "uns", "nie", "Krafft", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$(", "KOUS", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ach/ rieff der ", "tokens": ["Ach", "/", "rieff", "der"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$(", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.34": {"text": "So schrumpfft ihr Himmel ein/ brecht Felsen/ Erde rei\u00df!", "tokens": ["So", "schrumpfft", "ihr", "Him\u00b7mel", "ein", "/", "brecht", "Fel\u00b7sen", "/", "Er\u00b7de", "rei\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "$(", "VVFIN", "NN", "$(", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Mit rechter Hertzens-Lust eil\u2019 ich zu meinem Grabe/", "tokens": ["Mit", "rech\u00b7ter", "Hert\u00b7zens\u00b7Lust", "eil'", "ich", "zu", "mei\u00b7nem", "Gra\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da ich der Schmertzen Ziel/ der Freuden Anfang wei\u00df.", "tokens": ["Da", "ich", "der", "Schmert\u00b7zen", "Ziel", "/", "der", "Freu\u00b7den", "An\u00b7fang", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "$(", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Es sihet Simeon den Heiland mit den Augen/", "tokens": ["Es", "si\u00b7het", "Si\u00b7me\u00b7on", "den", "Hei\u00b7land", "mit", "den", "Au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Sein irrdisch Arm umbfast den/ dem die Welt zu klein/", "tokens": ["Sein", "irr\u00b7disch", "Arm", "umb\u00b7fast", "den", "/", "dem", "die", "Welt", "zu", "klein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "APPR", "ART", "$(", "ART", "ART", "NN", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Doch w\u00fcrde weder Sehn noch das Umbarmen taugen/", "tokens": ["Doch", "w\u00fcr\u00b7de", "we\u00b7der", "Sehn", "noch", "das", "Um\u00b7bar\u00b7men", "tau\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "KON", "VVFIN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wenn nicht des Glaubens-Arm ihn mehr geschlossen ein.", "tokens": ["Wenn", "nicht", "des", "Glau\u00b7bens\u00b7Arm", "ihn", "mehr", "ge\u00b7schlos\u00b7sen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "PPER", "ADV", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Des Glaubens rein Crystall zeigt\u2019 auch dem ", "tokens": ["Des", "Glau\u00b7bens", "rein", "Crys\u00b7tall", "zeigt'", "auch", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN", "VVFIN", "ADV", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Den Heiland/ welcher uns von S\u00fcnden hat befreyt/", "tokens": ["Den", "Hei\u00b7land", "/", "wel\u00b7cher", "uns", "von", "S\u00fcn\u00b