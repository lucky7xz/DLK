{"textgrid.poem.43744": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trauben, die ess' ich gern", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trauben, die ess' ich gern,", "tokens": ["Trau\u00b7ben", ",", "die", "ess'", "ich", "gern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PPER", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Das kannst du glauben,", "tokens": ["Das", "kannst", "du", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "S\u00fc\u00dfer als Mandelkern", "tokens": ["S\u00fc\u00b7\u00dfer", "als", "Man\u00b7del\u00b7kern"], "token_info": ["word", "word", "word"], "pos": ["NN", "KOUS", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Schmecken die Trauben.", "tokens": ["Schme\u00b7cken", "die", "Trau\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Trauben hol mir geschwind,", "tokens": ["Trau\u00b7ben", "hol", "mir", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Hole mir Trauben.", "tokens": ["Ho\u00b7le", "mir", "Trau\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Da\u00df sie gegessen sind,", "tokens": ["Da\u00df", "sie", "ge\u00b7ges\u00b7sen", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Kann ich nicht glauben.", "tokens": ["Kann", "ich", "nicht", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Mutter, an dem Spalier", "tokens": ["Mut\u00b7ter", ",", "an", "dem", "Spa\u00b7lier"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und an den Lauben,", "tokens": ["Und", "an", "den", "Lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ueberall, da und hier", "tokens": ["Ue\u00b7be\u00b7rall", ",", "da", "und", "hier"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "KON", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Giebt es noch Trauben.", "tokens": ["Giebt", "es", "noch", "Trau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Trauben, die ess' ich gern,", "tokens": ["Trau\u00b7ben", ",", "die", "ess'", "ich", "gern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PPER", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Das kannst du glauben,", "tokens": ["Das", "kannst", "du", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "S\u00fc\u00dfer als Mandelkern", "tokens": ["S\u00fc\u00b7\u00dfer", "als", "Man\u00b7del\u00b7kern"], "token_info": ["word", "word", "word"], "pos": ["NN", "KOUS", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Schmecken die Trauben.", "tokens": ["Schme\u00b7cken", "die", "Trau\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}