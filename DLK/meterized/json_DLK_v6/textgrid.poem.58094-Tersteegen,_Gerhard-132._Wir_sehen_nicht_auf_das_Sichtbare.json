{"textgrid.poem.58094": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "132. Wir sehen nicht auf das Sichtbare", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zieh Herz und Sinnen ab von dem, was mu\u00df verschwinden,", "tokens": ["Zieh", "Herz", "und", "Sin\u00b7nen", "ab", "von", "dem", ",", "was", "mu\u00df", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PTKVZ", "APPR", "ART", "$,", "PWS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sonst kannst du nie in Gott Lust, Ruh und Leben finden,", "tokens": ["Sonst", "kannst", "du", "nie", "in", "Gott", "Lust", ",", "Ruh", "und", "Le\u00b7ben", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "NN", "NN", "$,", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und leb in dieser Welt, als wenn du schon w\u00e4rst tot;", "tokens": ["Und", "leb", "in", "die\u00b7ser", "Welt", ",", "als", "wenn", "du", "schon", "w\u00e4rst", "tot", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN", "$,", "KOKOM", "KOUS", "PPER", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Tu es ", "tokens": ["Tu", "es"], "token_info": ["word", "word"], "pos": ["NE", "PPER"], "meter": "+-", "measure": "trochaic.single"}}}}}