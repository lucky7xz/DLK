{"textgrid.poem.25440": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kommt diese Dedication,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt diese Dedication,", "tokens": ["Kommt", "die\u00b7se", "De\u00b7di\u00b7ca\u00b7ti\u00b7on", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Schiffer ", "tokens": ["Durch", "Schif\u00b7fer"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Nach Wunsch, vor ", "tokens": ["Nach", "Wunsch", ",", "vor"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Und la\u00dft ", "tokens": ["Und", "la\u00dft"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "So seyd kein Kind, und denkt nicht gleich,", "tokens": ["So", "seyd", "kein", "Kind", ",", "und", "denkt", "nicht", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,", "KON", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df Deutschland, weil ein Deutscher ", "tokens": ["Da\u00df", "Deutschland", ",", "weil", "ein", "Deut\u00b7scher"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NE", "$,", "KOUS", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Besingt, gar sehr ", "tokens": ["Be\u00b7singt", ",", "gar", "sehr"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Zerbrecht ", "tokens": ["Zer\u00b7brecht"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Dar\u00fcber, wie ", "tokens": ["Da\u00b7r\u00fc\u00b7ber", ",", "wie"], "token_info": ["word", "punct", "word"], "pos": ["PAV", "$,", "PWAV"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Gekommen seyd? Wenn ", "tokens": ["Ge\u00b7kom\u00b7men", "seyd", "?", "Wenn"], "token_info": ["word", "word", "punct", "word"], "pos": ["VVPP", "VAFIN", "$.", "KOUS"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "(", "tokens": ["("], "token_info": ["punct"], "pos": ["$("]}, "line.12": {"text": "Wir ", "tokens": ["Wir"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.13": {"text": "Die Dichtkunst drischt bei uns nur Stroh,", "tokens": ["Die", "Dicht\u00b7kunst", "drischt", "bei", "uns", "nur", "Stroh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Die Kunst zu schmeicheln aber, Aehren.", "tokens": ["Die", "Kunst", "zu", "schmei\u00b7cheln", "a\u00b7ber", ",", "A\u00b7eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVFIN", "ADV", "$,", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Nun ist bei uns so der Gebrauch,", "tokens": ["Nun", "ist", "bei", "uns", "so", "der", "Ge\u00b7brauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Von Aehren, nicht von Stroh, zu leben.", "tokens": ["Von", "A\u00b7eh\u00b7ren", ",", "nicht", "von", "Stroh", ",", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PTKNEG", "APPR", "NE", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Drum lernen wir Poeten auch", "tokens": ["Drum", "ler\u00b7nen", "wir", "Po\u00b7et\u00b7en", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die Kunst, sie andern auszudreschen;", "tokens": ["Die", "Kunst", ",", "sie", "an\u00b7dern", "aus\u00b7zu\u00b7dre\u00b7schen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Das hei\u00dft: den Durst nach Schmeichelein", "tokens": ["Das", "hei\u00dft", ":", "den", "Durst", "nach", "Schmei\u00b7che\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$.", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "L\u00f6scht der Poet; den Durst nach Wein", "tokens": ["L\u00f6scht", "der", "Po\u00b7et", ";", "den", "Durst", "nach", "Wein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Mu\u00df ihm daf\u00fcr der Andre l\u00f6schen.", "tokens": ["Mu\u00df", "ihm", "da\u00b7f\u00fcr", "der", "And\u00b7re", "l\u00f6\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PAV", "ART", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Glaubt, ", "tokens": ["Glaubt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.23": {"text": "In unserm Welttheil', so besingen,", "tokens": ["In", "un\u00b7serm", "Welt\u00b7theil'", ",", "so", "be\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Als ich wohl k\u00f6nnt': er setzte gern", "tokens": ["Als", "ich", "wohl", "k\u00f6nnt'", ":", "er", "setz\u00b7te", "gern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Bei einer Arbeitfreien Stelle,", "tokens": ["Bei", "ei\u00b7ner", "Ar\u00b7beit\u00b7frei\u00b7en", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Auf seine Kosten, mich in Ruh',", "tokens": ["Auf", "sei\u00b7ne", "Kos\u00b7ten", ",", "mich", "in", "Ruh'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Denn jeder hat ein Haus dazu;", "tokens": ["Denn", "je\u00b7der", "hat", "ein", "Haus", "da\u00b7zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Man nennt es: Eine Zitadelle.", "tokens": ["Man", "nennt", "es", ":", "Ei\u00b7ne", "Zi\u00b7ta\u00b7del\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$.", "ART", "NN", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.29": {"text": "Und doch besang ich niemals sie.", "tokens": ["Und", "doch", "be\u00b7sang", "ich", "nie\u00b7mals", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Warum? das kann ", "tokens": ["Wa\u00b7rum", "?", "das", "kann"], "token_info": ["word", "punct", "word", "word"], "pos": ["PWAV", "$.", "PDS", "VMFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.31": {"text": "Doch w\u00e4r't ", "tokens": ["Doch", "w\u00e4r't"], "token_info": ["word", "word"], "pos": ["KON", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.32": {"text": "Geruhtet ", "tokens": ["Ge\u00b7ruh\u00b7tet"], "token_info": ["word"], "pos": ["VVPP"], "meter": "-+-", "measure": "amphibrach.single"}, "line.33": {"text": "Ich legt' ", "tokens": ["Ich", "legt'"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.34": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.35": {"text": "Der dem Verdienst' geb\u00fchrt, zu F\u00fc\u00dfen.", "tokens": ["Der", "dem", "Ver\u00b7dienst'", "ge\u00b7b\u00fchrt", ",", "zu", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Das thut nichts! daf\u00fcr seyd ", "tokens": ["Das", "thut", "nichts", "!", "da\u00b7f\u00fcr", "seyd"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "PIS", "$.", "PAV", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Wenn mir es sonst einmal gef\u00e4llt,", "tokens": ["Wenn", "mir", "es", "sonst", "ein\u00b7mal", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Wer darf in ", "tokens": ["Wer", "darf", "in"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VMFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.39": {"text": "Wie l\u00fcgt der Schurke!\u00ab Niemand ja", "tokens": ["Wie", "l\u00fcgt", "der", "Schur\u00b7ke", "!", "\u00ab", "Nie\u00b7mand", "ja"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$.", "$(", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "L\u00e4\u00dft gern sich schinden oder br\u00fchen!", "tokens": ["L\u00e4\u00dft", "gern", "sich", "schin\u00b7den", "o\u00b7der", "br\u00fc\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PRF", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Ich aber, ", "tokens": ["Ich", "a\u00b7ber", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.42": {"text": "Auf gutes Gl\u00fcck zu creditiren.", "tokens": ["Auf", "gu\u00b7tes", "Gl\u00fcck", "zu", "cre\u00b7di\u00b7ti\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Nein! Zug um Zug! ", "tokens": ["Nein", "!", "Zug", "um", "Zug", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "NN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.44": {"text": "Mir diese Zuschrift baar verg\u00fcten,", "tokens": ["Mir", "die\u00b7se", "Zu\u00b7schrift", "baar", "ver\u00b7g\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Sonst wird sich ihr Verfasser schier", "tokens": ["Sonst", "wird", "sich", "ihr", "Ver\u00b7fas\u00b7ser", "schier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Vor ", "tokens": ["Vor"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.47": {"text": "Vor einer M\u00fctzen-Schlange", "tokens": ["Vor", "ei\u00b7ner", "M\u00fct\u00b7zen\u00b7Schlan\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.48": {"text": "Es thut vielleicht ", "tokens": ["Es", "thut", "viel\u00b7leicht"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.49": {"text": "Ob ", "tokens": ["Ob"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.50": {"text": "Doch wenn ", "tokens": ["Doch", "wenn"], "token_info": ["word", "word"], "pos": ["KON", "KOUS"], "meter": "-+", "measure": "iambic.single"}, "line.51": {"text": "Mu\u00df f\u00fcr den Ruhm bei Nationen,", "tokens": ["Mu\u00df", "f\u00fcr", "den", "Ruhm", "bei", "Na\u00b7ti\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Wo jeder Bettler, ungescheut,", "tokens": ["Wo", "je\u00b7der", "Bett\u00b7ler", ",", "un\u00b7ge\u00b7scheut", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Doch wohl ein Wunsch noch in ", "tokens": ["Doch", "wohl", "ein", "Wunsch", "noch", "in"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ADV", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.54": {"text": "Soll ich den Wunsch erf\u00fcllen? Top!", "tokens": ["Soll", "ich", "den", "Wunsch", "er\u00b7f\u00fcl\u00b7len", "?", "Top", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$.", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Schickt mir nur einen Elephanten!", "tokens": ["Schickt", "mir", "nur", "ei\u00b7nen", "E\u00b7le\u00b7phan\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.56": {"text": "F\u00fcr ", "tokens": ["F\u00fcr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.57": {"text": "Weil ich's entbehren lernte, danken;", "tokens": ["Weil", "ich's", "ent\u00b7beh\u00b7ren", "lern\u00b7te", ",", "dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.58": {"text": "Allein ein Thier zum Reiten, kann", "tokens": ["Al\u00b7lein", "ein", "Thier", "zum", "Rei\u00b7ten", ",", "kann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$,", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.59": {"text": "Mir Dienste thun; ich kranker Mann", "tokens": ["Mir", "Diens\u00b7te", "thun", ";", "ich", "kran\u00b7ker", "Mann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "VVINF", "$.", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.60": {"text": "Fang' etwas fr\u00fch schon an zu wanken.", "tokens": ["Fang'", "et\u00b7was", "fr\u00fch", "schon", "an", "zu", "wan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "ADV", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.61": {"text": "Der Herr Professor ", "tokens": ["Der", "Herr", "Pro\u00b7fes\u00b7sor"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.62": {"text": "Zwar ausgelobt, doch ", "tokens": ["Zwar", "aus\u00b7ge\u00b7lobt", ",", "doch"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "VVPP", "$,", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.63": {"text": "Schreibt ", "tokens": ["Schreibt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.64": {"text": "Wenn ich das Reitthier ihm vermache;", "tokens": ["Wenn", "ich", "das", "Reit\u00b7thier", "ihm", "ver\u00b7ma\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.65": {"text": "Denn seyd ", "tokens": ["Denn", "seyd"], "token_info": ["word", "word"], "pos": ["KON", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.66": {"text": "Auch nach dem Namen unbekannt,", "tokens": ["Auch", "nach", "dem", "Na\u00b7men", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.67": {"text": "So thut das eben nichts zur Sache.", "tokens": ["So", "thut", "das", "e\u00b7ben", "nichts", "zur", "Sa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADV", "PIS", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}