{"dta.poem.6507": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Licht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man dencke nicht, da\u00df sie durch so viel Zeichen rennt,", "tokens": ["Man", "den\u00b7cke", "nicht", ",", "da\u00df", "sie", "durch", "so", "viel", "Zei\u00b7chen", "rennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "APPR", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und glaube sicherlich, ihr Sitz sey fest gestellt", "tokens": ["Und", "glau\u00b7be", "si\u00b7cher\u00b7lich", ",", "ihr", "Sitz", "sey", "fest", "ge\u00b7stellt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jm Mittelpunct der Welt,", "tokens": ["Jm", "Mit\u00b7tel\u00b7punct", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Formirt aus C\u00f6rperchen, die zart und rein,", "tokens": ["For\u00b7mirt", "aus", "C\u00f6r\u00b7per\u00b7chen", ",", "die", "zart", "und", "rein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Und sehr beweglich seyn,", "tokens": ["Und", "sehr", "be\u00b7weg\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ein rund Gef\u00e4\u00df, das voll vom ersten Element,", "tokens": ["Ein", "rund", "Ge\u00b7f\u00e4\u00df", ",", "das", "voll", "vom", "ers\u00b7ten", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "PRELS", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}