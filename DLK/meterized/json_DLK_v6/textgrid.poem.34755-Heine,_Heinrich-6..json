{"textgrid.poem.34755": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie floh vor mir wie 'n Reh so scheu,", "tokens": ["Sie", "floh", "vor", "mir", "wie", "'n", "Reh", "so", "scheu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "KOKOM", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und wie ein Reh geschwinde!", "tokens": ["Und", "wie", "ein", "Reh", "ge\u00b7schwin\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie kletterte von Klipp' zu Klipp',", "tokens": ["Sie", "klet\u00b7ter\u00b7te", "von", "Klipp'", "zu", "Klipp'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Haar, das flog im Winde.", "tokens": ["Ihr", "Haar", ",", "das", "flog", "im", "Win\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo sich zum Meer der Felsen senkt,", "tokens": ["Wo", "sich", "zum", "Meer", "der", "Fel\u00b7sen", "senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da hab ich sie erreichet,", "tokens": ["Da", "hab", "ich", "sie", "er\u00b7rei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da hab ich sanft mit sanftem Wort", "tokens": ["Da", "hab", "ich", "sanft", "mit", "sanf\u00b7tem", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr spr\u00f6des Herz erweichet.", "tokens": ["Ihr", "spr\u00f6\u00b7des", "Herz", "er\u00b7wei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hier sa\u00dfen wir so himmelhoch,", "tokens": ["Hier", "sa\u00b7\u00dfen", "wir", "so", "him\u00b7mel\u00b7hoch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und auch so himmelselig;", "tokens": ["Und", "auch", "so", "him\u00b7mel\u00b7se\u00b7lig", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Tief unter uns, ins dunkle Meer,", "tokens": ["Tief", "un\u00b7ter", "uns", ",", "ins", "dunk\u00b7le", "Meer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sonne sank allm\u00e4hlich.", "tokens": ["Die", "Son\u00b7ne", "sank", "all\u00b7m\u00e4h\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Tief unter uns, ins dunkle Meer,", "tokens": ["Tief", "un\u00b7ter", "uns", ",", "ins", "dunk\u00b7le", "Meer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Versank die sch\u00f6ne Sonne;", "tokens": ["Ver\u00b7sank", "die", "sch\u00f6\u00b7ne", "Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Wogen rauschten dr\u00fcber hin,", "tokens": ["Die", "Wo\u00b7gen", "rauschten", "dr\u00fc\u00b7ber", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit ungest\u00fcmer Wonne.", "tokens": ["Mit", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O weine nicht, die Sonne liegt", "tokens": ["O", "wei\u00b7ne", "nicht", ",", "die", "Son\u00b7ne", "liegt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht tot in jenen Fluten;", "tokens": ["Nicht", "tot", "in", "je\u00b7nen", "Flu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat sich in mein Herz versteckt", "tokens": ["Sie", "hat", "sich", "in", "mein", "Herz", "ver\u00b7steckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit allen ihren Gluten.", "tokens": ["Mit", "al\u00b7len", "ih\u00b7ren", "Glu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}