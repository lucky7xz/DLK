{"dta.poem.934": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N ACH   D ER   L ESE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Wir stehen an der hecken gradem wall               ", "tokens": ["Wir", "ste\u00b7hen", "an", "der", "he\u00b7cken", "gra\u00b7dem", "wall"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In reihen kommen kinder mit der nonne               ", "tokens": ["In", "rei\u00b7hen", "kom\u00b7men", "kin\u00b7der", "mit", "der", "non\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie singen lieder von der himmelswonne               ", "tokens": ["Sie", "sin\u00b7gen", "lie\u00b7der", "von", "der", "him\u00b7mels\u00b7won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In dieser erde sichrem klarem hall             ", "tokens": ["In", "die\u00b7ser", "er\u00b7de", "sich\u00b7rem", "kla\u00b7rem", "hall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die wir uns in der abendneige sonnten               ", "tokens": ["Die", "wir", "uns", "in", "der", "a\u00b7ben\u00b7dnei\u00b7ge", "sonn\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Uns schreckten deine worte und du meinst               ", "tokens": ["Uns", "schreck\u00b7ten", "dei\u00b7ne", "wor\u00b7te", "und", "du", "meinst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir waren gl&#252;cklich bloss solang wir einst               ", "tokens": ["Wir", "wa\u00b7ren", "gl", "&#252;", "ck\u00b7lich", "bloss", "so\u00b7lang", "wir", "einst"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$(", "ADJD", "ADV", "VMFIN", "PPER", "ADV"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nicht diese hecken &#252;berschauen konnten.             ", "tokens": ["Nicht", "die\u00b7se", "he\u00b7cken", "&#252;", "ber\u00b7schau\u00b7en", "konn\u00b7ten", "."], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["PTKNEG", "PDS", "VVINF", "$(", "VVINF", "VMFIN", "$."], "meter": "+-----+-+-", "measure": "dactylic.init"}}}}}