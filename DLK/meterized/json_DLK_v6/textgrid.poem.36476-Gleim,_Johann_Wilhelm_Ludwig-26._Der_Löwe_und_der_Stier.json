{"textgrid.poem.36476": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "26. Der L\u00f6we und der Stier", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein L\u00f6we wurde wild.", "tokens": ["Ein", "L\u00f6\u00b7we", "wur\u00b7de", "wild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ha! wie er br\u00fcllt!", "tokens": ["Ha", "!", "wie", "er", "br\u00fcllt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "PPER", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Rief's in dem Wald umher! Wehrlose Tiere nahmen", "tokens": ["Rie\u00b7f's", "in", "dem", "Wald", "um\u00b7her", "!", "Wehr\u00b7lo\u00b7se", "Tie\u00b7re", "nah\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "PTKVZ", "$.", "NN", "NN", "VVFIN"], "meter": "+---+-++--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Die Flucht bei Zeiten, und entkamen;", "tokens": ["Die", "Flucht", "bei", "Zei\u00b7ten", ",", "und", "ent\u00b7ka\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wehrhafte stellten sich in Haufen,", "tokens": ["Wehr\u00b7haf\u00b7te", "stell\u00b7ten", "sich", "in", "Hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu stehn f\u00fcr einen Mann;", "tokens": ["Zu", "stehn", "f\u00fcr", "ei\u00b7nen", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der wilde L\u00f6we kam gelaufen,", "tokens": ["Der", "wil\u00b7de", "L\u00f6\u00b7we", "kam", "ge\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und sah die Haufen an.", "tokens": ["Und", "sah", "die", "Hau\u00b7fen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was willst du? Fragt' ein Stier,", "tokens": ["Was", "willst", "du", "?", "Fragt'", "ein", "Stier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$.", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wir nehmens auf mit dir!", "tokens": ["Wir", "neh\u00b7mens", "auf", "mit", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mit Einem Alle! spricht der L\u00f6we zornig, gehet", "tokens": ["Mit", "Ei\u00b7nem", "Al\u00b7le", "!", "spricht", "der", "L\u00f6\u00b7we", "zor\u00b7nig", ",", "ge\u00b7het"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "PIAT", "$.", "VVFIN", "ART", "NE", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr all' in euren Stall;", "tokens": ["Ihr", "all'", "in", "eu\u00b7ren", "Stall", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und la\u00dft mir diesen Einen; sehet,", "tokens": ["Und", "la\u00dft", "mir", "die\u00b7sen", "Ei\u00b7nen", ";", "se\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PDAT", "NN", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist der Fall:", "tokens": ["Das", "ist", "der", "Fall", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Er ist ein Held, er messe sich!", "tokens": ["Er", "ist", "ein", "Held", ",", "er", "mes\u00b7se", "sich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hum! sprach der Stier, sein Diener!", "tokens": ["Hum", "!", "sprach", "der", "Stier", ",", "sein", "Die\u00b7ner", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Man wird durch gute Hilfe k\u00fchner;", "tokens": ["Man", "wird", "durch", "gu\u00b7te", "Hil\u00b7fe", "k\u00fch\u00b7ner", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich komm ihm nicht, er fr\u00e4\u00dfe mich!", "tokens": ["Ich", "komm", "ihm", "nicht", ",", "er", "fr\u00e4\u00b7\u00dfe", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}