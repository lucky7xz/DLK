{"textgrid.poem.44834": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nur halb zu wissen ist, man wei\u00df, bedenklich.", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur halb zu wissen ist, man wei\u00df, bedenklich.", "tokens": ["Nur", "halb", "zu", "wis\u00b7sen", "ist", ",", "man", "wei\u00df", ",", "be\u00b7denk\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "VAFIN", "$,", "PIS", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch wer die eine H\u00e4lfte kennt von einem Ganzen,", "tokens": ["Doch", "wer", "die", "ei\u00b7ne", "H\u00e4lf\u00b7te", "kennt", "von", "ei\u00b7nem", "Gan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das einig ist und eins, kennt auch die zweite.", "tokens": ["Das", "ei\u00b7nig", "ist", "und", "eins", ",", "kennt", "auch", "die", "zwei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VAFIN", "KON", "PIS", "$,", "VVFIN", "ADV", "ART", "ADJA", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nun hab ich den in meiner Brust erkannt,", "tokens": ["Nun", "hab", "ich", "den", "in", "mei\u00b7ner", "Brust", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von dem du bist die eine selge H\u00e4lfte;", "tokens": ["Von", "dem", "du", "bist", "die", "ei\u00b7ne", "sel\u00b7ge", "H\u00e4lf\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VAFIN", "ART", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und darum \u2013 schein ich gleich nur halb berechtigt \u2013", "tokens": ["Und", "da\u00b7rum", "\u2013", "schein", "ich", "gleich", "nur", "halb", "be\u00b7rech\u00b7tigt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "$(", "VVIMP", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Erk\u00fchn ich mich, dich zu verehren ganz.", "tokens": ["Er\u00b7k\u00fchn", "ich", "mich", ",", "dich", "zu", "ver\u00b7eh\u00b7ren", "ganz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "PPER", "PTKZU", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}