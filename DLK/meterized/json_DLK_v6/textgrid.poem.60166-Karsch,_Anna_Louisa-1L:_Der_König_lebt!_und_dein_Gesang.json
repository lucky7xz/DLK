{"textgrid.poem.60166": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der K\u00f6nig lebt! und dein Gesang", "genre": "verse", "period": "N.A.", "pub_year": 1760, "urn": "N.A.", "language": ["de:0.85", "cy:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig lebt! und dein Gesang", "tokens": ["Der", "K\u00f6\u00b7nig", "lebt", "!", "und", "dein", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Glogau! soll er seyn:", "tokens": ["O", "Glo\u00b7gau", "!", "soll", "er", "seyn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dich schlo\u00df er, nah am Untergang", "tokens": ["Dich", "schlo\u00df", "er", ",", "nah", "am", "Un\u00b7ter\u00b7gang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Arm des Schutzes ein.", "tokens": ["Im", "Arm", "des", "Schut\u00b7zes", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Schon zeichnete um dich der Krieg", "tokens": ["Schon", "zeich\u00b7ne\u00b7te", "um", "dich", "der", "Krieg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Flammen seine Spur,", "tokens": ["Mit", "Flam\u00b7men", "sei\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn der Rauch gen Himmel stieg,", "tokens": ["Und", "wenn", "der", "Rauch", "gen", "Him\u00b7mel", "stieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erstaunte die Natur.", "tokens": ["Er\u00b7staun\u00b7te", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mitleidig sah dein Blick umher.", "tokens": ["Mit\u00b7lei\u00b7dig", "sah", "dein", "Blick", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Fl\u00fcchtling schreckte dich;", "tokens": ["Der", "Fl\u00fccht\u00b7ling", "schreck\u00b7te", "dich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und brausend wie das wilde Meer,", "tokens": ["Und", "brau\u00b7send", "wie", "das", "wil\u00b7de", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wie\u00df der Russe sich.", "tokens": ["So", "wie\u00df", "der", "Rus\u00b7se", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dein Schutz-Geist kam, und Friedrich trat", "tokens": ["Dein", "Schutz\u00b7Geist", "kam", ",", "und", "Fried\u00b7rich", "trat"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KON", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Feinden vor den Schritt;", "tokens": ["Den", "Fein\u00b7den", "vor", "den", "Schritt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zur\u00fccke taumelte ihr Rath,", "tokens": ["Zu\u00b7r\u00fc\u00b7cke", "tau\u00b7mel\u00b7te", "ihr", "Rath", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihre Wuth gieng mit.", "tokens": ["Und", "ih\u00b7re", "Wuth", "gieng", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein Schrecken redte durch das Heer;", "tokens": ["Ein", "Schre\u00b7cken", "red\u00b7te", "durch", "das", "Heer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Auge sah empor,", "tokens": ["Ihr", "Au\u00b7ge", "sah", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und von dem Himmel rollte schwer", "tokens": ["Und", "von", "dem", "Him\u00b7mel", "roll\u00b7te", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Donner in ihr Ohr!", "tokens": ["Der", "Don\u00b7ner", "in", "ihr", "Ohr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du aber froh und unber\u00fchrt", "tokens": ["Du", "a\u00b7ber", "froh", "und", "un\u00b7be\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von eines Feindes Hand,", "tokens": ["Von", "ei\u00b7nes", "Fein\u00b7des", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sahst die Gefahr zur\u00fcckgef\u00fchrt,", "tokens": ["Sahst", "die", "Ge\u00b7fahr", "zu\u00b7r\u00fcck\u00b7ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sicher ward das Land.", "tokens": ["Und", "si\u00b7cher", "ward", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sey ruhig, sey voll Zuversicht!", "tokens": ["Sey", "ru\u00b7hig", ",", "sey", "voll", "Zu\u00b7ver\u00b7sicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "VAFIN", "ADJD", "NN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Dein K\u00f6nig lebt, und schlie\u00dft", "tokens": ["Dein", "K\u00f6\u00b7nig", "lebt", ",", "und", "schlie\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den rauhen Feldzug eher nicht,", "tokens": ["Den", "rau\u00b7hen", "Feld\u00b7zug", "e\u00b7her", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis er noch Sieger ist!", "tokens": ["Bis", "er", "noch", "Sie\u00b7ger", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Nordwind und der Mangel bringt", "tokens": ["Der", "Nord\u00b7wind", "und", "der", "Man\u00b7gel", "bringt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Feind nicht aus dem Feld;", "tokens": ["Den", "Feind", "nicht", "aus", "dem", "Feld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch wenn ihn nicht der Winter zwingt:", "tokens": ["Doch", "wenn", "ihn", "nicht", "der", "Win\u00b7ter", "zwingt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So zwingt ihn unser Held.", "tokens": ["So", "zwingt", "ihn", "un\u00b7ser", "Held", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Er lebt! und in ihm lebt der Geist", "tokens": ["Er", "lebt", "!", "und", "in", "ihm", "lebt", "der", "Geist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "KON", "APPR", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der gro\u00df ist in der Schlacht,", "tokens": ["Der", "gro\u00df", "ist", "in", "der", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und, wenn das Gl\u00fcck sich ihm entrei\u00dft,", "tokens": ["Und", ",", "wenn", "das", "Gl\u00fcck", "sich", "ihm", "ent\u00b7rei\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "PRF", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Feind noch zittern macht!", "tokens": ["Den", "Feind", "noch", "zit\u00b7tern", "macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Er lebt! Sein Leben und sein Sieg", "tokens": ["Er", "lebt", "!", "Sein", "Le\u00b7ben", "und", "sein", "Sieg"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sey heute dein Gesang!", "tokens": ["Sey", "heu\u00b7te", "dein", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihm singe wer sein Lob verschwieg", "tokens": ["Ihm", "sin\u00b7ge", "wer", "sein", "Lob", "ver\u00b7schwieg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PWS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch schwerer Zeiten Zwang.", "tokens": ["Durch", "schwe\u00b7rer", "Zei\u00b7ten", "Zwang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ihm singe wer ein redlich Herz", "tokens": ["Ihm", "sin\u00b7ge", "wer", "ein", "red\u00b7lich", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PWS", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Busen klopfen h\u00f6rt,", "tokens": ["Im", "Bu\u00b7sen", "klop\u00b7fen", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wer mit fromm empfundnem Schmerz", "tokens": ["Und", "wer", "mit", "fromm", "emp\u00b7fund\u00b7nem", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sorgen Friedrichs ehrt.", "tokens": ["Die", "Sor\u00b7gen", "Fried\u00b7richs", "ehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Er sch\u00fctzt sein Volk, und opfert ganz", "tokens": ["Er", "sch\u00fctzt", "sein", "Volk", ",", "und", "op\u00b7fert", "ganz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Uns seine Ruhe auf.", "tokens": ["Uns", "sei\u00b7ne", "Ru\u00b7he", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gro\u00df ist Er; ewig sey sein Glanz", "tokens": ["Gro\u00df", "ist", "Er", ";", "e\u00b7wig", "sey", "sein", "Glanz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "$.", "ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lang sein Lebenslauf!", "tokens": ["Und", "lang", "sein", "Le\u00b7bens\u00b7lauf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}