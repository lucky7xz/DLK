{"textgrid.poem.46576": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich hab' in l\u00e4\u00df'gen Ohren]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' in l\u00e4\u00df'gen Ohren,", "tokens": ["Ich", "hab'", "in", "l\u00e4\u00df'\u00b7gen", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O der Verlust ist gro\u00df,", "tokens": ["O", "der", "Ver\u00b7lust", "ist", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wohl manches Wort verloren,", "tokens": ["Wohl", "man\u00b7ches", "Wort", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das dir vom Munde flo\u00df.", "tokens": ["Das", "dir", "vom", "Mun\u00b7de", "flo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es flo\u00df und quoll und rollte", "tokens": ["Es", "flo\u00df", "und", "quoll", "und", "roll\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auch immer klar und hell,", "tokens": ["Auch", "im\u00b7mer", "klar", "und", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich dachte nicht, es sollte", "tokens": ["Ich", "dach\u00b7te", "nicht", ",", "es", "soll\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Versiegen je der Quell.", "tokens": ["Ver\u00b7sie\u00b7gen", "je", "der", "Quell", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da h\u00f6rt' ich ohne H\u00f6ren,", "tokens": ["Da", "h\u00f6rt'", "ich", "oh\u00b7ne", "H\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Antwortet' ohne Wort,", "tokens": ["Ant\u00b7wor\u00b7tet'", "oh\u00b7ne", "Wort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Arbeitet' ohne St\u00f6ren,", "tokens": ["Ar\u00b7bei\u00b7tet'", "oh\u00b7ne", "St\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und du sprachst immer fort.", "tokens": ["Und", "du", "sprachst", "im\u00b7mer", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nur manchmal h\u00f6rt' ich sagen,", "tokens": ["Nur", "manch\u00b7mal", "h\u00f6rt'", "ich", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn ich's zu arg gemacht:", "tokens": ["Wenn", "ich's", "zu", "arg", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Mutter, auf mein Fragen", "tokens": ["O", "Mut\u00b7ter", ",", "auf", "mein", "Fra\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gibst du auch gar nicht Acht.", "tokens": ["Gibst", "du", "auch", "gar", "nicht", "Acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und hatt' ich Acht zu geben", "tokens": ["Und", "hatt'", "ich", "Acht", "zu", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "CARD", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf Andres als auf dich?", "tokens": ["Auf", "And\u00b7res", "als", "auf", "dich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "KOKOM", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein s\u00fc\u00dfgeschw\u00e4tzges Leben,", "tokens": ["Mein", "s\u00fc\u00df\u00b7ge\u00b7schw\u00e4tz\u00b7ges", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nun bist du stumm f\u00fcr mich.", "tokens": ["Nun", "bist", "du", "stumm", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "In Gold nun m\u00f6cht' ich fassen", "tokens": ["In", "Gold", "nun", "m\u00f6cht'", "ich", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auch jedes kleinste Wort,", "tokens": ["Auch", "je\u00b7des", "kleins\u00b7te", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das mir dein Mund gelassen", "tokens": ["Das", "mir", "dein", "Mund", "ge\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In der Erinnrung Hort.", "tokens": ["In", "der", "E\u00b7rinn\u00b7rung", "Hort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}}}}