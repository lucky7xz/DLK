{"dta.poem.16126": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "180.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Bild der Ewigkeit, die Schlange die im Reif", "tokens": ["Das", "Bild", "der", "E\u00b7wig\u00b7keit", ",", "die", "Schlan\u00b7ge", "die", "im", "Reif"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich kr\u00fcmmt, und mit dem Kopf sich bei\u00dfet in den Schweif,", "tokens": ["Sich", "kr\u00fcmmt", ",", "und", "mit", "dem", "Kopf", "sich", "bei\u00b7\u00dfet", "in", "den", "Schweif", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "PRF", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mich wunderts, wie sie nicht erkrankt und stirbt, verwundet", "tokens": ["Mich", "wun\u00b7derts", ",", "wie", "sie", "nicht", "er\u00b7krankt", "und", "stirbt", ",", "ver\u00b7wun\u00b7det"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "PTKNEG", "VVPP", "KON", "VVFIN", "$,", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom gift'gen Bi\u00df, von dem nichts auf der Welt gesundet.", "tokens": ["Vom", "gift'\u00b7gen", "Bi\u00df", ",", "von", "dem", "nichts", "auf", "der", "Welt", "ge\u00b7sun\u00b7det", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ART", "PIS", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie stirbt in Wahrheit auch in jedem Nu davon,", "tokens": ["Sie", "stirbt", "in", "Wahr\u00b7heit", "auch", "in", "je\u00b7dem", "Nu", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "APPR", "PIS", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ist in jedem Nu auch neu geboren schon.", "tokens": ["Doch", "ist", "in", "je\u00b7dem", "Nu", "auch", "neu", "ge\u00b7bo\u00b7ren", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PIS", "ADV", "ADV", "ADJD", "VVPP", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}