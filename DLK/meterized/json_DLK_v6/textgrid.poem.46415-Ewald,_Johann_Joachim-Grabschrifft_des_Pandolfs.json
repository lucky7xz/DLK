{"textgrid.poem.46415": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft des Pandolfs", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Kr\u00e4hen, Eulen, Geier, Raben,", "tokens": ["Ihr", "Kr\u00e4\u00b7hen", ",", "Eu\u00b7len", ",", "Gei\u00b7er", ",", "Ra\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NE", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Klagt hier und schreyt! hier liegt Pandolf begraben,", "tokens": ["Klagt", "hier", "und", "schreyt", "!", "hier", "liegt", "Pan\u00b7dolf", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "$.", "ADV", "VVFIN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der eurem Schnabel j\u00fcngst entgieng,", "tokens": ["Der", "eu\u00b7rem", "Schna\u00b7bel", "j\u00fcngst", "ent\u00b7gieng", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als er sich selbst erhieng.", "tokens": ["Als", "er", "sich", "selbst", "er\u00b7hieng", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}