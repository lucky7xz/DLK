{"textgrid.poem.54092": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Zuckerbrot und Peitsche", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun senkt sich auf die Fluren nieder", "tokens": ["Nun", "senkt", "sich", "auf", "die", "Flu\u00b7ren", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "der s\u00fc\u00dfe Kitsch mit Zucker-Ei.", "tokens": ["der", "s\u00fc\u00b7\u00dfe", "Kitsch", "mit", "Zu\u00b7cke\u00b7rEi", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun kommen alle, alle wieder:", "tokens": ["Nun", "kom\u00b7men", "al\u00b7le", ",", "al\u00b7le", "wie\u00b7der", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PIS", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "das Schubert-Lied, die Holz-Schalmei . . .", "tokens": ["das", "Schu\u00b7ber\u00b7tLied", ",", "die", "Holz\u00b7Schal\u00b7mei", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das B\u00fcrgertum erliegt der Wucht:", "tokens": ["Das", "B\u00fcr\u00b7ger\u00b7tum", "er\u00b7liegt", "der", "Wucht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Flucht, Flucht, Flucht.", "tokens": ["Flucht", ",", "Flucht", ",", "Flucht", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Sie wollen sich mit Kunst bet\u00e4uben,", "tokens": ["Sie", "wol\u00b7len", "sich", "mit", "Kunst", "be\u00b7t\u00e4u\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "sie wollen nur noch M\u00e4rchen sehn;", "tokens": ["sie", "wol\u00b7len", "nur", "noch", "M\u00e4r\u00b7chen", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "sie wollen ihre Welt zerst\u00e4uben", "tokens": ["sie", "wol\u00b7len", "ih\u00b7re", "Welt", "zer\u00b7st\u00e4u\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und neben der Epoche gehn.", "tokens": ["und", "ne\u00b7ben", "der", "E\u00b7po\u00b7che", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus Not und milit\u00e4rscher Zucht:", "tokens": ["Aus", "Not", "und", "mi\u00b7li\u00b7t\u00e4r\u00b7scher", "Zucht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Flucht, Flucht, Flucht.", "tokens": ["Flucht", ",", "Flucht", ",", "Flucht", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "So dichtet. Dichter: vom Atlantik,", "tokens": ["So", "dich\u00b7tet", ".", "Dich\u00b7ter", ":", "vom", "At\u00b7lan\u00b7tik", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "NN", "$.", "APPRART", "NN", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "von Rittern und von Liebesnacht!", "tokens": ["von", "Rit\u00b7tern", "und", "von", "Lie\u00b7bes\u00b7nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Her, blaue Blume der Romantik!", "tokens": ["Her", ",", "blau\u00b7e", "Blu\u00b7me", "der", "Ro\u00b7man\u00b7tik", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bber l\u00f6ste ihr die Br\u00fcnne sacht . . . \u00ab", "tokens": ["\u00bb", "er", "l\u00f6s\u00b7te", "ihr", "die", "Br\u00fcn\u00b7ne", "sacht", ".", ".", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$.", "$.", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das ist Neudeutschlands gr\u00fcne Frucht:", "tokens": ["Das", "ist", "Neu\u00b7deutschlands", "gr\u00fc\u00b7ne", "Frucht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Flucht, Flucht, Flucht.", "tokens": ["Flucht", ",", "Flucht", ",", "Flucht", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Wie ihr euch durch Musik entbl\u00f6\u00dftet!", "tokens": ["Wie", "ihr", "euch", "durch", "Mu\u00b7sik", "ent\u00b7bl\u00f6\u00df\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "In eurer Kunst ist keine Faust.", "tokens": ["In", "eu\u00b7rer", "Kunst", "ist", "kei\u00b7ne", "Faust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So habt ihr euch noch stets getr\u00f6stet,", "tokens": ["So", "habt", "ihr", "euch", "noch", "stets", "ge\u00b7tr\u00f6s\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "wenn \u00fcber euch die Peitsche saust.", "tokens": ["wenn", "\u00fc\u00b7ber", "euch", "die", "Peit\u00b7sche", "saust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr wollt zu h\u00f6hern Harmonien", "tokens": ["Ihr", "wollt", "zu", "h\u00f6\u00b7hern", "Har\u00b7mo\u00b7ni\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "fliehn, fliehn, fliehn.", "tokens": ["fliehn", ",", "fliehn", ",", "fliehn", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVINF", "$,", "VVINF", "$,", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Es hilft euch nichts. Geht ihr zu Grunde:", "tokens": ["Es", "hilft", "euch", "nichts", ".", "Geht", "ihr", "zu", "Grun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$.", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "man braucht euch nicht. Kein Platz bleibt leer.", "tokens": ["man", "braucht", "euch", "nicht", ".", "Kein", "Platz", "bleibt", "leer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "$.", "PIAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr winselt wie die feigen Hunde \u2013", "tokens": ["Ihr", "win\u00b7selt", "wie", "die", "fei\u00b7gen", "Hun\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "schiebt ab! Euch gibt es gar nicht mehr!", "tokens": ["schiebt", "ab", "!", "Euch", "gibt", "es", "gar", "nicht", "mehr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir andern aber wirken weit", "tokens": ["Wir", "an\u00b7dern", "a\u00b7ber", "wir\u00b7ken", "weit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PIS", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "in die Zeit!", "tokens": ["in", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "In die Zeit!", "tokens": ["In", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "In die Zeit!", "tokens": ["In", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}