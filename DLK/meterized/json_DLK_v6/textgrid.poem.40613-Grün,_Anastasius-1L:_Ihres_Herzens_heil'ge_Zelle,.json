{"textgrid.poem.40613": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihres Herzens heil'ge Zelle,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihres Herzens heil'ge Zelle,", "tokens": ["Ih\u00b7res", "Her\u00b7zens", "heil'\u00b7ge", "Zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihres Blickes lichter Stern,", "tokens": ["Ih\u00b7res", "Bli\u00b7ckes", "lich\u00b7ter", "Stern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihres Wortes muntre Welle,", "tokens": ["Ih\u00b7res", "Wor\u00b7tes", "mun\u00b7tre", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir verloren, fremd und fern!", "tokens": ["Mir", "ver\u00b7lo\u00b7ren", ",", "fremd", "und", "fern", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wi\u00dft ihr, wie jetzt dem zu Muthe,", "tokens": ["Wi\u00dft", "ihr", ",", "wie", "jetzt", "dem", "zu", "Mu\u00b7the", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADV", "ART", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der vom Nordpoleis umfa\u00dft,", "tokens": ["Der", "vom", "Nord\u00b7po\u00b7leis", "um\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einst doch unter Palmen ruhte", "tokens": ["Einst", "doch", "un\u00b7ter", "Pal\u00b7men", "ruh\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als des Tropenhimmels Gast?", "tokens": ["Als", "des", "Tro\u00b7pen\u00b7him\u00b7mels", "Gast", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "K\u00f6nnt ihr dessen Leid ermessen,", "tokens": ["K\u00f6nnt", "ihr", "des\u00b7sen", "Leid", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDS", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der jetzt lechzt im W\u00fcstensand,", "tokens": ["Der", "jetzt", "lechzt", "im", "W\u00fcs\u00b7ten\u00b7sand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einst an Quellen doch gesessen", "tokens": ["Einst", "an", "Quel\u00b7len", "doch", "ge\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem gr\u00fcnsten Alpenland?", "tokens": ["In", "dem", "gr\u00fcns\u00b7ten", "Al\u00b7pen\u00b7land", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "K\u00f6nnt ihr f\u00fchlen wie der Blinde,", "tokens": ["K\u00f6nnt", "ihr", "f\u00fch\u00b7len", "wie", "der", "Blin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den einst Gottes Welt entz\u00fcckt,", "tokens": ["Den", "einst", "Got\u00b7tes", "Welt", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die mittern\u00e4cht'ge Binde", "tokens": ["Wenn", "die", "mit\u00b7ter\u00b7n\u00e4cht'\u00b7ge", "Bin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jetzt sein lichtlos Auge dr\u00fcckt?", "tokens": ["Jetzt", "sein", "licht\u00b7los", "Au\u00b7ge", "dr\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Oder wie der T\u00f6ne Meister,", "tokens": ["O\u00b7der", "wie", "der", "T\u00f6\u00b7ne", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den einst Wohllaut nur umflo\u00df,", "tokens": ["Den", "einst", "Wohl\u00b7laut", "nur", "um\u00b7flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Als der t\u00fcckisch'ste der Geister", "tokens": ["Als", "der", "t\u00fc\u00b7ckisch'\u00b7ste", "der", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihm der Tonwelt Pforten schlo\u00df?", "tokens": ["Ihm", "der", "Ton\u00b7welt", "Pfor\u00b7ten", "schlo\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dann zu ahnen m\u00f6gt ihr w\u00e4hnen", "tokens": ["Dann", "zu", "ah\u00b7nen", "m\u00f6gt", "ihr", "w\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKZU", "VVINF", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Des verwaisten Herzens Leid,", "tokens": ["Des", "ver\u00b7wais\u00b7ten", "Her\u00b7zens", "Leid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sein Erinnern, trostlos Sehnen,", "tokens": ["Sein", "E\u00b7rin\u00b7nern", ",", "trost\u00b7los", "Seh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Seine Todeseinsamkeit.", "tokens": ["Sei\u00b7ne", "To\u00b7de\u00b7sein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Schlie\u00dft in Eine eh'rne Klammer", "tokens": ["Schlie\u00dft", "in", "Ei\u00b7ne", "eh'r\u00b7ne", "Klam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Allen Schmerz zusammen ein,", "tokens": ["Al\u00b7len", "Schmerz", "zu\u00b7sam\u00b7men", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Es erreicht nicht seinen Jammer,", "tokens": ["Es", "er\u00b7reicht", "nicht", "sei\u00b7nen", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Es umfa\u00dft nicht seine Pein.", "tokens": ["Es", "um\u00b7fa\u00dft", "nicht", "sei\u00b7ne", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}