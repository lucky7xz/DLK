{"textgrid.poem.42258": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Die Eine", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht selten h\u00fcpft, dem Finken gleich im Haine,", "tokens": ["Nicht", "sel\u00b7ten", "h\u00fcpft", ",", "dem", "Fin\u00b7ken", "gleich", "im", "Hai\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "$,", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Flattersinn mir keck vors Angesicht:", "tokens": ["Der", "Flat\u00b7ter\u00b7sinn", "mir", "keck", "vors", "An\u00b7ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbwarum, warum bist du denn so auf Eine,", "tokens": ["\u00bb", "wa\u00b7rum", ",", "wa\u00b7rum", "bist", "du", "denn", "so", "auf", "Ei\u00b7ne", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$,", "PWAV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "ART", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf Eine nur bei Tag und Nacht erpicht?", "tokens": ["Auf", "Ei\u00b7ne", "nur", "bei", "Tag", "und", "Nacht", "er\u00b7picht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ha! glaubst du denn, weil diese dir gebricht,", "tokens": ["Ha", "!", "glaubst", "du", "denn", ",", "weil", "die\u00b7se", "dir", "ge\u00b7bricht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PDAT", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df Liebe dich mit Keiner mehr vereine?", "tokens": ["Da\u00df", "Lie\u00b7be", "dich", "mit", "Kei\u00b7ner", "mehr", "ver\u00b7ei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "APPR", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Gram um sie beflort dein Augenlicht;", "tokens": ["Der", "Gram", "um", "sie", "be\u00b7flort", "dein", "Au\u00b7gen\u00b7licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und freilich gl\u00e4nzt durch diesen Flor dir Keine.", "tokens": ["Und", "frei\u00b7lich", "gl\u00e4nzt", "durch", "die\u00b7sen", "Flor", "dir", "Kei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PDAT", "NN", "PPER", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Welt ist gro\u00df, und in der gro\u00dfen Welt", "tokens": ["Die", "Welt", "ist", "gro\u00df", ",", "und", "in", "der", "gro\u00b7\u00dfen", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bl\u00fchn sch\u00f6n und s\u00fc\u00df viel M\u00e4dchen noch und Frauen.", "tokens": ["Bl\u00fchn", "sch\u00f6n", "und", "s\u00fc\u00df", "viel", "M\u00e4d\u00b7chen", "noch", "und", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "PIAT", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du kannst dich ja in manches Herz noch bauen.\u00ab \u2013", "tokens": ["Du", "kannst", "dich", "ja", "in", "man\u00b7ches", "Herz", "noch", "bau\u00b7en", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "ADV", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ach, alles wahr! Vom Rhein an bis zum Belt", "tokens": ["Ach", ",", "al\u00b7les", "wahr", "!", "Vom", "Rhein", "an", "bis", "zum", "Belt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PIS", "ADJD", "$.", "APPRART", "NE", "APPR", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bl\u00fcht Reiz genug auf allen deutschen Auen.", "tokens": ["Bl\u00fcht", "Reiz", "ge\u00b7nug", "auf", "al\u00b7len", "deut\u00b7schen", "Au\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was hilft es mir, dem Molly nur gef\u00e4llt?", "tokens": ["Was", "hilft", "es", "mir", ",", "dem", "Mol\u00b7ly", "nur", "ge\u00b7f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$,", "ART", "NE", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}