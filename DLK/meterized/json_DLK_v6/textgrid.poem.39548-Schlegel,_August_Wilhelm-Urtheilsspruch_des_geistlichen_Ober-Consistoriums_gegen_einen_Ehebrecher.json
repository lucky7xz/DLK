{"textgrid.poem.39548": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Urtheilsspruch des geistlichen Ober-Consistoriums gegen einen Ehebrecher", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast des N\u00e4chsten Weib getreten:", "tokens": ["Du", "hast", "des", "N\u00e4chs\u00b7ten", "Weib", "ge\u00b7tre\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du mu\u00dft die M\u00fchle treten nun.", "tokens": ["Du", "mu\u00dft", "die", "M\u00fch\u00b7le", "tre\u00b7ten", "nun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Kuckuck ist der Weg zum fremden Nest vertreten,", "tokens": ["Dem", "Ku\u00b7ckuck", "ist", "der", "Weg", "zum", "frem\u00b7den", "Nest", "ver\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und jeder Ehmann darf vor H\u00f6rnern sicher ruhn.", "tokens": ["Und", "je\u00b7der", "Eh\u00b7mann", "darf", "vor", "H\u00f6r\u00b7nern", "si\u00b7cher", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "APPR", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Ehebrecherin soll spinnen,", "tokens": ["Die", "E\u00b7heb\u00b7re\u00b7che\u00b7rin", "soll", "spin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und ehrbar sich ihr Brod gewinnen.", "tokens": ["Und", "ehr\u00b7bar", "sich", "ihr", "Brod", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}