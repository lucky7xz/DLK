{"dta.poem.7101": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf Erden ist der Menschen Lust", "tokens": ["Auf", "Er\u00b7den", "ist", "der", "Men\u00b7schen", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So eng\u2019 verschrenckt, da\u00df unsre Brust", "tokens": ["So", "eng'", "ver\u00b7schrenckt", ",", "da\u00df", "uns\u00b7re", "Brust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus einer Sache nur auf einmahl Freud\u2019 empfindet,", "tokens": ["Aus", "ei\u00b7ner", "Sa\u00b7che", "nur", "auf", "ein\u00b7mahl", "Freud'", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "APPR", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und solches nur auf kurtze Zeit:", "tokens": ["Und", "sol\u00b7ches", "nur", "auf", "kurt\u00b7ze", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dort aber wird die Seel\u2019, und zwar in Ewigkeit,", "tokens": ["Dort", "a\u00b7ber", "wird", "die", "Seel'", ",", "und", "zwar", "in", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "$,", "KON", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit einer tausendfachen Krafft,", "tokens": ["Mit", "ei\u00b7ner", "tau\u00b7send\u00b7fa\u00b7chen", "Krafft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit einer gleichsam nicht umschr\u00e4nckten Eigenschafft,", "tokens": ["Mit", "ei\u00b7ner", "gleich\u00b7sam", "nicht", "um\u00b7schr\u00e4nck\u00b7ten", "Ei\u00b7gen\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die G\u00f6ttlichen Gesch\u00f6pffe zu begreiffen,", "tokens": ["Die", "G\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7sch\u00f6pf\u00b7fe", "zu", "be\u00b7greif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die F\u00e4higkeit noch immer h\u00e4uffen.", "tokens": ["Die", "F\u00e4\u00b7hig\u00b7keit", "noch", "im\u00b7mer", "h\u00e4uf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Seel\u2019 ist ja schon hier geschickt sich auszubreiten,", "tokens": ["Die", "Seel'", "ist", "ja", "schon", "hier", "ge\u00b7schickt", "sich", "aus\u00b7zu\u00b7brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADV", "VVPP", "PRF", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "In unterschiednen F\u00e4higkeiten.", "tokens": ["In", "un\u00b7ter\u00b7schied\u00b7nen", "F\u00e4\u00b7hig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sie kan verlangen, lieben, sehn,", "tokens": ["Sie", "kan", "ver\u00b7lan\u00b7gen", ",", "lie\u00b7ben", ",", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Empfinden, froh seyn und verstehn:", "tokens": ["Emp\u00b7fin\u00b7den", ",", "froh", "seyn", "und", "ver\u00b7stehn", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VAINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Was sie nun einzeln hier und nicht auf einmahl kann,", "tokens": ["Was", "sie", "nun", "ein\u00b7zeln", "hier", "und", "nicht", "auf", "ein\u00b7mahl", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "ADV", "KON", "PTKNEG", "APPR", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Das wird vermuthlich dort vereint, verbunden,", "tokens": ["Das", "wird", "ver\u00b7muth\u00b7lich", "dort", "ver\u00b7eint", ",", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "In vollenkomm\u2019ner Lust, zugleich von ihr empfunden.", "tokens": ["In", "vol\u00b7len\u00b7kom\u00b7m'\u00b7ner", "Lust", ",", "zu\u00b7gleich", "von", "ihr", "emp\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "APPR", "PPOSAT", "VVPP", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Ja, wie die Vorw\u00fcrff auf der Erden,", "tokens": ["Ja", ",", "wie", "die", "Vor\u00b7w\u00fcrff", "auf", "der", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Wodurch die Sinnen hier vergn\u00fcgt besch\u00e4fftigt werden,", "tokens": ["Wo\u00b7durch", "die", "Sin\u00b7nen", "hier", "ver\u00b7gn\u00fcgt", "be\u00b7sch\u00e4ff\u00b7tigt", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ohn Ende fast und sonder Zahl;", "tokens": ["Ohn", "En\u00b7de", "fast", "und", "son\u00b7der", "Zahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "So k\u00f6nnen auch durch GOTT viel tausend, tausendmahl", "tokens": ["So", "k\u00f6n\u00b7nen", "auch", "durch", "GoTT", "viel", "tau\u00b7send", ",", "tau\u00b7send\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VMFIN", "ADV", "APPR", "NE", "ADV", "ADJD", "$,", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die F\u00e4higkeiten selbst sich mehren,", "tokens": ["Die", "F\u00e4\u00b7hig\u00b7kei\u00b7ten", "selbst", "sich", "meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Die alle wiederum, die Seel\u2019gen zu vergn\u00fcgen,", "tokens": ["Die", "al\u00b7le", "wie\u00b7de\u00b7rum", ",", "die", "Seel'\u00b7gen", "zu", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Unz\u00e4hlbar sch\u00f6ne Vorw\u00fcrff kriegen.", "tokens": ["Un\u00b7z\u00e4hl\u00b7bar", "sch\u00f6\u00b7ne", "Vor\u00b7w\u00fcrff", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Indem wir ja allhier auf Erden,", "tokens": ["In\u00b7dem", "wir", "ja", "all\u00b7hier", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Durch mehr als einen Sinn, vergn\u00fcget werden:", "tokens": ["Durch", "mehr", "als", "ei\u00b7nen", "Sinn", ",", "ver\u00b7gn\u00fc\u00b7get", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "KOKOM", "ART", "NN", "$,", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "So kann man auch mit Wahrheits-Grunde dencken,", "tokens": ["So", "kann", "man", "auch", "mit", "Wahr\u00b7heits\u00b7Grun\u00b7de", "den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Ja gar fast \u00fcberzeuglich glauben,", "tokens": ["Ja", "gar", "fast", "\u00fc\u00b7berz\u00b7eug\u00b7lich", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Da\u00df GOTT, der schon allhier", "tokens": ["Da\u00df", "GoTT", ",", "der", "schon", "all\u00b7hier"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NE", "$,", "PRELS", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Uns manche Freuden-Th\u00fcr\u2019,", "tokens": ["Uns", "man\u00b7che", "Freu\u00b7den\u00b7T\u00b7h\u00fcr'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "In vielen Sinnen uns gew\u00fcrdiget zu schencken;", "tokens": ["In", "vie\u00b7len", "Sin\u00b7nen", "uns", "ge\u00b7w\u00fcr\u00b7di\u00b7get", "zu", "schen\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Uns, statt im Himmel dort dieselbe zu verschliessen,", "tokens": ["Uns", ",", "statt", "im", "Him\u00b7mel", "dort", "die\u00b7sel\u00b7be", "zu", "ver\u00b7schlies\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUI", "APPRART", "NN", "ADV", "PDAT", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und uns ein w\u00fcrckliches Geniessen", "tokens": ["Und", "uns", "ein", "w\u00fcrck\u00b7li\u00b7ches", "Ge\u00b7nies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Dadurch zu rauben;", "tokens": ["Da\u00b7durch", "zu", "rau\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.34": {"text": "Viel eh\u2019 uns mehr annoch er\u00f6ffnen werde.", "tokens": ["Viel", "eh'", "uns", "mehr", "an\u00b7noch", "er\u00b7\u00f6ff\u00b7nen", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Es ist viel glaublicher,", "tokens": ["Es", "ist", "viel", "glaub\u00b7li\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.36": {"text": "Da\u00df GOTT, der HERR,", "tokens": ["Da\u00df", "GoTT", ",", "der", "HeRR", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.37": {"text": "So Leib als Seel\u2019 annoch zu tausend F\u00e4higkeiten,", "tokens": ["So", "Leib", "als", "Seel'", "an\u00b7noch", "zu", "tau\u00b7send", "F\u00e4\u00b7hig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "ADV", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Vergn\u00fcget und erg\u00f6tzt zu seyn,", "tokens": ["Ver\u00b7gn\u00fc\u00b7get", "und", "er\u00b7g\u00f6tzt", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Uns k\u00f6nne, woll\u2019 und werde zubereiten.", "tokens": ["Uns", "k\u00f6n\u00b7ne", ",", "woll'", "und", "wer\u00b7de", "zu\u00b7be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VMFIN", "KON", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}