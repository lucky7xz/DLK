{"textgrid.poem.58305": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "343. Wie man wohl leiden soll", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du ein Kreuz nach Gottes Willen tragen,", "tokens": ["Willst", "du", "ein", "Kreuz", "nach", "Got\u00b7tes", "Wil\u00b7len", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So trag es sanft und heimlich ohne Klagen;", "tokens": ["So", "trag", "es", "sanft", "und", "heim\u00b7lich", "oh\u00b7ne", "Kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schau blo\u00df auf Gott und nicht auf deine Pein,", "tokens": ["Schau", "blo\u00df", "auf", "Gott", "und", "nicht", "auf", "dei\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "KON", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was er dir gibt, kann ja nicht b\u00f6se sein!", "tokens": ["Was", "er", "dir", "gibt", ",", "kann", "ja", "nicht", "b\u00f6\u00b7se", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "$,", "VMFIN", "ADV", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}