{"textgrid.poem.59423": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Schifferspruch", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn die Wogen unten toben,", "tokens": ["Wenn", "die", "Wo\u00b7gen", "un\u00b7ten", "to\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Menschenwitz zuschanden wird,", "tokens": ["Men\u00b7schen\u00b7witz", "zu\u00b7schan\u00b7den", "wird", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weist mit feur'gen Z\u00fcgen droben", "tokens": ["Weist", "mit", "feur'\u00b7gen", "Z\u00fc\u00b7gen", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heimw\u00e4rts dich der Wogen Hirt.", "tokens": ["Heim\u00b7w\u00e4rts", "dich", "der", "Wo\u00b7gen", "Hirt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sollst nach keinem andern fragen,", "tokens": ["Sollst", "nach", "kei\u00b7nem", "an\u00b7dern", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PIAT", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht zur\u00fcckschaun nach dem Land,", "tokens": ["Nicht", "zu\u00b7r\u00fcck\u00b7schaun", "nach", "dem", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fa\u00df das Steuer, la\u00df das Zagen!", "tokens": ["Fa\u00df", "das", "Steu\u00b7er", ",", "la\u00df", "das", "Za\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVIMP", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aufgerollt hat Gottes Hand", "tokens": ["Auf\u00b7ge\u00b7rollt", "hat", "Got\u00b7tes", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Diese Wogen zum Befahren", "tokens": ["Die\u00b7se", "Wo\u00b7gen", "zum", "Be\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und die Sterne, dich zu wahren.", "tokens": ["Und", "die", "Ster\u00b7ne", ",", "dich", "zu", "wah\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}