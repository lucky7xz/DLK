{"dta.poem.23321": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fleckseife.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, die Welt gelangt zur Reife,", "tokens": ["Ja", ",", "die", "Welt", "ge\u00b7langt", "zur", "Rei\u00b7fe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer klarer wird ihr Zweck:", "tokens": ["Im\u00b7mer", "kla\u00b7rer", "wird", "ihr", "Zweck", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jetzt erfand man eine Seife,", "tokens": ["Jetzt", "er\u00b7fand", "man", "ei\u00b7ne", "Sei\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die vertilget jeden Fleck.", "tokens": ["Die", "ver\u00b7til\u00b7get", "je\u00b7den", "Fleck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alt' und neuer Unrath schwindet", "tokens": ["Alt'", "und", "neu\u00b7er", "Un\u00b7rath", "schwin\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor der Seife wie ein Traum,", "tokens": ["Vor", "der", "Sei\u00b7fe", "wie", "ein", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ihr niemals wiederfindet", "tokens": ["Da\u00df", "ihr", "nie\u00b7mals", "wie\u00b7der\u00b7fin\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eines Fleckes Spur noch Saum.", "tokens": ["Ei\u00b7nes", "Fle\u00b7ckes", "Spur", "noch", "Saum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kauft die Seife, Diplomaten,", "tokens": ["Kauft", "die", "Sei\u00b7fe", ",", "Dip\u00b7lo\u00b7ma\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wascht uns die Geschichte rein,", "tokens": ["Wascht", "uns", "die", "Ge\u00b7schich\u00b7te", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn sie ist von euren Thaten", "tokens": ["Denn", "sie", "ist", "von", "eu\u00b7ren", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schw\u00e4rzer als von H\u00f6llenstein.", "tokens": ["Schw\u00e4r\u00b7zer", "als", "von", "H\u00f6l\u00b7len\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}