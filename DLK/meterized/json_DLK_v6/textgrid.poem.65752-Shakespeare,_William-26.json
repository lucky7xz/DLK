{"textgrid.poem.65752": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "26", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr meiner Liebe! Der zum Sklaven du", "tokens": ["Herr", "mei\u00b7ner", "Lie\u00b7be", "!", "Der", "zum", "Skla\u00b7ven", "du"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$.", "ART", "APPRART", "NN", "PPER"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Durch dein Verdienst mich ganz gemacht dein eigen,", "tokens": ["Durch", "dein", "Ver\u00b7dienst", "mich", "ganz", "ge\u00b7macht", "dein", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dir send' ich die geschriebne Botschaft zu,", "tokens": ["Dir", "send'", "ich", "die", "ge\u00b7schrieb\u00b7ne", "Bot\u00b7schaft", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ergebenheit, nicht Geisteskraft zu zeigen:", "tokens": ["Er\u00b7ge\u00b7ben\u00b7heit", ",", "nicht", "Geis\u00b7tes\u00b7kraft", "zu", "zei\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKNEG", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ergebenheit so gro\u00df, da\u00df sie mein schlichter Geist,", "tokens": ["Er\u00b7ge\u00b7ben\u00b7heit", "so", "gro\u00df", ",", "da\u00df", "sie", "mein", "schlich\u00b7ter", "Geist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$,", "KOUS", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem Worte fehlen, dir nur d\u00fcrftig beut,", "tokens": ["Dem", "Wor\u00b7te", "feh\u00b7len", ",", "dir", "nur", "d\u00fcrf\u00b7tig", "beut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wenn ich nicht hoffen darf, im guten Herzen seist", "tokens": ["Wenn", "ich", "nicht", "hof\u00b7fen", "darf", ",", "im", "gu\u00b7ten", "Her\u00b7zen", "seist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,", "APPRART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du auch die nackende zu kleiden mir bereit;", "tokens": ["Du", "auch", "die", "na\u00b7cken\u00b7de", "zu", "klei\u00b7den", "mir", "be\u00b7reit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "PTKZU", "VVINF", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bis das Gestirn, das meine Tage lenkt,", "tokens": ["Bis", "das", "Ge\u00b7stirn", ",", "das", "mei\u00b7ne", "Ta\u00b7ge", "lenkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wie es auch hei\u00dfe, mit vers\u00f6hntem Strahl", "tokens": ["Wie", "es", "auch", "hei\u00b7\u00dfe", ",", "mit", "ver\u00b7s\u00f6hn\u00b7tem", "Strahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Mir winkend, zarter Liebe Kleidung schenkt,", "tokens": ["Mir", "win\u00b7kend", ",", "zar\u00b7ter", "Lie\u00b7be", "Klei\u00b7dung", "schenkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Mich wert zu zeigen deiner s\u00fc\u00dfen Wahl.", "tokens": ["Mich", "wert", "zu", "zei\u00b7gen", "dei\u00b7ner", "s\u00fc\u00b7\u00dfen", "Wahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Dann wag' ich laut zu r\u00fchmen wie ich dein:", "tokens": ["Dann", "wag'", "ich", "laut", "zu", "r\u00fch\u00b7men", "wie", "ich", "dein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "KOKOM", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Bis dahin soll mein Haupt vor dir verborgen sein.", "tokens": ["Bis", "da\u00b7hin", "soll", "mein", "Haupt", "vor", "dir", "ver\u00b7bor\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PAV", "VMFIN", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}