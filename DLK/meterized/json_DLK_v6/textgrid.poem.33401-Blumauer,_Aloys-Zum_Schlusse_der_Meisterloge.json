{"textgrid.poem.33401": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Zum Schlusse der Meisterloge", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frohlocket, ihr w\u00fcrdigen Br\u00fcder!", "tokens": ["Froh\u00b7lo\u00b7cket", ",", "ihr", "w\u00fcr\u00b7di\u00b7gen", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die himmlische Seel' ist hernieder", "tokens": ["Die", "himm\u00b7li\u00b7sche", "Seel'", "ist", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Zum irdischen Leibe gekehrt:", "tokens": ["Zum", "ir\u00b7di\u00b7schen", "Lei\u00b7be", "ge\u00b7kehrt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Ein Wort, das die Himmel nicht fassen,", "tokens": ["Ein", "Wort", ",", "das", "die", "Him\u00b7mel", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Ein Lichtstrahl, dem Sonnen erblassen,", "tokens": ["Ein", "Licht\u00b7strahl", ",", "dem", "Son\u00b7nen", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Hat neu ihn beseelt und verkl\u00e4rt.", "tokens": ["Hat", "neu", "ihn", "be\u00b7seelt", "und", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "VVPP", "KON", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Vom Geiste der Sch\u00f6pfung belebet,", "tokens": ["Vom", "Geis\u00b7te", "der", "Sch\u00f6p\u00b7fung", "be\u00b7le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erhebt er sich wieder, und strebet", "tokens": ["Er\u00b7hebt", "er", "sich", "wie\u00b7der", ",", "und", "stre\u00b7bet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$,", "KON", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Zu dem auf, der \u00fcber ihm ruht:", "tokens": ["Zu", "dem", "auf", ",", "der", "\u00fc\u00b7ber", "ihm", "ruht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PTKVZ", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Er spricht nun in ewiger Jugend", "tokens": ["Er", "spricht", "nun", "in", "e\u00b7wi\u00b7ger", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Sein Werde der Weisheit und Tugend,", "tokens": ["Sein", "Wer\u00b7de", "der", "Weis\u00b7heit", "und", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und was er sich schaffet \u2013 ist gut!", "tokens": ["Und", "was", "er", "sich", "schaf\u00b7fet", "\u2013", "ist", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PRF", "VVFIN", "$(", "VAFIN", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}