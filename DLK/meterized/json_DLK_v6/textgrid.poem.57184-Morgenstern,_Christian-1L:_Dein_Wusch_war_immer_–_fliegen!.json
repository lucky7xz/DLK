{"textgrid.poem.57184": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein Wusch war immer \u2013 fliegen!", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Wusch war immer \u2013 fliegen!", "tokens": ["Dein", "Wusch", "war", "im\u00b7mer", "\u2013", "flie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$(", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun naht dir die Erf\u00fcllung.", "tokens": ["Nun", "naht", "dir", "die", "Er\u00b7f\u00fcl\u00b7lung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du wirst den Raum besiegen,", "tokens": ["Du", "wirst", "den", "Raum", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "nach jener Weltenth\u00fcllung,", "tokens": ["nach", "je\u00b7ner", "Wel\u00b7tent\u00b7h\u00fcl\u00b7lung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "die uns zu Freien machte", "tokens": ["die", "uns", "zu", "Frei\u00b7en", "mach\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "vom Schlaf der blinden Runden.", "tokens": ["vom", "Schlaf", "der", "blin\u00b7den", "Run\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun hast du, Mit-Erwachte,", "tokens": ["Nun", "hast", "du", ",", "Mit\u00b7Er\u00b7wach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "dein Schwingenkleid gefunden!", "tokens": ["dein", "Schwin\u00b7gen\u00b7kleid", "ge\u00b7fun\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}