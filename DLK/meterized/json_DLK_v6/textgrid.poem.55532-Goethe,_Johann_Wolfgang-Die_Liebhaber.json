{"textgrid.poem.55532": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Die Liebhaber", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein M\u00e4dchen im Schatten der Laube,", "tokens": ["Mein", "M\u00e4d\u00b7chen", "im", "Schat\u00b7ten", "der", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Umhangen von purpurner Traube,", "tokens": ["Um\u00b7han\u00b7gen", "von", "pur\u00b7pur\u00b7ner", "Trau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Bekr\u00e4nzte mit Rebenlaub sich", "tokens": ["Be\u00b7kr\u00e4nz\u00b7te", "mit", "Re\u00b7ben\u00b7laub", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "PRF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und wartete schmachtend auf mich.", "tokens": ["Und", "war\u00b7te\u00b7te", "schmach\u00b7tend", "auf", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPER", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da wallte der Herrscher der Tr\u00e4ume", "tokens": ["Da", "wall\u00b7te", "der", "Herr\u00b7scher", "der", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Durch zitternde Wipfel der B\u00e4ume,", "tokens": ["Durch", "zit\u00b7tern\u00b7de", "Wip\u00b7fel", "der", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Erblickte das liebliche Kind,", "tokens": ["Er\u00b7blick\u00b7te", "das", "lieb\u00b7li\u00b7che", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Sank nieder, umarmt' es geschwind.", "tokens": ["Sank", "nie\u00b7der", ",", "um\u00b7armt'", "es", "ge\u00b7schwind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Sie schlummert', er k\u00fc\u00dfte die Wangen,", "tokens": ["Sie", "schlum\u00b7mert'", ",", "er", "k\u00fc\u00df\u00b7te", "die", "Wan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sie gl\u00fchten von hei\u00dfem Verlangen,", "tokens": ["Sie", "gl\u00fch\u00b7ten", "von", "hei\u00b7\u00dfem", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Erhitzet, o Gottheit, von dir,", "tokens": ["Er\u00b7hit\u00b7zet", ",", "o", "Got\u00b7theit", ",", "von", "dir", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "NN", "$,", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach sterblichen K\u00fcssen von mir.", "tokens": ["Nach", "sterb\u00b7li\u00b7chen", "K\u00fcs\u00b7sen", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Da saugte mit atmenden Z\u00fcgen", "tokens": ["Da", "saug\u00b7te", "mit", "at\u00b7men\u00b7den", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Annette das gr\u00f6\u00dfte Vergn\u00fcgen", "tokens": ["An\u00b7net\u00b7te", "das", "gr\u00f6\u00df\u00b7te", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Der Tr\u00e4ume, die M\u00e4dchen erfreun,", "tokens": ["Der", "Tr\u00e4u\u00b7me", ",", "die", "M\u00e4d\u00b7chen", "er\u00b7freun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Vom Munde des G\u00f6ttlichen ein.", "tokens": ["Vom", "Mun\u00b7de", "des", "G\u00f6tt\u00b7li\u00b7chen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Schnell war sie von Leuten umgeben,", "tokens": ["Schnell", "war", "sie", "von", "Leu\u00b7ten", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die schmachteten seufzend nach Leben", "tokens": ["Die", "schmach\u00b7te\u00b7ten", "seuf\u00b7zend", "nach", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und harreten zitternd aufs Gl\u00fcck", "tokens": ["Und", "har\u00b7re\u00b7ten", "zit\u00b7ternd", "aufs", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Von einem beseelenden Blick.", "tokens": ["Von", "ei\u00b7nem", "be\u00b7see\u00b7len\u00b7den", "Blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Da lag nun auf Knien die Menge,", "tokens": ["Da", "lag", "nun", "auf", "Kni\u00b7en", "die", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Mein M\u00e4dchen erblickt' das Gedr\u00e4nge", "tokens": ["Mein", "M\u00e4d\u00b7chen", "er\u00b7blickt'", "das", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Und h\u00f6rte der Bittenden Schrei'n", "tokens": ["Und", "h\u00f6r\u00b7te", "der", "Bit\u00b7ten\u00b7den", "Schrei'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Und d\u00fcnkte sich Venus zu sein.", "tokens": ["Und", "d\u00fcnk\u00b7te", "sich", "Ve\u00b7nus", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Erst sah sie den schrecklichen Sieger,", "tokens": ["Erst", "sah", "sie", "den", "schreck\u00b7li\u00b7chen", "Sie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Da lag er geb\u00fcckt, wie ein Krieger,", "tokens": ["Da", "lag", "er", "ge\u00b7b\u00fcckt", ",", "wie", "ein", "Krie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Den st\u00e4rkerer Streitenden Macht", "tokens": ["Den", "st\u00e4r\u00b7ke\u00b7rer", "Strei\u00b7ten\u00b7den", "Macht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "In schimpfliche Fesseln gebracht.", "tokens": ["In", "schimpf\u00b7li\u00b7che", "Fes\u00b7seln", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "So sprach er: \u00bbDie m\u00e4chtigen Waffen,", "tokens": ["So", "sprach", "er", ":", "\u00bb", "Die", "m\u00e4ch\u00b7ti\u00b7gen", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Den Ruhm zu erobern geschaffen,", "tokens": ["Den", "Ruhm", "zu", "er\u00b7o\u00b7bern", "ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Erheben, erw\u00e4hlest du mich,", "tokens": ["Er\u00b7he\u00b7ben", ",", "er\u00b7w\u00e4h\u00b7lest", "du", "mich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Auf deine Befehle nur sich.", "tokens": ["Auf", "dei\u00b7ne", "Be\u00b7feh\u00b7le", "nur", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "PRF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Da f\u00fcrcht ich nicht W\u00e4ll, nicht Kanonen,", "tokens": ["Da", "f\u00fcrcht", "ich", "nicht", "W\u00e4ll", ",", "nicht", "Ka\u00b7no\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Nicht Tonnen, die Minen bewohnen,", "tokens": ["Nicht", "Ton\u00b7nen", ",", "die", "Mi\u00b7nen", "be\u00b7woh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Nicht Feinde, die scharenweis ziehn,", "tokens": ["Nicht", "Fein\u00b7de", ",", "die", "scha\u00b7ren\u00b7weis", "ziehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Du sprichst nur: \u203aEntflieht!\u2039 \u2013 sie entfliehn.", "tokens": ["Du", "sprichst", "nur", ":", "\u203a", "Ent\u00b7flieht", "!", "\u2039", "\u2013", "sie", "ent\u00b7fliehn", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "$(", "VVFIN", "$.", "$(", "$(", "PPER", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Doch mu\u00dft du f\u00fcr Eisen nicht beben,", "tokens": ["Doch", "mu\u00dft", "du", "f\u00fcr", "Ei\u00b7sen", "nicht", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Mein Arm, den jetzt Waffen umgeben,", "tokens": ["Mein", "Arm", ",", "den", "jetzt", "Waf\u00b7fen", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Schlie\u00dft sich in entwaffneter Ruh", "tokens": ["Schlie\u00dft", "sich", "in", "ent\u00b7waff\u00b7ne\u00b7ter", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Auch sanften Umarmungen zu.\u00ab", "tokens": ["Auch", "sanf\u00b7ten", "Um\u00b7ar\u00b7mun\u00b7gen", "zu", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Kaufmann mit Putzwerk und Stoffen,", "tokens": ["Der", "Kauf\u00b7mann", "mit", "Put\u00b7zwerk", "und", "Stof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Was eitele M\u00e4dchen nur hoffen,", "tokens": ["Was", "ei\u00b7te\u00b7le", "M\u00e4d\u00b7chen", "nur", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Trat n\u00e4her und beugte sein Knie,", "tokens": ["Trat", "n\u00e4\u00b7her", "und", "beug\u00b7te", "sein", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Verbreitet' es hoffend vor sie;", "tokens": ["Ver\u00b7brei\u00b7tet'", "es", "hof\u00b7fend", "vor", "sie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PPER", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "\u00bberh\u00f6re mich, werde die meine\u00ab,", "tokens": ["\u00bb", "er\u00b7h\u00f6\u00b7re", "mich", ",", "wer\u00b7de", "die", "mei\u00b7ne", "\u00ab", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "VAFIN", "ART", "PPOSAT", "$(", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "So sprach er, \u00bbdies alles ist deine,", "tokens": ["So", "sprach", "er", ",", "\u00bb", "dies", "al\u00b7les", "ist", "dei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "$(", "PDS", "PIS", "VAFIN", "PPOSAT", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Dich kleid ich in herrlicher Pracht", "tokens": ["Dich", "kleid", "ich", "in", "herr\u00b7li\u00b7cher", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Dann, wenn du mich gl\u00fccklich gemacht.\u00ab", "tokens": ["Dann", ",", "wenn", "du", "mich", "gl\u00fcck\u00b7lich", "ge\u00b7macht", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVPP", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Der Stutzer im scheckigen Kleide", "tokens": ["Der", "Stut\u00b7zer", "im", "sche\u00b7cki\u00b7gen", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Samt und von Gold und von Seide", "tokens": ["Von", "Samt", "und", "von", "Gold", "und", "von", "Sei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "KON", "APPR", "NE"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Kam summend wie K\u00e4fer im Mai", "tokens": ["Kam", "sum\u00b7mend", "wie", "K\u00e4\u00b7fer", "im", "Mai"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "KOKOM", "NN", "APPRART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Mit k\u00fcnstlichen Spr\u00fcngen herbei:", "tokens": ["Mit", "k\u00fcnst\u00b7li\u00b7chen", "Spr\u00fcn\u00b7gen", "her\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "\u00bbdu gl\u00e4nzest bei Ball und Konzerten,", "tokens": ["\u00bb", "du", "gl\u00e4n\u00b7zest", "bei", "Ball", "und", "Kon\u00b7zer\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Du herrschest beim Spiel und in G\u00e4rten,", "tokens": ["Du", "herr\u00b7schest", "beim", "Spiel", "und", "in", "G\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word",