{"textgrid.poem.32845": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn mich bejahrte Spr\u00f6den qu\u00e4len,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn mich bejahrte Spr\u00f6den qu\u00e4len,", "tokens": ["Wenn", "mich", "be\u00b7jahr\u00b7te", "Spr\u00f6\u00b7den", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir ihrer Jugend Gl\u00fcck erz\u00e4hlen,", "tokens": ["Mir", "ih\u00b7rer", "Ju\u00b7gend", "Gl\u00fcck", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und auf die ietzgen Zeiten schm\u00e4hlen:", "tokens": ["Und", "auf", "die", "ietz\u00b7gen", "Zei\u00b7ten", "schm\u00e4h\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie lang wird mir die Zeit!", "tokens": ["Wie", "lang", "wird", "mir", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn artge M\u00e4dchen mit mir spielen,", "tokens": ["Wenn", "art\u00b7ge", "M\u00e4d\u00b7chen", "mit", "mir", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die noch wie ich, ihr Leben f\u00fchlen,", "tokens": ["Die", "noch", "wie", "ich", ",", "ihr", "Le\u00b7ben", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KOKOM", "PPER", "$,", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und schlau nach meinem Herzen zielen,", "tokens": ["Und", "schlau", "nach", "mei\u00b7nem", "Her\u00b7zen", "zie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie hurtig verschwindet die Zeit!", "tokens": ["Wie", "hur\u00b7tig", "ver\u00b7schwin\u00b7det", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Wenn meine Vettern mich bet\u00e4uben,", "tokens": ["Wenn", "mei\u00b7ne", "Vet\u00b7tern", "mich", "be\u00b7t\u00e4u\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir weise Regeln niederschreiben,", "tokens": ["Mir", "wei\u00b7se", "Re\u00b7geln", "nie\u00b7der\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie ich soll gute Wirthschaft treiben:", "tokens": ["Wie", "ich", "soll", "gu\u00b7te", "Wirth\u00b7schaft", "trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie lang wird mir die Zeit!", "tokens": ["Wie", "lang", "wird", "mir", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch wenn sie wie der Tejer winken,", "tokens": ["Doch", "wenn", "sie", "wie", "der", "Te\u00b7jer", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "KOKOM", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo eingesch\u00e4nkte Gl\u00e4ser blinken,", "tokens": ["Wo", "ein\u00b7ge\u00b7sch\u00e4nk\u00b7te", "Gl\u00e4\u00b7ser", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit ihnen jugendlich zu trinken,", "tokens": ["Mit", "ih\u00b7nen", "ju\u00b7gend\u00b7lich", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie hurtig verschwindet die Zeit!", "tokens": ["Wie", "hur\u00b7tig", "ver\u00b7schwin\u00b7det", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Wenn mich politsche M\u00e4ckler st\u00f6ren,", "tokens": ["Wenn", "mich", "po\u00b7lit\u00b7sche", "M\u00e4ck\u00b7ler", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich wieder T\u00fcrk und Pabst verschw\u00f6ren,", "tokens": ["Sich", "wie\u00b7der", "T\u00fcrk", "und", "Pabst", "ver\u00b7schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bald Reiche baun, bald sie zerst\u00f6ren,", "tokens": ["Bald", "Rei\u00b7che", "baun", ",", "bald", "sie", "zer\u00b7st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVINF", "$,", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie lang wird mir die Zeit!", "tokens": ["Wie", "lang", "wird", "mir", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch wenn mit unsrer Zeit zufrieden,", "tokens": ["Doch", "wenn", "mit", "uns\u00b7rer", "Zeit", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mich muntre Freund im Krieg und Frieden,", "tokens": ["Mich", "mun\u00b7tre", "Freund", "im", "Krieg", "und", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Durch heitre Scherze nie erm\u00fcden:", "tokens": ["Durch", "heit\u00b7re", "Scher\u00b7ze", "nie", "er\u00b7m\u00fc\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Wie hurtig verschwindet die Zeit!", "tokens": ["Wie", "hur\u00b7tig", "ver\u00b7schwin\u00b7det", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Wenn meine Wechsel langsam gehen,", "tokens": ["Wenn", "mei\u00b7ne", "Wech\u00b7sel", "lang\u00b7sam", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Gl\u00e4ubiger nicht Spas verstehen", "tokens": ["Die", "Gl\u00e4u\u00b7bi\u00b7ger", "nicht", "Spas", "ver\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und Wirthe mich nicht gerne sehen,", "tokens": ["Und", "Wirt\u00b7he", "mich", "nicht", "ger\u00b7ne", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie lang wird mir die Zeit!", "tokens": ["Wie", "lang", "wird", "mir", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch wenn die volle B\u00f6rse klinget,", "tokens": ["Doch", "wenn", "die", "vol\u00b7le", "B\u00f6r\u00b7se", "klin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Man Wein mir ungefordert bringet,", "tokens": ["Man", "Wein", "mir", "un\u00b7ge\u00b7for\u00b7dert", "brin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn man bald tanzt, bald k\u00fc\u00dft, bald singet:", "tokens": ["Wenn", "man", "bald", "tanzt", ",", "bald", "k\u00fc\u00dft", ",", "bald", "sin\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie hurtig verschwindet die Zeit!", "tokens": ["Wie", "hur\u00b7tig", "ver\u00b7schwin\u00b7det", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}