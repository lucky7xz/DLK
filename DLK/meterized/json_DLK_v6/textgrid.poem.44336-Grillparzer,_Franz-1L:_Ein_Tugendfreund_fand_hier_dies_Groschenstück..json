{"textgrid.poem.44336": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Tugendfreund fand hier dies Groschenst\u00fcck.", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Tugendfreund fand hier dies Groschenst\u00fcck.", "tokens": ["Ein", "Tu\u00b7gend\u00b7freund", "fand", "hier", "dies", "Gro\u00b7schen\u00b7st\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PDS", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vergessend auf sein eignes Gl\u00fcck", "tokens": ["Ver\u00b7ges\u00b7send", "auf", "sein", "eig\u00b7nes", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mit dem Edelmute eines Briten", "tokens": ["Und", "mit", "dem", "E\u00b7del\u00b7mu\u00b7te", "ei\u00b7nes", "Bri\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Stellt ers dem Eigent\u00fcmer hier zur\u00fcck", "tokens": ["Stellt", "ers", "dem", "Ei\u00b7gen\u00b7t\u00fc\u00b7mer", "hier", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ART", "NN", "ADV", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und zeigt, da\u00df ", "tokens": ["Und", "zeigt", ",", "da\u00df"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Nicht bei den Erdeng\u00f6ttern thront,", "tokens": ["Nicht", "bei", "den", "Er\u00b7den\u00b7g\u00f6t\u00b7tern", "thront", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df er auch in der Armut H\u00fctte,", "tokens": ["Da\u00df", "er", "auch", "in", "der", "Ar\u00b7mut", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Ja, selber auf dem Abtritt wohnt.", "tokens": ["Ja", ",", "sel\u00b7ber", "auf", "dem", "Ab\u00b7tritt", "wohnt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}