{"textgrid.poem.41904": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie, die sich treu dem Wankenden verbanden,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie, die sich treu dem Wankenden verbanden,", "tokens": ["Sie", ",", "die", "sich", "treu", "dem", "Wan\u00b7ken\u00b7den", "ver\u00b7ban\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PRF", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als feindlich mir mein b\u00f6ser Stern erschien,", "tokens": ["Als", "feind\u00b7lich", "mir", "mein", "b\u00f6\u00b7ser", "Stern", "er\u00b7schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Einzigen, die ganz mein Herz verstanden", "tokens": ["Die", "Ein\u00b7zi\u00b7gen", ",", "die", "ganz", "mein", "Herz", "ver\u00b7stan\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Liebe mir f\u00fcr Liebe gern verliehn,", "tokens": ["Und", "Lie\u00b7be", "mir", "f\u00fcr", "Lie\u00b7be", "gern", "ver\u00b7liehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "APPR", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie seh' ich jetzt hinweg zu fernen Landen", "tokens": ["Sie", "seh'", "ich", "jetzt", "hin\u00b7weg", "zu", "fer\u00b7nen", "Lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der sch\u00f6nen Pflicht, dem Lohn entgegen ziehn;", "tokens": ["Der", "sch\u00f6\u00b7nen", "Pflicht", ",", "dem", "Lohn", "ent\u00b7ge\u00b7gen", "ziehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich mu\u00df allein in diesem armen Treiben", "tokens": ["Ich", "mu\u00df", "al\u00b7lein", "in", "die\u00b7sem", "ar\u00b7men", "Trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der kalten Welt mit gl\u00fch'ndem Herzen bleiben.", "tokens": ["Der", "kal\u00b7ten", "Welt", "mit", "gl\u00fch'n\u00b7dem", "Her\u00b7zen", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl w\u00fcrde dort in jenem reichen Leben,", "tokens": ["Wohl", "w\u00fcr\u00b7de", "dort", "in", "je\u00b7nem", "rei\u00b7chen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo, gleich dem Licht im hellen Edelstein,", "tokens": ["Wo", ",", "gleich", "dem", "Licht", "im", "hel\u00b7len", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ADV", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit freyem Glanz die regen Funken schweben", "tokens": ["Mit", "frey\u00b7em", "Glanz", "die", "re\u00b7gen", "Fun\u00b7ken", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und immer neu ihr farb'ges Feuer streun,", "tokens": ["Und", "im\u00b7mer", "neu", "ihr", "fa\u00b7rb'\u00b7ges", "Feu\u00b7er", "streun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Auch meine Kraft gereizter sich erheben", "tokens": ["Auch", "mei\u00b7ne", "Kraft", "ge\u00b7reiz\u00b7ter", "sich", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und fr\u00f6hlicher die Fantasie gedeihn.", "tokens": ["Und", "fr\u00f6h\u00b7li\u00b7cher", "die", "Fan\u00b7ta\u00b7sie", "ge\u00b7deihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wo Strahlen gern an Strahlen sich entz\u00fcnden,", "tokens": ["Wo", "Strah\u00b7len", "gern", "an", "Strah\u00b7len", "sich", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "APPR", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wohl w\u00fcrd' auch ich dort Gunst und Liebe finden.", "tokens": ["Wohl", "w\u00fcrd'", "auch", "ich", "dort", "Gunst", "und", "Lie\u00b7be", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hier mu\u00df ich scheu die heil'ge Gluth verh\u00fcllen", "tokens": ["Hier", "mu\u00df", "ich", "scheu", "die", "heil'\u00b7ge", "Gluth", "ver\u00b7h\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und d\u00e4mpfen, was ein Gott mir angefacht,", "tokens": ["Und", "d\u00e4mp\u00b7fen", ",", "was", "ein", "Gott", "mir", "an\u00b7ge\u00b7facht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PRELS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wenn auch hei\u00df mich tausend Flammen f\u00fcllen", "tokens": ["Und", "wenn", "auch", "hei\u00df", "mich", "tau\u00b7send", "Flam\u00b7men", "f\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ADJD", "PPER", "CARD", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und m\u00e4chtig Lust und Leid in mir erwacht,", "tokens": ["Und", "m\u00e4ch\u00b7tig", "Lust", "und", "Leid", "in", "mir", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "KON", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nicht kann ich hier den Durst des Herzens stillen,", "tokens": ["Nicht", "kann", "ich", "hier", "den", "Durst", "des", "Her\u00b7zens", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Denn keiner ist, der mit mir weint und lacht.", "tokens": ["Denn", "kei\u00b7ner", "ist", ",", "der", "mit", "mir", "weint", "und", "lacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In eigner Brust mu\u00df ich den Fr\u00fchling tragen,", "tokens": ["In", "eig\u00b7ner", "Brust", "mu\u00df", "ich", "den", "Fr\u00fch\u00b7ling", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und dennoch sieht mein Blick ihn nimmer tagen.", "tokens": ["Und", "den\u00b7noch", "sieht", "mein", "Blick", "ihn", "nim\u00b7mer", "ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und ach, doch h\u00e4lt ein tr\u00fcgerisches Sehnen,", "tokens": ["Und", "ach", ",", "doch", "h\u00e4lt", "ein", "tr\u00fc\u00b7ge\u00b7ri\u00b7sches", "Seh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein langer Wahn, ein nie errung'nes Gl\u00fcck,", "tokens": ["Ein", "lan\u00b7ger", "Wahn", ",", "ein", "nie", "er\u00b7rung'\u00b7nes", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein ew'ger Kampf um Schmerzen nur und Thr\u00e4nen", "tokens": ["Ein", "ew'\u00b7ger", "Kampf", "um", "Schmer\u00b7zen", "nur", "und", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von neuem stets den Fliehenden zur\u00fcck!", "tokens": ["Von", "neu\u00b7em", "stets", "den", "Flie\u00b7hen\u00b7den", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "F\u00fcr jenen Lenz, den tausend Strahlen kr\u00f6nen,", "tokens": ["F\u00fcr", "je\u00b7nen", "Lenz", ",", "den", "tau\u00b7send", "Strah\u00b7len", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "ART", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Begehr' ich hier nur ", "tokens": ["Be\u00b7gehr'", "ich", "hier", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Ach, du nur bist's, du meine Lust, mein Leiden,", "tokens": ["Ach", ",", "du", "nur", "bist's", ",", "du", "mei\u00b7ne", "Lust", ",", "mein", "Lei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "ADV", "NE", "$,", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Du mein Geschick, du l\u00e4\u00dft mich nimmer scheiden.", "tokens": ["Du", "mein", "Ge\u00b7schick", ",", "du", "l\u00e4\u00dft", "mich", "nim\u00b7mer", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wie tausendfach die Wurzeln sich durchwinden,", "tokens": ["Wie", "tau\u00b7send\u00b7fach", "die", "Wur\u00b7zeln", "sich", "durch\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Woraus der Baum zum Himmel sich erhebt,", "tokens": ["Wo\u00b7raus", "der", "Baum", "zum", "Him\u00b7mel", "sich", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So ist mein Thun, mein Denken, mein Empfinden,", "tokens": ["So", "ist", "mein", "Thun", ",", "mein", "Den\u00b7ken", ",", "mein", "Emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein ganzes Seyn mit diesem Ort verwebt,", "tokens": ["Mein", "gan\u00b7zes", "Seyn", "mit", "die\u00b7sem", "Ort", "ver\u00b7webt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Berg und Thal, Quell, Wies' und Hain verk\u00fcnden,", "tokens": ["Und", "Berg", "und", "Thal", ",", "Quell", ",", "Wies'", "und", "Hain", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "NN", "$,", "NE", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie ich geliebt, gelitten und gelebt.", "tokens": ["Wie", "ich", "ge\u00b7liebt", ",", "ge\u00b7lit\u00b7ten", "und", "ge\u00b7lebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das Theuerste, was mir die Welt beschieden,", "tokens": ["Das", "Theu\u00b7ers\u00b7te", ",", "was", "mir", "die", "Welt", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Es lacht und bl\u00fcht, es schlummert hier im Frieden.", "tokens": ["Es", "lacht", "und", "bl\u00fcht", ",", "es", "schlum\u00b7mert", "hier", "im", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wohl mag, geraubt den m\u00fctterlichen Auen,", "tokens": ["Wohl", "mag", ",", "ge\u00b7raubt", "den", "m\u00fct\u00b7ter\u00b7li\u00b7chen", "Au\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein zarter Strauch im fremden Boden stehn;", "tokens": ["Ein", "zar\u00b7ter", "Strauch", "im", "frem\u00b7den", "Bo\u00b7den", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch wird umsonst der Himmel ihn bethauen,", "tokens": ["Doch", "wird", "um\u00b7sonst", "der", "Him\u00b7mel", "ihn", "be\u00b7thau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Umsonst der Hauch des Lenzes ihn umwehn,", "tokens": ["Um\u00b7sonst", "der", "Hauch", "des", "Len\u00b7zes", "ihn", "um\u00b7wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sehns\u00fcchtig wird die Bl\u00fcthe dorthin schauen,", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "wird", "die", "Bl\u00fc\u00b7the", "dor\u00b7thin", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo sie zuerst das sch\u00f6nre Licht gesehn.", "tokens": ["Wo", "sie", "zu\u00b7erst", "das", "sch\u00f6n\u00b7re", "Licht", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der ku