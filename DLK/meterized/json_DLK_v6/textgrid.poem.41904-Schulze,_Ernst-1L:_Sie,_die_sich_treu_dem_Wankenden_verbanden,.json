, "measure": "iambic.penta"}, "line.3": {"text": "Kein Freundesarm kann jetzt mich rettend fassen,", "tokens": ["Kein", "Freun\u00b7des\u00b7arm", "kann", "jetzt", "mich", "ret\u00b7tend", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn mich der Sturm in seine Strudel zieht.", "tokens": ["Wenn", "mich", "der", "Sturm", "in", "sei\u00b7ne", "Stru\u00b7del", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O sey mir mild! du kannst ja den nicht hassen,", "tokens": ["O", "sey", "mir", "mild", "!", "du", "kannst", "ja", "den", "nicht", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "$.", "PPER", "VMFIN", "ADV", "ART", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der gern f\u00fcr dich von allem Theuern schied,", "tokens": ["Der", "gern", "f\u00fcr", "dich", "von", "al\u00b7lem", "Theu\u00b7ern", "schied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "APPR", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der nicht gezagt, ein ganzes reiches Leben", "tokens": ["Der", "nicht", "ge\u00b7zagt", ",", "ein", "gan\u00b7zes", "rei\u00b7ches", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "VVPP", "$,", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8":