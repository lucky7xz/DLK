{"textgrid.poem.37139": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "A.", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Grabt dem jungen Buchenhaine", "tokens": ["Grabt", "dem", "jun\u00b7gen", "Bu\u00b7chen\u00b7hai\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eure Sch\u00e4ferinnen ein;", "tokens": ["Eu\u00b7re", "Sch\u00e4\u00b7fe\u00b7rin\u00b7nen", "ein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tief dem Herzen soll die meine,", "tokens": ["Tief", "dem", "Her\u00b7zen", "soll", "die", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VMFIN", "ART", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00e4fer, eingegraben sein!", "tokens": ["Sch\u00e4\u00b7fer", ",", "ein\u00b7ge\u00b7gra\u00b7ben", "sein", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Voll der s\u00fc\u00dfesten Gef\u00fchle", "tokens": ["Voll", "der", "s\u00fc\u00b7\u00dfes\u00b7ten", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schl\u00e4gt mein Busen; doch der Mund", "tokens": ["Schl\u00e4gt", "mein", "Bu\u00b7sen", ";", "doch", "der", "Mund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mache bei dem Saitenspiele", "tokens": ["Ma\u00b7che", "bei", "dem", "Sai\u00b7ten\u00b7spie\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Niemals ihren Namen kund.", "tokens": ["Nie\u00b7mals", "ih\u00b7ren", "Na\u00b7men", "kund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Reizender ist das Vergn\u00fcgen", "tokens": ["Rei\u00b7zen\u00b7der", "ist", "das", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "In der tiefsten Einsamkeit.", "tokens": ["In", "der", "tiefs\u00b7ten", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsre Freuden sind verschwiegen,", "tokens": ["Uns\u00b7re", "Freu\u00b7den", "sind", "ver\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Zeugen, ohne Neid.", "tokens": ["Oh\u00b7ne", "Zeu\u00b7gen", ",", "oh\u00b7ne", "Neid", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Selbst den Schwur, den wir geschworen,", "tokens": ["Selbst", "den", "Schwur", ",", "den", "wir", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VVFIN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fl\u00fcsterten wir leis' am Bach:", "tokens": ["Fl\u00fcs\u00b7ter\u00b7ten", "wir", "leis'", "am", "Bach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "Eifersucht hat tausend Ohren,", "tokens": ["Ei\u00b7fer\u00b7sucht", "hat", "tau\u00b7send", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schilf und B\u00e4che plaudern nach.", "tokens": ["Schilf", "und", "B\u00e4\u00b7che", "plau\u00b7dern", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da wo ihre Heerde spielet,", "tokens": ["Da", "wo", "ih\u00b7re", "Heer\u00b7de", "spie\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Siehet man die meine nie;", "tokens": ["Sie\u00b7het", "man", "die", "mei\u00b7ne", "nie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "PPOSAT", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sch\u00fcchtern und bed\u00e4chtlich schielet", "tokens": ["Sch\u00fcch\u00b7tern", "und", "be\u00b7d\u00e4cht\u00b7lich", "schie\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein verstohlner Blick auf sie.", "tokens": ["Mein", "ver\u00b7stohl\u00b7ner", "Blick", "auf", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unverf\u00e4rbt h\u00f6r' ich sie nennen,", "tokens": ["Un\u00b7ver\u00b7f\u00e4rbt", "h\u00f6r'", "ich", "sie", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sorglos steh ich, wenn sie singt,", "tokens": ["Sorg\u00b7los", "steh", "ich", ",", "wenn", "sie", "singt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ich scheine nicht zu kennen", "tokens": ["Und", "ich", "schei\u00b7ne", "nicht", "zu", "ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihren Hund, der auf mich springt.", "tokens": ["Ih\u00b7ren", "Hund", ",", "der", "auf", "mich", "springt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sch\u00e4fer lernt von feinen Seelen", "tokens": ["Sch\u00e4\u00b7fer", "lernt", "von", "fei\u00b7nen", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kalte Worte, kalten Blick!", "tokens": ["Kal\u00b7te", "Wor\u00b7te", ",", "kal\u00b7ten", "Blick", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht die Seligkeit erz\u00e4hlen,", "tokens": ["Nicht", "die", "Se\u00b7lig\u00b7keit", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie verschweigen, das ist Gl\u00fcck.", "tokens": ["Sie", "ver\u00b7schwei\u00b7gen", ",", "das", "ist", "Gl\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PDS", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Immer, o Geliebte, h\u00fclle", "tokens": ["Im\u00b7mer", ",", "o", "Ge\u00b7lieb\u00b7te", ",", "h\u00fcl\u00b7le"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "$,", "FM", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unser B\u00fcndnis sich in Nacht!", "tokens": ["Un\u00b7ser", "B\u00fcnd\u00b7nis", "sich", "in", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Liebe sucht allein die Stille,", "tokens": ["Lie\u00b7be", "sucht", "al\u00b7lein", "die", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn sie gl\u00fccklich ist und macht.", "tokens": ["Wenn", "sie", "gl\u00fcck\u00b7lich", "ist", "und", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Unbedachtsam \u00fcberflie\u00dfet", "tokens": ["Un\u00b7be\u00b7dacht\u00b7sam", "\u00fc\u00b7berf\u00b7lie\u00b7\u00dfet"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur ein Thor von seiner Lust;", "tokens": ["Nur", "ein", "Thor", "von", "sei\u00b7ner", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ein kluger Hirt verschlie\u00dfet", "tokens": ["Doch", "ein", "klu\u00b7ger", "Hirt", "ver\u00b7schlie\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Selbst den Wunsch in tiefer Brust.", "tokens": ["Selbst", "den", "Wunsch", "in", "tie\u00b7fer", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Rein und hei\u00df sind meine Triebe;", "tokens": ["Rein", "und", "hei\u00df", "sind", "mei\u00b7ne", "Trie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ewig, ewig bin ich dein,", "tokens": ["E\u00b7wig", ",", "e\u00b7wig", "bin", "ich", "dein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VAFIN", "PPER", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sage dir da\u00df ich dich liebe,", "tokens": ["Sa\u00b7ge", "dir", "da\u00df", "ich", "dich", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber \u2013 sag' es dir allein.", "tokens": ["A\u00b7ber", "\u2013", "sag'", "es", "dir", "al\u00b7lein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}