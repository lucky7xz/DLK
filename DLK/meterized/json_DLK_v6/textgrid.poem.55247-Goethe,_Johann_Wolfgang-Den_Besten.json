{"textgrid.poem.55247": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Den Besten", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Abgeschiednen betracht ich gern,", "tokens": ["Die", "Ab\u00b7ge\u00b7schied\u00b7nen", "be\u00b7tracht", "ich", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "St\u00fcnd ihr Verdienst auch noch so fern;", "tokens": ["St\u00fcnd", "ihr", "Ver\u00b7dienst", "auch", "noch", "so", "fern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Doch mit den edlen lebendigen Neuen", "tokens": ["Doch", "mit", "den", "ed\u00b7len", "le\u00b7ben\u00b7di\u00b7gen", "Neu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+--+-+---+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Mag ich wetteifernd mich lieber freuen.", "tokens": ["Mag", "ich", "wett\u00b7ei\u00b7fernd", "mich", "lie\u00b7ber", "freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}