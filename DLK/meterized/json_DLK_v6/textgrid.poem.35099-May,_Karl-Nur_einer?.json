{"textgrid.poem.35099": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Nur einer?", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es leuchtete in tr\u00fcber Nacht", "tokens": ["Es", "leuch\u00b7te\u00b7te", "in", "tr\u00fc\u00b7ber", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Himmel einsam mir ein Stern,", "tokens": ["Vom", "Him\u00b7mel", "ein\u00b7sam", "mir", "ein", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ich, auch tr\u00fcb, ich hab gedacht,", "tokens": ["Und", "ich", ",", "auch", "tr\u00fcb", ",", "ich", "hab", "ge\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADV", "ADJD", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die andern seien mir so fern.", "tokens": ["Die", "an\u00b7dern", "sei\u00b7en", "mir", "so", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da l\u00e4chelte mit hellem Strahl", "tokens": ["Da", "l\u00e4\u00b7chel\u00b7te", "mit", "hel\u00b7lem", "Strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er mir die ganze Wahrheit zu", "tokens": ["Er", "mir", "die", "gan\u00b7ze", "Wahr\u00b7heit", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPER", "ART", "ADJA", "NN", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und brachte meines Zweifels Qual", "tokens": ["Und", "brach\u00b7te", "mei\u00b7nes", "Zwei\u00b7fels", "Qual"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit seinem Himmelsgru\u00df zur Ruh:", "tokens": ["Mit", "sei\u00b7nem", "Him\u00b7mels\u00b7gru\u00df", "zur", "Ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbes gl\u00e4nzen Millionen hier", "tokens": ["\u00bb", "es", "gl\u00e4n\u00b7zen", "Mil\u00b7lion\u00b7en", "hier"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "ADJA", "NN", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ganz in demselben Himmelslicht", "tokens": ["Ganz", "in", "dem\u00b7sel\u00b7ben", "Him\u00b7mels\u00b7licht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und in derselben Liebe dir,", "tokens": ["Und", "in", "der\u00b7sel\u00b7ben", "Lie\u00b7be", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Auge aber sieht es nicht!\u00ab", "tokens": ["Dein", "Au\u00b7ge", "a\u00b7ber", "sieht", "es", "nicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}