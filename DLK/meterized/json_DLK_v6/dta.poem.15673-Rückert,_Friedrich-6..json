{"dta.poem.15673": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Mutter, die dem Kind nicht selber Nahrung schenkt,", "tokens": ["Die", "Mut\u00b7ter", ",", "die", "dem", "Kind", "nicht", "sel\u00b7ber", "Nah\u00b7rung", "schenkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PTKNEG", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beneide nur die Brust der Amme, die es tr\u00e4nkt;", "tokens": ["Be\u00b7nei\u00b7de", "nur", "die", "Brust", "der", "Am\u00b7me", ",", "die", "es", "tr\u00e4nkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die f\u00fcr den ersten Quell des Lebens, den sie beut,", "tokens": ["Die", "f\u00fcr", "den", "ers\u00b7ten", "Quell", "des", "Le\u00b7bens", ",", "den", "sie", "beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom ersten L\u00e4cheln auch des Dankes wird erfreut.", "tokens": ["Vom", "ers\u00b7ten", "L\u00e4\u00b7cheln", "auch", "des", "Dan\u00b7kes", "wird", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So mag dem Vater auch, der selbst sein Kind nicht zieht,", "tokens": ["So", "mag", "dem", "Va\u00b7ter", "auch", ",", "der", "selbst", "sein", "Kind", "nicht", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "$,", "PRELS", "ADV", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der wecken Eifersucht, durch welchen es geschieht;", "tokens": ["Der", "we\u00b7cken", "Ei\u00b7fer\u00b7sucht", ",", "durch", "wel\u00b7chen", "es", "ge\u00b7schieht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PWAT", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der ihm ein geistiges Gepr\u00e4ge dr\u00fccket ein,", "tokens": ["Der", "ihm", "ein", "geis\u00b7ti\u00b7ges", "Ge\u00b7pr\u00e4\u00b7ge", "dr\u00fc\u00b7cket", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das wichtiger doch ist, als das von Fleisch und Bein.", "tokens": ["Das", "wich\u00b7ti\u00b7ger", "doch", "ist", ",", "als", "das", "von", "Fleisch", "und", "Bein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VAFIN", "$,", "KOUS", "ART", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}