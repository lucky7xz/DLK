{"textgrid.poem.62548": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Denk' ich deiner bittern Leiden,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denk' ich deiner bittern Leiden,", "tokens": ["Denk'", "ich", "dei\u00b7ner", "bit\u00b7tern", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und was du f\u00fcr mich gethan,", "tokens": ["Und", "was", "du", "f\u00fcr", "mich", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O, wie wenig gehn die Freuden", "tokens": ["O", ",", "wie", "we\u00b7nig", "gehn", "die", "Freu\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "PIS", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser armen Welt mich an!", "tokens": ["Die\u00b7ser", "ar\u00b7men", "Welt", "mich", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kann wol irdisches Vergn\u00fcgen", "tokens": ["Kann", "wol", "ir\u00b7di\u00b7sches", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ein entz\u00fcndet Herz besiegen,", "tokens": ["Ein", "ent\u00b7z\u00fcn\u00b7det", "Herz", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Das allein in deinen Wunden,", "tokens": ["Das", "al\u00b7lein", "in", "dei\u00b7nen", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seinen Schmerz und Trost gefunden?", "tokens": ["Sei\u00b7nen", "Schmerz", "und", "Trost", "ge\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}