{"textgrid.poem.41103": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Gram des Wissens", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh' ich hinaus ins Freie,", "tokens": ["Geh'", "ich", "hin\u00b7aus", "ins", "Frei\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo still die Blume bl\u00fcht,", "tokens": ["Wo", "still", "die", "Blu\u00b7me", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wo durch \u00c4thers Bl\u00e4ue", "tokens": ["Und", "wo", "durch", "\u00c4\u00b7thers", "Bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Vogel singend zieht,", "tokens": ["Der", "Vo\u00b7gel", "sin\u00b7gend", "zieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo Fischlein in dem Spiegel", "tokens": ["Wo", "Fisc\u00b7hlein", "in", "dem", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Quelle wonnig tut", "tokens": ["Der", "Quel\u00b7le", "won\u00b7nig", "tut"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und auf bemoostem H\u00fcgel", "tokens": ["Und", "auf", "be\u00b7moos\u00b7tem", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Sch\u00e4fer fl\u00f6tend ruht:", "tokens": ["Der", "Sch\u00e4\u00b7fer", "fl\u00f6\u00b7tend", "ruht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dann m\u00f6cht' ich aus mich weinen", "tokens": ["Dann", "m\u00f6cht'", "ich", "aus", "mich", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Busen der Natur,", "tokens": ["Am", "Bu\u00b7sen", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Auen und den Hainen", "tokens": ["Den", "Au\u00b7en", "und", "den", "Hai\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Laut sagen alles nur.", "tokens": ["Laut", "sa\u00b7gen", "al\u00b7les", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PIS", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "M\u00f6cht' sagen: Hab' Erbarmen,", "tokens": ["M\u00f6cht'", "sa\u00b7gen", ":", "Hab'", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$.", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Natur! o Mutter du!", "tokens": ["Na\u00b7tur", "!", "o", "Mut\u00b7ter", "du", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "FM", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zum Fischlein mach' mich Armen,", "tokens": ["Zum", "Fisc\u00b7hlein", "mach'", "mich", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur Blume voller Ruh',", "tokens": ["Zur", "Blu\u00b7me", "vol\u00b7ler", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zum Vogel, dessen Schwingen", "tokens": ["Zum", "Vo\u00b7gel", ",", "des\u00b7sen", "Schwin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durchwehen Luft und Lied,", "tokens": ["Durch\u00b7we\u00b7hen", "Luft", "und", "Lied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Dank dir zuzusingen,", "tokens": ["Der", "Dank", "dir", "zu\u00b7zu\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Natur! wird nimmer m\u00fcd.", "tokens": ["Na\u00b7tur", "!", "wird", "nim\u00b7mer", "m\u00fcd", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch soll ein Mensch ich bleiben", "tokens": ["Doch", "soll", "ein", "Mensch", "ich", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nimm 's Wissen mir zuvor;", "tokens": ["Nimm", "'s", "Wis\u00b7sen", "mir", "zu\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann lehr' mich Sch\u00e4flein treiben", "tokens": ["Dann", "lehr'", "mich", "Sch\u00e4f\u00b7lein", "trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und fl\u00f6ten auf dem Rohr.", "tokens": ["Und", "fl\u00f6\u00b7ten", "auf", "dem", "Rohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Will tun in jedem St\u00fccke,", "tokens": ["Will", "tun", "in", "je\u00b7dem", "St\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Natur! wie dir's gef\u00e4llt,", "tokens": ["Na\u00b7tur", "!", "wie", "dir's", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "PIS", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur sto\u00df mich nicht zur\u00fccke", "tokens": ["Nur", "sto\u00df", "mich", "nicht", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ins Wissen dieser Welt.", "tokens": ["Ins", "Wis\u00b7sen", "die\u00b7ser", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}