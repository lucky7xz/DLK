{"textgrid.poem.56799": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Unterm Schirme, tief im Tann,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unterm Schirme, tief im Tann,", "tokens": ["Un\u00b7term", "Schir\u00b7me", ",", "tief", "im", "Tann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "hab ich heut gelegen,", "tokens": ["hab", "ich", "heut", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "durch die schweren Zweige rann", "tokens": ["durch", "die", "schwe\u00b7ren", "Zwei\u00b7ge", "rann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "reicher Sommerregen.", "tokens": ["rei\u00b7cher", "Som\u00b7mer\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Pl\u00f6tzlich rauscht das nasse Gras \u2013", "tokens": ["Pl\u00f6tz\u00b7lich", "rauscht", "das", "nas\u00b7se", "Gras", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "stille! nicht gemuckt! \u2013:", "tokens": ["stil\u00b7le", "!", "nicht", "ge\u00b7muckt", "!", "\u2013", ":"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "$.", "PTKNEG", "VVPP", "$.", "$(", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mir zur Seite duckt", "tokens": ["Mir", "zur", "Sei\u00b7te", "duckt"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "sich ein junger Has ...", "tokens": ["sich", "ein", "jun\u00b7ger", "Has", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "$("], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Dummes H\u00e4schen,", "tokens": ["Dum\u00b7mes", "H\u00e4\u00b7schen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "bist du blind?", "tokens": ["bist", "du", "blind", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Hat dein N\u00e4schen", "tokens": ["Hat", "dein", "N\u00e4\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "keinen Wind?", "tokens": ["kei\u00b7nen", "Wind", "?"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Doch das H\u00e4schen, unbewegt,", "tokens": ["Doch", "das", "H\u00e4\u00b7schen", ",", "un\u00b7be\u00b7wegt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "nutzt, was ihm beschieden,", "tokens": ["nutzt", ",", "was", "ihm", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ohren, weit zur\u00fcckgelegt,", "tokens": ["Oh\u00b7ren", ",", "weit", "zu\u00b7r\u00fcck\u00b7ge\u00b7legt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Miene, schlau zufrieden.", "tokens": ["Mie\u00b7ne", ",", "schlau", "zu\u00b7frie\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Ohne Atem lieg ich fast,", "tokens": ["Oh\u00b7ne", "A\u00b7tem", "lieg", "ich", "fast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "la\u00df die M\u00fccken sitzen;", "tokens": ["la\u00df", "die", "M\u00fc\u00b7cken", "sit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "still besieht mein kleiner Gast", "tokens": ["still", "be\u00b7sieht", "mein", "klei\u00b7ner", "Gast"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "meine Stiefelspitzen ...", "tokens": ["mei\u00b7ne", "Stie\u00b7fel\u00b7spit\u00b7zen", "..."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Um uns beide \u2013 tropf \u2013 tropf \u2013 tropf \u2013", "tokens": ["Um", "uns", "bei\u00b7de", "\u2013", "tropf", "\u2013", "tropf", "\u2013", "tropf", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUI", "PPER", "PIS", "$(", "XY", "$(", "XY", "$(", "XY", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "traut eint\u00f6nig Rauschen ...", "tokens": ["traut", "ein\u00b7t\u00f6\u00b7nig", "Rau\u00b7schen", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Auf dem Schirmdach \u2013 klopf \u2013 klopf \u2013 klopf ...", "tokens": ["Auf", "dem", "Schirm\u00b7dach", "\u2013", "klopf", "\u2013", "klopf", "\u2013", "klopf", "..."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "VVFIN", "$(", "VVFIN", "$(", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wir lauschen ... lauschen ...", "tokens": ["Und", "wir", "lau\u00b7schen", "...", "lau\u00b7schen", "..."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$(", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Wunderw\u00fcrzig kommt ein Duft", "tokens": ["Wun\u00b7der\u00b7w\u00fcr\u00b7zig", "kommt", "ein", "Duft"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "durch den Wald geflogen;", "tokens": ["durch", "den", "Wald", "ge\u00b7flo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "H\u00e4schen schnubbert in die Luft,", "tokens": ["H\u00e4\u00b7schen", "schnub\u00b7bert", "in", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "f\u00fchlt sich fortgezogen;", "tokens": ["f\u00fchlt", "sich", "fort\u00b7ge\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "schiebt gem\u00e4chlich seitw\u00e4rts, macht", "tokens": ["schiebt", "ge\u00b7m\u00e4ch\u00b7lich", "seit\u00b7w\u00e4rts", ",", "macht"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADJD", "ADV", "$,", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00e4nnchen aller Ecken ...", "tokens": ["M\u00e4nn\u00b7chen", "al\u00b7ler", "E\u00b7cken", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Herzlich hab ich aufgelacht \u2013:", "tokens": ["Herz\u00b7lich", "hab", "ich", "auf\u00b7ge\u00b7lacht", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVPP", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ei! der wilde Schrecken!", "tokens": ["Ei", "!", "der", "wil\u00b7de", "Schre\u00b7cken", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}