{"dta.poem.5681": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von GOTT.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer h\u00e4tt\u2019 in uns von JHM ein Denck-Mahl stifften", "tokens": ["Wer", "h\u00e4tt'", "in", "uns", "von", "JhM", "ein", "Den\u00b7ck\u00b7Mahl", "stiff\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "PPER", "APPR", "NE", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und woher hat es kommen k\u00f6nnen?", "tokens": ["Und", "wo\u00b7her", "hat", "es", "kom\u00b7men", "k\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00e4tt ER in unsern Geist, der von so engen Schrancken,", "tokens": ["H\u00e4tt", "Er", "in", "un\u00b7sern", "Geist", ",", "der", "von", "so", "en\u00b7gen", "Schran\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "PRELS", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vom ", "tokens": ["Vom"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Unendlichen, Vollkommenen,", "tokens": ["Un\u00b7end\u00b7li\u00b7chen", ",", "Voll\u00b7kom\u00b7me\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Nicht selbst geleget die Gedancken,", "tokens": ["Nicht", "selbst", "ge\u00b7le\u00b7get", "die", "Ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Worauf ein Mensch sonst nimmermehr gedacht.", "tokens": ["Wo\u00b7rauf", "ein", "Mensch", "sonst", "nim\u00b7mer\u00b7mehr", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Denn es sey fern, zu dencken, da\u00df die Pracht", "tokens": ["Denn", "es", "sey", "fern", ",", "zu", "den\u00b7cken", ",", "da\u00df", "die", "Pracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$,", "PTKZU", "VVINF", "$,", "KOUS", "ART", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Des Allerherrlichsten, indem es uns sich zeiget", "tokens": ["Des", "Al\u00b7ler\u00b7herr\u00b7lichs\u00b7ten", ",", "in\u00b7dem", "es", "uns", "sich", "zei\u00b7get"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als wie gef\u00fcgt, geschm\u00fcckt, vereinet und vermehrt,", "tokens": ["Als", "wie", "ge\u00b7f\u00fcgt", ",", "ge\u00b7schm\u00fcckt", ",", "ver\u00b7ei\u00b7net", "und", "ver\u00b7mehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "VVPP", "$,", "VVPP", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Von dem, was lange nicht zu solcher H\u00f6he steiget,", "tokens": ["Von", "dem", ",", "was", "lan\u00b7ge", "nicht", "zu", "sol\u00b7cher", "H\u00f6\u00b7he", "stei\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADV", "PTKNEG", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dadurch abh\u00e4ngig sey gemacht.", "tokens": ["Da\u00b7durch", "ab\u00b7h\u00e4n\u00b7gig", "sey", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "VAFIN", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Last uns im Gegentheil ganz fest versichert gl\u00e4uben,", "tokens": ["Last", "uns", "im", "Ge\u00b7gen\u00b7theil", "ganz", "fest", "ver\u00b7si\u00b7chert", "gl\u00e4u\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "ADJD", "VVPP", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.14": {"text": "Es habe die Jdee der Vollenkommenheit", "tokens": ["Es", "ha\u00b7be", "die", "Jdee", "der", "Vol\u00b7len\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Die allerwahrste W\u00fcrcklichkeit,", "tokens": ["Die", "al\u00b7ler\u00b7wahrs\u00b7te", "W\u00fcrck\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wovon der mindern nur umschr\u00e4nckte Z\u00fcge bleiben.", "tokens": ["Wo\u00b7von", "der", "min\u00b7dern", "nur", "um\u00b7schr\u00e4nck\u00b7te", "Z\u00fc\u00b7ge", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dieweil der Mensch nur elend ist und th\u00f6rlich,", "tokens": ["Die\u00b7weil", "der", "Mensch", "nur", "e\u00b7lend", "ist", "und", "th\u00f6r\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "VAFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Macht er ja keinen GOTT ", "tokens": ["Macht", "er", "ja", "kei\u00b7nen", "GoTT"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "PIAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.19": {"text": "Die Z\u00fcge, so von JHM in unsern Seelen stehn,", "tokens": ["Die", "Z\u00fc\u00b7ge", ",", "so", "von", "JhM", "in", "un\u00b7sern", "See\u00b7len", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "NE", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die haben wir nicht anders anzusehn", "tokens": ["Die", "ha\u00b7ben", "wir", "nicht", "an\u00b7ders", "an\u00b7zu\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "ADV", "VVIZU"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Als eine wesentliche Wahrheit.", "tokens": ["Als", "ei\u00b7ne", "we\u00b7sent\u00b7li\u00b7che", "Wahr\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Da\u00df unser Geist sein\u2019 eigne Schwachheit kennt,", "tokens": ["Da\u00df", "un\u00b7ser", "Geist", "sein'", "eig\u00b7ne", "Schwach\u00b7heit", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Und sich nicht frey vom Jrrthum nennt;", "tokens": ["Und", "sich", "nicht", "frey", "vom", "Jrr\u00b7thum", "nennt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PTKNEG", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Kommt blo\u00df daher,", "tokens": ["Kommt", "blo\u00df", "da\u00b7her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PAV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.25": {"text": "Da\u00df er in GOTT erkennt der ew\u2019gen Wei\u00dfheit Meer;", "tokens": ["Da\u00df", "er", "in", "GoTT", "er\u00b7kennt", "der", "ew'\u00b7gen", "Wei\u00df\u00b7heit", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Woraus die w\u00fcrckliche Jdee", "tokens": ["Wo\u00b7raus", "die", "w\u00fcrck\u00b7li\u00b7che", "Jdee"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.27": {"text": "In ihn gegossen war. Nach diesem Muster seh", "tokens": ["In", "ihn", "ge\u00b7gos\u00b7sen", "war", ".", "Nach", "die\u00b7sem", "Mus\u00b7ter", "seh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVPP", "VAFIN", "$.", "APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ich meine grob\u2019 umschr\u00e4nckt\u2019 und schw\u00e4chliche Natur.", "tokens": ["Ich", "mei\u00b7ne", "grob'", "um\u00b7schr\u00e4n\u00b7ckt'", "und", "schw\u00e4ch\u00b7li\u00b7che", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "VVPP", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Bey der Vollkommenheit kennt man die M\u00e4ngel nur.", "tokens": ["Bey", "der", "Voll\u00b7kom\u00b7men\u00b7heit", "kennt", "man", "die", "M\u00e4n\u00b7gel", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIS", "ART", "NN", "ADV", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.30": {"text": "Wofern kein w\u00fcrcklichs Gut; w\u00e4r dessen Mangel nimmer.", "tokens": ["Wo\u00b7fern", "kein", "w\u00fcrck\u00b7lichs", "Gut", ";", "w\u00e4r", "des\u00b7sen", "Man\u00b7gel", "nim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "$.", "VAFIN", "PDS", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Es giebt sonst keine Finsternissen,", "tokens": ["Es", "giebt", "sonst", "kei\u00b7ne", "Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Als wenn wir nichts von Lichtes Schimmer", "tokens": ["Als", "wenn", "wir", "nichts", "von", "Lich\u00b7tes", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "PIS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Und seinen hellen Strahlen wissen.", "tokens": ["Und", "sei\u00b7nen", "hel\u00b7len", "Strah\u00b7len", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}