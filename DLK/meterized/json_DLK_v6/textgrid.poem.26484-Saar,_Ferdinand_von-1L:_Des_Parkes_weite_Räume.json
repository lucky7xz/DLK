{"textgrid.poem.26484": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Des Parkes weite R\u00e4ume", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Parkes weite R\u00e4ume", "tokens": ["Des", "Par\u00b7kes", "wei\u00b7te", "R\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Umflort die stille Nacht;", "tokens": ["Um\u00b7flort", "die", "stil\u00b7le", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es steh'n die alten B\u00e4ume", "tokens": ["Es", "steh'n", "die", "al\u00b7ten", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In d\u00fcst'rer Wipfelpracht.", "tokens": ["In", "d\u00fcst'\u00b7rer", "Wip\u00b7fel\u00b7pracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Pfade wie versunken,", "tokens": ["Die", "Pfa\u00b7de", "wie", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Himmel nicht ein Stern;", "tokens": ["Am", "Him\u00b7mel", "nicht", "ein", "Stern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verstummt ist schlummertrunken", "tokens": ["Ver\u00b7stummt", "ist", "schlum\u00b7mer\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Leben nah und fern.", "tokens": ["Das", "Le\u00b7ben", "nah", "und", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So m\u00fcd', so nachtumfangen,", "tokens": ["So", "m\u00fcd'", ",", "so", "nach\u00b7tum\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So lautlos bist auch du,", "tokens": ["So", "laut\u00b7los", "bist", "auch", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als w\u00e4rst du eingegangen", "tokens": ["Als", "w\u00e4rst", "du", "ein\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schon l\u00e4ngst zur ew'gen Ruh'.", "tokens": ["Schon", "l\u00e4ngst", "zur", "ew'\u00b7gen", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nur wie im Dunkel bl\u00fchen", "tokens": ["Nur", "wie", "im", "Dun\u00b7kel", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Rosen dort am Strauch,", "tokens": ["Die", "Ro\u00b7sen", "dort", "am", "Strauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Will leis' dich noch durchgl\u00fchen", "tokens": ["Will", "leis'", "dich", "noch", "durch\u00b7gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "VVIMP", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein letzter Liebeshauch.", "tokens": ["Ein", "letz\u00b7ter", "Lie\u00b7bes\u00b7hauch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}