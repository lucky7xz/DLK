{"dta.poem.19178": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gesundes Auge sieht, es h\u00f6rt gesundes Ohr", "tokens": ["Ge\u00b7sun\u00b7des", "Au\u00b7ge", "sieht", ",", "es", "h\u00f6rt", "ge\u00b7sun\u00b7des", "Ohr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch Kraft von innen das was au\u00dfen ist davor.", "tokens": ["Durch", "Kraft", "von", "in\u00b7nen", "das", "was", "au\u00b7\u00dfen", "ist", "da\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADV", "PDS", "PRELS", "ADV", "VAFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch wird der Sinn sich selbst zum Gegenstand, ein Graus", "tokens": ["Doch", "wird", "der", "Sinn", "sich", "selbst", "zum", "Ge\u00b7gen\u00b7stand", ",", "ein", "Graus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "PRF", "ADV", "APPRART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist kranken Augs Gefunk und kranken Ohrs Gebraus.", "tokens": ["Ist", "kran\u00b7ken", "Augs", "Ge\u00b7funk", "und", "kran\u00b7ken", "Ohrs", "Ge\u00b7braus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "NN", "KON", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So sei zum Gegenstand die Au\u00dfenwelt verliehn", "tokens": ["So", "sei", "zum", "Ge\u00b7gen\u00b7stand", "die", "Au\u00b7\u00dfen\u00b7welt", "ver\u00b7liehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gesunder Fantasie, nicht kranke Fantasien.", "tokens": ["Ge\u00b7sun\u00b7der", "Fan\u00b7ta\u00b7sie", ",", "nicht", "kran\u00b7ke", "Fan\u00b7ta\u00b7si\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}}}}}