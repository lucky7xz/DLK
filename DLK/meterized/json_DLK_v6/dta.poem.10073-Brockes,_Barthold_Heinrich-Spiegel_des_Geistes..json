{"dta.poem.10073": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Spiegel des Geistes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie die Augen alle Dinge, doch sich selber anders nicht,", "tokens": ["Wie", "die", "Au\u00b7gen", "al\u00b7le", "Din\u00b7ge", ",", "doch", "sich", "sel\u00b7ber", "an\u00b7ders", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PIAT", "NN", "$,", "ADV", "PRF", "ADV", "ADV", "PTKNEG", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Als im Spiegel, sehen k\u00f6nnen; also kann der Geist,", "tokens": ["Als", "im", "Spie\u00b7gel", ",", "se\u00b7hen", "k\u00f6n\u00b7nen", ";", "al\u00b7so", "kann", "der", "Geist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "$,", "VVINF", "VMINF", "$.", "ADV", "VMFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "allein", "tokens": ["al\u00b7lein"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "In der Creaturen Spiegel, seine Krafft und wahres Seyn", "tokens": ["In", "der", "Crea\u00b7tu\u00b7ren", "Spie\u00b7gel", ",", "sei\u00b7ne", "Krafft", "und", "wah\u00b7res", "Seyn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "PPOSAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Sehn, begreiffen und erkennen: Ja wie wir, nicht nur", "tokens": ["Sehn", ",", "be\u00b7greif\u00b7fen", "und", "er\u00b7ken\u00b7nen", ":", "Ja", "wie", "wir", ",", "nicht", "nur"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "VVPP", "KON", "VVINF", "$.", "PTKANT", "KON", "PPER", "$,", "PTKNEG", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "durchs Licht,", "tokens": ["durchs", "Licht", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Sondern selbst das Licht im Spiegel sehen; also werden", "tokens": ["Son\u00b7dern", "selbst", "das", "Licht", "im", "Spie\u00b7gel", "se\u00b7hen", ";", "al\u00b7so", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "APPRART", "NN", "VVINF", "$.", "ADV", "VAFIN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "wir", "tokens": ["wir"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Nicht nur, da\u00df wir durch den Sch\u00f6pfer alles sehen; son-", "tokens": ["Nicht", "nur", ",", "da\u00df", "wir", "durch", "den", "Sch\u00f6p\u00b7fer", "al\u00b7les", "se\u00b7hen", ";", "son"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PTKNEG", "ADV", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "PIS", "VVINF", "$.", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.10": {"text": "dern gar", "tokens": ["dern", "gar"], "token_info": ["word", "word"], "pos": ["PDS", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "In der Wercke Spiegel, Seiner, als der Licht-Quell selbst,", "tokens": ["In", "der", "Wer\u00b7cke", "Spie\u00b7gel", ",", "Sei\u00b7ner", ",", "als", "der", "Licht\u00b7Quell", "selbst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "PPOSAT", "$,", "KOUS", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-++-", "measure": "unknown.measure.septa"}, "line.12": {"text": "gewahr.", "tokens": ["ge\u00b7wahr", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}}}}