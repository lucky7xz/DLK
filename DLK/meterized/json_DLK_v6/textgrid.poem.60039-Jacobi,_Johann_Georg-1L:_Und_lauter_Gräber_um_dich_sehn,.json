{"textgrid.poem.60039": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und lauter Gr\u00e4ber um dich sehn,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und lauter Gr\u00e4ber um dich sehn,", "tokens": ["Und", "lau\u00b7ter", "Gr\u00e4\u00b7ber", "um", "dich", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und zwischen Knochenh\u00e4usern gehn,", "tokens": ["Und", "zwi\u00b7schen", "Kno\u00b7chen\u00b7h\u00e4u\u00b7sern", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Einher auf Leichensteinen kriechen,", "tokens": ["Ein\u00b7her", "auf", "Lei\u00b7chen\u00b7stei\u00b7nen", "krie\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihre Todtend\u00fcfte riechen.", "tokens": ["Und", "ih\u00b7re", "Tod\u00b7ten\u00b7d\u00fcf\u00b7te", "rie\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es m\u00f6chten sich zu dir Gespenster wagen,", "tokens": ["Es", "m\u00f6ch\u00b7ten", "sich", "zu", "dir", "Ge\u00b7spens\u00b7ter", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den Kopf in blassen H\u00e4nden tragen,", "tokens": ["Den", "Kopf", "in", "blas\u00b7sen", "H\u00e4n\u00b7den", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und f\u00fcrchterlich, im Mondenschein,", "tokens": ["Und", "f\u00fcrch\u00b7ter\u00b7lich", ",", "im", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit ihrer stumpfen Stimme schreyn.", "tokens": ["Mit", "ih\u00b7rer", "stump\u00b7fen", "Stim\u00b7me", "schreyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Es m\u00f6chten Teufelchen, in schwarzgemahlten Kappen,", "tokens": ["Es", "m\u00f6ch\u00b7ten", "Teu\u00b7fel\u00b7chen", ",", "in", "schwarz\u00b7ge\u00b7mahl\u00b7ten", "Kap\u00b7pen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Um dich herum die Z\u00e4hne klappen;", "tokens": ["Um", "dich", "he\u00b7rum", "die", "Z\u00e4h\u00b7ne", "klap\u00b7pen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APZR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Du s\u00e4hst, auf raschen Flederm\u00e4usen,", "tokens": ["Du", "s\u00e4hst", ",", "auf", "ra\u00b7schen", "Fle\u00b7der\u00b7m\u00e4u\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sie pr\u00e4chtig durch die L\u00fcfte reisen:", "tokens": ["Sie", "pr\u00e4ch\u00b7tig", "durch", "die", "L\u00fcf\u00b7te", "rei\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Du s\u00e4hest Hexenmeister, Hexen,", "tokens": ["Du", "s\u00e4\u00b7hest", "He\u00b7xen\u00b7meis\u00b7ter", ",", "He\u00b7xen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Im Phaeton, bespannt mit Sechsen,", "tokens": ["Im", "Phae\u00b7ton", ",", "be\u00b7spannt", "mit", "Sech\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Den Donnerwolken sich befehlen,", "tokens": ["Den", "Don\u00b7ner\u00b7wol\u00b7ken", "sich", "be\u00b7feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Zum Kutscher einen Kobolt w\u00e4hlen,", "tokens": ["Zum", "Kut\u00b7scher", "ei\u00b7nen", "Ko\u00b7bolt", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und Feuerm\u00e4nner, als Heyducken,", "tokens": ["Und", "Feu\u00b7er\u00b7m\u00e4n\u00b7ner", ",", "als", "Hey\u00b7du\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Aus gro\u00dfen Flammenaugen gucken;", "tokens": ["Aus", "gro\u00b7\u00dfen", "Flam\u00b7men\u00b7au\u00b7gen", "gu\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und endlich nickten dir, zur angenehmen Ruh,", "tokens": ["Und", "end\u00b7lich", "nick\u00b7ten", "dir", ",", "zur", "an\u00b7ge\u00b7neh\u00b7men", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die halb entschlafnen Eulen zu.", "tokens": ["Die", "halb", "ent\u00b7schlaf\u00b7nen", "Eu\u00b7len", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du sollst in Rosenlauben gehen,", "tokens": ["Du", "sollst", "in", "Ro\u00b7sen\u00b7lau\u00b7ben", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und lachende Gefilde sehen,", "tokens": ["Und", "la\u00b7chen\u00b7de", "Ge\u00b7fil\u00b7de", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und dich, im stillen Mondenschein,", "tokens": ["Und", "dich", ",", "im", "stil\u00b7len", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Grazien zur Schwester weihn;", "tokens": ["Den", "Gra\u00b7zi\u00b7en", "zur", "Schwes\u00b7ter", "weihn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nur vom Spiel der Amoretten tr\u00e4umen,", "tokens": ["Und", "nur", "vom", "Spiel", "der", "A\u00b7mo\u00b7ret\u00b7ten", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die, unter ihren Mirthenb\u00e4umen,", "tokens": ["Die", ",", "un\u00b7ter", "ih\u00b7ren", "Mir\u00b7then\u00b7b\u00e4u\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sich goldne Schmetterlinge z\u00e4umen,", "tokens": ["Sich", "gold\u00b7ne", "Schmet\u00b7ter\u00b7lin\u00b7ge", "z\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dann \u00fcber sch\u00f6ne Wiesen reiten,", "tokens": ["Dann", "\u00fc\u00b7ber", "sch\u00f6\u00b7ne", "Wie\u00b7sen", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Um Sch\u00e4ferinnen zu begleiten;", "tokens": ["Um", "Sch\u00e4\u00b7fe\u00b7rin\u00b7nen", "zu", "be\u00b7glei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dann, in Violen und Narcissen,", "tokens": ["Dann", ",", "in", "Vi\u00b7o\u00b7len", "und", "Nar\u00b7cis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Verwandelte Najaden k\u00fcssen.", "tokens": ["Ver\u00b7wan\u00b7del\u00b7te", "Na\u00b7ja\u00b7den", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Du sollst in ihrem Lieblingshain,", "tokens": ["Du", "sollst", "in", "ih\u00b7rem", "Lieb\u00b7lings\u00b7hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Der losen Knaben Zeuge seyn,", "tokens": ["Der", "lo\u00b7sen", "Kna\u00b7ben", "Zeu\u00b7ge", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wenn sie, zum Scherze, sich verkappen,", "tokens": ["Wenn", "sie", ",", "zum", "Scher\u00b7ze", ",", "sich", "ver\u00b7kap\u00b7pen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPRART", "NN", "$,", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Ein armes M\u00e4dchen zu ertappen,", "tokens": ["Ein", "ar\u00b7mes", "M\u00e4d\u00b7chen", "zu", "er\u00b7tap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Das auf der Weide Blumen pfl\u00fcckt,", "tokens": ["Das", "auf", "der", "Wei\u00b7de", "Blu\u00b7men", "pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und voller Unschuld sie an seinen Busen dr\u00fcckt.", "tokens": ["Und", "vol\u00b7ler", "Un\u00b7schuld", "sie", "an", "sei\u00b7nen", "Bu\u00b7sen", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}