etra"}}, "stanza.16": {"line.1": {"text": "Nun liefr' ich meines Leibes B\u00fcrde", "tokens": ["Nun", "lie\u00b7fr'", "ich", "mei\u00b7nes", "Lei\u00b7bes", "B\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In Kurtzem in des Todes Hand;", "tokens": ["In", "Kurt\u00b7zem", "in", "des", "To\u00b7des", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Begl\u00fcckt! wenn ich in deinem Sand,", "tokens": ["Be\u00b7gl\u00fcckt", "!", "wenn", "ich", "in", "dei\u00b7nem", "Sand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geliebtes Worms, verscharret w\u00fcrde.", "tokens": ["Ge\u00b7lieb\u00b7tes", "Worms", ",", "ver\u00b7schar\u00b7ret", "w\u00fcr\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mich d\u00fcnckt, da\u00df ich noch eins so wohl", "tokens": ["Mich", "d\u00fcnckt", ",", "da\u00df", "ich", "noch", "eins", "so", "wohl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PIS", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Alsdenn im Grabe ruhen soll.", "tokens": ["Als\u00b7denn", "im", "Gra\u00b7be", "ru\u00b7hen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "So sprach ich, und sah schon von weiten,", "tokens": ["So", "sprach", "ich", ",", "und", "sah", "schon", "von", "wei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "VVFIN", "ADV", "APPR", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Von dieser Erde j\u00e4hem Rand,", "tokens": ["Von", "die\u00b7ser", "Er\u00b7de", "j\u00e4\u00b7hem", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der blassen Schatten stilles Land,", "tokens": ["Der", "blas\u00b7sen", "Schat\u00b7ten", "stil\u00b7les", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das grosse Reich der Ewigkeiten:", "tokens": ["Das", "gros\u00b7se", "Reich", "der", "E\u00b7wig\u00b7kei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Geliebtester, da kamest du,", "tokens": ["Ge\u00b7lieb\u00b7tes\u00b7ter", ",", "da", "ka\u00b7mest", "du", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und mit dir all mein Gl\u00fcck und Ruh \u2013 \u2013 \u2013", "tokens": ["Und", "mit", "dir", "all", "mein", "Gl\u00fcck", "und", "Ruh", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "APPR", "PPER", "PIAT", "PPOSAT", "NN", "KON", "NN", "$