{"textgrid.poem.49484": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "An seinen \u00e4ltesten Bruder", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schreibe nur, was ich empfinde,", "tokens": ["Ich", "schrei\u00b7be", "nur", ",", "was", "ich", "emp\u00b7fin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und dichte, liebster Bruder, nicht.", "tokens": ["Und", "dich\u00b7te", ",", "liebs\u00b7ter", "Bru\u00b7der", ",", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJA", "NN", "$,", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann dieses Lied zu z\u00e4rtlich spricht,", "tokens": ["Wann", "die\u00b7ses", "Lied", "zu", "z\u00e4rt\u00b7lich", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So rechn' es der Natur zur S\u00fcnde.", "tokens": ["So", "rechn'", "es", "der", "Na\u00b7tur", "zur", "S\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Je weiter du entfernet bist,", "tokens": ["Je", "wei\u00b7ter", "du", "ent\u00b7fer\u00b7net", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Je minder dich mein Hertz vergist.", "tokens": ["Je", "min\u00b7der", "dich", "mein", "Hertz", "ver\u00b7gist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Bootsmann blicket noch mit Zittern", "tokens": ["Ein", "Boots\u00b7mann", "bli\u00b7cket", "noch", "mit", "Zit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom sichern Strand aufs hohe Meer,", "tokens": ["Vom", "si\u00b7chern", "Strand", "aufs", "ho\u00b7he", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wo ihm der Fluten st\u00fcrmend Heer,", "tokens": ["Wo", "ihm", "der", "Flu\u00b7ten", "st\u00fcr\u00b7mend", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bey finstrer Nacht und Ungewittern", "tokens": ["Bey", "finst\u00b7rer", "Nacht", "und", "Un\u00b7ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sein schwerbeladnes Schiff umrang,", "tokens": ["Sein", "schwer\u00b7be\u00b7lad\u00b7nes", "Schiff", "um\u00b7rang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am Fels zerschlug, und denn verschlang:", "tokens": ["Am", "Fels", "zer\u00b7schlug", ",", "und", "denn", "ver\u00b7schlang", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So schauert mir auch mein Gebeine,", "tokens": ["So", "schau\u00b7ert", "mir", "auch", "mein", "Ge\u00b7bei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "K\u00f6mmt mir die Kranckheit in den Sinn,", "tokens": ["K\u00f6mmt", "mir", "die", "Kran\u00b7ck\u00b7heit", "in", "den", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wovon ich zwar entbunden bin,", "tokens": ["Wo\u00b7von", "ich", "zwar", "ent\u00b7bun\u00b7den", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch die ich einsam noch beweine,", "tokens": ["Doch", "die", "ich", "ein\u00b7sam", "noch", "be\u00b7wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "ADJD", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Weil sie des Leibes Marck und Kraft,", "tokens": ["Weil", "sie", "des", "Lei\u00b7bes", "Marck", "und", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nebst meiner Jugend hingeraft.", "tokens": ["Nebst", "mei\u00b7ner", "Ju\u00b7gend", "hin\u00b7ge\u00b7raft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie kam in Nerven und Gelencke,", "tokens": ["Sie", "kam", "in", "Ner\u00b7ven", "und", "Ge\u00b7len\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vertrocknete der Adern Blut,", "tokens": ["Ver\u00b7trock\u00b7ne\u00b7te", "der", "A\u00b7dern", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und trotzte mit vermehrter Wuth", "tokens": ["Und", "trotz\u00b7te", "mit", "ver\u00b7mehr\u00b7ter", "Wuth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Artztes edlen Kr\u00e4utertr\u00e4ncke.", "tokens": ["Des", "Artz\u00b7tes", "ed\u00b7len", "Kr\u00e4u\u00b7ter\u00b7tr\u00e4n\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ach! rief man bald mitleidig aus,", "tokens": ["Ach", "!", "rief", "man", "bald", "mit\u00b7lei\u00b7dig", "aus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PIS", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erschrick nicht, und bestell dein Haus.", "tokens": ["Er\u00b7schrick", "nicht", ",", "und", "be\u00b7stell", "dein", "Haus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "$,", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich thats, und schwieg in meinen Schmertzen;", "tokens": ["Ich", "thats", ",", "und", "schwieg", "in", "mei\u00b7nen", "Schmert\u00b7zen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Gnade st\u00e4rckte den Verstand;", "tokens": ["Die", "Gna\u00b7de", "st\u00e4rck\u00b7te", "den", "Ver\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch lag mir noch mein Vaterland,", "tokens": ["Doch", "lag", "mir", "noch", "mein", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nebst meinem Sch\u00f6pfer, nah am Hertzen,", "tokens": ["Nebst", "mei\u00b7nem", "Sch\u00f6p\u00b7fer", ",", "nah", "am", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und jeder Freund den ich verlohr,", "tokens": ["Und", "je\u00b7der", "Freund", "den", "ich", "ver\u00b7lohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kam mir in der Verwirrung vor.", "tokens": ["Kam", "mir", "in", "der", "Ver\u00b7wir\u00b7rung", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da sah und gr\u00fcst ich meine Br\u00fcder,", "tokens": ["Da", "sah", "und", "gr\u00fcst", "ich", "mei\u00b7ne", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und gl\u00e4ubte, da\u00df ich sie umfieng.", "tokens": ["Und", "gl\u00e4ub\u00b7te", ",", "da\u00df", "ich", "sie", "um\u00b7fi\u00b7eng", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch wenn die Phantasie vergieng,", "tokens": ["Doch", "wenn", "die", "Phan\u00b7ta\u00b7sie", "ver\u00b7gieng", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach! so verschwanden sie auch wieder;", "tokens": ["Ach", "!", "so", "ver\u00b7schwan\u00b7den", "sie", "auch", "wie\u00b7der", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dies machte, da\u00df in meinen Schoos", "tokens": ["Dies", "mach\u00b7te", ",", "da\u00df", "in", "mei\u00b7nen", "Schoos"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Strom von bittern Z\u00e4hren flos.", "tokens": ["Ein", "Strom", "von", "bit\u00b7tern", "Z\u00e4h\u00b7ren", "flos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sprach man denn, mich vergn\u00fcgt zu machen,", "tokens": ["Sprach", "man", "denn", ",", "mich", "ver\u00b7gn\u00fcgt", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "ADV", "$,", "PPER", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie haben sich vielleicht versteckt,", "tokens": ["Sie", "ha\u00b7ben", "sich", "viel\u00b7leicht", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und werden, wenn der Morgen weckt,", "tokens": ["Und", "wer\u00b7den", ",", "wenn", "der", "Mor\u00b7gen", "weckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAINF", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir anmuthsvoll entgegen lachen;", "tokens": ["Dir", "an\u00b7muths\u00b7voll", "ent\u00b7ge\u00b7gen", "la\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So wacht ich, bis der Morgen kam,", "tokens": ["So", "wacht", "ich", ",", "bis", "der", "Mor\u00b7gen", "kam", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und fand sie nicht, und schlief f\u00fcr Gram.", "tokens": ["Und", "fand", "sie", "nicht", ",", "und", "schlief", "f\u00fcr", "Gram", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Hierauf erhub sich erst mein Leiden,", "tokens": ["Hier\u00b7auf", "er\u00b7hub", "sich", "erst", "mein", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weil mirs so denn im Traume schien,", "tokens": ["Weil", "mirs", "so", "denn", "im", "Trau\u00b7me", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie sie im gr\u00fcnen Felde fliehn,", "tokens": ["Wie", "sie", "im", "gr\u00fc\u00b7nen", "Fel\u00b7de", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sonder Abschied von mir scheiden,", "tokens": ["Und", "son\u00b7der", "Ab\u00b7schied", "von", "mir", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ja, auf mein wehmuthvolles Flehn", "tokens": ["Ja", ",", "auf", "mein", "weh\u00b7muth\u00b7vol\u00b7les", "Flehn"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht einmal freundlich r\u00fcckw\u00e4rts sehn.", "tokens": ["Nicht", "ein\u00b7mal", "freund\u00b7lich", "r\u00fcck\u00b7w\u00e4rts", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "So ward mir jeder Tag zur Wochen,", "tokens": ["So", "ward", "mir", "je\u00b7der", "Tag", "zur", "Wo\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und jede Woche wie ein Jahr;", "tokens": ["Und", "je\u00b7de", "Wo\u00b7che", "wie", "ein", "Jahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und was von mir noch \u00fcbrig war,", "tokens": ["Und", "was", "von", "mir", "noch", "\u00fcb\u00b7rig", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "War ein Geribbe d\u00fcrrer Knochen,", "tokens": ["War", "ein", "Ge\u00b7rib\u00b7be", "d\u00fcr\u00b7rer", "Kno\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das sonst nichts mehr vom Leben wies,", "tokens": ["Das", "sonst", "nichts", "mehr", "vom", "Le\u00b7ben", "wies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PIS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als da\u00df es nur noch Athem blies