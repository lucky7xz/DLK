{"textgrid.poem.51267": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Zeitlandschaft", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schimmernd liegt die Bahn im tiefen Tale,", "tokens": ["Schim\u00b7mernd", "liegt", "die", "Bahn", "im", "tie\u00b7fen", "Ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u00dcber Tal und Schienen geht die Br\u00fccke", "tokens": ["\u00dc\u00b7ber", "Tal", "und", "Schie\u00b7nen", "geht", "die", "Br\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hoch hinweg, ein Turm ist jeder Pfeiler,", "tokens": ["Hoch", "hin\u00b7weg", ",", "ein", "Turm", "ist", "je\u00b7der", "Pfei\u00b7ler", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "ART", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Kunstgekr\u00f6net in die L\u00fcfte ragend,", "tokens": ["Kunst\u00b7ge\u00b7kr\u00f6\u00b7net", "in", "die", "L\u00fcf\u00b7te", "ra\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Zu den Wolken weite Bogen tragend.", "tokens": ["Zu", "den", "Wol\u00b7ken", "wei\u00b7te", "Bo\u00b7gen", "tra\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wie ein R\u00f6merwerk, doch neu und gl\u00e4nzend,", "tokens": ["Wie", "ein", "R\u00f6\u00b7mer\u00b7werk", ",", "doch", "neu", "und", "gl\u00e4n\u00b7zend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bindet wald'ge Berge sie zusammen;", "tokens": ["Bin\u00b7det", "wald'\u00b7ge", "Ber\u00b7ge", "sie", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Auf der Br\u00fccke fahren keine Wagen,", "tokens": ["Auf", "der", "Br\u00fc\u00b7cke", "fah\u00b7ren", "kei\u00b7ne", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Denn kristallnes Wasser geht dort oben,", "tokens": ["Denn", "kris\u00b7tall\u00b7nes", "Was\u00b7ser", "geht", "dort", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-++-+-+-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Dessen fromme Flut die Schiffer loben.", "tokens": ["Des\u00b7sen", "from\u00b7me", "Flut", "die", "Schif\u00b7fer", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Unten auf des Tales Eisensohle", "tokens": ["Un\u00b7ten", "auf", "des", "Ta\u00b7les", "Ei\u00b7sen\u00b7soh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schnurrt hindurch der Wagen lange Reihe,", "tokens": ["Schnurrt", "hin\u00b7durch", "der", "Wa\u00b7gen", "lan\u00b7ge", "Rei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hundert unruhvolle Herzen tragend,", "tokens": ["Hun\u00b7dert", "un\u00b7ruh\u00b7vol\u00b7le", "Her\u00b7zen", "tra\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Straff von Nord nach S\u00fcd mit Vogels Schnelle.", "tokens": ["Straff", "von", "Nord", "nach", "S\u00fcd", "mit", "Vo\u00b7gels", "Schnel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "APPR", "NN", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Dr\u00fcber streicht das Fischlein durch die Welle.", "tokens": ["Dr\u00fc\u00b7ber", "streicht", "das", "Fisc\u00b7hlein", "durch", "die", "Wel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Langsam, wie ein Schwan, mit wei\u00dfem Segel,", "tokens": ["Lang\u00b7sam", ",", "wie", "ein", "Schwan", ",", "mit", "wei\u00b7\u00dfem", "Se\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Herrlich auf des Himmels blauem Grunde", "tokens": ["Herr\u00b7lich", "auf", "des", "Him\u00b7mels", "blau\u00b7em", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Oben f\u00e4hrt ein Schiff von Ost nach Westen; \u2013", "tokens": ["O\u00b7ben", "f\u00e4hrt", "ein", "Schiff", "von", "Ost", "nach", "Wes\u00b7ten", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NE", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ruhvoll lehnt der Schiffer an dem Steuer:", "tokens": ["Ruh\u00b7voll", "lehnt", "der", "Schif\u00b7fer", "an", "dem", "Steu\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ist das nicht ein sch\u00f6nes Abenteuer?", "tokens": ["Ist", "das", "nicht", "ein", "sch\u00f6\u00b7nes", "A\u00b7bent\u00b7eu\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}