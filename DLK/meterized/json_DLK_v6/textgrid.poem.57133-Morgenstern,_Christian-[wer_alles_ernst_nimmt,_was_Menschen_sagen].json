{"textgrid.poem.57133": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "[wer alles ernst nimmt, was Menschen sagen]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer alles ernst nimmt, was Menschen sagen,", "tokens": ["Wer", "al\u00b7les", "ernst", "nimmt", ",", "was", "Men\u00b7schen", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADJD", "VVFIN", "$,", "PWS", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "darf sich nicht \u00fcber Menschen beklagen.", "tokens": ["darf", "sich", "nicht", "\u00fc\u00b7ber", "Men\u00b7schen", "be\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Alles Reden ist meist nur Gered.", "tokens": ["Al\u00b7les", "Re\u00b7den", "ist", "meist", "nur", "Ge\u00b7red", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADV", "NE", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Wei\u00df man erst, was dahinter steht,", "tokens": ["Wei\u00df", "man", "erst", ",", "was", "da\u00b7hin\u00b7ter", "steht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,", "PRELS", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "l\u00e4\u00dft man's klappern wie die M\u00fchlen am Bach", "tokens": ["l\u00e4\u00dft", "man's", "klap\u00b7pern", "wie", "die", "M\u00fch\u00b7len", "am", "Bach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "VVINF", "KOKOM", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "und geht stillfein in sein eigen Gemach.", "tokens": ["und", "geht", "still\u00b7fein", "in", "sein", "ei\u00b7gen", "Ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}