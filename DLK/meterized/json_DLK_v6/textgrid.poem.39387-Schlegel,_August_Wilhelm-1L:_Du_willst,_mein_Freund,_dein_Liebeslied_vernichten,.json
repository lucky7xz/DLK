{"textgrid.poem.39387": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du willst, mein Freund, dein Liebeslied vernichten,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du willst, mein Freund, dein Liebeslied vernichten,", "tokens": ["Du", "willst", ",", "mein", "Freund", ",", "dein", "Lie\u00b7bes\u00b7lied", "ver\u00b7nich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil untreu dir seitdem die Sch\u00f6ne war?", "tokens": ["Weil", "un\u00b7treu", "dir", "seit\u00b7dem", "die", "Sch\u00f6\u00b7ne", "war", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "PAV", "ART", "NN", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Da mag der Teufel Liebeslieder dichten!", "tokens": ["Da", "mag", "der", "Teu\u00b7fel", "Lie\u00b7bes\u00b7lie\u00b7der", "dich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein jedes l\u00e4uft die n\u00e4mliche Gefahr.", "tokens": ["Ein", "je\u00b7des", "l\u00e4uft", "die", "n\u00e4m\u00b7li\u00b7che", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Beim Stirnhaar die Gelegenheit erhaschen,", "tokens": ["Beim", "Stirn\u00b7haar", "die", "Ge\u00b7le\u00b7gen\u00b7heit", "er\u00b7ha\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und, schl\u00fcpft sie fort, dich tr\u00f6sten, sei dein Sinn.", "tokens": ["Und", ",", "schl\u00fcpft", "sie", "fort", ",", "dich", "tr\u00f6s\u00b7ten", ",", "sei", "dein", "Sinn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie lie\u00df dich doch mehr oder minder naschen,", "tokens": ["Sie", "lie\u00df", "dich", "doch", "mehr", "o\u00b7der", "min\u00b7der", "na\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "KON", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und die Erinnrung bleibt dir als Gewinn.", "tokens": ["Und", "die", "E\u00b7rinn\u00b7rung", "bleibt", "dir", "als", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Flucht der Jahre l\u00e4\u00dft sich nicht verschieben;", "tokens": ["Die", "Flucht", "der", "Jah\u00b7re", "l\u00e4\u00dft", "sich", "nicht", "ver\u00b7schie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit ihr entflieht die reizende Gestalt.", "tokens": ["Mit", "ihr", "ent\u00b7flieht", "die", "rei\u00b7zen\u00b7de", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Willst du sie denn auch so verwandelt lieben,", "tokens": ["Willst", "du", "sie", "denn", "auch", "so", "ver\u00b7wan\u00b7delt", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADV", "ADV", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und selber so geliebt sein, mit Gewalt?", "tokens": ["Und", "sel\u00b7ber", "so", "ge\u00b7liebt", "sein", ",", "mit", "Ge\u00b7walt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVPP", "VAINF", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Liebe pa\u00dft sich nicht zu Wackelk\u00f6pfen.", "tokens": ["Die", "Lie\u00b7be", "pa\u00dft", "sich", "nicht", "zu", "Wa\u00b7ckel\u00b7k\u00f6p\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr liebt nicht mehr: gesteht es ohne Scheu.", "tokens": ["Ihr", "liebt", "nicht", "mehr", ":", "ge\u00b7steht", "es", "oh\u00b7ne", "Scheu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$.", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch, k\u00f6nnt ihr beid' am Born der Jugend sch\u00f6pfen,", "tokens": ["Doch", ",", "k\u00f6nnt", "ihr", "beid'", "am", "Born", "der", "Ju\u00b7gend", "sch\u00f6p\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So lob' ich die Jahrhundert-lange Treu.", "tokens": ["So", "lob'", "ich", "die", "Jahr\u00b7hun\u00b7der\u00b7tlan\u00b7ge", "Treu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}