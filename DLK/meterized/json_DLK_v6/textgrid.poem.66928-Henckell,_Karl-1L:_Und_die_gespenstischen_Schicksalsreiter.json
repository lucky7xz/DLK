{"textgrid.poem.66928": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und die gespenstischen Schicksalsreiter", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und die gespenstischen Schicksalsreiter", "tokens": ["Und", "die", "ge\u00b7spens\u00b7ti\u00b7schen", "Schick\u00b7sals\u00b7rei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf den gelbschwarz getigerten Rossen", "tokens": ["Auf", "den", "gelb\u00b7schwarz", "ge\u00b7ti\u00b7ger\u00b7ten", "Ros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mit den gierig gebl\u00e4hten N\u00fcstern,", "tokens": ["Mit", "den", "gie\u00b7rig", "ge\u00b7bl\u00e4h\u00b7ten", "N\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Die von giftigen Gasen dampfen,", "tokens": ["Die", "von", "gif\u00b7ti\u00b7gen", "Ga\u00b7sen", "damp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Sausen weiter, sausen weiter ...", "tokens": ["Sau\u00b7sen", "wei\u00b7ter", ",", "sau\u00b7sen", "wei\u00b7ter", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dumpf die Hufe vor\u00fcberstampfen,", "tokens": ["Dumpf", "die", "Hu\u00b7fe", "vor\u00b7\u00fc\u00b7bers\u00b7tamp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und die Hexen des Schlachtfelds fl\u00fcstern.", "tokens": ["Und", "die", "He\u00b7xen", "des", "Schlacht\u00b7felds", "fl\u00fcs\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Blut\u00fcbergossen,", "tokens": ["Blut\u00b7\u00fc\u00b7ber\u00b7gos\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Gerippehager,", "tokens": ["Ge\u00b7rip\u00b7pe\u00b7ha\u00b7ger", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Lehnen zu dritt sie am toten Tank,", "tokens": ["Leh\u00b7nen", "zu", "dritt", "sie", "am", "to\u00b7ten", "Tank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Der angeschossen", "tokens": ["Der", "an\u00b7ge\u00b7schos\u00b7sen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Plump sich gew\u00e4lzt und im Sumpf versank.", "tokens": ["Plump", "sich", "ge\u00b7w\u00e4lzt", "und", "im", "Sumpf", "ver\u00b7sank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "VVPP", "KON", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Hungermager", "tokens": ["Hun\u00b7ger\u00b7ma\u00b7ger"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Fl\u00fcstern die Schwestern und Schicksalssager:", "tokens": ["Fl\u00fcs\u00b7tern", "die", "Schwes\u00b7tern", "und", "Schick\u00b7sals\u00b7sa\u00b7ger", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "\u00bbwelt todkrank!", "tokens": ["\u00bb", "welt", "tod\u00b7krank", "!"], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Menschen l\u00fcstern", "tokens": ["Men\u00b7schen", "l\u00fcs\u00b7tern"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Nach Mord, Mord, Mord.", "tokens": ["Nach", "Mord", ",", "Mord", ",", "Mord", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Eisenhy\u00e4ne,", "tokens": ["Ei\u00b7sen\u00b7hy\u00b7\u00e4\u00b7ne", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Zerfetzende Z\u00e4hne!", "tokens": ["Zer\u00b7fet\u00b7zen\u00b7de", "Z\u00e4h\u00b7ne", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Mord ist der Lord,", "tokens": ["Mord", "ist", "der", "Lord", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Wahnwitz der Meister", "tokens": ["Wahn\u00b7witz", "der", "Meis\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ruchloser Geister \u2013", "tokens": ["Ruch\u00b7lo\u00b7ser", "Geis\u00b7ter", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Gold-Kapit\u00e4ne", "tokens": ["Gold\u00b7Ka\u00b7pi\u00b7t\u00e4\u00b7ne"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Schrein:", "tokens": ["Schrein", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Gott \u00fcber Bord!", "tokens": ["Gott", "\u00fc\u00b7ber", "Bord", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Welt ward gemein.", "tokens": ["Welt", "ward", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Strotzende F\u00fclle", "tokens": ["Strot\u00b7zen\u00b7de", "F\u00fcl\u00b7le"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Hetzt sie zu Ha\u00df,", "tokens": ["Hetzt", "sie", "zu", "Ha\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Heilige Stille", "tokens": ["Hei\u00b7li\u00b7ge", "Stil\u00b7le"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Sch\u00e4nden sie mit Granatengebr\u00fclle,", "tokens": ["Sch\u00e4n\u00b7den", "sie", "mit", "Gra\u00b7na\u00b7ten\u00b7ge\u00b7br\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-++--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Himmelhoch steigt ihr zerst\u00f6render Wille \u2013", "tokens": ["Him\u00b7mel\u00b7hoch", "steigt", "ihr", "zer\u00b7st\u00f6\u00b7ren\u00b7der", "Wil\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Sinnlos Sein!", "tokens": ["Sinn\u00b7los", "Sein", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Mild beh\u00fctet", "tokens": ["Mild", "be\u00b7h\u00fc\u00b7tet"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Im Mutterscho\u00df \u2013", "tokens": ["Im", "Mut\u00b7ter\u00b7scho\u00df", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wild zerw\u00fctet,", "tokens": ["Wild", "zer\u00b7w\u00fc\u00b7tet", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Metzgersto\u00df,", "tokens": ["Metz\u00b7ger\u00b7sto\u00df", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Wuchs der Knabe zum Jungmann gro\u00df \u2013", "tokens": ["Wuchs", "der", "Kna\u00b7be", "zum", "Jung\u00b7mann", "gro\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "ADJD", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Markverheerendes Menschheitslos!", "tokens": ["Mark\u00b7ver\u00b7hee\u00b7ren\u00b7des", "Menschheits\u00b7los", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Feld der L\u00fcge,", "tokens": ["Feld", "der", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Wahnesfeld,", "tokens": ["Wah\u00b7nes\u00b7feld", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Irre Z\u00fcge", "tokens": ["Ir\u00b7re", "Z\u00fc\u00b7ge"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Zeigt die Welt ...", "tokens": ["Zeigt", "die", "Welt", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Recht verzerrt,", "tokens": ["Recht", "ver\u00b7zerrt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Wert entstellt \u2013", "tokens": ["Wert", "ent\u00b7stellt", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Horcht, wie der H\u00f6llhund der D\u00e4mmerung bellt!\u00ab", "tokens": ["Horcht", ",", "wie", "der", "H\u00f6ll\u00b7hund", "der", "D\u00e4m\u00b7me\u00b7rung", "bellt", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.8": {"line.1": {"text": "Tag erwacht", "tokens": ["Tag", "er\u00b7wacht"], "token_info": ["word", "word"], "pos": ["NN", "VVPP"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Fahl zur Schlacht,", "tokens": ["Fahl", "zur", "Schlacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Schauerlich fl\u00fcstern die Schicksalsfraun", "tokens": ["Schau\u00b7er\u00b7lich", "fl\u00fcs\u00b7tern", "die", "Schick\u00b7sals\u00b7fraun"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und zerflie\u00dfen in Nacht und Graun.", "tokens": ["Und", "zer\u00b7flie\u00b7\u00dfen", "in", "Nacht", "und", "Graun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}