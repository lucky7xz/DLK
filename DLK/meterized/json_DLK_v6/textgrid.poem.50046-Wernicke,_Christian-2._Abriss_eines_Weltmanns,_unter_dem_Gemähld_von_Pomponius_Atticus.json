{"textgrid.poem.50046": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "2. Abriss eines Weltmanns, unter dem Gem\u00e4hld von Pomponius Atticus", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh' Atticus ", "tokens": ["Sieh'", "At\u00b7ti\u00b7cus"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Er war ein ", "tokens": ["Er", "war", "ein"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Er hatt' im Uberfluss, das was die gantze Welt", "tokens": ["Er", "hatt'", "im", "U\u00b7ber\u00b7fluss", ",", "das", "was", "die", "gant\u00b7ze", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$,", "PDS", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu des Besitzers Ruhm verblendet: ", "tokens": ["Zu", "des", "Be\u00b7sit\u00b7zers", "Ruhm", "ver\u00b7blen\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er hatte keinen ", "tokens": ["Er", "hat\u00b7te", "kei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Zu dem ", "tokens": ["Zu", "dem"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "So dass er ", "tokens": ["So", "dass", "er"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOUS", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Der tugendhafften ", "tokens": ["Der", "tu\u00b7gend\u00b7haff\u00b7ten"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Und weil Pompejus das was Cesar von ihm meint',", "tokens": ["Und", "weil", "Pom\u00b7pe\u00b7jus", "das", "was", "Ce\u00b7sar", "von", "ihm", "meint'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "PDS", "PRELS", "NE", "APPR", "PPER", "VVFIN", "$,"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}