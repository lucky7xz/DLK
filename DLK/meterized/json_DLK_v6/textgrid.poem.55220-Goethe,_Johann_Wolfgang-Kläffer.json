{"textgrid.poem.55220": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Kl\u00e4ffer", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir reiten in die Kreuz und Quer", "tokens": ["Wir", "rei\u00b7ten", "in", "die", "Kreuz", "und", "Quer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Freuden und Gesch\u00e4ften;", "tokens": ["Nach", "Freu\u00b7den", "und", "Ge\u00b7sch\u00e4f\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch immer kl\u00e4fft es hinterher", "tokens": ["Doch", "im\u00b7mer", "kl\u00e4fft", "es", "hin\u00b7ter\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und billt aus allen Kr\u00e4ften.", "tokens": ["Und", "billt", "aus", "al\u00b7len", "Kr\u00e4f\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So will der Spitz aus unserm Stall", "tokens": ["So", "will", "der", "Spitz", "aus", "un\u00b7serm", "Stall"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Uns immerfort begleiten,", "tokens": ["Uns", "im\u00b7mer\u00b7fort", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und seines Bellens lauter Schall", "tokens": ["Und", "sei\u00b7nes", "Bel\u00b7lens", "lau\u00b7ter", "Schall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Beweist nur, da\u00df wir reiten.", "tokens": ["Be\u00b7weist", "nur", ",", "da\u00df", "wir", "rei\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}