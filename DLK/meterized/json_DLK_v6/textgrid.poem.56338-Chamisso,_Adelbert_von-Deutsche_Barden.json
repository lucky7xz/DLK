{"textgrid.poem.56338": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Deutsche Barden", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schimmerten in r\u00f6tlich heller Pracht", "tokens": ["Es", "schim\u00b7mer\u00b7ten", "in", "r\u00f6t\u00b7lich", "hel\u00b7ler", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die schnee'gen Gipfel \u00fcber mir; es lagen", "tokens": ["Die", "schnee'\u00b7gen", "Gip\u00b7fel", "\u00fc\u00b7ber", "mir", ";", "es", "la\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die T\u00e4ler tief und fern in dunkler Nacht.", "tokens": ["Die", "T\u00e4\u00b7ler", "tief", "und", "fern", "in", "dunk\u00b7ler", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der fr\u00fche Nebel ward empor getragen;", "tokens": ["Der", "fr\u00fc\u00b7he", "Ne\u00b7bel", "ward", "em\u00b7por", "ge\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich sah ihn in den Schluchten bald zerflie\u00dfen,", "tokens": ["Ich", "sah", "ihn", "in", "den", "Schluch\u00b7ten", "bald", "zer\u00b7flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bald \u00fcber mich die feuchte H\u00fclle schlagen,", "tokens": ["Bald", "\u00fc\u00b7ber", "mich", "die", "feuch\u00b7te", "H\u00fcl\u00b7le", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Den Bergstrom h\u00f6rt ich brausend sich ergie\u00dfen,", "tokens": ["Den", "Berg\u00b7strom", "h\u00f6rt", "ich", "brau\u00b7send", "sich", "er\u00b7gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das starre Meer des Gletschers sich zerspalten,", "tokens": ["Das", "star\u00b7re", "Meer", "des", "Glet\u00b7schers", "sich", "zer\u00b7spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und donnernde Lauvinen niederschie\u00dfen.", "tokens": ["Und", "don\u00b7nern\u00b7de", "Lau\u00b7vi\u00b7nen", "nie\u00b7der\u00b7schie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ich hatte M\u00fch den steilen Pfad zu halten,", "tokens": ["Ich", "hat\u00b7te", "M\u00fch", "den", "stei\u00b7len", "Pfad", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Auf dem ich klomm zum hohen Bergestor,", "tokens": ["Auf", "dem", "ich", "klomm", "zum", "ho\u00b7hen", "Ber\u00b7ge\u00b7stor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Von wo die Blicke ostw\u00e4rts sich entfalten.", "tokens": ["Von", "wo", "die", "Bli\u00b7cke", "ost\u00b7w\u00e4rts", "sich", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "ART", "NN", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und wie ich zu der H\u00f6he mich empor", "tokens": ["Und", "wie", "ich", "zu", "der", "H\u00f6\u00b7he", "mich", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "NN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Geschwungen hatte, traf mit heim'schem Klange", "tokens": ["Ge\u00b7schwun\u00b7gen", "hat\u00b7te", ",", "traf", "mit", "heim'\u00b7schem", "Klan\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Hochdeutsche Mundart lockend mir das Ohr.", "tokens": ["Hoch\u00b7deut\u00b7sche", "Mund\u00b7art", "lo\u00b7ckend", "mir", "das", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Ich stand gefesselt und ich lauschte lange,", "tokens": ["Ich", "stand", "ge\u00b7fes\u00b7selt", "und", "ich", "lauschte", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "KON", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Und h\u00f6rte der gewalt'gen Rede Fluten", "tokens": ["Und", "h\u00f6r\u00b7te", "der", "ge\u00b7walt'\u00b7gen", "Re\u00b7de", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Melodisch schwellend werden zum Gesange.", "tokens": ["Me\u00b7lo\u00b7disch", "schwel\u00b7lend", "wer\u00b7den", "zum", "Ge\u00b7san\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "VAINF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Es stand der S\u00e4nger einsam, in die Gluten", "tokens": ["Es", "stand", "der", "S\u00e4n\u00b7ger", "ein\u00b7sam", ",", "in", "die", "Glu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Der Sonne starrend, die sich nun erhoben", "tokens": ["Der", "Son\u00b7ne", "star\u00b7rend", ",", "die", "sich", "nun", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "PRF", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Aus Wolken, die am Horizonte ruhten.", "tokens": ["Aus", "Wol\u00b7ken", ",", "die", "am", "Ho\u00b7ri\u00b7zon\u00b7te", "ruh\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Der Schleier, blutigrot aus Dunst gewoben,", "tokens": ["Der", "Schlei\u00b7er", ",", "blu\u00b7ti\u00b7grot", "aus", "Dunst", "ge\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Auf ebne, weite Landschaft ausgebreitet;", "tokens": ["Auf", "eb\u00b7ne", ",", "wei\u00b7te", "Land\u00b7schaft", "aus\u00b7ge\u00b7brei\u00b7tet", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Das tiefe Blau der Himmelsw\u00f6lbung oben;", "tokens": ["Das", "tie\u00b7fe", "Blau", "der", "Him\u00b7mels\u00b7w\u00f6l\u00b7bung", "o\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Die Bilder, so der Morgen hier bereitet,", "tokens": ["Die", "Bil\u00b7der", ",", "so", "der", "Mor\u00b7gen", "hier", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Sie wurden auf der Griechen Heldenkampf", "tokens": ["Sie", "wur\u00b7den", "auf", "der", "Grie\u00b7chen", "Hel\u00b7den\u00b7kampf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Verherrlichend vom Liede hingeleitet.", "tokens": ["Ver\u00b7herr\u00b7li\u00b7chend", "vom", "Lie\u00b7de", "hin\u00b7ge\u00b7lei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Ich h\u00f6rt ihm zu, sah \u00fcber Blut und Dampf", "tokens": ["Ich", "h\u00f6rt", "ihm", "zu", ",", "sah", "\u00fc\u00b7ber", "Blut", "und", "Dampf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.29": {"text": "Die Freiheitssonne Hellas' sich erheben,", "tokens": ["Die", "Frei\u00b7heits\u00b7son\u00b7ne", "Hel\u00b7las'", "sich", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Das Leben siegen ob dem Todeskrampf:", "tokens": ["Das", "Le\u00b7ben", "sie\u00b7gen", "ob", "dem", "To\u00b7des\u00b7krampf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "\u00bbdu goldne Freiheit, bist das Licht, das Leben;", "tokens": ["\u00bb", "du", "gold\u00b7ne", "Frei\u00b7heit", ",", "bist", "das", "Licht", ",", "das", "Le\u00b7ben", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$,", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Die blut'ge Taufe tilgt der Ketten Schmach;", "tokens": ["Die", "blut'\u00b7ge", "Tau\u00b7fe", "tilgt", "der", "Ket\u00b7ten", "Schmach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Du hast dir, Heldenvolk, das Sein gegeben.\u00ab", "tokens": ["Du", "hast", "dir", ",", "Hel\u00b7den\u00b7volk", ",", "das", "Sein", "ge\u00b7ge\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "NN", "$,", "PRELS", "PPOSAT", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Er schwieg, ich lauschte noch; vortretend sprach", "tokens": ["Er", "schwieg", ",", "ich", "lauschte", "noch", ";", "vor\u00b7tre\u00b7tend", "sprach"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$.", "VVPP", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.35": {"text": "Den Mann ich an mit dargereichter Rechten:", "tokens": ["Den", "Mann", "ich", "an", "mit", "dar\u00b7ge\u00b7reich\u00b7ter", "Rech\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "\u00bbdu deutscher Bard', der sich die Palme brach,", "tokens": ["\u00bb", "du", "deut\u00b7scher", "Bard'", ",", "der", "sich", "die", "Pal\u00b7me", "brach", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Du siehst mein Aug von deines Liedes M\u00e4chten", "tokens": ["Du", "siehst", "mein", "Aug", "von", "dei\u00b7nes", "Lie\u00b7des", "M\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Geschm\u00fcckt noch mit der Tr\u00e4nen Perlenzier,", "tokens": ["Ge\u00b7schm\u00fcckt", "noch", "mit", "der", "Tr\u00e4\u00b7nen", "Per\u00b7len\u00b7zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Und nicht ob meinem Antrag wirst du rechten.", "tokens": ["Und", "nicht", "ob", "mei\u00b7nem", "An\u00b7trag", "wirst", "du", "rech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "KOUS", "PPOSAT", "NN", "VAFIN", "PPER", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Ich bin ein Deutscher, so wie du, und mir", "tokens": ["Ich", "bin", "ein", "Deut\u00b7scher", ",", "so", "wie", "du", ",", "und", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADV", "KOKOM", "PPER", "$,", "KON", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Entstr\u00f6met der Gesang aus Herzens Grunde", "tokens": ["Ent\u00b7str\u00f6\u00b7met", "der", "Ge\u00b7sang", "aus", "Her\u00b7zens", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Um Freiheit, Recht und Glauben, so wie dir.", "tokens": ["Um", "Frei\u00b7heit", ",", "Recht", "und", "Glau\u00b7ben", ",", "so", "wie", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$,", "NN", "KON", "NN", "$,", "ADV", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Die Wildnis bringt uns n\u00e4her und die Stunde,", "tokens": ["Die", "Wild\u00b7nis", "bringt", "uns", "n\u00e4\u00b7her", "und", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Was in der Brust wir tragen und im Schilde;", "tokens": ["Was", "in", "der", "Brust", "wir", "tra\u00b7gen", "und", "im", "Schil\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PPER", "VVINF", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "O reiche mir die Hand zu heil'gem Bunde!\u00ab", "tokens": ["O", "rei\u00b7che", "mir", "die", "Hand", "zu", "heil'\u00b7gem", "Bun\u00b7de", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Drauf er mit Wehmut l\u00e4chelnd und mit Milde:", "tokens": ["Drauf", "er", "mit", "Weh\u00b7mut", "l\u00e4\u00b7chelnd", "und", "mit", "Mil\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "APPR", "NN", "ADJD", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "\u00bbmich freut in deinem Aug der Widerschein", "tokens": ["\u00bb", "mich", "freut", "in", "dei\u00b7nem", "Aug", "der", "Wi\u00b7der\u00b7schein"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {