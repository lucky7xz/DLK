{"textgrid.poem.35018": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn Sorge mich und Unmuth qu\u00e4let,", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Sorge mich und Unmuth qu\u00e4let,", "tokens": ["Wenn", "Sor\u00b7ge", "mich", "und", "Un\u00b7muth", "qu\u00e4\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn mir's an Moos im Beutel fehlet,", "tokens": ["Wenn", "mir's", "an", "Moos", "im", "Beu\u00b7tel", "feh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn mich ein schwerer Kummer dr\u00fcckt,", "tokens": ["Wenn", "mich", "ein", "schwe\u00b7rer", "Kum\u00b7mer", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Schicksal mich mit Pech begl\u00fcckt:", "tokens": ["Das", "Schick\u00b7sal", "mich", "mit", "Pech", "be\u00b7gl\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was ist es dann, wonach ich greife?", "tokens": ["Was", "ist", "es", "dann", ",", "wo\u00b7nach", "ich", "grei\u00b7fe", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "I nun! Die liebe Tabakspfeife!", "tokens": ["I", "nun", "!", "Die", "lie\u00b7be", "Ta\u00b7baks\u00b7pfei\u00b7fe", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "ADV", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bei meinen Freuden, meinen Scherzen,", "tokens": ["Bei", "mei\u00b7nen", "Freu\u00b7den", ",", "mei\u00b7nen", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Beim Austausch gleichgesinnter Herzen,", "tokens": ["Beim", "Aus\u00b7tausch", "gleich\u00b7ge\u00b7sinn\u00b7ter", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In all' den traulich frohen Stunden,", "tokens": ["In", "all'", "den", "trau\u00b7lich", "fro\u00b7hen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ich im Freundeskreis gefunden,", "tokens": ["Die", "ich", "im", "Freun\u00b7des\u00b7kreis", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bei meines Gl\u00fcck's so seltner Reife", "tokens": ["Bei", "mei\u00b7nes", "Gl\u00fc\u00b7ck's", "so", "selt\u00b7ner", "Rei\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ist stets um mich die liebe Pfeife.", "tokens": ["Ist", "stets", "um", "mich", "die", "lie\u00b7be", "Pfei\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf all' den Reisen, die ich machte,", "tokens": ["Auf", "all'", "den", "Rei\u00b7sen", ",", "die", "ich", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo die Natur mir freundlich lachte,", "tokens": ["Wo", "die", "Na\u00b7tur", "mir", "freund\u00b7lich", "lach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf all' den einsam trauten Wegen,", "tokens": ["Auf", "all'", "den", "ein\u00b7sam", "trau\u00b7ten", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Waldesgr\u00fcn, wo ich gelegen,", "tokens": ["Im", "Wal\u00b7des\u00b7gr\u00fcn", ",", "wo", "ich", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In Feld und Flur, die ich durchstreife,", "tokens": ["In", "Feld", "und", "Flur", ",", "die", "ich", "durch\u00b7strei\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Begleitet mich die treue Pfeife.", "tokens": ["Be\u00b7glei\u00b7tet", "mich", "die", "treu\u00b7e", "Pfei\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie bleibt mir Braut durch's ganze Leben;", "tokens": ["Sie", "bleibt", "mir", "Braut", "durch's", "gan\u00b7ze", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ja, sie in Adel zu erheben", "tokens": ["Ja", ",", "sie", "in", "A\u00b7del", "zu", "er\u00b7he\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist wohl ein Leichtes: Das Diplom", "tokens": ["Ist", "wohl", "ein", "Leich\u00b7tes", ":", "Das", "Dip\u00b7lom"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "$.", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schreibt sie sich selbst durch ihr Arom.", "tokens": ["Schreibt", "sie", "sich", "selbst", "durch", "ihr", "A\u00b7rom", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Sie hei\u00dfe d'rum, ob man auch keife,", "tokens": ["Sie", "hei\u00b7\u00dfe", "d'\u00b7rum", ",", "ob", "man", "auch", "kei\u00b7fe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Von jetzt an: Edle von der Pfeife!", "tokens": ["Von", "jetzt", "an", ":", "Ed\u00b7le", "von", "der", "Pfei\u00b7fe", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PTKVZ", "$.", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}