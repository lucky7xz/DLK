{"textgrid.poem.35222": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "[du Erstlinger und End]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Erstlinger und End", "tokens": ["Du", "Erst\u00b7lin\u00b7ger", "und", "End"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NN", "KON", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "\u2013 \u2013 O blanker N\u00e4chtewinker", "tokens": ["\u2013", "\u2013", "O", "blan\u00b7ker", "N\u00e4ch\u00b7te\u00b7win\u00b7ker"], "token_info": ["punct", "punct", "word", "word", "word"], "pos": ["$(", "$(", "NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd heller Morgenstern/ Ich seh noch dein Geflinker/", "tokens": ["Vnd", "hel\u00b7ler", "Mor\u00b7gens\u00b7tern", "/", "Ich", "seh", "noch", "dein", "Ge\u00b7flin\u00b7ker", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$(", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das heut den Morgen wie\u00df/ den braunen Abend jetzt/", "tokens": ["Das", "heut", "den", "Mor\u00b7gen", "wie\u00df", "/", "den", "brau\u00b7nen", "A\u00b7bend", "jetzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVFIN", "$(", "ART", "ADJA", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du Bot der Nacht/ und wann die Sonn zu Wagen sitzt/", "tokens": ["Du", "Bot", "der", "Nacht", "/", "und", "wann", "die", "Sonn", "zu", "Wa\u00b7gen", "sitzt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$(", "KON", "PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du Erstlinger und End/", "tokens": ["Du", "Erst\u00b7lin\u00b7ger", "und", "End", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "\u2013 \u2013 Es geht zu Ruh die Welt:", "tokens": ["\u2013", "\u2013", "Es", "geht", "zu", "Ruh", "die", "Welt", ":"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "PPER", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Bestirne/ wie du thust/ das dunkle Wolkenfeld/", "tokens": ["Bes\u00b7tir\u00b7ne", "/", "wie", "du", "thust", "/", "das", "dunk\u00b7le", "Wol\u00b7ken\u00b7feld", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWAV", "PPER", "VVFIN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch schaffe/ da\u00df dein Liecht den Morgen morgen sende", "tokens": ["Doch", "schaf\u00b7fe", "/", "da\u00df", "dein", "Liecht", "den", "Mor\u00b7gen", "mor\u00b7gen", "sen\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPOSAT", "NN", "ART", "NN", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mit Freuden wieder her/ \u2013 \u2013", "tokens": ["Mit", "Freu\u00b7den", "wie\u00b7der", "her", "/", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NN", "ADV", "PTKVZ", "$(", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Du Erstlinger und", "tokens": ["Du", "Erst\u00b7lin\u00b7ger", "und"], "token_info": ["word", "word", "word"], "pos": ["PPER", "NN", "KON"], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}