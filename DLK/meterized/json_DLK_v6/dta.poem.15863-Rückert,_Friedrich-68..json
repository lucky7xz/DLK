{"dta.poem.15863": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "68.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Empor vom Berge strebt, und zwischen Wolken duftig", "tokens": ["Em\u00b7por", "vom", "Ber\u00b7ge", "strebt", ",", "und", "zwi\u00b7schen", "Wol\u00b7ken", "duf\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "VVFIN", "$,", "KON", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alswie auf Fl\u00fcgeln schwebt Gem\u00e4uer hoch und luftig.", "tokens": ["Als\u00b7wie", "auf", "Fl\u00fc\u00b7geln", "schwebt", "Ge\u00b7m\u00e4u\u00b7er", "hoch", "und", "luf\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es herrscht ins Land und schaut auf jedes Thal hinein,", "tokens": ["Es", "herrscht", "ins", "Land", "und", "schaut", "auf", "je\u00b7des", "Thal", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hat am ersten und am letzten Sonnenschein.", "tokens": ["Und", "hat", "am", "ers\u00b7ten", "und", "am", "letz\u00b7ten", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "ADJA", "KON", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gewis der Freiheit Schlo\u00df! O nein, mit Zellen dumpf", "tokens": ["Ge\u00b7wis", "der", "Frei\u00b7heit", "Schlo\u00df", "!", "O", "nein", ",", "mit", "Zel\u00b7len", "dumpf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN", "$.", "NE", "PTKANT", "$,", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Kloster; auf die H\u00f6h wie kommt hinauf der Sumpf?", "tokens": ["Ein", "Klos\u00b7ter", ";", "auf", "die", "H\u00f6h", "wie", "kommt", "hin\u00b7auf", "der", "Sumpf", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "APPR", "ART", "NN", "KOKOM", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}