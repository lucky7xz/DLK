{"textgrid.poem.59391": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachtgru\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil jetzo alles stille ist", "tokens": ["Weil", "jet\u00b7zo", "al\u00b7les", "stil\u00b7le", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIAT", "ADJA", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und alle Menschen schlafen,", "tokens": ["Und", "al\u00b7le", "Men\u00b7schen", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Seel das ew'ge Licht begr\u00fc\u00dft,", "tokens": ["Mein", "Seel", "das", "ew'\u00b7ge", "Licht", "be\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ruht wie ein Schiff im Hafen.", "tokens": ["Ruht", "wie", "ein", "Schiff", "im", "Ha\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der falsche Flei\u00df, die Eitelkeit,", "tokens": ["Der", "fal\u00b7sche", "Flei\u00df", ",", "die", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was keinen mag erlaben,", "tokens": ["Was", "kei\u00b7nen", "mag", "er\u00b7la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Darin der Tag das Herz zerstreut,", "tokens": ["Da\u00b7rin", "der", "Tag", "das", "Herz", "zer\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Liegt alles tief begraben.", "tokens": ["Liegt", "al\u00b7les", "tief", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein andrer K\u00f6nig wunderreich", "tokens": ["Ein", "an\u00b7drer", "K\u00f6\u00b7nig", "wun\u00b7der\u00b7reich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit k\u00f6niglichen Sinnen,", "tokens": ["Mit", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+----+-", "measure": "dactylic.init"}, "line.3": {"text": "Zieht herrlich ein im stillen Reich,", "tokens": ["Zieht", "herr\u00b7lich", "ein", "im", "stil\u00b7len", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Besteigt die ew'gen Zinnen.", "tokens": ["Be\u00b7steigt", "die", "ew'\u00b7gen", "Zin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}