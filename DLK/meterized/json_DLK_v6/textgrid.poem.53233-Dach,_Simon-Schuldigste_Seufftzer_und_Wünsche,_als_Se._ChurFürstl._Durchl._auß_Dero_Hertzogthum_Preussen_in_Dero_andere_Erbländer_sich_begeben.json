{"textgrid.poem.53233": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Schuldigste Seufftzer und W\u00fcnsche, als Se. ChurF\u00fcrstl. Durchl. au\u00df Dero Hertzogthum Preussen in Dero andere Erbl\u00e4nder sich begeben", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Seufftzer hier im Lande,", "tokens": ["Ihr", "Seufft\u00b7zer", "hier", "im", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Tr\u00e4hnen in gemein,", "tokens": ["Ihr", "Tr\u00e4h\u00b7nen", "in", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So viel au\u00df jedem Stande", "tokens": ["So", "viel", "au\u00df", "je\u00b7dem", "Stan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur auffzubringen seyn,", "tokens": ["Nur", "auff\u00b7zu\u00b7brin\u00b7gen", "seyn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sucht jetzt vor Gott zu schweben,", "tokens": ["Sucht", "jetzt", "vor", "Gott", "zu", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bringt aller Hertz vor Ihn,", "tokens": ["Bringt", "al\u00b7ler", "Hertz", "vor", "Ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der ChurF\u00fcrst, unser Leben,", "tokens": ["Der", "Chur", "F\u00fcrst", ",", "un\u00b7ser", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wil numehr von uns ziehn.", "tokens": ["Wil", "nu\u00b7mehr", "von", "uns", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sein Vorsatz mu\u00df nur walten,", "tokens": ["Sein", "Vor\u00b7satz", "mu\u00df", "nur", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ja keine Bitt hat Macht", "tokens": ["Ja", "kei\u00b7ne", "Bitt", "hat", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "PIAT", "NN", "VAFIN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihn l\u00e4nger an-zu-halten,", "tokens": ["Ihn", "l\u00e4n\u00b7ger", "an\u00b7zu\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er giebt uns gute Nacht,", "tokens": ["Er", "giebt", "uns", "gu\u00b7te", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verl\u00e4sst den edlen Pregel", "tokens": ["Ver\u00b7l\u00e4sst", "den", "ed\u00b7len", "Pre\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und diesen fruchtbarn Ort,", "tokens": ["Und", "die\u00b7sen", "frucht\u00b7barn", "Ort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sein Volck geht theils zu Segel", "tokens": ["Sein", "Volck", "geht", "theils", "zu", "Se\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und theils zu Lande fort.", "tokens": ["Und", "theils", "zu", "Lan\u00b7de", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich seh' auch unsre Frewde", "tokens": ["Ich", "seh'", "auch", "uns\u00b7re", "Frew\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Zuflucht mit Ihm gehn,", "tokens": ["Und", "Zu\u00b7flucht", "mit", "Ihm", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wir werden stets in Leide", "tokens": ["Wir", "wer\u00b7den", "stets", "in", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und tieffem Trawren stehn.", "tokens": ["Und", "tief\u00b7fem", "Traw\u00b7ren", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein Heerpauck wird nun kingen,", "tokens": ["Kein", "Heer\u00b7pauck", "wird", "nun", "kin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der zw\u00f6lff Trompeten Schall", "tokens": ["Der", "zw\u00f6lff", "Trom\u00b7pe\u00b7ten", "Schall"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "CARD", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wird nicht, wie vormals, dringen", "tokens": ["Wird", "nicht", ",", "wie", "vor\u00b7mals", ",", "drin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VAFIN", "PTKNEG", "$,", "PWAV", "ADV", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Durch St\u00e4dte, Berg' und Thal.", "tokens": ["Durch", "St\u00e4d\u00b7te", ",", "Ber\u00b7g'", "und", "Thal", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Des Schlosses Pracht, die D\u00e4cher", "tokens": ["Des", "Schlos\u00b7ses", "Pracht", ",", "die", "D\u00e4\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Empfinden jetzund Pein,", "tokens": ["Emp\u00b7fin\u00b7den", "je\u00b7tzund", "Pein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die F\u00fcrstlichen Gem\u00e4cher", "tokens": ["Die", "F\u00fcrst\u00b7li\u00b7chen", "Ge\u00b7m\u00e4\u00b7cher"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nimmt Staub und Grawen ein,", "tokens": ["Nimmt", "Staub", "und", "Gra\u00b7wen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Mey ist selbs, von wegen", "tokens": ["Der", "Mey", "ist", "selbs", ",", "von", "we\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NE", "VAFIN", "ADV", "$,", "APPR", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Auffbruchs, K\u00e4lt und Wust,", "tokens": ["Des", "Auff\u00b7bruchs", ",", "K\u00e4lt", "und", "Wust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wil durchau\u00df nicht hegen", "tokens": ["Und", "wil", "durch\u00b7au\u00df", "nicht", "he\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gew\u00fcnschte Vorjahrslust.", "tokens": ["Ge\u00b7w\u00fcnschte", "Vor\u00b7jahrs\u00b7lust", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+---", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "O fasst recht zu Gem\u00fcte,", "tokens": ["O", "fasst", "recht", "zu", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer jetzund von uns eilt,", "tokens": ["Wer", "je\u00b7tzund", "von", "uns", "eilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch welchen alle G\u00fcte", "tokens": ["Durch", "wel\u00b7chen", "al\u00b7le", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Gott uns wird ertheilt.", "tokens": ["Von", "Gott", "uns", "wird", "er\u00b7theilt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer ist f\u00fcr Elend, Trawren,", "tokens": ["Wer", "ist", "f\u00fcr", "E\u00b7lend", ",", "Traw\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Angst, Schrecken und Beschwer", "tokens": ["Angst", ",", "Schre\u00b7cken", "und", "Be\u00b7schwer"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Uns hohe W\u00e4ll' und Mawren?", "tokens": ["Uns", "ho\u00b7he", "W\u00e4ll'", "und", "Maw\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der H\u00f6chste nur und Er.", "tokens": ["Der", "H\u00f6chs\u00b7te", "nur", "und", "Er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gleich wie uns hie best\u00e4mmet", "tokens": ["Gleich", "wie", "uns", "hie", "be\u00b7st\u00e4m\u00b7met"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Nehrung wilde H\u00f6h,", "tokens": ["Der", "Neh\u00b7rung", "wil\u00b7de", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sonst w\u00fcrden wir verschwemmet", "tokens": ["Sonst", "w\u00fcr\u00b7den", "wir", "ver\u00b7schwem\u00b7met"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von strenger Fluth der See,", "tokens": ["Von", "stren\u00b7ger", "Fluth", "der", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So ist auch unsern Sachen", "tokens": ["So", "ist", "auch", "un\u00b7sern", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er einig Schutz und Rhue,", "tokens": ["Er", "ei\u00b7nig", "Schutz", "und", "Rhue", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "KON", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sonst schl\u00fcgen alle Wachen", "tokens": ["Sonst", "schl\u00fc\u00b7gen", "al\u00b7le", "Wa\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Jammers auff uns zu.", "tokens": ["Des", "Jam\u00b7mers", "auff", "uns", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wer Andacht hat zu behten,", "tokens": ["Wer", "An\u00b7dacht", "hat", "zu", "beh\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Reich, Arm, sampt Jung und Alt", "tokens": ["Reich", ",", "Arm", ",", "sampt", "Jung", "und", "Alt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Komm' jetzt vor Gott getreten,", "tokens": ["Komm'", "jetzt", "vor", "Gott", "ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und \u00f6ffn' Ihm mannigfalt", "tokens": ["Und", "\u00f6ffn'", "Ihm", "man\u00b7nig\u00b7falt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die heimlich' HertzensPforten,", "tokens": ["Die", "heim\u00b7lich'", "Hert\u00b7zens", "Pfor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der kleinen Kinder Schaar", "tokens": ["Der", "klei\u00b7nen", "Kin\u00b7der", "Schaar"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Soll mit zerbrochnen Worten", "tokens": ["Soll", "mit", "zer\u00b7broch\u00b7nen", "Wor\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihm klagen die Gefahr.", "tokens": ["Ihm", "kla\u00b7gen", "die", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Man h\u00f6rt die Rede fliegen,", "tokens": ["Man", "h\u00f6rt", "die", "Re\u00b7de", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da aber Gott vor sey,", "tokens": ["Da", "a\u00b7ber", "Gott", "vor", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "APPR", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Nort woll' uns bekriegen,", "tokens": ["Der", "Nort", "woll'", "uns", "be\u00b7krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer steht uns Armen bey", "tokens": ["Wer", "steht", "uns", "Ar\u00b7men", "bey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "NN", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In diesem schweren Stande?", "tokens": ["In", "die\u00b7sem", "schwe\u00b7ren", "Stan\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was Schutz wird uns gewehrt,", "tokens": ["Was", "Schutz", "wird", "uns", "ge\u00b7wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wann unser Haupt im Lande", "tokens": ["Wann", "un\u00b7ser", "Haupt", "im", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sich auch nun von uns kehrt?", "tokens": ["Sich", "auch", "nun", "von", "uns", "kehrt", "?"], "token_info": ["word", "wo