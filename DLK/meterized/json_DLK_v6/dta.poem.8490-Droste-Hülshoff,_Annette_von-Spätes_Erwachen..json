{"dta.poem.8490": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Sp\u00e4tes Erwachen.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie war mein Dasein abgeschlossen,", "tokens": ["Wie", "war", "mein", "Da\u00b7sein", "ab\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als ich im gr\u00fcnumhegten Haus,", "tokens": ["Als", "ich", "im", "gr\u00fc\u00b7num\u00b7heg\u00b7ten", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Lerchenschlag und Fichtensprossen", "tokens": ["Durch", "Ler\u00b7chen\u00b7schlag", "und", "Fich\u00b7tens\u00b7pros\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch tr\u00e4umt\u2019 in den Azur hinaus!", "tokens": ["Noch", "tr\u00e4umt'", "in", "den", "A\u00b7zur", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Als keinen Blick ich noch erkannte,", "tokens": ["Als", "kei\u00b7nen", "Blick", "ich", "noch", "er\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als den des Strahles durch\u2019s Gezweig,", "tokens": ["Als", "den", "des", "Strah\u00b7les", "durch's", "Ge\u00b7zweig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die Felsen meine Br\u00fcder nannte,", "tokens": ["Die", "Fel\u00b7sen", "mei\u00b7ne", "Br\u00fc\u00b7der", "nann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schwester mein Spiegelbild im Teich!", "tokens": ["Schwes\u00b7ter", "mein", "Spie\u00b7gel\u00b7bild", "im", "Teich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Nicht rede ich von jenen Jahren,", "tokens": ["Nicht", "re\u00b7de", "ich", "von", "je\u00b7nen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die d\u00e4mmernd uns die Kindheit beut, \u2014", "tokens": ["Die", "d\u00e4m\u00b7mernd", "uns", "die", "Kind\u00b7heit", "beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "PPER", "ART", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nein, so verd\u00e4mmert und zerfahren", "tokens": ["Nein", ",", "so", "ver\u00b7d\u00e4m\u00b7mert", "und", "zer\u00b7fah\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "VVPP", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "War meine ganze Jugendzeit!", "tokens": ["War", "mei\u00b7ne", "gan\u00b7ze", "Ju\u00b7gend\u00b7zeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wohl sah ich freundliche Gestalten", "tokens": ["Wohl", "sah", "ich", "freund\u00b7li\u00b7che", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am Horizont vor\u00fcberflieh\u2019n;", "tokens": ["Am", "Ho\u00b7ri\u00b7zont", "vor\u00b7\u00fcber\u00b7flieh'n", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich konnte hei\u00dfe H\u00e4nde halten", "tokens": ["Ich", "konn\u00b7te", "hei\u00b7\u00dfe", "H\u00e4n\u00b7de", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und hei\u00dfe Lippen an mich zieh\u2019n.", "tokens": ["Und", "hei\u00b7\u00dfe", "Lip\u00b7pen", "an", "mich", "zieh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "ADJA", "NN", "APPR", "PPER", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich h\u00f6rte ihres Gru\u00dfes Pochen,", "tokens": ["Ich", "h\u00f6r\u00b7te", "ih\u00b7res", "Gru\u00b7\u00dfes", "Po\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr leises Wispern um mein Haus,", "tokens": ["Ihr", "lei\u00b7ses", "Wis\u00b7pern", "um", "mein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sandte schwimmend, halbgebrochen,", "tokens": ["Und", "sand\u00b7te", "schwim\u00b7mend", ",", "halb\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur einen Seufzer halb hinaus.", "tokens": ["Nur", "ei\u00b7nen", "Seuf\u00b7zer", "halb", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich f\u00fchlte ihres Hauches F\u00e4cheln", "tokens": ["Ich", "f\u00fchl\u00b7te", "ih\u00b7res", "Hau\u00b7ches", "F\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und war doch keine Blume s\u00fc\u00df!", "tokens": ["Und", "war", "doch", "kei\u00b7ne", "Blu\u00b7me", "s\u00fc\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sah der Liebe Engel l\u00e4cheln,", "tokens": ["Ich", "sah", "der", "Lie\u00b7be", "En\u00b7gel", "l\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und hatte doch kein Paradies.", "tokens": ["Und", "hat\u00b7te", "doch", "kein", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Mir war, als habe in den Noten", "tokens": ["Mir", "war", ",", "als", "ha\u00b7be", "in", "den", "No\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich jeder Ton an mich verwirrt,", "tokens": ["Sich", "je\u00b7der", "Ton", "an", "mich", "ver\u00b7wirrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich jede Hand, die mir geboten,", "tokens": ["Sich", "je\u00b7de", "Hand", ",", "die", "mir", "ge\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Dunkel wunderlich verirrt.", "tokens": ["Im", "Dun\u00b7kel", "wun\u00b7der\u00b7lich", "ver\u00b7irrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Verschlossen blieb ich, eingeschlossen", "tokens": ["Ver\u00b7schlos\u00b7sen", "blieb", "ich", ",", "ein\u00b7ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In meiner Tr\u00e4ume Zauberthurm,", "tokens": ["In", "mei\u00b7ner", "Tr\u00e4u\u00b7me", "Zau\u00b7ber\u00b7thurm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Blitze waren mir Genossen", "tokens": ["Die", "Blit\u00b7ze", "wa\u00b7ren", "mir", "Ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Liebesstimme mir der Sturm.", "tokens": ["Und", "Lie\u00b7bess\u00b7tim\u00b7me", "mir", "der", "Sturm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Dem Wald lie\u00df ich ein Lied erschallen,", "tokens": ["Dem", "Wald", "lie\u00df", "ich", "ein", "Lied", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie nie vor einem Menschenohr,", "tokens": ["Wie", "nie", "vor", "ei\u00b7nem", "Men\u00b7schen\u00b7ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und meine Thr\u00e4ne lie\u00df ich fallen,", "tokens": ["Und", "mei\u00b7ne", "Thr\u00e4\u00b7ne", "lie\u00df", "ich", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die hei\u00dfe, in den Blumenflor.", "tokens": ["Die", "hei\u00b7\u00dfe", ",", "in", "den", "Blu\u00b7men\u00b7flor", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und alle Pfade mu\u00dft\u2019 ich fragen:", "tokens": ["Und", "al\u00b7le", "Pfa\u00b7de", "mu\u00dft'", "ich", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kennt V\u00f6gel ihr und Strahlen auch?", "tokens": ["Kennt", "V\u00f6\u00b7gel", "ihr", "und", "Strah\u00b7len", "auch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch keinen: wohin magst du tragen,", "tokens": ["Doch", "kei\u00b7nen", ":", "wo\u00b7hin", "magst", "du", "tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "$.", "PWAV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von welchem Odem schwillt dein Hauch?", "tokens": ["Von", "wel\u00b7chem", "O\u00b7dem", "schwillt", "dein", "Hauch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Wie ist das anders nun geworden,", "tokens": ["Wie", "ist", "das", "an\u00b7ders", "nun", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PDS", "ADV", "ADV", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seit ich in\u2019s Auge dir geblickt;", "tokens": ["Seit", "ich", "in's", "Au\u00b7ge", "dir", "ge\u00b7blickt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie ist nun jeder Welle Borden", "tokens": ["Wie", "ist", "nun", "je\u00b7der", "Wel\u00b7le", "Bor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Menschenbildni\u00df eingedr\u00fcckt!", "tokens": ["Ein", "Men\u00b7schen\u00b7bild\u00b7ni\u00df", "ein\u00b7ge\u00b7dr\u00fcckt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Wie f\u00fchl\u2019 ich allen warmen H\u00e4nden", "tokens": ["Wie", "f\u00fchl'", "ich", "al\u00b7len", "war\u00b7men", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun ihre leisen Pulse nach,", "tokens": ["Nun", "ih\u00b7re", "lei\u00b7sen", "Pul\u00b7se", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jedem Blick sein scheues Wenden", "tokens": ["Und", "je\u00b7dem", "Blick", "sein", "scheu\u00b7es", "Wen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und jeder schweren Brust ihr Ach.", "tokens": ["Und", "je\u00b7der", "schwe\u00b7ren", "Brust", "ihr", "Ach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Und alle Pfade m\u00f6cht\u2019 ich fragen:", "tokens": ["Und", "al\u00b7le", "Pfa\u00b7de", "m\u00f6cht'", "ich", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo zieht ihr hin, wo ist das Haus,", "tokens": ["Wo", "zieht", "ihr", "hin", ",", "wo", "ist", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In dem lebend\u2019ge Herzen schlagen,", "tokens": ["In", "dem", "le\u00b7ben\u00b7d'\u00b7ge", "Her\u00b7zen", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lebend\u2019ger Odem schwillt hinaus?", "tokens": ["Le\u00b7ben\u00b7d'\u00b7ger", "O\u00b7dem", "schwillt", "hin\u00b7aus", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.14": {"line.1": {"text": "Entz\u00fcnden m\u00f6cht\u2019 ich alle Kerzen", "tokens": ["Ent\u00b7z\u00fcn\u00b7den", "m\u00f6cht'", "ich", "al\u00b7le", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und rufen jedem m\u00fcden Sein:", "tokens": ["Und", "ru\u00b7fen", "je\u00b7dem", "m\u00fc\u00b7den", "Sein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf ist mein Paradies im Herzen,", "tokens": ["Auf", "ist", "mein", "Pa\u00b7ra\u00b7dies", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zieht Alle, Alle nun hinein!", "tokens": ["Zieht", "Al\u00b7le", ",", "Al\u00b7le", "nun", "hin\u00b7ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PIS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}