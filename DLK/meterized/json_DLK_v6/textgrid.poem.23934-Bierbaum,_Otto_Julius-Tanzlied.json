{"textgrid.poem.23934": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Tanzlied", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Reihen geschlungen,", "tokens": ["Es", "ist", "ein", "Rei\u00b7hen", "ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein Reihen auf dem gr\u00fcnen Plan,", "tokens": ["Ein", "Rei\u00b7hen", "auf", "dem", "gr\u00fc\u00b7nen", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ist ein Lied gesungen,", "tokens": ["Und", "ist", "ein", "Lied", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das hebt mit Sehnen an,", "tokens": ["Das", "hebt", "mit", "Seh\u00b7nen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit Sehnen, also s\u00fc\u00dfe,", "tokens": ["Mit", "Seh\u00b7nen", ",", "al\u00b7so", "s\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df Weinen sich mit Lachen paart:", "tokens": ["Da\u00df", "Wei\u00b7nen", "sich", "mit", "La\u00b7chen", "paart", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hebt, hebt im Tanz die F\u00fc\u00dfe", "tokens": ["Hebt", ",", "hebt", "im", "Tanz", "die", "F\u00fc\u00b7\u00dfe"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auf lenzeliche Art.", "tokens": ["Auf", "len\u00b7ze\u00b7li\u00b7che", "Art", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}