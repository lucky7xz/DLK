{"textgrid.poem.42035": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Ebene Landschaft", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Erde kam, ein grauer Strom, geflossen.", "tokens": ["Die", "Er\u00b7de", "kam", ",", "ein", "grau\u00b7er", "Strom", ",", "ge\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein Damm, der ihre Flut zusammenh\u00e4lt,", "tokens": ["Kein", "Damm", ",", "der", "ih\u00b7re", "Flut", "zu\u00b7sam\u00b7men\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie hat sich \u00fcber Berg und Tal und Haus ergossen.", "tokens": ["Sie", "hat", "sich", "\u00fc\u00b7ber", "Berg", "und", "Tal", "und", "Haus", "er\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "NN", "KON", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Fern, wo ein schmaler Strich den Horizont erhellt,", "tokens": ["Fern", ",", "wo", "ein", "schma\u00b7ler", "Strich", "den", "Ho\u00b7ri\u00b7zont", "er\u00b7hellt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Baum. Entwurzelt. Der ins Leere f\u00e4llt.", "tokens": ["Ein", "Baum", ".", "Ent\u00b7wur\u00b7zelt", ".", "Der", "ins", "Lee\u00b7re", "f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "$.", "ART", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}