{"textgrid.poem.39009": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: So oft mein Fu\u00df hier wandelt", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So oft mein Fu\u00df hier wandelt", "tokens": ["So", "oft", "mein", "Fu\u00df", "hier", "wan\u00b7delt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vernehm' ich Geistergefl\u00fcster,", "tokens": ["Ver\u00b7nehm'", "ich", "Geis\u00b7ter\u00b7ge\u00b7fl\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Herab vom hohen Capitol,", "tokens": ["Her\u00b7ab", "vom", "ho\u00b7hen", "Ca\u00b7pi\u00b7tol", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch der S\u00e4ulen Lockenhaupt,", "tokens": ["Durch", "der", "S\u00e4u\u00b7len", "Lo\u00b7cken\u00b7haupt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "An den Pallasttr\u00fcmmern C\u00e4sars.", "tokens": ["An", "den", "Pal\u00b7last\u00b7tr\u00fcm\u00b7mern", "C\u00e4\u00b7sars", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Welche Welt lehrt aus dem Schutte,", "tokens": ["Wel\u00b7che", "Welt", "lehrt", "aus", "dem", "Schut\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus des Coliseums W\u00f6lbungen,", "tokens": ["Aus", "des", "Co\u00b7li\u00b7seums", "W\u00f6l\u00b7bun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vom Friedenstempel, und Titus Triumph,", "tokens": ["Vom", "Frie\u00b7dens\u00b7tem\u00b7pel", ",", "und", "Ti\u00b7tus", "Tri\u00b7umph", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "NE", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Welche Sage wandelt noch Wunder sprechend", "tokens": ["Wel\u00b7che", "Sa\u00b7ge", "wan\u00b7delt", "noch", "Wun\u00b7der", "spre\u00b7chend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "NN", "VVFIN", "ADV", "NN", "VVPP"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.5": {"text": "Unter diesen B\u00f6gen!", "tokens": ["Un\u00b7ter", "die\u00b7sen", "B\u00f6\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Hier m\u00fcssen in heiliger Stimmung", "tokens": ["Hier", "m\u00fcs\u00b7sen", "in", "hei\u00b7li\u00b7ger", "Stim\u00b7mung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "F\u00fcrsten und Priester einhergehn,", "tokens": ["F\u00fcrs\u00b7ten", "und", "Pries\u00b7ter", "ein\u00b7her\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und der Denker, dem die Geschichte", "tokens": ["Und", "der", "Den\u00b7ker", ",", "dem", "die", "Ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Gottes Gegenwart furchtbar zeigt,", "tokens": ["Got\u00b7tes", "Ge\u00b7gen\u00b7wart", "furcht\u00b7bar", "zeigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Furchtbar und tr\u00f6stend,", "tokens": ["Furcht\u00b7bar", "und", "tr\u00f6s\u00b7tend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ersch\u00fctternd und beruhigend.", "tokens": ["Er\u00b7sch\u00fct\u00b7ternd", "und", "be\u00b7ru\u00b7hi\u00b7gend", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Schaut alle hier die schmerzlichste Wunde,", "tokens": ["Schaut", "al\u00b7le", "hier", "die", "schmerz\u00b7lichs\u00b7te", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die die Zerst\u00f6rung schlug,", "tokens": ["Die", "die", "Zer\u00b7st\u00f6\u00b7rung", "schlug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und die noch immer blutet.", "tokens": ["Und", "die", "noch", "im\u00b7mer", "blu\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier spricht der zuckende Leichnam", "tokens": ["Hier", "spricht", "der", "zu\u00b7cken\u00b7de", "Leich\u00b7nam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Erhabne Worte.", "tokens": ["Er\u00b7hab\u00b7ne", "Wor\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Aber die Sterblichen", "tokens": ["A\u00b7ber", "die", "Sterb\u00b7li\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wandeln ruhig dahin,", "tokens": ["Wan\u00b7deln", "ru\u00b7hig", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PAV", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und wohl ist Allen,", "tokens": ["Und", "wohl", "ist", "Al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NE", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Da\u00df ein l\u00e4chelnder Genius", "tokens": ["Da\u00df", "ein", "l\u00e4\u00b7cheln\u00b7der", "Ge\u00b7nius"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihnen schalkhaft die h\u00fcllende", "tokens": ["Ih\u00b7nen", "schalk\u00b7haft", "die", "h\u00fcl\u00b7len\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.6": {"text": "Binde vor das Auge geheftet.", "tokens": ["Bin\u00b7de", "vor", "das", "Au\u00b7ge", "ge\u00b7hef\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}