{"textgrid.poem.34490": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "[oftmals, wenn ich gewahrte]", "genre": "verse", "period": "N.A.", "pub_year": 1888, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oftmals, wenn ich gewahrte, wie tief und wie treu du mich liebtest,", "tokens": ["Oft\u00b7mals", ",", "wenn", "ich", "ge\u00b7wahr\u00b7te", ",", "wie", "tief", "und", "wie", "treu", "du", "mich", "lieb\u00b7test", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "PWAV", "ADJD", "KON", "PWAV", "ADJD", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "sind mir die Worte entflohn: Ach ich verdiene das nicht.", "tokens": ["sind", "mir", "die", "Wor\u00b7te", "ent\u00b7flohn", ":", "Ach", "ich", "ver\u00b7die\u00b7ne", "das", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PTKVZ", "$.", "ITJ", "PPER", "VVFIN", "PDS", "PTKNEG", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Angstvoll sahst du mich an mit deinen leuchtenden Augen,", "tokens": ["Angst\u00b7voll", "sahst", "du", "mich", "an", "mit", "dei\u00b7nen", "leuch\u00b7ten\u00b7den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "APPR", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "---+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "nieder schlug ich den Blick, k\u00fcsste dir schweigend die Hand.", "tokens": ["nie\u00b7der", "schlug", "ich", "den", "Blick", ",", "k\u00fcss\u00b7te", "dir", "schwei\u00b7gend", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}}}}}