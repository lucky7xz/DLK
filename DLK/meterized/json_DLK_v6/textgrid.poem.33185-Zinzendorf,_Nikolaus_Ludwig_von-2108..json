{"textgrid.poem.33185": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "2108.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie die kr\u00e4fte von der h\u00f6ll, die noch nicht bezwungen, unserem Immanuel auf den leib gedrungen;", "tokens": ["Wie", "die", "kr\u00e4f\u00b7te", "von", "der", "h\u00f6ll", ",", "die", "noch", "nicht", "be\u00b7zwun\u00b7gen", ",", "un\u00b7se\u00b7rem", "Im\u00b7ma\u00b7nu\u00b7el", "auf", "den", "leib", "ge\u00b7drun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "PTKNEG", "VVPP", "$,", "PPOSAT", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "--+------+-+-+--+--+--+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie der GOTT von ewigkeit, voller pracht und wonne, hinge ohne einigs kleid an der lichten sonne;", "tokens": ["Wie", "der", "GoTT", "von", "e\u00b7wig\u00b7keit", ",", "vol\u00b7ler", "pracht", "und", "won\u00b7ne", ",", "hin\u00b7ge", "oh\u00b7ne", "ei\u00b7nigs", "kleid", "an", "der", "lich\u00b7ten", "son\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "$,", "ADJA", "NN", "KON", "PWAV", "$,", "VVFIN", "APPR", "PIS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+-+-+-+-+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Und was da das keusche herz etwa wol gef\u00fchlet, als ", "tokens": ["Und", "was", "da", "das", "keu\u00b7sche", "herz", "et\u00b7wa", "wol", "ge\u00b7f\u00fch\u00b7let", ",", "als"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PWS", "ADV", "ART", "ADJA", "NN", "ADV", "ADV", "VVPP", "$,", "KOUS"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Aber freylich ist es so, allerliebste br\u00fcder! flammt ich stets so in der loh der verwundten glieder;", "tokens": ["A\u00b7ber", "frey\u00b7lich", "ist", "es", "so", ",", "al\u00b7ler\u00b7liebs\u00b7te", "br\u00fc\u00b7der", "!", "flammt", "ich", "stets", "so", "in", "der", "loh", "der", "ver\u00b7wund\u00b7ten", "glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$.", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+-+-+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.5": {"line.1": {"text": "W\u00e4r es nur der creuzes-thon, der mich afficirte, und die meditation, die mein herze r\u00fchrte:", "tokens": ["W\u00e4r", "es", "nur", "der", "creu\u00b7zes\u00b7thon", ",", "der", "mich", "af\u00b7fi\u00b7cir\u00b7te", ",", "und", "die", "me\u00b7di\u00b7ta\u00b7ti\u00b7on", ",", "die", "mein", "her\u00b7ze", "r\u00fchr\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "KON", "ART", "NN", "$,", "PRELS", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-+--+--++-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.6": {"line.1": {"text": "Nach der absolution aus der sechsten schrunde Kyrie Eleison! nim mich in die wunde:", "tokens": ["Nach", "der", "ab\u00b7so\u00b7lu\u00b7ti\u00b7on", "aus", "der", "sechs\u00b7ten", "schrun\u00b7de", "Ky\u00b7rie", "E\u00b7lei\u00b7son", "!", "nim", "mich", "in", "die", "wun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "ADJA", "ADJA", "NN", "NE", "$.", "VVIMP", "PPER", "APPR", "ART", "ADJA", "$."], "meter": "+-+-+-+--+-+-+--+--++-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.7": {"line.1": {"text": "Bis ich gebe ku\u00df auf ku\u00df, soll mein herze lachzen nach dem seligen genu\u00df der gel\u00e4hmten fl\u00e4chsen.", "tokens": ["Bis", "ich", "ge\u00b7be", "ku\u00df", "auf", "ku\u00df", ",", "soll", "mein", "her\u00b7ze", "lach\u00b7zen", "nach", "dem", "se\u00b7li\u00b7gen", "ge\u00b7nu\u00df", "der", "ge\u00b7l\u00e4hm\u00b7ten", "fl\u00e4ch\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "NN", "APPR", "NN", "$,", "VMFIN", "PPOSAT", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-+-+--+-+-", "measure": "iambic.octa.plus"}}}}}