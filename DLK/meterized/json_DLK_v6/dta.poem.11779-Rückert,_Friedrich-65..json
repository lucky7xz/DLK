{"dta.poem.11779": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "65.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein hohes R\u00e4thsel ists, wie alle sind berufen", "tokens": ["Ein", "ho\u00b7hes", "R\u00e4th\u00b7sel", "ists", ",", "wie", "al\u00b7le", "sind", "be\u00b7ru\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "PWAV", "PIS", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum H\u00f6chsten, keiner doch ersteiget alle Stufen;", "tokens": ["Zum", "H\u00f6chs\u00b7ten", ",", "kei\u00b7ner", "doch", "er\u00b7stei\u00b7get", "al\u00b7le", "Stu\u00b7fen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PIS", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie mancher auch vorlieb mit einer untern nimmt,", "tokens": ["Wie", "man\u00b7cher", "auch", "vor\u00b7lieb", "mit", "ei\u00b7ner", "un\u00b7tern", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und unbescheiden den wol nennt, der h\u00f6her klimmt.", "tokens": ["Und", "un\u00b7be\u00b7schei\u00b7den", "den", "wol", "nennt", ",", "der", "h\u00f6\u00b7her", "klimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADV", "VVFIN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch weislich hats gef\u00fcgt, der h\u00f6her sitzt als alle,", "tokens": ["Doch", "weis\u00b7lich", "hats", "ge\u00b7f\u00fcgt", ",", "der", "h\u00f6\u00b7her", "sitzt", "als", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "VVPP", "$,", "PRELS", "ADJD", "VVFIN", "KOKOM", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df jeder, wo er steht und stehn kann, sich gefalle;", "tokens": ["Da\u00df", "je\u00b7der", ",", "wo", "er", "steht", "und", "stehn", "kann", ",", "sich", "ge\u00b7fal\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVINF", "VMFIN", "$,", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da\u00df jeder gleich entfernt von sich das H\u00f6chste sieht,", "tokens": ["Da\u00df", "je\u00b7der", "gleich", "ent\u00b7fernt", "von", "sich", "das", "H\u00f6chs\u00b7te", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADJD", "APPR", "PRF", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und es in seiner Weis' heran, herunter, zieht.", "tokens": ["Und", "es", "in", "sei\u00b7ner", "Weis'", "he\u00b7ran", ",", "her\u00b7un\u00b7ter", ",", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$,", "PTKVZ", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und wen hinan es zieht, der zieht ihm nach, und sieht,", "tokens": ["Und", "wen", "hi\u00b7nan", "es", "zieht", ",", "der", "zieht", "ihm", "nach", ",", "und", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je h\u00f6her hin er folgt, je h\u00f6her hin es flieht.", "tokens": ["Je", "h\u00f6\u00b7her", "hin", "er", "folgt", ",", "je", "h\u00f6\u00b7her", "hin", "es", "flieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PPER", "VVFIN", "$,", "ADV", "ADJD", "PTKVZ", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Hoch hebe deinen Geist zum Ew'gen ein Verlangen,", "tokens": ["Hoch", "he\u00b7be", "dei\u00b7nen", "Geist", "zum", "Ew'\u00b7gen", "ein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Doch f\u00fchle dich mit Lust von Endlichkeit umfangen.", "tokens": ["Doch", "f\u00fch\u00b7le", "dich", "mit", "Lust", "von", "End\u00b7lich\u00b7keit", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Alles ist gar zu viel, und gar zu wenig Nichts;", "tokens": ["Al\u00b7les", "ist", "gar", "zu", "viel", ",", "und", "gar", "zu", "we\u00b7nig", "Nichts", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "PTKA", "PIS", "$,", "KON", "ADV", "PTKA", "PIS", "PIS", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Die Malerei bedarf der Schatten und des Lichts.", "tokens": ["Die", "Ma\u00b7le\u00b7rei", "be\u00b7darf", "der", "Schat\u00b7ten", "und", "des", "Lichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}