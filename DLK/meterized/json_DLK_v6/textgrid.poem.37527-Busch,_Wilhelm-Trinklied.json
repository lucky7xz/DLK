{"textgrid.poem.37527": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Trinklied", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gestern ging ich wieder mal", "tokens": ["Ge\u00b7stern", "ging", "ich", "wie\u00b7der", "mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "In die Schenke schnelle,", "tokens": ["In", "die", "Schen\u00b7ke", "schnel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie der durst'ge Pilgersmann", "tokens": ["Wie", "der", "dur\u00b7st'\u00b7ge", "Pil\u00b7gers\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eilt aus der Kapelle.", "tokens": ["Eilt", "aus", "der", "Ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Alldieweil der Durst so gro\u00df,", "tokens": ["All\u00b7die\u00b7weil", "der", "Durst", "so", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Trink ich etwas eil'ger", "tokens": ["Trink", "ich", "et\u00b7was", "eil'\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und ergl\u00e4nze alsobald", "tokens": ["Und", "er\u00b7gl\u00e4n\u00b7ze", "al\u00b7so\u00b7bald"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein neuer Heilger.", "tokens": ["Wie", "ein", "neu\u00b7er", "Heil\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wie der Pater Gabriel", "tokens": ["Wie", "der", "Pa\u00b7ter", "Ga\u00b7bri\u00b7el"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Werd ich allnachgrade;", "tokens": ["Werd", "ich", "all\u00b7nach\u00b7gra\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Zwicke schon der Kellnerin", "tokens": ["Zwi\u00b7cke", "schon", "der", "Kell\u00b7ne\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Listig in die Wade. \u2013", "tokens": ["Lis\u00b7tig", "in", "die", "Wa\u00b7de", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Beim Getr\u00e4nke lieb ich mir", "tokens": ["Beim", "Ge\u00b7tr\u00e4n\u00b7ke", "lieb", "ich", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "So ein Spiel ein kleines;", "tokens": ["So", "ein", "Spiel", "ein", "klei\u00b7nes", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ach, mein Geld ist hin wie einst", "tokens": ["Ach", ",", "mein", "Geld", "ist", "hin", "wie", "einst"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "KOKOM", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kozmianen seines.", "tokens": ["Koz\u00b7mi\u00b7a\u00b7nen", "sei\u00b7nes", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPOSAT", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Da der Wirt auf Zahlung dringt,", "tokens": ["Da", "der", "Wirt", "auf", "Zah\u00b7lung", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fang ich an zu tosen.", "tokens": ["Fang", "ich", "an", "zu", "to\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Drauf ergeht's mir wie dem Erz-", "tokens": ["Drauf", "er\u00b7geht's", "mir", "wie", "dem", "Er\u00b7z"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "KOKOM", "ART", "TRUNC"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bischof hint in Posen.", "tokens": ["Bi\u00b7schof", "hint", "in", "Po\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Meinen Rock verwahrt der Wirt", "tokens": ["Mei\u00b7nen", "Rock", "ver\u00b7wahrt", "der", "Wirt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Schelle zieht er:", "tokens": ["Und", "die", "Schel\u00b7le", "zieht", "er", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "\u00bbheda, Hausel! Schiebe fort", "tokens": ["\u00bb", "he\u00b7da", ",", "Hau\u00b7sel", "!", "Schie\u00b7be", "fort"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["$(", "ITJ", "$,", "NN", "$.", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Diesen Jesuiter!\u00ab", "tokens": ["Die\u00b7sen", "Je\u00b7su\u00b7i\u00b7ter", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PDAT", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Als ich auf der Gasse lag,", "tokens": ["Als", "ich", "auf", "der", "Gas\u00b7se", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00e4gt die Glocke zw\u00f6lfe,", "tokens": ["Schl\u00e4gt", "die", "Glo\u00b7cke", "zw\u00f6l\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und ich grolle tiefemp\u00f6rt", "tokens": ["Und", "ich", "grol\u00b7le", "tie\u00b7fem\u00b7p\u00f6rt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein alter Welfe.", "tokens": ["Wie", "ein", "al\u00b7ter", "Wel\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Gleich so fragt mich ein Gendarm,", "tokens": ["Gleich", "so", "fragt", "mich", "ein", "Gen\u00b7darm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Was ich hier bezweckte.", "tokens": ["Was", "ich", "hier", "be\u00b7zweck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Keine Auskunft geben wir", "tokens": ["Kei\u00b7ne", "Aus\u00b7kunft", "ge\u00b7ben", "wir"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seminarpr\u00e4fekte!", "tokens": ["Se\u00b7mi\u00b7nar\u00b7pr\u00e4\u00b7fek\u00b7te", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Darum sitz ich heut im Loch. \u2013", "tokens": ["Da\u00b7rum", "sitz", "ich", "heut", "im", "Loch", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$.", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Ach! und dieser Kater!", "tokens": ["Ach", "!", "und", "die\u00b7ser", "Ka\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "PDAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Fluchend geh ich auf und ab,", "tokens": ["Flu\u00b7chend", "geh", "ich", "auf", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein heil'ger Vater.", "tokens": ["Wie", "ein", "heil'\u00b7ger", "Va\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}}}}}