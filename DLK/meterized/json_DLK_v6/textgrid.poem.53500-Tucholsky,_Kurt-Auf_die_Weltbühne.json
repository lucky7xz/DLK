{"textgrid.poem.53500": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Weltb\u00fchne", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein gutes Blatt! Wie hast du dich ver\u00e4ndert!", "tokens": ["Mein", "gu\u00b7tes", "Blatt", "!", "Wie", "hast", "du", "dich", "ver\u00b7\u00e4n\u00b7dert", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PWAV", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Musentempel schlie\u00dft du beinah zu;", "tokens": ["Den", "Mu\u00b7sen\u00b7tem\u00b7pel", "schlie\u00dft", "du", "bei\u00b7nah", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "mit Politik, Kunst, Wirtschaft dicht beb\u00e4ndert,", "tokens": ["mit", "Po\u00b7li\u00b7tik", ",", "Kunst", ",", "Wirt\u00b7schaft", "dicht", "be\u00b7b\u00e4n\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "so geht dein Vorhang auf: auch du, mein Kind, auch du?", "tokens": ["so", "geht", "dein", "Vor\u00b7hang", "auf", ":", "auch", "du", ",", "mein", "Kind", ",", "auch", "du", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$.", "ADV", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du willst dich gleichfalls in den Strudel st\u00fcrzen?", "tokens": ["Du", "willst", "dich", "gleich\u00b7falls", "in", "den", "Stru\u00b7del", "st\u00fcr\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Randstaaten? Westfront? Die Ver\u00e4nderungswahl?", "tokens": ["Rand\u00b7staa\u00b7ten", "?", "West\u00b7front", "?", "Die", "Ver\u00b7\u00e4n\u00b7de\u00b7rungs\u00b7wahl", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "$.", "ART", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Nur eines kann mir meinen Kummer w\u00fcrzen:", "tokens": ["Nur", "ei\u00b7nes", "kann", "mir", "mei\u00b7nen", "Kum\u00b7mer", "w\u00fcr\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es war einmal . . .", "tokens": ["Es", "war", "ein\u00b7mal", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$.", "$.", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Es war einmal . . . da glaubten wir noch beide", "tokens": ["Es", "war", "ein\u00b7mal", ".", ".", ".", "da", "glaub\u00b7ten", "wir", "noch", "bei\u00b7de"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$.", "$.", "$.", "ADV", "VVFIN", "PPER", "ADV", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "an Kunst und an Kultur, an Menschentum \u2013", "tokens": ["an", "Kunst", "und", "an", "Kul\u00b7tur", ",", "an", "Men\u00b7schen\u00b7tum", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,", "APPR", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "an deine ziegelrote Wand schrieb ich mit Kreide", "tokens": ["an", "dei\u00b7ne", "zie\u00b7gel\u00b7ro\u00b7te", "Wand", "schrieb", "ich", "mit", "Krei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die Namen meiner Lieben an zum Ruhm.", "tokens": ["die", "Na\u00b7men", "mei\u00b7ner", "Lie\u00b7ben", "an", "zum", "Ruhm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir dachten: essen und organisieren", "tokens": ["Wir", "dach\u00b7ten", ":", "es\u00b7sen", "und", "or\u00b7ga\u00b7ni\u00b7sie\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "VVINF", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "sind Selbstverst\u00e4ndlichkeiten, tief im Tal \u2013", "tokens": ["sind", "Selbst\u00b7ver\u00b7st\u00e4nd\u00b7lich\u00b7kei\u00b7ten", ",", "tief", "im", "Tal", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "und auf den Bergen gehen wir spazieren . . .", "tokens": ["und", "auf", "den", "Ber\u00b7gen", "ge\u00b7hen", "wir", "spa\u00b7zie\u00b7ren", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "VVINF", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es war einmal . . .", "tokens": ["Es", "war", "ein\u00b7mal", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$.", "$.", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Du lieber Gott, wie hat sich das gewandelt!", "tokens": ["Du", "lie\u00b7ber", "Gott", ",", "wie", "hat", "sich", "das", "ge\u00b7wan\u00b7delt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "$,", "PWAV", "VAFIN", "PRF", "PDS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir schuften, bis dem Land die Schwarte knackt.", "tokens": ["Wir", "schuf\u00b7ten", ",", "bis", "dem", "Land", "die", "Schwar\u00b7te", "knackt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und kein Professor, der nicht gerne handelt", "tokens": ["Und", "kein", "Pro\u00b7fes\u00b7sor", ",", "der", "nicht", "ger\u00b7ne", "han\u00b7delt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PTKNEG", "ADV", "VVFIN"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "mit weichem Klitschebrot, das er sich backt.", "tokens": ["mit", "wei\u00b7chem", "Klit\u00b7sche\u00b7brot", ",", "das", "er", "sich", "backt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es war einmal . . . Gl\u00fcck auf zur neuen Reise!", "tokens": ["Es", "war", "ein\u00b7mal", ".", ".", ".", "Gl\u00fcck", "auf", "zur", "neu\u00b7en", "Rei\u00b7se", "!"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$.", "$.", "$.", "NN", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Eng wars einmal \u2013 heut bist du bunt und weit.", "tokens": ["Eng", "wars", "ein\u00b7mal", "\u2013", "heut", "bist", "du", "bunt", "und", "weit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "$(", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch kehr noch manchmal dich zur\u00fcck im Kreise", "tokens": ["Doch", "kehr", "noch", "manch\u00b7mal", "dich", "zu\u00b7r\u00fcck", "im", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADV", "PPER", "PTKVZ", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "zur alten Zeit!", "tokens": ["zur", "al\u00b7ten", "Zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}