{"textgrid.poem.25155": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "[ich lachte heut - warum? Wer sagt es mir?]", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lachte heut \u2013 warum? Wer sagt es mir?", "tokens": ["Ich", "lach\u00b7te", "heut", "\u2013", "wa\u00b7rum", "?", "Wer", "sagt", "es", "mir", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "PWAV", "$.", "PWS", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kein Gott, kein D\u00e4mon ist, der Antwort sagt,", "tokens": ["Kein", "Gott", ",", "kein", "D\u00e4\u00b7mon", "ist", ",", "der", "Ant\u00b7wort", "sagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VAFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der mir aus Himmel, H\u00f6lle Antwort wagt!", "tokens": ["Der", "mir", "aus", "Him\u00b7mel", ",", "H\u00f6l\u00b7le", "Ant\u00b7wort", "wagt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "$,", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nur Schweigen, \u2013 Herz, so wend ich mich zu dir:", "tokens": ["Nur", "Schwei\u00b7gen", ",", "\u2013", "Herz", ",", "so", "wend", "ich", "mich", "zu", "dir", ":"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "$(", "NN", "$,", "ADV", "KOUS", "PPER", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Herz! Du und ich sind traurig und allein;", "tokens": ["Herz", "!", "Du", "und", "ich", "sind", "trau\u00b7rig", "und", "al\u00b7lein", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "KON", "PPER", "VAFIN", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich frage: weshalb lachte ich? \u2013 Nun? Nun? \u2013", "tokens": ["Ich", "fra\u00b7ge", ":", "we\u00b7shalb", "lach\u00b7te", "ich", "?", "\u2013", "Nun", "?", "Nun", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWAV", "VVFIN", "PPER", "$.", "$(", "ADV", "$.", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O Dunkel, Dunkel! Und ich kann nicht ruhn,", "tokens": ["O", "Dun\u00b7kel", ",", "Dun\u00b7kel", "!", "Und", "ich", "kann", "nicht", "ruhn", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$.", "KON", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Himmel, H\u00f6lle, Herz h\u00f6hnt meine Pein!", "tokens": ["Und", "Him\u00b7mel", ",", "H\u00f6l\u00b7le", ",", "Herz", "h\u00f6hnt", "mei\u00b7ne", "Pein", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich lachte heut \u2013 warum? \u2013 Kurz ist das Leben,", "tokens": ["Ich", "lach\u00b7te", "heut", "\u2013", "wa\u00b7rum", "?", "\u2013", "Kurz", "ist", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "PWAV", "$.", "$(", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sein Seligstes geno\u00df beschwingt mein Geist \u2013", "tokens": ["Sein", "Se\u00b7ligs\u00b7tes", "ge\u00b7no\u00df", "be\u00b7schwingt", "mein", "Geist", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch w\u00fcrd' ich heute gern dem Tod mich geben,", "tokens": ["Doch", "w\u00fcrd'", "ich", "heu\u00b7te", "gern", "dem", "Tod", "mich", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der unsre bunten Fahnen schrill zerrei\u00dft:", "tokens": ["Der", "uns\u00b7re", "bun\u00b7ten", "Fah\u00b7nen", "schrill", "zer\u00b7rei\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Lied, Ruhm und Sch\u00f6nheit t\u00fcrmen nur den Thron", "tokens": ["Lied", ",", "Ruhm", "und", "Sch\u00f6n\u00b7heit", "t\u00fcr\u00b7men", "nur", "den", "Thron"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr K\u00f6nig Tod \u2013 des Lebens h\u00f6chsten Lohn.", "tokens": ["F\u00fcr", "K\u00f6\u00b7nig", "Tod", "\u2013", "des", "Le\u00b7bens", "h\u00f6chs\u00b7ten", "Lohn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$(", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}