{"dta.poem.1054": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Die 2. Strophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die m\u00e4chtige streitten vmbsunst/", "tokens": ["Die", "m\u00e4ch\u00b7ti\u00b7ge", "streit\u00b7ten", "vmbsunst", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vmbsunst die helden triumfieren/", "tokens": ["Vmbsunst", "die", "hel\u00b7den", "tri\u00b7um\u00b7fie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wan jhre namen als ein dunst", "tokens": ["Wan", "jhre", "na\u00b7men", "als", "ein", "dunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "KOKOM", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In kurtzen jahren sich verlieren.", "tokens": ["In", "kurt\u00b7zen", "jah\u00b7ren", "sich", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es ist nicht gnug seine kh\u00fcnheit", "tokens": ["Es", "ist", "nicht", "gnug", "sei\u00b7ne", "kh\u00fcn\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "PPOSAT", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.6": {"text": "Seiner fl\u00fcchtigen feinden rucken", "tokens": ["Sei\u00b7ner", "fl\u00fcch\u00b7ti\u00b7gen", "fein\u00b7den", "ru\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Mit scharpfen eysen aufzutrucken", "tokens": ["Mit", "scharp\u00b7fen", "ey\u00b7sen", "auf\u00b7zu\u00b7tru\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zu seines lobs vnsterblichkeit:", "tokens": ["Zu", "sei\u00b7nes", "lobs", "vns\u00b7terb\u00b7lich\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Noch der tugent g\u00e4ntzlich ergeben", "tokens": ["Noch", "der", "tu\u00b7gent", "g\u00e4ntz\u00b7lich", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJD", "VVPP"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "(wie O grosser Printz ewer pracht)", "tokens": ["(", "wie", "O", "gros\u00b7ser", "Printz", "e\u00b7wer", "pracht", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "NE", "ADJA", "NN", "PPOSAT", "NN", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Der Vergessung zu widerstreben;", "tokens": ["Der", "Ver\u00b7ges\u00b7sung", "zu", "wi\u00b7der\u00b7stre\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Sondern es ist der Musen macht", "tokens": ["Son\u00b7dern", "es", "ist", "der", "Mu\u00b7sen", "macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Euch vnzug\u00e4nglich zubeleben.", "tokens": ["Euch", "vn\u00b7zu\u00b7g\u00e4ng\u00b7lich", "zu\u00b7be\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}