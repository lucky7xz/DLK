{"dta.poem.3201": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wieder ein Strebender, der hohes wol und vieles", "tokens": ["Wie\u00b7der", "ein", "Stre\u00b7ben\u00b7der", ",", "der", "ho\u00b7hes", "wol", "und", "vie\u00b7les"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "ART", "ADJA", "ADV", "KON", "PIS"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erstrebte, ging dahin, und unerreichten Zieles;", "tokens": ["Er\u00b7streb\u00b7te", ",", "ging", "da\u00b7hin", ",", "und", "un\u00b7er\u00b7reich\u00b7ten", "Zie\u00b7les", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PAV", "$,", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und hat, indem er es verfehlt, erreicht das Ziel,", "tokens": ["Und", "hat", ",", "in\u00b7dem", "er", "es", "ver\u00b7fehlt", ",", "er\u00b7reicht", "das", "Ziel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie jeder, der mitspielt dies Weltlusttrauerspiel.", "tokens": ["Wie", "je\u00b7der", ",", "der", "mit\u00b7spielt", "dies", "Welt\u00b7lust\u00b7trau\u00b7er\u00b7spiel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PRELS", "VVFIN", "PDS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure