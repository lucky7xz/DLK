{"textgrid.poem.33294": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Der Blick der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie die Sonne das, was Mutter Erde", "tokens": ["Wie", "die", "Son\u00b7ne", "das", ",", "was", "Mut\u00b7ter", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PDS", "$,", "PRELS", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Todt in ihrem Schoosse tr\u00e4gt,", "tokens": ["Todt", "in", "ih\u00b7rem", "Schoos\u00b7se", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00e4chtig, wie des hohen Sch\u00f6pfers Werde,", "tokens": ["M\u00e4ch\u00b7tig", ",", "wie", "des", "ho\u00b7hen", "Sch\u00f6p\u00b7fers", "Wer\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auf in's neue Leben weckt;", "tokens": ["Auf", "in's", "neu\u00b7e", "Le\u00b7ben", "weckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie sich alle Lebenspulse heben", "tokens": ["Wie", "sich", "al\u00b7le", "Le\u00b7bens\u00b7pul\u00b7se", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Von der Sonne Strahl durchz\u00fcckt,", "tokens": ["Von", "der", "Son\u00b7ne", "Strahl", "durch\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und empor der jungen Keime Streben", "tokens": ["Und", "em\u00b7por", "der", "jun\u00b7gen", "Kei\u00b7me", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aus dem Schoos der Erde dr\u00fcckt;", "tokens": ["Aus", "dem", "Schoos", "der", "Er\u00b7de", "dr\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie beseelt der Sch\u00f6pfung Fiebern beben,", "tokens": ["Wie", "be\u00b7seelt", "der", "Sch\u00f6p\u00b7fung", "Fie\u00b7bern", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn der Strahl, der sie durchgl\u00fcht,", "tokens": ["Wenn", "der", "Strahl", ",", "der", "sie", "durch\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Allbefruchtend Millionen Leben", "tokens": ["All\u00b7be\u00b7fruch\u00b7tend", "Mil\u00b7lion\u00b7en", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "NN", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Weckt und aus dem Grabe zieht;", "tokens": ["Weckt", "und", "aus", "dem", "Gra\u00b7be", "zieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "So ein Leben, f\u00fchl' ich, strahlet Liebe", "tokens": ["So", "ein", "Le\u00b7ben", ",", "f\u00fchl'", "ich", ",", "strah\u00b7let", "Lie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In dem Feuerauge dir,", "tokens": ["In", "dem", "Feu\u00b7er\u00b7au\u00b7ge", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Regen hundertf\u00e4lt'ger Triebe", "tokens": ["Und", "ein", "Re\u00b7gen", "hun\u00b7dert\u00b7f\u00e4lt'\u00b7ger", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Pocht in Herz und Seele mir.", "tokens": ["Pocht", "in", "Herz", "und", "See\u00b7le", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}