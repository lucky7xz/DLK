{"textgrid.poem.58741": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer des Wassers trinken wird, das ich ihm gebe, den wird nicht d\u00fcrsten in Ewigkeit. Joh. 4, 14", "tokens": ["Wer", "des", "Was\u00b7sers", "trin\u00b7ken", "wird", ",", "das", "ich", "ihm", "ge\u00b7be", ",", "den", "wird", "nicht", "d\u00fcrs\u00b7ten", "in", "E\u00b7wig\u00b7keit", ".", "Joh", ".", "4", ",", "14"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "number", "punct", "number"], "pos": ["PWS", "ART", "NN", "VVINF", "VAFIN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "ART", "VAFIN", "PTKNEG", "VMFIN", "APPR", "NN", "$.", "NE", "$.", "CARD", "$,", "CARD"], "meter": "+-+-+-+-+----+-+--+--+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "O g\u00f6ttlichs Wasser, das ins ew'ge Leben quillet,", "tokens": ["O", "g\u00f6tt\u00b7lichs", "Was\u00b7ser", ",", "das", "ins", "ew'\u00b7ge", "Le\u00b7ben", "quil\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ausl\u00f6scht alle Fleischesbrunst", "tokens": ["Das", "aus\u00b7l\u00f6scht", "al\u00b7le", "Flei\u00b7sches\u00b7brunst"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und uns mit dem Geschmack der reinsten Lust erf\u00fcllet,", "tokens": ["Und", "uns", "mit", "dem", "Ge\u00b7schmack", "der", "reins\u00b7ten", "Lust", "er\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach, alles andre ist nur Dunst!", "tokens": ["Ach", ",", "al\u00b7les", "and\u00b7re", "ist", "nur", "Dunst", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIS", "PIS", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du machest unsre Seel' verzehren", "tokens": ["Du", "ma\u00b7chest", "uns\u00b7re", "Seel'", "ver\u00b7zeh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sterben stets all ihr'm Begehren.", "tokens": ["Und", "ster\u00b7ben", "stets", "all", "ih\u00b7r'm", "Be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Flie\u00df, lieblichs Wasser, flie\u00df, der Seele Grund zu f\u00fcllen,", "tokens": ["Flie\u00df", ",", "lieb\u00b7lichs", "Was\u00b7ser", ",", "flie\u00df", ",", "der", "See\u00b7le", "Grund", "zu", "f\u00fcl\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "VVFIN", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du wirst zur Flamme, s\u00fc\u00df und zart;", "tokens": ["Du", "wirst", "zur", "Flam\u00b7me", ",", "s\u00fc\u00df", "und", "zart", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn du mein Herz entz\u00fcndst, kannst du den Durst auch stillen,", "tokens": ["Wenn", "du", "mein", "Herz", "ent\u00b7z\u00fcndst", ",", "kannst", "du", "den", "Durst", "auch", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "O hohe Wunderred, Verwandlung s\u00fc\u00dfer Art,", "tokens": ["O", "ho\u00b7he", "Wun\u00b7der\u00b7red", ",", "Ver\u00b7wand\u00b7lung", "s\u00fc\u00b7\u00dfer", "Art", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wodurch das Wasser Feu'r, das Feu'r zur K\u00fchlung ward!", "tokens": ["Wo\u00b7durch", "das", "Was\u00b7ser", "Feu'r", ",", "das", "Feu'r", "zur", "K\u00fch\u00b7lung", "ward", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,", "ART", "NN", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Willst du der Sache Grund ergr\u00fcnden,", "tokens": ["Willst", "du", "der", "Sa\u00b7che", "Grund", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dann lerne nur dein Heil in reiner Liebe finden!", "tokens": ["Dann", "ler\u00b7ne", "nur", "dein", "Heil", "in", "rei\u00b7ner", "Lie\u00b7be", "fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}