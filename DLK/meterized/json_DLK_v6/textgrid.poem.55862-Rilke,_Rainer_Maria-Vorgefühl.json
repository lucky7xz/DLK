{"textgrid.poem.55862": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Vorgef\u00fchl", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin wie eine Fahne von Fernen umgeben.", "tokens": ["Ich", "bin", "wie", "ei\u00b7ne", "Fah\u00b7ne", "von", "Fer\u00b7nen", "um\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich ahne die Winde, die kommen, und mu\u00df sie leben,", "tokens": ["Ich", "ah\u00b7ne", "die", "Win\u00b7de", ",", "die", "kom\u00b7men", ",", "und", "mu\u00df", "sie", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "VVINF", "$,", "KON", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "w\u00e4hrend die Dinge unten sich noch nicht r\u00fchren:", "tokens": ["w\u00e4h\u00b7rend", "die", "Din\u00b7ge", "un\u00b7ten", "sich", "noch", "nicht", "r\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PRF", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "die T\u00fcren schlie\u00dfen noch sanft, und in den Kaminen ist Stille;", "tokens": ["die", "T\u00fc\u00b7ren", "schlie\u00b7\u00dfen", "noch", "sanft", ",", "und", "in", "den", "Ka\u00b7mi\u00b7nen", "ist", "Stil\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,", "KON", "APPR", "ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "die Fenster zittern noch nicht, und der Staub ist noch schwer.", "tokens": ["die", "Fens\u00b7ter", "zit\u00b7tern", "noch", "nicht", ",", "und", "der", "Staub", "ist", "noch", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKNEG", "$,", "KON", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Da wei\u00df ich die St\u00fcrme schon und bin erregt wie das Meer.", "tokens": ["Da", "wei\u00df", "ich", "die", "St\u00fcr\u00b7me", "schon", "und", "bin", "er\u00b7regt", "wie", "das", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "KON", "VAFIN", "VVPP", "KOKOM", "ART", "NN", "$."], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und breite mich aus und falle in mich hinein", "tokens": ["Und", "brei\u00b7te", "mich", "aus", "und", "fal\u00b7le", "in", "mich", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "APPR", "PRF", "APZR"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "und werfe mich ab und bin ganz allein", "tokens": ["und", "wer\u00b7fe", "mich", "ab", "und", "bin", "ganz", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "KON", "VAFIN", "ADV", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "in dem gro\u00dfen Sturm.", "tokens": ["in", "dem", "gro\u00b7\u00dfen", "Sturm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}