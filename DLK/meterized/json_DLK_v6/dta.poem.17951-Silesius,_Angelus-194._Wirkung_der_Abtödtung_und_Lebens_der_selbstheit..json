{"dta.poem.17951": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "194.  Wirkung der Abt\u00f6dtung und Lebens der  \n selbstheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Durch t\u00f6dtung deiner selbst wirstu Gotts Lamb darstell\u0113/", "tokens": ["Durch", "t\u00f6d\u00b7tung", "dei\u00b7ner", "selbst", "wirs\u00b7tu", "Gotts", "Lamb", "dar\u00b7stell\u0113", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "ADV", "VAFIN", "NE", "NN", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Leben bleibstu ein todter Hund der H\u00f6llen.", "tokens": ["Mit", "Le\u00b7ben", "bleibs\u00b7tu", "ein", "tod\u00b7ter", "Hund", "der", "H\u00f6l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}}}}