{"textgrid.poem.51199": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: So bist du eine Leiche!", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So bist du eine Leiche!", "tokens": ["So", "bist", "du", "ei\u00b7ne", "Lei\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So ist die alte Eiche", "tokens": ["So", "ist", "die", "al\u00b7te", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch endlich abgedorrt!", "tokens": ["Doch", "end\u00b7lich", "ab\u00b7ge\u00b7dorrt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es ist ein lang St\u00fcck Leben,", "tokens": ["Es", "ist", "ein", "lang", "St\u00fcck", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das wir dem Staube geben,", "tokens": ["Das", "wir", "dem", "Stau\u00b7be", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein ausgeklungen Gotteswort.", "tokens": ["Ein", "aus\u00b7ge\u00b7klun\u00b7gen", "Got\u00b7tes\u00b7wort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da wir vor zwanzig Jahren", "tokens": ["Da", "wir", "vor", "zwan\u00b7zig", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als Kinder um dich waren,", "tokens": ["Als", "Kin\u00b7der", "um", "dich", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Standst du schon silberwei\u00df:", "tokens": ["Standst", "du", "schon", "sil\u00b7ber\u00b7wei\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und noch ein J\u00fcnglingsleben,", "tokens": ["Und", "noch", "ein", "J\u00fcng\u00b7lings\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein zwanzigj\u00e4hriges eben,", "tokens": ["Ein", "zwan\u00b7zig\u00b7j\u00e4h\u00b7ri\u00b7ges", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Trankst du begierig, durst'ger Greis!", "tokens": ["Trankst", "du", "be\u00b7gie\u00b7rig", ",", "dur\u00b7st'\u00b7ger", "Greis", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Des Mittelalters Schwingen", "tokens": ["Des", "Mit\u00b7tel\u00b7al\u00b7ters", "Schwin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit letztem bebendem Klingen", "tokens": ["Mit", "letz\u00b7tem", "be\u00b7ben\u00b7dem", "Klin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Umfachten die Wiege dir:", "tokens": ["Um\u00b7fach\u00b7ten", "die", "Wie\u00b7ge", "dir", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jetzt, voll von Sturmesahnen,", "tokens": ["Jetzt", ",", "voll", "von", "Stur\u00b7me\u00b7sah\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Umrauschen die dunklen Fahnen", "tokens": ["Um\u00b7rau\u00b7schen", "die", "dunk\u00b7len", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Der neuen Welt dein Bahrtuch hier.", "tokens": ["Der", "neu\u00b7en", "Welt", "dein", "Bahr\u00b7tuch", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Darin wir uns vertieften,", "tokens": ["Da\u00b7rin", "wir", "uns", "ver\u00b7tief\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die aberhundert Schriften,", "tokens": ["Die", "a\u00b7ber\u00b7hun\u00b7dert", "Schrif\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was uns erf\u00fcllt die Brust:", "tokens": ["Was", "uns", "er\u00b7f\u00fcllt", "die", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das zog dir all vor\u00fcber,", "tokens": ["Das", "zog", "dir", "all", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "D\u00e4mmernd heran, hin\u00fcber,", "tokens": ["D\u00e4m\u00b7mernd", "he\u00b7ran", ",", "hin\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "ADV", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Du aber hast es nicht gewu\u00dft.", "tokens": ["Du", "a\u00b7ber", "hast", "es", "nicht", "ge\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "In jenen fernen Tagen", "tokens": ["In", "je\u00b7nen", "fer\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u2013 Ich h\u00f6r die Finken schlagen \u2013,", "tokens": ["\u2013", "Ich", "h\u00f6r", "die", "Fin\u00b7ken", "schla\u00b7gen", "\u2013", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "VVINF", "$(", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als durch den gr\u00fcnen Wald", "tokens": ["Als", "durch", "den", "gr\u00fc\u00b7nen", "Wald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Herr Ge\u00dfner las im Brockes:", "tokens": ["Herr", "Ge\u00df\u00b7ner", "las", "im", "Bro\u00b7ckes", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ins Herz des Eichenstockes", "tokens": ["Ins", "Herz", "des", "Ei\u00b7chen\u00b7sto\u00b7ckes"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hat deiner Jugend Axt geschallt.", "tokens": ["Hat", "dei\u00b7ner", "Ju\u00b7gend", "Axt", "ge\u00b7schallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Hast du dem deutschen S\u00e4nger,", "tokens": ["Hast", "du", "dem", "deut\u00b7schen", "S\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem edlen Schlittschuhg\u00e4nger,", "tokens": ["Dem", "ed\u00b7len", "Schlitt\u00b7schuh\u00b7g\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Stahlschuh hier gereicht?", "tokens": ["Den", "Stahl\u00b7schuh", "hier", "ge\u00b7reicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du hast vor f\u00fcnfzig Jahren", "tokens": ["Du", "hast", "vor", "f\u00fcnf\u00b7zig", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den See hinauf gefahren", "tokens": ["Den", "See", "hin\u00b7auf", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den f\u00fcnfzigj\u00e4hrigen Goethe vielleicht.", "tokens": ["Den", "f\u00fcnf\u00b7zig\u00b7j\u00e4h\u00b7ri\u00b7gen", "Goe\u00b7the", "viel\u00b7leicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Vor\u00fcber deiner Leiche", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "dei\u00b7ner", "Lei\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Flieht heut der zornesbleiche", "tokens": ["Flieht", "heut", "der", "zor\u00b7nes\u00b7blei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Poet den See entlang;", "tokens": ["Po\u00b7et", "den", "See", "ent\u00b7lang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPO", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Verschwunden sind die Spuren,", "tokens": ["Ver\u00b7schwun\u00b7den", "sind", "die", "Spu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wo heitre Dichter fuhren,", "tokens": ["Wo", "heit\u00b7re", "Dich\u00b7ter", "fuh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und anders t\u00f6nt des Fl\u00fcchtlings Sang!", "tokens": ["Und", "an\u00b7ders", "t\u00f6nt", "des", "Fl\u00fccht\u00b7lings", "Sang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die Scherben stolzer Kronen,", "tokens": ["Die", "Scher\u00b7ben", "stol\u00b7zer", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zwei Revolutionen,", "tokens": ["Zwei", "Re\u00b7vo\u00b7lu\u00b7ti\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die haben dich umklirrt;", "tokens": ["Die", "ha\u00b7ben", "dich", "um\u00b7klirrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Erdbeben und Kometen,", "tokens": ["Erd\u00b7be\u00b7ben", "und", "Ko\u00b7me\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Sturmglocken und Schlachtdrommeten", "tokens": ["Sturm\u00b7glo\u00b7cken", "und", "Schlacht\u00b7drom\u00b7me\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Sind deiner Stirn vorbeigeschwirrt.", "tokens": ["Sind", "dei\u00b7ner", "Stirn", "vor\u00b7bei\u00b7ge\u00b7schwirrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der unsre Welt gewendet", "tokens": ["Der", "uns\u00b7re", "Welt", "ge\u00b7wen\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie seine Hand, geendet", "tokens": ["Wie", "sei\u00b7ne", "Hand", ",", "ge\u00b7en\u00b7det"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Meere still und fern,", "tokens": ["Im", "Mee\u00b7re", "still", "und", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit seinem ehrnen Tritte", "tokens": ["Mit", "sei\u00b7nem", "ehr\u00b7nen", "Trit\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Fiel just er in die Mitte", "tokens": ["Fiel", "just", "er", "in", "die", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Lebens dir, ein irrer Stern.", "tokens": ["Des", "Le\u00b7bens", "dir", ",", "ein", "ir\u00b7rer", "Stern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Du sahst auf deinem Felde", "tokens": ["Du", "sahst", "auf", "dei\u00b7nem", "Fel\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erstaunt die fremden Zelte,", "tokens": ["Er\u00b7staunt", "die", "frem\u00b7den", "Zel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Flucht durch Saatengr\u00fcn", "tokens": ["Die", "Flucht", "durch", "Saa\u00b7ten\u00b7gr\u00fcn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und, als sie abgezogen,", "tokens": ["Und", ",", "als", "sie", "ab\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zum alten Sternenbogen", "tokens": ["Zum", "al\u00b7ten", "Ster\u00b7nen\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der V\u00e4ter Haus in Flammen spr\u00fchn.", "tokens": ["Der", "V\u00e4\u00b7ter", "Haus", "in", "Flam\u00b7men", "spr\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Doch alles ist in tr\u00fcben", "tokens": ["Doch", "al\u00b7les", "ist", "in", "tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gebilden dir fremd geblieben,", "tokens": ["Ge\u00b7bil\u00b7den", "dir", "fremd", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein R\u00e4tsel dir und Traum;", "tokens": ["Ein", "R\u00e4t\u00b7sel", "dir", "und", "Traum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Auch die vor\u00fcberjagten,", "