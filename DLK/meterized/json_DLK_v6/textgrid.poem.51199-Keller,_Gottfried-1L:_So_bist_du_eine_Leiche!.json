de", "tokens": ["Vom", "Wel\u00b7tend", "die", "vier", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch deiner Heimat Gr\u00fcnde", "tokens": ["Durch", "dei\u00b7ner", "Hei\u00b7mat", "Gr\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sahst wallen du und wehn;", "tokens": ["Sahst", "wal\u00b7len", "du", "und", "wehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "PPER", "KON", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch jener nahen Firnen,", "tokens": ["Doch", "je\u00b7ner", "na\u00b7hen", "Fir\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die ragen zu den Gestirnen,", "tokens": ["Die", "ra\u00b7gen", "zu", "den", "Ge\u00b7stir\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Hast selber den Fu\u00df du nie gesehn.", "tokens": ["Hast", "sel\u00b7ber", "den", "Fu\u00df", "du", "nie", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.16": {"line.1": {"text": "Und dennoch ist's das echte,", "tokens": ["Und", "den\u00b7noch", "ist's", "das", "ech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das bleibende Volk, das rechte,", "tokens": ["Das", "blei\u00b7ben\u00b7de", "Volk", ",", "das", "rech\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das auf der Scholl erbla\u00dft,", "tokens": ["Das", "auf", "der", "Scholl", "er\u00b7bla\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Auf der es ward geboren!", "tokens": ["Auf", "der", "es", "ward", "ge\u00b7bo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Schifflein geht verloren,", "tokens": ["Das", "Schif\u00b7flein", "geht", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Anker diesen Grund nicht fa\u00dft.", "tokens": ["Des", "An\u00b7ker", "die\u00b7sen", "Grund", "nicht", "fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Propheten, lernt euch neigen!", "tokens": ["Pro\u00b7phe\u00b7ten", ",", "lernt", "euch", "nei\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht auf zu euch soll steigen", "tokens": ["Nicht", "auf", "zu", "euch", "soll", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR"