{"textgrid.poem.60251": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Das Mondlicht", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein gedenkend irr ich einsam", "tokens": ["Dein", "ge\u00b7den\u00b7kend", "irr", "ich", "ein\u00b7sam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "VVPP", "NN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diesen Strom entlang;", "tokens": ["Die\u00b7sen", "Strom", "ent\u00b7lang", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "K\u00f6nnten lauschen wir gemeinsam", "tokens": ["K\u00f6nn\u00b7ten", "lau\u00b7schen", "wir", "ge\u00b7mein\u00b7sam"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seinem Wellenklang!", "tokens": ["Sei\u00b7nem", "Wel\u00b7len\u00b7klang", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "K\u00f6nnten wir zusammen schauen", "tokens": ["K\u00f6nn\u00b7ten", "wir", "zu\u00b7sam\u00b7men", "schau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Mond empor,", "tokens": ["In", "den", "Mond", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der da dr\u00fcben aus den Auen", "tokens": ["Der", "da", "dr\u00fc\u00b7ben", "aus", "den", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leise taucht hervor.", "tokens": ["Lei\u00b7se", "taucht", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Freundlich streut er meinem Blicke", "tokens": ["Freund\u00b7lich", "streut", "er", "mei\u00b7nem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem Silberschein", "tokens": ["Aus", "dem", "Sil\u00b7ber\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Stromhin\u00fcber eine Br\u00fccke", "tokens": ["Strom\u00b7hin\u00b7\u00fc\u00b7ber", "ei\u00b7ne", "Br\u00fc\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis zum stillen Hain. \u2013", "tokens": ["Bis", "zum", "stil\u00b7len", "Hain", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Wo des Stromes frohe Wellen", "tokens": ["Wo", "des", "Stro\u00b7mes", "fro\u00b7he", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch den Schimmer ziehn,", "tokens": ["Durch", "den", "Schim\u00b7mer", "ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Seh ich, wie hinab die schnellen", "tokens": ["Seh", "ich", ",", "wie", "hin\u00b7ab", "die", "schnel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unaufhaltsam fliehn.", "tokens": ["Un\u00b7auf\u00b7halt\u00b7sam", "fliehn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Aber wo im schimmerlosen", "tokens": ["A\u00b7ber", "wo", "im", "schim\u00b7mer\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPRART", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dunkel geht die Flut,", "tokens": ["Dun\u00b7kel", "geht", "die", "Flut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ist sie nur ein dumpfes Tosen,", "tokens": ["Ist", "sie", "nur", "ein", "dum\u00b7pfes", "To\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das dem Auge ruht.", "tokens": ["Das", "dem", "Au\u00b7ge", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Da\u00df doch mein Geschick mir br\u00e4chte", "tokens": ["Da\u00df", "doch", "mein", "Ge\u00b7schick", "mir", "br\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen Blick von dir!", "tokens": ["Ei\u00b7nen", "Blick", "von", "dir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "S\u00fc\u00dfes Mondlicht meiner N\u00e4chte,", "tokens": ["S\u00fc\u00b7\u00dfes", "Mond\u00b7licht", "mei\u00b7ner", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00e4dchen, bist du mir!", "tokens": ["M\u00e4d\u00b7chen", ",", "bist", "du", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Wenn nach dir ich oft vergebens", "tokens": ["Wenn", "nach", "dir", "ich", "oft", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "PPER", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die Nacht gesehn,", "tokens": ["In", "die", "Nacht", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Scheint der dunkle Strom des Lebens", "tokens": ["Scheint", "der", "dunk\u00b7le", "Strom", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trauernd stillzustehn;", "tokens": ["Trau\u00b7ernd", "still\u00b7zu\u00b7stehn", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Wenn du \u00fcber seinen Wogen", "tokens": ["Wenn", "du", "\u00fc\u00b7ber", "sei\u00b7nen", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Strahlest zauberhell,", "tokens": ["Strah\u00b7lest", "zau\u00b7ber\u00b7hell", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Seh ich sie dahingezogen,", "tokens": ["Seh", "ich", "sie", "da\u00b7hin\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach! nur allzuschnell!", "tokens": ["Ach", "!", "nur", "all\u00b7zu\u00b7schnell", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}