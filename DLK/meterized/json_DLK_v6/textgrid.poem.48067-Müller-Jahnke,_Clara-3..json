{"textgrid.poem.48067": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm du, wir wollen traulich schreiten", "tokens": ["Komm", "du", ",", "wir", "wol\u00b7len", "trau\u00b7lich", "schrei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VMFIN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "wie selige Kinder, Hand in Hand,", "tokens": ["wie", "se\u00b7li\u00b7ge", "Kin\u00b7der", ",", "Hand", "in", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "durch bl\u00fctenschwangre Fr\u00fchlingsweiten,", "tokens": ["durch", "bl\u00fc\u00b7ten\u00b7schwang\u00b7re", "Fr\u00fch\u00b7lings\u00b7wei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "durch warmbesonnten Meeressand.", "tokens": ["durch", "warm\u00b7be\u00b7sonn\u00b7ten", "Mee\u00b7res\u00b7sand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Luft erklingt, die Wipfel lauschen,", "tokens": ["Die", "Luft", "er\u00b7klingt", ",", "die", "Wip\u00b7fel", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die Sonne gr\u00fc\u00dft uns trunknen Blicks, \u2013", "tokens": ["die", "Son\u00b7ne", "gr\u00fc\u00dft", "uns", "trunk\u00b7nen", "Blicks", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und \u00fcber unsere Seelen rauschen", "tokens": ["und", "\u00fc\u00b7ber", "un\u00b7se\u00b7re", "See\u00b7len", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "die Wogen des Ostaragl\u00fccks.", "tokens": ["die", "Wo\u00b7gen", "des", "O\u00b7sta\u00b7ra\u00b7gl\u00fccks", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das ist ein Wachsen und ein Werden,", "tokens": ["Das", "ist", "ein", "Wach\u00b7sen", "und", "ein", "Wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "wir wandeln wie voll s\u00fc\u00dfen Weins:", "tokens": ["wir", "wan\u00b7deln", "wie", "voll", "s\u00fc\u00b7\u00dfen", "Weins", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eins sind wir mit der Kraft der Erden,", "tokens": ["Eins", "sind", "wir", "mit", "der", "Kraft", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und mit dem Himmel sind wir eins.", "tokens": ["und", "mit", "dem", "Him\u00b7mel", "sind", "wir", "eins", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie rings die Ferne sich entschleiert", "tokens": ["Wie", "rings", "die", "Fer\u00b7ne", "sich", "ent\u00b7schlei\u00b7ert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "PRF", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "in Glut und Glanz und Windeswehn,", "tokens": ["in", "Glut", "und", "Glanz", "und", "Win\u00b7des\u00b7wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "so \u2013 Aug in Auge! \u2013 leuchtend feiert", "tokens": ["so", "\u2013", "Aug", "in", "Au\u00b7ge", "!", "\u2013", "leuch\u00b7tend", "fei\u00b7ert"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ADV", "$(", "NN", "APPR", "NN", "$.", "$(", "VVPP", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die Gottheit in uns Auferstehn!", "tokens": ["die", "Got\u00b7theit", "in", "uns", "Auf\u00b7er\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}