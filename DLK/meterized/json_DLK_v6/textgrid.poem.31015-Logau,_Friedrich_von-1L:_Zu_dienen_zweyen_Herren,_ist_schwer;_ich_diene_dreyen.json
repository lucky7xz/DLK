{"textgrid.poem.31015": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zu dienen zweyen Herren, ist schwer; ich diene dreyen", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu dienen zweyen Herren, ist schwer; ich diene dreyen", "tokens": ["Zu", "die\u00b7nen", "zwe\u00b7yen", "Her\u00b7ren", ",", "ist", "schwer", ";", "ich", "die\u00b7ne", "drey\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "VVFIN", "NN", "$,", "VAFIN", "ADJD", "$.", "PPER", "VVFIN", "CARD"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und darff mich doch bey keinem der Redligkeit verzeihen.", "tokens": ["Und", "darff", "mich", "doch", "bey", "kei\u00b7nem", "der", "Red\u00b7lig\u00b7keit", "ver\u00b7zei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPR", "PIS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Gott dien ich mit dem Hertzen nach meinem besten k\u00fcnnen,", "tokens": ["Gott", "di\u00b7en", "ich", "mit", "dem", "Hert\u00b7zen", "nach", "mei\u00b7nem", "bes\u00b7ten", "k\u00fcn\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "PPOSAT", "ADJA", "ADJA", "$,"], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}, "line.4": {"text": "Dem F\u00fcrsten mit dem Kopffe nach meinen besten Sinnen,", "tokens": ["Dem", "F\u00fcrs\u00b7ten", "mit", "dem", "Kopf\u00b7fe", "nach", "mei\u00b7nen", "bes\u00b7ten", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Dem Nechsten mit den H\u00e4nden durch H\u00fclff au\u00df gutem Willen;", "tokens": ["Dem", "Nechs\u00b7ten", "mit", "den", "H\u00e4n\u00b7den", "durch", "H\u00fclff", "au\u00df", "gu\u00b7tem", "Wil\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Kan hoffentlich bey allen so meine Pflicht erv\u00f6llen.", "tokens": ["Kan", "hof\u00b7fent\u00b7lich", "bey", "al\u00b7len", "so", "mei\u00b7ne", "Pflicht", "er\u00b7v\u00f6l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PIS", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}