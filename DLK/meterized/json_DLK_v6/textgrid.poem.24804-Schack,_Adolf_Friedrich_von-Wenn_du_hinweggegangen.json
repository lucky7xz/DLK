{"textgrid.poem.24804": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Wenn du hinweggegangen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du hinweggegangen,", "tokens": ["Wenn", "du", "hin\u00b7weg\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Glaub' ich lange dich noch zu sehn;", "tokens": ["Glaub'", "ich", "lan\u00b7ge", "dich", "noch", "zu", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Um die Schl\u00e4fe und um die Wangen", "tokens": ["Um", "die", "Schl\u00e4\u00b7fe", "und", "um", "die", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "KON", "APPR", "ART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Deinen Atem mir f\u00fchl' ich wehn.", "tokens": ["Dei\u00b7nen", "A\u00b7tem", "mir", "f\u00fchl'", "ich", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Wenn von deinen Reden", "tokens": ["Wenn", "von", "dei\u00b7nen", "Re\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "L\u00e4ngst der Ton dem Ohre verklang,", "tokens": ["L\u00e4ngst", "der", "Ton", "dem", "Oh\u00b7re", "ver\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "H\u00f6rt die entz\u00fcckte Seele jeden", "tokens": ["H\u00f6rt", "die", "ent\u00b7z\u00fcck\u00b7te", "See\u00b7le", "je\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PIAT"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Laut, den du gesprochen, noch lang.", "tokens": ["Laut", ",", "den", "du", "ge\u00b7spro\u00b7chen", ",", "noch", "lang", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "PRELS", "PPER", "VVPP", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "In der Stille der N\u00e4chte,", "tokens": ["In", "der", "Stil\u00b7le", "der", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wenn voll Bangen das Herz mir schl\u00e4gt,", "tokens": ["Wenn", "voll", "Ban\u00b7gen", "das", "Herz", "mir", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "F\u00fchl' ich, wie leise sich deine Rechte", "tokens": ["F\u00fchl'", "ich", ",", "wie", "lei\u00b7se", "sich", "dei\u00b7ne", "Rech\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "ADJD", "PRF", "PPOSAT", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf die Stirne, die Brust mir legt.", "tokens": ["Auf", "die", "Stir\u00b7ne", ",", "die", "Brust", "mir", "legt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Arme, die weich mich umranken,", "tokens": ["Ar\u00b7me", ",", "die", "weich", "mich", "um\u00b7ran\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "PPER", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Wiegen mich ein; ich atme kaum;", "tokens": ["Wie\u00b7gen", "mich", "ein", ";", "ich", "at\u00b7me", "kaum", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Deine Worte, deine Gedanken", "tokens": ["Dei\u00b7ne", "Wor\u00b7te", ",", "dei\u00b7ne", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Klingen und duften um mich im Traum.", "tokens": ["Klin\u00b7gen", "und", "duf\u00b7ten", "um", "mich", "im", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPR", "PRF", "APPRART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}