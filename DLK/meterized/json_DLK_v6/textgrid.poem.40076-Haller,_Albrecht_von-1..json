{"textgrid.poem.40076": {"metadata": {"author": {"name": "Haller, Albrecht von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1748, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als unbesiegt an Muth der letzte R\u00f6mer starb,", "tokens": ["Als", "un\u00b7be\u00b7siegt", "an", "Muth", "der", "letz\u00b7te", "R\u00f6\u00b7mer", "starb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "War Rom von Ruhm noch stolz, den ihm sein Blut erwarb;", "tokens": ["War", "Rom", "von", "Ruhm", "noch", "stolz", ",", "den", "ihm", "sein", "Blut", "er\u00b7warb", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "APPR", "NN", "ADV", "ADJD", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O seliger als Rom, du freies Albion!", "tokens": ["O", "se\u00b7li\u00b7ger", "als", "Rom", ",", "du", "frei\u00b7es", "Al\u00b7bi\u00b7on", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KOKOM", "NE", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Wie damals Cato sprach, so denkt itzt C\u00e4sars Sohn.", "tokens": ["Wie", "da\u00b7mals", "Ca\u00b7to", "sprach", ",", "so", "denkt", "itzt", "C\u00e4\u00b7sars", "Sohn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NE", "VVFIN", "$,", "ADV", "VVFIN", "ADV", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "a