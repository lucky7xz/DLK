{"dta.poem.21451": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Kind aus dem Volke.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Kind aus dem Volke so schlicht und rein,", "tokens": ["Kind", "aus", "dem", "Vol\u00b7ke", "so", "schlicht", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "H\u00fcte dich, h\u00fcte dich Bl\u00fcmelein!", "tokens": ["H\u00fc\u00b7te", "dich", ",", "h\u00fc\u00b7te", "dich", "Bl\u00fc\u00b7me\u00b7lein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "VVFIN", "PPER", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Bist so lieblich und zart von Gestalt,", "tokens": ["Bist", "so", "lieb\u00b7lich", "und", "zart", "von", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Uebst so s\u00fc\u00dfe Zaubergewalt \u2026", "tokens": ["Uebst", "so", "s\u00fc\u00b7\u00dfe", "Zau\u00b7ber\u00b7ge\u00b7walt", "\u2026"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Bald ist der Liebe Knospe erwacht,", "tokens": ["Bald", "ist", "der", "Lie\u00b7be", "Knos\u00b7pe", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Liebe kommt tr\u00e4umend und \u00fcber Nacht.", "tokens": ["Lie\u00b7be", "kommt", "tr\u00e4u\u00b7mend", "und", "\u00fc\u00b7ber", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "KON", "APPR", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Hei\u00dfer zum Herzen str\u00f6mt dir das Blut,", "tokens": ["Hei\u00b7\u00dfer", "zum", "Her\u00b7zen", "str\u00f6mt", "dir", "das", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Deine Wang\u2019 ergl\u00fch\u2019t in Rosenglut.", "tokens": ["Dei\u00b7ne", "Wang'", "er\u00b7gl\u00fch't", "in", "Ro\u00b7seng\u00b7lut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Dein Mund dem Kusse entgegenschwillt,", "tokens": ["Dein", "Mund", "dem", "Kus\u00b7se", "ent\u00b7ge\u00b7gen\u00b7schwillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Nur des Mannes Arm deine Sehnsucht stillt.", "tokens": ["Nur", "des", "Man\u00b7nes", "Arm", "dei\u00b7ne", "Sehn\u00b7sucht", "stillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Wild pre\u00dft du ihn an die wogende Brust,", "tokens": ["Wild", "pre\u00dft", "du", "ihn", "an", "die", "wo\u00b7gen\u00b7de", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "Du giebst dich hin im Taumel der Lust.", "tokens": ["Du", "giebst", "dich", "hin", "im", "Tau\u00b7mel", "der", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "F\u00fcr ewig scheidet von dir das Gl\u00fcck,", "tokens": ["F\u00fcr", "e\u00b7wig", "schei\u00b7det", "von", "dir", "das", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Nie kehrt deine Munterkeit zur\u00fcck.", "tokens": ["Nie", "kehrt", "dei\u00b7ne", "Mun\u00b7ter\u00b7keit", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "In Thr\u00e4nen stirbt deiner Seele Mai,", "tokens": ["In", "Thr\u00e4\u00b7nen", "stirbt", "dei\u00b7ner", "See\u00b7le", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "In d\u00fcstrer Verzweiflung dein Todesschrei.", "tokens": ["In", "d\u00fcst\u00b7rer", "Ver\u00b7zwei\u00b7flung", "dein", "To\u00b7des\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}