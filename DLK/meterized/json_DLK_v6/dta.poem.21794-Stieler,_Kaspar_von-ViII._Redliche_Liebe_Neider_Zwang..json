{"dta.poem.21794": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "ViII.  \n  Redliche Liebe/ Neider Zwang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Macht euch lustig Neider-Herzen", "tokens": ["Macht", "euch", "lus\u00b7tig", "Nei\u00b7der\u00b7Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "blaset/ wie ihr tuht", "tokens": ["bla\u00b7set", "/", "wie", "ihr", "tuht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$(", "PWAV", "PPER", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Blizz/ Schwefel blaue Gluht", "tokens": ["Blizz", "/", "Schwe\u00b7fel", "blau\u00b7e", "Gluht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "unser\u2019 heisse Liebes Kerzen", "tokens": ["un\u00b7ser'", "heis\u00b7se", "Lie\u00b7bes", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Amors Straal und Licht", "tokens": ["A\u00b7mors", "Straal", "und", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "verdunkelt ihr doch nicht.", "tokens": ["ver\u00b7dun\u00b7kelt", "ihr", "doch", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie da\u00df heitre Sonnen 