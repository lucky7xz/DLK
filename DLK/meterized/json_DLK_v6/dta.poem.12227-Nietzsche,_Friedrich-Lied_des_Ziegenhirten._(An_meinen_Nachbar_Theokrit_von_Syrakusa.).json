{"dta.poem.12227": {"metadata": {"author": {"name": "Nietzsche, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Lied des Ziegenhirten.  \n (An meinen Nachbar Theokrit von Syrakusa.)", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-30172-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "             Da lieg ich, krank im Ged\u00e4rm \u2014", "tokens": ["Da", "lieg", "ich", ",", "krank", "im", "Ge\u00b7d\u00e4rm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "             Mich fressen die Wanzen.", "tokens": ["Mich", "fres\u00b7sen", "die", "Wan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "             Und dr\u00fcben noch Licht und L\u00e4rm:", "tokens": ["Und", "dr\u00fc\u00b7ben", "noch", "Licht", "und", "L\u00e4rm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "             Ich h\u00f6r\u2019s, sie tanzen.", "tokens": ["Ich", "h\u00f6r's", ",", "sie", "tan\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Sie wollte um diese Stund\u2019", "tokens": ["Sie", "woll\u00b7te", "um", "die\u00b7se", "Stund'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PDAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "         Zu mir sich schleichen:", "tokens": ["Zu", "mir", "sich", "schlei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "             Ich warte wie ein Hund \u2014", "tokens": ["Ich", "war\u00b7te", "wie", "ein", "Hund"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "             Es kommt kein Zeichen!", "tokens": ["Es", "kommt", "kein", "Zei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Das Kreuz, als sie\u2019s versprach!", "tokens": ["Das", "Kreuz", ",", "als", "sie's", "ver\u00b7sprach", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "         Wie konnte sie l\u00fcgen?", "tokens": ["Wie", "konn\u00b7te", "sie", "l\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "         Oder l\u00e4uft sie Jedem nach,", "tokens": ["O\u00b7der", "l\u00e4uft", "sie", "Je\u00b7dem", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "APPR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "             Wie meine Ziegen?", "tokens": ["Wie", "mei\u00b7ne", "Zie\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Woher ihr seidner Rock? \u2014", "tokens": ["Wo\u00b7her", "ihr", "seid\u00b7ner", "Rock", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "             Ah, meine Stolze?", "tokens": ["Ah", ",", "mei\u00b7ne", "Stol\u00b7ze", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "         Es wohnt noch mancher Bock", "tokens": ["Es", "wohnt", "noch", "man\u00b7cher", "Bock"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "         An diesem Holze?", "tokens": ["An", "die\u00b7sem", "Hol\u00b7ze", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Wie kraus und giftig macht", "tokens": ["Wie", "kraus", "und", "gif\u00b7tig", "macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "             Verliebtes Warten!", "tokens": ["Ver\u00b7lieb\u00b7tes", "War\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "             So w\u00e4chst bei schw\u00fcler Nacht", "tokens": ["So", "w\u00e4chst", "bei", "schw\u00fc\u00b7ler", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "         Giftpilz im Garten.", "tokens": ["Gift\u00b7pilz", "im", "Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "             Die Liebe zehrt an mir", "tokens": ["Die", "Lie\u00b7be", "zehrt", "an", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "             Gleich sieben Uebeln \u2014", "tokens": ["Gleich", "sie\u00b7ben", "Ue\u00b7beln"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "             Nichts mag ich essen schier,", "tokens": ["Nichts", "mag", "ich", "es\u00b7sen", "schier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "             Lebt wohl, ihr Zwiebeln!", "tokens": ["Lebt", "wohl", ",", "ihr", "Zwie\u00b7beln", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "         Der Mond ging schon in\u2019s Meer,", "tokens": ["Der", "Mond", "ging", "schon", "in's", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "         M\u00fcd sind alle Sterne,", "tokens": ["M\u00fcd", "sind", "al\u00b7le", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "             Grau kommt der Tag daher \u2014", "tokens": ["Grau", "kommt", "der", "Tag", "da\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PAV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "                 Ich st\u00fcrbe gerne.", "tokens": ["Ich", "st\u00fcr\u00b7be", "ger\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}