{"textgrid.poem.62437": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freiheit, die ich meine,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freiheit, die ich meine,", "tokens": ["Frei\u00b7heit", ",", "die", "ich", "mei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die mein Herz erf\u00fcllt,", "tokens": ["Die", "mein", "Herz", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Komm' mit deinem Scheine,", "tokens": ["Komm'", "mit", "dei\u00b7nem", "Schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "S\u00fc\u00dfes Engelbild.", "tokens": ["S\u00fc\u00b7\u00dfes", "En\u00b7gel\u00b7bild", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Magst du nie dich zeigen", "tokens": ["Magst", "du", "nie", "dich", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Der bedr\u00e4ngten Welt?", "tokens": ["Der", "be\u00b7dr\u00e4ng\u00b7ten", "Welt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "F\u00fchrest deinen Reigen", "tokens": ["F\u00fch\u00b7rest", "dei\u00b7nen", "Rei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Nur am Sternenzelt?", "tokens": ["Nur", "am", "Ster\u00b7nen\u00b7zelt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Auch bei gr\u00fcnen B\u00e4umen", "tokens": ["Auch", "bei", "gr\u00fc\u00b7nen", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "In dem lust'gen Wald,", "tokens": ["In", "dem", "lust'\u00b7gen", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "Unter Bl\u00fctentr\u00e4umen", "tokens": ["Un\u00b7ter", "Bl\u00fc\u00b7ten\u00b7tr\u00e4u\u00b7men"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ist dein Aufenthalt.", "tokens": ["Ist", "dein", "Auf\u00b7ent\u00b7halt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Ach! das ist ein Leben,", "tokens": ["Ach", "!", "das", "ist", "ein", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wenn es weht und klingt,", "tokens": ["Wenn", "es", "weht", "und", "klingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn dein stilles Weben", "tokens": ["Wenn", "dein", "stil\u00b7les", "We\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wonnig uns durchdringt.", "tokens": ["Won\u00b7nig", "uns", "durch\u00b7dringt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Wenn die Bl\u00e4tter rauschen", "tokens": ["Wenn", "die", "Bl\u00e4t\u00b7ter", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "S\u00fc\u00dfen Freundesgru\u00df,", "tokens": ["S\u00fc\u00b7\u00dfen", "Freun\u00b7des\u00b7gru\u00df", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn wir Blicke tauschen,", "tokens": ["Wenn", "wir", "Bli\u00b7cke", "tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Liebeswort und Ku\u00df.", "tokens": ["Lie\u00b7bes\u00b7wort", "und", "Ku\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Aber immer weiter", "tokens": ["A\u00b7ber", "im\u00b7mer", "wei\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Nimmt das Herz den Lauf,", "tokens": ["Nimmt", "das", "Herz", "den", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Auf der Himmelsleiter", "tokens": ["Auf", "der", "Him\u00b7mels\u00b7lei\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Steigt die Sehnsucht auf;", "tokens": ["Steigt", "die", "Sehn\u00b7sucht", "auf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Aus den stillen Kreisen", "tokens": ["Aus", "den", "stil\u00b7len", "Krei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Kommt mein Hirtenkind,", "tokens": ["Kommt", "mein", "Hir\u00b7ten\u00b7kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Will der Welt beweisen,", "tokens": ["Will", "der", "Welt", "be\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Was es denkt und minnt.", "tokens": ["Was", "es", "denkt", "und", "minnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Bl\u00fcht ihm doch ein Garten,", "tokens": ["Bl\u00fcht", "ihm", "doch", "ein", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Reist ihm doch ein Feld", "tokens": ["Reist", "ihm", "doch", "ein", "Feld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Auch in jener harten", "tokens": ["Auch", "in", "je\u00b7ner", "har\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Steinerbauten Welt.", "tokens": ["Stei\u00b7ner\u00b7bau\u00b7ten", "Welt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Wo sich Gottes Flamme", "tokens": ["Wo", "sich", "Got\u00b7tes", "Flam\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PRF", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "In ein Herz gesenkt,", "tokens": ["In", "ein", "Herz", "ge\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Das am alten Stamme", "tokens": ["Das", "am", "al\u00b7ten", "Stam\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPRART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Treu und liebend h\u00e4ngt;", "tokens": ["Treu", "und", "lie\u00b7bend", "h\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Wo sich M\u00e4nner finden,", "tokens": ["Wo", "sich", "M\u00e4n\u00b7ner", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die f\u00fcr Ehr' und Recht", "tokens": ["Die", "f\u00fcr", "Ehr'", "und", "Recht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Muthig sich verbinden,", "tokens": ["Mut\u00b7hig", "sich", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Weilt ein frei Geschlecht.", "tokens": ["Weilt", "ein", "frei", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Hinter dunkeln W\u00e4llen,", "tokens": ["Hin\u00b7ter", "dun\u00b7keln", "W\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Hinter ehrnem Thor", "tokens": ["Hin\u00b7ter", "ehr\u00b7nem", "Thor"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Kann das Herz noch schwellen", "tokens": ["Kann", "das", "Herz", "noch", "schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Zu dem Licht empor;", "tokens": ["Zu", "dem", "Licht", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.12": {"line.1": {"text": "F\u00fcr die Kirchenhallen,", "tokens": ["F\u00fcr", "die", "Kir\u00b7chen\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "F\u00fcr der V\u00e4ter Gruft,", "tokens": ["F\u00fcr", "der", "V\u00e4\u00b7ter", "Gruft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "F\u00fcr die Liebsten fallen,", "tokens": ["F\u00fcr", "die", "Liebs\u00b7ten", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wenn die Freiheit ruft.", "tokens": ["Wenn", "die", "Frei\u00b7heit", "ruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "Das ist rechtes Gl\u00fchen", "tokens": ["Das", "ist", "rech\u00b7tes", "Gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Frisch und rosenroth:", "tokens": ["Frisch", "und", "ro\u00b7sen\u00b7roth", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Heldenwangen bl\u00fchen", "tokens": ["Hel\u00b7den\u00b7wan\u00b7gen", "bl\u00fc\u00b7hen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sch\u00f6ner auf im Tod.", "tokens": ["Sch\u00f6\u00b7ner", "auf", "im", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "Wollest auf uns lenken", "tokens": ["Wol\u00b7lest", "auf", "uns", "len\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gottes Lieb und Lust,", "tokens": ["Got\u00b7tes", "Lieb", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wollest gern dich senken", "tokens": ["Wol\u00b7lest", "gern", "dich", "sen\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "In die deutsche Brust.", "tokens": ["In", "die", "deut\u00b7sche", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.15": {"line.1": {"text": "Freiheit, holdes Wesen,", "tokens": ["Frei\u00b7heit", ",", "hol\u00b7des", "We\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gl\u00e4ubig, k\u00fchn und zart,", "tokens": ["Gl\u00e4u\u00b7big", ",", "k\u00fchn", "und", "zart", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Hast ja lang erlesen", "tokens": ["Hast", "ja", "lang", "er\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dir die deutsche Art.", "tokens": ["Dir", "die", "deut\u00b7sche", "Art", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}