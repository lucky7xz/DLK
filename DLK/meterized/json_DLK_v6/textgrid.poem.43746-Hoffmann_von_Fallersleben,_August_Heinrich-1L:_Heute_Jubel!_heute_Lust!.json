{"textgrid.poem.43746": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heute Jubel! heute Lust!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heute Jubel! heute Lust!", "tokens": ["Heu\u00b7te", "Ju\u00b7bel", "!", "heu\u00b7te", "Lust", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$.", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Juchheidi! juchheida!", "tokens": ["Juch\u00b7hei\u00b7di", "!", "juch\u00b7hei\u00b7da", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sang und Klang aus voller Brust!", "tokens": ["Sang", "und", "Klang", "aus", "vol\u00b7ler", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Juchheidi! heida!", "tokens": ["Juch\u00b7hei\u00b7di", "!", "hei\u00b7da", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Freud' und Leben dort und hier!", "tokens": ["Freud'", "und", "Le\u00b7ben", "dort", "und", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Heute, heute herbsten wir!", "tokens": ["Heu\u00b7te", ",", "heu\u00b7te", "herbs\u00b7ten", "wir", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Juchheidi, heidi, heida!", "tokens": ["Juch\u00b7hei\u00b7di", ",", "hei\u00b7di", ",", "hei\u00b7da", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "ITJ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Unsre Reben rings umher", "tokens": ["Uns\u00b7re", "Re\u00b7ben", "rings", "um\u00b7her"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Juchheidi! juchheida!", "tokens": ["Juch\u00b7hei\u00b7di", "!", "juch\u00b7hei\u00b7da", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sind von reifen Trauben schwer.", "tokens": ["Sind", "von", "rei\u00b7fen", "Trau\u00b7ben", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Juchheidi! heida!", "tokens": ["Juch\u00b7hei\u00b7di", "!", "hei\u00b7da", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Heuer giebt es guten Wein,", "tokens": ["Heu\u00b7er", "giebt", "es", "gu\u00b7ten", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Heuer kann man fr\u00f6hlich sein.", "tokens": ["Heu\u00b7er", "kann", "man", "fr\u00f6h\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Juchheidi, heidi, heida!", "tokens": ["Juch\u00b7hei\u00b7di", ",", "hei\u00b7di", ",", "hei\u00b7da", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "ITJ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Jung und Alt, nun frisch heran!", "tokens": ["Jung", "und", "Alt", ",", "nun", "frisch", "he\u00b7ran", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$,", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Juchheidi! juchheida!", "tokens": ["Juch\u00b7hei\u00b7di", "!", "juch\u00b7hei\u00b7da", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Heute f\u00e4ngt das Lesen an.", "tokens": ["Heu\u00b7te", "f\u00e4ngt", "das", "Le\u00b7sen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Juchheidi! heida!", "tokens": ["Juch\u00b7hei\u00b7di", "!", "hei\u00b7da", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "In den Bergen hier und dort", "tokens": ["In", "den", "Ber\u00b7gen", "hier", "und", "dort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hall' es fort und immer fort:", "tokens": ["Hall'", "es", "fort", "und", "im\u00b7mer", "fort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "KON", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Juchheidi, heidi, heida!", "tokens": ["Juch\u00b7hei\u00b7di", ",", "hei\u00b7di", ",", "hei\u00b7da", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "ITJ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}