{"textgrid.poem.34277": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo bist du itzt, mein unverge\u00dflich M\u00e4dchen,", "tokens": ["Wo", "bist", "du", "itzt", ",", "mein", "un\u00b7ver\u00b7ge\u00df\u00b7lich", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo singst du itzt?", "tokens": ["Wo", "singst", "du", "itzt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wo lacht die Flur, wo triumfirt das St\u00e4dtchen,", "tokens": ["Wo", "lacht", "die", "Flur", ",", "wo", "tri\u00b7um\u00b7firt", "das", "St\u00e4dt\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das dich besitzt?", "tokens": ["Das", "dich", "be\u00b7sitzt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Seit du entfernt, will keine Sonne scheinen,", "tokens": ["Seit", "du", "ent\u00b7fernt", ",", "will", "kei\u00b7ne", "Son\u00b7ne", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "VMFIN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und es vereint", "tokens": ["Und", "es", "ver\u00b7eint"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Himmel sich, dir z\u00e4rtlich nachzuweinen,", "tokens": ["Der", "Him\u00b7mel", "sich", ",", "dir", "z\u00e4rt\u00b7lich", "nach\u00b7zu\u00b7wei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "$,", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit deinem Freund.", "tokens": ["Mit", "dei\u00b7nem", "Freund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "All unsre Lust ist fort mit dir gezogen,", "tokens": ["All", "uns\u00b7re", "Lust", "ist", "fort", "mit", "dir", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN", "PTKVZ", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Still \u00fcberall", "tokens": ["Still", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word"], "pos": ["ADJD", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ist Wald und Feld. Dir nach ist sie geflogen", "tokens": ["Ist", "Wald", "und", "Feld", ".", "Dir", "nach", "ist", "sie", "ge\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "KON", "NN", "$.", "PPER", "APPR", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Nachtigall.", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "O komm zur\u00fcck! Schon rufen Hirt und Heerden", "tokens": ["O", "komm", "zu\u00b7r\u00fcck", "!", "Schon", "ru\u00b7fen", "Hirt", "und", "Heer\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKVZ", "$.", "ADV", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich bang herbei.", "tokens": ["Dich", "bang", "her\u00b7bei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Komm bald zur\u00fcck! Sonst wird es Winter werden", "tokens": ["Komm", "bald", "zu\u00b7r\u00fcck", "!", "Sonst", "wird", "es", "Win\u00b7ter", "wer\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKVZ", "$.", "ADV", "VAFIN", "PPER", "NN", "VAINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Monat Mai.", "tokens": ["Im", "Mo\u00b7nat", "Mai", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}