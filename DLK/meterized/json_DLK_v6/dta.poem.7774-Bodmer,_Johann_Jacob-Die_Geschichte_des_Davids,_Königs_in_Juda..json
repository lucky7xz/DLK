{"dta.poem.7774": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Die Geschichte des Davids,  \n K\u00f6nigs in Juda.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er h\u00fctete nunmehr, in seinem Sinn zufrieden,", "tokens": ["Er", "h\u00fc\u00b7te\u00b7te", "nun\u00b7mehr", ",", "in", "sei\u00b7nem", "Sinn", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sch\u00f6ne Wollenheerd, wozu er war beschieden,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Wol\u00b7len\u00b7heerd", ",", "wo\u00b7zu", "er", "war", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nichts war ihm ungewohnt, der Hof, die Stadt, das Feld,", "tokens": ["Nichts", "war", "ihm", "un\u00b7ge\u00b7wohnt", ",", "der", "Hof", ",", "die", "Stadt", ",", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADJD", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es galt ihm alles gleich, wozu man ihn erwehlt.", "tokens": ["Es", "galt", "ihm", "al\u00b7les", "gleich", ",", "wo\u00b7zu", "man", "ihn", "er\u00b7wehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "ADV", "$,", "PWAV", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der vor dem K\u00f6nig stuhnd, dient nunmehr seinen Schafen,", "tokens": ["Der", "vor", "dem", "K\u00f6\u00b7nig", "stuhnd", ",", "dient", "nun\u00b7mehr", "sei\u00b7nen", "Scha\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Statt weicher Federbett will er in H\u00fcrden schlafen,", "tokens": ["Statt", "wei\u00b7cher", "Fe\u00b7der\u00b7bett", "will", "er", "in", "H\u00fcr\u00b7den", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Des Hofes hoher Schein hat ihn nicht so verblendt,", "tokens": ["Des", "Ho\u00b7fes", "ho\u00b7her", "Schein", "hat", "ihn", "nicht", "so", "ver\u00b7blendt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "PPER", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Da\u00df er nicht wie zuvor die Sch\u00e4ferfreude kennt.", "tokens": ["Da\u00df", "er", "nicht", "wie", "zu\u00b7vor", "die", "Sch\u00e4\u00b7fer\u00b7freu\u00b7de", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "KOKOM", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er ist hie mehr vergn\u00fcgt, er ist hie mehr sein eigen,", "tokens": ["Er", "ist", "hie", "mehr", "ver\u00b7gn\u00fcgt", ",", "er", "ist", "hie", "mehr", "sein", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,", "PPER", "VAFIN", "ADV", "ADV", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Hie thut er was er will, hie darff er sich nicht neigen", "tokens": ["Hie", "thut", "er", "was", "er", "will", ",", "hie", "darff", "er", "sich", "nicht", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PWS", "PPER", "VMFIN", "$,", "ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "F\u00fcr diesem oder dem, sein Wort das f\u00fchrt er frey,", "tokens": ["F\u00fcr", "die\u00b7sem", "o\u00b7der", "dem", ",", "sein", "Wort", "das", "f\u00fchrt", "er", "frey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "KON", "ART", "$,", "PPOSAT", "NN", "PDS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und sorgt nicht ob es so noch so zu deuten sey.", "tokens": ["Und", "sorgt", "nicht", "ob", "es", "so", "noch", "so", "zu", "deu\u00b7ten", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "KOUS", "PPER", "ADV", "ADV", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hie hat er keinen Streit, er wei\u00df von keinem Neiden,", "tokens": ["Hie", "hat", "er", "kei\u00b7nen", "Streit", ",", "er", "wei\u00df", "von", "kei\u00b7nem", "Nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$,", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Er darff hie seinen Leib, wie ihm gef\u00e4llt, bekleiden.", "tokens": ["Er", "darff", "hie", "sei\u00b7nen", "Leib", ",", "wie", "ihm", "ge\u00b7f\u00e4llt", ",", "be\u00b7klei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "$,", "PWAV", "PPER", "VVPP", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Mahlzeit richt er zu, wo es ihm selbst beliebt,", "tokens": ["Die", "Mahl\u00b7zeit", "richt", "er", "zu", ",", "wo", "es", "ihm", "selbst", "be\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Bald unter einem Baum, allwo es Schatten giebt,", "tokens": ["Bald", "un\u00b7ter", "ei\u00b7nem", "Baum", ",", "all\u00b7wo", "es", "Schat\u00b7ten", "giebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Bald wenn die Sonne nicht die heissen Strahlen schiesset,", "tokens": ["Bald", "wenn", "die", "Son\u00b7ne", "nicht", "die", "heis\u00b7sen", "Strah\u00b7len", "schies\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Steht ihm ein gr\u00fcnes Thal, das da ein Bach umfliesset,", "tokens": ["Steht", "ihm", "ein", "gr\u00fc\u00b7nes", "Thal", ",", "das", "da", "ein", "Bach", "um\u00b7flies\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Zu seinem Tisch bereit; er isset was er mag,", "tokens": ["Zu", "sei\u00b7nem", "Tisch", "be\u00b7reit", ";", "er", "is\u00b7set", "was", "er", "mag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$.", "PPER", "VVFIN", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und trinckt nicht \u00fcber Durst, folgt der Gesundheit nach.", "tokens": ["Und", "trinckt", "nicht", "\u00fc\u00b7ber", "Durst", ",", "folgt", "der", "Ge\u00b7sund\u00b7heit", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Er gehet zu der Ruh, wenn sich die Sonn verliehret,", "tokens": ["Er", "ge\u00b7het", "zu", "der", "Ruh", ",", "wenn", "sich", "die", "Sonn", "ver\u00b7lieh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und stehet wieder auf, wenn sie den Himmel zieret.", "tokens": ["Und", "ste\u00b7het", "wie\u00b7der", "auf", ",", "wenn", "sie", "den", "Him\u00b7mel", "zie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Niemahls ist er allein, denn Gott der alles f\u00fcllt,", "tokens": ["Nie\u00b7mahls", "ist", "er", "al\u00b7lein", ",", "denn", "Gott", "der", "al\u00b7les", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "KON", "NN", "ART", "PIS", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.24": {"text": "Hat ihn mit seinem Licht, und Gnaden-Geist umh\u00fcllt.", "tokens": ["Hat", "ihn", "mit", "sei\u00b7nem", "Licht", ",", "und", "Gna\u00b7den\u00b7Geist", "um\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Denselben lobet er in diesen gr\u00fcnen Auen", "tokens": ["Den\u00b7sel\u00b7ben", "lo\u00b7bet", "er", "in", "die\u00b7sen", "gr\u00fc\u00b7nen", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Mit manchem Kunst-Geb\u00e4ud. In Gott sich zu erbauen,", "tokens": ["Mit", "man\u00b7chem", "Kunst\u00b7Ge\u00b7b\u00e4ud", ".", "In", "Gott", "sich", "zu", "er\u00b7bau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$.", "APPR", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ist nur sein Flei\u00df gewandt. Er dencket stets dahin", "tokens": ["Ist", "nur", "sein", "Flei\u00df", "ge\u00b7wandt", ".", "Er", "den\u00b7cket", "stets", "da\u00b7hin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$.", "PPER", "VVFIN", "ADV", "PAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wie er Gott dienen m\u00f6g mit Andachtsvollem Sinn.", "tokens": ["Wie", "er", "Gott", "die\u00b7nen", "m\u00f6g", "mit", "An\u00b7dachts\u00b7vol\u00b7lem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVINF", "VMFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So brachte David hin die Zeit bey seinen Heerden,", "tokens": ["So", "brach\u00b7te", "Da\u00b7vid", "hin", "die", "Zeit", "bey", "sei\u00b7nen", "Heer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die ihm nicht lange konnt auf diese Weise werden,", "tokens": ["Die", "ihm", "nicht", "lan\u00b7ge", "konnt", "auf", "die\u00b7se", "Wei\u00b7se", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "ADV", "VMFIN", "APPR", "PDAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Als ihm an einem Tag, wie f\u00fcr der Sonnenschein", "tokens": ["Als", "ihm", "an", "ei\u00b7nem", "Tag", ",", "wie", "f\u00fcr", "der", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Er k\u00fchlen Schatten sucht, di\u00df eben fiele ein:", "tokens": ["Er", "k\u00fch\u00b7len", "Schat\u00b7ten", "sucht", ",", "di\u00df", "e\u00b7ben", "fie\u00b7le", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "$,", "PDS", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wie Gott der Wunder-Gott, den Sauel fallen lassen,", "tokens": ["Wie", "Gott", "der", "Wun\u00b7der\u00b7Gott", ",", "den", "Sau\u00b7el", "fal\u00b7len", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "$,", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Den er doch erst erwehlt den Scepter zu umfassen,", "tokens": ["Den", "er", "doch", "erst", "er\u00b7wehlt", "den", "Scep\u00b7ter", "zu", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und seufzte bitterlich, da\u00df der verworffen gar,", "tokens": ["Und", "seufz\u00b7te", "bit\u00b7ter\u00b7lich", ",", "da\u00df", "der", "ver\u00b7worf\u00b7fen", "gar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "KOUS", "ART", "VVFIN", "ADV", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Der da in Jsrael der erste K\u00f6nig war.", "tokens": ["Der", "da", "in", "Js\u00b7rael", "der", "ers\u00b7te", "K\u00f6\u00b7nig", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NE", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Ach! dacht er bey sich selbst, was hat doch Saul begangen,", "tokens": ["Ach", "!", "dacht", "er", "bey", "sich", "selbst", ",", "was", "hat", "doch", "Saul", "be\u00b7gan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "APPR", "PRF", "ADV", "$,", "PWS", "VAFIN", "ADV", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Da\u00df er nicht wiederum Vergebung kan erlangen?", "tokens": ["Da\u00df", "er", "nicht", "wie\u00b7de\u00b7rum", "Ver\u00b7ge\u00b7bung", "kan", "er\u00b7lan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wohin ist Gottes G\u00fct? es ist zwar Gott gerecht,", "tokens": ["Wo\u00b7hin", "ist", "Got\u00b7tes", "G\u00fct", "?", "es", "ist", "zwar", "Gott", "ge\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "NN", "$.", "PPER", "VAFIN", "ADV", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Doch geht er ins Gericht nicht oft mit seinem Knecht.", "tokens": ["Doch", "geht", "er", "ins", "Ge\u00b7richt", "nicht", "oft", "mit", "sei\u00b7nem", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wann alle Missethat der H\u00f6chste wollte straffen,", "tokens": ["Wann", "al\u00b7le", "Mis\u00b7se\u00b7that", "der", "H\u00f6chs\u00b7te", "woll\u00b7te", "straf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "So w\u00fcrd uns allen ja sein strenger Zorn hinraffen;", "tokens": ["So", "w\u00fcrd", "uns", "al\u00b7len", "ja", "sein", "stren\u00b7ger", "Zorn", "hin\u00b7raf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Warum findt Saul allein denn jetzo keine Gnad?", "tokens": ["Wa\u00b7rum", "findt", "Saul", "al\u00b7lein", "denn", "jet\u00b7zo", "kei\u00b7ne", "Gnad", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NE", "ADV", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Ach, fuhr er weiter fort, es ist des H\u00f6chsten Rath,", "tokens": ["Ach", ",", "fuhr", "er", "wei\u00b7ter", "fort", ",", "es", "ist", "des", "H\u00f6chs\u00b7ten", "Rath", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Dem mu\u00df man halten still; der H\u00f6chste kennt die Hertzen,", "tokens": ["Dem", "mu\u00df", "man", "hal\u00b7ten", "still", ";", "der", "H\u00f6chs\u00b7te", "kennt", "die", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Wer wei\u00df ob Saul nicht will mit Bu\u00df und Reue schertzen,", "tokens": ["Wer", "wei\u00df", "ob", "Saul", "nicht", "will", "mit", "Bu\u00df", "und", "Reu\u00b7e", "schert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KOUS", "NE", "PTKNEG", "VMFIN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Vielleicht erkennt er nicht die S\u00fcnd die er gethan,", "tokens": ["Viel\u00b7leicht", "er\u00b7kennt", "er", "nicht", "die", "S\u00fcnd", "die", "er", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "ART", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Und will doch, da\u00df ihn Gott soll wieder nehmen an.", "tokens": ["Und", "will", "doch", ",", "da\u00df", "ihn", "Gott", "soll", "wie\u00b7der", "neh\u00b7men", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "$,", "KOUS", "PPER", "NN", "VMFIN", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Hierauf schwieg David still und stellt ihm f\u00fcr die Augen", "tokens": ["Hier\u00b7auf", "schwieg", "Da\u00b7vid", "still", "und", "stellt", "ihm", "f\u00fcr", "die", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "NE", "PTKVZ", "KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Wie hoch und schwer es sey, f\u00fcr Gottes Geist zu taugen.", "tokens": ["Wie", "hoch", "und", "schwer", "es", "sey", ",", "f\u00fcr", "Got\u00b7tes", "Geist", "zu", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "PPER", "VAFIN", "$,", "APPR", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Drum ward er sehr betr\u00fcbt, wie er daran gedacht,", "tokens": ["Drum", "ward", "er", "sehr", "be\u00b7tr\u00fcbt", ",", "wie", "er", "da\u00b7ran", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "VVPP", "$,", "PWAV", "PPER", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Was unl\u00e4ngst Samuel bey ihnen hat gemacht,", "tokens": ["Was", "un\u00b7l\u00e4ngst", "Sa\u00b7muel", "bey", "ih\u00b7nen", "hat", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NE", "APPR", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.53": {"text": "Wie der sein Haupt gesalbt, da\u00df er solt K\u00f6nig werden,", "tokens": ["Wie", "der", "sein", "Haupt", "ge\u00b7salbt", ",", "da\u00df", "er", "solt", "K\u00f6\u00b7nig", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "PPOSAT", "NN", "VVPP", "$,", "KOUS", "PPER", "VMFIN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Da\u00df er ein grosses Volck, statt seiner schwachen Heerden,", "tokens": ["Da\u00df", "er", "ein", "gros\u00b7ses", "Volck", ",", "statt", "sei\u00b7ner", "schwa\u00b7chen", "Heer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,", "KOUI", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Einsmahls regieren sollt. Dis gab ihm keine Ruh;", "tokens": ["Eins\u00b7mahls", "re\u00b7gie\u00b7ren", "sollt", ".", "Dis", "gab", "ihm", "kei\u00b7ne", "Ruh", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$.", "PDS", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.56": {"text": "Sein Hertze fragt\u2019 ihn stets: sag an, was machest du?", "tokens": ["Sein", "Hert\u00b7ze", "fragt'", "ihn", "stets", ":", "sag", "an", ",", "was", "ma\u00b7chest", "du", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$.", "VVIMP", "PTKVZ", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Nicht, da\u00df sein hoher Muth wolt zu regieren scheuen,", "tokens": ["Nicht", ",", "da\u00df", "sein", "ho\u00b7her", "Muth", "wolt", "zu", "re\u00b7gie\u00b7ren", "scheu\u0