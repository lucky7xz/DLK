 "PPER", "PIAT", "KOKOM", "NE", "$,", "VVFIN", "PPER", "$,", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Was soll mir dann die Kron, die w\u00fcrde mich nur qu\u00e4len;", "tokens": ["Was", "soll", "mir", "dann", "die", "Kron", ",", "die", "w\u00fcr\u00b7de", "mich", "nur", "qu\u00e4\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ART", "NN", "$,", "PRELS", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Jedoch, hie hielt er inn, befahl es seinem Gott,", "tokens": ["Je\u00b7doch", ",", "hie", "hielt", "er", "inn", ",", "be\u00b7fahl", "es", "sei\u00b7nem", "Gott", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Und dacht es mu\u00df geschehn des H\u00f6chsten sein Geboth;", "tokens": ["Und", "dacht", "es", "mu\u00df", "ge\u00b7schehn", "des", "H\u00f6chs\u00b7ten", "sein", "Ge\u00b7both", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VMFIN", "VVINF", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Desselben Wille war sein Will und macht ihn schweigen,", "tokens": ["Des\u00b7sel\u00b7ben", "Wil\u00b7le", "war", "sein", "Will", "und", "macht", "ihn", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "VAINF", "VMFIN", "KON", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Er will nicht gr\u00fcbeln mehr, er will Gehorsam zeigen", "tokens": ["Er", "will", "nicht", "gr\u00fc\u00b7beln", "mehr", ",", "er", "will", "Ge\u00b7hor\u00b7sam", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVFIN", "ADV", "$,", "PPER", "VMFIN", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Und folgen dem Beruff, in Demuth schlecht und recht,", "tokens": ["Und", "fol\u00b7gen", "dem", "Be\u00b7ruff", ",", "in", "De\u00b7muth", "schlecht", "und", "recht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Er ist bereit zu seyn ein K\u00f6nig oder Knecht.", "tokens": ["Er", "ist", "be\u00b7reit", "zu", "seyn", "ein", "K\u00f6\u00b7nig", "o\u00b7der", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PTKZU", "VAINF", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}