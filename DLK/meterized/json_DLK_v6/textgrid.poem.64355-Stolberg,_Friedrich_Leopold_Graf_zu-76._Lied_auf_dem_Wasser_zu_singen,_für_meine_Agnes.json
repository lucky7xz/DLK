{"textgrid.poem.64355": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "76. Lied auf dem Wasser zu singen, f\u00fcr meine Agnes", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mitten im Schimmer der spiegelnden Wellen", "tokens": ["Mit\u00b7ten", "im", "Schim\u00b7mer", "der", "spie\u00b7geln\u00b7den", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Gleitet wie Schw\u00e4ne der wankende Kahn;", "tokens": ["Glei\u00b7tet", "wie", "Schw\u00e4\u00b7ne", "der", "wan\u00b7ken\u00b7de", "Kahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Ach, auf der Freude sanftschimmernden Wellen", "tokens": ["Ach", ",", "auf", "der", "Freu\u00b7de", "sanft\u00b7schim\u00b7mern\u00b7den", "Wel\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Gleitet die Seele dahin wie der Kahn;", "tokens": ["Glei\u00b7tet", "die", "See\u00b7le", "da\u00b7hin", "wie", "der", "Kahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PAV", "KOKOM", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Denn von dem Himmel herab auf die Wellen", "tokens": ["Denn", "von", "dem", "Him\u00b7mel", "her\u00b7ab", "auf", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "APPR", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Tanzet das Abendrot rund um den Kahn.", "tokens": ["Tan\u00b7zet", "das", "A\u00b7ben\u00b7drot", "rund", "um", "den", "Kahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00dcber den Wipfeln des westlichen Haines", "tokens": ["\u00dc\u00b7ber", "den", "Wip\u00b7feln", "des", "west\u00b7li\u00b7chen", "Hai\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Winket uns freundlich der r\u00f6tliche Schein;", "tokens": ["Win\u00b7ket", "uns", "freund\u00b7lich", "der", "r\u00f6t\u00b7li\u00b7che", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Unter den Zweigen des \u00f6stlichen Haines", "tokens": ["Un\u00b7ter", "den", "Zwei\u00b7gen", "des", "\u00f6st\u00b7li\u00b7chen", "Hai\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "S\u00e4uselt der Kalmus im r\u00f6tlichen Schein;", "tokens": ["S\u00e4u\u00b7selt", "der", "Kal\u00b7mus", "im", "r\u00f6t\u00b7li\u00b7chen", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Freude des Himmels und Ruhe des Haines", "tokens": ["Freu\u00b7de", "des", "Him\u00b7mels", "und", "Ru\u00b7he", "des", "Hai\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "NN", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Atmet die Seel' im err\u00f6tenden Schein.", "tokens": ["At\u00b7met", "die", "Seel'", "im", "er\u00b7r\u00f6\u00b7ten\u00b7den", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach es entschwindet mit tauigem Fl\u00fcgel", "tokens": ["Ach", "es", "ent\u00b7schwin\u00b7det", "mit", "tau\u00b7i\u00b7gem", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Mir auf den wiegenden Wellen die Zeit.", "tokens": ["Mir", "auf", "den", "wie\u00b7gen\u00b7den", "Wel\u00b7len", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Morgen entschwinde mit schimmerndem Fl\u00fcgel", "tokens": ["Mor\u00b7gen", "ent\u00b7schwin\u00b7de", "mit", "schim\u00b7mern\u00b7dem", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Wieder wie gestern uns heute die Zeit,", "tokens": ["Wie\u00b7der", "wie", "ge\u00b7stern", "uns", "heu\u00b7te", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADV", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Bis ich auf h\u00f6herem strahlenden Fl\u00fcgel", "tokens": ["Bis", "ich", "auf", "h\u00f6\u00b7he\u00b7rem", "strah\u00b7len\u00b7den", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Selber entschwinde der wechselnden Zeit.", "tokens": ["Sel\u00b7ber", "ent\u00b7schwin\u00b7de", "der", "wech\u00b7seln\u00b7den", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}