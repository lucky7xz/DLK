{"textgrid.poem.38698": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Eros", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann du l\u00e4chelst, o Knabe, s\u00e4umt mit Golde", "tokens": ["Wann", "du", "l\u00e4\u00b7chelst", ",", "o", "Kna\u00b7be", ",", "s\u00e4umt", "mit", "Gol\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "FM", "NN", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich die donnernde Wolke, ruhn Orkane,", "tokens": ["Sich", "die", "don\u00b7nern\u00b7de", "Wol\u00b7ke", ",", "ruhn", "Or\u00b7ka\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Spriessen Blumen, wo nie des lauen Zefyrs", "tokens": ["Spries\u00b7sen", "Blu\u00b7men", ",", "wo", "nie", "des", "lau\u00b7en", "Ze\u00b7fyrs"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PWAV", "ADV", "ART", "ADJA", "NN"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Odem gewandelt.", "tokens": ["O\u00b7dem", "ge\u00b7wan\u00b7delt", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Selig! l\u00e4chelst du mit Aurorens Milde!", "tokens": ["Se\u00b7lig", "!", "l\u00e4\u00b7chelst", "du", "mit", "Au\u00b7ro\u00b7rens", "Mil\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Aber, wehe! dem zarten Fl\u00fcgel Psyches,", "tokens": ["A\u00b7ber", ",", "we\u00b7he", "!", "dem", "zar\u00b7ten", "Fl\u00fc\u00b7gel", "Psy\u00b7ches", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "$.", "ART", "ADJA", "NN", "NE", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wann vom Auge der niedern Herzenswildheit", "tokens": ["Wann", "vom", "Au\u00b7ge", "der", "nie\u00b7dern", "Her\u00b7zens\u00b7wild\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Flamme dir lodert.", "tokens": ["Flam\u00b7me", "dir", "lo\u00b7dert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}