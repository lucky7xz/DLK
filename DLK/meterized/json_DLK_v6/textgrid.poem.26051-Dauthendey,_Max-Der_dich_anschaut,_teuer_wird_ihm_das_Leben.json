{"textgrid.poem.26051": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Der dich anschaut, teuer wird ihm das Leben", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An deinem Haar, das dich wie ein Himmel schm\u00fcckt,", "tokens": ["An", "dei\u00b7nem", "Haar", ",", "das", "dich", "wie", "ein", "Him\u00b7mel", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PPER", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Daran hangen mit schwerem Gewicht", "tokens": ["Da\u00b7ran", "han\u00b7gen", "mit", "schwe\u00b7rem", "Ge\u00b7wicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Wie goldene Spangen meine Freuden.", "tokens": ["Wie", "gol\u00b7de\u00b7ne", "Span\u00b7gen", "mei\u00b7ne", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die B\u00e4ume meiner W\u00fcnsche haben", "tokens": ["Die", "B\u00e4u\u00b7me", "mei\u00b7ner", "W\u00fcn\u00b7sche", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sich mit Wurzeln und Kronen verfangen.", "tokens": ["Sich", "mit", "Wur\u00b7zeln", "und", "Kro\u00b7nen", "ver\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Dein Haar tr\u00e4gt der Liebe wunderbar Feuer;", "tokens": ["Dein", "Haar", "tr\u00e4gt", "der", "Lie\u00b7be", "wun\u00b7der\u00b7bar", "Feu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Der dich anschaut, teuer wird ihm das Leben.", "tokens": ["Der", "dich", "an\u00b7schaut", ",", "teu\u00b7er", "wird", "ihm", "das", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ADJD", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}}}}