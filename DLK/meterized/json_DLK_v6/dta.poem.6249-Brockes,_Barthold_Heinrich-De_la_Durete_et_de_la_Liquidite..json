{"dta.poem.6249": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "De la Durete et de la  \n Liquidite.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lasst uns mit h\u00f6chstem Fleis bem\u00fcht seyn zu entdecken,", "tokens": ["Lasst", "uns", "mit", "h\u00f6chs\u00b7tem", "Fleis", "be\u00b7m\u00fcht", "seyn", "zu", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "VVPP", "VAINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was das, so sie bewegt, was die Empfindungs-Krafft,", "tokens": ["Was", "das", ",", "so", "sie", "be\u00b7wegt", ",", "was", "die", "Emp\u00b7fin\u00b7dungs\u00b7Krafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "$,", "ADV", "PPER", "VVFIN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auch endlich alles das, was man an ihnen nennt", "tokens": ["Auch", "end\u00b7lich", "al\u00b7les", "das", ",", "was", "man", "an", "ih\u00b7nen", "nennt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIS", "PDS", "$,", "PRELS", "PIS", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Beschaffenheit, Gestalt, Natur und Eigenschafft.", "tokens": ["Be\u00b7schaf\u00b7fen\u00b7heit", ",", "Ge\u00b7stalt", ",", "Na\u00b7tur", "und", "Ei\u00b7gen\u00b7schafft", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON