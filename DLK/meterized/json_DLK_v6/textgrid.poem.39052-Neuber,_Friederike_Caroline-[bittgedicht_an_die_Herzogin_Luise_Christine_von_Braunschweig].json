{"textgrid.poem.39052": {"metadata": {"author": {"name": "Neuber, Friederike Caroline", "birth": "N.A.", "death": "N.A."}, "title": "[bittgedicht an die Herzogin Luise Christine von Braunschweig]", "genre": "verse", "period": "N.A.", "pub_year": 1728, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O gro\u00dfe Herzogin!", "tokens": ["O", "gro\u00b7\u00dfe", "Her\u00b7zo\u00b7gin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "H\u00fcll diese meine Bitte", "tokens": ["H\u00fcll", "die\u00b7se", "mei\u00b7ne", "Bit\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PDAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "die rein und billig ist in Deinen Vorspruch ein!", "tokens": ["die", "rein", "und", "bil\u00b7lig", "ist", "in", "Dei\u00b7nen", "Vor\u00b7spruch", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ich klagte nicht so sehr wenn ich nicht schmerzlich litte,", "tokens": ["ich", "klag\u00b7te", "nicht", "so", "sehr", "wenn", "ich", "nicht", "schmerz\u00b7lich", "lit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ADV", "KOUS", "PPER", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "denn wenn ich klagen soll so mu\u00df es n\u00f6thig seyn.", "tokens": ["denn", "wenn", "ich", "kla\u00b7gen", "soll", "so", "mu\u00df", "es", "n\u00f6\u00b7thig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVINF", "VMFIN", "ADV", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gieb meinen Worten krafft, da\u00df sie das Herze r\u00fchren,", "tokens": ["Gieb", "mei\u00b7nen", "Wor\u00b7ten", "krafft", ",", "da\u00df", "sie", "das", "Her\u00b7ze", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "PDS", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "und da\u00df die K\u00f6nigin mich arme Magd erh\u00f6rt,", "tokens": ["und", "da\u00df", "die", "K\u00f6\u00b7ni\u00b7gin", "mich", "ar\u00b7me", "Magd", "er\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "damit ich nicht mein Guth nur darum mu\u00df verliehren,", "tokens": ["da\u00b7mit", "ich", "nicht", "mein", "Guth", "nur", "da\u00b7rum", "mu\u00df", "ver\u00b7lieh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "ADV", "PAV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "weil es der M\u00fcller hat von Ihr f\u00fcr sich begehrt.", "tokens": ["weil", "es", "der", "M\u00fcl\u00b7ler", "hat", "von", "Ihr", "f\u00fcr", "sich", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NE", "VAFIN", "APPR", "PPER", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du weist bey Hofe mu\u00df die Wahrheit \u00f6ffters leiden", "tokens": ["Du", "weist", "bey", "Ho\u00b7fe", "mu\u00df", "die", "Wahr\u00b7heit", "\u00f6ff\u00b7ters", "lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "VMFIN", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "da\u00df sie verstecket wird, nimm Du Dich meiner an,", "tokens": ["da\u00df", "sie", "ver\u00b7ste\u00b7cket", "wird", ",", "nimm", "Du", "Dich", "mei\u00b7ner", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "VVIMP", "PPER", "PRF", "PPOSAT", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "la\u00df mich Dein Hohes Wort vor ihrem Thron begleiten,", "tokens": ["la\u00df", "mich", "Dein", "Ho\u00b7hes", "Wort", "vor", "ih\u00b7rem", "Thron", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "ich will nur redlich seyn sonst hab ich nichts gethan.", "tokens": ["ich", "will", "nur", "red\u00b7lich", "seyn", "sonst", "hab", "ich", "nichts", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "VAINF", "ADV", "VAFIN", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "ich habe M\u00fcllern nicht von seinem Brod verdrungen,", "tokens": ["ich", "ha\u00b7be", "M\u00fcl\u00b7lern", "nicht", "von", "sei\u00b7nem", "Brod", "ver\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "ich habe nichts gesucht was ihm k\u00f6nt sch\u00e4dlich seyn,", "tokens": ["ich", "ha\u00b7be", "nichts", "ge\u00b7sucht", "was", "ihm", "k\u00f6nt", "sch\u00e4d\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVPP", "PWS", "PPER", "VVFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "nun ist ihm durch Betrug einmahl ein Streich gelungen", "tokens": ["nun", "ist", "ihm", "durch", "Be\u00b7trug", "ein\u00b7mahl", "ein", "Streich", "ge\u00b7lun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "so dringt er mit Gewalt in mein Verm\u00f6gen ein;", "tokens": ["so", "dringt", "er", "mit", "Ge\u00b7walt", "in", "mein", "Ver\u00b7m\u00f6\u00b7gen", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Bel\u00fcgt den Landes Herrn, und braucht die H\u00f6chste Gnade", "tokens": ["Be\u00b7l\u00fcgt", "den", "Lan\u00b7des", "Herrn", ",", "und", "braucht", "die", "H\u00f6chs\u00b7te", "Gna\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN", "$,", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "zu nichts als nur dem Troz damit genug zu thun,", "tokens": ["zu", "nichts", "als", "nur", "dem", "Troz", "da\u00b7mit", "ge\u00b7nug", "zu", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "KOKOM", "ADV", "ART", "NN", "PAV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "da\u00df er mich elend macht, und da\u00df mein gr\u00f6ster Schade", "tokens": ["da\u00df", "er", "mich", "e\u00b7lend", "macht", ",", "und", "da\u00df", "mein", "gr\u00f6s\u00b7ter", "Scha\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$,", "KON", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "allein in seiner Macht und Willen soll beruhn.", "tokens": ["al\u00b7lein", "in", "sei\u00b7ner", "Macht", "und", "Wil\u00b7len", "soll", "be\u00b7ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Er bittet was das Land ganz in Verwundrung sezet,", "tokens": ["Er", "bit\u00b7tet", "was", "das", "Land", "ganz", "in", "Ver\u00b7wund\u00b7rung", "se\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "denn so ists nicht erh\u00f6rt, da\u00df man ", "tokens": ["denn", "so", "ists", "nicht", "er\u00b7h\u00f6rt", ",", "da\u00df", "man"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PTKNEG", "VVPP", "$,", "KOUS", "PIS"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.24": {"text": "Er macht die Sache klein da\u00df man sie gar nichts sch\u00e4zet", "tokens": ["Er", "macht", "die", "Sa\u00b7che", "klein", "da\u00df", "man", "sie", "gar", "nichts", "sch\u00e4\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "KOUS", "PIS", "PPER", "ADV", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "und da\u00df man sie wohl gar von Recht und g\u00fcltig spricht.", "tokens": ["und", "da\u00df", "man", "sie", "wohl", "gar", "von", "Recht", "und", "g\u00fcl\u00b7tig", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PPER", "ADV", "ADV", "APPR", "NN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "sonst spr\u00e4ch er nicht mein Guth dem b\u00f6\u00dfen M\u00fcller zu,", "tokens": ["sonst", "spr\u00e4ch", "er", "nicht", "mein", "Guth", "dem", "b\u00f6\u00b7\u00dfen", "M\u00fcl\u00b7ler", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "ART", "ADJA", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "unm\u00f6glich k\u00f6nt es seyn da\u00df er mich lie\u00df verjagen.", "tokens": ["un\u00b7m\u00f6g\u00b7lich", "k\u00f6nt", "es", "seyn", "da\u00df", "er", "mich", "lie\u00df", "ver\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "VAINF", "KOUS", "PPER", "PRF", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Sprich gro\u00dfe Frau vor mich, wer kennt mich sonst als Du?", "tokens": ["Sprich", "gro\u00b7\u00dfe", "Frau", "vor", "mich", ",", "wer", "kennt", "mich", "sonst", "als", "Du", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "APPR", "PPER", "$,", "PWS", "VVFIN", "PPER", "ADV", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Du kennst dem M\u00fcller auch Du kennst auch meine Leute", "tokens": ["Du", "kennst", "dem", "M\u00fcl\u00b7ler", "auch", "Du", "kennst", "auch", "mei\u00b7ne", "Leu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NE", "ADV", "PPER", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Du hast so viele Jahr uns \u00f6ffters Gnad geschenkt", "tokens": ["Du", "hast", "so", "vie\u00b7le", "Jahr", "uns", "\u00f6ff\u00b7ters", "Gnad", "ge\u00b7schenkt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "PPER", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Verla\u00df mich nicht iezund, erh\u00f6re mich auch heute,", "tokens": ["Ver\u00b7la\u00df", "mich", "nicht", "ie\u00b7zund", ",", "er\u00b7h\u00f6\u00b7re", "mich", "auch", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADV", "$,", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "weil sich mein armes Herz so gar empfindlich kr\u00e4nckt", "tokens": ["weil", "sich", "mein", "ar\u00b7mes", "Herz", "so", "gar", "emp\u00b7find\u00b7lich", "kr\u00e4nckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Sprich hab ich was gethan, ob alle meine Sachen", "tokens": ["Sprich", "hab", "ich", "was", "ge\u00b7than", ",", "ob", "al\u00b7le", "mei\u00b7ne", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "VAFIN", "PPER", "PIS", "VVPP", "$,", "KOUS", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "nicht wahr und redlich sind, ob Wercke Wort und that,", "tokens": ["nicht", "wahr", "und", "red\u00b7lich", "sind", ",", "ob", "Wer\u00b7cke", "Wort", "und", "that", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "KON", "ADJD", "VAFIN", "$,", "KOUS", "NN", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "nicht unsre Spiele rein und sehens w\u00fcrdig machen,", "tokens": ["nicht", "uns\u00b7re", "Spie\u00b7le", "rein", "und", "se\u00b7hens", "w\u00fcr\u00b7dig", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "ADJD", "KON", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "und ob man \u00fcber uns in was zu klagen hat!", "tokens": ["und", "ob", "man", "\u00fc\u00b7ber", "uns", "in", "was", "zu", "kla\u00b7gen", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "APPR", "PPER", "APPR", "PRELS", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Dein Herzog hat uns gar dis schwere Jahr erhalten", "tokens": ["Dein", "Her\u00b7zog", "hat", "uns", "gar", "dis", "schwe\u00b7re", "Jahr", "er\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "PDS", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "da mich, die Trauerzeit, mein Vaterland verlie\u00df", "tokens": ["da", "mich", ",", "die", "Trau\u00b7er\u00b7zeit", ",", "mein", "Va\u00b7ter\u00b7land", "ver\u00b7lie\u00df"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "und nach dem ersten Spruch von unsern lieben alten", "tokens": ["und", "nach", "dem", "ers\u00b7ten", "Spruch", "von", "un\u00b7sern", "lie\u00b7ben", "al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "mit schmerzlichen Verlust der Nahrung trauern hie\u00df;", "tokens": ["mit", "schmerz\u00b7li\u00b7chen", "Ver\u00b7lust", "der", "Nah\u00b7rung", "trau\u00b7ern", "hie\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "w\u00e4r dieses nicht geschehen, so w\u00e4r schon mehr verlohren,", "tokens": ["w\u00e4r", "die\u00b7ses", "nicht", "ge\u00b7sche\u00b7hen", 