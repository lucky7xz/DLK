{"textgrid.poem.41381": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Kenner", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es lie\u00df sich in der V\u00f6gel Ch\u00f6ren", "tokens": ["Es", "lie\u00df", "sich", "in", "der", "V\u00f6\u00b7gel", "Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Unl\u00e4ngst ein junger Vogel h\u00f6ren,", "tokens": ["Un\u00b7l\u00e4ngst", "ein", "jun\u00b7ger", "Vo\u00b7gel", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und suchte nichts so sehr, als wahrer Kenner Gunst.", "tokens": ["Und", "such\u00b7te", "nichts", "so", "sehr", ",", "als", "wah\u00b7rer", "Ken\u00b7ner", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "ADV", "$,", "KOUS", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gemeiner S\u00e4nger List wirbt manchen feilen G\u00f6nner:", "tokens": ["Ge\u00b7mei\u00b7ner", "S\u00e4n\u00b7ger", "List", "wirbt", "man\u00b7chen", "fei\u00b7len", "G\u00f6n\u00b7ner", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Allein das Lobgeschrei, der Beifall halber Kenner", "tokens": ["Al\u00b7lein", "das", "Lob\u00b7ge\u00b7schrei", ",", "der", "Bei\u00b7fall", "hal\u00b7ber", "Ken\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "APPO", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Entehrt, und zieret keine Kunst.", "tokens": ["Ent\u00b7ehrt", ",", "und", "zie\u00b7ret", "kei\u00b7ne", "Kunst", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es lobten ihn die Haidelerche,", "tokens": ["Es", "lob\u00b7ten", "ihn", "die", "Hai\u00b7de\u00b7ler\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein reisend Paar verirrter St\u00f6rche,", "tokens": ["Ein", "rei\u00b7send", "Paar", "ver\u00b7irr\u00b7ter", "St\u00f6r\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Staar, der Zitscherling, der Wendehals, der Specht.", "tokens": ["Der", "Staar", ",", "der", "Zit\u00b7scher\u00b7ling", ",", "der", "Wen\u00b7de\u00b7hals", ",", "der", "Specht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "ADV", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der H\u00e4nfling kam hervor, und bat ihn, mehr zu singen;", "tokens": ["Der", "H\u00e4nf\u00b7ling", "kam", "her\u00b7vor", ",", "und", "bat", "ihn", ",", "mehr", "zu", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der heischre Kiebitz schrie: Nichts kann mir besser klingen;", "tokens": ["Der", "heischre", "Kie\u00b7bitz", "schrie", ":", "Nichts", "kann", "mir", "bes\u00b7ser", "klin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "PIS", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "--+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Der Reiger sagte: Du hast Recht.", "tokens": ["Der", "Rei\u00b7ger", "sag\u00b7te", ":", "Du", "hast", "Recht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Elster schwatzte ganze Stunden,", "tokens": ["Die", "Els\u00b7ter", "schwatz\u00b7te", "gan\u00b7ze", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und r\u00fchmte was sie sch\u00f6n befunden,", "tokens": ["Und", "r\u00fchm\u00b7te", "was", "sie", "sch\u00f6n", "be\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des freien Schalles H\u00f6h', und sanfter T\u00f6ne Fall.", "tokens": ["Des", "frei\u00b7en", "Schal\u00b7les", "H\u00f6h'", ",", "und", "sanf\u00b7ter", "T\u00f6\u00b7ne", "Fall", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "KON", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der ekle Vogel sprach: Soll nichts dem Wunsche fehlen,", "tokens": ["Der", "ek\u00b7le", "Vo\u00b7gel", "sprach", ":", "Soll", "nichts", "dem", "Wun\u00b7sche", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "VMFIN", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und darf sich mein Versuch selbst einen Richter w\u00e4hlen,", "tokens": ["Und", "darf", "sich", "mein", "Ver\u00b7such", "selbst", "ei\u00b7nen", "Rich\u00b7ter", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "PPOSAT", "NN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So w\u00e4hl' ich mir die Nachtigall.", "tokens": ["So", "w\u00e4hl'", "ich", "mir", "die", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mich d\u00fcnkt, sein Wunsch ist nicht zu tadeln.", "tokens": ["Mich", "d\u00fcnkt", ",", "sein", "Wunsch", "ist", "nicht", "zu", "ta\u00b7deln", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Soll uns ein \u00e4chter Vorzug adeln,", "tokens": ["Soll", "uns", "ein", "\u00e4ch\u00b7ter", "Vor\u00b7zug", "a\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So mu\u00df der Einsicht Kraft den Stimmen Werth verleihn.", "tokens": ["So", "mu\u00df", "der", "Ein\u00b7sicht", "Kraft", "den", "Stim\u00b7men", "Werth", "ver\u00b7leihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Man kennt, man \u00fcberlebt des Nachruhms Ewigkeiten,", "tokens": ["Man", "kennt", ",", "man", "\u00fc\u00b7ber\u00b7lebt", "des", "Nach\u00b7ruhms", "E\u00b7wig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die der Gelehrten Schaum, die Schmeichler unsrer Zeiten,", "tokens": ["Die", "der", "Ge\u00b7lehr\u00b7ten", "Schaum", ",", "die", "Schmeich\u00b7ler", "uns\u00b7rer", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Einander ohn' Err\u00f6then weihn.", "tokens": ["Ein\u00b7an\u00b7der", "ohn'", "Er\u00b7r\u00f6\u00b7then", "weihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du Freund und Muster deutscher Dichter,", "tokens": ["Du", "Freund", "und", "Mus\u00b7ter", "deut\u00b7scher", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wahrheit liebensw\u00fcrdger Richter,", "tokens": ["Der", "Wahr\u00b7heit", "lie\u00b7bens\u00b7w\u00fcrd\u00b7ger", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Wilkens, den vorl\u00e4ngst der Pindus lieb gewann;", "tokens": ["Mein", "Wil\u00b7kens", ",", "den", "vor\u00b7l\u00e4ngst", "der", "Pin\u00b7dus", "lieb", "ge\u00b7wann", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wie reizend werden mir doch meine Lieder schallen!", "tokens": ["Wie", "rei\u00b7zend", "wer\u00b7den", "mir", "doch", "mei\u00b7ne", "Lie\u00b7der", "schal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie werd' ich, Werthester, mir endlich selbst gefallen,", "tokens": ["Wie", "werd'", "ich", ",", "Wert\u00b7hes\u00b7ter", ",", "mir", "end\u00b7lich", "selbst", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "NN", "$,", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn ich nur dir gefallen kann!", "tokens": ["Wenn", "ich", "nur", "dir", "ge\u00b7fal\u00b7len", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}