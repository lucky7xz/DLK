{"textgrid.poem.39540": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Ehren-Verleihungen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Papiergeld sind die Orden und die Titel:", "tokens": ["Pa\u00b7pier\u00b7geld", "sind", "die", "Or\u00b7den", "und", "die", "Ti\u00b7tel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Diener kargen Sold erg\u00e4nzen solche Mittel;", "tokens": ["Der", "Die\u00b7ner", "kar\u00b7gen", "Sold", "er\u00b7g\u00e4n\u00b7zen", "sol\u00b7che", "Mit\u00b7tel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So h\u00e4lt man fein das baare Geld zu Rath.", "tokens": ["So", "h\u00e4lt", "man", "fein", "das", "baa\u00b7re", "Geld", "zu", "Rath", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch durch Verschwendung sinken sie im Preise,", "tokens": ["Doch", "durch", "Ver\u00b7schwen\u00b7dung", "sin\u00b7ken", "sie", "im", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann geht es nach der Assignate Weise:", "tokens": ["Dann", "geht", "es", "nach", "der", "As\u00b7sig\u00b7na\u00b7te", "Wei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Staat macht bankerott mit seinem Bettelstaat.", "tokens": ["Der", "Staat", "macht", "ban\u00b7ke\u00b7rott", "mit", "sei\u00b7nem", "Bet\u00b7tel\u00b7staat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}