{"dta.poem.14616": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Zauberin im Walde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eseitdem sa\u00df ich wie gebannt dort,", "tokens": ["\u201e", "seit\u00b7dem", "sa\u00df", "ich", "wie", "ge\u00b7bannt", "dort", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "VVFIN", "PPER", "KOKOM", "ADJD", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wenn neu der Lenz erwachte,", "tokens": ["Und", "wenn", "neu", "der", "Lenz", "er\u00b7wach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer von dem Halsgeschmeide", "tokens": ["Im\u00b7mer", "von", "dem", "Hals\u00b7ge\u00b7schmei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine Perle sie mir brachte.\u201c", "tokens": ["Ei\u00b7ne", "Per\u00b7le", "sie", "mir", "brach\u00b7te", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPER", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}