{"textgrid.poem.33042": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und immer weiter f\u00fchrt dein Weg", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und immer weiter f\u00fchrt dein Weg", "tokens": ["Und", "im\u00b7mer", "wei\u00b7ter", "f\u00fchrt", "dein", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und immer mehr legt sich allm\u00e4hlich alles hinter dich,", "tokens": ["und", "im\u00b7mer", "mehr", "legt", "sich", "all\u00b7m\u00e4h\u00b7lich", "al\u00b7les", "hin\u00b7ter", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PRF", "ADJD", "PIS", "APPR", "PPER", "$,"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "was du in Kindertagen einst", "tokens": ["was", "du", "in", "Kin\u00b7der\u00b7ta\u00b7gen", "einst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mit gl\u00fchendem Wunsch in deine Zukunft tr\u00e4umtest ...", "tokens": ["mit", "gl\u00fc\u00b7hen\u00b7dem", "Wunsch", "in", "dei\u00b7ne", "Zu\u00b7kunft", "tr\u00e4um\u00b7test", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "du h\u00e4ttest bis zum letzten Rest darum gek\u00e4mpft.", "tokens": ["du", "h\u00e4t\u00b7test", "bis", "zum", "letz\u00b7ten", "Rest", "da\u00b7rum", "ge\u00b7k\u00e4mpft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "ADJA", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch wie du weiterschreitest durch die Jahre", "tokens": ["Doch", "wie", "du", "wei\u00b7ter\u00b7schrei\u00b7test", "durch", "die", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "reift eins ums andre dir von selbst entgegen", "tokens": ["reift", "eins", "ums", "and\u00b7re", "dir", "von", "selbst", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPRART", "PIS", "PPER", "APPR", "ADV", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und h\u00e4ngt mit vollen Fr\u00fcchten \u00fcber deinem Weg.", "tokens": ["und", "h\u00e4ngt", "mit", "vol\u00b7len", "Fr\u00fcch\u00b7ten", "\u00fc\u00b7ber", "dei\u00b7nem", "Weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du siehst es, l\u00e4chelst und ... gehst weiter!", "tokens": ["Du", "siehst", "es", ",", "l\u00e4\u00b7chelst", "und", "...", "gehst", "wei\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVFIN", "KON", "$(", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du h\u00e4ttest bis zum letzten Rest darum gek\u00e4mpft", "tokens": ["Du", "h\u00e4t\u00b7test", "bis", "zum", "letz\u00b7ten", "Rest", "da\u00b7rum", "ge\u00b7k\u00e4mpft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "ADJA", "NN", "PAV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "in Kindertagen einst ... und nun", "tokens": ["in", "Kin\u00b7der\u00b7ta\u00b7gen", "einst", "...", "und", "nun"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "ADV", "$(", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "vermissest du es kaum", "tokens": ["ver\u00b7mis\u00b7sest", "du", "es", "kaum"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "im breiten Reichtum des gewonnenen Lebens", "tokens": ["im", "brei\u00b7ten", "Reich\u00b7tum", "des", "ge\u00b7won\u00b7ne\u00b7nen", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "und wunderst dich, es je begehrt zu haben!", "tokens": ["und", "wun\u00b7derst", "dich", ",", "es", "je", "be\u00b7gehrt", "zu", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPER", "ADV", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du l\u00e4chelst und gehst weiter, still und froh ...", "tokens": ["Du", "l\u00e4\u00b7chelst", "und", "gehst", "wei\u00b7ter", ",", "still", "und", "froh", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$,", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und so erf\u00fcllst du dich an deinem Ziel,", "tokens": ["Und", "so", "er\u00b7f\u00fcllst", "du", "dich", "an", "dei\u00b7nem", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "wie sich allm\u00e4hlich so dein Ziel an dir erf\u00fcllt.", "tokens": ["wie", "sich", "all\u00b7m\u00e4h\u00b7lich", "so", "dein", "Ziel", "an", "dir", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADJD", "ADV", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}