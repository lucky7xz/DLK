{"dta.poem.23693": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn B\u00f6hlau.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du Meister mit dem Pinsel!", "tokens": ["Du", "Meis\u00b7ter", "mit", "dem", "Pin\u00b7sel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich seh, du malest Engel;", "tokens": ["Ich", "seh", ",", "du", "ma\u00b7lest", "En\u00b7gel", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du malest ihre K\u00f6pfe,", "tokens": ["Du", "ma\u00b7lest", "ih\u00b7re", "K\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie sch\u00f6ner M\u00e4dchen K\u00f6pfe;", "tokens": ["Wie", "sch\u00f6\u00b7ner", "M\u00e4d\u00b7chen", "K\u00f6p\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du soltest um die H\u00e4lse", "tokens": ["Du", "sol\u00b7test", "um", "die", "H\u00e4l\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auch sch\u00f6ne Busen malen.", "tokens": ["Auch", "sch\u00f6\u00b7ne", "Bu\u00b7sen", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}