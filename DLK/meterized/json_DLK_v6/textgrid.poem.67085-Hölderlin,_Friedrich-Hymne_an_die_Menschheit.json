{"textgrid.poem.67085": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Hymne an die Menschheit", "genre": "verse", "period": "N.A.", "pub_year": 1791, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ernste Stunde hat geschlagen;", "tokens": ["Die", "erns\u00b7te", "Stun\u00b7de", "hat", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz gebeut; erkoren ist die Bahn!", "tokens": ["Mein", "Herz", "ge\u00b7beut", ";", "er\u00b7ko\u00b7ren", "ist", "die", "Bahn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Wolke fleucht, und neue Sterne tagen,", "tokens": ["Die", "Wol\u00b7ke", "fleucht", ",", "und", "neu\u00b7e", "Ster\u00b7ne", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Hesperidenwonne lacht mich an!", "tokens": ["Und", "Hes\u00b7pe\u00b7ri\u00b7den\u00b7won\u00b7ne", "lacht", "mich", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Vertrocknet ist der Liebe stille Z\u00e4hre,", "tokens": ["Ver\u00b7trock\u00b7net", "ist", "der", "Lie\u00b7be", "stil\u00b7le", "Z\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcr dich geweint, mein br\u00fcderlich Geschlecht!", "tokens": ["F\u00fcr", "dich", "ge\u00b7weint", ",", "mein", "br\u00fc\u00b7der\u00b7lich", "Ge\u00b7schlecht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich opfre dir; bei deiner V\u00e4ter Ehre!", "tokens": ["Ich", "opf\u00b7re", "dir", ";", "bei", "dei\u00b7ner", "V\u00e4\u00b7ter", "Eh\u00b7re", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Beim nahen Heil! das Opfer ist gerecht.", "tokens": ["Beim", "na\u00b7hen", "Heil", "!", "das", "Op\u00b7fer", "ist", "ge\u00b7recht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Schon w\u00f6lbt zu reinerem Genusse", "tokens": ["Schon", "w\u00f6lbt", "zu", "rei\u00b7ne\u00b7rem", "Ge\u00b7nus\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Auge sich der Sch\u00f6nheit Heiligtum;", "tokens": ["Dem", "Au\u00b7ge", "sich", "der", "Sch\u00f6n\u00b7heit", "Hei\u00b7lig\u00b7tum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir kosten oft, von ihrem Mutterkusse", "tokens": ["Wir", "kos\u00b7ten", "oft", ",", "von", "ih\u00b7rem", "Mut\u00b7ter\u00b7kus\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gel\u00e4utert und gest\u00e4rkt, Elysium;", "tokens": ["Ge\u00b7l\u00e4u\u00b7tert", "und", "ge\u00b7st\u00e4rkt", ",", "E\u00b7ly\u00b7si\u00b7um", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des Schaffens s\u00fc\u00dfe Lust, wie sie, zu f\u00fchlen,", "tokens": ["Des", "Schaf\u00b7fens", "s\u00fc\u00b7\u00dfe", "Lust", ",", "wie", "sie", ",", "zu", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PWAV", "PPER", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Belauscht sie k\u00fchn der zartgewebte Sinn,", "tokens": ["Be\u00b7lauscht", "sie", "k\u00fchn", "der", "zart\u00b7ge\u00b7web\u00b7te", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und magisch t\u00f6nt von unsern Saitenspielen", "tokens": ["Und", "ma\u00b7gisch", "t\u00f6nt", "von", "un\u00b7sern", "Sai\u00b7ten\u00b7spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Melodie der ernsten Meisterin.", "tokens": ["Die", "Me\u00b7lo\u00b7die", "der", "erns\u00b7ten", "Meis\u00b7te\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Schon lernen wir das Band der Sterne,", "tokens": ["Schon", "ler\u00b7nen", "wir", "das", "Band", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Liebe Stimme m\u00e4nnlicher verstehn,", "tokens": ["Der", "Lie\u00b7be", "Stim\u00b7me", "m\u00e4nn\u00b7li\u00b7cher", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir reichen uns die Bruderrechte gerne,", "tokens": ["Wir", "rei\u00b7chen", "uns", "die", "Bru\u00b7der\u00b7rech\u00b7te", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Heereskraft der Geister Bahn zu gehn;", "tokens": ["Mit", "Hee\u00b7res\u00b7kraft", "der", "Geis\u00b7ter", "Bahn", "zu", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Schon h\u00f6hnen wir des Stolzes Ungeb\u00e4rde,", "tokens": ["Schon", "h\u00f6h\u00b7nen", "wir", "des", "Stol\u00b7zes", "Un\u00b7ge\u00b7b\u00e4r\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Scheidewand, von Flittern aufgebaut,", "tokens": ["Die", "Schei\u00b7de\u00b7wand", ",", "von", "Flit\u00b7tern", "auf\u00b7ge\u00b7baut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und an des Pfl\u00fcgers unentweihtem Herde", "tokens": ["Und", "an", "des", "Pfl\u00fc\u00b7gers", "un\u00b7ent\u00b7weih\u00b7tem", "Her\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wird sich die Menschheit wieder angetraut.", "tokens": ["Wird", "sich", "die", "Menschheit", "wie\u00b7der", "an\u00b7ge\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Schon f\u00fchlen an der Freiheit Fahnen", "tokens": ["Schon", "f\u00fch\u00b7len", "an", "der", "Frei\u00b7heit", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich J\u00fcnglinge, wie G\u00f6tter, gut und gro\u00df,", "tokens": ["Sich", "J\u00fcng\u00b7lin\u00b7ge", ",", "wie", "G\u00f6t\u00b7ter", ",", "gut", "und", "gro\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "$,", "PWAV", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und, ha! die stolzen W\u00fcstlinge zu mahnen,", "tokens": ["Und", ",", "ha", "!", "die", "stol\u00b7zen", "W\u00fcst\u00b7lin\u00b7ge", "zu", "mah\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$.", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bricht jede Kraft von Bann und Kette los;", "tokens": ["Bricht", "je\u00b7de", "Kraft", "von", "Bann", "und", "Ket\u00b7te", "los", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "NE", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Schon schwingt er k\u00fchn und z\u00fcrnend das Gefieder,", "tokens": ["Schon", "schwingt", "er", "k\u00fchn", "und", "z\u00fcr\u00b7nend", "das", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Wahrheit unbesiegter Genius,", "tokens": ["Der", "Wahr\u00b7heit", "un\u00b7be\u00b7sieg\u00b7ter", "Ge\u00b7nius", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Schon tr\u00e4gt der Aar des R\u00e4chers Blitze nieder,", "tokens": ["Schon", "tr\u00e4gt", "der", "Aar", "des", "R\u00e4\u00b7chers", "Blit\u00b7ze", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und donnert laut, und k\u00fcndet Siegsgenu\u00df.", "tokens": ["Und", "don\u00b7nert", "laut", ",", "und", "k\u00fcn\u00b7det", "Siegs\u00b7ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So wahr, von Giften unbetastet,", "tokens": ["So", "wahr", ",", "von", "Gif\u00b7ten", "un\u00b7be\u00b7tas\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Elysens Bl\u00fcte zur Vollendung eilt,", "tokens": ["E\u00b7ly\u00b7sens", "Bl\u00fc\u00b7te", "zur", "Vol\u00b7len\u00b7dung", "eilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Heldinnen, der Sonnen keine rastet,", "tokens": ["Der", "Hel\u00b7din\u00b7nen", ",", "der", "Son\u00b7nen", "kei\u00b7ne", "ras\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PIAT", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und Orellana nicht im Sturze weilt!", "tokens": ["Und", "O\u00b7rel\u00b7la\u00b7na", "nicht", "im", "Stur\u00b7ze", "weilt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PTKNEG", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was unsre Lieb und Siegeskraft begonnen,", "tokens": ["Was", "uns\u00b7re", "Lieb", "und", "Sie\u00b7ges\u00b7kraft", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gedeiht zu \u00fcppiger Vollkommenheit;", "tokens": ["Ge\u00b7deiht", "zu", "\u00fcp\u00b7pi\u00b7ger", "Voll\u00b7kom\u00b7men\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Enkel Heer geneu\u00dft der Ernte Wonnen;", "tokens": ["Der", "En\u00b7kel", "Heer", "ge\u00b7neu\u00dft", "der", "Ern\u00b7te", "Won\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Uns lohnt die Palme der Unsterblichkeit.", "tokens": ["Uns", "lohnt", "die", "Pal\u00b7me", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Hinunter dann mit deinen Taten,", "tokens": ["Hin\u00b7un\u00b7ter", "dann", "mit", "dei\u00b7nen", "Ta\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit deinen Hoffnungen, o Gegenwart!", "tokens": ["Mit", "dei\u00b7nen", "Hoff\u00b7nun\u00b7gen", ",", "o", "Ge\u00b7gen\u00b7wart", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von Schwei\u00df betaut, entkeimten unsre Saaten!", "tokens": ["Von", "Schwei\u00df", "be\u00b7taut", ",", "ent\u00b7keim\u00b7ten", "uns\u00b7re", "Saa\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hinunter dann, wo Ruh der K\u00e4mpfer harrt!", "tokens": ["Hin\u00b7un\u00b7ter", "dann", ",", "wo", "Ruh", "der", "K\u00e4mp\u00b7fer", "harrt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Schon geht verherrlichter aus unsern Gr\u00fcften", "tokens": ["Schon", "geht", "ver\u00b7herr\u00b7lich\u00b7ter", "aus", "un\u00b7sern", "Gr\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Glorie der Endlichkeit hervor;", "tokens": ["Die", "Glo\u00b7rie", "der", "End\u00b7lich\u00b7keit", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Auf Gr\u00e4bern hier Elysium zu stiften,", "tokens": ["Auf", "Gr\u00e4\u00b7bern", "hier", "E\u00b7ly\u00b7si\u00b7um", "zu", "stif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ringt neue Kraft zu G\u00f6ttlichem empor.", "tokens": ["Ringt", "neu\u00b7e", "Kraft", "zu", "G\u00f6tt\u00b7li\u00b7chem", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "In Melodie den Geist zu wiegen,", "tokens": ["In", "Me\u00b7lo\u00b7die", "den", "Geist", "zu", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ert\u00f6net nun der Saite Zauber nur;", "tokens": ["Er\u00b7t\u00f6\u00b7net", "nun", "der", "Sai