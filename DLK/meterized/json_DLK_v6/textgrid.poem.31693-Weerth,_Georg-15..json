{"textgrid.poem.31693": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Turm, der aus dem Dorfe ragt,", "tokens": ["Der", "Turm", ",", "der", "aus", "dem", "Dor\u00b7fe", "ragt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erhebt der Glocken hell Get\u00f6n;", "tokens": ["Er\u00b7hebt", "der", "Glo\u00b7cken", "hell", "Ge\u00b7t\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und neulich hat mir Franz gesagt,", "tokens": ["Und", "neu\u00b7lich", "hat", "mir", "Franz", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "NE", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er sagte mir: ich w\u00e4re sch\u00f6n.", "tokens": ["Er", "sag\u00b7te", "mir", ":", "ich", "w\u00e4\u00b7re", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie das nun kommt, gern w\u00fc\u00dft ich's bald \u2013", "tokens": ["Wie", "das", "nun", "kommt", ",", "gern", "w\u00fc\u00dft", "ich's", "bald", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "PIS", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bin doch erst sechzehn Jahre alt.", "tokens": ["Bin", "doch", "erst", "sech\u00b7zehn", "Jah\u00b7re", "alt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "CARD", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Franz ist ein wilder Junge, traun!", "tokens": ["Franz", "ist", "ein", "wil\u00b7der", "Jun\u00b7ge", ",", "traun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denn gestern in der D\u00e4mmerung,", "tokens": ["Denn", "ge\u00b7stern", "in", "der", "D\u00e4m\u00b7me\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da sprang er \u00fcbern Gartenzaun", "tokens": ["Da", "sprang", "er", "\u00fc\u00b7bern", "Gar\u00b7ten\u00b7zaun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und k\u00fc\u00dfte hurtig meinen Mund.", "tokens": ["Und", "k\u00fc\u00df\u00b7te", "hur\u00b7tig", "mei\u00b7nen", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wie's geschah, ich wei\u00df es nicht \u2013", "tokens": ["Und", "wie's", "ge\u00b7schah", ",", "ich", "wei\u00df", "es", "nicht", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Franz hat ein allerliebst Gesicht.", "tokens": ["Franz", "hat", "ein", "al\u00b7ler\u00b7liebst", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Franz hat mir diesen Ring geschenkt", "tokens": ["Franz", "hat", "mir", "die\u00b7sen", "Ring", "ge\u00b7schenkt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "PDAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dieses Kreuz an rotem Band!", "tokens": ["Und", "die\u00b7ses", "Kreuz", "an", "ro\u00b7tem", "Band", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er hat's mir selber umgeh\u00e4ngt,", "tokens": ["Er", "hat's", "mir", "sel\u00b7ber", "um\u00b7ge\u00b7h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und als ich sinnend vor ihm stand,", "tokens": ["Und", "als", "ich", "sin\u00b7nend", "vor", "ihm", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Viel seltsam Fragen macht' er da,", "tokens": ["Viel", "selt\u00b7sam", "Fra\u00b7gen", "macht'", "er", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ich, ich glaub, ich sagte: Ja!", "tokens": ["Und", "ich", ",", "ich", "glaub", ",", "ich", "sag\u00b7te", ":", "Ja", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$.", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich wei\u00df nicht, was ich ihm gesagt!", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "ich", "ihm", "ge\u00b7sagt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Genug, der Abend war so sch\u00f6n,", "tokens": ["Ge\u00b7nug", ",", "der", "A\u00b7bend", "war", "so", "sch\u00f6n", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Turm, der aus dem Dorfe ragt,", "tokens": ["Der", "Turm", ",", "der", "aus", "dem", "Dor\u00b7fe", "ragt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erhub der Glocken hell Get\u00f6n;", "tokens": ["Er\u00b7hub", "der", "Glo\u00b7cken", "hell", "Ge\u00b7t\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ich, ich ging nach Haus und dacht,", "tokens": ["Und", "ich", ",", "ich", "ging", "nach", "Haus", "und", "dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "APPR", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich dacht an Franz die halbe Nacht.", "tokens": ["Ich", "dacht", "an", "Franz", "die", "hal\u00b7be", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}