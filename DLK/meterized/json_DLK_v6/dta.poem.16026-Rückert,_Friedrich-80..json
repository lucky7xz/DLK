{"dta.poem.16026": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "80.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verstand ist vom Verstehn, Vernunft ist vom Vernehmen;", "tokens": ["Ver\u00b7stand", "ist", "vom", "Ver\u00b7stehn", ",", "Ver\u00b7nunft", "ist", "vom", "Ver\u00b7neh\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "NN", "$,", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die beiden brauchen sich nicht ihres Stamms zu sch\u00e4men.", "tokens": ["Die", "bei\u00b7den", "brau\u00b7chen", "sich", "nicht", "ih\u00b7res", "Stamms", "zu", "sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "PTKNEG", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Verstanden haben zwar ist mehr als blos vernommen,", "tokens": ["Ver\u00b7stan\u00b7den", "ha\u00b7ben", "zwar", "ist", "mehr", "als", "blos", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "VAFIN", "PIS", "KOKOM", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein unverstandenes Vernommnes kann nicht frommen.", "tokens": ["Ein", "un\u00b7ver\u00b7stan\u00b7de\u00b7nes", "Ver\u00b7nomm\u00b7nes", "kann", "nicht", "from\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch kann der Mensch verstehn nur was er recht vernahm,", "tokens": ["Doch", "kann", "der", "Mensch", "ver\u00b7stehn", "nur", "was", "er", "recht", "ver\u00b7nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "VVFIN", "ADV", "PWS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ihm von au\u00dfen her, was ihm von oben kam.", "tokens": ["Was", "ihm", "von", "au\u00b7\u00dfen", "her", ",", "was", "ihm", "von", "o\u00b7ben", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADV", "PTKVZ", "$,", "PWS", "PPER", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}