{"textgrid.poem.35705": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn k\u00fchl der Morgen atmet, gehn", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn k\u00fchl der Morgen atmet, gehn", "tokens": ["Wenn", "k\u00fchl", "der", "Mor\u00b7gen", "at\u00b7met", ",", "gehn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVFIN", "$,", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir schon auf gr\u00fcner Au,", "tokens": ["Wir", "schon", "auf", "gr\u00fc\u00b7ner", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit rotbegl\u00e4nzter Sens', und m\u00e4hn", "tokens": ["Mit", "rot\u00b7be\u00b7gl\u00e4nz\u00b7ter", "Sens'", ",", "und", "m\u00e4hn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wies' im blanken Tau.", "tokens": ["Die", "Wies'", "im", "blan\u00b7ken", "Tau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir M\u00e4her, dalderaldei!", "tokens": ["Wir", "M\u00e4\u00b7her", ",", "dal\u00b7de\u00b7ral\u00b7dei", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Wir m\u00e4hen Blumen und Heu!", "tokens": ["Wir", "m\u00e4\u00b7hen", "Blu\u00b7men", "und", "Heu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Juchhei!", "tokens": ["Juch\u00b7hei", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Die Lerche singt aus blauer Luft,", "tokens": ["Die", "Ler\u00b7che", "singt", "aus", "blau\u00b7er", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Grasem\u00fcck' im Klee,", "tokens": ["Die", "Gra\u00b7se\u00b7m\u00fcck", "im", "Klee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dumpf dazu als Brummba\u00df ruft", "tokens": ["Und", "dumpf", "da\u00b7zu", "als", "Brumm\u00b7ba\u00df", "ruft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PAV", "KOUS", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Rohrdommel fern am See.", "tokens": ["Rohr\u00b7dom\u00b7mel", "fern", "am", "See", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Wir M\u00e4her, dalderaldei!", "tokens": ["Wir", "M\u00e4\u00b7her", ",", "dal\u00b7de\u00b7ral\u00b7dei", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Wir m\u00e4hn in Schwade das Heu!", "tokens": ["Wir", "m\u00e4hn", "in", "Schwa\u00b7de", "das", "Heu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Juchhei!", "tokens": ["Juch\u00b7hei", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Und scheint die liebe Sonne warm,", "tokens": ["Und", "scheint", "die", "lie\u00b7be", "Son\u00b7ne", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann kommt der M\u00e4gdlein Schar,", "tokens": ["Dann", "kommt", "der", "M\u00e4gd\u00b7lein", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Rock gesch\u00fcrzt, mit blo\u00dfem Arm,", "tokens": ["Den", "Rock", "ge\u00b7sch\u00fcrzt", ",", "mit", "blo\u00b7\u00dfem", "Arm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Strohh\u00fct' auf glattem Haar.", "tokens": ["Stroh\u00b7h\u00fct'", "auf", "glat\u00b7tem", "Haar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Die M\u00e4gdlein, dalderaldei!", "tokens": ["Die", "M\u00e4gd\u00b7lein", ",", "dal\u00b7de\u00b7ral\u00b7dei", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Sie harken Blumen und Heu!", "tokens": ["Sie", "har\u00b7ken", "Blu\u00b7men", "und", "Heu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Juchhei!", "tokens": ["Juch\u00b7hei", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "Der Bursch, umweht vom Duft des Heus,", "tokens": ["Der", "Bursch", ",", "um\u00b7weht", "vom", "Duft", "des", "Heus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Winkt oft den M\u00e4gdlein zu,", "tokens": ["Winkt", "oft", "den", "M\u00e4gd\u00b7lein", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und streicht die Sens', und wischt den Schwei\u00df,", "tokens": ["Und", "streicht", "die", "Sens'", ",", "und", "wischt", "den", "Schwei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und seufzt: Ach, harktest du!", "tokens": ["Und", "seufzt", ":", "Ach", ",", "hark\u00b7test", "du", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ITJ", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die M\u00e4gdlein, dalderaldei!", "tokens": ["Die", "M\u00e4gd\u00b7lein", ",", "dal\u00b7de\u00b7ral\u00b7dei", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Sie h\u00e4ufen Schober von Heu!", "tokens": ["Sie", "h\u00e4u\u00b7fen", "Scho\u00b7ber", "von", "Heu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Juchhei!", "tokens": ["Juch\u00b7hei", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.5": {"line.1": {"text": "Ist weit hinab die Wiese kahl,", "tokens": ["Ist", "weit", "hin\u00b7ab", "die", "Wie\u00b7se", "kahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann lagern wir uns frisch", "tokens": ["Dann", "la\u00b7gern", "wir", "uns", "frisch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In bunter Reih zum frohen Mahl,", "tokens": ["In", "bun\u00b7ter", "Reih", "zum", "fro\u00b7hen", "Mahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am bl\u00fch'nden Dorngeb\u00fcsch.", "tokens": ["Am", "bl\u00fch'n\u00b7den", "Dorn\u00b7ge\u00b7b\u00fcsch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die M\u00e4gdlein, dalderaldei!", "tokens": ["Die", "M\u00e4gd\u00b7lein", ",", "dal\u00b7de\u00b7ral\u00b7dei", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Ruhn gern selbander im Heu!", "tokens": ["Ruhn", "gern", "sel\u00b7ban\u00b7der", "im", "Heu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Juchhei!", "tokens": ["Juch\u00b7hei", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.6": {"line.1": {"text": "Bepackt wird dann der Wagen ganz,", "tokens": ["Be\u00b7packt", "wird", "dann", "der", "Wa\u00b7gen", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Achs' und Leiter knackt;", "tokens": ["Da\u00df", "Achs'", "und", "Lei\u00b7ter", "knackt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die sch\u00f6nste Dirn' im Blumenkranz", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Dirn'", "im", "Blu\u00b7men\u00b7kranz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird oben drauf gepackt.", "tokens": ["Wird", "o\u00b7ben", "drauf", "ge\u00b7packt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hell kreischt sie, dalderaldei!", "tokens": ["Hell", "kreischt", "sie", ",", "dal\u00b7de\u00b7ral\u00b7dei", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PAV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Gewiegt von duftendem Heu!", "tokens": ["Ge\u00b7wiegt", "von", "duf\u00b7ten\u00b7dem", "Heu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Juchhei!", "tokens": ["Juch\u00b7hei", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.7": {"line.1": {"text": "Zur Bodenluk' hereingebracht", "tokens": ["Zur", "Bo\u00b7den\u00b7luk'", "her\u00b7ein\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird dann die Last des Heus,", "tokens": ["Wird", "dann", "die", "Last", "des", "Heus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und brav gesch\u00e4kert und gelacht;", "tokens": ["Und", "brav", "ge\u00b7sch\u00e4\u00b7kert", "und", "ge\u00b7lacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn Sch\u00e4kern spornt den Flei\u00df.", "tokens": ["Denn", "Sch\u00e4\u00b7kern", "spornt", "den", "Flei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Am Giebel, dalderaldei!", "tokens": ["Am", "Gie\u00b7bel", ",", "dal\u00b7de\u00b7ral\u00b7dei", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Stehn wir, und rasseln im Heu!", "tokens": ["Stehn", "wir", ",", "und", "ras\u00b7seln", "im", "Heu", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Juchhei!", "tokens": ["Juch\u00b7hei", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.8": {"line.1": {"text": "Zuletzt beim Schmaus' und Reigen t\u00f6nt", "tokens": ["Zu\u00b7letzt", "beim", "Schmaus'", "und", "Rei\u00b7gen", "t\u00f6nt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schalmein- und Fiedelklang:", "tokens": ["Schal\u00b7mein", "und", "Fie\u00b7del\u00b7klang", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da tanzt man, da\u00df der Boden dr\u00f6hnt", "tokens": ["Da", "tanzt", "man", ",", "da\u00df", "der", "Bo\u00b7den", "dr\u00f6hnt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den ganzen Abend lang;", "tokens": ["Den", "gan\u00b7zen", "A\u00b7bend", "lang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und schl\u00e4ft dann, dalderaldei!", "tokens": ["Und", "schl\u00e4ft", "dann", ",", "dal\u00b7de\u00b7ral\u00b7dei", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Wir Bursche schlafen im Heu!", "tokens": ["Wir", "Bur\u00b7sche", "schla\u00b7fen", "im", "Heu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Juchhei!", "tokens": ["Juch\u00b7hei", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}}}}