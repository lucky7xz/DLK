{"textgrid.poem.37731": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: W\u00e4r mir's an die Stirn geschrieben", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r mir's an die Stirn geschrieben", "tokens": ["W\u00e4r", "mir's", "an", "die", "Stirn", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NE", "APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4r ich nimmer hier geblieben,", "tokens": ["W\u00e4r", "ich", "nim\u00b7mer", "hier", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4r's am Aug mir abzusehen", "tokens": ["W\u00e4r's", "am", "Aug", "mir", "ab\u00b7zu\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "PPER", "VVIZU"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcrde ich in Angst vergehen,", "tokens": ["W\u00fcr\u00b7de", "ich", "in", "Angst", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6nnte er ins Herz mir sehen,", "tokens": ["K\u00f6nn\u00b7te", "er", "ins", "Herz", "mir", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00fcrd es lange stille stehen!", "tokens": ["W\u00fcrd", "es", "lan\u00b7ge", "stil\u00b7le", "ste\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sey gepriesen blinde Nacht,", "tokens": ["Sey", "ge\u00b7prie\u00b7sen", "blin\u00b7de", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo ich ruhig sein gedacht,", "tokens": ["Wo", "ich", "ru\u00b7hig", "sein", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VAINF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Sey gepriesen dunkler Schleier", "tokens": ["Sey", "ge\u00b7prie\u00b7sen", "dunk\u00b7ler", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "In die Welt seh ich nun freyer,", "tokens": ["In", "die", "Welt", "seh", "ich", "nun", "frey\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Sey gepriesen edle Kraft,", "tokens": ["Sey", "ge\u00b7prie\u00b7sen", "ed\u00b7le", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Die im Schlafe bildend schafft,", "tokens": ["Die", "im", "Schla\u00b7fe", "bil\u00b7dend", "schafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Ja der Herr verl\u00e4sst doch keinen,", "tokens": ["Ja", "der", "Herr", "ver\u00b7l\u00e4sst", "doch", "kei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "VVFIN", "ADV", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Giebt's im Schlafe all den Seinen.", "tokens": ["Giebt's", "im", "Schla\u00b7fe", "all", "den", "Sei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PIAT", "ART", "PPOSS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}