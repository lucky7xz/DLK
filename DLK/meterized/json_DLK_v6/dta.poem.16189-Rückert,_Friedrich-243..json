{"dta.poem.16189": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "243.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Zukunft habet ihr, ihr habt das Vaterland,", "tokens": ["Die", "Zu\u00b7kunft", "ha\u00b7bet", "ihr", ",", "ihr", "habt", "das", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr habt der Jugend Herz, Erzieher, in der Hand.", "tokens": ["Ihr", "habt", "der", "Ju\u00b7gend", "Herz", ",", "Er\u00b7zie\u00b7her", ",", "in", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was ihr dem lockern Grund einpflanzt, wird Wurzel schlagen;", "tokens": ["Was", "ihr", "dem", "lo\u00b7ckern", "Grund", "ein\u00b7pflanzt", ",", "wird", "Wur\u00b7zel", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ihr dem zarten Zweig einimpft, wird Fr\u00fcchte tragen.", "tokens": ["Was", "ihr", "dem", "zar\u00b7ten", "Zweig", "ein\u00b7impft", ",", "wird", "Fr\u00fcch\u00b7te", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bedenkt, da\u00df sie zum Heil der Welt das werden sollen,", "tokens": ["Be\u00b7denkt", ",", "da\u00df", "sie", "zum", "Heil", "der", "Welt", "das", "wer\u00b7den", "sol\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "APPRART", "NN", "ART", "NN", "ART", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was wir geworden nicht, und haben werden wollen.", "tokens": ["Was", "wir", "ge\u00b7wor\u00b7den", "nicht", ",", "und", "ha\u00b7ben", "wer\u00b7den", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAPP", "PTKNEG", "$,", "KON", "VAFIN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}