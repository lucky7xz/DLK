{"textgrid.poem.64293": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "11. An einen Wegweiser", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freundlicher Greis, wie du den Weg mich lehrtest,", "tokens": ["Freund\u00b7li\u00b7cher", "Greis", ",", "wie", "du", "den", "Weg", "mich", "lehr\u00b7test", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "PPER", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Also leite dich Gott zu jenen H\u00fctten,", "tokens": ["Al\u00b7so", "lei\u00b7te", "dich", "Gott", "zu", "je\u00b7nen", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Deren Weg der kl\u00fcgelnde Weise sp\u00e4t und", "tokens": ["De\u00b7ren", "Weg", "der", "kl\u00fc\u00b7geln\u00b7de", "Wei\u00b7se", "sp\u00e4t", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "ART", "ADJA", "NN", "ADJD", "KON"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Selten erforschet!", "tokens": ["Sel\u00b7ten", "er\u00b7for\u00b7schet", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Einfalt und Liebe sprach dein sanftes Auge.", "tokens": ["Ein\u00b7falt", "und", "Lie\u00b7be", "sprach", "dein", "sanf\u00b7tes", "Au\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Einfalt f\u00fchret auch dorthin! Bruderliebe", "tokens": ["Ein\u00b7falt", "f\u00fch\u00b7ret", "auch", "dor\u00b7thin", "!", "Bru\u00b7der\u00b7lie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "ADV", "ADV", "$.", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "S\u00fchnt des Schwachen Irrungen! Seinen Fehlen", "tokens": ["S\u00fchnt", "des", "Schwa\u00b7chen", "Ir\u00b7run\u00b7gen", "!", "Sei\u00b7nen", "Feh\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Donnert kein Richter!", "tokens": ["Don\u00b7nert", "kein", "Rich\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}