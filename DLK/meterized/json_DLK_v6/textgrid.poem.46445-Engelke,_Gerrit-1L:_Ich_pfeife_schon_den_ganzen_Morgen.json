{"textgrid.poem.46445": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich pfeife schon den ganzen Morgen", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich pfeife schon den ganzen Morgen", "tokens": ["Ich", "pfei\u00b7fe", "schon", "den", "gan\u00b7zen", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und d\u00f6se f\u00fcr mich hin.", "tokens": ["Und", "d\u00f6\u00b7se", "f\u00fcr", "mich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Sonne ist in Regenlust verborgen \u2013", "tokens": ["Die", "Son\u00b7ne", "ist", "in", "Re\u00b7gen\u00b7lust", "ver\u00b7bor\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch irgendwas erheitert meinen Sinn.", "tokens": ["Doch", "ir\u00b7gend\u00b7was", "er\u00b7hei\u00b7tert", "mei\u00b7nen", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Menschen sehn heute anders aus,", "tokens": ["Die", "Men\u00b7schen", "sehn", "heu\u00b7te", "an\u00b7ders", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Zeitungsm\u00e4dchen h\u00fcpft so niedlich, \u2013", "tokens": ["Das", "Zei\u00b7tungs\u00b7m\u00e4d\u00b7chen", "h\u00fcpft", "so", "nied\u00b7lich", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die lange Stra\u00dfe, Haus an Haus,", "tokens": ["Die", "lan\u00b7ge", "Stra\u00b7\u00dfe", ",", "Haus", "an", "Haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So regengrau \u2013 und schl\u00e4fert doch so friedlich.", "tokens": ["So", "re\u00b7gen\u00b7grau", "\u2013", "und", "schl\u00e4\u00b7fert", "doch", "so", "fried\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "KON", "VVFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was gestern hier l\u00e4rmte, roh und fuselkehlig,", "tokens": ["Was", "ge\u00b7stern", "hier", "l\u00e4rm\u00b7te", ",", "roh", "und", "fu\u00b7sel\u00b7keh\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Das ist heute alles stumm. \u2013", "tokens": ["Das", "ist", "heu\u00b7te", "al\u00b7les", "stumm", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PIS", "ADJD", "$.", "$("], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Wie bin ich heute selig \u2013", "tokens": ["Wie", "bin", "ich", "heu\u00b7te", "se\u00b7lig", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wei\u00df doch nicht warum \u2013", "tokens": ["Und", "wei\u00df", "doch", "nicht", "wa\u00b7rum", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "PWAV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr lieben Leute, ich schalt euch: unausstehlich,", "tokens": ["Ihr", "lie\u00b7ben", "Leu\u00b7te", ",", "ich", "schalt", "euch", ":", "un\u00b7aus\u00b7steh\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "$.", "ADJD", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Fluchte manchmal, schalt euch: schlecht und dumm,", "tokens": ["Fluch\u00b7te", "manch\u00b7mal", ",", "schalt", "euch", ":", "schlecht", "und", "dumm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "VVFIN", "PPER", "$.", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Vergebt mir heute, ich bin so selig", "tokens": ["Ver\u00b7gebt", "mir", "heu\u00b7te", ",", "ich", "bin", "so", "se\u00b7lig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "ADV", "ADJD"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wei\u00df doch nicht warum.", "tokens": ["Und", "wei\u00df", "doch", "nicht", "wa\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "PWAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}