{"dta.poem.14018": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Spaziergang .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ho da! Kartoffeln und ihr \u00f6konomische Knollengew\u00e4chse,", "tokens": ["Ho", "da", "!", "Kar\u00b7tof\u00b7feln", "und", "ihr", "\u00f6\u00b7kon\u00b7o\u00b7mi\u00b7sche", "Knol\u00b7len\u00b7ge\u00b7w\u00e4ch\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$.", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Schreiten kaum kann man; gemach! macht euch nicht", "tokens": ["Schrei\u00b7ten", "kaum", "kann", "man", ";", "ge\u00b7mach", "!", "macht", "euch", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "VMFIN", "PIS", "$.", "ADV", "$.", "VVFIN", "PPER", "PTKNEG"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "gar zu sehr breit!", "tokens": ["gar", "zu", "sehr", "breit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADV", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}