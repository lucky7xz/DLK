{"textgrid.poem.63398": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "L\u00fcnk un G\u00e4lg\u00f6sch", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jung, smit mi nich mit Steen un Str\u00fcnk", "tokens": ["Jung", ",", "smit", "mi", "nich", "mit", "Steen", "un", "Str\u00fcnk"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "NE", "PTKNEG", "APPR", "NN", "FM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "hier v\u00f6r min Sch\u00fcnd\u00f6r up de L\u00fcnk", "tokens": ["hier", "v\u00f6r", "min", "Sch\u00fcn\u00b7d\u00f6r", "up", "de", "L\u00fcnk"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NE", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "un up de l\u00fctt G\u00e4lg\u00f6sch!", "tokens": ["un", "up", "de", "l\u00fctt", "G\u00e4l\u00b7g\u00f6sch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wat du f\u00f6r'n ollen Racker b\u00fcst!", "tokens": ["Wat", "du", "f\u00f6r'n", "ol\u00b7len", "Ra\u00b7cker", "b\u00fcst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "S\u00fchst nich, wat de arm V\u0153gel fr\u00fcst?", "tokens": ["S\u00fchst", "nich", ",", "wat", "de", "arm", "V\u0153\u00b7gel", "fr\u00fcst", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "$,", "PRELS", "NE", "ADJD", "NN", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "De s\u00fcnd ok Blot un Fleesch.", "tokens": ["De", "s\u00fcnd", "ok", "Blot", "un", "Fleesch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du hest to Hus de Sch\u00f6ttels vull,", "tokens": ["Du", "hest", "to", "Hus", "de", "Sch\u00f6t\u00b7tels", "vull", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NE", "NE", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "du frettst Dag in Dag ut f\u00f6r dull", "tokens": ["du", "frettst", "Dag", "in", "Dag", "ut", "f\u00f6r", "dull"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "un premst di vull din Mag' \u2013", "tokens": ["un", "premst", "di", "vull", "din", "Mag'", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "G\u00e4lg\u00f6sch un L\u00fcnk, wat hungert de!", "tokens": ["G\u00e4l\u00b7g\u00f6sch", "un", "L\u00fcnk", ",", "wat", "hun\u00b7gert", "de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "FM", "NN", "$,", "PWS", "VVFIN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ehr Disch liggt \u00fcnner Is un Snee,", "tokens": ["Ehr", "Disch", "liggt", "\u00fcn\u00b7ner", "Is", "un", "Snee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "dat geiht ehr gor to schrag.", "tokens": ["dat", "geiht", "ehr", "gor", "to", "schrag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Ehr Tit is hart, ehr Winter d\u00fcr,", "tokens": ["Ehr", "Tit", "is", "hart", ",", "ehr", "Win\u00b7ter", "d\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "KOUS", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "dat geiht ehr as de Pracherl\u00fcr,", "tokens": ["dat", "geiht", "ehr", "as", "de", "Pra\u00b7cher\u00b7l\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "glik vunne Hand to M\u00fcnd.", "tokens": ["glik", "vun\u00b7ne", "Hand", "to", "M\u00fcnd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Un wenn een Weitkurn vun de D\u00e4l", "tokens": ["Un", "wenn", "e\u00b7en", "Weit\u00b7kurn", "vun", "de", "D\u00e4l"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "ART", "NN", "APPR", "NE", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "ok rut eens fl\u00fcggt, dat is nich v\u00e4l,", "tokens": ["ok", "rut", "e\u00b7ens", "fl\u00fcggt", ",", "dat", "is", "nich", "v\u00e4l", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "un dat is girn ehr g\u00fcnnt.", "tokens": ["un", "dat", "is", "girn", "ehr", "g\u00fcnnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Kumm herl Da, nimm di\u00df Hand vull Ohr'n", "tokens": ["Kumm", "herl", "Da", ",", "nimm", "di\u00df", "Hand", "vull", "Ohr'n"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ADV", "$,", "VVIMP", "PDS", "NN", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "un dr\u00e4g s' ehr achter in'n Gor'n", "tokens": ["un", "dr\u00e4g", "s'", "ehr", "ach\u00b7ter", "in'n", "Gor'n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "un w\u00e4s doch eenmal klok!", "tokens": ["un", "w\u00e4s", "doch", "e\u00b7en\u00b7mal", "klok", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "ADV", "ADV", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Dat is 'ne slichte Wirtschaft man,", "tokens": ["Dat", "is", "'ne", "slich\u00b7te", "Wirt\u00b7schaft", "man", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "ART", "ADJA", "NN", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "de K\u00e4uh un Schap d\u00f6rchfodern kann", "tokens": ["de", "K\u00e4uh", "un", "Schap", "d\u00f6r\u00b7ch\u00b7fo\u00b7dern", "kann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "NN", "VVINF", "VMFIN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "un nich ehr L\u00fcnkens ok.", "tokens": ["un", "nich", "ehr", "L\u00fcn\u00b7kens", "ok", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "PTKNEG", "NN", "NE", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}