{"textgrid.poem.56867": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00dcber der Erde", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber der Erde", "tokens": ["\u00dc\u00b7ber", "der", "Er\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "erhabne Rundung,", "tokens": ["er\u00b7hab\u00b7ne", "Run\u00b7dung", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "hart dem Tag", "tokens": ["hart", "dem", "Tag"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "auf fliehender Ferse,", "tokens": ["auf", "flie\u00b7hen\u00b7der", "Fer\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "rollende R\u00e4der", "tokens": ["rol\u00b7len\u00b7de", "R\u00e4\u00b7der"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "rauscht mit mir!", "tokens": ["rauscht", "mit", "mir", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Vom Horizonte", "tokens": ["Vom", "Ho\u00b7ri\u00b7zon\u00b7te"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "sch\u00fcrzen Gew\u00f6lke sich", "tokens": ["sch\u00fcr\u00b7zen", "Ge\u00b7w\u00f6l\u00b7ke", "sich"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "PRF"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "n\u00e4chtlich herauf", "tokens": ["n\u00e4cht\u00b7lich", "her\u00b7auf"], "token_info": ["word", "word"], "pos": ["ADJD", "PTKVZ"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "und schw\u00e4rzen seltsam", "tokens": ["und", "schw\u00e4r\u00b7zen", "selt\u00b7sam"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "ein formlos Gesicht", "tokens": ["ein", "form\u00b7los", "Ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "in den stahlhellen Himmel.", "tokens": ["in", "den", "stahl\u00b7hel\u00b7len", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Ein breiter Stierkopf,", "tokens": ["Ein", "brei\u00b7ter", "Stier\u00b7kopf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "tr\u00e4g und t\u00fcckisch,", "tokens": ["tr\u00e4g", "und", "t\u00fc\u00b7ckisch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "w\u00e4chst und schwillt es", "tokens": ["w\u00e4chst", "und", "schwillt", "es"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "\u00fcber die Welt ...", "tokens": ["\u00fc\u00b7ber", "die", "Welt", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "dehnt und verzerrt sich ...", "tokens": ["dehnt", "und", "ver\u00b7zerrt", "sich", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PRF", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "verschwimmt in Nacht ...", "tokens": ["ver\u00b7schwimmt", "in", "Nacht", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Rollet, raset", "tokens": ["Rol\u00b7let", ",", "ra\u00b7set"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "den R\u00fccken der Erde,", "tokens": ["den", "R\u00fc\u00b7cken", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "R\u00e4der, empor!", "tokens": ["R\u00e4\u00b7der", ",", "em\u00b7por", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Des Tages Gewandsaum", "tokens": ["Des", "Ta\u00b7ges", "Ge\u00b7wand\u00b7saum"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "m\u00f6cht ich noch ", "tokens": ["m\u00f6cht", "ich", "noch"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "inbr\u00fcnstig k\u00fcssen!", "tokens": ["in\u00b7br\u00fcns\u00b7tig", "k\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}