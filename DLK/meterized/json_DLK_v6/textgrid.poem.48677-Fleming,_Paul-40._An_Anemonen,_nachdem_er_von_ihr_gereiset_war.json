{"textgrid.poem.48677": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "40. An Anemonen, nachdem er von ihr gereiset war", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach einig di\u00df war \u00fcbrig noch", "tokens": ["Ach", "ei\u00b7nig", "di\u00df", "war", "\u00fcb\u00b7rig", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADJD", "PDS", "VAFIN", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "von allen meinen Plagen,", "tokens": ["von", "al\u00b7len", "mei\u00b7nen", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "da\u00df ich das schwere Liebesjoch", "tokens": ["da\u00df", "ich", "das", "schwe\u00b7re", "Lie\u00b7bes\u00b7joch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mu\u00df abgeschieden tragen.", "tokens": ["mu\u00df", "ab\u00b7ge\u00b7schie\u00b7den", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die mir das gr\u00f6\u00dfte Leiden tut,", "tokens": ["Die", "mir", "das", "gr\u00f6\u00df\u00b7te", "Lei\u00b7den", "tut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die tr\u00f6stet meine Sinnen.", "tokens": ["die", "tr\u00f6s\u00b7tet", "mei\u00b7ne", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich brenn und meines Brandes Glut", "tokens": ["Ich", "brenn", "und", "mei\u00b7nes", "Bran\u00b7des", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "ist ach! wie weit von hinnen!", "tokens": ["ist", "ach", "!", "wie", "weit", "von", "hin\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$.", "PWAV", "ADJD", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nicht gl\u00e4ub' ich, da\u00df die letzte Not", "tokens": ["Nicht", "gl\u00e4ub'", "ich", ",", "da\u00df", "die", "letz\u00b7te", "Not"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mir gr\u00f6\u00dfre Qual kan machen.", "tokens": ["mir", "gr\u00f6\u00df\u00b7re", "Qual", "kan", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "An mir lebt nichts nicht als der Tod,", "tokens": ["An", "mir", "lebt", "nichts", "nicht", "als", "der", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIS", "PTKNEG", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "der stark ist in mir Schwachen.", "tokens": ["der", "stark", "ist", "in", "mir", "Schwa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das kranke Herze windet sich,", "tokens": ["Das", "kran\u00b7ke", "Her\u00b7ze", "win\u00b7det", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die matten Augen brechen.", "tokens": ["die", "mat\u00b7ten", "Au\u00b7gen", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nichts denk' ich, Liebste, denn an dich,", "tokens": ["Nichts", "denk'", "ich", ",", "Liebs\u00b7te", ",", "denn", "an", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "NN", "$,", "KON", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "doch kan mein Mund nichts sprechen.", "tokens": ["doch", "kan", "mein", "Mund", "nichts", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nach dir zu warten ist umsonst,", "tokens": ["Nach", "dir", "zu", "war\u00b7ten", "ist", "um\u00b7sonst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "o \u00c4rztin meiner Seelen.", "tokens": ["o", "\u00c4rz\u00b7tin", "mei\u00b7ner", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin zu weit von dieser Gunst,", "tokens": ["Ich", "bin", "zu", "weit", "von", "die\u00b7ser", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ich mu\u00df mich nur so qu\u00e4len.", "tokens": ["ich", "mu\u00df", "mich", "nur", "so", "qu\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch freu' ich mich bei h\u00f6chster Pein", "tokens": ["Doch", "freu'", "ich", "mich", "bei", "h\u00f6chs\u00b7ter", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und setze di\u00df entgegen,", "tokens": ["und", "set\u00b7ze", "di\u00df", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "mu\u00df ich gleich der Betr\u00fcbtste sein,", "tokens": ["mu\u00df", "ich", "gleich", "der", "Be\u00b7tr\u00b7\u00fcbts\u00b7te", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VAINF", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "es ist der Werten wegen.", "tokens": ["es", "ist", "der", "Wer\u00b7ten", "we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ach, ", "tokens": ["Ach", ","], "token_info": ["word", "punct"], "pos": ["ITJ", "$,"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "bleib unverwant im Herzen.", "tokens": ["bleib", "un\u00b7ver\u00b7want", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich tu dasselbe, wie du tust,", "tokens": ["Ich", "tu", "das\u00b7sel\u00b7be", ",", "wie", "du", "tust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und f\u00fchle gleiche Schmerzen.", "tokens": ["und", "f\u00fch\u00b7le", "glei\u00b7che", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ists wahr, da\u00df alle Fr\u00f6lichkeit", "tokens": ["Ists", "wahr", ",", "da\u00df", "al\u00b7le", "Fr\u00f6\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "KOUS", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "wird s\u00fc\u00dfer nach dem Leiden,", "tokens": ["wird", "s\u00fc\u00b7\u00dfer", "nach", "dem", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "so schicke, Schatz, dich in die Zeit.", "tokens": ["so", "schi\u00b7cke", ",", "Schatz", ",", "dich", "in", "die", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "NN", "$,", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wir sehen uns mit Freuden!", "tokens": ["Wir", "se\u00b7hen", "uns", "mit", "Freu\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}