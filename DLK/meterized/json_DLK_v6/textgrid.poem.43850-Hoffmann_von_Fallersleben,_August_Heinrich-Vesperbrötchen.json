{"textgrid.poem.43850": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vesperbr\u00f6tchen", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vesperbr\u00f6tchen, stell' dich ein,", "tokens": ["Ves\u00b7per\u00b7br\u00f6t\u00b7chen", ",", "stell'", "dich", "ein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber komm nur nicht zu klein!", "tokens": ["A\u00b7ber", "komm", "nur", "nicht", "zu", "klein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn das Spitzchen und das Sp\u00e4tzchen", "tokens": ["Denn", "das", "Spitz\u00b7chen", "und", "das", "Sp\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das liebe Mausek\u00e4tzchen,", "tokens": ["Und", "das", "lie\u00b7be", "Mau\u00b7se\u00b7k\u00e4tz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle, alle warten dein:", "tokens": ["Al\u00b7le", ",", "al\u00b7le", "war\u00b7ten", "dein", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VVFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vesperbr\u00f6tchen, stell' dich ein!", "tokens": ["Ves\u00b7per\u00b7br\u00f6t\u00b7chen", ",", "stell'", "dich", "ein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mutter, eben schl\u00e4gt es vier,", "tokens": ["Mut\u00b7ter", ",", "e\u00b7ben", "schl\u00e4gt", "es", "vier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "CARD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und um viere vespern wir.", "tokens": ["Und", "um", "vie\u00b7re", "ves\u00b7pern", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wartet Spitzchen auch und Sp\u00e4tzchen", "tokens": ["War\u00b7tet", "Spitz\u00b7chen", "auch", "und", "Sp\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "ADV", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das liebe Mausek\u00e4tzchen,", "tokens": ["Und", "das", "lie\u00b7be", "Mau\u00b7se\u00b7k\u00e4tz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Warten kann dein Franz nicht mehr,", "tokens": ["War\u00b7ten", "kann", "dein", "Franz", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn ihn hungert gar zu sehr.", "tokens": ["Denn", "ihn", "hun\u00b7gert", "gar", "zu", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PTKA", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}