{"textgrid.poem.43504": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlings Bewillkommnung", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So sei gegr\u00fc\u00dft viel tausendmal,", "tokens": ["So", "sei", "ge\u00b7gr\u00fc\u00dft", "viel", "tau\u00b7send\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Holder Fr\u00fchling!", "tokens": ["Hol\u00b7der", "Fr\u00fch\u00b7ling", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Willkommen hier in unserm Thal,", "tokens": ["Will\u00b7kom\u00b7men", "hier", "in", "un\u00b7serm", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Holder Fr\u00fchling!", "tokens": ["Hol\u00b7der", "Fr\u00fch\u00b7ling", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Holder Fr\u00fchling, \u00fcberall", "tokens": ["Hol\u00b7der", "Fr\u00fch\u00b7ling", ",", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "NE", "$,", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gr\u00fc\u00dfen wir dich froh mit Sang und Schall,", "tokens": ["Gr\u00fc\u00b7\u00dfen", "wir", "dich", "froh", "mit", "Sang", "und", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Mit Sang und Schall.", "tokens": ["Mit", "Sang", "und", "Schall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Du kommst, und froh ist alle Welt,", "tokens": ["Du", "kommst", ",", "und", "froh", "ist", "al\u00b7le", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADJD", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Holder Fr\u00fchling!", "tokens": ["Hol\u00b7der", "Fr\u00fch\u00b7ling", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Es freut sich Wiese, Wald und Feld,", "tokens": ["Es", "freut", "sich", "Wie\u00b7se", ",", "Wald", "und", "Feld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Holder Fr\u00fchling!", "tokens": ["Hol\u00b7der", "Fr\u00fch\u00b7ling", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Jubel t\u00f6nt dir \u00fcberall,", "tokens": ["Ju\u00b7bel", "t\u00f6nt", "dir", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dich begr\u00fc\u00dfet Lerch' und Nachtigall,", "tokens": ["Dich", "be\u00b7gr\u00fc\u00b7\u00dfet", "Ler\u00b7ch'", "und", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Und Nachtigall.", "tokens": ["Und", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "So sei gegr\u00fc\u00dft viel tausendmal,", "tokens": ["So", "sei", "ge\u00b7gr\u00fc\u00dft", "viel", "tau\u00b7send\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Holder Fr\u00fchling!", "tokens": ["Hol\u00b7der", "Fr\u00fch\u00b7ling", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "O bleib recht lang in unserm Thal,", "tokens": ["O", "bleib", "recht", "lang", "in", "un\u00b7serm", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Holder Fr\u00fchling!", "tokens": ["Hol\u00b7der", "Fr\u00fch\u00b7ling", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Kehr in alle Herzen ein,", "tokens": ["Kehr", "in", "al\u00b7le", "Her\u00b7zen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "La\u00df doch alle mit uns fr\u00f6hlich sein,", "tokens": ["La\u00df", "doch", "al\u00b7le", "mit", "uns", "fr\u00f6h\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PIS", "APPR", "PPER", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Ja fr\u00f6hlich sein!", "tokens": ["Ja", "fr\u00f6h\u00b7lich", "sein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}