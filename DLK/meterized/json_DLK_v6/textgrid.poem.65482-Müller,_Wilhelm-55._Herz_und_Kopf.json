{"textgrid.poem.65482": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "55. Herz und Kopf", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In des Busens enger Zelle wohnt das Herz, als Eremit,", "tokens": ["In", "des", "Bu\u00b7sens", "en\u00b7ger", "Zel\u00b7le", "wohnt", "das", "Herz", ",", "als", "E\u00b7re\u00b7mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "ART", "NN", "$,", "KOUS", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Aug' und Ohren ihm erz\u00e4hlen, was es selbst nicht h\u00f6rt und sieht.", "tokens": ["Aug'", "und", "Oh\u00b7ren", "ihm", "er\u00b7z\u00e4h\u00b7len", ",", "was", "es", "selbst", "nicht", "h\u00f6rt", "und", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VVINF", "$,", "PWS", "PPER", "ADV", "PTKNEG", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Gl\u00e4ubig ist's und leicht zu t\u00e4uschen, lauscht auch einem M\u00e4hrchen gern.", "tokens": ["Gl\u00e4u\u00b7big", "ist's", "und", "leicht", "zu", "t\u00e4u\u00b7schen", ",", "lauscht", "auch", "ei\u00b7nem", "M\u00e4hr\u00b7chen", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "KON", "ADJD", "PTKZU", "VVINF", "$,", "VVFIN", "ADV", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Stets bereit zu Klag' und Jubel, aber sch\u00fcchtern vor dem Herrn,", "tokens": ["Stets", "be\u00b7reit", "zu", "Klag'", "und", "Ju\u00b7bel", ",", "a\u00b7ber", "sch\u00fcch\u00b7tern", "vor", "dem", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "NN", "$,", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Der mit finsterm Stirnerunzeln oft ein dummes Kind es schilt,", "tokens": ["Der", "mit", "fins\u00b7term", "Stir\u00b7ne\u00b7run\u00b7zeln", "oft", "ein", "dum\u00b7mes", "Kind", "es", "schilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ADV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Und als Wacht im Kopfe thronet mit der Wahrheit blankem Schild.", "tokens": ["Und", "als", "Wacht", "im", "Kop\u00b7fe", "thro\u00b7net", "mit", "der", "Wahr\u00b7heit", "blan\u00b7kem", "Schild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "APPRART", "NN", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Aber an den bunten Bildern, die ihm zeigt der Sinne Spiel,", "tokens": ["A\u00b7ber", "an", "den", "bun\u00b7ten", "Bil\u00b7dern", ",", "die", "ihm", "zeigt", "der", "Sin\u00b7ne", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Kann das Herz nicht satt sich sehen, nach dem Schilde blickt's nicht viel.", "tokens": ["Kann", "das", "Herz", "nicht", "satt", "sich", "se\u00b7hen", ",", "nach", "dem", "Schil\u00b7de", "blickt's", "nicht", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "ADJD", "PRF", "VVINF", "$,", "APPR", "ART", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}