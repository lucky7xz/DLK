{"dta.poem.7263": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die Jagd.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Die Luft hat schlafen sich gelegt,", "tokens": ["Die", "Luft", "hat", "schla\u00b7fen", "sich", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Behaglich in das Moos gestreckt,", "tokens": ["Be\u00b7hag\u00b7lich", "in", "das", "Moos", "ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Rispeln, das die Kr\u00e4uter regt,", "tokens": ["Kein", "Ris\u00b7peln", ",", "das", "die", "Kr\u00e4u\u00b7ter", "regt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Seufzer, der die Halme weckt.", "tokens": ["Kein", "Seuf\u00b7zer", ",", "der", "die", "Hal\u00b7me", "weckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur eine Wolke tr\u00e4umt mitunter", "tokens": ["Nur", "ei\u00b7ne", "Wol\u00b7ke", "tr\u00e4umt", "mi\u00b7tun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Am blassen Horizont hinunter,", "tokens": ["Am", "blas\u00b7sen", "Ho\u00b7ri\u00b7zont", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dort, wo das Tannicht \u00fcber'm Wall", "tokens": ["Dort", ",", "wo", "das", "Tan\u00b7nicht", "\u00fc\u00b7ber'm", "Wall"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die dunkeln Candelabern streckt.", "tokens": ["Die", "dun\u00b7keln", "Can\u00b7de\u00b7la\u00b7bern", "streckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da horch, ein Ruf, ein ferner Schall:", "tokens": ["Da", "horch", ",", "ein", "Ruf", ",", "ein", "fer\u00b7ner", "Schall", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "\u201ehalloh! hoho!\u201c so lang gezogen,", "tokens": ["\u201e", "hal\u00b7loh", "!", "ho\u00b7ho", "!", "\u201c", "so", "lang", "ge\u00b7zo\u00b7gen", ","], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "ITJ", "$.", "$(", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Man meint, die Kl\u00e4nge schlagen Wogen", "tokens": ["Man", "meint", ",", "die", "Kl\u00e4n\u00b7ge", "schla\u00b7gen", "Wo\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Im Ginsterfeld, und wieder dort:", "tokens": ["Im", "Gins\u00b7ter\u00b7feld", ",", "und", "wie\u00b7der", "dort", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "\u201ehalloh! hoho!\u201c \u2014 am Dickicht fort", "tokens": ["\u201e", "hal\u00b7loh", "!", "ho\u00b7ho", "!", "\u201c", "am", "Di\u00b7ckicht", "fort"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "$.", "ITJ", "$.", "$(", "$(", "APPRART", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Ein z\u00f6gernd Echo, \u2014 alles still!", "tokens": ["Ein", "z\u00f6\u00b7gernd", "E\u00b7cho", ",", "al\u00b7les", "still", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "$(", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Man h\u00f6rt der Fliege Angstgeschrill", "tokens": ["Man", "h\u00f6rt", "der", "Flie\u00b7ge", "Angst\u00b7ge\u00b7schrill"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Im Mettennetz, den Fall der Beere,", "tokens": ["Im", "Met\u00b7ten\u00b7netz", ",", "den", "Fall", "der", "Bee\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Man h\u00f6rt im Kraut des K\u00e4fers Gang,", "tokens": ["Man", "h\u00f6rt", "im", "Kraut", "des", "K\u00e4\u00b7fers", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und dann wie zieh'nder Kranichheere", "tokens": ["Und", "dann", "wie", "zieh'n\u00b7der", "Kra\u00b7nich\u00b7hee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Kling klang! von ihrer luft'gen F\u00e4hre,", "tokens": ["Kling", "klang", "!", "von", "ih\u00b7rer", "luft'\u00b7gen", "F\u00e4h\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Wie ferner Unkenruf: Kling! klang!", "tokens": ["Wie", "fer\u00b7ner", "Un\u00b7ken\u00b7ruf", ":", "Kling", "!", "klang", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "$.", "NN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Ein L\u00e4uten das Gew\u00e4ld entlang,", "tokens": ["Ein", "L\u00e4u\u00b7ten", "das", "Ge\u00b7w\u00e4ld", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Hui schl\u00fcpft der Fuchs den Wall hinab", "tokens": ["Hui", "schl\u00fcpft", "der", "Fuchs", "den", "Wall", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "VVFIN", "ART", "NE", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Er gleitet durch die Binsenspeere,", "tokens": ["Er", "glei\u00b7tet", "durch", "die", "Bin\u00b7sen\u00b7spee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Und zuckelt f\u00fcrder seinen Trab:", "tokens": ["Und", "zu\u00b7ckelt", "f\u00fcr\u00b7der", "sei\u00b7nen", "Trab", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Und aus dem Dickicht, wei\u00df wie Flocken,", "tokens": ["Und", "aus", "dem", "Di\u00b7ckicht", ",", "wei\u00df", "wie", "Flo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Nach st\u00e4uben die lebend'gen Glocken,", "tokens": ["Nach", "st\u00e4u\u00b7ben", "die", "le\u00b7ben\u00b7d'\u00b7gen", "Glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Radschlagend an des Dammes Hang;", "tokens": ["Rad\u00b7schla\u00b7gend", "an", "des", "Dam\u00b7mes", "Hang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.28": {"text": "Wie Aale schnellen sie vom Grund,", "tokens": ["Wie", "Aa\u00b7le", "schnel\u00b7len", "sie", "vom", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Und weiter, weiter, Fuchs und Hund.", "tokens": ["Und", "wei\u00b7ter", ",", "wei\u00b7ter", ",", "Fuchs", "und", "Hund", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "$,", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Der schwankende Wachholder fl\u00fcstert,", "tokens": ["Der", "schwan\u00b7ken\u00b7de", "Wach\u00b7hol\u00b7der", "fl\u00fcs\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.31": {"text": "Die Binse rauscht, die Haide knistert,", "tokens": ["Die", "Bin\u00b7se", "rauscht", ",", "die", "Hai\u00b7de", "knis\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Und st\u00e4ubt Phal\u00e4nen um die Meute.", "tokens": ["Und", "st\u00e4ubt", "Pha\u00b7l\u00e4\u00b7nen", "um", "die", "Meu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Sie jappen, klaffen nach der Beute,", "tokens": ["Sie", "jap\u00b7pen", ",", "klaf\u00b7fen", "nach", "der", "Beu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Schaumflocken spr\u00fchn aus Nas' und Mund;", "tokens": ["Schaum\u00b7flo\u00b7cken", "spr\u00fchn", "aus", "Nas'", "und", "Mund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NE", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.35": {"text": "Noch hat der Fuchs die rechte Weite,", "tokens": ["Noch", "hat", "der", "Fuchs", "die", "rech\u00b7te", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NE", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Gelassen trabt er, schleppt den Schweif,", "tokens": ["Ge\u00b7las\u00b7sen", "trabt", "er", ",", "schleppt", "den", "Schweif", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Zieht in dem Thaue dunklen Streif,", "tokens": ["Zieht", "in", "dem", "Thau\u00b7e", "dunk\u00b7len", "Streif", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.38": {"text": "Und zeigt ver\u00e4chtlich seine Socken.", "tokens": ["Und", "zeigt", "ver\u00b7\u00e4cht\u00b7lich", "sei\u00b7ne", "So\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Doch bald hebt er die Lunte frisch,", "tokens": ["Doch", "bald", "hebt", "er", "die", "Lun\u00b7te", "frisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.40": {"text": "Und, wie im Weiher schnellt der Fisch,", "tokens": ["Und", ",", "wie", "im", "Wei\u00b7her", "schnellt", "der", "Fisch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Fort setzt er \u00fcber Kraut und Schmehlen,", "tokens": ["Fort", "setzt", "er", "\u00fc\u00b7ber", "Kraut", "und", "Schmeh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Wirft mit den L\u00e4ufen Kies und Staub;", "tokens": ["Wirft", "mit", "den", "L\u00e4u\u00b7fen", "Kies", "und", "Staub", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NE", "KON", "NN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.43": {"text": "Die Meute mit geschwoll'nen Kehlen", "tokens": ["Die", "Meu\u00b7te", "mit", "ge\u00b7schwoll'\u00b7nen", "Keh\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Ihm nach wie rasselnd Winterlaub.", "tokens": ["Ihm", "nach", "wie", "ras\u00b7selnd", "Win\u00b7ter\u00b7laub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "KOKOM", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Man h\u00f6ret ihre Kiefern knacken,", "tokens": ["Man", "h\u00f6\u00b7ret", "ih\u00b7re", "Kie\u00b7fern", "kna\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Wenn fletschend in die Luft sie hacken;", "tokens": ["Wenn", "flet\u00b7schend", "in", "die", "Luft", "sie", "ha\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "In weitem Kreise so zum Tann,", "tokens": ["In", "wei\u00b7tem", "Krei\u00b7se", "so", "zum", "Tann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Und wieder aus dem Dickicht dann", "tokens": ["Und", "wie\u00b7der", "aus", "dem", "Di\u00b7ckicht", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Ert\u00f6nt das Glockenspiel der Bracken.", "tokens": ["Er\u00b7t\u00f6nt", "das", "Glo\u00b7cken\u00b7spiel", "der", "Bra\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was bricht dort im Gestrippe am Revier?", "tokens": ["Was", "bricht", "dort", "im", "Ge\u00b7strip\u00b7pe", "am", "Re\u00b7vier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im holprichten Galopp stampft es den Grund;", "tokens": ["Im", "hol\u00b7prich\u00b7ten", "Ga\u00b7lopp", "stampft", "es", "den", "Grund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ha! br\u00fcllend Heerdenvieh! voran der Stier,", "tokens": ["Ha", "!", "br\u00fcl\u00b7lend", "Heer\u00b7den\u00b7vieh", "!", "vo\u00b7ran", "der", "Stier", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJD", "NN", "$.", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ihnen nach klafft ein versprengter Hund.", "tok