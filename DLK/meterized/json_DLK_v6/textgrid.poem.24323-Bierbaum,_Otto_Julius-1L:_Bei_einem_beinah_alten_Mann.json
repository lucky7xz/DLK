{"textgrid.poem.24323": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bei einem beinah alten Mann", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei einem beinah alten Mann", "tokens": ["Bei", "ei\u00b7nem", "bei\u00b7nah", "al\u00b7ten", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Meldete sich klein Amor an", "tokens": ["Mel\u00b7de\u00b7te", "sich", "klein", "A\u00b7mor", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "NE", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "(ein M\u00e4dchen wars in einer Hosenrolle).", "tokens": ["(", "ein", "M\u00e4d\u00b7chen", "wars", "in", "ei\u00b7ner", "Ho\u00b7sen\u00b7rol\u00b7le", ")", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der \u00dcberraschte fragte, was er wolle.", "tokens": ["Der", "\u00dc\u00b7berr\u00b7aschte", "frag\u00b7te", ",", "was", "er", "wol\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "\u00bbdich pr\u00fcfen will ich,\u00ab sprach das liebe Ding", "tokens": ["\u00bb", "dich", "pr\u00fc\u00b7fen", "will", "ich", ",", "\u00ab", "sprach", "das", "lie\u00b7be", "Ding"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVINF", "VMFIN", "PPER", "$,", "$(", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "(halb Gassenbub, halb Schmetterling),", "tokens": ["(", "halb", "Gas\u00b7sen\u00b7bub", ",", "halb", "Schmet\u00b7ter\u00b7ling", ")", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "NN", "$,", "ADJD", "NN", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbob du noch brennen kannst\u00ab und k\u00fc\u00dft ihn so,", "tokens": ["\u00bb", "ob", "du", "noch", "bren\u00b7nen", "kannst", "\u00ab", "und", "k\u00fc\u00dft", "ihn", "so", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$(", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df augenblicks er Feuer fing.", "tokens": ["Da\u00df", "au\u00b7gen\u00b7blicks", "er", "Feu\u00b7er", "fing", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dar\u00fcber war der Mann nat\u00fcrlich froh.", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "war", "der", "Mann", "na\u00b7t\u00fcr\u00b7lich", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Denn allzulange war er wie ein Besen,", "tokens": ["Denn", "all\u00b7zu\u00b7lan\u00b7ge", "war", "er", "wie", "ein", "Be\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Zwar d\u00fcrr, doch ohne Glut gewesen.", "tokens": ["Zwar", "d\u00fcrr", ",", "doch", "oh\u00b7ne", "Glut", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "APPR", "NN", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Wie aber dann der Kleine wieder ging,", "tokens": ["Wie", "a\u00b7ber", "dann", "der", "Klei\u00b7ne", "wie\u00b7der", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "ADJA", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Da trat herein zur T\u00fcre gro\u00df", "tokens": ["Da", "trat", "her\u00b7ein", "zur", "T\u00fc\u00b7re", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Madam Vernunft, setzt schwer sich auf den Scho\u00df", "tokens": ["Ma\u00b7dam", "Ver\u00b7nunft", ",", "setzt", "schwer", "sich", "auf", "den", "Scho\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "VVFIN", "ADJD", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Noch warm von Amors Hinterteilchen", "tokens": ["Noch", "warm", "von", "A\u00b7mors", "Hin\u00b7ter\u00b7teil\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und sprach: Herr Lichterloh, glaubt nicht dem M\u00e4del,", "tokens": ["Und", "sprach", ":", "Herr", "Lich\u00b7ter\u00b7loh", ",", "glaubt", "nicht", "dem", "M\u00e4\u00b7del", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NN", "NN", "$,", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Das jetzt zu Euch in Amors Maske kam", "tokens": ["Das", "jetzt", "zu", "Euch", "in", "A\u00b7mors", "Mas\u00b7ke", "kam"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "PPER", "APPR", "NE", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und augenblicks Besitz von Euerm Sch\u00e4del,", "tokens": ["Und", "au\u00b7gen\u00b7blicks", "Be\u00b7sitz", "von", "Eu\u00b7erm", "Sch\u00e4\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Von Euerm Torensch\u00e4del nahm,", "tokens": ["Von", "Eu\u00b7erm", "To\u00b7ren\u00b7sch\u00e4\u00b7del", "nahm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Denn es vertrieb sich blo\u00df ein Langeweilchen.", "tokens": ["Denn", "es", "ver\u00b7trieb", "sich", "blo\u00df", "ein", "Lan\u00b7ge\u00b7weil\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Da bot der Mann Madam Vernunft den Arm", "tokens": ["Da", "bot", "der", "Mann", "Ma\u00b7dam", "Ver\u00b7nunft", "den", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und f\u00fchrte sie zur T\u00fcr und sprach: Au revoir,", "tokens": ["Und", "f\u00fchr\u00b7te", "sie", "zur", "T\u00fcr", "und", "sprach", ":", "Au", "re\u00b7voir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "KON", "VVFIN", "$.", "NN", "NE", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Ihr sprecht wahrscheinlich wie gew\u00f6hnlich wahr,", "tokens": ["Ihr", "sprecht", "wahr\u00b7schein\u00b7lich", "wie", "ge\u00b7w\u00f6hn\u00b7lich", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KOKOM", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Doch allzuk\u00fchle, und ich bin von Herzen", "tokens": ["Doch", "all\u00b7zu\u00b7k\u00fch\u00b7le", ",", "und", "ich", "bin", "von", "Her\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KON", "PPER", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Froh, da\u00df mir endlich wieder einmal warm", "tokens": ["Froh", ",", "da\u00df", "mir", "end\u00b7lich", "wie\u00b7der", "ein\u00b7mal", "warm"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "PPER", "ADV", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Zumute ist. Der Liebe helle Kerzen", "tokens": ["Zu\u00b7mu\u00b7te", "ist", ".", "Der", "Lie\u00b7be", "hel\u00b7le", "Ker\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$.", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "L\u00f6sch ich nicht aus. Wer wei\u00df, wie bald ein Wind", "tokens": ["L\u00f6sch", "ich", "nicht", "aus", ".", "Wer", "wei\u00df", ",", "wie", "bald", "ein", "Wind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PTKNEG", "PTKVZ", "$.", "PWS", "VVFIN", "$,", "PWAV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Sie niederweht und ich im Finstern tr\u00e4ume", "tokens": ["Sie", "nie\u00b7der\u00b7weht", "und", "ich", "im", "Fins\u00b7tern", "tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Von hellen Kerzen, die erloschen sind.", "tokens": ["Von", "hel\u00b7len", "Ker\u00b7zen", ",", "die", "er\u00b7lo\u00b7schen", "sind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}