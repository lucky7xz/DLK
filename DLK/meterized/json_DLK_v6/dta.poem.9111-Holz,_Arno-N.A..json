{"dta.poem.9111": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Am andern Morgen ist der Biela krank.", "tokens": ["Am", "an\u00b7dern", "Mor\u00b7gen", "ist", "der", "Bie\u00b7la", "krank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der arme Biela!", "tokens": ["Der", "ar\u00b7me", "Bie\u00b7la", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Er sitzt vergn\u00fcgt im Bett und pappt Kuchen.", "tokens": ["Er", "sitzt", "ver\u00b7gn\u00fcgt", "im", "Bett", "und", "pappt", "Ku\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Sein Bruder", "tokens": ["Sein", "Bru\u00b7der"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "spielt.", "tokens": ["spielt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "\u201edu \u2014 Mutter?", "tokens": ["\u201e", "du", "Mut\u00b7ter", "?"], "token_info": ["punct", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "$(", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Mein B\u00e4r is auch krank!\u201c", "tokens": ["Mein", "B\u00e4r", "is", "auch", "krank", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "FM", "ADV", "ADJD", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "So?", "tokens": ["So", "?"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Na was fehlt ihm denn?", "tokens": ["Na", "was", "fehlt", "ihm", "denn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "\u201ena, den hat doch n Schmetterling gebissen?\u201c", "tokens": ["\u201e", "na", ",", "den", "hat", "doch", "n", "Schmet\u00b7ter\u00b7ling", "ge\u00b7bis\u00b7sen", "?", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "ART", "VAFIN", "ADV", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}