{"textgrid.poem.54512": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel hat bi\u00dfher sich auf die Erd erhitzt", "tokens": ["Der", "Him\u00b7mel", "hat", "bi\u00df\u00b7her", "sich", "auf", "die", "Erd", "er\u00b7hitzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit viel Schwefel/ Feur und Flammen lichten krachen/", "tokens": ["Und", "mit", "viel", "Schwe\u00b7fel", "/", "Feur", "und", "Flam\u00b7men", "lich\u00b7ten", "kra\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "$(", "NN", "KON", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gelodert und getobt; Die Erd hat aus dem Rachen", "tokens": ["Ge\u00b7lo\u00b7dert", "und", "ge\u00b7tobt", ";", "Die", "Erd", "hat", "aus", "dem", "Ra\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "VVPP", "$.", "ART", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Abgrunds auf die Luft und Sternen selbst geblitzt:", "tokens": ["Des", "Ab\u00b7grunds", "auf", "die", "Luft", "und", "Ster\u00b7nen", "selbst", "ge\u00b7blitzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nicht einer der das Schlo\u00df des grossen Throns besitzt", "tokens": ["Nicht", "ei\u00b7ner", "der", "das", "Schlo\u00df", "des", "gros\u00b7sen", "Throns", "be\u00b7sitzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ART", "ART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hat iemahls sich ins Feld der Erden wollen machen/", "tokens": ["Hat", "ie\u00b7mahls", "sich", "ins", "Feld", "der", "Er\u00b7den", "wol\u00b7len", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PRF", "APPRART", "NN", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nicht einer der allhier mu\u00df auf der Erden wachen", "tokens": ["Nicht", "ei\u00b7ner", "der", "all\u00b7hier", "mu\u00df", "auf", "der", "Er\u00b7den", "wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ART", "ADV", "VMFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hat auf den g\u00fclden Bau des Himmels sich gespitzt.", "tokens": ["Hat", "auf", "den", "g\u00fcl\u00b7den", "Bau", "des", "Him\u00b7mels", "sich", "ge\u00b7spitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nunmehr wil Gott Friede machen und schl\u00e4gt edle Mittel vor/", "tokens": ["Nun\u00b7mehr", "wil", "Gott", "Frie\u00b7de", "ma\u00b7chen", "und", "schl\u00e4gt", "ed\u00b7le", "Mit\u00b7tel", "vor", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "NN", "VVINF", "KON", "VVFIN", "ADJA", "NN", "PTKVZ", "$("], "meter": "+-+-+-+-+++-+-+", "measure": "unknown.measure.octa.plus"}, "line.10": {"text": "Er wird selbst ein Mensch gebohren und entschleust des Himmels-Thor", "tokens": ["Er", "wird", "selbst", "ein", "Mensch", "ge\u00b7boh\u00b7ren", "und", "ent\u00b7schleust", "des", "Him\u00b7mels\u00b7Thor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VVPP", "KON", "VVFIN", "ART", "NN"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "Was haben wir f\u00fcr Ursach mehr zu zancken?", "tokens": ["Was", "ha\u00b7ben", "wir", "f\u00fcr", "Ur\u00b7sach", "mehr", "zu", "zan\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Gott und Engel steigen nieder; Schwache Menschen fahren auf", "tokens": ["Gott", "und", "En\u00b7gel", "stei\u00b7gen", "nie\u00b7der", ";", "Schwa\u00b7che", "Men\u00b7schen", "fah\u00b7ren", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "NE", "VVFIN", "PTKVZ", "$.", "ADJA", "NN", "VVFIN", "APPR"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Nunmehr wird eins Erd und Himmel/ Engel/ Menschen sind ein Hauff.", "tokens": ["Nun\u00b7mehr", "wird", "eins", "Erd", "und", "Him\u00b7mel", "/", "En\u00b7gel", "/", "Men\u00b7schen", "sind", "ein", "Hauff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "CARD", "NN", "KON", "NN", "$(", "NE", "$(", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Wer wundert sich da\u00df Mensch und Engel dancken.", "tokens": ["Wer", "wun\u00b7dert", "sich", "da\u00df", "Mensch", "und", "En\u00b7gel", "dan\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "KOUS", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}