{"textgrid.poem.45193": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[fahr fort, o F\u00fcrst, in deinem hohen Amt]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fahr fort, o F\u00fcrst, in deinem hohen Amt,", "tokens": ["Fahr", "fort", ",", "o", "F\u00fcrst", ",", "in", "dei\u00b7nem", "ho\u00b7hen", "Amt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "FM", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Um das der Ehrgeiz selbst dich nicht beneidet,", "tokens": ["Um", "das", "der", "Ehr\u00b7geiz", "selbst", "dich", "nicht", "be\u00b7nei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "NN", "ADV", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das rettet nur, indems zugleich verdammt,", "tokens": ["Das", "ret\u00b7tet", "nur", ",", "in\u00b7dems", "zu\u00b7gleich", "ver\u00b7dammt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie einst der j\u00fcngste Tag die Seelen scheidet.", "tokens": ["Wie", "einst", "der", "j\u00fcngs\u00b7te", "Tag", "die", "See\u00b7len", "schei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gesetzt in eine Zeit von blutgem Wahn,", "tokens": ["Ge\u00b7setzt", "in", "ei\u00b7ne", "Zeit", "von", "blut\u00b7gem", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die reife glaubt, was F\u00e4ulnis bis zur Mitten,", "tokens": ["Die", "rei\u00b7fe", "glaubt", ",", "was", "F\u00e4ul\u00b7nis", "bis", "zur", "Mit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "PWS", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Fortschritt, wenn nicht weiter in der Bahn,", "tokens": ["Und", "Fort\u00b7schritt", ",", "wenn", "nicht", "wei\u00b7ter", "in", "der", "Bahn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KOUS", "PTKNEG", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn seitw\u00e4rts aus der Bahn sie fortgeschritten.", "tokens": ["Wenn", "seit\u00b7w\u00e4rts", "aus", "der", "Bahn", "sie", "fort\u00b7ge\u00b7schrit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}