{"dta.poem.14634": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Riesen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und bei dem L\u00e4rm sie stunden", "tokens": ["Und", "bei", "dem", "L\u00e4rm", "sie", "stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Wolken bis an die Knie,", "tokens": ["In", "Wol\u00b7ken", "bis", "an", "die", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das Schlo\u00df, das war verschwunden,", "tokens": ["Das", "Schlo\u00df", ",", "das", "war", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wu\u00dften gar nicht wie. \u2014", "tokens": ["Und", "wu\u00df\u00b7ten", "gar", "nicht", "wie", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "PWAV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}