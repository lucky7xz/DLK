{"textgrid.poem.26587": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schelte man doch nicht den Dichter,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schelte man doch nicht den Dichter,", "tokens": ["Schel\u00b7te", "man", "doch", "nicht", "den", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn auch ", "tokens": ["Wenn", "auch"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und wie anderes Gelichter", "tokens": ["Und", "wie", "an\u00b7de\u00b7res", "Ge\u00b7lich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus des Lebens Pf\u00fctze trinkt.", "tokens": ["Aus", "des", "Le\u00b7bens", "Pf\u00fct\u00b7ze", "trinkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Reiner nur in Gegens\u00e4tzen,", "tokens": ["Rei\u00b7ner", "nur", "in", "Ge\u00b7gen\u00b7s\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heller t\u00f6nt empor sein Lied;", "tokens": ["Hel\u00b7ler", "t\u00f6nt", "em\u00b7por", "sein", "Lied", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmer wei\u00df das Licht zu sch\u00e4tzen,", "tokens": ["Nim\u00b7mer", "wei\u00df", "das", "Licht", "zu", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer das Dunkel stets vermied.", "tokens": ["Wer", "das", "Dun\u00b7kel", "stets", "ver\u00b7mied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie ihn auch sein Wipfel kr\u00f6ne,", "tokens": ["Wie", "ihn", "auch", "sein", "Wip\u00b7fel", "kr\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wurzelt doch in Nacht der Stamm \u2013", "tokens": ["Wur\u00b7zelt", "doch", "in", "Nacht", "der", "Stamm", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Lilie keusche Sch\u00f6ne", "tokens": ["Und", "der", "Li\u00b7lie", "keu\u00b7sche", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Bl\u00fchet aus des Teiches Schlamm!", "tokens": ["Bl\u00fc\u00b7het", "aus", "des", "Tei\u00b7ches", "Schlamm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}