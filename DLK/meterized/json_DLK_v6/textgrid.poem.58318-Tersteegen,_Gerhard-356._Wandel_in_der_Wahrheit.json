{"textgrid.poem.58318": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "356. Wandel in der Wahrheit", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer in dem Licht der Wahrheit lebt,", "tokens": ["Wer", "in", "dem", "Licht", "der", "Wahr\u00b7heit", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gott innig liebt und hoch erhebt", "tokens": ["Gott", "in\u00b7nig", "liebt", "und", "hoch", "er\u00b7hebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "VVFIN", "KON", "ADJD", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und stets sich selbst verachtet,", "tokens": ["Und", "stets", "sich", "selbst", "ver\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der beugt sich unter jedermann,", "tokens": ["Der", "beugt", "sich", "un\u00b7ter", "je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nimmt Kreuz und Leiden willig an,", "tokens": ["Nimmt", "Kreuz", "und", "Lei\u00b7den", "wil\u00b7lig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil er sein Nichts betrachtet.", "tokens": ["Weil", "er", "sein", "Nichts", "be\u00b7trach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}