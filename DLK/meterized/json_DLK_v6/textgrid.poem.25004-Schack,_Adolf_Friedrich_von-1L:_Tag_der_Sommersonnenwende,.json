{"textgrid.poem.25004": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tag der Sommersonnenwende,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tag der Sommersonnenwende,", "tokens": ["Tag", "der", "Som\u00b7mer\u00b7son\u00b7nen\u00b7wen\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6nster in der Br\u00fcder Schar,", "tokens": ["Sch\u00f6ns\u00b7ter", "in", "der", "Br\u00fc\u00b7der", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seines Segens reichste Spende", "tokens": ["Sei\u00b7nes", "Se\u00b7gens", "reichs\u00b7te", "Spen\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4uft durch dich auf uns das Jahr.", "tokens": ["H\u00e4uft", "durch", "dich", "auf", "uns", "das", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "APPR", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alle deine goldnen Stunden", "tokens": ["Al\u00b7le", "dei\u00b7ne", "gold\u00b7nen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu genie\u00dfen voll und ganz,", "tokens": ["Zu", "ge\u00b7nie\u00b7\u00dfen", "voll", "und", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "KON", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00fch dem Schlummer schon entwunden", "tokens": ["Fr\u00fch", "dem", "Schlum\u00b7mer", "schon", "ent\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hab' ich mich beim Sternenglanz.", "tokens": ["Hab'", "ich", "mich", "beim", "Ster\u00b7nen\u00b7glanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sah die D\u00e4mmernebel brechen,", "tokens": ["Sah", "die", "D\u00e4m\u00b7mer\u00b7ne\u00b7bel", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als sein Thor der Ost erschlo\u00df,", "tokens": ["Als", "sein", "Thor", "der", "Ost", "er\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dein Licht in Flammenb\u00e4chen", "tokens": ["Und", "dein", "Licht", "in", "Flam\u00b7men\u00b7b\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf die Erde niederflo\u00df;", "tokens": ["Auf", "die", "Er\u00b7de", "nie\u00b7der\u00b7flo\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sah, wie sie in durst'gen Z\u00fcgen", "tokens": ["Sah", ",", "wie", "sie", "in", "dur\u00b7st'\u00b7gen", "Z\u00fc\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schl\u00fcrfte von dem reinen Trank,", "tokens": ["Schl\u00fcrf\u00b7te", "von", "dem", "rei\u00b7nen", "Trank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis in seligem Gen\u00fcgen", "tokens": ["Bis", "in", "se\u00b7li\u00b7gem", "Ge\u00b7n\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie in Mittagstr\u00e4ume sank.", "tokens": ["Sie", "in", "Mit\u00b7tags\u00b7tr\u00e4u\u00b7me", "sank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hoch mit dir am Himmelsbogen", "tokens": ["Hoch", "mit", "dir", "am", "Him\u00b7mels\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist auf deiner lichten Bahn", "tokens": ["Ist", "auf", "dei\u00b7ner", "lich\u00b7ten", "Bahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Seele hingezogen", "tokens": ["Mei\u00b7ne", "See\u00b7le", "hin\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber Berg und Ocean.", "tokens": ["Ue\u00b7ber", "Berg", "und", "O\u00b7cean", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Und in sich, bis tief, tiefinnen", "tokens": ["Und", "in", "sich", ",", "bis", "tief", ",", "tie\u00b7fin\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "PRF", "$,", "KOUS", "ADJD", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie ges\u00e4ttigt war von Glut,", "tokens": ["Sie", "ge\u00b7s\u00e4t\u00b7tigt", "war", "von", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VAFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lie\u00df in vollem Strom sie rinnen", "tokens": ["Lie\u00df", "in", "vol\u00b7lem", "Strom", "sie", "rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner Strahlen heil'ge Flut.", "tokens": ["Dei\u00b7ner", "Strah\u00b7len", "heil'\u00b7ge", "Flut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Noch im Sinken lange, lange", "tokens": ["Noch", "im", "Sin\u00b7ken", "lan\u00b7ge", ",", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "APPRART", "NN", "ADV", "$,", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leuchtetest du, goldner Tag;", "tokens": ["Leuch\u00b7te\u00b7test", "du", ",", "gold\u00b7ner", "Tag", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lang noch nach dem Untergange", "tokens": ["Lang", "noch", "nach", "dem", "Un\u00b7ter\u00b7gan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00fche mir im Herzen nach!", "tokens": ["Gl\u00fc\u00b7he", "mir", "im", "Her\u00b7zen", "nach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}