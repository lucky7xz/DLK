{"textgrid.poem.43794": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rathe, wer da rathen kann!", "tokens": ["Ra\u00b7the", ",", "wer", "da", "ra\u00b7then", "kann", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sagt, wer sieht es dann und wann?", "tokens": ["Sagt", ",", "wer", "sieht", "es", "dann", "und", "wann", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "VVFIN", "PPER", "ADV", "KON", "PWAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sagt, wer sieht es immer?", "tokens": ["Sagt", ",", "wer", "sieht", "es", "im\u00b7mer", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sagt, wer sieht es nimmer?", "tokens": ["Sagt", ",", "wer", "sieht", "es", "nim\u00b7mer", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Jeder, der es hat errathen,", "tokens": ["Je\u00b7der", ",", "der", "es", "hat", "er\u00b7ra\u00b7then", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Strecke die Hand aus!", "tokens": ["Stre\u00b7cke", "die", "Hand", "aus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}