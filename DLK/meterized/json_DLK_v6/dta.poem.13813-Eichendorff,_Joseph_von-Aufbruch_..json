{"dta.poem.13813": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Aufbruch .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ereiter sind's, die blitzend ziehen,", "tokens": ["\u201e", "rei\u00b7ter", "sin\u00b7d's", ",", "die", "blit\u00b7zend", "zie\u00b7hen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wie viel gl\u00e4nz'ger Str\u00f6me Lauf,", "tokens": ["Wie", "viel", "gl\u00e4nz'\u00b7ger", "Str\u00f6\u00b7me", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Fahnen, Liliengleich, erbl\u00fchen,", "tokens": ["Fah\u00b7nen", ",", "Li\u00b7li\u00b7en\u00b7gleich", ",", "er\u00b7bl\u00fc\u00b7hen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Lerchenwirbel, Trommelwirbel", "tokens": ["Ler\u00b7chen\u00b7wir\u00b7bel", ",", "Trom\u00b7mel\u00b7wir\u00b7bel"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Wecken rings den Fr\u00fchling auf.\u201c", "tokens": ["We\u00b7cken", "rings", "den", "Fr\u00fch\u00b7ling", "auf", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}