{"textgrid.poem.44621": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Apologie f\u00fcr manche Autoren (nach dem Franz\u00f6sischen)", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr scheltet, da\u00df der reiche Dichter Krum,", "tokens": ["Ihr", "schel\u00b7tet", ",", "da\u00df", "der", "rei\u00b7che", "Dich\u00b7ter", "Krum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn er sein Machwerk r\u00fchmt, mit fremdem Gute prahle!", "tokens": ["Wenn", "er", "sein", "Mach\u00b7werk", "r\u00fchmt", ",", "mit", "frem\u00b7dem", "Gu\u00b7te", "prah\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie falsch! Ist denn das nicht mein Eigentum,", "tokens": ["Wie", "falsch", "!", "Ist", "denn", "das", "nicht", "mein", "Ei\u00b7gen\u00b7tum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$.", "VAFIN", "KON", "PDS", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was ich mit meinem Geld bezahle?", "tokens": ["Was", "ich", "mit", "mei\u00b7nem", "Geld", "be\u00b7zah\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}