{"textgrid.poem.41138": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die fliegende Todesb\u00f6tin schau',", "tokens": ["Die", "flie\u00b7gen\u00b7de", "To\u00b7des\u00b7b\u00f6\u00b7tin", "schau'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein schlimmes Gespenst wie die wei\u00dfe Frau;", "tokens": ["Ein", "schlim\u00b7mes", "Ge\u00b7spenst", "wie", "die", "wei\u00b7\u00dfe", "Frau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wenn solche nachts flieget in ein Haus,", "tokens": ["Wenn", "sol\u00b7che", "nachts", "flie\u00b7get", "in", "ein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "An das Fensterglas legt wie Gl\u00fchwurms Schein", "tokens": ["An", "das", "Fens\u00b7ter\u00b7glas", "legt", "wie", "Gl\u00fch\u00b7wurms", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "KOKOM", "NN", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Den Kopf, da\u00df er leuchtet ins Zimmer hinein,", "tokens": ["Den", "Kopf", ",", "da\u00df", "er", "leuch\u00b7tet", "ins", "Zim\u00b7mer", "hin\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "So tr\u00e4gt man da eines bald tot hinaus.", "tokens": ["So", "tr\u00e4gt", "man", "da", "ei\u00b7nes", "bald", "tot", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}