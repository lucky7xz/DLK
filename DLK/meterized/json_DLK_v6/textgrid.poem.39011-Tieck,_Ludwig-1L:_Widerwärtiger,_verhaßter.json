{"textgrid.poem.39011": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Widerw\u00e4rtiger, verha\u00dfter", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Widerw\u00e4rtiger, verha\u00dfter", "tokens": ["Wi\u00b7der\u00b7w\u00e4r\u00b7ti\u00b7ger", ",", "ver\u00b7ha\u00df\u00b7ter"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als stechende Fliegen und Ungeziefer", "tokens": ["Als", "ste\u00b7chen\u00b7de", "Flie\u00b7gen", "und", "Un\u00b7ge\u00b7zie\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "KON", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ist mir der unerm\u00fcdliche", "tokens": ["Ist", "mir", "der", "un\u00b7er\u00b7m\u00fcd\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Verwirrte Schw\u00e4tzer,", "tokens": ["Ver\u00b7wirr\u00b7te", "Schw\u00e4t\u00b7zer", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Der fragend, belehrend, erz\u00e4hlend,", "tokens": ["Der", "fra\u00b7gend", ",", "be\u00b7leh\u00b7rend", ",", "er\u00b7z\u00e4h\u00b7lend", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "VVPP", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Empfindsam und ohne Ursach lachend,", "tokens": ["Emp\u00b7find\u00b7sam", "und", "oh\u00b7ne", "Ur\u00b7sach", "la\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "APPR", "NN", "ADJD", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Salzlose Geschichtchen erz\u00e4hlend,", "tokens": ["Salz\u00b7lo\u00b7se", "Ge\u00b7schicht\u00b7chen", "er\u00b7z\u00e4h\u00b7lend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVPP", "$,"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.8": {"text": "Oft sich mir an die Seite schiebt,", "tokens": ["Oft", "sich", "mir", "an", "die", "Sei\u00b7te", "schiebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und alle Kr\u00fcmmen des Weges", "tokens": ["Und", "al\u00b7le", "Kr\u00fcm\u00b7men", "des", "We\u00b7ges"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Nach Tempel und Pallast", "tokens": ["Nach", "Tem\u00b7pel", "und", "Pal\u00b7last"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Oder den heiligen Tr\u00fcmmern", "tokens": ["O\u00b7der", "den", "hei\u00b7li\u00b7gen", "Tr\u00fcm\u00b7mern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.12": {"text": "Mit Geschw\u00e4tz mir mi\u00dft.", "tokens": ["Mit", "Ge\u00b7schw\u00e4tz", "mir", "mi\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Langsam wandl' ich die Stadt hinunter,", "tokens": ["Lang\u00b7sam", "wandl'", "ich", "die", "Stadt", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Da h\u00f6r' ich hinter mir sein verdr\u00fc\u00dflich Husten:", "tokens": ["Da", "h\u00f6r'", "ich", "hin\u00b7ter", "mir", "sein", "ver\u00b7dr\u00fc\u00df\u00b7lich", "Hus\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O weh! keine Flucht,", "tokens": ["O", "weh", "!", "kei\u00b7ne", "Flucht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PIAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Kein Hausthor in der N\u00e4he,", "tokens": ["Kein", "Haus\u00b7thor", "in", "der", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Keiner Kirche Asyl!", "tokens": ["Kei\u00b7ner", "Kir\u00b7che", "A\u00b7syl", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Helft mir, ihr alten Jugendk\u00fcnste,", "tokens": ["Helft", "mir", ",", "ihr", "al\u00b7ten", "Ju\u00b7gend\u00b7k\u00fcns\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Durch die ich in fr\u00fchen Jahren", "tokens": ["Durch", "die", "ich", "in", "fr\u00fc\u00b7hen", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wohl selbst die Freunde get\u00e4uscht!", "tokens": ["Wohl", "selbst", "die", "Freun\u00b7de", "ge\u00b7t\u00e4uscht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Schon hat der B\u00f6sewicht,", "tokens": ["Schon", "hat", "der", "B\u00f6\u00b7se\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So w\u00e4hnt er, die sichre Beute,", "tokens": ["So", "w\u00e4hnt", "er", ",", "die", "sich\u00b7re", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sich auf Stunden erjagt.", "tokens": ["Sich", "auf", "Stun\u00b7den", "er\u00b7jagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Da h\u00e4lt ihm der Wandelnde,", "tokens": ["Da", "h\u00e4lt", "ihm", "der", "Wan\u00b7deln\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.5": {"text": "Der sich m\u00fchsam aufrecht stellt,", "tokens": ["Der", "sich", "m\u00fch\u00b7sam", "auf\u00b7recht", "stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihn gerad anschauend", "tokens": ["Ihn", "ge\u00b7rad", "an\u00b7schau\u00b7end"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Ein fremdes Gesicht", "tokens": ["Ein", "frem\u00b7des", "Ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Voll Falten und Runzeln,", "tokens": ["Voll", "Fal\u00b7ten", "und", "Run\u00b7zeln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Verschobenen Mundes,", "tokens": ["Ver\u00b7scho\u00b7be\u00b7nen", "Mun\u00b7des", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Schielenden Auges,", "tokens": ["Schie\u00b7len\u00b7den", "Au\u00b7ges", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Ernst und feierlich entgegen.", "tokens": ["Ernst", "und", "fei\u00b7er\u00b7lich", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Er stutzt und weicht zur\u00fcck,", "tokens": ["Er", "stutzt", "und", "weicht", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Er l\u00fcftet den Hut", "tokens": ["Er", "l\u00fcf\u00b7tet", "den", "Hut"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Und schreitet pr\u00fcfend n\u00e4her:", "tokens": ["Und", "schrei\u00b7tet", "pr\u00fc\u00b7fend", "n\u00e4\u00b7her", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Doch irr' und beth\u00f6rt", "tokens": ["Doch", "irr'", "und", "be\u00b7th\u00f6rt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "Rennt er verwirrt", "tokens": ["Rennt", "er", "ver\u00b7wirrt"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Dem Fremden vor\u00fcber,", "tokens": ["Dem", "Frem\u00b7den", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Nach andrer Beute sp\u00e4hend.", "tokens": ["Nach", "an\u00b7drer", "Beu\u00b7te", "sp\u00e4\u00b7hend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Dank dir, du komische Muse,", "tokens": ["Dank", "dir", ",", "du", "ko\u00b7mi\u00b7sche", "Mu\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Die mir die Fratze bilden half,", "tokens": ["Die", "mir", "die", "Frat\u00b7ze", "bil\u00b7den", "half", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "F\u00fcr Rettung und H\u00fclfe.", "tokens": ["F\u00fcr", "Ret\u00b7tung", "und", "H\u00fcl\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}