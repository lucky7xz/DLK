{"textgrid.poem.41977": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sympathieengewalt verlachst du und der Ber\u00fchrung", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sympathieengewalt verlachst du und der Ber\u00fchrung", "tokens": ["Sym\u00b7pa\u00b7thie\u00b7en\u00b7ge\u00b7walt", "ver\u00b7lachst", "du", "und", "der", "Be\u00b7r\u00fch\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "KON", "ART", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "M\u00e4chtigen Zauber, der rasch schlummernde Kr\u00e4fte bewegt?", "tokens": ["M\u00e4ch\u00b7ti\u00b7gen", "Zau\u00b7ber", ",", "der", "rasch", "schlum\u00b7mern\u00b7de", "Kr\u00e4f\u00b7te", "be\u00b7wegt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Nennst nur Wahn die geheime Verwandtschaft \u00e4hnlicher Seelen?", "tokens": ["Nennst", "nur", "Wahn", "die", "ge\u00b7hei\u00b7me", "Ver\u00b7wandt\u00b7schaft", "\u00e4hn\u00b7li\u00b7cher", "See\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Ungl\u00fcckseliger, ach, hast du denn nimmer geliebt?", "tokens": ["Un\u00b7gl\u00fcck\u00b7se\u00b7li\u00b7ger", ",", "ach", ",", "hast", "du", "denn", "nim\u00b7mer", "ge\u00b7liebt", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Beug' ich mich still zum schwellenden Mund der Geliebten, o sprich, was", "tokens": ["Beug'", "ich", "mich", "still", "zum", "schwel\u00b7len\u00b7den", "Mund", "der", "Ge\u00b7lieb\u00b7ten", ",", "o", "sprich", ",", "was"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVIMP", "PPER", "PRF", "ADJD", "APPRART", "ADJA", "NN", "ART", "NN", "$,", "FM", "ADJD", "$,", "PWS"], "meter": "+--+-+--+--+-++-", "measure": "iambic.septa.invert"}, "line.6": {"text": "Zieht mich mit s\u00fc\u00dfer Gewalt hin zu dem seligen Ku\u00df?", "tokens": ["Zieht", "mich", "mit", "s\u00fc\u00b7\u00dfer", "Ge\u00b7walt", "hin", "zu", "dem", "se\u00b7li\u00b7gen", "Ku\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Rede, woher der elektrische Druck, wenn die Hand sich der Hand naht,", "tokens": ["Re\u00b7de", ",", "wo\u00b7her", "der", "e\u00b7lekt\u00b7ri\u00b7sche", "Druck", ",", "wenn", "die", "Hand", "sich", "der", "Hand", "naht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+--+--+-", "measure": "iambic.septa.invert"}, "line.8": {"text": "Und was fesselt den Arm um die Umschlungene fest?", "tokens": ["Und", "was", "fes\u00b7selt", "den", "Arm", "um", "die", "Um\u00b7schlun\u00b7ge\u00b7ne", "fest", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Schaff' ich die Glorie selbst, die mit rosigem Glanz mir das Daseyn", "tokens": ["Schaff'", "ich", "die", "Glo\u00b7rie", "selbst", ",", "die", "mit", "ro\u00b7si\u00b7gem", "Glanz", "mir", "das", "Da\u00b7seyn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADV", "$,", "PRELS", "APPR", "ADJA", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+--+--+--++", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Kr\u00e4nzt, wenn holder ihr Blick l\u00e4chelt und n\u00e4her sie weilt?", "tokens": ["Kr\u00e4nzt", ",", "wenn", "hol\u00b7der", "ihr", "Blick", "l\u00e4\u00b7chelt", "und", "n\u00e4\u00b7her", "sie", "weilt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ADJA", "PPOSAT", "NN", "VVFIN", "KON", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Bin ich es selbst, der den Geist aufschwingt und mit tieferm Gef\u00fchl das", "tokens": ["Bin", "ich", "es", "selbst", ",", "der", "den", "Geist", "auf\u00b7schwingt", "und", "mit", "tie\u00b7ferm", "Ge\u00b7f\u00fchl", "das"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "$,", "PRELS", "ART", "NN", "VVPP", "KON", "APPR", "ADJA", "NN", "ART"], "meter": "-+-+--+-+-+++-+-", "measure": "iambic.octa.plus.relaxed"}, "line.12": {"text": "Herz mir begabt, wenn sie hold in dem Liede mir schwebt?", "tokens": ["Herz", "mir", "be\u00b7gabt", ",", "wenn", "sie", "hold", "in", "dem", "Lie\u00b7de", "mir", "schwebt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$,", "KOUS", "PPER", "ADJD", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.13": {"text": "Warum w\u00e4hl' ich zum Ruhen so gern die Stelle, wo sie sa\u00df,", "tokens": ["Wa\u00b7rum", "w\u00e4hl'", "ich", "zum", "Ru\u00b7hen", "so", "gern", "die", "Stel\u00b7le", ",", "wo", "sie", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPRART", "NN", "ADV", "ADV", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Finde den Pfad, den sie wandelte, reizend allein?", "tokens": ["Fin\u00b7de", "den", "Pfad", ",", "den", "sie", "wan\u00b7del\u00b7te", ",", "rei\u00b7zend", "al\u00b7lein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADJD", "ADV", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.15": {"text": "Pfl\u00fccke so gern von dem Strauche, wo sie sich Bl\u00fcthen gepfl\u00fcckt hat,", "tokens": ["Pfl\u00fc\u00b7cke", "so", "gern", "von", "dem", "Strau\u00b7che", ",", "wo", "sie", "sich", "Bl\u00fc\u00b7then", "ge\u00b7pfl\u00fcckt", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "PRF", "NN", "VVPP", "VAFIN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Nippe so gern, wo sie nippt', an dem Rande des Kelchs?", "tokens": ["Nip\u00b7pe", "so", "gern", ",", "wo", "sie", "nippt'", ",", "an", "dem", "Ran\u00b7de", "des", "Kelchs", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.17": {"text": "Sprich, was bebt mir dahin durch's Herz, wenn ihr seidnes Gewand mich", "tokens": ["Sprich", ",", "was", "bebt", "mir", "da\u00b7hin", "durch's", "Herz", ",", "wenn", "ihr", "seid\u00b7nes", "Ge\u00b7wand", "mich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "PWS", "VVFIN", "PPER", "PAV", "APPRART", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "PPER"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Streift, was lodert in mir, wenn mich ihr Athem ber\u00fchrt?", "tokens": ["Streift", ",", "was", "lo\u00b7dert", "in", "mir", ",", "wenn", "mich", "ihr", "A\u00b7them", "be\u00b7r\u00fchrt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "APPR", "PPER", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+----+-+--+", "measure": "iambic.penta.chol"}, "line.19": {"text": "Warum tr\u00fcbt im Spiele der Lust mein Auge sich pl\u00f6tzlich,", "tokens": ["Wa\u00b7rum", "tr\u00fcbt", "im", "Spie\u00b7le", "der", "Lust", "mein", "Au\u00b7ge", "sich", "pl\u00f6tz\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPRART", "NN", "ART", "NN", "PPOSAT", "NN", "PRF", "ADJD", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Wenn ihr trauriger Blick weinend zur Erde sich senkt?", "tokens": ["Wenn", "ihr", "trau\u00b7ri\u00b7ger", "Blick", "wei\u00b7nend", "zur", "Er\u00b7de", "sich", "senkt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVPP", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "Warum flieht aus der Brust mir die d\u00fcstere Nacht, wenn hold rings", "tokens": ["Wa\u00b7rum", "flieht", "aus", "der", "Brust", "mir", "die", "d\u00fcs\u00b7te\u00b7re", "Nacht", ",", "wenn", "hold", "rings"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "ART", "NN", "PPER", "ART", "ADJA", "NN", "$,", "KOUS", "ADJD", "ADV"], "meter": "--+--+--+--+---", "measure": "anapaest.tetra.plus"}, "line.22": {"text": "Ihr um den rosigen Mund l\u00e4chelt der Morgen der Lust?", "tokens": ["Ihr", "um", "den", "ro\u00b7si\u00b7gen", "Mund", "l\u00e4\u00b7chelt", "der", "Mor\u00b7gen", "der", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Ach, wir weilten gewi\u00df in sch\u00f6neren Welten zuvor schon,", "tokens": ["Ach", ",", "wir", "weil\u00b7ten", "ge\u00b7wi\u00df", "in", "sch\u00f6\u00b7ne\u00b7ren", "Wel\u00b7ten", "zu\u00b7vor", "schon", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "ADV", "ADV", "$,"], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.24": {"text": "Und der Erinnrung Trost blieb dem verbanneten Geist;", "tokens": ["Und", "der", "E\u00b7rinn\u00b7rung", "Trost", "blieb", "dem", "ver\u00b7ban\u00b7ne\u00b7ten", "Geist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "---+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.25": {"text": "Was wir f\u00fchlen, wir f\u00fchlten es einst, wir f\u00fchlen es ewig,", "tokens": ["Was", "wir", "f\u00fch\u00b7len", ",", "wir", "f\u00fchl\u00b7ten", "es", "einst", ",", "wir", "f\u00fch\u00b7len", "es", "e\u00b7wig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.26": {"text": "Jegliche Wonne sie w\u00fcrzt sch\u00f6ner die kommende Zeit.", "tokens": ["Jeg\u00b7li\u00b7che", "Won\u00b7ne", "sie", "w\u00fcrzt", "sch\u00f6\u00b7ner", "die", "kom\u00b7men\u00b7de", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}}}}