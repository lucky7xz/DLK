{"textgrid.poem.26888": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die \u00dcbereinstimmung", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wahrheit suchen wir beide; du au\u00dfen im Leben, ich innen", "tokens": ["Wahr\u00b7heit", "su\u00b7chen", "wir", "bei\u00b7de", ";", "du", "au\u00b7\u00dfen", "im", "Le\u00b7ben", ",", "ich", "in\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PIS", "$.", "PPER", "ADV", "APPRART", "NN", "$,", "PPER", "ADV"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "In dem Herzen, und so findet sie jeder gewi\u00df.", "tokens": ["In", "dem", "Her\u00b7zen", ",", "und", "so", "fin\u00b7det", "sie", "je\u00b7der", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "ADV", "VVFIN", "PPER", "PIS", "ADV", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ist das Auge gesund, so begegnet es au\u00dfen dem Sch\u00f6pfer,", "tokens": ["Ist", "das", "Au\u00b7ge", "ge\u00b7sund", ",", "so", "be\u00b7geg\u00b7net", "es", "au\u00b7\u00dfen", "dem", "Sch\u00f6p\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Ist es das Herz, dann gewi\u00df spiegelt es innen die Welt.", "tokens": ["Ist", "es", "das", "Herz", ",", "dann", "ge\u00b7wi\u00df", "spie\u00b7gelt", "es", "in\u00b7nen", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "ADV", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}}}}}