{"textgrid.poem.34003": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Mein P\u00e4an", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich m\u00f6gte auch einmal von Freiheit singen,", "tokens": ["Ich", "m\u00f6g\u00b7te", "auch", "ein\u00b7mal", "von", "Frei\u00b7heit", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch, ist der Drang auch gro\u00df, den ich versp\u00fcre,", "tokens": ["Doch", ",", "ist", "der", "Drang", "auch", "gro\u00df", ",", "den", "ich", "ver\u00b7sp\u00fc\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer sagt mir, wie viel Odem ihm geb\u00fchre?", "tokens": ["Wer", "sagt", "mir", ",", "wie", "viel", "O\u00b7dem", "ihm", "ge\u00b7b\u00fch\u00b7re", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PWAV", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir d\u00e4ucht, zuvor mu\u00df ich den Flamberg schwingen.", "tokens": ["Mir", "d\u00e4ucht", ",", "zu\u00b7vor", "mu\u00df", "ich", "den", "Flam\u00b7berg", "schwin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Tag erst, wo um mich die Schwerter klingen,", "tokens": ["Der", "Tag", "erst", ",", "wo", "um", "mich", "die", "Schwer\u00b7ter", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PWAV", "KOUI", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo ich, so wie ich jetzt die Saiten r\u00fchre,", "tokens": ["Wo", "ich", ",", "so", "wie", "ich", "jetzt", "die", "Sai\u00b7ten", "r\u00fch\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADV", "KOKOM", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit eig'ner Faust mein gutes Eisen f\u00fchre,", "tokens": ["Mit", "eig'\u00b7ner", "Faust", "mein", "gu\u00b7tes", "Ei\u00b7sen", "f\u00fch\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Tag erst wird die rechte Antwort bringen.", "tokens": ["Der", "Tag", "erst", "wird", "die", "rech\u00b7te", "Ant\u00b7wort", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auch dann noch fecht' ich still und stumm, gleich Allen,", "tokens": ["Auch", "dann", "noch", "fecht'", "ich", "still", "und", "stumm", ",", "gleich", "Al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,", "ADV", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die schweigend ihren Ha\u00df und Grimm getragen,", "tokens": ["Die", "schwei\u00b7gend", "ih\u00b7ren", "Ha\u00df", "und", "Grimm", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "KON", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch endlich wird mein Blut die Erde f\u00e4rben.", "tokens": ["Doch", "end\u00b7lich", "wird", "mein", "Blut", "die", "Er\u00b7de", "f\u00e4r\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann soll der Freiheit mein P\u00e4an erschallen,", "tokens": ["Dann", "soll", "der", "Frei\u00b7heit", "mein", "P\u00e4\u00b7an", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn so viel Worte, glaub' ich, darf ich wagen,", "tokens": ["Denn", "so", "viel", "Wor\u00b7te", ",", "glaub'", "ich", ",", "darf", "ich", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als Odem zwischen Fallen bleibt und Sterben.", "tokens": ["Als", "O\u00b7dem", "zwi\u00b7schen", "Fal\u00b7len", "bleibt", "und", "Ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}