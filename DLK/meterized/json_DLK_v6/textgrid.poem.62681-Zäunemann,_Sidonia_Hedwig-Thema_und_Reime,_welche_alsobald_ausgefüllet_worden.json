{"textgrid.poem.62681": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Thema und Reime, welche alsobald ausgef\u00fcllet worden", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du werthes Jungfer-Volk mit krum-und gleichen-Nasen,", "tokens": ["Du", "wert\u00b7hes", "Jung\u00b7fer\u00b7Volk", "mit", "krum\u00b7\u00b7und", "glei\u00b7chen\u00b7Na\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von mancherley Gestalt, was suchest du zu \u2013 \u2013 rasen,", "tokens": ["Von", "man\u00b7cher\u00b7ley", "Ge\u00b7stalt", ",", "was", "su\u00b7chest", "du", "zu", "\u2013", "\u2013", "ra\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PWS", "VVFIN", "PPER", "PTKZU", "$(", "$(", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sprichst: Mein Angesicht ist weder zart noch \u2013 \u2013 fein,", "tokens": ["Und", "sprichst", ":", "Mein", "An\u00b7ge\u00b7sicht", "ist", "we\u00b7der", "zart", "noch", "\u2013", "\u2013", "fein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPOSAT", "NN", "VAFIN", "KON", "ADJD", "ADV", "$(", "$(", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von Fleck und Narben Zahl ists warlich auch nicht \u2013 \u2013 rein.", "tokens": ["Von", "Fleck", "und", "Nar\u00b7ben", "Zahl", "ists", "war\u00b7lich", "auch", "nicht", "\u2013", "\u2013", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "NN", "VAFIN", "ADV", "ADV", "PTKNEG", "$(", "$(", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es will das Mannes-Volk mit seinen scharfen \u2013 \u2013 Augen", "tokens": ["Es", "will", "das", "Man\u00b7nes\u00b7Volk", "mit", "sei\u00b7nen", "schar\u00b7fen", "\u2013", "\u2013", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "VVINF", "$(", "$(", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Aus meinem Angesicht gar kein Vergn\u00fcgen \u2013 \u2013 saugen.", "tokens": ["Aus", "mei\u00b7nem", "An\u00b7ge\u00b7sicht", "gar", "kein", "Ver\u00b7gn\u00fc\u00b7gen", "\u2013", "\u2013", "sau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "PIAT", "NN", "$(", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die\u00df ist ein Donner-Wort, und grosser Herzens \u2013 \u2013 Sto\u00df!", "tokens": ["Die\u00df", "ist", "ein", "Don\u00b7ner\u00b7Wort", ",", "und", "gros\u00b7ser", "Her\u00b7zens", "\u2013", "\u2013", "Sto\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "KON", "ADJA", "NN", "$(", "$(", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wohlan! so schm\u00fcnk ich mich, wird gleich der Beutel \u2013 \u2013 blo\u00df,", "tokens": ["Wo\u00b7hlan", "!", "so", "schm\u00fcnk", "ich", "mich", ",", "wird", "gleich", "der", "Beu\u00b7tel", "\u2013", "\u2013", "blo\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "$.", "ADV", "VVFIN", "PPER", "PRF", "$,", "VAFIN", "ADV", "ART", "NN", "$(", "$(", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Von Silber, Gold und Geld; So werd ich doch ein \u2013 \u2013 Spiegel", "tokens": ["Von", "Sil\u00b7ber", ",", "Gold", "und", "Geld", ";", "So", "werd", "ich", "doch", "ein", "\u2013", "\u2013", "Spie\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$.", "ADV", "VAFIN", "PPER", "ADV", "ART", "$(", "$(", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Sch\u00f6nheit, den man ehrt. Ich aber dr\u00fcck das \u2013 \u2013 Siegel", "tokens": ["Der", "Sch\u00f6n\u00b7heit", ",", "den", "man", "ehrt", ".", "Ich", "a\u00b7ber", "dr\u00fcck", "das", "\u2013", "\u2013", "Sie\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$.", "PPER", "ADV", "ADV", "ART", "$(", "$(", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Auf dieses Wort, und sag: Es wird in jedem \u2013 \u2013 Land,", "tokens": ["Auf", "die\u00b7ses", "Wort", ",", "und", "sag", ":", "Es", "wird", "in", "je\u00b7dem", "\u2013", "\u2013", "Land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "KON", "VVFIN", "$.", "PPER", "VAFIN", "APPR", "PIAT", "$(", "$(", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Sch\u00f6nheit von Natur der Vorzug zuer \u2013 \u2013 kant.", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "von", "Na\u00b7tur", "der", "Vor\u00b7zug", "zu\u00b7er", "\u2013", "\u2013", "kant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ART", "NN", "ADJD", "$(", "$(", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}