{"textgrid.poem.24915": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: In den S\u00e4uleng\u00e4ngen auf den Foren", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In den S\u00e4uleng\u00e4ngen auf den Foren", "tokens": ["In", "den", "S\u00e4u\u00b7len\u00b7g\u00e4n\u00b7gen", "auf", "den", "Fo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ringt das Volk die H\u00e4nde schreckensbang:", "tokens": ["Ringt", "das", "Volk", "die", "H\u00e4n\u00b7de", "schre\u00b7ckens\u00b7bang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbchristus! scheuch die Goten von den Thoren!", "tokens": ["\u00bb", "chris\u00b7tus", "!", "scheuch", "die", "Go\u00b7ten", "von", "den", "Tho\u00b7ren", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "$.", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Rette Rom vom Untergang!\u00ab", "tokens": ["Ret\u00b7te", "Rom", "vom", "Un\u00b7ter\u00b7gang", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch Etruriens weise M\u00e4nner treten", "tokens": ["Doch", "E\u00b7tru\u00b7ri\u00b7ens", "wei\u00b7se", "M\u00e4n\u00b7ner", "tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN", "VVINF"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Vor den Stadtgebieter Pompejan:", "tokens": ["Vor", "den", "Stadt\u00b7ge\u00b7bie\u00b7ter", "Pom\u00b7pe\u00b7jan", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbder Gekreuzigte, zu dem sie beten,", "tokens": ["\u00bb", "der", "Ge\u00b7kreu\u00b7zig\u00b7te", ",", "zu", "dem", "sie", "be\u00b7ten", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sollte helfen? Eitler Wahn!", "tokens": ["Soll\u00b7te", "hel\u00b7fen", "?", "Eit\u00b7ler", "Wahn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$.", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Folg uns! Auf der sieben H\u00fcgel Spitze", "tokens": ["Folg", "uns", "!", "Auf", "der", "sie\u00b7ben", "H\u00fc\u00b7gel", "Spit\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "La\u00df die alten G\u00f6tteropfer lohn,", "tokens": ["La\u00df", "die", "al\u00b7ten", "G\u00f6t\u00b7te\u00b7rop\u00b7fer", "lohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und beschw\u00f6ren wollen wir die Blitze,", "tokens": ["Und", "be\u00b7schw\u00f6\u00b7ren", "wol\u00b7len", "wir", "die", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die der Stadt Verderben drohn!\u00ab", "tokens": ["Die", "der", "Stadt", "Ver\u00b7der\u00b7ben", "drohn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ART", "NN", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbja\u00ab \u2013 ruft jener \u2013 \u00bbdie ihr mich erzogen,", "tokens": ["\u00bb", "ja", "\u00ab", "\u2013", "ruft", "je\u00b7ner", "\u2013", "\u00bb", "die", "ihr", "mich", "er\u00b7zo\u00b7gen", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$(", "$(", "VVFIN", "PDAT", "$(", "$(", "PRELS", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Macht, Olympier, habt ihr allein!", "tokens": ["Macht", ",", "O\u00b7lym\u00b7pier", ",", "habt", "ihr", "al\u00b7lein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Jahrtausend wart ihr uns gewogen,", "tokens": ["Ein", "Jahr\u00b7tau\u00b7send", "wart", "ihr", "uns", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zieht in Rom denn wieder ein!\u00ab", "tokens": ["Zieht", "in", "Rom", "denn", "wie\u00b7der", "ein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "NE", "KON", "ADV", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sieh! und zu des Jupiters Alt\u00e4ren,", "tokens": ["Sieh", "!", "und", "zu", "des", "Ju\u00b7pi\u00b7ters", "Al\u00b7t\u00e4\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Des Apoll, erschlie\u00dft sich neu das Thor;", "tokens": ["Des", "A\u00b7poll", ",", "er\u00b7schlie\u00dft", "sich", "neu", "das", "Thor", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PRF", "ADJD", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Leuchtend heben aus dem Schutt die hehren", "tokens": ["Leuch\u00b7tend", "he\u00b7ben", "aus", "dem", "Schutt", "die", "heh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Marmorbilder sich empor.", "tokens": ["Mar\u00b7mor\u00b7bil\u00b7der", "sich", "em\u00b7por", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und bei Hymnensang bekr\u00e4nzter Beter,", "tokens": ["Und", "bei", "Hym\u00b7nen\u00b7sang", "be\u00b7kr\u00e4nz\u00b7ter", "Be\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bei der Lydierfl\u00f6ten sanftem Hauch", "tokens": ["Bei", "der", "Ly\u00b7dier\u00b7fl\u00f6\u00b7ten", "sanf\u00b7tem", "Hauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wirbelt wieder in den dunklen Aether", "tokens": ["Wir\u00b7belt", "wie\u00b7der", "in", "den", "dunk\u00b7len", "A\u00b7e\u00b7ther"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "---+--+-+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hekatomben-Opferrauch.", "tokens": ["He\u00b7ka\u00b7tom\u00b7ben\u00b7Op\u00b7fer\u00b7rauch", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und das Volk in wei\u00dfer Festgewandung", "tokens": ["Und", "das", "Volk", "in", "wei\u00b7\u00dfer", "Fest\u00b7ge\u00b7wan\u00b7dung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Flutet zu des Donnrers S\u00e4ulenhaus \u2013", "tokens": ["Flu\u00b7tet", "zu", "des", "Donn\u00b7rers", "S\u00e4u\u00b7len\u00b7haus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Pl\u00f6tzlich \u2013 horch! was hallt wie Wogenbrandung?", "tokens": ["Pl\u00f6tz\u00b7lich", "\u2013", "horch", "!", "was", "hallt", "wie", "Wo\u00b7gen\u00b7bran\u00b7dung", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "VVIMP", "$.", "PWS", "VVFIN", "KOKOM", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auf dem Forum welch Gebraus?", "tokens": ["Auf", "dem", "Fo\u00b7rum", "welch", "Ge\u00b7braus", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PWAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "N\u00e4her nun \u2013 sie sind es, die Barbaren!", "tokens": ["N\u00e4\u00b7her", "nun", "\u2013", "sie", "sind", "es", ",", "die", "Bar\u00b7ba\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "PPER", "VAFIN", "PPER", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "W\u00fcrgend, ein verheerender Orkan,", "tokens": ["W\u00fcr\u00b7gend", ",", "ein", "ver\u00b7hee\u00b7ren\u00b7der", "Or\u00b7kan", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "W\u00e4lzen Alarichs entmenschte Scharen", "tokens": ["W\u00e4l\u00b7zen", "A\u00b7la\u00b7richs", "ent\u00b7menschte", "Scha\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NE", "ADJA", "NN"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Sich zum Kapitol heran.", "tokens": ["Sich", "zum", "Ka\u00b7pi\u00b7tol", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Zwischen Flammenzischen, Horngeschmetter", "tokens": ["Zwi\u00b7schen", "Flam\u00b7men\u00b7zi\u00b7schen", ",", "Horn\u00b7ge\u00b7schmet\u00b7ter"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "T\u00f6nen Jammerrufe durch die Nacht:", "tokens": ["T\u00f6\u00b7nen", "Jam\u00b7mer\u00b7ru\u00b7fe", "durch", "die", "Nacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Flieht! Gestorben sind die alten G\u00f6tter", "tokens": ["Flieht", "!", "Ge\u00b7stor\u00b7ben", "sind", "die", "al\u00b7ten", "G\u00f6t\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die neuen ohne Macht!", "tokens": ["Und", "die", "neu\u00b7en", "oh\u00b7ne", "Macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wer soll nun die Himmelss\u00e4ulen tragen?", "tokens": ["Wer", "soll", "nun", "die", "Him\u00b7mels\u00b7s\u00e4u\u00b7len", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Weh! sie st\u00fcrzen! Rom, die ew'ge, f\u00e4llt!", "tokens": ["Weh", "!", "sie", "st\u00fcr\u00b7zen", "!", "Rom", ",", "die", "ew'\u00b7ge", ",", "f\u00e4llt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVINF", "$.", "NE", "$,", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Mit den Tr\u00fcmmern sinken wir erschlagen", "tokens": ["Mit", "den", "Tr\u00fcm\u00b7mern", "sin\u00b7ken", "wir", "er\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "In das Riesengrab der Welt!", "tokens": ["In", "das", "Rie\u00b7sen\u00b7grab", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}