{"textgrid.poem.56281": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Herzog Huldreich und Beatrix", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Huldreich, der Herzog im B\u00f6hmerland,", "tokens": ["Herr", "Huld\u00b7reich", ",", "der", "Her\u00b7zog", "im", "B\u00f6h\u00b7mer\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er jagt auf den H\u00f6hen zur Stund;", "tokens": ["Er", "jagt", "auf", "den", "H\u00f6\u00b7hen", "zur", "Stund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die B\u00e4uerin w\u00e4scht die Leinewand", "tokens": ["Die", "B\u00e4u\u00b7e\u00b7rin", "w\u00e4scht", "die", "Lei\u00b7ne\u00b7wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Am Bach im schattigen Grund.", "tokens": ["Am", "Bach", "im", "schat\u00b7ti\u00b7gen", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "\u00bbbed\u00fcrftig und m\u00fcde verirrtest du", "tokens": ["\u00bb", "be\u00b7d\u00fcrf\u00b7tig", "und", "m\u00fc\u00b7de", "ver\u00b7irr\u00b7test", "du"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "KON", "ADJD", "VVFIN", "PPER"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Dich J\u00e4ger in unser Tal;", "tokens": ["Dich", "J\u00e4\u00b7ger", "in", "un\u00b7ser", "Tal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "La\u00df hier dich nieder zu kurzer Ruh,", "tokens": ["La\u00df", "hier", "dich", "nie\u00b7der", "zu", "kur\u00b7zer", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und teile mit mir das Mahl.\u00ab \u2013", "tokens": ["Und", "tei\u00b7le", "mit", "mir", "das", "Mahl", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "NN", "APPR", "PPER", "ART", "NN", "$.", "$(", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbhab Dank, hab Dank, du freundliches Kind,", "tokens": ["\u00bb", "hab", "Dank", ",", "hab", "Dank", ",", "du", "freund\u00b7li\u00b7ches", "Kind", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "NN", "$,", "VAFIN", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Du spendest, wo mancher raubt;", "tokens": ["Du", "spen\u00b7dest", ",", "wo", "man\u00b7cher", "raubt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie mir ermattet die Glieder sind,", "tokens": ["Wie", "mir", "er\u00b7mat\u00b7tet", "die", "Glie\u00b7der", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sinkt sorgenschwer auch mein Haupt.\u00ab \u2013", "tokens": ["Sinkt", "sor\u00b7gen\u00b7schwer", "auch", "mein", "Haupt", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbund naht die Sorge bei freudiger Jagd", "tokens": ["\u00bb", "und", "naht", "die", "Sor\u00b7ge", "bei", "freu\u00b7di\u00b7ger", "Jagd"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dir J\u00e4ger im lustigen Wald?", "tokens": ["Dir", "J\u00e4\u00b7ger", "im", "lus\u00b7ti\u00b7gen", "Wald", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wann nagend den alten Vater sie plagt,", "tokens": ["Wann", "na\u00b7gend", "den", "al\u00b7ten", "Va\u00b7ter", "sie", "plagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Verscheuchet mein Lied sie bald.\u00ab \u2013", "tokens": ["Ver\u00b7scheu\u00b7chet", "mein", "Lied", "sie", "bald", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "ADV", "$.", "$(", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbkein Lied aus treuer, freudiger Brust!", "tokens": ["\u00bb", "kein", "Lied", "aus", "treu\u00b7er", ",", "freu\u00b7di\u00b7ger", "Brust", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "So einsam inmitten der Schar!", "tokens": ["So", "ein\u00b7sam", "in\u00b7mit\u00b7ten", "der", "Schar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Kein Stern der heiteren, innigen Lust,", "tokens": ["Kein", "Stern", "der", "hei\u00b7te\u00b7ren", ",", "in\u00b7ni\u00b7gen", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kein Aug, wie das deine so klar!\u00ab \u2013", "tokens": ["Kein", "Aug", ",", "wie", "das", "dei\u00b7ne", "so", "klar", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PIAT", "NN", "$,", "PWAV", "ART", "PPOSAT", "ADV", "ADJD", "$.", "$(", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "\u00bbdoch leuchtet aus k\u00fchngew\u00f6lbten Braun", "tokens": ["\u00bb", "doch", "leuch\u00b7tet", "aus", "k\u00fchn\u00b7ge\u00b7w\u00f6lb\u00b7ten", "Braun"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mildfreundlich dein Augenstern;", "tokens": ["Mild\u00b7freund\u00b7lich", "dein", "Au\u00b7gens\u00b7tern", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Wer m\u00f6chte nicht in den Himmel schaun,", "tokens": ["Wer", "m\u00f6ch\u00b7te", "nicht", "in", "den", "Him\u00b7mel", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Wer nicht in das Auge dir gern?\u00ab", "tokens": ["Wer", "nicht", "in", "das", "Au\u00b7ge", "dir", "gern", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PTKNEG", "APPR", "ART", "NN", "PPER", "ADV", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "\u00bbzu mir hinauf wohl manche sah,", "tokens": ["\u00bb", "zu", "mir", "hin\u00b7auf", "wohl", "man\u00b7che", "sah", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "ADV", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Frug nicht nach des Auges Licht,", "tokens": ["Frug", "nicht", "nach", "des", "Au\u00b7ges", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und h\u00e4tte gestanden ein anderer da", "tokens": ["Und", "h\u00e4t\u00b7te", "ge\u00b7stan\u00b7den", "ein", "an\u00b7de\u00b7rer", "da"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "VVPP", "ART", "ADJA", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Statt meiner, sie merkt' es nicht.\u00ab \u2013", "tokens": ["Statt", "mei\u00b7ner", ",", "sie", "merkt'", "es", "nicht", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "PPOSAT", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "$(", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "\u00bbauf, J\u00e4ger, es mag geschieden nun sein;", "tokens": ["\u00bb", "auf", ",", "J\u00e4\u00b7ger", ",", "es", "mag", "ge\u00b7schie\u00b7den", "nun", "sein", ";"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "NN", "$,", "PPER", "VMFIN", "VVPP", "ADV", "VAINF", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dort windet dein Pfad sich hinan.", "tokens": ["Dort", "win\u00b7det", "dein", "Pfad", "sich", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PRF", "PTKVZ", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Noch schaut ich ins Auge dem Vater allein,", "tokens": ["Noch", "schaut", "ich", "ins", "Au\u00b7ge", "dem", "Va\u00b7ter", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sonst keinem anderen Mann.\u00ab \u2013", "tokens": ["Sonst", "kei\u00b7nem", "an\u00b7de\u00b7ren", "Mann", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "\u00bbmi\u00dfdeute nicht ein tr\u00fcbes Wort,", "tokens": ["\u00bb", "mi\u00df\u00b7deu\u00b7te", "nicht", "ein", "tr\u00fc\u00b7bes", "Wort", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das nicht, du Gute, dir galt;", "tokens": ["Das", "nicht", ",", "du", "Gu\u00b7te", ",", "dir", "galt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "$,", "PPER", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und schickst du von hinnen mich z\u00fcrnend fort,", "tokens": ["Und", "schickst", "du", "von", "hin\u00b7nen", "mich", "z\u00fcr\u00b7nend", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADV", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wo find ich auf Erden noch Halt?\u00ab \u2013", "tokens": ["Wo", "find", "ich", "auf", "Er\u00b7den", "noch", "Halt", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "NN", "ADV", "NN", "$.", "$(", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.10": {"line.1": {"text": "\u00bbich z\u00fcrne nicht, wie du es meinst,", "tokens": ["\u00bb", "ich", "z\u00fcr\u00b7ne", "nicht", ",", "wie", "du", "es", "meinst", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin vom Z\u00fcrnen, wie fern!", "tokens": ["Ich", "bin", "vom", "Z\u00fcr\u00b7nen", ",", "wie", "fern", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Gott segne dich, und die dereinst", "tokens": ["Gott", "seg\u00b7ne", "dich", ",", "und", "die", "de\u00b7reinst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird deines Himmels Stern.\u00ab \u2013", "tokens": ["Wird", "dei\u00b7nes", "Him\u00b7mels", "Stern", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "\u00bbgott segne dich, du liebe Maid;", "tokens": ["\u00bb", "gott", "seg\u00b7ne", "dich", ",", "du", "lie\u00b7be", "Maid", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch eins verk\u00fcnde mir mild:", "tokens": ["Noch", "eins", "ver\u00b7k\u00fcn\u00b7de", "mir", "mild", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Gedenk ich dein in Freud und Leid,", "tokens": ["Ge\u00b7denk", "ich", "dein", "in", "Freud", "und", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie nenn ich das s\u00fc\u00dfe Bild?\u00ab \u2013", "tokens": ["Wie", "nenn", "ich", "das", "s\u00fc\u00b7\u00dfe", "Bild", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "\u00bbbeatrix nennt der Vater mich,", "tokens": ["\u00bb", "be\u00b7at\u00b7rix", "nennt", "der", "Va\u00b7ter", "mich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des H\u00fctte dort sich zeigt;", "tokens": ["Des", "H\u00fct\u00b7te", "dort", "sich", "zeigt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du aber sprich, wie nenn ich dich,", "tokens": ["Du", "a\u00b7ber", "sprich", ",", "wie", "nenn", "ich", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$,", "PWAV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der huldreich sich mir geneigt?\u00ab \u2013", "tokens": ["Der", "huld\u00b7reich", "sich", "mir", "ge\u00b7neigt", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJD", "PRF", "PPER", "VVPP", "$.", "$(", "$("], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}}, "stanza.13": {"line.1": {"text": "\u00bbbeatrix, Heilesbringerin!", "tokens": ["\u00bb", "be\u00b7at\u00b7rix", ",", "Hei\u00b7les\u00b7brin\u00b7ge\u00b7rin", "!"], "token_info": ["punct", "word", "punct", "word", "punct"], "pos": ["$(", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl wirst du als solche bekannt;", "tokens": ["Wohl", "wirst", "du", "als", "sol\u00b7che", "be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "KOUS", "PIS", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {