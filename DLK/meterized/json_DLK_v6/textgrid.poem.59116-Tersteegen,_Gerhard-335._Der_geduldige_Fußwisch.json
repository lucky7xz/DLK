{"textgrid.poem.59116": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "335. Der geduldige Fu\u00dfwisch", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Fu\u00dfwisch l\u00e4\u00dft sich schlepp'n und zerren,", "tokens": ["Der", "Fu\u00df\u00b7wisch", "l\u00e4\u00dft", "sich", "schlepp'n", "und", "zer\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er widerstrebt in keinem nicht;", "tokens": ["Er", "wi\u00b7der\u00b7strebt", "in", "kei\u00b7nem", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So sei auch deine Seel' dem Herren,", "tokens": ["So", "sei", "auch", "dei\u00b7ne", "Seel'", "dem", "Her\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was geist- und leiblich dir geschicht!", "tokens": ["Was", "geist", "und", "leib\u00b7lich", "dir", "ge\u00b7schicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "TRUNC", "KON", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}