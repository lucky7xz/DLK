{"textgrid.poem.36112": {"metadata": {"author": {"name": "Gloger, Georg", "birth": "N.A.", "death": "N.A."}, "title": "9. An das erl\u00f6sete Leipzig", "genre": "verse", "period": "N.A.", "pub_year": 1617, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du zwierbeherschte Stadt, fast inner zweien Tagen,", "tokens": ["Du", "zwier\u00b7be\u00b7herschte", "Stadt", ",", "fast", "in\u00b7ner", "zwei\u00b7en", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "ADV", "ADJD", "CARD", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die du von Vngel\u00fcck vnd Gl\u00fccke weist zu sagen,", "tokens": ["Die", "du", "von", "Vn\u00b7ge\u00b7l\u00fcck", "vnd", "Gl\u00fc\u00b7cke", "weist", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie war dir wol zu Muth, als ", "tokens": ["Wie", "war", "dir", "wol", "zu", "Muth", ",", "als"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPR", "NN", "$,", "KOUS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Wagen, Heer vnd Macht? Furcht, Angst vnd Noth mich drange.", "tokens": ["Mit", "Wa\u00b7gen", ",", "Heer", "vnd", "Macht", "?", "Furcht", ",", "Angst", "vnd", "Noth", "mich", "dran\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$.", "NN", "$,", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie, als dich wieder frey dein Siegesf\u00fcrste machte,", "tokens": ["Wie", ",", "als", "dich", "wie\u00b7der", "frey", "dein", "Sie\u00b7ges\u00b7f\u00fcrs\u00b7te", "mach\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "PPER", "ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd schlug den Feind ins ", "tokens": ["Vnd", "schlug", "den", "Feind", "ins"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "O frewe lange dich! vnd la\u00df dich ja nicht tawren,", "tokens": ["O", "fre\u00b7we", "lan\u00b7ge", "dich", "!", "vnd", "la\u00df", "dich", "ja", "nicht", "taw\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "PPER", "$.", "KON", "VVIMP", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df dieser Ehr vnd Ruhmbs noch weit nicht werde trawren.", "tokens": ["Da\u00df", "die\u00b7ser", "Ehr", "vnd", "Ruhmbs", "noch", "weit", "nicht", "wer\u00b7de", "traw\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "KON", "NN", "ADV", "ADJD", "PTKNEG", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Denn itzt f\u00e4hrt Fama hin, in aller Welt zu sagen,", "tokens": ["Denn", "itzt", "f\u00e4hrt", "Fa\u00b7ma", "hin", ",", "in", "al\u00b7ler", "Welt", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "NN", "PTKVZ", "$,", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}}}}}