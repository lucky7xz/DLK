{"textgrid.poem.52479": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Melodie! du Laut aus h\u00f6herm Leben!", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Melodie! du Laut aus h\u00f6herm Leben!", "tokens": ["Me\u00b7lo\u00b7die", "!", "du", "Laut", "aus", "h\u00f6\u00b7herm", "Le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deiner \u00c4therschwingen reinstes Streben", "tokens": ["Dei\u00b7ner", "\u00c4\u00b7ther\u00b7schwin\u00b7gen", "reins\u00b7tes", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "W\u00fcrde zum Unendlichen sich heben;", "tokens": ["W\u00fcr\u00b7de", "zum", "Un\u00b7end\u00b7li\u00b7chen", "sich", "he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Doch ein Schall, der fl\u00fcchtige, vert\u00f6nt,", "tokens": ["Doch", "ein", "Schall", ",", "der", "fl\u00fcch\u00b7ti\u00b7ge", ",", "ver\u00b7t\u00f6nt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Hallt kein be\u00dfrer Wohllaut uns von innen,", "tokens": ["Hallt", "kein", "be\u00df\u00b7rer", "Wohl\u00b7laut", "uns", "von", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "PPER", "APPR", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Der, was hier verklang an \u00e4u\u00dfern Sinnen,", "tokens": ["Der", ",", "was", "hier", "ver\u00b7klang", "an", "\u00e4u\u00b7\u00dfern", "Sin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Dauernder zum Geistigen versch\u00f6nt!", "tokens": ["Dau\u00b7ern\u00b7der", "zum", "Geis\u00b7ti\u00b7gen", "ver\u00b7sch\u00f6nt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Durch des Lebens Kr\u00e4nz' und seine Flittern", "tokens": ["Durch", "des", "Le\u00b7bens", "Kr\u00e4nz'", "und", "sei\u00b7ne", "Flit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "St\u00fcrmt das Schicksal oft aus Nachtgewittern,", "tokens": ["St\u00fcrmt", "das", "Schick\u00b7sal", "oft", "aus", "Nacht\u00b7ge\u00b7wit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und die bang' verscheuchten T\u00f6n' erzittern,", "tokens": ["Und", "die", "bang'", "ver\u00b7scheuch\u00b7ten", "T\u00f6n'", "er\u00b7zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie verwehter Freuden Bl\u00fctenhauch.", "tokens": ["Wie", "ver\u00b7weh\u00b7ter", "Freu\u00b7den", "Bl\u00fc\u00b7ten\u00b7hauch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Alles Sch\u00f6ne mu\u00df verbl\u00fcht entschweben;", "tokens": ["Al\u00b7les", "Sch\u00f6\u00b7ne", "mu\u00df", "ver\u00b7bl\u00fcht", "ent\u00b7schwe\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVPP", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die Empfindung flieht, es flieht das Leben,", "tokens": ["Die", "Emp\u00b7fin\u00b7dung", "flieht", ",", "es", "flieht", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und des Wohllauts Strom entrinnet auch.", "tokens": ["Und", "des", "Wohl\u00b7lauts", "Strom", "ent\u00b7rin\u00b7net", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Soll die F\u00fclle seiner Harmonieen", "tokens": ["Soll", "die", "F\u00fcl\u00b7le", "sei\u00b7ner", "Har\u00b7mo\u00b7ni\u00b7e\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+----+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Leuchtend durch des Lebens Thale ziehen,", "tokens": ["Leuch\u00b7tend", "durch", "des", "Le\u00b7bens", "Tha\u00b7le", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Darf die Glut der R\u00fchrung nie vergl\u00fchen,", "tokens": ["Darf", "die", "Glut", "der", "R\u00fch\u00b7rung", "nie", "ver\u00b7gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die der Geister Sonn' auf ihn ergo\u00df.", "tokens": ["Die", "der", "Geis\u00b7ter", "Sonn'", "auf", "ihn", "er\u00b7go\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wie vom Engel, der die Wolken teilte,", "tokens": ["Wie", "vom", "En\u00b7gel", ",", "der", "die", "Wol\u00b7ken", "teil\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sp\u00e4t noch Licht auf jenem Teiche weilte,", "tokens": ["Sp\u00e4t", "noch", "Licht", "auf", "je\u00b7nem", "Tei\u00b7che", "weil\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und die Heilungskraft, die ihm entflo\u00df.", "tokens": ["Und", "die", "Hei\u00b7lungs\u00b7kraft", ",", "die", "ihm", "ent\u00b7flo\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Nicht dem Ohre schmeichelnd nur zu kosen, \u2013", "tokens": ["Nicht", "dem", "Oh\u00b7re", "schmei\u00b7chelnd", "nur", "zu", "ko\u00b7sen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJD", "ADV", "PTKZU", "VVINF", "$,", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Zur Erquickung dem Erquickungslosen,", "tokens": ["Zur", "Er\u00b7quic\u00b7kung", "dem", "Er\u00b7quic\u00b7kungs\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Fl\u00f6\u00dft das \u00d6l aus deinen Himmelsrosen", "tokens": ["Fl\u00f6\u00dft", "das", "\u00d6l", "aus", "dei\u00b7nen", "Him\u00b7mels\u00b7ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Holdes Labsal in des Menschen Herz! \u2013", "tokens": ["Hol\u00b7des", "Lab\u00b7sal", "in", "des", "Men\u00b7schen", "Herz", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "In der T\u00f6ne zarten Knospen liegen", "tokens": ["In", "der", "T\u00f6\u00b7ne", "zar\u00b7ten", "Knos\u00b7pen", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Leise Wehmut, r\u00fchrendes Vergn\u00fcgen", "tokens": ["Lei\u00b7se", "Weh\u00b7mut", ",", "r\u00fch\u00b7ren\u00b7des", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und der Sehnsucht wonnevoller Schmerz.", "tokens": ["Und", "der", "Sehn\u00b7sucht", "won\u00b7ne\u00b7vol\u00b7ler", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Hellas Tochter! holdeste A\u00f6de!", "tokens": ["Hel\u00b7las", "Toch\u00b7ter", "!", "hol\u00b7des\u00b7te", "A\u00b7\u00f6\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dort in Ch\u00f6ren sang dir der Trag\u00f6de,", "tokens": ["Dort", "in", "Ch\u00f6\u00b7ren", "sang", "dir", "der", "Tra\u00b7g\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dort beseeltest du der Suada Rede,", "tokens": ["Dort", "be\u00b7seel\u00b7test", "du", "der", "Sua\u00b7da", "Re\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schwebtest auf der Dichtung reichem Strom;", "tokens": ["Schweb\u00b7test", "auf", "der", "Dich\u00b7tung", "rei\u00b7chem", "Strom", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Schwandest dann, verkl\u00e4rter zu erscheinen,", "tokens": ["Schwan\u00b7dest", "dann", ",", "ver\u00b7kl\u00e4r\u00b7ter", "zu", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Stiegst in Hymnen christlicher Gemeinen", "tokens": ["Stiegst", "in", "Hym\u00b7nen", "christ\u00b7li\u00b7cher", "Ge\u00b7mei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Durch des Tempels Dom zum Sternendom!", "tokens": ["Durch", "des", "Tem\u00b7pels", "Dom", "zum", "Ster\u00b7nen\u00b7dom", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Heil'ge Andacht, tief und doch erhaben,", "tokens": ["Heil'\u00b7ge", "An\u00b7dacht", ",", "tief", "und", "doch", "er\u00b7ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "N\u00e4hrtest du des Glaubens Himmelsgaben,", "tokens": ["N\u00e4hr\u00b7test", "du", "des", "Glau\u00b7bens", "Him\u00b7mels\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "M\u00e4rtyrer im Todeskampf zu laben,", "tokens": ["M\u00e4r\u00b7ty\u00b7rer", "im", "To\u00b7des\u00b7kampf", "zu", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Lichte Hoffnung, frommes Gottvertrau'n;", "tokens": ["Lich\u00b7te", "Hoff\u00b7nung", ",", "from\u00b7mes", "Gott\u00b7ver\u00b7trau'n", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Gabst Choral und Psalm die h\u00f6chsten Weihen,", "tokens": ["Gabst", "Cho\u00b7ral", "und", "Psalm", "die", "h\u00f6chs\u00b7ten", "Wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und erhellst mit leisen Litaneien", "tokens": ["Und", "er\u00b7hellst", "mit", "lei\u00b7sen", "Li\u00b7ta\u00b7nei\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Sterbenden des offnen Grabes Grau'n.", "tokens": ["Ster\u00b7ben\u00b7den", "des", "off\u00b7nen", "Gra\u00b7bes", "Grau'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["NN", "ART", "ADJA", "NN", "NE", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "T\u00e4uschung w\u00e4rest du, und bald entflohen,", "tokens": ["T\u00e4u\u00b7schung", "w\u00e4\u00b7rest", "du", ",", "und", "bald", "ent\u00b7flo\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "KON", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Was den Mut erhebt, wo St\u00fcrme drohen?", "tokens": ["Was", "den", "Mut", "er\u00b7hebt", ",", "wo", "St\u00fcr\u00b7me", "dro\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "PWAV", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Was die Heere st\u00e4rkt, und die Heroen", "tokens": ["Was", "die", "Hee\u00b7re", "st\u00e4rkt", ",", "und", "die", "He\u00b7roen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Zart zu schonen \u2013 k\u00fchn zu sterben drang?", "tokens": ["Zart", "zu", "scho\u00b7nen", "\u2013", "k\u00fchn", "zu", "ster\u00b7ben", "drang", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$(", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Was, wie Spartas Helden, edle Streiter", "tokens": ["Was", ",", "wie", "Spar\u00b7tas", "Hel\u00b7den", ",", "ed\u00b7le", "Strei\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "$,", "PWAV", "NE", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "In die Schlachten f\u00fchret freudig heiter,", "tokens": ["In", "die", "Schlach\u00b7ten", "f\u00fch\u00b7ret", "freu\u00b7dig", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Ihr Ged\u00e4chtnis feiert im Gesang? \u2013", "tokens": ["Ihr", "Ge\u00b7d\u00e4cht\u00b7nis", "fei\u00b7ert", "im", "Ge\u00b7sang", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Woll' auch uns nicht wirkungslos entschwinden;", "tokens": ["Woll'", "auch", "uns", "nicht", "wir\u00b7kungs\u00b7los", "ent\u00b7schwin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Immer sollst du offne Herzen finden", "tokens": ["Im\u00b7mer", "sollst", "du", "off\u00b7ne", "Her\u00b7zen", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und zu sch\u00f6nen Zwecken sie verbinden,", "tokens": ["Und", "zu", "sch\u00f6\u00b7nen", "Zwe\u00b7cken", "sie", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die Gem\u00fcter stimmen rein und klar!", "tokens": ["Die", "Ge\u00b7m\u00fc\u00b7ter", "stim\u00b7men", "rein", "und", "klar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und des Vaterlandes edle S\u00f6hne,", "tokens": ["Und", "des", "Va\u00b7ter\u00b7lan\u00b7des", "ed\u00b7le", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Seiner sanften T\u00f6chter Seelent\u00f6ne,", "tokens": ["Sei\u00b7ner", "sanf\u00b7ten", "T\u00f6ch\u00b7ter", "See\u00b7len\u00b7t\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Bringen dir ein w\u00fcrdig Opfer dar!", "tokens": ["Brin\u00b7gen", "dir", "ein", "w\u00fcr\u00b7dig", "Op\u00b7fer", "dar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJD", "NN", "PTKVZ", "