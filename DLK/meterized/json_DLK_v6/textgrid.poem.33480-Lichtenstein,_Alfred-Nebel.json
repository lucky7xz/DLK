{"textgrid.poem.33480": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Nebel", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Nebel hat die Welt so weich zerst\u00f6rt.", "tokens": ["Ein", "Ne\u00b7bel", "hat", "die", "Welt", "so", "weich", "zer\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Blutlose B\u00e4ume l\u00f6sen sich in Rauch.", "tokens": ["Blut\u00b7lo\u00b7se", "B\u00e4u\u00b7me", "l\u00f6\u00b7sen", "sich", "in", "Rauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und Schatten schweben, wo man Schreie h\u00f6rt.", "tokens": ["Und", "Schat\u00b7ten", "schwe\u00b7ben", ",", "wo", "man", "Schrei\u00b7e", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,", "PWAV", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Brennende Biester schwinden hin wie Hauch.", "tokens": ["Bren\u00b7nen\u00b7de", "Bies\u00b7ter", "schwin\u00b7den", "hin", "wie", "Hauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "KOKOM", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Gefangne Fliegen sind die Gaslaternen.", "tokens": ["Ge\u00b7fang\u00b7ne", "Flie\u00b7gen", "sind", "die", "Gas\u00b7la\u00b7ter\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und jede flackert, da\u00df sie noch entrinne.", "tokens": ["Und", "je\u00b7de", "fla\u00b7ckert", ",", "da\u00df", "sie", "noch", "ent\u00b7rin\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch seitlich lauert glimmend hoch in Fernen", "tokens": ["Doch", "seit\u00b7lich", "lau\u00b7ert", "glim\u00b7mend", "hoch", "in", "Fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADJD", "ADJD", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der giftge Mond, die fette Nebelspinne.", "tokens": ["Der", "gift\u00b7ge", "Mond", ",", "die", "fet\u00b7te", "Ne\u00b7bel\u00b7spin\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir aber, die, verrucht, zum Tode taugen,", "tokens": ["Wir", "a\u00b7ber", ",", "die", ",", "ver\u00b7rucht", ",", "zum", "To\u00b7de", "tau\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "$,", "VVPP", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zerschreiten knirschend diese w\u00fcste Pracht.", "tokens": ["Zer\u00b7schrei\u00b7ten", "knir\u00b7schend", "die\u00b7se", "w\u00fcs\u00b7te", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und stechen stumm die wei\u00dfen Elendsaugen", "tokens": ["Und", "ste\u00b7chen", "stumm", "die", "wei\u00b7\u00dfen", "E\u00b7lend\u00b7sau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie Spie\u00dfe in die aufgeschwollne Nacht.", "tokens": ["Wie", "Spie\u00b7\u00dfe", "in", "die", "auf\u00b7ge\u00b7schwoll\u00b7ne", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}