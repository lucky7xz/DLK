{"textgrid.poem.54989": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Gefunden", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging im Walde", "tokens": ["Ich", "ging", "im", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "So f\u00fcr mich hin,", "tokens": ["So", "f\u00fcr", "mich", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und nichts zu suchen,", "tokens": ["Und", "nichts", "zu", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Das war mein Sinn.", "tokens": ["Das", "war", "mein", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Im Schatten sah ich", "tokens": ["Im", "Schat\u00b7ten", "sah", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ein Bl\u00fcmchen stehn,", "tokens": ["Ein", "Bl\u00fcm\u00b7chen", "stehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wie Sterne leuchtend,", "tokens": ["Wie", "Ster\u00b7ne", "leuch\u00b7tend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wie \u00c4uglein sch\u00f6n.", "tokens": ["Wie", "\u00c4ug\u00b7lein", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ich wollt es brechen,", "tokens": ["Ich", "wollt", "es", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Da sagt' es fein:", "tokens": ["Da", "sagt'", "es", "fein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "\u00bbsoll ich zum Welken", "tokens": ["\u00bb", "soll", "ich", "zum", "Wel\u00b7ken"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "VMFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Gebrochen sein?\u00ab", "tokens": ["Ge\u00b7bro\u00b7chen", "sein", "?", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["VVPP", "VAINF", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ich grub's mit allen", "tokens": ["Ich", "grub's", "mit", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NE", "APPR", "PIAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Den W\u00fcrzlein aus,", "tokens": ["Den", "W\u00fcrz\u00b7lein", "aus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Zum Garten trug ich's", "tokens": ["Zum", "Gar\u00b7ten", "trug", "ich's"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PIS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Am h\u00fcbschen Haus.", "tokens": ["Am", "h\u00fcb\u00b7schen", "Haus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Und pflanzt es wieder", "tokens": ["Und", "pflanzt", "es", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Am stillen Ort;", "tokens": ["Am", "stil\u00b7len", "Ort", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nun zweigt es immer", "tokens": ["Nun", "zweigt", "es", "im\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-++", "measure": "unknown.measure.tri"}, "line.4": {"text": "Und bl\u00fcht so fort.", "tokens": ["Und", "bl\u00fcht", "so", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}