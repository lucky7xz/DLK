{"textgrid.poem.55987": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn er denen, die ihm gastlich waren,", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn er denen, die ihm gastlich waren,", "tokens": ["Wenn", "er", "de\u00b7nen", ",", "die", "ihm", "gast\u00b7lich", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "sp\u00e4t, nach ihrem Tage noch, da sie", "tokens": ["sp\u00e4t", ",", "nach", "ih\u00b7rem", "Ta\u00b7ge", "noch", ",", "da", "sie"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "$,", "APPR", "PPOSAT", "NN", "ADV", "$,", "KOUS", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "fragten nach den Fahrten und Gefahren,", "tokens": ["frag\u00b7ten", "nach", "den", "Fahr\u00b7ten", "und", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "still berichtete: er wu\u00dfte nie,", "tokens": ["still", "be\u00b7rich\u00b7te\u00b7te", ":", "er", "wu\u00df\u00b7te", "nie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "wie sie schrecken und mit welchem j\u00e4hen", "tokens": ["wie", "sie", "schre\u00b7cken", "und", "mit", "wel\u00b7chem", "j\u00e4\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVINF", "KON", "APPR", "PWAT", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wort sie wenden, da\u00df sie so wie er", "tokens": ["Wort", "sie", "wen\u00b7den", ",", "da\u00df", "sie", "so", "wie", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VVINF", "$,", "KOUS", "PPER", "ADV", "KOKOM", "PPER"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "in dem blau gestillten Inselmeer", "tokens": ["in", "dem", "blau", "ge\u00b7still\u00b7ten", "In\u00b7sel\u00b7meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "die Vergoldung jener Inseln s\u00e4hen,", "tokens": ["die", "Ver\u00b7gol\u00b7dung", "je\u00b7ner", "In\u00b7seln", "s\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "deren Anblick macht, da\u00df die Gefahr", "tokens": ["de\u00b7ren", "An\u00b7blick", "macht", ",", "da\u00df", "die", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRELAT", "NN", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "umschl\u00e4gt; denn nun ist sie nicht im Tosen", "tokens": ["um\u00b7schl\u00e4gt", ";", "denn", "nun", "ist", "sie", "nicht", "im", "To\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "KON", "ADV", "VAFIN", "PPER", "PTKNEG", "APPRART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "und im W\u00fcten, wo sie immer war.", "tokens": ["und", "im", "W\u00fc\u00b7ten", ",", "wo", "sie", "im\u00b7mer", "war", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Lautlos kommt sie \u00fcber die Matrosen,", "tokens": ["Laut\u00b7los", "kommt", "sie", "\u00fc\u00b7ber", "die", "Mat\u00b7ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "welche wissen, da\u00df es dort auf jenen", "tokens": ["wel\u00b7che", "wis\u00b7sen", ",", "da\u00df", "es", "dort", "auf", "je\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAT", "VVINF", "$,", "KOUS", "PPER", "ADV", "APPR", "PDAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "goldnen Inseln manchmal singt \u2013,", "tokens": ["gold\u00b7nen", "In\u00b7seln", "manch\u00b7mal", "singt", "\u2013", ","], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "ADV", "VVFIN", "$(", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und sich blindlings in die Ruder lehnen,", "tokens": ["und", "sich", "blind\u00b7lings", "in", "die", "Ru\u00b7der", "leh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "wie umringt", "tokens": ["wie", "um\u00b7ringt"], "token_info": ["word", "word"], "pos": ["PWAV", "VVPP"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "von der Stille, die die ganze Weite", "tokens": ["von", "der", "Stil\u00b7le", ",", "die", "die", "gan\u00b7ze", "Wei\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "in sich hat und an die Ohren weht,", "tokens": ["in", "sich", "hat", "und", "an", "die", "Oh\u00b7ren", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VAFIN", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "so als w\u00e4re ihre andre Seite", "tokens": ["so", "als", "w\u00e4\u00b7re", "ih\u00b7re", "and\u00b7re", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "der Gesang, dem keiner widersteht.", "tokens": ["der", "Ge\u00b7sang", ",", "dem", "kei\u00b7ner", "wi\u00b7der\u00b7steht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}