{"textgrid.poem.48216": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "8. Such nicht, wie's eigentlich gewesen", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Such nicht, wie's eigentlich gewesen,", "tokens": ["Such", "nicht", ",", "wie's", "ei\u00b7gent\u00b7lich", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "VVFIN", "ADV", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wolle nicht in den Herzen lesen.", "tokens": ["Wol\u00b7le", "nicht", "in", "den", "Her\u00b7zen", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Sieht's freundlich aus, nimm's freundlich an,", "tokens": ["Sieht's", "freund\u00b7lich", "aus", ",", "nim\u00b7m's", "freund\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKVZ", "$,", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nimm den Biedertuer als Biedermann.", "tokens": ["Nimm", "den", "Bie\u00b7der\u00b7tu\u00b7er", "als", "Bie\u00b7der\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KOUS", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Alle Fl\u00fcgelm\u00e4nner auf Sammellisten,", "tokens": ["Al\u00b7le", "Fl\u00fc\u00b7gel\u00b7m\u00e4n\u00b7ner", "auf", "Sam\u00b7mel\u00b7lis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nimm sie hin als Musterchristen.", "tokens": ["Nimm", "sie", "hin", "als", "Mus\u00b7ter\u00b7chris\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "KOUS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn sie nur ", "tokens": ["Wenn", "sie", "nur"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Forsche nicht nach den letzten Gr\u00fcnden.", "tokens": ["For\u00b7sche", "nicht", "nach", "den", "letz\u00b7ten", "Gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}