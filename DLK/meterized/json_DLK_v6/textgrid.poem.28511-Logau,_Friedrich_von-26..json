{"textgrid.poem.28511": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rappinus schenckt dem Herren, was er ihm vor entwand;", "tokens": ["Rap\u00b7pi\u00b7nus", "schenckt", "dem", "Her\u00b7ren", ",", "was", "er", "ihm", "vor", "ent\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "PWS", "PPER", "PPER", "APPR", "NE", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Er nimmt es mit der lincken, gibts mit der rechten Hand;", "tokens": ["Er", "nimmt", "es", "mit", "der", "lin\u00b7cken", ",", "gibts", "mit", "der", "rech\u00b7ten", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Drum wird er treuer Diener, nicht schlimmer Dieb genant.", "tokens": ["Drum", "wird", "er", "treu\u00b7er", "Die\u00b7ner", ",", "nicht", "schlim\u00b7mer", "Dieb", "ge\u00b7nant", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADJA", "NN", "$,", "PTKNEG", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}