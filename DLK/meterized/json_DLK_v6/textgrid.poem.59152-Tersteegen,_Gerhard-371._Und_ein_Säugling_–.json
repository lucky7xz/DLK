{"textgrid.poem.59152": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "371. Und ein S\u00e4ugling \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schau doch, Maria s\u00e4uget mich,", "tokens": ["Schau", "doch", ",", "Ma\u00b7ria", "s\u00e4u\u00b7get", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "NE", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich, mein Kind, will s\u00e4ugen dich;", "tokens": ["Und", "ich", ",", "mein", "Kind", ",", "will", "s\u00e4u\u00b7gen", "dich", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,", "VMFIN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tu auf den Mund, kehr alle Lust", "tokens": ["Tu", "auf", "den", "Mund", ",", "kehr", "al\u00b7le", "Lust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "$,", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In mich hinein und saug die Brust!", "tokens": ["In", "mich", "hin\u00b7ein", "und", "saug", "die", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}