{"textgrid.poem.39465": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie schlank ihr Leib im Schlummer hingego\u00dfen", "tokens": ["Wie", "schlank", "ihr", "Leib", "im", "Schlum\u00b7mer", "hin\u00b7ge\u00b7go\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf Flaum sich wiegt, von keiner H\u00fcll' umfangen!", "tokens": ["Auf", "Flaum", "sich", "wiegt", ",", "von", "kei\u00b7ner", "H\u00fcll'", "um\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVFIN", "$,", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So goldnes Haar ist auf so sch\u00f6ne Wangen,", "tokens": ["So", "gold\u00b7nes", "Haar", "ist", "auf", "so", "sch\u00f6\u00b7ne", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So zarten Hals, noch nie herabgeflo\u00dfen.", "tokens": ["So", "zar\u00b7ten", "Hals", ",", "noch", "nie", "her\u00b7ab\u00b7ge\u00b7flo\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch Todesnacht hat schon ihr Aug' umschlo\u00dfen,", "tokens": ["Doch", "To\u00b7des\u00b7nacht", "hat", "schon", "ihr", "Aug'", "um\u00b7schlo\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Lippen ist der k\u00fchne Geist entgangen;", "tokens": ["Den", "Lip\u00b7pen", "ist", "der", "k\u00fch\u00b7ne", "Geist", "ent\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie selber gab den Stichen wilder Schlangen", "tokens": ["Sie", "sel\u00b7ber", "gab", "den", "Sti\u00b7chen", "wil\u00b7der", "Schlan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Lilienbl\u00fcthen, die am Busen spro\u00dfen.", "tokens": ["Die", "Li\u00b7li\u00b7en\u00b7bl\u00fc\u00b7then", ",", "die", "am", "Bu\u00b7sen", "spro\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Oft hat ja Liebe G\u00f6tter umgestaltet:", "tokens": ["Oft", "hat", "ja", "Lie\u00b7be", "G\u00f6t\u00b7ter", "um\u00b7ge\u00b7stal\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Heldin, h\u00e4tt' auch dich ein Gott ber\u00fccket,", "tokens": ["O", "Hel\u00b7din", ",", "h\u00e4tt'", "auch", "dich", "ein", "Gott", "be\u00b7r\u00fc\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VAFIN", "ADV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Beneidend deinen Reiz dem Land der Todten!", "tokens": ["Be\u00b7nei\u00b7dend", "dei\u00b7nen", "Reiz", "dem", "Land", "der", "Tod\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hielt jener nicht, der mit dem Donner waltet,", "tokens": ["Hielt", "je\u00b7ner", "nicht", ",", "der", "mit", "dem", "Don\u00b7ner", "wal\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "PTKNEG", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als Schlang' Olympien br\u00fcnstiglich umstricket?", "tokens": ["Als", "Schlang'", "O\u00b7lym\u00b7pien", "br\u00fcns\u00b7tig\u00b7lich", "um\u00b7stri\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du h\u00e4ttest ihm der Freuden mehr geboten.", "tokens": ["Du", "h\u00e4t\u00b7test", "ihm", "der", "Freu\u00b7den", "mehr", "ge\u00b7bo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}