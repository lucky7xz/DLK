{"textgrid.poem.36262": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Freudige", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Tr\u00e4ume verschwinden, Aurora erscheint.", "tokens": ["Die", "Tr\u00e4u\u00b7me", "ver\u00b7schwin\u00b7den", ",", "Au\u00b7ro\u00b7ra", "er\u00b7scheint", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "NE", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es lebte und strebte verschlossen im Dunkel", "tokens": ["Es", "leb\u00b7te", "und", "streb\u00b7te", "ver\u00b7schlos\u00b7sen", "im", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "VVPP", "APPRART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die Kraft meiner Liebe wie Licht des Karfunkel,", "tokens": ["Die", "Kraft", "mei\u00b7ner", "Lie\u00b7be", "wie", "Licht", "des", "Kar\u00b7fun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KOKOM", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Bis da ich umarmte dich, ewigen Freund.", "tokens": ["Bis", "da", "ich", "um\u00b7arm\u00b7te", "dich", ",", "e\u00b7wi\u00b7gen", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Zu dir hab' ich fr\u00fche die Tr\u00e4nen geweint,", "tokens": ["Zu", "dir", "hab'", "ich", "fr\u00fc\u00b7he", "die", "Tr\u00e4\u00b7nen", "ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Noch eh' ich die Sonne des Lebens erkannte,", "tokens": ["Noch", "eh'", "ich", "die", "Son\u00b7ne", "des", "Le\u00b7bens", "er\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Noch eh' ich im Feuer der Freude entbrannte,", "tokens": ["Noch", "eh'", "ich", "im", "Feu\u00b7er", "der", "Freu\u00b7de", "ent\u00b7brann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Im Herzen des Herzens dich immer gemeint.", "tokens": ["Im", "Her\u00b7zen", "des", "Her\u00b7zens", "dich", "im\u00b7mer", "ge\u00b7meint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Nun darf ich der Freude Musik nicht entfliehen;", "tokens": ["Nun", "darf", "ich", "der", "Freu\u00b7de", "Mu\u00b7sik", "nicht", "ent\u00b7flie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+--++-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es sind ja die Schmerzen in Wohllaut verschwunden.", "tokens": ["Es", "sind", "ja", "die", "Schmer\u00b7zen", "in", "Wohl\u00b7laut", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "K\u00fchn heb' ich die Stirne von Kr\u00e4nzen umwunden,", "tokens": ["K\u00fchn", "heb'", "ich", "die", "Stir\u00b7ne", "von", "Kr\u00e4n\u00b7zen", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Zu singen mit dir der Lust Harmonien.", "tokens": ["Zu", "sin\u00b7gen", "mit", "dir", "der", "Lust", "Har\u00b7mo\u00b7ni\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "PPER", "ART", "NN", "NE", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ja wollte hinunter der Abgrund uns ziehen,", "tokens": ["Ja", "woll\u00b7te", "hin\u00b7un\u00b7ter", "der", "Ab\u00b7grund", "uns", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VMFIN", "ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und w\u00e4re zum Tode die Braut nur erwacht;", "tokens": ["Und", "w\u00e4\u00b7re", "zum", "To\u00b7de", "die", "Braut", "nur", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Wo du mir zugegen, da leuchtet die Nacht,", "tokens": ["Wo", "du", "mir", "zu\u00b7ge\u00b7gen", ",", "da", "leuch\u00b7tet", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADJD", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Und m\u00f6chte am Himmel die Sonne verbl\u00fchen.", "tokens": ["Und", "m\u00f6ch\u00b7te", "am", "Him\u00b7mel", "die", "Son\u00b7ne", "ver\u00b7bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Ich schaue vom Felsen den Teppich der Fluren,", "tokens": ["Ich", "schau\u00b7e", "vom", "Fel\u00b7sen", "den", "Tep\u00b7pich", "der", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Als h\u00e4tt' ich sie nie zuvor noch gesehen,", "tokens": ["Als", "h\u00e4tt'", "ich", "sie", "nie", "zu\u00b7vor", "noch", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PPER", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Wasser, die B\u00e4ume, so K\u00fchlung uns wehen,", "tokens": ["Die", "Was\u00b7ser", ",", "die", "B\u00e4u\u00b7me", ",", "so", "K\u00fch\u00b7lung", "uns", "we\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ADV", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Das freudige Spiel der jungen Naturen;", "tokens": ["Das", "freu\u00b7di\u00b7ge", "Spiel", "der", "jun\u00b7gen", "Na\u00b7tu\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "An Sternen, in Blumen die heiligen Spuren,", "tokens": ["An", "Ster\u00b7nen", ",", "in", "Blu\u00b7men", "die", "hei\u00b7li\u00b7gen", "Spu\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Ich kann es nicht sagen, doch f\u00fchl' ich die Tiefe,", "tokens": ["Ich", "kann", "es", "nicht", "sa\u00b7gen", ",", "doch", "f\u00fchl'", "ich", "die", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Als ob aus der Ferne Aurora mich riefe,", "tokens": ["Als", "ob", "aus", "der", "Fer\u00b7ne", "Au\u00b7ro\u00b7ra", "mich", "rie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPR", "ART", "NN", "NE", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Ein leuchtender Wink aus dunkeln Azuren.", "tokens": ["Ein", "leuch\u00b7ten\u00b7der", "Wink", "aus", "dun\u00b7keln", "A\u00b7zu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie d\u00fcrstet mich ach! nach den himmlischen Quellen.", "tokens": ["Wie", "d\u00fcrs\u00b7tet", "mich", "ach", "!", "nach", "den", "himm\u00b7li\u00b7schen", "Quel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "$.", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Das Dunkel ist klar, und offen die Pforte,", "tokens": ["Das", "Dun\u00b7kel", "ist", "klar", ",", "und", "of\u00b7fen", "die", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich h\u00f6re der Mutter erzeugende Worte,", "tokens": ["Ich", "h\u00f6\u00b7re", "der", "Mut\u00b7ter", "er\u00b7zeu\u00b7gen\u00b7de", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ich sehe der Liebe das Leben entquellen.", "tokens": ["Ich", "se\u00b7he", "der", "Lie\u00b7be", "das", "Le\u00b7ben", "ent\u00b7quel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Ich kann nicht entsteigen den lieblichen Wellen,", "tokens": ["Ich", "kann", "nicht", "ent\u00b7stei\u00b7gen", "den", "lieb\u00b7li\u00b7chen", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "So dringen zur Seele die s\u00fc\u00dfesten Gluten;", "tokens": ["So", "drin\u00b7gen", "zur", "See\u00b7le", "die", "s\u00fc\u00b7\u00dfes\u00b7ten", "Glu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Die Erde begr\u00fc\u00dft mich, in Fr\u00fchlinges Fluten,", "tokens": ["Die", "Er\u00b7de", "be\u00b7gr\u00fc\u00dft", "mich", ",", "in", "Fr\u00fch\u00b7lin\u00b7ges", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--++-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Ich f\u00fchle die Sehnsucht den Busen mir schwellen.", "tokens": ["Ich", "f\u00fch\u00b7le", "die", "Sehn\u00b7sucht", "den", "Bu\u00b7sen", "mir", "schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}