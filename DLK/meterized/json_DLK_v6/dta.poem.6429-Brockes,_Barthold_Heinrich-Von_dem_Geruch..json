{"dta.poem.6429": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein jeder kan an sich, was riechen sey, verstehen:", "tokens": ["Ein", "je\u00b7der", "kan", "an", "sich", ",", "was", "rie\u00b7chen", "sey", ",", "ver\u00b7ste\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "APPR", "PRF", "$,", "PWS", "VVINF", "VAFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein wir m\u00fcssen itzo sehen,", "tokens": ["Al\u00b7lein", "wir", "m\u00fcs\u00b7sen", "it\u00b7zo", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was der Geruch in dufftgen C\u00f6rpern sey.", "tokens": ["Was", "der", "Ge\u00b7ruch", "in", "dufft\u00b7gen", "C\u00f6r\u00b7pern", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}