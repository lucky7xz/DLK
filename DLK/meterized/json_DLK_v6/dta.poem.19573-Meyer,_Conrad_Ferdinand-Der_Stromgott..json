{"dta.poem.19573": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der Stromgott.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Morgengraun. Die Karavane windet sich dem Nil zur Seite,", "tokens": ["Mor\u00b7gen\u00b7graun", ".", "Die", "Ka\u00b7ra\u00b7va\u00b7ne", "win\u00b7det", "sich", "dem", "Nil", "zur", "Sei\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Eine Rede dr\u00f6hnt und murmelt \u00fcber dunkler Stromesbreite.", "tokens": ["Ei\u00b7ne", "Re\u00b7de", "dr\u00f6hnt", "und", "mur\u00b7melt", "\u00fc\u00b7ber", "dunk\u00b7ler", "Stro\u00b7mes\u00b7brei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "L\u00e4ngs dem Ufer nippen durstig silbergrau geperlte Tauben,", "tokens": ["L\u00e4ngs", "dem", "U\u00b7fer", "nip\u00b7pen", "durs\u00b7tig", "sil\u00b7ber\u00b7grau", "ge\u00b7perl\u00b7te", "Tau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADJD", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Trinken Ibisse mit blankem Fl\u00fcgelpaar und schwarzen Hauben.", "tokens": ["Trin\u00b7ken", "I\u00b7bis\u00b7se", "mit", "blan\u00b7kem", "Fl\u00fc\u00b7gel\u00b7paar", "und", "schwar\u00b7zen", "Hau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+---+-+-+-+-+-", "measure": "unknown.measure.septa"}}, "stanza.3": {"line.1": {"text": "Nil, der segenreiche Vater, sorgt f\u00fcr alle seine Kinder,", "tokens": ["Nil", ",", "der", "se\u00b7gen\u00b7rei\u00b7che", "Va\u00b7ter", ",", "sorgt", "f\u00fcr", "al\u00b7le", "sei\u00b7ne", "Kin\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Speist und tr\u00e4nkt aus seiner F\u00fclle keines mehr und keines", "tokens": ["Speist", "und", "tr\u00e4nkt", "aus", "sei\u00b7ner", "F\u00fcl\u00b7le", "kei\u00b7nes", "mehr", "und", "kei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "PIS", "ADV", "KON", "PIS"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.4": {"line.1": {"text": "Neben einem braunen Reiter ein gebundner Knabe wandelt,", "tokens": ["Ne\u00b7ben", "ei\u00b7nem", "brau\u00b7nen", "Rei\u00b7ter", "ein", "ge\u00b7bund\u00b7ner", "Kna\u00b7be", "wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Joseph ist's, den seine Br\u00fcder in die Sklaverei verhandelt.", "tokens": ["Jo\u00b7se\u00b7ph", "ist's", ",", "den", "sei\u00b7ne", "Br\u00fc\u00b7der", "in", "die", "Skla\u00b7ve\u00b7rei", "ver\u00b7han\u00b7delt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "PPOSAT", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}}, "stanza.5": {"line.1": {"text": "Taub' und Ibis flattern nur um wenig Fl\u00fcgelschl\u00e4ge weiter.", "tokens": ["Taub'", "und", "I\u00b7bis", "flat\u00b7tern", "nur", "um", "we\u00b7nig", "Fl\u00fc\u00b7gel\u00b7schl\u00e4\u00b7ge", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "VVFIN", "ADV", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Joseph lauscht des Stromes Worten. Ruhig sitzt der stumme Reiter.", "tokens": ["Jo\u00b7se\u00b7ph", "lauscht", "des", "Stro\u00b7mes", "Wor\u00b7ten", ".", "Ru\u00b7hig", "sitzt", "der", "stum\u00b7me", "Rei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$.", "NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}}, "stanza.6": {"line.1": {"text": "\u201eknabe, deine Blicke trauern! J\u00fcngling, deine F\u00fc\u00dfe bluten!", "tokens": ["\u201e", "kna\u00b7be", ",", "dei\u00b7ne", "Bli\u00b7cke", "trau\u00b7ern", "!", "J\u00fcng\u00b7ling", ",", "dei\u00b7ne", "F\u00fc\u00b7\u00dfe", "blu\u00b7ten", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PPOSAT", "NN", "VVINF", "$.", "NN", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Dich verkauften deine Br\u00fcder ... Sei willkomm an meinen", "tokens": ["Dich", "ver\u00b7kauf\u00b7ten", "dei\u00b7ne", "Br\u00fc\u00b7der", "...", "Sei", "will\u00b7komm", "an", "mei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "VAIMP", "ADJD", "APPR", "PPOSAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.7": {"line.1": {"text": "Joseph, fremder Knabe Joseph, du gefesselter, du m\u00fcder,", "tokens": ["Jo\u00b7se\u00b7ph", ",", "frem\u00b7der", "Kna\u00b7be", "Jo\u00b7se\u00b7ph", ",", "du", "ge\u00b7fes\u00b7sel\u00b7ter", ",", "du", "m\u00fc\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "NE", "$,", "PPER", "ADJA", "$,", "PPER", "ADJD", "$,"], "meter": "+--+-+-+-+-+-+--+-", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "Bist du einst der Herr der Ernten, speise deine schlimmen Br\u00fcder!", "tokens": ["Bist", "du", "einst", "der", "Herr", "der", "Ern\u00b7ten", ",", "spei\u00b7se", "dei\u00b7ne", "schlim\u00b7men", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Knabe Joseph!\u201c rauscht es dumpfer. Das erstaunte Kind in Banden", "tokens": ["Kna\u00b7be", "Jo\u00b7se\u00b7ph", "!", "\u201c", "rauscht", "es", "dum\u00b7pfer", ".", "Das", "er\u00b7staun\u00b7te", "Kind", "in", "Ban\u00b7den"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$.", "$(", "VVFIN", "PPER", "ADJD", "$.", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "+-+--+-+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Tr\u00f6stet sich des g\u00fct'gen Gru\u00dfes, bleibt er auch ihm unverstanden.", "tokens": ["Tr\u00f6s\u00b7tet", "sich", "des", "g\u00fct'\u00b7gen", "Gru\u00b7\u00dfes", ",", "bleibt", "er", "auch", "ihm", "un\u00b7ver\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Auf des Niles weiten Wassern ist des Stromgotts Wort ver-", "tokens": ["Auf", "des", "Ni\u00b7les", "wei\u00b7ten", "Was\u00b7sern", "ist", "des", "Strom\u00b7gotts", "Wort", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "ADJA", "NN", "VAFIN", "ART", "NN", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Nur ein Antlitz schwimmt und schimmert, dessen Haare lockig", "tokens": ["Nur", "ein", "Ant\u00b7litz", "schwimmt", "und", "schim\u00b7mert", ",", "des\u00b7sen", "Haa\u00b7re", "lo\u00b7ckig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,", "PRELAT", "NN", "ADJD"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}}, "stanza.10": {"line.1": {"text": "Jetzt beleben sich die Pfade. Schiffe bl\u00e4hen ihre Fl\u00fcgel.", "tokens": ["Jetzt", "be\u00b7le\u00b7ben", "sich", "die", "Pfa\u00b7de", ".", "Schif\u00b7fe", "bl\u00e4\u00b7hen", "ih\u00b7re", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$.", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Kleebeladene Kamele wandern, sacht bewegte H\u00fcgel.", "tokens": ["Klee\u00b7be\u00b7la\u00b7de\u00b7ne", "Ka\u00b7me\u00b7le", "wan\u00b7dern", ",", "sacht", "be\u00b7weg\u00b7te", "H\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.11": {"line.1": {"text": "Frauen kommen mit dem schlanken Kruge, die gemessen schreiten", "tokens": ["Frau\u00b7en", "kom\u00b7men", "mit", "dem", "schlan\u00b7ken", "Kru\u00b7ge", ",", "die", "ge\u00b7mes\u00b7sen", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "VVPP", "VVFIN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "In verh\u00fclltem stillem Zuge, wie die Jahre, wie die Zeiten ...", "tokens": ["In", "ver\u00b7h\u00fcll\u00b7tem", "stil\u00b7lem", "Zu\u00b7ge", ",", "wie", "die", "Jah\u00b7re", ",", "wie", "die", "Zei\u00b7ten", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "$,", "PWAV", "ART", "NN", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.12": {"line.1": {"text": "Aus der ahnungsvollen Ferne ragen Spitzen, hell besonnte,", "tokens": ["Aus", "der", "ah\u00b7nungs\u00b7vol\u00b7len", "Fer\u00b7ne", "ra\u00b7gen", "Spit\u00b7zen", ",", "hell", "be\u00b7sonn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Steigen wie beschneite Gipfel wei\u00df am reinen Horizonte \u2014", "tokens": ["Stei\u00b7gen", "wie", "be\u00b7schnei\u00b7te", "Gip\u00b7fel", "wei\u00df", "am", "rei\u00b7nen", "Ho\u00b7ri\u00b7zon\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJA", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.13": {"line.1": {"text": "Joseph schaut empor zum Reiter: \u201eMit dir meiner V\u00e4ter Frieden!", "tokens": ["Jo\u00b7se\u00b7ph", "schaut", "em\u00b7por", "zum", "Rei\u00b7ter", ":", "\u201e", "Mit", "dir", "mei\u00b7ner", "V\u00e4\u00b7ter", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "APPRART", "NN", "$.", "$(", "APPR", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "Herr, wie nennst du dort die Berge?\u201c \u201eKind, du schaust die", "tokens": ["Herr", ",", "wie", "nennst", "du", "dort", "die", "Ber\u00b7ge", "?", "\u201c", "\u201e", "Kind", ",", "du", "schaust", "die"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ART", "NN", "$.", "$(", "$(", "NN", "$,", "PPER", "VVFIN", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}