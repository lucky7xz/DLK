{"textgrid.poem.61065": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Von Dingen, die man nicht antasten soll", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hatt' ein Bildnis wunderfein,", "tokens": ["Ich", "hatt'", "ein", "Bild\u00b7nis", "wun\u00b7der\u00b7fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit zarten Farben ausgemalt,", "tokens": ["Mit", "zar\u00b7ten", "Far\u00b7ben", "aus\u00b7ge\u00b7malt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das hat mit seinem bunten Schein", "tokens": ["Das", "hat", "mit", "sei\u00b7nem", "bun\u00b7ten", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar lieb ins Auge mir gestrahlt;", "tokens": ["Gar", "lieb", "ins", "Au\u00b7ge", "mir", "ge\u00b7strahlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich hielt es ganz f\u00fcr mich allein,", "tokens": ["Ich", "hielt", "es", "ganz", "f\u00fcr", "mich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wo ich war, da mu\u00dft' es sein.", "tokens": ["Und", "wo", "ich", "war", ",", "da", "mu\u00dft'", "es", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Tags stand's an meiner Arbeitsst\u00e4tte,", "tokens": ["Tags", "stan\u00b7d's", "an", "mei\u00b7ner", "Ar\u00b7beits\u00b7st\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Zu Nacht hing's \u00fcber meinem Bette,", "tokens": ["Zu", "Nacht", "hin\u00b7g's", "\u00fc\u00b7ber", "mei\u00b7nem", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und selbst in meinem sch\u00f6nsten Traum", "tokens": ["Und", "selbst", "in", "mei\u00b7nem", "sch\u00f6ns\u00b7ten", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wie hold es bl\u00fcht', ihr glaubt es kaum.", "tokens": ["Wie", "hold", "es", "bl\u00fcht'", ",", "ihr", "glaubt", "es", "kaum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da dachten die Leute in der Stadt:", "tokens": ["Da", "dach\u00b7ten", "die", "Leu\u00b7te", "in", "der", "Stadt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbwas der wohl so Besondres hat!\u00ab", "tokens": ["\u00bb", "was", "der", "wohl", "so", "Be\u00b7sond\u00b7res", "hat", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "ART", "ADV", "ADV", "NN", "VAFIN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Kamen herbei von allen Enden,", "tokens": ["Ka\u00b7men", "her\u00b7bei", "von", "al\u00b7len", "En\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Betasteten es mit plumpen H\u00e4nden,", "tokens": ["Be\u00b7tas\u00b7te\u00b7ten", "es", "mit", "plum\u00b7pen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+----+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Hielten es gegen Feuer und Licht,", "tokens": ["Hiel\u00b7ten", "es", "ge\u00b7gen", "Feu\u00b7er", "und", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Ob auch die Farben in der Richt,", "tokens": ["Ob", "auch", "die", "Far\u00b7ben", "in", "der", "Richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wischten am Firnis hier und dort", "tokens": ["Wischten", "am", "Fir\u00b7nis", "hier", "und", "dort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "ADV", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und hingen's dann an seinen Ort.", "tokens": ["Und", "hin\u00b7gen's", "dann", "an", "sei\u00b7nen", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Die Leute sind ein eigen Geschlecht,", "tokens": ["Die", "Leu\u00b7te", "sind", "ein", "ei\u00b7gen", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Meinen, sie h\u00e4tten vollkommen recht,", "tokens": ["Mei\u00b7nen", ",", "sie", "h\u00e4t\u00b7ten", "voll\u00b7kom\u00b7men", "recht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sagen, mir bliebe das Bild ja doch,", "tokens": ["Sa\u00b7gen", ",", "mir", "blie\u00b7be", "das", "Bild", "ja", "doch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und ich auch sei derselbe noch;", "tokens": ["Und", "ich", "auch", "sei", "der\u00b7sel\u00b7be", "noch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VAFIN", "PDAT", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich aber schlage die Augen nieder,", "tokens": ["Ich", "a\u00b7ber", "schla\u00b7ge", "die", "Au\u00b7gen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und wenn ich auf mein Kleinod seh,", "tokens": ["Und", "wenn", "ich", "auf", "mein", "Klei\u00b7nod", "seh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Tut's mir im tiefsten Herzen weh;", "tokens": ["Tut's", "mir", "im", "tiefs\u00b7ten", "Her\u00b7zen", "weh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Schmelz ist hin und kommt nicht wieder.", "tokens": ["Der", "Schmelz", "ist", "hin", "und", "kommt", "nicht", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "KON", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}