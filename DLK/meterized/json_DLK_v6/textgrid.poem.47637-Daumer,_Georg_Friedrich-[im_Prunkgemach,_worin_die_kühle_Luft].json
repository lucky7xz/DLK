{"textgrid.poem.47637": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[im Prunkgemach, worin die k\u00fchle Luft]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Prunkgemach, worin die k\u00fchle Luft", "tokens": ["Im", "Prunk\u00b7ge\u00b7mach", ",", "wo\u00b7rin", "die", "k\u00fch\u00b7le", "Luft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durchwonniget von reichem Ambraduft,", "tokens": ["Durch\u00b7won\u00b7ni\u00b7get", "von", "rei\u00b7chem", "Am\u00b7bra\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da ruhten in behaglichem Verein", "tokens": ["Da", "ruh\u00b7ten", "in", "be\u00b7hag\u00b7li\u00b7chem", "Ver\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Drei sch\u00f6ne Kinder traulich und allein,", "tokens": ["Drei", "sch\u00f6\u00b7ne", "Kin\u00b7der", "trau\u00b7lich", "und", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und man beschlo\u00df, geheime, zarte Sachen", "tokens": ["Und", "man", "be\u00b7schlo\u00df", ",", "ge\u00b7hei\u00b7me", ",", "zar\u00b7te", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "$,", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im Wettgesange reimend kund zu machen;", "tokens": ["Im", "Wett\u00b7ge\u00b7san\u00b7ge", "rei\u00b7mend", "kund", "zu", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein Beutel Goldes sollte f\u00fcr den Sieg", "tokens": ["Ein", "Beu\u00b7tel", "Gol\u00b7des", "soll\u00b7te", "f\u00fcr", "den", "Sieg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und ich der Richter sein in diesem Krieg.", "tokens": ["Und", "ich", "der", "Rich\u00b7ter", "sein", "in", "die\u00b7sem", "Krieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "PPOSAT", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Nicht ohne Scheu sofort zum Anbeginn", "tokens": ["Nicht", "oh\u00b7ne", "Scheu", "so\u00b7fort", "zum", "An\u00b7be\u00b7ginn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Verk\u00fcndete die erste S\u00e4ngerin:", "tokens": ["Ver\u00b7k\u00fcn\u00b7de\u00b7te", "die", "ers\u00b7te", "S\u00e4n\u00b7ge\u00b7rin", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "\u00bbich schlummerte; da nahte meine Lust", "tokens": ["\u00bb", "ich", "schlum\u00b7mer\u00b7te", ";", "da", "nah\u00b7te", "mei\u00b7ne", "Lust"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und weckte nicht \u2013 o trauriger Verlust!\u00ab", "tokens": ["Und", "weck\u00b7te", "nicht", "\u2013", "o", "trau\u00b7ri\u00b7ger", "Ver\u00b7lust", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$(", "FM", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Die andere, nicht ohne R\u00fcckbehalt,", "tokens": ["Die", "an\u00b7de\u00b7re", ",", "nicht", "oh\u00b7ne", "R\u00fcck\u00b7be\u00b7halt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Entschleierte sich im Sange dergestalt:", "tokens": ["Ent\u00b7schlei\u00b7er\u00b7te", "sich", "im", "San\u00b7ge", "der\u00b7ge\u00b7stalt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "ADV", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "\u00bbmit meinem Freund im Traume koset' ich;", "tokens": ["\u00bb", "mit", "mei\u00b7nem", "Freund", "im", "Trau\u00b7me", "ko\u00b7set'", "ich", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "O tr\u00e4umte mir so sch\u00f6n allewiglich!\u00ab", "tokens": ["O", "tr\u00e4um\u00b7te", "mir", "so", "sch\u00f6n", "al\u00b7le\u00b7wig\u00b7lich", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADJD", "ADJD", "$.", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Nun kam die Reih' auch an die dritte Sch\u00f6ne,", "tokens": ["Nun", "kam", "die", "Reih'", "auch", "an", "die", "drit\u00b7te", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und es verriethen ihre Silbert\u00f6ne:", "tokens": ["Und", "es", "ver\u00b7rie\u00b7then", "ih\u00b7re", "Sil\u00b7ber\u00b7t\u00f6\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "\u00bbsink' ich dahin in seinem Arme s\u00fc\u00df,", "tokens": ["\u00bb", "sink'", "ich", "da\u00b7hin", "in", "sei\u00b7nem", "Ar\u00b7me", "s\u00fc\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "PAV", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Mein Lager wird zum Rosenparadies.\u00ab", "tokens": ["Mein", "La\u00b7ger", "wird", "zum", "Ro\u00b7sen\u00b7pa\u00b7ra\u00b7dies", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Drauf hat man eine Sklavin abgesendet", "tokens": ["Drauf", "hat", "man", "ei\u00b7ne", "Skla\u00b7vin", "ab\u00b7ge\u00b7sen\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PIS", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und mir die Verse s\u00e4mmtlich eingeh\u00e4ndet.", "tokens": ["Und", "mir", "die", "Ver\u00b7se", "s\u00e4mmt\u00b7lich", "ein\u00b7ge\u00b7h\u00e4n\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Ich kr\u00f6nte rasch den dritten Wettgesang,", "tokens": ["Ich", "kr\u00f6n\u00b7te", "rasch", "den", "drit\u00b7ten", "Wett\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Denn holde Wahrheit athmete sein Klang.", "tokens": ["Denn", "hol\u00b7de", "Wahr\u00b7heit", "ath\u00b7me\u00b7te", "sein", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Noch aber ist die M\u00e4hre nicht geendet;", "tokens": ["Noch", "a\u00b7ber", "ist", "die", "M\u00e4h\u00b7re", "nicht", "ge\u00b7en\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bald wurde mir noch etwas eingeh\u00e4ndet;", "tokens": ["Bald", "wur\u00b7de", "mir", "noch", "et\u00b7was", "ein\u00b7ge\u00b7h\u00e4n\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Beutel mit Dukaten ward gesendet.", "tokens": ["Ein", "Beu\u00b7tel", "mit", "Du\u00b7ka\u00b7ten", "ward", "ge\u00b7sen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Siegerin, was ich ihr zugewendet", "tokens": ["Die", "Sie\u00b7ge\u00b7rin", ",", "was", "ich", "ihr", "zu\u00b7ge\u00b7wen\u00b7det"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWS", "PPER", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Durch meinen Spruch, das hat sie ", "tokens": ["Durch", "mei\u00b7nen", "Spruch", ",", "das", "hat", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PDS", "VAFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}