{"textgrid.poem.42423": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der K\u00f6nig, der in Banden war", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig, der in Banden war", "tokens": ["Der", "K\u00f6\u00b7nig", ",", "der", "in", "Ban\u00b7den", "war"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Grafen von Schwerin.", "tokens": ["Des", "Gra\u00b7fen", "von", "Schwe\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Das war der K\u00f6nig Waldemar,", "tokens": ["Das", "war", "der", "K\u00f6\u00b7nig", "Wal\u00b7de\u00b7mar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verst\u00e4ubter Hermelin.", "tokens": ["Ver\u00b7st\u00e4ub\u00b7ter", "Her\u00b7me\u00b7lin", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er sah vom Gitterfenster aus", "tokens": ["Er", "sah", "vom", "Git\u00b7ter\u00b7fens\u00b7ter", "aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur Schwalbenflug und Fledermaus,", "tokens": ["Nur", "Schwal\u00b7ben\u00b7flug", "und", "Fle\u00b7der\u00b7maus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sah die Wolken ziehn.", "tokens": ["Und", "sah", "die", "Wol\u00b7ken", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bis er versprach, das ganze Land,", "tokens": ["Bis", "er", "ver\u00b7sprach", ",", "das", "gan\u00b7ze", "Land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo deutscher Stamm und Kern,", "tokens": ["Wo", "deut\u00b7scher", "Stamm", "und", "Kern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zur\u00fcckzugeben in die Hand", "tokens": ["Zu\u00b7r\u00fcck\u00b7zu\u00b7ge\u00b7ben", "in", "die", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der anerkannten Herrn.", "tokens": ["Der", "an\u00b7er\u00b7kann\u00b7ten", "Herrn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch als er los in Lenz und Flur,", "tokens": ["Doch", "als", "er", "los", "in", "Lenz", "und", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKVZ", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vergi\u00dft er bald den Friedenschwur,", "tokens": ["Ver\u00b7gi\u00dft", "er", "bald", "den", "Frie\u00b7den\u00b7schwur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und glaubt an seinen Stern.", "tokens": ["Und", "glaubt", "an", "sei\u00b7nen", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auf M\u00e4rschen lang und M\u00e4rschen hei\u00df", "tokens": ["Auf", "M\u00e4r\u00b7schen", "lang", "und", "M\u00e4r\u00b7schen", "hei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "KON", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des K\u00f6nigs Helmbusch vorn,", "tokens": ["Des", "K\u00f6\u00b7nigs", "Helm\u00b7busch", "vorn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der nickt und winkt scharlach und wei\u00df", "tokens": ["Der", "nickt", "und", "winkt", "schar\u00b7lach", "und", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gr\u00fc\u00dft den G\u00fcldensporn.", "tokens": ["Und", "gr\u00fc\u00dft", "den", "G\u00fcl\u00b7den\u00b7sporn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bis mitt' im Holstenland er h\u00e4lt,", "tokens": ["Bis", "mitt'", "im", "Hols\u00b7ten\u00b7land", "er", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Pflock einschl\u00e4gt f\u00fcr Zaum und Zelt", "tokens": ["Den", "Pflock", "ein\u00b7schl\u00e4gt", "f\u00fcr", "Zaum", "und", "Zelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im sichelreifen Korn.", "tokens": ["Im", "si\u00b7chel\u00b7rei\u00b7fen", "Korn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Gen\u00fcber schnitzt sein Widerpart", "tokens": ["Ge\u00b7n\u00fc\u00b7ber", "schnitzt", "sein", "Wi\u00b7der\u00b7part"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Pfeil sich und den Bolz,", "tokens": ["Den", "Pfeil", "sich", "und", "den", "Bolz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "KON", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Bremen Bischof Gerihardt,", "tokens": ["Von", "Bre\u00b7men", "Bi\u00b7schof", "Ge\u00b7ri\u00b7hardt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Graf Adolf, Holstenstolz.", "tokens": ["Graf", "A\u00b7dolf", ",", "Hols\u00b7ten\u00b7stolz", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und L\u00fcbeck B\u00fcrgermeister fuhr", "tokens": ["Und", "L\u00fc\u00b7beck", "B\u00fcr\u00b7ger\u00b7meis\u00b7ter", "fuhr"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem D\u00e4nen an die Gurgelschnur,", "tokens": ["Dem", "D\u00e4\u00b7nen", "an", "die", "Gur\u00b7gel\u00b7schnur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df dem die Seele schmolz.", "tokens": ["Da\u00df", "dem", "die", "See\u00b7le", "schmolz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Maria Magdalenentag,", "tokens": ["Ma\u00b7ria", "Mag\u00b7da\u00b7le\u00b7nen\u00b7tag", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mittsommersonnenschein,", "tokens": ["Mitt\u00b7som\u00b7mer\u00b7son\u00b7nen\u00b7schein", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gel\u00e4rm auf Schild und Eisendach,", "tokens": ["Ge\u00b7l\u00e4rm", "auf", "Schild", "und", "Ei\u00b7sen\u00b7dach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lanzen rasseln drein.", "tokens": ["Die", "Lan\u00b7zen", "ras\u00b7seln", "drein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch allzuscharf die Sonne sticht", "tokens": ["Doch", "all\u00b7zu\u00b7scharf", "die", "Son\u00b7ne", "sticht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Holstenvolk ins Treugesicht,", "tokens": ["Dem", "Hols\u00b7ten\u00b7volk", "ins", "Treu\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Reihen werden klein.", "tokens": ["Die", "Rei\u00b7hen", "wer\u00b7den", "klein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wie Blatt und Zweig im Bachgesp\u00fcl,", "tokens": ["Wie", "Blatt", "und", "Zweig", "im", "Bach\u00b7ge\u00b7sp\u00fcl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So treibt manch blond Gesell.", "tokens": ["So", "treibt", "manch", "blond", "Ge\u00b7sell", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Graf Adolf nur im Kampfgew\u00fchl,", "tokens": ["Graf", "A\u00b7dolf", "nur", "im", "Kampf\u00b7ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er treibt nicht von der Stell'.", "tokens": ["Er", "treibt", "nicht", "von", "der", "Stell'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und bald aus Bach wird Strom und Schaum,", "tokens": ["Und", "bald", "aus", "Bach", "wird", "Strom", "und", "Schaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NE", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nimmt Blumen mit und Ast und Baum,", "tokens": ["Nimmt", "Blu\u00b7men", "mit", "und", "Ast", "und", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "KON", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie treibt die Woge schnell!", "tokens": ["Wie", "treibt", "die", "Wo\u00b7ge", "schnell", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbmaria Magdalena, hilf,", "tokens": ["\u00bb", "ma\u00b7ria", "Mag\u00b7da\u00b7le\u00b7na", ",", "hilf", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "$,", "VVIMP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "D\u00e4mm' ab die D\u00e4nenflut,", "tokens": ["D\u00e4mm'", "ab", "die", "D\u00e4\u00b7nen\u00b7flut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du hebst zerknicktes Rohr und Schilf,", "tokens": ["Du", "hebst", "zer\u00b7knick\u00b7tes", "Rohr", "und", "Schilf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gieb uns den alten Mut,", "tokens": ["Gieb", "uns", "den", "al\u00b7ten", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Am Himmel zeig' dein Siegpanier,", "tokens": ["Am", "Him\u00b7mel", "zeig'", "dein", "Sieg\u00b7pa\u00b7nier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf immer will ich dienen dir", "tokens": ["Auf", "im\u00b7mer", "will", "ich", "die\u00b7nen", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VMFIN", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In Hulden treu und gut.\u00ab", "tokens": ["In", "Hul\u00b7den", "treu", "und", "gut", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Graf packt fest in Zeug und Riem,", "tokens": ["Der", "Graf", "packt", "fest", "in", "Zeug", "und", "Riem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sieg oder untergehn.", "tokens": ["Sieg", "o\u00b7der", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da sieh! am Himmel zeigt sich ihm", "tokens": ["Da", "sieh", "!", "am", "Him\u00b7mel", "zeigt", "sich", "ihm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$.", "APPRART", "NN", "VVFIN", "PRF", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Maria Magdalen,", "tokens": ["Ma\u00b7ria", "Mag\u00b7da\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und breitet ihren Mantel aus,", "tokens": ["Und", "brei\u00b7tet", "ih\u00b7ren", "Man\u00b7tel", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Sonne zieht ins Wolkenhaus,", "tokens": ["Die", "Son\u00b7ne", "zieht", "ins", "Wol\u00b7ken\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und k\u00fchle Winde wehn.", "tokens": ["Und", "k\u00fch\u00b7le", "Win\u00b7de", "wehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Hei! flog der Graf ins Schlachtgedr\u00e4ng,", "tokens": ["Hei", "!", "flog", "der", "Graf", "ins", "Schlacht\u00b7ge\u00b7dr\u00e4ng", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Die Axt durchbricht den Wald,", "tokens": ["Die", "Axt", "durch\u00b7bricht", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um seinen Harnisch im Gemeng", "tokens": ["Um", "sei\u00b7nen", "Har\u00b7nisch", "im", "Ge\u00b7meng"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Holstentatze krallt.", "tokens": ["Die", "Hols\u00b7ten\u00b7tat\u00b7ze", "krallt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und kratzt dem D\u00e4nen Bart und Bein,", "tokens": ["Und", "kratzt", "dem", "D\u00e4\