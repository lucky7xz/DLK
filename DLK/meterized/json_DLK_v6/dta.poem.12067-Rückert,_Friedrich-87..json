{"dta.poem.12067": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "87.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Wunder wird der Mensch empfangen und gezeugt,", "tokens": ["Ein", "Wun\u00b7der", "wird", "der", "Mensch", "emp\u00b7fan\u00b7gen", "und", "ge\u00b7zeugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Wunder lebt er, wird geboren und ges\u00e4ugt.", "tokens": ["Ein", "Wun\u00b7der", "lebt", "er", ",", "wird", "ge\u00b7bo\u00b7ren", "und", "ge\u00b7s\u00e4ugt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "VAFIN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Wunder w\u00e4chst er, h\u00f6rt und sieht, und f\u00fchlt sein Wunder,", "tokens": ["Ein", "Wun\u00b7der", "w\u00e4chst", "er", ",", "h\u00f6rt", "und", "sieht", ",", "und", "f\u00fchlt", "sein", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "VVFIN", "KON", "VVFIN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Wunder, da\u00df er denkt, und was er denkt ein Wunder.", "tokens": ["Ein", "Wun\u00b7der", ",", "da\u00df", "er", "denkt", ",", "und", "was", "er", "denkt", "ein", "Wun\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "KON", "PWS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein Wunder steht er da in aller Wunder Mitte,", "tokens": ["Ein", "Wun\u00b7der", "steht", "er", "da", "in", "al\u00b7ler", "Wun\u00b7der", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Wunder gehn ihm vor und nach auf Tritt und Schritte.", "tokens": ["Und", "Wun\u00b7der", "gehn", "ihm", "vor", "und", "nach", "auf", "Tritt", "und", "Schrit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PTKVZ", "KON", "APPR", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "An Wunder wird er so allm\u00e4hlich unwillk\u00fchrlich", "tokens": ["An", "Wun\u00b7der", "wird", "er", "so", "all\u00b7m\u00e4h\u00b7lich", "un\u00b7will\u00b7k\u00fchr\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "ADJD", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gew\u00f6hnet, da\u00df sie ihm erscheinen ganz nat\u00fcrlich.", "tokens": ["Ge\u00b7w\u00f6h\u00b7net", ",", "da\u00df", "sie", "ihm", "er\u00b7schei\u00b7nen", "ganz", "na\u00b7t\u00fcr\u00b7lich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und wunderbar erscheint ihm Ungewohntes nur,", "tokens": ["Und", "wun\u00b7der\u00b7bar", "er\u00b7scheint", "ihm", "Un\u00b7ge\u00b7wohn\u00b7tes", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der unverwundert sieht das Wunder der Natur.", "tokens": ["Der", "un\u00b7ver\u00b7wun\u00b7dert", "sieht", "das", "Wun\u00b7der", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}