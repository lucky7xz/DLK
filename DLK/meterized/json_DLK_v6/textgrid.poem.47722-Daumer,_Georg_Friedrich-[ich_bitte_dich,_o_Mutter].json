{"textgrid.poem.47722": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich bitte dich, o Mutter]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bitte dich, o Mutter,", "tokens": ["Ich", "bit\u00b7te", "dich", ",", "o", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich bitte flehentlich:", "tokens": ["Ich", "bit\u00b7te", "fle\u00b7hent\u00b7lich", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Nicht huldige dem Stolze", "tokens": ["Nicht", "hul\u00b7di\u00b7ge", "dem", "Stol\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit ungebeugtem Sinne,", "tokens": ["Mit", "un\u00b7ge\u00b7beug\u00b7tem", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nicht opfere die Tochter", "tokens": ["Nicht", "op\u00b7fe\u00b7re", "die", "Toch\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Unseligem Gewinne,", "tokens": ["Un\u00b7se\u00b7li\u00b7gem", "Ge\u00b7win\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Nicht einem ungeliebten", "tokens": ["Nicht", "ei\u00b7nem", "un\u00b7ge\u00b7lieb\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gemahl vereine mich!", "tokens": ["Ge\u00b7mahl", "ver\u00b7ei\u00b7ne", "mich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zwar werd' ich oft, o Mutter,", "tokens": ["Zwar", "werd'", "ich", "oft", ",", "o", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vereinst du diesem mich,", "tokens": ["Ver\u00b7einst", "du", "die\u00b7sem", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir zum Besuch erscheinen,", "tokens": ["Dir", "zum", "Be\u00b7such", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch nicht zu meines Herzens", "tokens": ["Doch", "nicht", "zu", "mei\u00b7nes", "Her\u00b7zens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Erg\u00f6tzen und des deinen;", "tokens": ["Er\u00b7g\u00f6t\u00b7zen", "und", "des", "dei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "PPOSAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich werde Thr\u00e4nen weinen", "tokens": ["Ich", "wer\u00b7de", "Thr\u00e4\u00b7nen", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Des Grames und des Vorwurfs", "tokens": ["Des", "Gra\u00b7mes", "und", "des", "Vor\u00b7wurfs"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Viel tausend bitterlich.", "tokens": ["Viel", "tau\u00b7send", "bit\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch giebst du mich, o Mutter,", "tokens": ["Doch", "giebst", "du", "mich", ",", "o", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Manne meiner Wahl,", "tokens": ["Dem", "Man\u00b7ne", "mei\u00b7ner", "Wahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich komme nicht so h\u00e4ufig,", "tokens": ["Ich", "kom\u00b7me", "nicht", "so", "h\u00e4u\u00b7fig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die allzu rein Begl\u00fcckte;", "tokens": ["Die", "all\u00b7zu", "rein", "Be\u00b7gl\u00fcck\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wofern ich aber komme,", "tokens": ["Wo\u00b7fern", "ich", "a\u00b7ber", "kom\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nicht eine Thr\u00e4ne wein' ich;", "tokens": ["Nicht", "ei\u00b7ne", "Thr\u00e4\u00b7ne", "wein'", "ich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich komme mit des Dankes,", "tokens": ["Ich", "kom\u00b7me", "mit", "des", "Dan\u00b7kes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Freude hellem Strahl.", "tokens": ["Der", "Freu\u00b7de", "hel\u00b7lem", "Strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}