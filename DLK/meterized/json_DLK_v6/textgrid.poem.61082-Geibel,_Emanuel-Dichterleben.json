{"textgrid.poem.61082": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Dichterleben", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wen einst die Muse mit dem Blick der Weihe", "tokens": ["Wen", "einst", "die", "Mu\u00b7se", "mit", "dem", "Blick", "der", "Wei\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mild angel\u00e4chelt, da er ward geboren,", "tokens": ["Mild", "an\u00b7ge\u00b7l\u00e4\u00b7chelt", ",", "da", "er", "ward", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "KOUS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der ist und bleibt zum Dichter auserkoren,", "tokens": ["Der", "ist", "und", "bleibt", "zum", "Dich\u00b7ter", "au\u00b7ser\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "KON", "VVFIN", "APPRART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ob auch erst sp\u00e4t der Kern zur Frucht gedeihe.", "tokens": ["Ob", "auch", "erst", "sp\u00e4t", "der", "Kern", "zur", "Frucht", "ge\u00b7dei\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADJD", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Des Lebens Pfade zeigt in bunter Reihe", "tokens": ["Des", "Le\u00b7bens", "Pfa\u00b7de", "zeigt", "in", "bun\u00b7ter", "Rei\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr ihm umsonst; er wandelt wie verloren,", "tokens": ["Ihr", "ihm", "um\u00b7sonst", ";", "er", "wan\u00b7delt", "wie", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADV", "$.", "PPER", "VVFIN", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es klingt ein ferner Klang in seinen Ohren,", "tokens": ["Es", "klingt", "ein", "fer\u00b7ner", "Klang", "in", "sei\u00b7nen", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er sinnt und sinnt, da\u00df er Gestalt ihm leihe.", "tokens": ["Er", "sinnt", "und", "sinnt", ",", "da\u00df", "er", "Ge\u00b7stalt", "ihm", "lei\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "KOUS", "PPER", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Lenz erscheint mit seinen Bl\u00fctenzweigen:", "tokens": ["Der", "Lenz", "er\u00b7scheint", "mit", "sei\u00b7nen", "Bl\u00fc\u00b7ten\u00b7zwei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er f\u00fchlt so seltsam sich vom Hauch durchdrungen;", "tokens": ["Er", "f\u00fchlt", "so", "selt\u00b7sam", "sich", "vom", "Hauch", "durch\u00b7drun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Liebe kommt: er wei\u00df nicht mehr zu schweigen.", "tokens": ["Die", "Lie\u00b7be", "kommt", ":", "er", "wei\u00df", "nicht", "mehr", "zu", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wie ein Quell, der lang ans Licht gerungen,", "tokens": ["Und", "wie", "ein", "Quell", ",", "der", "lang", "ans", "Licht", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bricht's nun hervor gewaltig, tonreich, eigen,", "tokens": ["Bricht's", "nun", "her\u00b7vor", "ge\u00b7wal\u00b7tig", ",", "ton\u00b7reich", ",", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sieh, er hat sein erstes Lied gesungen.", "tokens": ["Und", "sieh", ",", "er", "hat", "sein", "ers\u00b7tes", "Lied", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}