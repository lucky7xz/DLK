{"textgrid.poem.58986": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "205. Abgeschiedene Bruderliebe", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieb sehr, doch liebe rein die Frommen,", "tokens": ["Lieb", "sehr", ",", "doch", "lie\u00b7be", "rein", "die", "From\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADV", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df sie, wie Gott will, gehn und kommen;", "tokens": ["La\u00df", "sie", ",", "wie", "Gott", "will", ",", "gehn", "und", "kom\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "NN", "VMFIN", "$,", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bei Frommen bleib nur Gott gemein,", "tokens": ["Bei", "From\u00b7men", "bleib", "nur", "Gott", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sonst bleibst du, wann sie gehn, allein!", "tokens": ["Sonst", "bleibst", "du", ",", "wann", "sie", "gehn", ",", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVINF", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}