{"textgrid.poem.40591": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das D\u00f6rflein Rosenhaida", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das D\u00f6rflein Rosenhaida", "tokens": ["Das", "D\u00f6r\u00b7flein", "Ro\u00b7sen\u00b7hai\u00b7da"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lag mitten im Wiesengr\u00fcn,", "tokens": ["Lag", "mit\u00b7ten", "im", "Wie\u00b7sen\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Viel duftige, gl\u00fchende Rosen", "tokens": ["Viel", "duf\u00b7ti\u00b7ge", ",", "gl\u00fc\u00b7hen\u00b7de", "Ro\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sah man auf der Wiese bl\u00fchn.", "tokens": ["Sah", "man", "auf", "der", "Wie\u00b7se", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da kam einst aus dem Dorfe", "tokens": ["Da", "kam", "einst", "aus", "dem", "Dor\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein dicker Bauersmann;", "tokens": ["Ein", "di\u00b7cker", "Bau\u00b7ers\u00b7mann", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er wetzte seine Sense", "tokens": ["Er", "wetz\u00b7te", "sei\u00b7ne", "Sen\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und hub zu m\u00e4hen an.", "tokens": ["Und", "hub", "zu", "m\u00e4\u00b7hen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er m\u00e4hte Gras und Rosen, \u2013", "tokens": ["Er", "m\u00e4h\u00b7te", "Gras", "und", "Ro\u00b7sen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O la\u00df die Rosen verschont!", "tokens": ["O", "la\u00df", "die", "Ro\u00b7sen", "ver\u00b7schont", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Bedenke, da\u00df dahinter", "tokens": ["Be\u00b7den\u00b7ke", ",", "da\u00df", "da\u00b7hin\u00b7ter"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gar oft die Schlange wohnt!", "tokens": ["Gar", "oft", "die", "Schlan\u00b7ge", "wohnt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er m\u00e4hte Gras und Rosen,", "tokens": ["Er", "m\u00e4h\u00b7te", "Gras", "und", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da zischte die Schlang' auf ihn,", "tokens": ["Da", "zischte", "die", "Schlang'", "auf", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Gift traf ihn zu Tode,", "tokens": ["Ihr", "Gift", "traf", "ihn", "zu", "To\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur Erde taumelt er hin.", "tokens": ["Zur", "Er\u00b7de", "tau\u00b7melt", "er", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Der Pfarrer von Rosenhaida,", "tokens": ["Der", "Pfar\u00b7rer", "von", "Ro\u00b7sen\u00b7hai\u00b7da", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit Stol' und Chorgewand,", "tokens": ["Mit", "Stol'", "und", "Chor\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In heiligem Seeleneifer", "tokens": ["In", "hei\u00b7li\u00b7gem", "See\u00b7len\u00b7ei\u00b7fer"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kam schnell herbeigerannt.", "tokens": ["Kam", "schnell", "her\u00b7bei\u00b7ge\u00b7rannt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ach, wie die Stirn ihm triefet!", "tokens": ["Ach", ",", "wie", "die", "Stirn", "ihm", "trie\u00b7fet", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ach, wie sein Athem keucht!", "tokens": ["Ach", ",", "wie", "sein", "A\u00b7them", "keucht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er rennt durch Dorn und Stoppeln,", "tokens": ["Er", "rennt", "durch", "Dorn", "und", "Stop\u00b7peln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sinkt um, st\u00f6hnt und erbleicht.", "tokens": ["Sinkt", "um", ",", "st\u00f6hnt", "und", "er\u00b7bleicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "KON", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Die Bauern von Rosenhaida,", "tokens": ["Die", "Bau\u00b7ern", "von", "Ro\u00b7sen\u00b7hai\u00b7da", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die liefen eilig herbei", "tokens": ["Die", "lie\u00b7fen", "ei\u00b7lig", "her\u00b7bei"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und taumelten vor Schrecken", "tokens": ["Und", "tau\u00b7mel\u00b7ten", "vor", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Zu Boden nach der Reih'.", "tokens": ["Zu", "Bo\u00b7den", "nach", "der", "Reih'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Wittwen zu Rosenhaida,", "tokens": ["Die", "Witt\u00b7wen", "zu", "Ro\u00b7sen\u00b7hai\u00b7da", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die weinten Tag und Nacht,", "tokens": ["Die", "wein\u00b7ten", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis sie der Todesengel", "tokens": ["Bis", "sie", "der", "To\u00b7de\u00b7sen\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu ihren M\u00e4nnern gebracht.", "tokens": ["Zu", "ih\u00b7ren", "M\u00e4n\u00b7nern", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Die Waisen zu Rosenhaida,", "tokens": ["Die", "Wai\u00b7sen", "zu", "Ro\u00b7sen\u00b7hai\u00b7da", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die rangen die H\u00e4ndlein drob,", "tokens": ["Die", "ran\u00b7gen", "die", "H\u00e4nd\u00b7lein", "drob", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bis sie der Vater der Waisen", "tokens": ["Bis", "sie", "der", "Va\u00b7ter", "der", "Wai\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zu sich empor auch hob.", "tokens": ["Zu", "sich", "em\u00b7por", "auch", "hob", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "PTKVZ", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Der K\u00fcster von Rosenhaida", "tokens": ["Der", "K\u00fcs\u00b7ter", "von", "Ro\u00b7sen\u00b7hai\u00b7da"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sang nun ihr Seelenamt,", "tokens": ["Sang", "nun", "ihr", "See\u00b7len\u00b7amt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis ihm vom vielen Singen", "tokens": ["Bis", "ihm", "vom", "vie\u00b7len", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPRART", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zuletzt die Lung' erlahmt.", "tokens": ["Zu\u00b7letzt", "die", "Lung'", "er\u00b7lahmt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Als er's dem Letzten gesungen,", "tokens": ["Als", "er's", "dem", "Letz\u00b7ten", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ging ihm der Athem aus;", "tokens": ["Ging", "ihm", "der", "A\u00b7them", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer wird ihm seines singen,", "tokens": ["Wer", "wird", "ihm", "sei\u00b7nes", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer bringt den Alten nach Haus?", "tokens": ["Wer", "bringt", "den", "Al\u00b7ten", "nach", "Haus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.12": {"line.1": {"text": "Es blieb der Todtengr\u00e4ber,", "tokens": ["Es", "blieb", "der", "Tod\u00b7ten\u00b7gr\u00e4\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch der kam nun ums Brod;", "tokens": ["Doch", "der", "kam", "nun", "ums", "Brod", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verloren alle Kunden!", "tokens": ["Ver\u00b7lo\u00b7ren", "al\u00b7le", "Kun\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da starb er den Hungertod.", "tokens": ["Da", "starb", "er", "den", "Hun\u00b7ger\u00b7tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Oed' ist's in Rosenhaida,", "tokens": ["O\u00b7ed'", "ist's", "in", "Ro\u00b7sen\u00b7hai\u00b7da", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "W\u00fcst stehn die H\u00e4userreihn,", "tokens": ["W\u00fcst", "stehn", "die", "H\u00e4u\u00b7ser\u00b7reihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Mauern brechen zusammen,", "tokens": ["Die", "Mau\u00b7ern", "bre\u00b7chen", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die D\u00e4cher st\u00fcrzen ein.", "tokens": ["Die", "D\u00e4\u00b7cher", "st\u00fcr\u00b7zen", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Gem\u00e4hte Rosen haben", "tokens": ["Ge\u00b7m\u00e4h\u00b7te", "Ro\u00b7sen", "ha\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Solch Unheil einst gebracht; \u2013", "tokens": ["Solch", "Un\u00b7heil", "einst", "ge\u00b7bracht", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr, die ihr m\u00e4ht auf Wiesen,", "tokens": ["Ihr", ",", "die", "ihr", "m\u00e4ht", "auf", "Wie\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gebt auf die Rosen Acht!", "tokens": ["Gebt", "auf", "die", "Ro\u00b7sen", "Acht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Nun trauert Rosenhaida", "tokens": ["Nun", "trau\u00b7ert", "Ro\u00b7sen\u00b7hai\u00b7da"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Schutt und Tr\u00fcmmern dort,", "tokens": ["In", "Schutt", "und", "Tr\u00fcm\u00b7mern", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch auf der Wiese drau\u00dfen", "tokens": ["Doch", "auf", "der", "Wie\u00b7se", "drau\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bl\u00fchn lustig die Rosen fort.", "tokens": ["Bl\u00fchn", "lus\u00b7tig", "die", "Ro\u00b7sen", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}