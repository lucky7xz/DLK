{"textgrid.poem.38818": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir h\u00f6ren gro\u00dfe Wunder", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir h\u00f6ren gro\u00dfe Wunder", "tokens": ["Wir", "h\u00f6\u00b7ren", "gro\u00b7\u00dfe", "Wun\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom klugen Weland sagen,", "tokens": ["Vom", "klu\u00b7gen", "We\u00b7land", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein Vater, Riese Vade", "tokens": ["Sein", "Va\u00b7ter", ",", "Rie\u00b7se", "Va\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NE", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bracht' ihn in jungen Tagen", "tokens": ["Bracht'", "ihn", "in", "jun\u00b7gen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zu Mimer, dem verst\u00e4ndgen,", "tokens": ["Zu", "Mi\u00b7mer", ",", "dem", "ver\u00b7st\u00e4nd\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Schmid im dunkeln Than,", "tokens": ["Dem", "Schmid", "im", "dun\u00b7keln", "Than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann kam Weland zu Zwergen,", "tokens": ["Dann", "kam", "We\u00b7land", "zu", "Zwer\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wo er mehr Kunst gewann.", "tokens": ["Wo", "er", "mehr", "Kunst", "ge\u00b7wann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zum K\u00f6nig Nidung ging er,", "tokens": ["Zum", "K\u00f6\u00b7nig", "Ni\u00b7dung", "ging", "er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein Ruhm war weit bekannt,", "tokens": ["Sein", "Ruhm", "war", "weit", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er wirkte sch\u00f6ne Schwerdter,", "tokens": ["Er", "wirk\u00b7te", "sch\u00f6\u00b7ne", "Schwerd\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und manchen Schildesrand.", "tokens": ["Und", "man\u00b7chen", "Schil\u00b7des\u00b7rand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und Messer, wunderk\u00fcnstlich,", "tokens": ["Und", "Mes\u00b7ser", ",", "wun\u00b7der\u00b7k\u00fcnst\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auch Becher goldner Pracht,", "tokens": ["Auch", "Be\u00b7cher", "gold\u00b7ner", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er wurde f\u00fcr den kl\u00fcgsten", "tokens": ["Er", "wur\u00b7de", "f\u00fcr", "den", "kl\u00fcgs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und besten Schmid geacht't.", "tokens": ["Und", "bes\u00b7ten", "Schmid", "ge\u00b7acht'", "t."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["KON", "ADJA", "NN", "VVPP", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Durch Welands Weisheit siegte", "tokens": ["Durch", "We\u00b7lands", "Weis\u00b7heit", "sieg\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nidung, vom Feind gequ\u00e4lt,", "tokens": ["Ni\u00b7dung", ",", "vom", "Feind", "ge\u00b7qu\u00e4lt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Drum ward des K\u00f6nigs Tochter", "tokens": ["Drum", "ward", "des", "K\u00f6\u00b7nigs", "Toch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dankbar dem Mann verm\u00e4hlt.", "tokens": ["Dank\u00b7bar", "dem", "Mann", "ver\u00b7m\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Als nun der Feind geschlagen,", "tokens": ["Als", "nun", "der", "Feind", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weland zum K\u00f6nig trat,", "tokens": ["We\u00b7land", "zum", "K\u00f6\u00b7nig", "trat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Doch z\u00fcrnend h\u00f6rte dieser", "tokens": ["Doch", "z\u00fcr\u00b7nend", "h\u00f6r\u00b7te", "die\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PDAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht, was der Schmid ihn bat.", "tokens": ["Nicht", ",", "was", "der", "Schmid", "ihn", "bat", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er stie\u00df ihn hart zur\u00fccke,", "tokens": ["Er", "stie\u00df", "ihn", "hart", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du kannst nicht seyn mein Sohn,", "tokens": ["Du", "kannst", "nicht", "seyn", "mein", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VAINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Kind find't ander Gl\u00fccke! \u2013", "tokens": ["Mein", "Kind", "find't", "an\u00b7der", "Gl\u00fc\u00b7cke", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So gebt in meinen Lohn,", "tokens": ["So", "gebt", "in", "mei\u00b7nen", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "La\u00dft mich von dannen ziehen,", "tokens": ["La\u00dft", "mich", "von", "dan\u00b7nen", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Rief Weland, fern von hier;", "tokens": ["Rief", "We\u00b7land", ",", "fern", "von", "hier", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "APPR", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du bleibst hier, rief der K\u00f6nig,", "tokens": ["Du", "bleibst", "hier", ",", "rief", "der", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Fortgehn hindr' ich dir.", "tokens": ["Das", "Fort\u00b7gehn", "hindr'", "ich", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Am Fu\u00df lie\u00df er die Sehnen", "tokens": ["Am", "Fu\u00df", "lie\u00df", "er", "die", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihm schneiden; nun geh hin,", "tokens": ["Ihm", "schnei\u00b7den", ";", "nun", "geh", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht fliehst du, sollt' ich w\u00e4hnen,", "tokens": ["Nicht", "fliehst", "du", ",", "sollt'", "ich", "w\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sprach er mit falschem Sinn.", "tokens": ["Sprach", "er", "mit", "fal\u00b7schem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Hinkend und ungemuthet", "tokens": ["Hin\u00b7kend", "und", "un\u00b7ge\u00b7mu\u00b7thet"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "KON", "ADJD"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Schlich Weland in sein Haus,", "tokens": ["Schlich", "We\u00b7land", "in", "sein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie schwach er war, doch Rache", "tokens": ["Wie", "schwach", "er", "war", ",", "doch", "Ra\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "VAFIN", "$,", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sann seine List ihm aus.", "tokens": ["Sann", "sei\u00b7ne", "List", "ihm", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Um schmieden ihn zu sehen", "tokens": ["Um", "schmie\u00b7den", "ihn", "zu", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ADJA", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Sohn des K\u00f6nigs kam,", "tokens": ["Der", "Sohn", "des", "K\u00f6\u00b7nigs", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Beim Ambos stand der Listge,", "tokens": ["Beim", "Am\u00b7bos", "stand", "der", "List\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er schnell den Knaben nahm,", "tokens": ["Er", "schnell", "den", "Kna\u00b7ben", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Und t\u00f6dtete ihn heimlich,", "tokens": ["Und", "t\u00f6d\u00b7te\u00b7te", "ihn", "heim\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dann fa\u00dft' er sein Gebein,", "tokens": ["Dann", "fa\u00dft'", "er", "sein", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und go\u00df von Erz und Silber", "tokens": ["Und", "go\u00df", "von", "Erz", "und", "Sil\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Viel Leuchter sch\u00f6n und fein,", "tokens": ["Viel", "Leuch\u00b7ter", "sch\u00f6n", "und", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Die Knochen in den S\u00e4ulen,", "tokens": ["Die", "Kno\u00b7chen", "in", "den", "S\u00e4u\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Sch\u00e4del nahm er dann,", "tokens": ["Den", "Sch\u00e4\u00b7del", "nahm", "er", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es machte den zum Becher", "tokens": ["Es", "mach\u00b7te", "den", "zum", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Gold der kluge Mann.", "tokens": ["Von", "Gold", "der", "klu\u00b7ge", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Man suchte wohl den Knaben,", "tokens": ["Man", "such\u00b7te", "wohl", "den", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie fragten auch Weland;", "tokens": ["Sie", "frag\u00b7ten", "auch", "We\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der sprach: ich sah ihn nimmer,", "tokens": ["Der", "sprach", ":", "ich", "sah", "ihn", "nim\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er ist zum Wald gerannt.", "tokens": ["Er", "ist", "zum", "Wald", "ge\u00b7rannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Des K\u00f6nigs sch\u00f6ne Tochter", "tokens": ["Des", "K\u00f6\u00b7nigs", "sch\u00f6\u00b7ne", "Toch\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein junges M\u00e4gdelein", "tokens": ["Ein", "jun\u00b7ges", "M\u00e4g\u00b7del\u00b7ein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Trug einen Ring von Golde", "tokens": ["Trug", "ei\u00b7nen", "Ring", "von", "Gol\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit manchem Edelstein,", "tokens": ["Mit", "man\u00b7chem", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Den ihr der Vater schenkte", "tokens": ["Den", "ihr", "der", "Va\u00b7ter", "schenk\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und gern ihn schimmern sah", "tokens": ["Und", "gern", "ihn", "schim\u00b7mern", "sah"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "VVINF", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An ihren wei\u00dfen Fingern,", "tokens": ["An", "ih\u00b7ren", "wei\u00b7\u00dfen", "Fin\u00b7gern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Garten es geschah", "tokens": ["Im", "Gar\u00b7ten", "es", "ge\u00b7schah"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Als sie dort Blumen suchte,", "tokens": ["Als", "sie", "dort", "Blu\u00b7men", "such\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ihr der Ring zerbrach,", "tokens": ["Da\u00df", "ihr", "der", "Ring", "zer\u00b7brach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Jungfrau rang die H\u00e4nde,", "tokens": ["Die", "Jung\u00b7frau", "rang", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und klagte Weh! und Ach!", "tokens": ["Und", "klag\u00b7te", "Weh", "!", "und", "Ach", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$.", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Sie furchte ihres Vaters", "tokens": ["Sie", "furch\u00b7te", "ih\u00b7res", "Va\u00b7ters"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bestrafung, seinen Zorn,", "tokens": ["Be\u00