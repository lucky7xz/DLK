"word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geh heimlich zum Weland,", "tokens": ["Geh", "heim\u00b7lich", "zum", "We\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Von seinen klugen Sinnen", "tokens": ["Von", "sei\u00b7nen", "klu\u00b7gen", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird bald dein Leid gewandt.", "tokens": ["Wird", "bald", "dein", "Leid", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Sie trat in seine Schmiede,", "tokens": ["Sie", "trat", "in", "sei\u00b7ne", "Schmie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und klagte ihre Noth,", "tokens": ["Und", "klag\u00b7te", "ih\u00b7re", "Noth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er nahm den Ring und fachte", "tokens": ["Er", "nahm", "den", "Ring", "und", "fach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schnell an das Feuer roth,", "tokens": ["Schnell", "an", "das", "Feu\u00b7er", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Er f\u00fcgte ihn zusammen", "tokens": ["Er", "f\u00fcg\u00b7te", "ihn", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und schmolz wohl Gold und Erz,", "tokens": ["Und", "schmolz", "wohl", "Gold", "und", "Erz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie sah froh in die Flammen,", "tokens": ["Sie", "sah", "froh", "in", "die", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vergessen war ihr Schmerz.", "tokens": ["Ver\u00b7ges\u00b7sen", "war", "ihr", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "Sie l\u00e4chelte ihm freundlich,", "tokens": ["Sie", "l\u00e4\u00b7chel\u00b7te", "ihm", "freund\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da schlo\u00df der Schmid Weland", "tokens": ["Da", "schlo\u00df", "der", "Schmid", "We\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Schnell seine feste Th\u00fcre", "tokens": ["Schnell", "sei\u00b7ne", "fes\u00b7te", "Th\u00fc\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und nahm sie bei der Hand.", "tokens": ["Und", "nahm", "sie", "bei", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.24": {"line.1": {"text": "Er zwang das M\u00e4gdlein dorten,", "tokens": ["Er", "zwang", "das", "M\u00e4gd\u00b7lein", "dor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Nidung ihm versprach,", "tokens": ["Die", "Ni\u00b7dung", "ihm", "ver\u00b7sprach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie weinte in den N\u00f6then,", "tokens": ["Sie", "wein\u00b7te", "in", "den", "N\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sah nie schlimmern Tag.", "tokens": ["Und", "sah", "nie", "schlim\u00b7mern", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.25": {"line.1": {"text": "Sie kehrte heim zum Vater,", "tokens": ["Sie", "kehr\u00b7te", "heim", "zum", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie hat ihm nichts gesagt.", "tokens": ["Sie", "hat", "ihm", "nichts", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald f\u00fchlte sie sich schwanger,", "tokens": ["Bald", "f\u00fchl\u00b7te", "sie", "sich", "schwan\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da wurde viel geklagt.", "tokens": ["Da", "wur\u00b7de", "viel", "ge\u00b7klagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.26": {"line.1": {"text": "So hatte sich ger\u00e4chet", "tokens": ["So", "hat\u00b7te", "sich", "ge\u00b7r\u00e4\u00b7chet"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weland, der kluge Mann,", "tokens": ["We\u00b7land", ",", "der", "klu\u00b7ge", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Drauf macht er gro\u00dfe Fl\u00fcgel", "tokens": ["Drauf", "macht", "er", "gro\u00b7\u00dfe", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und band sich diese an,", "tokens": ["Und", "band", "sich", "die\u00b7se", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PDS", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.27": {"line.1": {"text": "So stand er auf der Zinne,", "tokens": ["So", "stand", "er", "auf", "der", "Zin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Leute riefen: seht!", "tokens": ["Die", "Leu\u00b7te", "rie\u00b7fen", ":", "seht", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weland ist nun ein Vogel,", "tokens": ["We\u00b7land", "ist", "nun", "ein", "Vo\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er fliegend von uns geht.", "tokens": ["Er", "flie\u00b7gend", "von", "uns", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.28": {"line.1": {"text": "Auch Nidung kam, der K\u00f6nig,", "tokens": ["Auch", "Ni\u00b7dung", "kam", ",", "der", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es nahm ihn Wunder gro\u00df,", "tokens": ["Es", "nahm", "ihn", "Wun\u00b7der", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weland rief: fort ich fliege,", "tokens": ["We\u00b7land", "rief", ":", "fort", "ich", "flie\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "PTKVZ", "PPER", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Du bist nun erbelos.", "tokens": ["Du", "bist", "nun", "er\u00b7be\u00b7los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.29": {"line.1": {"text": "Den Sohn hab' ich get\u00f6dtet,", "tokens": ["Den", "Sohn", "hab'", "ich", "ge\u00b7t\u00f6d\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Leuchtern, golden fein,", "tokens": ["In", "Leuch\u00b7tern", ",", "gol\u00b7den", "fein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die deine Tafel schm\u00fccken", "tokens": ["Die", "dei\u00b7ne", "Ta\u00b7fel", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schmolz ich des Sohns Gebein.", "tokens": ["Schmolz", "ich", "des", "Sohns", "Ge\u00b7bein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.30": {"line.1": {"text": "Auch hast du einen Becher,", "tokens": ["Auch", "hast", "du", "ei\u00b7nen", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Leuchtend von Golde roth,", "tokens": ["Leuch\u00b7tend", "von", "Gol\u00b7de", "roth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Du trinkst aus seinem Sch\u00e4del,", "tokens": ["Du", "trinkst", "aus", "sei\u00b7nem", "Sch\u00e4\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und kennst nicht deine Noth.", "tokens": ["Und", "kennst", "nicht", "dei\u00b7ne", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.31": {"line.1": {"text": "Und deine sch\u00f6ne Tochter", "tokens": ["Und", "dei\u00b7ne", "sch\u00f6\u00b7ne", "Toch\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geht nicht mehr Jungfrau dir,", "tokens": ["Geht", "nicht", "mehr", "Jung\u00b7frau", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PIAT", "NN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie zwang ich dir erz\u00fcrnet,", "tokens": ["Sie", "zwang", "ich", "dir", "er\u00b7z\u00fcr\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie tr\u00e4gt ein Kind von mir.", "tokens": ["Sie", "tr\u00e4gt", "ein", "Kind", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.32": {"line.1": {"text": "Der K\u00f6nig nahm den Bogen,", "tokens": ["Der", "K\u00f6\u00b7nig", "nahm", "den", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Legt' auf den scharfen Pfeil,", "tokens": ["Legt'", "auf", "den", "schar\u00b7fen", "Pfeil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Alle Ritter im Zorne", "tokens": ["Al\u00b7le", "Rit\u00b7ter", "im", "Zor\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPRART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Spannten in grimmer Eil.", "tokens": ["Spann\u00b7ten", "in", "grim\u00b7mer", "Eil", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.33": {"line.1": {"text": "Da flogen scharfe Stralen", "tokens": ["Da", "flo\u00b7gen", "schar\u00b7fe", "Stra\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und schossen durch das Licht,", "tokens": ["Und", "schos\u00b7sen", "durch", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch Weland hob die Schwingen,", "tokens": ["Doch", "We\u00b7land", "hob", "die", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Eisen traf ihn nicht.", "tokens": ["Kein", "Ei\u00b7sen", "traf", "ihn", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.34": {"line.1": {"text": "Er flog mit klugen Sinnen", "tokens": ["Er", "flog", "mit", "klu\u00b7gen", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf seines Vaters Schlo\u00df,", "tokens": ["Auf", "sei\u00b7nes", "Va\u00b7ters", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Nidung starb, der K\u00f6nig,", "tokens": ["Und", "Ni\u00b7dung", "starb", ",", "der", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Herzeleid war gro\u00df:", "tokens": ["Das", "Her\u00b7ze\u00b7leid", "war", "gro\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.35": {"line.1": {"text": "Sein j\u00fcngster Sohn ward K\u00f6nig,", "tokens": ["Sein", "j\u00fcngs\u00b7ter", "Sohn", "ward", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Schwester sein gebar", "tokens": ["Die", "Schwes\u00b7ter", "sein", "ge\u00b7bar"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wittich, den k\u00fchnen Helden", "tokens": ["Wit\u00b7tich", ",", "den", "k\u00fch\u00b7nen", "Hel\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ART", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Noch in demselben Jahr.", "tokens": ["Noch", "in", "dem\u00b7sel\u00b7ben", "Jahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}