{"textgrid.poem.24961": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Wiedersehen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ward mir, Freundin, meinem Geist Verwandte,", "tokens": ["Wie", "ward", "mir", ",", "Freun\u00b7din", ",", "mei\u00b7nem", "Geist", "Ver\u00b7wand\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "NN", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als mich dein Auge wieder traf! Mir deuchte,", "tokens": ["Als", "mich", "dein", "Au\u00b7ge", "wie\u00b7der", "traf", "!", "Mir", "deuch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df wieder mir die goldne Sonne leuchte,", "tokens": ["Da\u00df", "wie\u00b7der", "mir", "die", "gold\u00b7ne", "Son\u00b7ne", "leuch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die an dem Himmel meiner Kindheit brannte.", "tokens": ["Die", "an", "dem", "Him\u00b7mel", "mei\u00b7ner", "Kind\u00b7heit", "brann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O diese Stimme war die altbekannte,", "tokens": ["O", "die\u00b7se", "Stim\u00b7me", "war", "die", "alt\u00b7be\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die mir zuerst der Seele Dunkel scheuchte,", "tokens": ["Die", "mir", "zu\u00b7erst", "der", "See\u00b7le", "Dun\u00b7kel", "scheuch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zuerst ins Auge lockte heil'ge Feuchte", "tokens": ["Zu\u00b7erst", "ins", "Au\u00b7ge", "lock\u00b7te", "heil'\u00b7ge", "Feuch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mir die ersten heil'gen Namen nannte!", "tokens": ["Und", "mir", "die", "ers\u00b7ten", "heil'\u00b7gen", "Na\u00b7men", "nann\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Viel irrt' ich, o ich f\u00fchl's mit tiefem Harme,", "tokens": ["Viel", "irrt'", "ich", ",", "o", "ich", "f\u00fchl's", "mit", "tie\u00b7fem", "Har\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "FM", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Viel, weise Freundin, ab von deinen Lehren,", "tokens": ["Viel", ",", "wei\u00b7se", "Freun\u00b7din", ",", "ab", "von", "dei\u00b7nen", "Leh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und oft verga\u00df ich dein im Lebensschwarme.", "tokens": ["Und", "oft", "ver\u00b7ga\u00df", "ich", "dein", "im", "Le\u00b7bens\u00b7schwar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Vergieb! Vergieb! Sieh diese Reuez\u00e4hren,", "tokens": ["Ver\u00b7gieb", "!", "Ver\u00b7gieb", "!", "Sieh", "die\u00b7se", "Reu\u00b7ez\u00b7\u00e4h\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "NN", "$.", "NE", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und den Verlornen la\u00df in deine Arme", "tokens": ["Und", "den", "Ver\u00b7lor\u00b7nen", "la\u00df", "in", "dei\u00b7ne", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie zu der sel'gen Kindheit wiederkehren.", "tokens": ["Wie", "zu", "der", "sel'\u00b7gen", "Kind\u00b7heit", "wie\u00b7der\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}