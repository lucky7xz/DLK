{"textgrid.poem.66164": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Gedicht Schubarts f\u00fcr seine Enkelin auf den Geburtstag ihres Vaters", "genre": "verse", "period": "N.A.", "pub_year": 1791, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fcck und Heil am Tage,", "tokens": ["Gl\u00fcck", "und", "Heil", "am", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Der dich einst gebar!", "tokens": ["Der", "dich", "einst", "ge\u00b7bar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Vater, was ich sage,", "tokens": ["Va\u00b7ter", ",", "was", "ich", "sa\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ist so fromm und wahr.", "tokens": ["Ist", "so", "fromm", "und", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Lies auf diesem Bl\u00e4ttchen,", "tokens": ["Lies", "auf", "die\u00b7sem", "Bl\u00e4tt\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Das mein Herz dir giebt,", "tokens": ["Das", "mein", "Herz", "dir", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wie dein kleines M\u00e4dchen", "tokens": ["Wie", "dein", "klei\u00b7nes", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dich so herzlich liebt.", "tokens": ["Dich", "so", "herz\u00b7lich", "liebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "K\u00fcsse deine Kleine,", "tokens": ["K\u00fcs\u00b7se", "dei\u00b7ne", "Klei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sieh ihr ins Gesicht;", "tokens": ["Sieh", "ihr", "ins", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Liebe, wie die meine,", "tokens": ["Lie\u00b7be", ",", "wie", "die", "mei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "PPOSAT", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Schildert kein Gedicht.", "tokens": ["Schil\u00b7dert", "kein", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Wo die Sternlein stehen,", "tokens": ["Wo", "die", "Stern\u00b7lein", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ist ein gro\u00dfer Mann,", "tokens": ["Ist", "ein", "gro\u00b7\u00dfer", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der, was Kinder flehen,", "tokens": ["Der", ",", "was", "Kin\u00b7der", "fle\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWS", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Auch erf\u00fcllen kann.", "tokens": ["Auch", "er\u00b7f\u00fcl\u00b7len", "kann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "O, der wird dir geben", "tokens": ["O", ",", "der", "wird", "dir", "ge\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "\u2013 Denn ich bitt' ihn ja \u2013", "tokens": ["\u2013", "Denn", "ich", "bitt'", "ihn", "ja", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "VVFIN", "PPER", "ADV", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Langes, frohes Leben,", "tokens": ["Lan\u00b7ges", ",", "fro\u00b7hes", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Herziger Papa.", "tokens": ["Her\u00b7zi\u00b7ger", "Pa\u00b7pa", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+---+", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Und mir wird's gelingen,", "tokens": ["Und", "mir", "wird's", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Immer wohlgemuth", "tokens": ["Im\u00b7mer", "wohl\u00b7ge\u00b7muth"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Um dich her zu springen,", "tokens": ["Um", "dich", "her", "zu", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APZR", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wie das L\u00e4mmlein thut.", "tokens": ["Wie", "das", "L\u00e4mm\u00b7lein", "thut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Will dich streicheln, herzen,", "tokens": ["Will", "dich", "strei\u00b7cheln", ",", "her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Will mit M\u00e4dchenlist", "tokens": ["Will", "mit", "M\u00e4d\u00b7chen\u00b7list"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Um dich h\u00fcpfen, scherzen,", "tokens": ["Um", "dich", "h\u00fcp\u00b7fen", ",", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUI", "PRF", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wenn du traurig bist.", "tokens": ["Wenn", "du", "trau\u00b7rig", "bist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Will dein Knie umschlingen,", "tokens": ["Will", "dein", "Knie", "um\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Bester Vater, du!", "tokens": ["Bes\u00b7ter", "Va\u00b7ter", ",", "du", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Will ein Lied dir singen,", "tokens": ["Will", "ein", "Lied", "dir", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und du geigst dazu.", "tokens": ["Und", "du", "geigst", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PAV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Heisa! Welche Freude!", "tokens": ["Hei\u00b7sa", "!", "Wel\u00b7che", "Freu\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "PWAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Vater, tanz' mit mir!", "tokens": ["Va\u00b7ter", ",", "tanz'", "mit", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Alles freut sich heute", "tokens": ["Al\u00b7les", "freut", "sich", "heu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PRF", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Innig \u00fcber dir.", "tokens": ["In\u00b7nig", "\u00fc\u00b7ber", "dir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Mama w\u00fcnscht dir Gl\u00fccke,", "tokens": ["Ma\u00b7ma", "w\u00fcnscht", "dir", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und der Ludwig zeigt", "tokens": ["Und", "der", "Lud\u00b7wig", "zeigt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NE", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Schon in seinem Blicke,", "tokens": ["Schon", "in", "sei\u00b7nem", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Was der Mund verschweigt.", "tokens": ["Was", "der", "Mund", "ver\u00b7schweigt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}