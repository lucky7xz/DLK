{"textgrid.poem.53036": {"metadata": {"author": {"name": "Pyra, Jakob Immanuel", "birth": "N.A.", "death": "N.A."}, "title": "1. Gem\u00e4lde des Schreckens \u00fcber den schnellen Todt", "genre": "verse", "period": "N.A.", "pub_year": 1729, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als wenn ein reiches Schiff mit Erzbewehrtem Schnabel", "tokens": ["Als", "wenn", "ein", "rei\u00b7ches", "Schiff", "mit", "Erz\u00b7be\u00b7wehr\u00b7tem", "Schna\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In seinem frohen Lauf die blaue Bahn zertheilt:", "tokens": ["In", "sei\u00b7nem", "fro\u00b7hen", "Lauf", "die", "blau\u00b7e", "Bahn", "zer\u00b7theilt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des Zephirs holder Hauch die T\u00fccher spielend bl\u00e4het,", "tokens": ["Des", "Ze\u00b7phirs", "hol\u00b7der", "Hauch", "die", "T\u00fc\u00b7cher", "spie\u00b7lend", "bl\u00e4\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und der Pilot vom Bord in die cristallne See", "tokens": ["Und", "der", "Pi\u00b7lot", "vom", "Bord", "in", "die", "cris\u00b7tall\u00b7ne", "See"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Voll sichrer Hofnung schaut: indem der Sonne Kugel", "tokens": ["Voll", "sich\u00b7rer", "Hof\u00b7nung", "schaut", ":", "in\u00b7dem", "der", "Son\u00b7ne", "Ku\u00b7gel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "$.", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hoch \u00fcber seinem Haupt, im Meer sich spiegelnd, schwebt,", "tokens": ["Hoch", "\u00fc\u00b7ber", "sei\u00b7nem", "Haupt", ",", "im", "Meer", "sich", "spie\u00b7gelnd", ",", "schwebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,", "APPRART", "NN", "PRF", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und das vergn\u00fcgte Chor der schimmernden Delphinen", "tokens": ["Und", "das", "ver\u00b7gn\u00fcg\u00b7te", "Chor", "der", "schim\u00b7mern\u00b7den", "Del\u00b7phi\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In dem besch\u00e4umten Kreis, um seines Nachens Bauch", "tokens": ["In", "dem", "be\u00b7sch\u00e4um\u00b7ten", "Kreis", ",", "um", "sei\u00b7nes", "Na\u00b7chens", "Bauch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KOUI", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sich scherzend jagt und flieht, bald aber auch zerstreut,", "tokens": ["Sich", "scher\u00b7zend", "jagt", "und", "flieht", ",", "bald", "a\u00b7ber", "auch", "zer\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "KON", "VVFIN", "$,", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und schnell in Tief' und Kluft und schlammigt Schilf verbirgt.", "tokens": ["Und", "schnell", "in", "Tie\u00b7f'", "und", "Kluft", "und", "schlam\u00b7migt", "Schilf", "ver\u00b7birgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NE", "KON", "NN", "KON", "VVFIN", "NE", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Und dann die schwebenden verd\u00fcnnten Feuchtigkeiten", "tokens": ["Und", "dann", "die", "schwe\u00b7ben\u00b7den", "ver\u00b7d\u00fcnn\u00b7ten", "Feuch\u00b7tig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "In dicke Wolken sich sogleich zusammen ziehn", "tokens": ["In", "di\u00b7cke", "Wol\u00b7ken", "sich", "sog\u00b7leich", "zu\u00b7sam\u00b7men", "ziehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PRF", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die in dem schwarzen Schoo\u00df, Nachtsturm und G\u00fcsse tragen.", "tokens": ["Die", "in", "dem", "schwar\u00b7zen", "Schoo\u00df", ",", "Nachts\u00b7turm", "und", "G\u00fcs\u00b7se", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Wind aus Mitternacht erhebet sein Gebr\u00fcll,", "tokens": ["Der", "Wind", "aus", "Mit\u00b7ter\u00b7nacht", "er\u00b7he\u00b7bet", "sein", "Ge\u00b7br\u00fcll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und brauset f\u00fcrchterlich am Mastkorb, Strick und Segel,", "tokens": ["Und", "brau\u00b7set", "f\u00fcrch\u00b7ter\u00b7lich", "am", "Mast\u00b7korb", ",", "Strick", "und", "Se\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der alte Ocean emp\u00f6rt sich auf einmal", "tokens": ["Der", "al\u00b7te", "O\u00b7cean", "em\u00b7p\u00f6rt", "sich", "auf", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ADV"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "In dicker Finsterni\u00df mit allen seinen Wellen,", "tokens": ["In", "di\u00b7cker", "Fins\u00b7ter\u00b7ni\u00df", "mit", "al\u00b7len", "sei\u00b7nen", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Bedeckt das Schif mit Schaum, best\u00fcrmt das bange Volk,", "tokens": ["Be\u00b7deckt", "das", "Schif", "mit", "Schaum", ",", "be\u00b7st\u00fcrmt", "das", "ban\u00b7ge", "Volk", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und seines Zepters Macht ersch\u00fcttert selbst das Ufer,", "tokens": ["Und", "sei\u00b7nes", "Zep\u00b7ters", "Macht", "er\u00b7sch\u00fct\u00b7tert", "selbst", "das", "U\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der Himmel blitzt und knallt, sein immer leuchtend Feuer", "tokens": ["Der", "Him\u00b7mel", "blitzt", "und", "knallt", ",", "sein", "im\u00b7mer", "leuch\u00b7tend", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,", "PPOSAT", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "L\u00e4sst die halbtodte Schaar in ihr Verderben schauen.", "tokens": ["L\u00e4sst", "die", "halb\u00b7tod\u00b7te", "Schaar", "in", "ihr", "Ver\u00b7der\u00b7ben", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die Wellen, deren Schlag die Fugen locker macht,", "tokens": ["Die", "Wel\u00b7len", ",", "de\u00b7ren", "Schlag", "die", "Fu\u00b7gen", "lo\u00b7cker", "macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Besiegen nun das Schif, und str\u00f6men durch die B\u00f6rsten,", "tokens": ["Be\u00b7sie\u00b7gen", "nun", "das", "Schif", ",", "und", "str\u00f6\u00b7men", "durch", "die", "B\u00f6rs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Bis da\u00df es Fluth und Sturm in tausend Tr\u00fcmmern schl\u00e4gt;", "tokens": ["Bis", "da\u00df", "es", "Fluth", "und", "Sturm", "in", "tau\u00b7send", "Tr\u00fcm\u00b7mern", "schl\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "NN", "KON", "NN", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "In dieser Noth vergisst das arme Volk sich selber,", "tokens": ["In", "die\u00b7ser", "Noth", "ver\u00b7gisst", "das", "ar\u00b7me", "Volk", "sich", "sel\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "ADJA", "NN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Kein Mensch gedenkt an Trost und keiner h\u00f6rt auf Trost.", "tokens": ["Kein", "Mensch", "ge\u00b7denkt", "an", "Trost", "und", "kei\u00b7ner", "h\u00f6rt", "auf", "Trost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "NN", "KON", "PIS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Sie schwimmen hin und her und heulen durch die Wellen.", "tokens": ["Sie", "schwim\u00b7men", "hin", "und", "her", "und", "heu\u00b7len", "durch", "die", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "KON", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wenn sie nach grosser M\u00fch das l\u00e4ngst erseufzte Land,", "tokens": ["Wenn", "sie", "nach", "gros\u00b7ser", "M\u00fch", "das", "l\u00e4ngst", "er\u00b7seufz\u00b7te", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Nun endlich noch erreicht, und tr\u00e4uflend sich erholet,", "tokens": ["Nun", "end\u00b7lich", "noch", "er\u00b7reicht", ",", "und", "tr\u00e4uf\u00b7lend", "sich", "er\u00b7ho\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVPP", "$,", "KON", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So \u00fcberschauen sie, das Ungl\u00fccksfeld, die See,", "tokens": ["So", "\u00fc\u00b7bersc\u00b7hau\u00b7en", "sie", ",", "das", "Un\u00b7gl\u00fccks\u00b7feld", ",", "die", "See", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Von einem Fels, jedoch nur mit bethr\u00e4nten Blicken,", "tokens": ["Von", "ei\u00b7nem", "Fels", ",", "je\u00b7doch", "nur", "mit", "be\u00b7thr\u00e4n\u00b7ten", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und fangen, selbst betr\u00fcbt, einander an zu tr\u00f6sten,", "tokens": ["Und", "fan\u00b7gen", ",", "selbst", "be\u00b7tr\u00fcbt", ",", "ein\u00b7an\u00b7der", "an", "zu", "tr\u00f6s\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "VVPP", "$,", "PRF", "APZR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und h\u00f6ren ruhiger, indem die sanfte Fluth", "tokens": ["Und", "h\u00f6\u00b7ren", "ru\u00b7hi\u00b7ger", ",", "in\u00b7dem", "die", "sanf\u00b7te", "Fluth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.34": {"text": "De Schifbruchs Ueberrest an die Gestade spielet,", "tokens": ["De", "Schif\u00b7bruchs", "Ue\u00b7ber\u00b7rest", "an", "die", "Ge\u00b7sta\u00b7de", "spie\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Den \u00fcberbliebnen Trost.", "tokens": ["Den", "\u00fc\u00b7berb\u00b7lieb\u00b7nen", "Trost", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}