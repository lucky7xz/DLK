{"dta.poem.2901": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "102.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du f\u00fchlst, durch Irrthum nur kannst du zum Ziele kommen;", "tokens": ["Du", "f\u00fchlst", ",", "durch", "Irr\u00b7thum", "nur", "kannst", "du", "zum", "Zie\u00b7le", "kom\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "NN", "ADV", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch nur ein Thor hat sich zu irren vorgenommen.", "tokens": ["Doch", "nur", "ein", "Thor", "hat", "sich", "zu", "ir\u00b7ren", "vor\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "PRF", "PTKZU", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du f\u00fchlst, erheben kannst du dich, wo du gefallen;", "tokens": ["Du", "f\u00fchlst", ",", "er\u00b7he\u00b7ben", "kannst", "du", "dich", ",", "wo", "du", "ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVINF", "VMFIN", "PPER", "PRF", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch nur ein Toller wird dem Fall entgegen wallen.", "tokens": ["Doch", "nur", "ein", "Tol\u00b7ler", "wird", "dem", "Fall", "ent\u00b7ge\u00b7gen", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "ART", "NN", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mit M\u00e4ngeln kommt man zwar, doch nicht durch sie zum Ziel,", "tokens": ["Mit", "M\u00e4n\u00b7geln", "kommt", "man", "zwar", ",", "doch", "nicht", "durch", "sie", "zum", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "ADV", "$,", "ADV", "PTKNEG", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht weil man fiel und irrt', obgleich man irrt' und fiel.", "tokens": ["Nicht", "weil", "man", "fiel", "und", "irrt'", ",", "ob\u00b7gleich", "man", "irrt'", "und", "fiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "PIS", "VVFIN", "KON", "VVFIN", "$,", "KOUS", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}