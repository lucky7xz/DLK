{"dta.poem.21579": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Gott .  \n 1884.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Der Du nicht Stein bist, doch des Steines Kraft,", "tokens": ["Der", "Du", "nicht", "Stein", "bist", ",", "doch", "des", "Stei\u00b7nes", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "NN", "VAFIN", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Kern und Schale h\u00e4lt in enger Haft.", "tokens": ["Die", "Kern", "und", "Scha\u00b7le", "h\u00e4lt", "in", "en\u00b7ger", "Haft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Du nicht Rose bist, doch ihre Pracht,", "tokens": ["Der", "Du", "nicht", "Ro\u00b7se", "bist", ",", "doch", "ih\u00b7re", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "NN", "VAFIN", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Duft, ihr Auge, das zur Sonne lacht.", "tokens": ["Ihr", "Duft", ",", "ihr", "Au\u00b7ge", ",", "das", "zur", "Son\u00b7ne", "lacht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Du nicht Eiche bist, doch wohl ihr Mark,", "tokens": ["Der", "Du", "nicht", "Ei\u00b7che", "bist", ",", "doch", "wohl", "ihr", "Mark", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "NN", "VAFIN", "$,", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Stolz, der aus ihr athmet, lebensstark.", "tokens": ["Der", "Stolz", ",", "der", "aus", "ihr", "ath\u00b7met", ",", "le\u00b7bens\u00b7stark", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Welt ist nichts als Form, in der Du pr\u00e4gst,", "tokens": ["Die", "Welt", "ist", "nichts", "als", "Form", ",", "in", "der", "Du", "pr\u00e4gst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "KOKOM", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist nichts als die Gewandung, die Du tr\u00e4gst.", "tokens": ["Ist", "nichts", "als", "die", "Ge\u00b7wan\u00b7dung", ",", "die", "Du", "tr\u00e4gst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KOKOM", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ist nichts als Spiegelbild von Deinem Sein;", "tokens": ["Ist", "nichts", "als", "Spie\u00b7gel\u00b7bild", "von", "Dei\u00b7nem", "Sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KOKOM", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur Du bist Wahrheit, doch das Bild ist Schein.", "tokens": ["Nur", "Du", "bist", "Wahr\u00b7heit", ",", "doch", "das", "Bild", "ist", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "NN", "$,", "ADV", "ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ich bin ein Mensch, mein Geist umspannt das All,", "tokens": ["Ich", "bin", "ein", "Mensch", ",", "mein", "Geist", "um\u00b7spannt", "das", "All", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch meine Seele rauscht der Sph\u00e4ren Hall.", "tokens": ["Durch", "mei\u00b7ne", "See\u00b7le", "rauscht", "der", "Sph\u00e4\u00b7ren", "Hall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ich h\u00f6re was der Lerche Jubel sagt,", "tokens": ["Ich", "h\u00f6\u00b7re", "was", "der", "Ler\u00b7che", "Ju\u00b7bel", "sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich h\u00f6re was des Meeres Brandung klagt.", "tokens": ["Ich", "h\u00f6\u00b7re", "was", "des", "Mee\u00b7res", "Bran\u00b7dung", "klagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ich sehe was des Feuers Auge gl\u00fcht,", "tokens": ["Ich", "se\u00b7he", "was", "des", "Feu\u00b7ers", "Au\u00b7ge", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich sehe was im Scho\u00df der Lilie bl\u00fcht.", "tokens": ["Ich", "se\u00b7he", "was", "im", "Scho\u00df", "der", "Li\u00b7lie", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Ich f\u00fchle was im Blut der Erde ringt,", "tokens": ["Ich", "f\u00fch\u00b7le", "was", "im", "Blut", "der", "Er\u00b7de", "ringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Hauch, der von den Sternen niederdringt.", "tokens": ["Den", "Hauch", ",", "der", "von", "den", "Ster\u00b7nen", "nie\u00b7der\u00b7dringt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Nein, nein, nicht ich; was gilt dem Fleische Duft,", "tokens": ["Nein", ",", "nein", ",", "nicht", "ich", ";", "was", "gilt", "dem", "Flei\u00b7sche", "Duft", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PTKNEG", "PPER", "$.", "PWS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was gilt dem Leibe reine Himmelsluft!", "tokens": ["Was", "gilt", "dem", "Lei\u00b7be", "rei\u00b7ne", "Him\u00b7mel\u00b7sluft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Was gilt dem Staubkorn unermess\u2019ner Raum,", "tokens": ["Was", "gilt", "dem", "Staub\u00b7korn", "un\u00b7er\u00b7mess'\u00b7ner", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was gilt der F\u00e4ulni\u00df ewigen Lebens Traum!", "tokens": ["Was", "gilt", "der", "F\u00e4ul\u00b7ni\u00df", "e\u00b7wi\u00b7gen", "Le\u00b7bens", "Traum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.12": {"line.1": {"text": "Nicht ich, nicht ich; mein Ich, dem Tod geweiht,", "tokens": ["Nicht", "ich", ",", "nicht", "ich", ";", "mein", "Ich", ",", "dem", "Tod", "ge\u00b7weiht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "$,", "PTKNEG", "PPER", "$.", "PPOSAT", "PPER", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist lauter Elend, lauter Niedrigkeit.", "tokens": ["Ist", "lau\u00b7ter", "E\u00b7lend", ",", "lau\u00b7ter", "Nied\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Mein Ich h\u00f6rt nur den Schrei der eignen Noth,", "tokens": ["Mein", "Ich", "h\u00f6rt", "nur", "den", "Schrei", "der", "eig\u00b7nen", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PPER", "VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Du h\u00f6rst in mir der Liebe Allgebot.", "tokens": ["Du", "h\u00f6rst", "in", "mir", "der", "Lie\u00b7be", "All\u00b7ge\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Mein Ich sieht nur den Glimmer, nur den Schein,", "tokens": ["Mein", "Ich", "sieht", "nur", "den", "Glim\u00b7mer", ",", "nur", "den", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PPER", "VVFIN", "ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Du siehst in mir ins Herz der Welt hinein.", "tokens": ["Du", "siehst", "in", "mir", "ins", "Herz", "der", "Welt", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Mein Ich f\u00fchlt nur, was schmeichelnd ihm behagt,", "tokens": ["Mein", "Ich", "f\u00fchlt", "nur", ",", "was", "schmei\u00b7chelnd", "ihm", "be\u00b7hagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PPER", "VVFIN", "ADV", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Du f\u00fchlst in mir, was sich zu opfern wagt.", "tokens": ["Du", "f\u00fchlst", "in", "mir", ",", "was", "sich", "zu", "op\u00b7fern", "wagt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PRELS", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Du zehrst an mir, wie Glut an Eisen zehrt,", "tokens": ["Du", "zehrst", "an", "mir", ",", "wie", "Glut", "an", "Ei\u00b7sen", "zehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PWAV", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du ruhst nicht, bis ich schlackenlos verkl\u00e4rt.", "tokens": ["Du", "ruhst", "nicht", ",", "bis", "ich", "schla\u00b7cken\u00b7los", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.17": {"line.1": {"text": "L\u00e4\u00dft Du von mir, bin ich ein Spiel, ein Spott;", "tokens": ["L\u00e4\u00dft", "Du", "von", "mir", ",", "bin", "ich", "ein", "Spiel", ",", "ein", "Spott", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "VAFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Ich, erf\u00fcllt mit Dir, ist selber Gott.", "tokens": ["Mein", "Ich", ",", "er\u00b7f\u00fcllt", "mit", "Dir", ",", "ist", "sel\u00b7ber", "Gott", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PPER", "$,", "VVFIN", "APPR", "PPER", "$,", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}