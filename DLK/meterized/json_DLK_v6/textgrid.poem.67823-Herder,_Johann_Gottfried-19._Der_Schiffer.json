{"textgrid.poem.67823": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "19. Der Schiffer", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig sizt in Dumferlingschlo\u00df,", "tokens": ["Der", "K\u00f6\u00b7nig", "sizt", "in", "Dum\u00b7fer\u00b7ling\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Er trinkt blutrothen Wein,", "tokens": ["Er", "trinkt", "blut\u00b7ro\u00b7then", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbo wo treff ich ein'n Segler an,", "tokens": ["\u00bb", "o", "wo", "treff", "ich", "ein'n", "Seg\u00b7ler", "an", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PWAV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dies Schiff zu segeln mein?\u00ab", "tokens": ["Dies", "Schiff", "zu", "se\u00b7geln", "mein", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "NN", "PTKZU", "VVINF", "PPOSAT", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auf und sprach ein alter Ritter,", "tokens": ["Auf", "und", "sprach", "ein", "al\u00b7ter", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "(sa\u00df rechts an K\u00f6nigs Knie)", "tokens": ["(", "sa\u00df", "rechts", "an", "K\u00f6\u00b7nigs", "Knie", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "APPR", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbsir Patrik Spence ist der beste Segler,", "tokens": ["\u00bb", "sir", "Pa\u00b7trik", "Spen\u00b7ce", "ist", "der", "bes\u00b7te", "Seg\u00b7ler", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "NN", "NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Im ganzen Land allhie.\u00ab", "tokens": ["Im", "gan\u00b7zen", "Land", "all\u00b7hie", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der K\u00f6nig schrieb ein'n breiten Brief", "tokens": ["Der", "K\u00f6\u00b7nig", "schrieb", "ein'n", "brei\u00b7ten", "Brief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Versiegelt ihn mit seiner Hand,", "tokens": ["Ver\u00b7sie\u00b7gelt", "ihn", "mit", "sei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sandt ihn zu Sir Patrik Spence,", "tokens": ["Und", "sandt", "ihn", "zu", "Sir", "Pa\u00b7trik", "Spen\u00b7ce", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "NE", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der wohnt an Meeres Strand.", "tokens": ["Der", "wohnt", "an", "Mee\u00b7res", "Strand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Erste Zeil Sir Patrik las,", "tokens": ["Die", "Ers\u00b7te", "Zeil", "Sir", "Pa\u00b7trik", "las", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Laut Lachen schlug er auf;", "tokens": ["Laut", "La\u00b7chen", "schlug", "er", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die zweite Zeil Sir Patrik las,", "tokens": ["Die", "zwei\u00b7te", "Zeil", "Sir", "Pa\u00b7trik", "las", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eine Thr\u00e4n' ihm folgte drauf.", "tokens": ["Ei\u00b7ne", "Thr\u00e4n'", "ihm", "folg\u00b7te", "drauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "O wer, wer hat mir das gethan?", "tokens": ["O", "wer", ",", "wer", "hat", "mir", "das", "ge\u00b7than", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "$,", "PWS", "VAFIN", "PPER", "PDS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat wehgethan mir sehr!", "tokens": ["Hat", "weh\u00b7ge\u00b7than", "mir", "sehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich auszusenden in dieser Zeit!", "tokens": ["Mich", "aus\u00b7zu\u00b7sen\u00b7den", "in", "die\u00b7ser", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Zu segeln auf dem Meer.", "tokens": ["Zu", "se\u00b7geln", "auf", "dem", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Macht fort, macht fort, mein' wackre Leut,", "tokens": ["Macht", "fort", ",", "macht", "fort", ",", "mein'", "wack\u00b7re", "Leut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Unser gut Schiff segelt morgen.", "tokens": ["Un\u00b7ser", "gut", "Schiff", "se\u00b7gelt", "mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "ADV", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "\u00bbo sprecht nicht so, mein lieber Herr,", "tokens": ["\u00bb", "o", "sprecht", "nicht", "so", ",", "mein", "lie\u00b7ber", "Herr", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VVFIN", "PTKNEG", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da sind wir sehr in Sorgen.", "tokens": ["Da", "sind", "wir", "sehr", "in", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Gestern Abend sah ich den neuen Mond,", "tokens": ["Ge\u00b7stern", "A\u00b7bend", "sah", "ich", "den", "neu\u00b7en", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ein Hof war um ihn her.", "tokens": ["Ein", "Hof", "war", "um", "ihn", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich f\u00fcrcht', ich f\u00fcrcht, mein lieber Herr,", "tokens": ["Ich", "f\u00fcrcht'", ",", "ich", "f\u00fcrcht", ",", "mein", "lie\u00b7ber", "Herr", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Sturm uns wartet schwer.\u00ab", "tokens": ["Ein", "Sturm", "uns", "war\u00b7tet", "schwer", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O edle Schotten, sie wu\u00dften lang,", "tokens": ["O", "ed\u00b7le", "Schot\u00b7ten", ",", "sie", "wu\u00df\u00b7ten", "lang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Zu wahr'n ihre Korkholzschu;", "tokens": ["Zu", "wahr'n", "ih\u00b7re", "Kork\u00b7holz\u00b7schu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch lang \u00fcberall das Spiel gespielt,", "tokens": ["Doch", "lang", "\u00fc\u00b7be\u00b7rall", "das", "Spiel", "ge\u00b7spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schwammen ihre H\u00fcte dazu.", "tokens": ["Schwam\u00b7men", "ih\u00b7re", "H\u00fc\u00b7te", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.9": {"line.1": {"text": "O lang, lang m\u00f6gen ihre Frauen sizen,", "tokens": ["O", "lang", ",", "lang", "m\u00f6\u00b7gen", "ih\u00b7re", "Frau\u00b7en", "si\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "ADJD", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den F\u00e4cher in ihrer Hand;", "tokens": ["Den", "F\u00e4\u00b7cher", "in", "ih\u00b7rer", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Eh je sie sehn Sir Patrik Spence", "tokens": ["Eh", "je", "sie", "sehn", "Sir", "Pa\u00b7trik", "Spen\u00b7ce"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PPER", "VVFIN", "NN", "NE", "NE"], "meter": "-+-++-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Ansegeln an das Land.", "tokens": ["An\u00b7se\u00b7geln", "an", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "O lang, lang m\u00f6gen ihre Frauen stehn", "tokens": ["O", "lang", ",", "lang", "m\u00f6\u00b7gen", "ih\u00b7re", "Frau\u00b7en", "stehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "$,", "ADJD", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Goldkamm in dem Haar,", "tokens": ["Den", "Gold\u00b7kamm", "in", "dem", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und warten ihrer lieben Herr'n,", "tokens": ["Und", "war\u00b7ten", "ih\u00b7rer", "lie\u00b7ben", "Herr'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sehn sie nimmer gar.", "tokens": ["Sie", "sehn", "sie", "nim\u00b7mer", "gar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Dort \u00fcber, hin\u00fcber nach Aberdour!", "tokens": ["Dort", "\u00fc\u00b7ber", ",", "hin\u00b7\u00fc\u00b7ber", "nach", "A\u00b7ber\u00b7dour", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "$,", "ADV", "APPR", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Tief Funfzig Fad'n im Meer,", "tokens": ["Tief", "Funf\u00b7zig", "Fa\u00b7d'n", "im", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "CARD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da liegt der gute Sir Patrik Spence,", "tokens": ["Da", "liegt", "der", "gu\u00b7te", "Sir", "Pa\u00b7trik", "Spen\u00b7ce", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "NE", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sein' Edlen um ihn her.", "tokens": ["Sein'", "Ed\u00b7len", "um", "ihn", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}