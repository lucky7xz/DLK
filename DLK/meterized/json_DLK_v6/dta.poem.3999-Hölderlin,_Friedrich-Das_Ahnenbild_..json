{"dta.poem.3999": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Ahnenbild .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Alter Vater! Du blickst immer, wie ehmals, noch,", "tokens": ["Al\u00b7ter", "Va\u00b7ter", "!", "Du", "blickst", "im\u00b7mer", ",", "wie", "eh\u00b7mals", ",", "noch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$.", "PPER", "VVFIN", "ADV", "$,", "PWAV", "ADV", "$,", "ADV", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Da Du gerne gelebt unter den Sterblichen,", "tokens": ["Da", "Du", "ger\u00b7ne", "ge\u00b7lebt", "un\u00b7ter", "den", "Sterb\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "--+--++--+--", "measure": "anapaest.di.plus"}, "line.3": {"text": "Aber ruhiger nur und", "tokens": ["A\u00b7ber", "ru\u00b7hi\u00b7ger", "nur", "und"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "KON"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wie die Seligen heiterer,", "tokens": ["Wie", "die", "Se\u00b7li\u00b7gen", "hei\u00b7te\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "In die Wohnung, wo Dich Vater! das S\u00f6hnlein", "tokens": ["In", "die", "Woh\u00b7nung", ",", "wo", "Dich", "Va\u00b7ter", "!", "das", "S\u00f6hn\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "PPER", "NN", "$.", "ART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "nennt,", "tokens": ["nennt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Wo es l\u00e4chelnd vor Dir spielt und den Muthwill \u00fcbt,", "tokens": ["Wo", "es", "l\u00e4\u00b7chelnd", "vor", "Dir", "spielt", "und", "den", "Mut\u00b7hwill", "\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "--+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie die L\u00e4mmer im Feld', auf", "tokens": ["Wie", "die", "L\u00e4m\u00b7mer", "im", "Feld'", ",", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$,", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gr\u00fcnem Teppiche, den zur Lust", "tokens": ["Gr\u00fc\u00b7nem", "Tep\u00b7pi\u00b7che", ",", "den", "zur", "Lust"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Ihm die Mutter geg\u00f6nnt. Ferne sich haltend, sieht", "tokens": ["Ihm", "die", "Mut\u00b7ter", "ge\u00b7g\u00f6nnt", ".", "Fer\u00b7ne", "sich", "hal\u00b7tend", ",", "sieht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "ART", "NN", "VVPP", "$.", "NN", "PRF", "VVPP", "$,", "VVFIN"], "meter": "--+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihm die Liebende zu, wundert der Sprache schon", "tokens": ["Ihm", "die", "Lie\u00b7ben\u00b7de", "zu", ",", "wun\u00b7dert", "der", "Spra\u00b7che", "schon"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "PTKVZ", "$,", "VVFIN", "ART", "NN", "ADV"], "meter": "--+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und des jungen Verstandes", "tokens": ["Und", "des", "jun\u00b7gen", "Ver\u00b7stan\u00b7des"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und des bl\u00fchenden Auges sich.", "tokens": ["Und", "des", "bl\u00fc\u00b7hen\u00b7den", "Au\u00b7ges", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PRF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Und an andere Zeit mahnt sie der Mann, Dein", "tokens": ["Und", "an", "an\u00b7de\u00b7re", "Zeit", "mahnt", "sie", "der", "Mann", ",", "Dein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,", "PPOSAT"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sohn,", "tokens": ["Sohn", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "An die L\u00fcfte des Mais, da er geseufzt um sie,", "tokens": ["An", "die", "L\u00fcf\u00b7te", "des", "Mais", ",", "da", "er", "ge\u00b7seufzt", "um", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "KOUS", "PPER", "VVPP", "APPR", "PPER", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "An die Br\u00e4utigamstage,", "tokens": ["An", "die", "Br\u00e4u\u00b7ti\u00b7gams\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo der Stolze die Demuth lernt;", "tokens": ["Wo", "der", "Stol\u00b7ze", "die", "De\u00b7muth", "lernt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+--", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Doch es wandte sich bald. Sicherer, denn er war,", "tokens": ["Doch", "es", "wand\u00b7te", "sich", "bald", ".", "Si\u00b7che\u00b7rer", ",", "denn", "er", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADV", "$.", "NN", "$,", "KON", "PPER", "VAFIN", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ist er, herrlicher ist unter den Seinigen", "tokens": ["Ist", "er", ",", "herr\u00b7li\u00b7cher", "ist", "un\u00b7ter", "den", "Sei\u00b7ni\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "ADJA", "VAFIN", "APPR", "ART", "PPOSS"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Nun der Zweifachgeliebte,", "tokens": ["Nun", "der", "Zwei\u00b7fach\u00b7ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und ihm gehet sein Tagewerk.", "tokens": ["Und", "ihm", "ge\u00b7het", "sein", "Ta\u00b7ge\u00b7werk", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Stiller Vater! auch Du lebtest und liebtest so;", "tokens": ["Stil\u00b7ler", "Va\u00b7ter", "!", "auch", "Du", "leb\u00b7test", "und", "lieb\u00b7test", "so", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "ADV", "PPER", "VVFIN", "KON", "VVFIN", "ADV", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Darum wohnest Du nun, als ein Unsterblicher,", "tokens": ["Da\u00b7rum", "woh\u00b7nest", "Du", "nun", ",", "als", "ein", "U\u00b7nsterb\u00b7li\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Bei den Kindern, und Segen,", "tokens": ["Bei", "den", "Kin\u00b7dern", ",", "und", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wie aus Wolken des Himmels, k\u00f6mmt", "tokens": ["Wie", "aus", "Wol\u00b7ken", "des", "Him\u00b7mels", ",", "k\u00f6mmt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "$,", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Oefters \u00fcber das Haus, ruhiger Mann! von Dir,", "tokens": ["Oef\u00b7ters", "\u00fc\u00b7ber", "das", "Haus", ",", "ru\u00b7hi\u00b7ger", "Mann", "!", "von", "Dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADJA", "NN", "$.", "APPR", "PPER", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und es mehrt sich, es reift, edler von Jahr zu Jahr,", "tokens": ["Und", "es", "mehrt", "sich", ",", "es", "reift", ",", "ed\u00b7ler", "von", "Jahr", "zu", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "$,", "PPER", "VVFIN", "$,", "ADJA", "APPR", "NN", "APPR", "NN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "In bescheidenem Gl\u00fccke,", "tokens": ["In", "be\u00b7schei\u00b7de\u00b7nem", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Was mit Hoffnungen Du gepflanzt.", "tokens": ["Was", "mit", "Hoff\u00b7nun\u00b7gen", "Du", "ge\u00b7pflanzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die Du liebend erzogst, siehe! sie gr\u00fcnen Dir,", "tokens": ["Die", "Du", "lie\u00b7bend", "er\u00b7zogst", ",", "sie\u00b7he", "!", "sie", "gr\u00fc\u00b7nen", "Dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$,", "VVIMP", "$.", "PPER", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Deine B\u00e4ume, wie sonst, breiten ums Haus den", "tokens": ["Dei\u00b7ne", "B\u00e4u\u00b7me", ",", "wie", "sonst", ",", "brei\u00b7ten", "ums", "Haus", "den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "ADV", "$,", "ADJA", "APPRART", "NN", "ART"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Arm,", "tokens": ["Arm", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Voll von dankenden Gaben;", "tokens": ["Voll", "von", "dan\u00b7ken\u00b7den", "Ga\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Sicher stehen die St\u00e4mme schon.", "tokens": ["Si\u00b7cher", "ste\u00b7hen", "die", "St\u00e4m\u00b7me", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Und am H\u00fcgel hinab, wo Du den sonnigen", "tokens": ["Und", "am", "H\u00fc\u00b7gel", "hin\u00b7ab", ",", "wo", "Du", "den", "son\u00b7ni\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PTKVZ", "$,", "PWAV", "PPER", "ART", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Boden ihnen gebaut, neigen und schwingen sich", "tokens": ["Bo\u00b7den", "ih\u00b7nen", "ge\u00b7baut", ",", "nei\u00b7gen", "und", "schwin\u00b7gen", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VVPP", "$,", "VVFIN", "KON", "VVFIN", "PRF"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Deine freudigen Reden,", "tokens": ["Dei\u00b7ne", "freu\u00b7di\u00b7gen", "Re\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Trunken, purpurner Trauben voll.", "tokens": ["Trun\u00b7ken", ",", "pur\u00b7pur\u00b7ner", "Trau\u00b7ben", "voll", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "Aber unten im Haus ruhet, besorgt von Dir,", "tokens": ["A\u00b7ber", "un\u00b7ten", "im", "Haus", "ru\u00b7het", ",", "be\u00b7sorgt", "von", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "$,", "ADJD", "APPR", "PPER", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Der gekelterte Wein; theuer ist der dem Sohn,", "tokens": ["Der", "ge\u00b7kel\u00b7ter\u00b7te", "Wein", ";", "theu\u00b7er", "ist", "der", "dem", "Sohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADJD", "VAFIN", "ART", "ART", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Und er sparet zum Fest das", "tokens": ["Und", "er", "spa\u00b7ret", "zum", "Fest", "das"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "ART"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Alte, lautere Feuer sich.", "tokens": ["Al\u00b7te", ",", "lau\u00b7te\u00b7re", "Feu\u00b7er", "sich", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "PRF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "Dann beim n\u00e4chtlichen Mahl, wenn er, in Lust", "tokens": ["Dann", "beim", "n\u00e4cht\u00b7li\u00b7chen", "Mahl", ",", "wenn", "er", ",", "in", "Lust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,", "KOUS", "PPER", "$,", "APPR", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "und Ernst,", "tokens": ["und", "Ernst", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NE", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Von Vergangenem viel, vieles von K\u00fcnftigem", "tokens": ["Von", "Ver\u00b7gan\u00b7ge\u00b7nem", "viel", ",", "vie\u00b7les", "von", "K\u00fcnf\u00b7ti\u00b7gem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "$,", "PIS", "APPR", "NN"], "meter": "+-+---+--+--", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Mit den Freunden gesprochen,", "tokens": ["Mit", "den", "Freun\u00b7den", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und der letzte Gesang noch hallt,", "tokens": ["Und", "der", "letz\u00b7te", "Ge\u00b7sang", "noch", "hallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.12": {"line.1": {"text": "H\u00e4lt er h\u00f6her den Kelch, siehet dein Bild und spricht:", "tokens": ["H\u00e4lt", "er", "h\u00f6\u00b7her", "den", "Kelch", ",", "sie\u00b7het", "dein", "Bild", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$,", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Deiner denken wir nun, Dein, und so werd' und", "tokens": ["Dei\u00b7ner", "den\u00b7ken", "wir", "nun", ",", "Dein", ",", "und", "so", "werd'", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "$,", "KON", "ADV", "VAFIN", "KON"], "meter": "+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "bleib'", "tokens": ["bleib'"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "\u201dihre Ehre des Hauses", "tokens": ["\"", "ih\u00b7re", "Eh\u00b7re", "des", "Hau\u00b7ses"