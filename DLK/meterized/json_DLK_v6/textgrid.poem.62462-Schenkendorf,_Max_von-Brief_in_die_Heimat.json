, "measure": "iambic.tetra"}, "line.2": {"text": "In diesem Hause wohnen,", "tokens": ["In", "die\u00b7sem", "Hau\u00b7se", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bei Gottesdienst und Saitenspiel", "tokens": ["Bei", "Got\u00b7tes\u00b7dienst", "und", "Sai\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Herrscher in ihm thronen.", "tokens": ["Ein", "Herr\u00b7scher", "in", "ihm", "thro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Herrlichste der ganzen Welt,", "tokens": ["Der", "Herr\u00b7lichs\u00b7te", "der", "gan\u00b7zen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Priester und ein Rittersheld,", "tokens": ["Ein", "Pries\u00b7ter", "und", "ein", "Rit\u00b7ter\u00b7sheld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Man hei\u00dft ihn deutscher Kaiser.", "tokens": ["Man", "hei\u00dft", "ihn", "deut\u00b7scher", "Kai\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "In diesem Hause soll ein Quell", "tokens": ["In", "die\u00b7sem", "Hau\u00b7se", "soll", "ein", "Quell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Gottes Huld entspringen,", "tokens": ["Durch", "Got\u00b7tes", "Huld", "ent\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der wird so rein und silberhell", "tokens": ["Der", "wird", "so", "rein", "und", "sil\u00b7ber\u00b7hell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch viele L\u00e4nder dringen,", "tokens": ["Durch", "vie\u00b7le", "L\u00e4n\u00b7der", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wo er flie\u00dfet, bl\u00fcht ein Strau\u00df,", "tokens": ["Und", "wo", "er", "flie\u00b7\u00dfet", ",", "bl\u00fcht", "ein", "Strau\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O Heimat s\u00fc\u00df, o Vaterhaus,", "tokens": ["O", "Hei\u00b7mat", "s\u00fc\u00df", ",", "o", "Va\u00b7ter\u00b7haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Euch alle wird er laben.", "tokens": ["Euch", "al\u00b7le", "wird", "er", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Kehrt' ich nun heim, ein halber Mann,", "tokens": ["Kehrt'", "ich", "nun", "heim", ",", "ein", "hal\u00b7ber", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eh' ganz das Werk vollzogen,", "tokens": ["Eh'", "ganz", "das", "Werk", "voll\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So s\u00e4hen mich wol fragend an,", "tokens": ["So", "s\u00e4\u00b7hen", "mich", "wol", "fra\u00b7gend", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die fr\u00fcher mir gewogen.", "tokens": ["Die", "fr\u00fc\u00b7her", "mir", "ge\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich selber f\u00fchlte mich verbannt,", "tokens": ["Ich", "sel\u00b7ber", "f\u00fchl\u00b7te", "mich", "ver\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die alten Bilder an der Wand,", "tokens": ["Die", "al\u00b7ten", "Bil\u00b7der", "an", "der", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich d\u00fcrfte sie nicht gr\u00fc\u00dfen.", "tokens": ["Ich", "d\u00fcrf\u00b7te", "sie", "nicht", "gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Doch was ich denke, was ich sinn',", "tokens": ["Doch", "was", "ich", "den\u00b7ke", ",", "was", "ich", "sinn'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Heimat ist dein eigen,", "tokens": ["O", "Hei\u00b7mat", "ist", "dein", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich dein treuer K\u00e4mpfer bin,", "tokens": ["Da\u00df", "ich", "dein", "treu\u00b7er", "K\u00e4mp\u00b7fer", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll Schwert und Zither zeigen.", "tokens": ["Soll", "Schwert", "und", "Zi\u00b7ther", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es kommt ein Jahr, es kommt ein Tag,", "tokens": ["Es", "kommt", "ein", "Jahr", ",", "es", "kommt", "ein", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich dich wieder sehen mag,", "tokens": ["Da\u00df", "ich", "dich", "wie\u00b7der", "se\u00b7hen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das wird mir Freude geben.", "tokens": ["Das", "wird", "mir", "Freu\u00b7de", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und f\u00e4nd' ich nimmer mein Quartier,", "tokens": ["Und", "f\u00e4nd'", "ich", "nim\u00b7mer", "mein", "Quar\u00b7tier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "W\u00e4r' anders mir gesponnen,", "tokens": ["W\u00e4r'", "an\u00b7ders", "mir", "ge\u00b7spon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vielleicht aus sch\u00f6nen Wunden mir", "tokens": ["Viel\u00b7leicht", "aus", "sch\u00f6\u00b7nen", "Wun\u00b7den", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das hei\u00dfe Blut entronnen;", "tokens": ["Das", "hei\u00b7\u00dfe", "Blut", "ent\u00b7ron\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auch noch im Grabe bin ich dein,", "tokens": ["Auch", "noch", "im", "Gra\u00b7be", "bin", "ich", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "VAFIN", "PPER", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Man soll auf meinem Leichenstein", "tokens": ["Man", "soll", "auf", "mei\u00b7nem", "Lei\u00b7chen\u00b7stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von meinem Lande lesen.", "tokens": ["Von", "mei\u00b7nem", "Lan\u00b7de", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Du heil'ges Meer, du stiller Strand,", "tokens": ["Du", "heil'\u00b7ges", "Meer", ",", "du", "stil\u00b7ler", "Strand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch fern euch zu geh\u00f6ren,", "tokens": ["Auch", "fern", "euch", "zu", "ge\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Heimatland, mein Preu\u00dfenland,", "tokens": ["Mein", "Hei\u00b7mat\u00b7land", ",", "mein", "Preu\u00b7\u00dfen\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mag ich mich k\u00fchn verschw\u00f6ren.", "tokens": ["Mag", "ich", "mich", "k\u00fchn", "ver\u00b7schw\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Volk, du bist zuerst erwacht", "tokens": ["Mein", "Volk", ",", "du", "bist", "zu\u00b7erst", "er\u00b7wacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So fest und freudig in der Schlacht,", "tokens": ["So", "fest", "und", "freu\u00b7dig", "in", "der", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O Volk zu Gottes Ehre.", "tokens": ["O", "Volk", "zu", "Got\u00b7tes", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}