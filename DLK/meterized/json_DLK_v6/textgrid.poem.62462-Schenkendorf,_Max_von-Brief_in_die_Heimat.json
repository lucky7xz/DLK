{"textgrid.poem.62462": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Brief in die Heimat", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.57", "en:0.42"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was locket ihr, was winkest du", "tokens": ["Was", "lo\u00b7cket", "ihr", ",", "was", "win\u00b7kest", "du"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PWS", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Vaters Hof und Garten?", "tokens": ["O", "Va\u00b7ters", "Hof", "und", "Gar\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie darf ich nun in schn\u00f6der Ruh", "tokens": ["Wie", "darf", "ich", "nun", "in", "schn\u00f6\u00b7der", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der stillen Felder warten?", "tokens": ["Der", "stil\u00b7len", "Fel\u00b7der", "war\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das w\u00e4re mir ein schlechter Ruhm,", "tokens": ["Das", "w\u00e4\u00b7re", "mir", "ein", "schlech\u00b7ter", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An Haus und Gut und Eigenthum", "tokens": ["An", "Haus", "und", "Gut", "und", "Ei\u00b7gen\u00b7thum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In solcher Zeit zu denken.", "tokens": ["In", "sol\u00b7cher", "Zeit", "zu", "den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Preu\u00dfen, s\u00fc\u00dfes Heimatland,", "tokens": ["Mein", "Preu\u00b7\u00dfen", ",", "s\u00fc\u00b7\u00dfes", "Hei\u00b7mat\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist mir nimmer ferne,", "tokens": ["Du", "bist", "mir", "nim\u00b7mer", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du heil'ges Meer, mein Ostseestrand,", "tokens": ["Du", "heil'\u00b7ges", "Meer", ",", "mein", "Ost\u00b7see\u00b7strand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich gr\u00fc\u00dft' euch gar zu gerne:", "tokens": ["Ich", "gr\u00fc\u00dft'", "euch", "gar", "zu", "ger\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKA", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wo ich die fr\u00fch'ste Lust empfand,", "tokens": ["Wo", "ich", "die", "fr\u00fch'\u00b7ste", "Lust", "emp\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo mich die erste Liebe band,", "tokens": ["Wo", "mich", "die", "ers\u00b7te", "Lie\u00b7be", "band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da bl\u00fcht ein Garten Gottes.", "tokens": ["Da", "bl\u00fcht", "ein", "Gar\u00b7ten", "Got\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich ging im Hain, am Bach, ich trank", "tokens": ["Ich", "ging", "im", "Hain", ",", "am", "Bach", ",", "ich", "trank"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "APPRART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Lust mit vollen Z\u00fcgen;", "tokens": ["Die", "Lust", "mit", "vol\u00b7len", "Z\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch andre Zeit bringt andern Drang,", "tokens": ["Doch", "and\u00b7re", "Zeit", "bringt", "an\u00b7dern", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das konnte mir nicht gn\u00fcgen.", "tokens": ["Das", "konn\u00b7te", "mir", "nicht", "gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Viel' Stimmen in mir klangen laut,", "tokens": ["Viel'", "Stim\u00b7men", "in", "mir", "klan\u00b7gen", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Frisch auf, du junges Blut, die Braut", "tokens": ["Frisch", "auf", ",", "du", "jun\u00b7ges", "Blut", ",", "die", "Braut"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von fernher heim zu f\u00fchren.", "tokens": ["Von", "fern\u00b7her", "heim", "zu", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und als das Heer der Welschen kam", "tokens": ["Und", "als", "das", "Heer", "der", "Wel\u00b7schen", "kam"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In jenen finstern Tagen,", "tokens": ["In", "je\u00b7nen", "fins\u00b7tern", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als keiner noch die Waffen nahm,", "tokens": ["Als", "kei\u00b7ner", "noch", "die", "Waf\u00b7fen", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die R\u00e4uber zu erschlagen,", "tokens": ["Die", "R\u00e4u\u00b7ber", "zu", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mocht' ich den Jammer nimmer schau'n,", "tokens": ["Mocht'", "ich", "den", "Jam\u00b7mer", "nim\u00b7mer", "schau'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weit ging ich von der Heimat Au'n,", "tokens": ["Weit", "ging", "ich", "von", "der", "Hei\u00b7mat", "Au'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem Rhein die Noth zu klagen.", "tokens": ["Dem", "Rhein", "die", "Noth", "zu", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich sah ihn, wie er z\u00fcrnend flo\u00df", "tokens": ["Ich", "sah", "ihn", ",", "wie", "er", "z\u00fcr\u00b7nend", "flo\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVPP", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schm\u00e4hlich trug die Bande;", "tokens": ["Und", "schm\u00e4h\u00b7lich", "trug", "die", "Ban\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich sah auch manch zerfall'nes Schlo\u00df", "tokens": ["Ich", "sah", "auch", "manch", "zer\u00b7fall'\u00b7nes", "Schlo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An seinem Felsenstrande.", "tokens": ["An", "sei\u00b7nem", "Fel\u00b7sen\u00b7stran\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da dacht' ich: Weh' dir, schn\u00f6de Welt,", "tokens": ["Da", "dacht'", "ich", ":", "Weh'", "dir", ",", "schn\u00f6\u00b7de", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo Kraft und Herrlichkeit zerf\u00e4llt,", "tokens": ["Wo", "Kraft", "und", "Herr\u00b7lich\u00b7keit", "zer\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du liegest recht im Argen.", "tokens": ["Du", "lie\u00b7gest", "recht", "im", "Ar\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und aus den grauen Tr\u00fcmmern klang", "tokens": ["Und", "aus", "den", "grau\u00b7en", "Tr\u00fcm\u00b7mern", "klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der strengen Geister Schelten:", "tokens": ["Der", "stren\u00b7gen", "Geis\u00b7ter", "Schel\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Heimat, die in Schutt versank,", "tokens": ["Die", "Hei\u00b7mat", ",", "die", "in", "Schutt", "ver\u00b7sank", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll dir nicht Alles gelten.", "tokens": ["Soll", "dir", "nicht", "Al\u00b7les", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die alten Steine liegen da,", "tokens": ["Die", "al\u00b7ten", "Stei\u00b7ne", "lie\u00b7gen", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der V\u00e4ter Segen ist euch nah',", "tokens": ["Der", "V\u00e4\u00b7ter", "Se\u00b7gen", "ist", "euch", "nah'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Erbaut euch neue Schl\u00f6sser.", "tokens": ["Er\u00b7baut", "euch", "neu\u00b7e", "Schl\u00f6s\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Im hohen Ost, in Moskau stieg", "tokens": ["Im", "ho\u00b7hen", "Ost", ",", "in", "Mos\u00b7kau", "stieg"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Empor die Oriflamme,", "tokens": ["Em\u00b7por", "die", "O\u00b7ri\u00b7flam\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und alle V\u00f6lker riefen Krieg", "tokens": ["Und", "al\u00b7le", "V\u00f6l\u00b7ker", "rie\u00b7fen", "Krieg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Ha\u00df dem fremden Stamme.", "tokens": ["Und", "Ha\u00df", "dem", "frem\u00b7den", "Stam\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da brach hervor aus jeder Brust", "tokens": ["Da", "brach", "her\u00b7vor", "aus", "je\u00b7der", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Tyrannenha\u00df und Freiheitslust,", "tokens": ["Ty\u00b7ran\u00b7nen\u00b7ha\u00df", "und", "Frei\u00b7heits\u00b7lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der alten V\u00e4ter Leben.", "tokens": ["Der", "al\u00b7ten", "V\u00e4\u00b7ter", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O Knabenspiel, o Jugendlust,", "tokens": ["O", "Kna\u00b7ben\u00b7spiel", ",", "o", "Ju\u00b7gend\u00b7lust", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie mag ich eurer denken?", "tokens": ["Wie", "mag", "ich", "eu\u00b7rer", "den\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt gilt es nur, in Feindesbrust", "tokens": ["Jetzt", "gilt", "es", "nur", ",", "in", "Fein\u00b7des\u00b7brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den scharfen Speer zu senken.", "tokens": ["Den", "schar\u00b7fen", "Speer", "zu", "sen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zerfallen magst du kleines Haus,", "tokens": ["Zer\u00b7fal\u00b7len", "magst", "du", "klei\u00b7nes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit vielen Br\u00fcdern zog ich aus,", "tokens": ["Mit", "vie\u00b7len", "Br\u00fc\u00b7dern", "zog", "ich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein gr\u00f6\u00dferes zu bauen.", "tokens": ["Ein", "gr\u00f6\u00b7\u00dfe\u00b7res", "zu", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ein Haus der Freiheit und des Ruhms,", "tokens": ["Ein", "Haus", "der", "Frei\u00b7heit", "und", "des", "Ruhms", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Weisheit, Sch\u00f6nheit, St\u00e4rke.", "tokens": ["Der", "Weis\u00b7heit", ",", "Sch\u00f6n\u00b7heit", ",", "St\u00e4r\