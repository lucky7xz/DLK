{"textgrid.poem.37372": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Er war nicht unbegabt. Die Geisteskr\u00e4fte", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er war nicht unbegabt. Die Geisteskr\u00e4fte", "tokens": ["Er", "war", "nicht", "un\u00b7be\u00b7gabt", ".", "Die", "Geis\u00b7tes\u00b7kr\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gen\u00fcgten f\u00fcr die laufenden Gesch\u00e4fte.", "tokens": ["Ge\u00b7n\u00fcg\u00b7ten", "f\u00fcr", "die", "lau\u00b7fen\u00b7den", "Ge\u00b7sch\u00e4f\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur hat er die Marotte,", "tokens": ["Nur", "hat", "er", "die", "Ma\u00b7rot\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er sei der Papst. Dies sagt er oft und gern", "tokens": ["Er", "sei", "der", "Papst", ".", "Dies", "sagt", "er", "oft", "und", "gern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PDS", "VVFIN", "PPER", "ADV", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "F\u00fcr jedermann zum \u00c4rgernis und Spotte,", "tokens": ["F\u00fcr", "je\u00b7der\u00b7mann", "zum", "\u00c4r\u00b7ger\u00b7nis", "und", "Spot\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bis sie zuletzt ins Narrenhaus ihn sperr'n.", "tokens": ["Bis", "sie", "zu\u00b7letzt", "ins", "Nar\u00b7ren\u00b7haus", "ihn", "sperr'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "PPER", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein guter Freund, der ihn daselbst besuchte,", "tokens": ["Ein", "gu\u00b7ter", "Freund", ",", "der", "ihn", "da\u00b7selbst", "be\u00b7such\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fand ihn h\u00f6chst aufgeregt. Er fluchte:", "tokens": ["Fand", "ihn", "h\u00f6chst", "auf\u00b7ge\u00b7regt", ".", "Er", "fluch\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Kuckuck, das ist doch zu dumm.", "tokens": ["Zum", "Ku\u00b7ckuck", ",", "das", "ist", "doch", "zu", "dumm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PDS", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich soll ein Narr sein und wei\u00df nicht warum.", "tokens": ["Ich", "soll", "ein", "Narr", "sein", "und", "wei\u00df", "nicht", "wa\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "KON", "VVFIN", "PTKNEG", "PWAV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ja, sprach der Freund, so sind die Leute.", "tokens": ["Ja", ",", "sprach", "der", "Freund", ",", "so", "sind", "die", "Leu\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Man hat an einem Papst genug.", "tokens": ["Man", "hat", "an", "ei\u00b7nem", "Papst", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist der zweite.", "tokens": ["Du", "bist", "der", "zwei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Das eben kann man nicht vertragen.", "tokens": ["Das", "e\u00b7ben", "kann", "man", "nicht", "ver\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VMFIN", "PIS", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "H\u00f6r zu, ich will dir mal was sagen:", "tokens": ["H\u00f6r", "zu", ",", "ich", "will", "dir", "mal", "was", "sa\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPER", "VMFIN", "PPER", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wer schweigt, ist klug.", "tokens": ["Wer", "schweigt", ",", "ist", "klug", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Der Narr verstummt, als ob er \u00fcberlege.", "tokens": ["Der", "Narr", "ver\u00b7stummt", ",", "als", "ob", "er", "\u00fc\u00b7berl\u00b7e\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der gute Freund ging leise seiner Wege.", "tokens": ["Der", "gu\u00b7te", "Freund", "ging", "lei\u00b7se", "sei\u00b7ner", "We\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und schau, nach vierzehn Tagen grade", "tokens": ["Und", "schau", ",", "nach", "vier\u00b7zehn", "Ta\u00b7gen", "gra\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "$,", "APPR", "CARD", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da traf er ihn schon auf der Promenade.", "tokens": ["Da", "traf", "er", "ihn", "schon", "auf", "der", "Pro\u00b7me\u00b7na\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ei, rief der Freund, wo kommst du her?", "tokens": ["Ei", ",", "rief", "der", "Freund", ",", "wo", "kommst", "du", "her", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bist du denn jetzt der Papst nicht mehr?", "tokens": ["Bist", "du", "denn", "jetzt", "der", "Papst", "nicht", "mehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Freund, sprach der Narr und l\u00e4chelt schlau,", "tokens": ["Freund", ",", "sprach", "der", "Narr", "und", "l\u00e4\u00b7chelt", "schlau", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du scheinst zur Neugier sehr geneigt.", "tokens": ["Du", "scheinst", "zur", "Neu\u00b7gier", "sehr", "ge\u00b7neigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das, was wir sind, wei\u00df ich genau.", "tokens": ["Das", ",", "was", "wir", "sind", ",", "wei\u00df", "ich", "ge\u00b7nau", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "VAFIN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Wir alle haben unsern Sparren,", "tokens": ["Wir", "al\u00b7le", "ha\u00b7ben", "un\u00b7sern", "Spar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch sagen tun es nur die Narren.", "tokens": ["Doch", "sa\u00b7gen", "tun", "es", "nur", "die", "Nar\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Weise schweigt.", "tokens": ["Der", "Wei\u00b7se", "schweigt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}