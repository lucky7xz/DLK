{"textgrid.poem.62217": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: O, la\u00df mir die Welt der Erscheinungen stehn, sie ist so sch\u00f6n,", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O, la\u00df mir die Welt der Erscheinungen stehn, sie ist so sch\u00f6n,", "tokens": ["O", ",", "la\u00df", "mir", "die", "Welt", "der", "Er\u00b7schei\u00b7nun\u00b7gen", "stehn", ",", "sie", "ist", "so", "sch\u00f6n", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "O, la\u00df mich die Sonne immer seh'n,", "tokens": ["O", ",", "la\u00df", "mich", "die", "Son\u00b7ne", "im\u00b7mer", "seh'n", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Die B\u00e4ume und der Bl\u00e4tter Weh'n,", "tokens": ["Die", "B\u00e4u\u00b7me", "und", "der", "Bl\u00e4t\u00b7ter", "Weh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Blumen, die auf Erden steh'n,", "tokens": ["Die", "Blu\u00b7men", ",", "die", "auf", "Er\u00b7den", "steh'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sterne in den lichten H\u00f6h'n,", "tokens": ["Die", "Ster\u00b7ne", "in", "den", "lich\u00b7ten", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O, la\u00df mir das Licht, das herrliche Licht,", "tokens": ["O", ",", "la\u00df", "mir", "das", "Licht", ",", "das", "herr\u00b7li\u00b7che", "Licht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "PPER", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "++--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Ein anderes Gl\u00fcck begehr' ich nicht.", "tokens": ["Ein", "an\u00b7de\u00b7res", "Gl\u00fcck", "be\u00b7gehr'", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Du gabst's jedem Wurme, den Wesen all',", "tokens": ["Du", "gab\u00b7st's", "je\u00b7dem", "Wur\u00b7me", ",", "den", "We\u00b7sen", "all'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "ART", "NN", "PIS", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf jedem Erd- und Sonnenball,", "tokens": ["Auf", "je\u00b7dem", "Erd", "und", "Son\u00b7nen\u00b7ball", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O, schlie\u00dfe mich nicht, nicht mich g'rade aus", "tokens": ["O", ",", "schlie\u00b7\u00dfe", "mich", "nicht", ",", "nicht", "mich", "g'\u00b7ra\u00b7de", "aus"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "PTKNEG", "$,", "PTKNEG", "PPER", "VVFIN", "PTKVZ"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Aus Deines Lichtes gl\u00fccklichem Haus,", "tokens": ["Aus", "Dei\u00b7nes", "Lich\u00b7tes", "gl\u00fcck\u00b7li\u00b7chem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "O, la\u00df mich die Sonne immer seh'n,", "tokens": ["O", ",", "la\u00df", "mich", "die", "Son\u00b7ne", "im\u00b7mer", "seh'n", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.6": {"text": "Die Berge und die gr\u00fcnen Seen,", "tokens": ["Die", "Ber\u00b7ge", "und", "die", "gr\u00fc\u00b7nen", "Seen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die B\u00e4ume und der Bl\u00e4tter Weh'n,", "tokens": ["Die", "B\u00e4u\u00b7me", "und", "der", "Bl\u00e4t\u00b7ter", "Weh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Blumen, die auf Erden steh'n.", "tokens": ["Die", "Blu\u00b7men", ",", "die", "auf", "Er\u00b7den", "steh'", "n."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}