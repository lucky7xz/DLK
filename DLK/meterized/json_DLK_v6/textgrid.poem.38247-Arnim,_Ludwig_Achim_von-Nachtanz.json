{"textgrid.poem.38247": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Nachtanz", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schau wie er trabt, der wackre Gaul,", "tokens": ["Schau", "wie", "er", "trabt", ",", "der", "wack\u00b7re", "Gaul", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Auf scharfe Sporn thut er nit harren;", "tokens": ["Auf", "schar\u00b7fe", "Sporn", "thut", "er", "nit", "har\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Stroh, Heu, dient gar nit f\u00fcr sein Maul,", "tokens": ["Stroh", ",", "Heu", ",", "dient", "gar", "nit", "f\u00fcr", "sein", "Maul", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Bei ihm kann man das alles sparen.", "tokens": ["Bei", "ihm", "kann", "man", "das", "al\u00b7les", "spa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PIS", "ART", "PIS", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Ein reichen Herrn mu\u00df es traun han,", "tokens": ["Ein", "rei\u00b7chen", "Herrn", "mu\u00df", "es", "traun", "han", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "VAFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der es allzeit so wohl mag warten,", "tokens": ["Der", "es", "all\u00b7zeit", "so", "wohl", "mag", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der dies R\u00f6\u00dflein fein z\u00e4umen kann,", "tokens": ["Der", "dies", "R\u00f6\u00df\u00b7lein", "fein", "z\u00e4u\u00b7men", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "NN", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu reiten es in seim Lustgarten.", "tokens": ["Zu", "rei\u00b7ten", "es", "in", "seim", "Lust\u00b7gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}