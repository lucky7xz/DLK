{"textgrid.poem.55640": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "[woher ich kam? Es ist noch eine Frage]", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Woher ich kam? Es ist noch eine Frage,", "tokens": ["Wo\u00b7her", "ich", "kam", "?", "Es", "ist", "noch", "ei\u00b7ne", "Fra\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Weg hierher, der ist mir kaum bewu\u00dft,", "tokens": ["Mein", "Weg", "hier\u00b7her", ",", "der", "ist", "mir", "kaum", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PAV", "$,", "PRELS", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Heut nun und hier, am himmelfrohen Tage,", "tokens": ["Heut", "nun", "und", "hier", ",", "am", "him\u00b7mel\u00b7fro\u00b7hen", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Begegnen sich, wie Freunde, Schmerz und Lust.", "tokens": ["Be\u00b7geg\u00b7nen", "sich", ",", "wie", "Freun\u00b7de", ",", "Schmerz", "und", "Lust", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "$,", "PWAV", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O s\u00fc\u00dfes Gl\u00fcck, wenn beide sich vereinen!", "tokens": ["O", "s\u00fc\u00b7\u00dfes", "Gl\u00fcck", ",", "wenn", "bei\u00b7de", "sich", "ver\u00b7ei\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "KOUS", "PIS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Einsam, wer m\u00f6chte lachen, m\u00f6chte weinen?", "tokens": ["Ein\u00b7sam", ",", "wer", "m\u00f6ch\u00b7te", "la\u00b7chen", ",", "m\u00f6ch\u00b7te", "wei\u00b7nen", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWS", "VMFIN", "VVINF", "$,", "VMFIN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}