{"dta.poem.21701": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Odins H\u00f6llenfahrt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Rasch auf sprang der K\u00f6nig gross,", "tokens": ["Rasch", "auf", "sprang", "der", "K\u00f6\u00b7nig", "gross", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "VVFIN", "ART", "NN", "ADJA", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sattelte sein kohlschwarzes Ross,", "tokens": ["Sat\u00b7tel\u00b7te", "sein", "kohl\u00b7schwar\u00b7zes", "Ross", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Reitet die Schl\u00fccht' und Schr\u00fcnd' hindurch,", "tokens": ["Rei\u00b7tet", "die", "Schl\u00fccht'", "und", "Schr\u00fcnd'", "hin\u00b7durch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "NN", "PAV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Gelangt zu Helens d\u00fcstrer Burg.", "tokens": ["Ge\u00b7langt", "zu", "He\u00b7lens", "d\u00fcst\u00b7rer", "Burg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein gewahrt der H\u00f6llenhund;", "tokens": ["Sein", "ge\u00b7wahrt", "der", "H\u00f6l\u00b7len\u00b7hund", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Weit aufsperrt er den grimmen Schlund.", "tokens": ["Weit", "auf\u00b7sperrt", "er", "den", "grim\u00b7men", "Schlund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem Rachen grauss, den Hauern stark", "tokens": ["Dem", "Ra\u00b7chen", "grauss", ",", "den", "Hau\u00b7ern", "stark"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Entgeifert Schaum und Menschenmark.", "tokens": ["Ent\u00b7gei\u00b7fert", "Schaum", "und", "Men\u00b7schen\u00b7mark", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er schnaubt, er schnarcht den K\u00f6nig an.", "tokens": ["Er", "schnaubt", ",", "er", "schnarcht", "den", "K\u00f6\u00b7nig", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nichts irr't den zauberkundigen Mann.", "tokens": ["Nichts", "irr't", "den", "zau\u00b7ber\u00b7kun\u00b7di\u00b7gen", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Mit funkelndem Auge, mit knirschendem Fang", "tokens": ["Mit", "fun\u00b7keln\u00b7dem", "Au\u00b7ge", ",", "mit", "knir\u00b7schen\u00b7dem", "Fang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.12": {"text": "Mit lautem Geklaff verfolgt er ihn lang.", "tokens": ["Mit", "lau\u00b7tem", "Ge\u00b7klaff", "ver\u00b7folgt", "er", "ihn", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Der K\u00f6nig schreitet raschen Schritts,", "tokens": ["Der", "K\u00f6\u00b7nig", "schrei\u00b7tet", "ra\u00b7schen", "Schritts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Die Erd' erdr\u00f6hnt der Kraft des Tritts.", "tokens": ["Die", "Erd'", "er\u00b7dr\u00f6hnt", "der", "Kraft", "des", "Tritts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Furchtlos schaut er herab herauf,", "tokens": ["Furcht\u00b7los", "schaut", "er", "her\u00b7ab", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.16": {"text": "Der H\u00f6lle Pforten thun sich auf.", "tokens": ["Der", "H\u00f6l\u00b7le", "Pfor\u00b7ten", "thun", "sich", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Rechts ein Pfeiler am \u00f6stlichen Thor,", "tokens": ["Rechts", "ein", "Pfei\u00b7ler", "am", "\u00f6st\u00b7li\u00b7chen", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Der K\u00f6nig ihn zum Sitz erkohr,", "tokens": ["Der", "K\u00f6\u00b7nig", "ihn", "zum", "Sitz", "er\u00b7kohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo tausend Jahr im Grabe tief", "tokens": ["Wo", "tau\u00b7send", "Jahr", "im", "Gra\u00b7be", "tief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "CARD", "NN", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die zukunf", "tokens": ["Die", "zu\u00b7kunf"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Der K\u00f6nig schaut zum starren Nord.", "tokens": ["Der", "K\u00f6\u00b7nig", "schaut", "zum", "star\u00b7ren", "Nord", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dreymal raunt er das Runenwort.", "tokens": ["Drey\u00b7mal", "raunt", "er", "das", "Ru\u00b7nen\u00b7wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Dreymal schreibt er den Spruch in Sand,", "tokens": ["Drey\u00b7mal", "schreibt", "er", "den", "Spruch", "in", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Der die Todten aus den Gr\u00e4bern bannt.", "tokens": ["Der", "die", "Tod\u00b7ten", "aus", "den", "Gr\u00e4\u00b7bern", "bannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Nun s\u00e4uselt's aus hohler Tief hervor.", "tokens": ["Nun", "s\u00e4u\u00b7selt's", "aus", "hoh\u00b7ler", "Tief", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Dumpf murmelt's in sein lauschend Ohr.", "tokens": ["Dumpf", "mur\u00b7melt's", "in", "sein", "lau\u00b7schend", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}