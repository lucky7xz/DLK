{"textgrid.poem.63000": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Santon Hadem sa\u00df an der Kaskade", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Santon Hadem sa\u00df an der Kaskade", "tokens": ["Der", "San\u00b7ton", "Ha\u00b7dem", "sa\u00df", "an", "der", "Kas\u00b7ka\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "NE", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor Bagdad. Pl\u00f6tzlich drang ein M\u00e4dchen sich zu ihm,", "tokens": ["Vor", "Bag\u00b7dad", ".", "Pl\u00f6tz\u00b7lich", "drang", "ein", "M\u00e4d\u00b7chen", "sich", "zu", "ihm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$.", "ADJD", "VVFIN", "ART", "NN", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Selbst Venus kam nie sch\u00f6ner aus dem Bade:", "tokens": ["Selbst", "Ve\u00b7nus", "kam", "nie", "sch\u00f6\u00b7ner", "aus", "dem", "Ba\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "O hilf mir, heilger Mann! rief sie mit Ungest\u00fcm,", "tokens": ["O", "hilf", "mir", ",", "heil\u00b7ger", "Mann", "!", "rief", "sie", "mit", "Un\u00b7ge\u00b7st\u00fcm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "ADJA", "NN", "$.", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Erhalte mir mein h\u00f6chstes Gut, die Tugend.", "tokens": ["Er\u00b7hal\u00b7te", "mir", "mein", "h\u00f6chs\u00b7tes", "Gut", ",", "die", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein B\u00f6sewicht, der junge Muselim,", "tokens": ["Ein", "B\u00f6\u00b7se\u00b7wicht", ",", "der", "jun\u00b7ge", "Mu\u00b7se\u00b7lim", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Stellt meiner unerfahrnen Jugend", "tokens": ["Stellt", "mei\u00b7ner", "un\u00b7er\u00b7fahr\u00b7nen", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So hitzig nach, da\u00df ich kaum widerstehen kann:", "tokens": ["So", "hit\u00b7zig", "nach", ",", "da\u00df", "ich", "kaum", "wi\u00b7der\u00b7ste\u00b7hen", "kann", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was soll ich thun? \u2013 Nach kurzem Schweigen", "tokens": ["Was", "soll", "ich", "thun", "?", "\u2013", "Nach", "kur\u00b7zem", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$.", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Versetzt der M\u00f6nch: Hier ist ein Talisman;", "tokens": ["Ver\u00b7setzt", "der", "M\u00f6nch", ":", "Hier", "ist", "ein", "Ta\u00b7lis\u00b7man", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der wird ihm dein Gesicht so h\u00e4\u00dflich zeigen,", "tokens": ["Der", "wird", "ihm", "dein", "Ge\u00b7sicht", "so", "h\u00e4\u00df\u00b7lich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da\u00df er, so lang der Ring an deinem Finger steckt,", "tokens": ["Da\u00df", "er", ",", "so", "lang", "der", "Ring", "an", "dei\u00b7nem", "Fin\u00b7ger", "steckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sich, wie durch ein Phantom geschreckt,", "tokens": ["Sich", ",", "wie", "durch", "ein", "Phan\u00b7tom", "ge\u00b7schreckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$,", "PWAV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.14": {"text": "Von dir entfernen wird. Die holde Sch\u00f6ne", "tokens": ["Von", "dir", "ent\u00b7fer\u00b7nen", "wird", ".", "Die", "hol\u00b7de", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "VVINF", "VAFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Empf\u00e4ngt den Schmuck aus seiner Hand", "tokens": ["Emp\u00b7f\u00e4ngt", "den", "Schmuck", "aus", "sei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und danket ihm mit einer frohen Thr\u00e4ne.", "tokens": ["Und", "dan\u00b7ket", "ihm", "mit", "ei\u00b7ner", "fro\u00b7hen", "Thr\u00e4\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Es war der dritte Tag, als er am Tigerstrand", "tokens": ["Es", "war", "der", "drit\u00b7te", "Tag", ",", "als", "er", "am", "Ti\u00b7ger\u00b7strand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "An ihres Buhlen Arm, in einem Busch sie fand:", "tokens": ["An", "ih\u00b7res", "Buh\u00b7len", "Arm", ",", "in", "ei\u00b7nem", "Busch", "sie", "fand", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Was macht der Talisman, bedr\u00e4ngte Nuredine?", "tokens": ["Was", "macht", "der", "Ta\u00b7lis\u00b7man", ",", "be\u00b7dr\u00e4ng\u00b7te", "Nu\u00b7re\u00b7di\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Rief er ihr l\u00e4chelnd zu. Das arme Kind", "tokens": ["Rief", "er", "ihr", "l\u00e4\u00b7chelnd", "zu", ".", "Das", "ar\u00b7me", "Kind"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Verstummt, und mit verwirrter Miene", "tokens": ["Ver\u00b7stummt", ",", "und", "mit", "ver\u00b7wirr\u00b7ter", "Mie\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Zog sie die Hand zur\u00fcck. Der Santon war nicht blind:", "tokens": ["Zog", "sie", "die", "Hand", "zu\u00b7r\u00fcck", ".", "Der", "San\u00b7ton", "war", "nicht", "blind", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$.", "ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich h\u00e4tte dich, sprach er, so hart nicht pr\u00fcfen sollen;", "tokens": ["Ich", "h\u00e4t\u00b7te", "dich", ",", "sprach", "er", ",", "so", "hart", "nicht", "pr\u00fc\u00b7fen", "sol\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ADV", "ADJD", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.24": {"text": "Wo wird ein M\u00e4dchen in der Welt,", "tokens": ["Wo", "wird", "ein", "M\u00e4d\u00b7chen", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Auch selbst, wenn es dadurch der Unschuld Kranz erh\u00e4lt,", "tokens": ["Auch", "selbst", ",", "wenn", "es", "da\u00b7durch", "der", "Un\u00b7schuld", "Kranz", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "PAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Dem J\u00fcngling h\u00e4\u00dflich scheinen wollen?", "tokens": ["Dem", "J\u00fcng\u00b7ling", "h\u00e4\u00df\u00b7lich", "schei\u00b7nen", "wol\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}