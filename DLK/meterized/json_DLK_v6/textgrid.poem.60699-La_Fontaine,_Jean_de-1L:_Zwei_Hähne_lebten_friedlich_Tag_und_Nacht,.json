{"textgrid.poem.60699": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwei H\u00e4hne lebten friedlich Tag und Nacht,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei H\u00e4hne lebten friedlich Tag und Nacht,", "tokens": ["Zwei", "H\u00e4h\u00b7ne", "leb\u00b7ten", "fried\u00b7lich", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bis eine Henne kam; da war der Streit entfacht.", "tokens": ["Bis", "ei\u00b7ne", "Hen\u00b7ne", "kam", ";", "da", "war", "der", "Streit", "ent\u00b7facht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$.", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Liebe, um dich entstand das wilde Spiel,", "tokens": ["Lie\u00b7be", ",", "um", "dich", "ent\u00b7stand", "das", "wil\u00b7de", "Spiel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Dem Troja einst zum Opfer fiel,", "tokens": ["Dem", "Tro\u00b7ja", "einst", "zum", "Op\u00b7fer", "fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Um dich nur wurde einst in hei\u00dfer Schlacht", "tokens": ["Um", "dich", "nur", "wur\u00b7de", "einst", "in", "hei\u00b7\u00dfer", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "ADV", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von G\u00f6tterblut sogar der Xanthos rot gemacht!", "tokens": ["Von", "G\u00f6t\u00b7ter\u00b7blut", "so\u00b7gar", "der", "Xan\u00b7thos", "rot", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NE", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Kampf der H\u00e4hne w\u00e4hrte lang genug,", "tokens": ["Der", "Kampf", "der", "H\u00e4h\u00b7ne", "w\u00e4hr\u00b7te", "lang", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Auf seinen L\u00e4rm hin war bald alles da,", "tokens": ["Auf", "sei\u00b7nen", "L\u00e4rm", "hin", "war", "bald", "al\u00b7les", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VAFIN", "ADV", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Was einen Kamm auf seinem Kopfe trug,", "tokens": ["Was", "ei\u00b7nen", "Kamm", "auf", "sei\u00b7nem", "Kop\u00b7fe", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und mehr als ", "tokens": ["Und", "mehr", "als"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIS", "KOKOM"], "meter": "+--", "measure": "dactylic.init"}, "line.11": {"text": "War Preis des Siegers. Der Besiegte aber schlug", "tokens": ["War", "Preis", "des", "Sie\u00b7gers", ".", "Der", "Be\u00b7sieg\u00b7te", "a\u00b7ber", "schlug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "ART", "NN", "$.", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sich seitw\u00e4rts in die Hecken voller Scham,", "tokens": ["Sich", "seit\u00b7w\u00e4rts", "in", "die", "He\u00b7cken", "vol\u00b7ler", "Scham", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Beweinte sein Geschick und seufzte, da er sah,", "tokens": ["Be\u00b7wein\u00b7te", "sein", "Ge\u00b7schick", "und", "seufz\u00b7te", ",", "da", "er", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wie sein Rivale stolz der Sch\u00f6nen Liebe nahm.", "tokens": ["Wie", "sein", "Ri\u00b7va\u00b7le", "stolz", "der", "Sch\u00f6\u00b7nen", "Lie\u00b7be", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADJD", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Dies fachte neu den Zorn des Unterlegnen an;", "tokens": ["Dies", "fach\u00b7te", "neu", "den", "Zorn", "des", "Un\u00b7ter\u00b7leg\u00b7nen", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er schliff den Schnabel, schlug die Luft und seine Seiten,", "tokens": ["Er", "schliff", "den", "Schna\u00b7bel", ",", "schlug", "die", "Luft", "und", "sei\u00b7ne", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Um derart sich zu neuem Streite zu bereiten;", "tokens": ["Um", "der\u00b7art", "sich", "zu", "neu\u00b7em", "Strei\u00b7te", "zu", "be\u00b7rei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "PRF", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Denn nichts mehr hielt den Eifers\u00fcchtigen in Bann.", "tokens": ["Denn", "nichts", "mehr", "hielt", "den", "Ei\u00b7fer\u00b7s\u00fcch\u00b7ti\u00b7gen", "in", "Bann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "VVFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch war's nicht n\u00f6tig, da\u00df er neu den Krieg begann.", "tokens": ["Doch", "wa\u00b7r's", "nicht", "n\u00f6\u00b7tig", ",", "da\u00df", "er", "neu", "den", "Krieg", "be\u00b7gann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADJD", "$,", "KOUS", "PPER", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sein \u00dcberwinder flog zum Dach hinan", "tokens": ["Sein", "\u00dc\u00b7berw\u00b7in\u00b7der", "flog", "zum", "Dach", "hi\u00b7nan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und kr\u00e4hte laut Triumph. Ein Geier", "tokens": ["Und", "kr\u00e4h\u00b7te", "laut", "Tri\u00b7umph", ".", "Ein", "Gei\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ersp\u00e4hte dort den eitlen Schreier.", "tokens": ["Er\u00b7sp\u00e4h\u00b7te", "dort", "den", "eit\u00b7len", "Schrei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Lebt wohl nun, Lieb und Ruhm,", "tokens": ["Lebt", "wohl", "nun", ",", "Lieb", "und", "Ruhm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Leb wohl, du Heldentum,", "tokens": ["Leb", "wohl", ",", "du", "Hel\u00b7den\u00b7tum", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Da alles nebst dem Siegeslied", "tokens": ["Da", "al\u00b7les", "nebst", "dem", "Sie\u00b7ges\u00b7lied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In Geierkrallen j\u00e4h verschied!", "tokens": ["In", "Gei\u00b7er\u00b7kral\u00b7len", "j\u00e4h", "ver\u00b7schied", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Infolge dieser F\u00fcgung kehrt der andre Hahn", "tokens": ["In\u00b7fol\u00b7ge", "die\u00b7ser", "F\u00fc\u00b7gung", "kehrt", "der", "and\u00b7re", "Hahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PDAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zum Hennenvolk zur\u00fcck und spielte den Galan;", "tokens": ["Zum", "Hen\u00b7nen\u00b7volk", "zu\u00b7r\u00fcck", "und", "spiel\u00b7te", "den", "Ga\u00b7lan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Stellt selbst euch das Gegacker vor", "tokens": ["Stellt", "selbst", "euch", "das", "Ge\u00b7ga\u00b7cker", "vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Von seinem gro\u00dfen Frauenchor!", "tokens": ["Von", "sei\u00b7nem", "gro\u00b7\u00dfen", "Frau\u00b7enc\u00b7hor", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Gern spielt das Schicksal den Betr\u00fcger.", "tokens": ["Gern", "spielt", "das", "Schick\u00b7sal", "den", "Be\u00b7tr\u00fc\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am eignen Untergang schafft jeder eitle Sieger.", "tokens": ["Am", "eig\u00b7nen", "Un\u00b7ter\u00b7gang", "schafft", "je\u00b7der", "eit\u00b7le", "Sie\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mi\u00dftraut dem Gl\u00fcck und nehmt euch wohl in acht,", "tokens": ["Mi\u00df\u00b7traut", "dem", "Gl\u00fcck", "und", "nehmt", "euch", "wohl", "in", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "VVFIN", "PPER", "ADV", "APPR", "CARD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nachdem ihr Sieger bliebt in einer Schlacht.", "tokens": ["Nach\u00b7dem", "ihr", "Sie\u00b7ger", "bliebt", "in", "ei\u00b7ner", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}