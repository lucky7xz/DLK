{"textgrid.poem.42950": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Aus der Kundenkunde", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Kunden kommen und gehn,", "tokens": ["Die", "Kun\u00b7den", "kom\u00b7men", "und", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Gro\u00dfeltern, Eltern und Kind.", "tokens": ["Gro\u00df\u00b7el\u00b7tern", ",", "El\u00b7tern", "und", "Kind", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Doch wenn es schlimme sind,", "tokens": ["Doch", "wenn", "es", "schlim\u00b7me", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dann bleiben sie lange stehn;", "tokens": ["Dann", "blei\u00b7ben", "sie", "lan\u00b7ge", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Sekundenkunden", "tokens": ["Die", "Se\u00b7kun\u00b7den\u00b7kun\u00b7den"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Sind noch nicht erfunden.", "tokens": ["Sind", "noch", "nicht", "er\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Die Kunden kaufen und zahlen,", "tokens": ["Die", "Kun\u00b7den", "kau\u00b7fen", "und", "zah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Doch manche wollen nur Waren besehn,", "tokens": ["Doch", "man\u00b7che", "wol\u00b7len", "nur", "Wa\u00b7ren", "be\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sich orientieren. Man nennt sie", "tokens": ["Sich", "o\u00b7rien\u00b7tie\u00b7ren", ".", "Man", "nennt", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "VVINF", "$.", "PIS", "VVFIN", "PPER"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "\u00bbsehleute\u00ab und \u00bbOrientalen\u00ab;", "tokens": ["\u00bb", "seh\u00b7leu\u00b7te", "\u00ab", "und", "\u00bb", "O\u00b7rien\u00b7ta\u00b7len", "\u00ab", ";"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$(", "KON", "$(", "NN", "$(", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der flei\u00dfige Kaufmann kennt sie.", "tokens": ["Der", "flei\u00b7\u00dfi\u00b7ge", "Kauf\u00b7mann", "kennt", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Es stottern und feilschen die Kunden", "tokens": ["Es", "stot\u00b7tern", "und", "feil\u00b7schen", "die", "Kun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und schwatzen und lassen sich stunden.", "tokens": ["Und", "schwat\u00b7zen", "und", "las\u00b7sen", "sich", "stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KON", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und stehlen sogar. Dagegen stiehlt nie", "tokens": ["Und", "steh\u00b7len", "so\u00b7gar", ".", "Da\u00b7ge\u00b7gen", "stiehlt", "nie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "$.", "PAV", "VVFIN", "ADV"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die aristokratische Kleptomanie.", "tokens": ["Die", "a\u00b7ris\u00b7to\u00b7kra\u00b7ti\u00b7sche", "Klep\u00b7to\u00b7ma\u00b7nie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Der lockere Kunde von Beruf", "tokens": ["Der", "lo\u00b7cke\u00b7re", "Kun\u00b7de", "von", "Be\u00b7ruf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hat meistens einen Pferdehuf.", "tokens": ["Hat", "meis\u00b7tens", "ei\u00b7nen", "Pfer\u00b7de\u00b7huf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wer seinen Kunden k\u00fcndigt", "tokens": ["Wer", "sei\u00b7nen", "Kun\u00b7den", "k\u00fcn\u00b7digt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und meint, es ginge so: allein,", "tokens": ["Und", "meint", ",", "es", "gin\u00b7ge", "so", ":", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$.", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Selber sein eigener Kunde zu sein,", "tokens": ["Sel\u00b7ber", "sein", "ei\u00b7ge\u00b7ner", "Kun\u00b7de", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "PTKZU", "VAINF", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Der wird leicht vom Schicksal entm\u00fcndigt.", "tokens": ["Der", "wird", "leicht", "vom", "Schick\u00b7sal", "ent\u00b7m\u00fcn\u00b7digt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}