{"textgrid.poem.55639": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "[frage nicht, durch welche Pforte]", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frage nicht, durch welche Pforte", "tokens": ["Fra\u00b7ge", "nicht", ",", "durch", "wel\u00b7che", "Pfor\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "APPR", "PWAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du in Gottes Stadt gekommen,", "tokens": ["Du", "in", "Got\u00b7tes", "Stadt", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sondern bleib am stillen Orte,", "tokens": ["Son\u00b7dern", "bleib", "am", "stil\u00b7len", "Or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo du einmal Platz genommen.", "tokens": ["Wo", "du", "ein\u00b7mal", "Platz", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schaue dann umher nach Weisen", "tokens": ["Schau\u00b7e", "dann", "um\u00b7her", "nach", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKVZ", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und nach M\u00e4cht'gen, die befehlen;", "tokens": ["Und", "nach", "M\u00e4cht'\u00b7gen", ",", "die", "be\u00b7feh\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "PRELS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jene werden unterweisen,", "tokens": ["Je\u00b7ne", "wer\u00b7den", "un\u00b7ter\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese Tat und Kr\u00e4fte st\u00e4hlen.", "tokens": ["Die\u00b7se", "Tat", "und", "Kr\u00e4f\u00b7te", "st\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn du n\u00fctzlich und gelassen", "tokens": ["Wenn", "du", "n\u00fctz\u00b7lich", "und", "ge\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "KON", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So dem Staate treu geblieben,", "tokens": ["So", "dem", "Staa\u00b7te", "treu", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wisse! niemand wird dich hassen,", "tokens": ["Wis\u00b7se", "!", "nie\u00b7mand", "wird", "dich", "has\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PIS", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dich werden viele lieben.", "tokens": ["Und", "dich", "wer\u00b7den", "vie\u00b7le", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und der F\u00fcrst erkennt die Treue,", "tokens": ["Und", "der", "F\u00fcrst", "er\u00b7kennt", "die", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie erh\u00e4lt die Tat lebendig;", "tokens": ["Sie", "er\u00b7h\u00e4lt", "die", "Tat", "le\u00b7ben\u00b7dig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann bew\u00e4hrt sich auch das Neue", "tokens": ["Dann", "be\u00b7w\u00e4hrt", "sich", "auch", "das", "Neu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "N\u00e4chst dem Alten erst best\u00e4ndig.", "tokens": ["N\u00e4chst", "dem", "Al\u00b7ten", "erst", "be\u00b7st\u00e4n\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}