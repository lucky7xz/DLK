{"textgrid.poem.48824": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "49. Er bittet sie zu sich", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erfreue mich und dich, o Freude meiner Seelen,", "tokens": ["Er\u00b7freu\u00b7e", "mich", "und", "dich", ",", "o", "Freu\u00b7de", "mei\u00b7ner", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "PPER", "$,", "FM", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ohn' die ich traurig noch bei h\u00f6chster Wonne bin.", "tokens": ["ohn'", "die", "ich", "trau\u00b7rig", "noch", "bei", "h\u00f6chs\u00b7ter", "Won\u00b7ne", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "ADV", "APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Komm, du mein selber Ich, komm, Liebste komm dorthin,", "tokens": ["Komm", ",", "du", "mein", "sel\u00b7ber", "Ich", ",", "komm", ",", "Liebs\u00b7te", "komm", "dor\u00b7thin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "PPOSAT", "ADV", "PPER", "$,", "VVFIN", "$,", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wo wir uns beiderseits oft pflegen zu verh\u00f6len.", "tokens": ["wo", "wir", "uns", "bei\u00b7der\u00b7seits", "oft", "pfle\u00b7gen", "zu", "ver\u00b7h\u00f6\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "ADV", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich bin, Schatz, krank nach dir. Komm, la\u00df mich nicht so qu\u00e4len.", "tokens": ["Ich", "bin", ",", "Schatz", ",", "krank", "nach", "dir", ".", "Komm", ",", "la\u00df", "mich", "nicht", "so", "qu\u00e4\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "NN", "$,", "ADJD", "APPR", "PPER", "$.", "VVFIN", "$,", "VVIMP", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier wart' ich deines Trosts, den du mir, o mein Sinn,", "tokens": ["Hier", "wart'", "ich", "dei\u00b7nes", "Trosts", ",", "den", "du", "mir", ",", "o", "mein", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "PRELS", "PPER", "PPER", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "alleine geben kanst, komm, meine Tr\u00f6sterin.", "tokens": ["al\u00b7lei\u00b7ne", "ge\u00b7ben", "kanst", ",", "komm", ",", "mei\u00b7ne", "Tr\u00f6s\u00b7te\u00b7rin", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$,", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Hier findest du und ich, was ich und du erwelen;", "tokens": ["Hier", "fin\u00b7dest", "du", "und", "ich", ",", "was", "ich", "und", "du", "er\u00b7we\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "PPER", "$,", "PWS", "PPER", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "kein Gott, kein Mensch, kein Wild und keine Kreatur", "tokens": ["kein", "Gott", ",", "kein", "Mensch", ",", "kein", "Wild", "und", "kei\u00b7ne", "Kre\u00b7a\u00b7tur"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ist hier, auch keine Luft, ohn' die alleine nur,", "tokens": ["ist", "hier", ",", "auch", "kei\u00b7ne", "Luft", ",", "ohn'", "die", "al\u00b7lei\u00b7ne", "nur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "ADV", "PIAT", "NN", "$,", "APPR", "ART", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die ich, ich Seufzender, alleine nach dir schicke.", "tokens": ["die", "ich", ",", "ich", "Seuf\u00b7zen\u00b7der", ",", "al\u00b7lei\u00b7ne", "nach", "dir", "schi\u00b7cke", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "$,", "PPER", "NN", "$,", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Tu's, Herze, sei bald hier! K\u00f6mst oder k\u00f6mst du nicht,", "tokens": ["Tu's", ",", "Her\u00b7ze", ",", "sei", "bald", "hier", "!", "K\u00f6mst", "o\u00b7der", "k\u00f6mst", "du", "nicht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "VAFIN", "ADV", "ADV", "$.", "NN", "KON", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "so h\u00f6re, was zu dir dein eignes Herze spricht:", "tokens": ["so", "h\u00f6\u00b7re", ",", "was", "zu", "dir", "dein", "eig\u00b7nes", "Her\u00b7ze", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du bist mein gr\u00f6\u00dftes Gl\u00fcck' und gr\u00f6\u00dftes Ungel\u00fccke.", "tokens": ["Du", "bist", "mein", "gr\u00f6\u00df\u00b7tes", "Gl\u00fcck", "und", "gr\u00f6\u00df\u00b7tes", "Un\u00b7ge\u00b7l\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}