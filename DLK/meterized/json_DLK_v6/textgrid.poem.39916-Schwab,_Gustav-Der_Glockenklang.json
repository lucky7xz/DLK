{"textgrid.poem.39916": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Glockenklang", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Graf Azo, m\u00fcd vom fl\u00fccht'gen Wild,", "tokens": ["Graf", "A\u00b7zo", ",", "m\u00fcd", "vom", "fl\u00fccht'\u00b7gen", "Wild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schlief k\u00fchl im Gras bethaut;", "tokens": ["Schlief", "k\u00fchl", "im", "Gras", "be\u00b7thaut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihm naht' ein Traum, doch ohne Bild,", "tokens": ["Ihm", "naht'", "ein", "Traum", ",", "doch", "oh\u00b7ne", "Bild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In's Ohr weht ihm ein Laut:", "tokens": ["In's", "Ohr", "weht", "ihm", "ein", "Laut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er h\u00f6rt' einen hallenden Glockenklang,", "tokens": ["Er", "h\u00f6rt'", "ei\u00b7nen", "hal\u00b7len\u00b7den", "Glo\u00b7cken\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "----+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Er h\u00f6rt' einen herrlichen Kirchensang,", "tokens": ["Er", "h\u00f6rt'", "ei\u00b7nen", "herr\u00b7li\u00b7chen", "Kir\u00b7chen\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Kein Gotteshaus er schaut!", "tokens": ["Kein", "Got\u00b7tes\u00b7haus", "er", "schaut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er wiegte sanft sein Haupt im Schall,", "tokens": ["Er", "wieg\u00b7te", "sanft", "sein", "Haupt", "im", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die T\u00f6ne klangen aus,", "tokens": ["Die", "T\u00f6\u00b7ne", "klan\u00b7gen", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und endlich ward's ein leiser Hall,", "tokens": ["Und", "end\u00b7lich", "ward's", "ein", "lei\u00b7ser", "Hall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie weicher Bl\u00e4tter Saus.", "tokens": ["Wie", "wei\u00b7cher", "Bl\u00e4t\u00b7ter", "Saus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und als verschwunden war der Traum,", "tokens": ["Und", "als", "ver\u00b7schwun\u00b7den", "war", "der", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Lauscht in des Waldes stillen Raum", "tokens": ["Lauscht", "in", "des", "Wal\u00b7des", "stil\u00b7len", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Er sehnsuchtsvoll hinaus:", "tokens": ["Er", "sehn\u00b7suchts\u00b7voll", "hin\u00b7aus", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbo Klang, o Klang! wo find' ich dich?", "tokens": ["\u00bb", "o", "Klang", ",", "o", "Klang", "!", "wo", "find'", "ich", "dich", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "FM", "NN", "$.", "PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Klang wie aus Gottes Mund!", "tokens": ["Klang", "wie", "aus", "Got\u00b7tes", "Mund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPR", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wann gr\u00fc\u00dfest du aus L\u00fcften mich", "tokens": ["Wann", "gr\u00fc\u00b7\u00dfest", "du", "aus", "L\u00fcf\u00b7ten", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus hohem Domesrund!", "tokens": ["Aus", "ho\u00b7hem", "Do\u00b7mes\u00b7rund", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O h\u00e4tt' ich dich, du Wunderklang,", "tokens": ["O", "h\u00e4tt'", "ich", "dich", ",", "du", "Wun\u00b7der\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PRF", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So w\u00e4r' ich all' mein Leben lang", "tokens": ["So", "w\u00e4r'", "ich", "all'", "mein", "Le\u00b7ben", "lang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "An Herz und Geist gesund!\u00ab", "tokens": ["An", "Herz", "und", "Geist", "ge\u00b7sund", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und als er aufgestanden war,", "tokens": ["Und", "als", "er", "auf\u00b7ge\u00b7stan\u00b7den", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da hallt' es durch die Luft:", "tokens": ["Da", "hallt'", "es", "durch", "die", "Luft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl t\u00f6nt' es laut, wohl t\u00f6nt' es klar", "tokens": ["Wohl", "t\u00f6nt'", "es", "laut", ",", "wohl", "t\u00f6nt'", "es", "klar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch B\u00e4um' und Felsenkluft:", "tokens": ["Durch", "B\u00e4um'", "und", "Fel\u00b7sen\u00b7kluft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit Sehnen eilet er nach vorn \u2013", "tokens": ["Mit", "Seh\u00b7nen", "ei\u00b7let", "er", "nach", "vorn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch ist es nur das J\u00e4gerhorn,", "tokens": ["Doch", "ist", "es", "nur", "das", "J\u00e4\u00b7ger\u00b7horn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das ihn zum Werke ruft.", "tokens": ["Das", "ihn", "zum", "Wer\u00b7ke", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und als er tiefer in den Wald", "tokens": ["Und", "als", "er", "tie\u00b7fer", "in", "den", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und in die B\u00fcsche drang,", "tokens": ["Und", "in", "die", "B\u00fc\u00b7sche", "drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hoch \u00fcber seinem Haupte hallt'", "tokens": ["Hoch", "\u00fc\u00b7ber", "sei\u00b7nem", "Haup\u00b7te", "hallt'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein leichter, leiser Klang.", "tokens": ["Ein", "leich\u00b7ter", ",", "lei\u00b7ser", "Klang", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er streckte sich, er lauscht' empor,", "tokens": ["Er", "streck\u00b7te", "sich", ",", "er", "lauscht'", "em\u00b7por", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch nur den Wind vernahm sein Ohr,", "tokens": ["Doch", "nur", "den", "Wind", "ver\u00b7nahm", "sein", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der sich durch Wipfel schwang.", "tokens": ["Der", "sich", "durch", "Wip\u00b7fel", "schwang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und weiter in die Finsterni\u00df", "tokens": ["Und", "wei\u00b7ter", "in", "die", "Fins\u00b7ter\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lockt ihn der Wald hinein:", "tokens": ["Lockt", "ihn", "der", "Wald", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbihr Knappen, eurer Spur gewi\u00df,", "tokens": ["\u00bb", "ihr", "Knap\u00b7pen", ",", "eu\u00b7rer", "Spur", "ge\u00b7wi\u00df", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verfolgt nur Hirsch und Schwein!", "tokens": ["Ver\u00b7folgt", "nur", "Hirsch", "und", "Schwein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich jage, was kein Gl\u00fcck ersp\u00e4ht,", "tokens": ["Ich", "ja\u00b7ge", ",", "was", "kein", "Gl\u00fcck", "er\u00b7sp\u00e4ht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich jage, was kein Windspiel f\u00e4ht!\u00ab \u2013", "tokens": ["Ich", "ja\u00b7ge", ",", "was", "kein", "Wind\u00b7spiel", "f\u00e4ht", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wieder klingt's im Hain.", "tokens": ["Und", "wie\u00b7der", "klingt's", "im", "Hain", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ein weicher Schall, ein Wiegensang \u2013", "tokens": ["Ein", "wei\u00b7cher", "Schall", ",", "ein", "Wie\u00b7gen\u00b7sang", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ach, das ist wohl sein Traum?", "tokens": ["Ach", ",", "das", "ist", "wohl", "sein", "Traum", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDS", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Doch ist es nur der Wellendrang", "tokens": ["Doch", "ist", "es", "nur", "der", "Wel\u00b7len\u00b7drang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im See am steilen Saum.", "tokens": ["Im", "See", "am", "stei\u00b7len", "Saum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dort jubelt laut sein Knappenchor,", "tokens": ["Dort", "ju\u00b7belt", "laut", "sein", "Knap\u00b7pen\u00b7chor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Eber schie\u00dft getroffen vor,", "tokens": ["Ein", "E\u00b7ber", "schie\u00dft", "ge\u00b7trof\u00b7fen", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "St\u00fcrzt in des Wassers Schaum.", "tokens": ["St\u00fcrzt", "in", "des", "Was\u00b7sers", "Schaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Diener drangen durch das Rohr", "tokens": ["Die", "Die\u00b7ner", "dran\u00b7gen", "durch", "das", "Rohr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu haschen ihren Fund:", "tokens": ["Zu", "ha\u00b7schen", "ih\u00b7ren", "Fund", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da blinkt' es aus dem Schilf hervor", "tokens": ["Da", "blinkt'", "es", "aus", "dem", "Schilf", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus heller Wasser Grund;", "tokens": ["Aus", "hel\u00b7ler", "Was\u00b7ser", "Grund", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vorbei rauscht seiner Knechte Schar,", "tokens": ["Vor\u00b7bei", "rauscht", "sei\u00b7ner", "Knech\u00b7te", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Dem Grafen winkte wunderbar", "tokens": ["Dem", "Gra\u00b7fen", "wink\u00b7te", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein hohles, lichtes Rund.", "tokens": ["Ein", "hoh\u00b7les", ",", "lich\u00b7tes", "Rund", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Es ist ein Erz, ein Glockenkranz \u2013", "tokens": ["Es", "ist", "ein", "Erz", ",", "ein", "Glo\u00b7cken\u00b7kranz", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2