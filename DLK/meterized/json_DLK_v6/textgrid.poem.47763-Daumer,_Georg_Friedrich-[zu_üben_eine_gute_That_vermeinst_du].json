{"textgrid.poem.47763": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[zu \u00fcben eine gute That vermeinst du]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu \u00fcben eine gute That vermeinst du,", "tokens": ["Zu", "\u00fc\u00b7ben", "ei\u00b7ne", "gu\u00b7te", "That", "ver\u00b7meinst", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem D\u00fcrftigen eine kleine Spende reichend,", "tokens": ["Dem", "D\u00fcrf\u00b7ti\u00b7gen", "ei\u00b7ne", "klei\u00b7ne", "Spen\u00b7de", "rei\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zu \u00fcben eine gute That zugleich,", "tokens": ["Zu", "\u00fc\u00b7ben", "ei\u00b7ne", "gu\u00b7te", "That", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mir Armen eine kleine Labe weigernd.", "tokens": ["Mir", "Ar\u00b7men", "ei\u00b7ne", "klei\u00b7ne", "La\u00b7be", "wei\u00b7gernd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Urtheile besser, denke richtiger,", "tokens": ["Ur\u00b7thei\u00b7le", "bes\u00b7ser", ",", "den\u00b7ke", "rich\u00b7ti\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht huldige so ganz dem Widerspruche,", "tokens": ["Nicht", "hul\u00b7di\u00b7ge", "so", "ganz", "dem", "Wi\u00b7der\u00b7spru\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wisse, da\u00df ein Ku\u00df, dem Minnebettler,", "tokens": ["Und", "wis\u00b7se", ",", "da\u00df", "ein", "Ku\u00df", ",", "dem", "Min\u00b7ne\u00b7bett\u00b7ler", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem sterbenden in seiner Sehnsucht Weh,", "tokens": ["Dem", "ster\u00b7ben\u00b7den", "in", "sei\u00b7ner", "Sehn\u00b7sucht", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit lindem Mund gespendet, hehrer ist", "tokens": ["Mit", "lin\u00b7dem", "Mund", "ge\u00b7spen\u00b7det", ",", "heh\u00b7rer", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "ADJA", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und heiliger, als jeder andere", "tokens": ["Und", "hei\u00b7li\u00b7ger", ",", "als", "je\u00b7der", "an\u00b7de\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "$,", "KOUS", "PIAT", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der D\u00fcrftigkeit entrichtete Tribut!", "tokens": ["Der", "D\u00fcrf\u00b7tig\u00b7keit", "ent\u00b7rich\u00b7te\u00b7te", "Tri\u00b7but", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}