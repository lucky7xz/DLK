{"textgrid.poem.45602": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[\u00f6streich der Schild und Preu\u00dfen das Schwert!]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bb\u00f6streich der Schild und Preu\u00dfen das Schwert!\u00ab", "tokens": ["\u00bb", "\u00f6s\u00b7treich", "der", "Schild", "und", "Preu\u00b7\u00dfen", "das", "Schwert", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "ART", "NN", "KON", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Nur leider sind die Arme verkehrt,", "tokens": ["Nur", "lei\u00b7der", "sind", "die", "Ar\u00b7me", "ver\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Der Schild blinkt trotzend in der Rechten,", "tokens": ["Der", "Schild", "blinkt", "trot\u00b7zend", "in", "der", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und das Schwert soll mit der Linken fechten.", "tokens": ["Und", "das", "Schwert", "soll", "mit", "der", "Lin\u00b7ken", "fech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}