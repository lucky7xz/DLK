{"textgrid.poem.65645": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Eule", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vogel der ", "tokens": ["Vo\u00b7gel", "der"], "token_info": ["word", "word"], "pos": ["NE", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Ward ich genannt;", "tokens": ["Ward", "ich", "ge\u00b7nannt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich sa\u00df auf Minervens Altare,", "tokens": ["Ich", "sa\u00df", "auf", "Mi\u00b7ner\u00b7vens", "Al\u00b7ta\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr heiliges Feuer h\u00fctend.", "tokens": ["Ihr", "hei\u00b7li\u00b7ges", "Feu\u00b7er", "h\u00fc\u00b7tend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nun liegt er in Tr\u00fcmmern,", "tokens": ["Nun", "liegt", "er", "in", "Tr\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der Tempel der G\u00f6ttin", "tokens": ["Der", "Tem\u00b7pel", "der", "G\u00f6t\u00b7tin"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Auf Cecrops Burg,", "tokens": ["Auf", "Ce\u00b7crops", "Burg", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Erloschen und verweht", "tokens": ["Er\u00b7lo\u00b7schen", "und", "ver\u00b7weht"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Von ihrem Hochaltare", "tokens": ["Von", "ih\u00b7rem", "Hoc\u00b7hal\u00b7ta\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die letzten Opferfunken.", "tokens": ["Die", "letz\u00b7ten", "Op\u00b7fer\u00b7fun\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Da hab' ich der ", "tokens": ["Da", "hab'", "ich", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART"], "meter": "+---", "measure": "dactylic.init"}, "line.12": {"text": "Und schlafe den langen Tag;", "tokens": ["Und", "schla\u00b7fe", "den", "lan\u00b7gen", "Tag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Und wann die Menschen tr\u00e4umen,", "tokens": ["Und", "wann", "die", "Men\u00b7schen", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Dann schau' ich mit blitzenden Augen", "tokens": ["Dann", "schau'", "ich", "mit", "blit\u00b7zen\u00b7den", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.15": {"text": "\u00dcber die dunkle Erde", "tokens": ["\u00dc\u00b7ber", "die", "dunk\u00b7le", "Er\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.16": {"text": "Und schreie Wehe! Wehe!", "tokens": ["Und", "schrei\u00b7e", "We\u00b7he", "!", "We\u00b7he", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$.", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "\u00dcber die ", "tokens": ["\u00dc\u00b7ber", "die"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.18": {"text": "Aber die Menschen verstehn mich nicht;", "tokens": ["A\u00b7ber", "die", "Men\u00b7schen", "ver\u00b7stehn", "mich", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Sie zittern, wenn sie mich h\u00f6ren,", "tokens": ["Sie", "zit\u00b7tern", ",", "wenn", "sie", "mich", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.20": {"text": "Nennen mich Weheverk\u00fcnderin,", "tokens": ["Nen\u00b7nen", "mich", "We\u00b7he\u00b7ver\u00b7k\u00fcn\u00b7de\u00b7rin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.21": {"text": "Und ich verk\u00fcnde doch Wahrheit nur.", "tokens": ["Und", "ich", "ver\u00b7k\u00fcn\u00b7de", "doch", "Wahr\u00b7heit", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "NN", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00dcber Hellas flog ich hin", "tokens": ["\u00dc\u00b7ber", "Hel\u00b7las", "flog", "ich", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "PPER", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Um Mitternacht;", "tokens": ["Um", "Mit\u00b7ter\u00b7nacht", ";"], "token_info": ["word", "word", "punct"], "pos": ["KOUI", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Am Himmel war kein Stern zu sehn,", "tokens": ["Am", "Him\u00b7mel", "war", "kein", "Stern", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und blutigroth in Nebelwolken", "tokens": ["Und", "blu\u00b7ti\u00b7groth", "in", "Ne\u00b7bel\u00b7wol\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Schwamm des Mondes Sichel hin.", "tokens": ["Schwamm", "des", "Mon\u00b7des", "Si\u00b7chel", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber von flammenden St\u00e4dten,", "tokens": ["A\u00b7ber", "von", "flam\u00b7men\u00b7den", "St\u00e4d\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Aber von rauchenden H\u00fctten,", "tokens": ["A\u00b7ber", "von", "rau\u00b7chen\u00b7den", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Aber von gl\u00fchenden Scheiterhaufen", "tokens": ["A\u00b7ber", "von", "gl\u00fc\u00b7hen\u00b7den", "Schei\u00b7ter\u00b7hau\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.9": {"text": "War es weit und breit so hell,", "tokens": ["War", "es", "weit", "und", "breit", "so", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "ADJD", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Hell wie der Tag,", "tokens": ["Hell", "wie", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "NN", "$,"], "meter": "++-+", "measure": "iambic.di"}, "line.11": {"text": "Und ich rief Wehe! Wehe!", "tokens": ["Und", "ich", "rief", "We\u00b7he", "!", "We\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "$.", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "\u00dcber den Schimmer des hellen Tages.", "tokens": ["\u00dc\u00b7ber", "den", "Schim\u00b7mer", "des", "hel\u00b7len", "Ta\u00b7ges", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Ich h\u00f6rte blutende S\u00e4uglinge winseln", "tokens": ["Ich", "h\u00f6r\u00b7te", "blu\u00b7ten\u00b7de", "S\u00e4ug\u00b7lin\u00b7ge", "win\u00b7seln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "VVINF"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "An gemordeter M\u00fctter Br\u00fcsten,", "tokens": ["An", "ge\u00b7mor\u00b7de\u00b7ter", "M\u00fct\u00b7ter", "Br\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Sah aus den Klausen heilige Jungfraun", "tokens": ["Sah", "aus", "den", "Klau\u00b7sen", "hei\u00b7li\u00b7ge", "Jung\u00b7fraun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Schleifen zur Schlachtbank rasender Lust,", "tokens": ["Schlei\u00b7fen", "zur", "Schlacht\u00b7bank", "ra\u00b7sen\u00b7der", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Sahe die Tempel des Kreuzes", "tokens": ["Sa\u00b7he", "die", "Tem\u00b7pel", "des", "Kreu\u00b7zes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Niedergerissen in Tr\u00fcmmern liegen,", "tokens": ["Nie\u00b7der\u00b7ge\u00b7ris\u00b7sen", "in", "Tr\u00fcm\u00b7mern", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "Und die zerst\u00fcckten Gebeine", "tokens": ["Und", "die", "zer\u00b7st\u00fcck\u00b7ten", "Ge\u00b7bei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ihrer Priester dazwischen", "tokens": ["Ih\u00b7rer", "Pries\u00b7ter", "da\u00b7zwi\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "PAV"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "\u00dcber die Steine gestreut.", "tokens": ["\u00dc\u00b7ber", "die", "Stei\u00b7ne", "ge\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.10": {"text": "Da dr\u00fcckt' ich die blitzenden Augen zu", "tokens": ["Da", "dr\u00fcckt'", "ich", "die", "blit\u00b7zen\u00b7den", "Au\u00b7gen", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKZU"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Und unter mir h\u00f6rt' ich noch lange", "tokens": ["Und", "un\u00b7ter", "mir", "h\u00f6rt'", "ich", "noch", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "Ein Heulen, ein Jammern, ein Wimmern,", "tokens": ["Ein", "Heu\u00b7len", ",", "ein", "Jam\u00b7mern", ",", "ein", "Wim\u00b7mern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.13": {"text": "Ein Jauchzen, ein Fluchen, ein Knirschen \u2013", "tokens": ["Ein", "Jauch\u00b7zen", ",", "ein", "Flu\u00b7chen", ",", "ein", "Knir\u00b7schen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.14": {"text": "Dann ward es still.", "tokens": ["Dann", "ward", "es", "still", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Und ich schlug die blitzenden Augen auf,", "tokens": ["Und", "ich", "schlug", "die", "blit\u00b7zen\u00b7den", "Au\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da standen an eines Flusses Ufer", "tokens": ["Da", "stan\u00b7den", "an", "ei\u00b7nes", "Flus\u00b7ses", "U\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Heere des Kreuzes zu Ro\u00df und zu Fu\u00df;", "tokens": ["Hee\u00b7re", "des", "Kreu\u00b7zes", "zu", "Ro\u00df", "und", "zu", "Fu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Ich konnte sie nicht absehen,", "tokens": ["Ich", "konn\u00b7te", "sie", "nicht", "ab\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "So hoch ich mich mochte schwingen.", "tokens": ["So", "hoch", "ich", "mich", "moch\u00b7te", "schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PRF", "VVFIN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und Waffen trugen sie in den H\u00e4nden,", "tokens": ["Und", "Waf\u00b7fen", "tru\u00b7gen", "sie", "in", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.7": {"text": "Und ihre Blicke gl\u00fchten,", "tokens": ["Und", "ih\u00b7re", "Bli\u00b7cke", "gl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie ihre Lanzenspitzen,", "tokens": ["Wie", "ih\u00b7re", "Lan\u00b7zens\u00b7pit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Nach Blut.", "tokens": ["Nach", "Blut", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Da rief ich Wehe! Wehe!", "tokens": ["Da", "rief", "ich", "We\u00b7he", "!", "We\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$.", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Da rief ich Rache! Rache!", "tokens": ["Da", "rief", "ich", "Ra\u00b7che", "!", "Ra\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$.", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Da rief ich H\u00fclfe! H\u00fclfe!", "tokens": ["Da", "rief", "ich", "H\u00fcl\u00b7fe", "!", "H\u00fcl\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$.", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und lange h\u00e4tt' ich noch geschrien,", "tokens": ["Und", "lan\u00b7ge", "h\u00e4tt'", "ich", "noch", "ge\u00b7schri\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Da ward's im Morgen helle,", "tokens": ["Da", "ward's", "im", "Mor\u00b7gen", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Und in die Augen flimmerte", "tokens": ["Und", "in", "die", "Au\u00b7gen", "flim\u00b7mer\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Verblendend mir das Tageslicht.", "tokens": ["Ver\u00b7blen\u00b7dend", "mir", "das", "Ta\u00b7ges\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und ein Schwarm von h\u00f6hnischem Luftgesindel", "tokens": ["Und", "ein", "Schwarm", "von", "h\u00f6h\u00b7ni\u00b7schem", "Luft\u00b7ge\u00b7sin\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Flog schnarrend und pfeifend mir um das Haupt,", "tokens": ["Flog", "schnar\u00b7rend", "und", "pfei\u00b7fend", "mir", "um", "das", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "Mein Schreien \u00fcbert\u00e4ubend.", "tokens": ["Mein", "Schrei\u00b7en", "\u00fc\u00b7bert\u00b7\u00e4u\u00b7bend", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Da rief ich Wehe! Wehe!", "tokens": ["Da", "rief", "ich", "We\u00b7he", "!", "We\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$.", "NN", "$."], "meter": "-+-+