{"dta.poem.16322": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "58.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Meister (in der Brust genannt mit Andacht sei er)", "tokens": ["Mein", "Meis\u00b7ter", "(", "in", "der", "Brust", "ge\u00b7nannt", "mit", "An\u00b7dacht", "sei", "er", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "APPR", "ART", "NN", "VVPP", "APPR", "NN", "VAFIN", "PPER", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sprach auch: Melodisch klingt die durchgespielte Leier.", "tokens": ["Sprach", "auch", ":", "Me\u00b7lo\u00b7disch", "klingt", "die", "durch\u00b7ge\u00b7spiel\u00b7te", "Lei\u00b7er", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-++-+-+-+-+-", "measure": "unknown.measure.septa"}}, "stanza.2": {"line.1": {"text": "Er sprach es sich zum Trost und zur Beruhigung,", "tokens": ["Er", "sprach", "es", "sich", "zum", "Trost", "und", "zur", "Be\u00b7ru\u00b7hi\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PRF", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Weil er so sch\u00f6n noch spielt' und war schon alt genung.", "tokens": ["Weil", "er", "so", "sch\u00f6n", "noch", "spielt'", "und", "war", "schon", "alt", "ge\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ADV", "VVFIN", "KON", "VAFIN", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auch mir erzittert, und er sprachs auch mir zum Trost,", "tokens": ["Auch", "mir", "er\u00b7zit\u00b7tert", ",", "und", "er", "sprachs", "auch", "mir", "zum", "Trost", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "ADV", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Brust von anderm Schaur als von des Alters Frost.", "tokens": ["Die", "Brust", "von", "an\u00b7derm", "Schaur", "als", "von", "des", "Al\u00b7ters", "Frost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "NN", "KOKOM", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Geist, der mir dis Spiel besaitet, la\u00df es zittern", "tokens": ["Der", "Geist", ",", "der", "mir", "dis", "Spiel", "be\u00b7sai\u00b7tet", ",", "la\u00df", "es", "zit\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PDS", "NN", "VVPP", "$,", "VVIMP", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch froh in seinem Hauch, bis es daran wird splittern.", "tokens": ["Noch", "froh", "in", "sei\u00b7nem", "Hauch", ",", "bis", "es", "da\u00b7ran", "wird", "split\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PAV", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}