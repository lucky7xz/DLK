{"textgrid.poem.45913": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Meine Liebe an Sophien, die ihre Mutter ist", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Mutter, halte dein Kindlein warm", "tokens": ["O", "Mut\u00b7ter", ",", "hal\u00b7te", "dein", "Kin\u00b7dlein", "warm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVFIN", "PPOSAT", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Welt ist kalt und helle", "tokens": ["Die", "Welt", "ist", "kalt", "und", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und leg' es sanft in deinen Arm,", "tokens": ["Und", "leg'", "es", "sanft", "in", "dei\u00b7nen", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An deines Herzens Schwelle.", "tokens": ["An", "dei\u00b7nes", "Her\u00b7zens", "Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Leg' still es, wo dein Busen bebt,", "tokens": ["Leg'", "still", "es", ",", "wo", "dein", "Bu\u00b7sen", "bebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und treu herabgeb\u00fccket,", "tokens": ["Und", "treu", "her\u00b7ab\u00b7ge\u00b7b\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Harr' liebend, bis es die \u00c4uglein hebt,", "tokens": ["Harr'", "lie\u00b7bend", ",", "bis", "es", "die", "\u00c4ug\u00b7lein", "hebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zum Himmel selig blicket.", "tokens": ["Zum", "Him\u00b7mel", "se\u00b7lig", "bli\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du strahlender Augenhimmel, du,", "tokens": ["Du", "strah\u00b7len\u00b7der", "Au\u00b7gen\u00b7him\u00b7mel", ",", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du taust aus Mutteraugen,", "tokens": ["Du", "taust", "aus", "Mut\u00b7ter\u00b7au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach Herzenspochen, ach Lust, ach Ruh'!", "tokens": ["Ach", "Her\u00b7zen\u00b7spo\u00b7chen", ",", "ach", "Lust", ",", "ach", "Ruh'", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "XY", "NN", "$,", "XY", "XY", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "An deinen Br\u00fcsten saugen!", "tokens": ["An", "dei\u00b7nen", "Br\u00fcs\u00b7ten", "sau\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich schau' zu dir so Tag als Nacht,", "tokens": ["Ich", "schau'", "zu", "dir", "so", "Tag", "als", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df ewig nach dir schauen,", "tokens": ["Mu\u00df", "e\u00b7wig", "nach", "dir", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du mu\u00dft mir die mich zur Welt gebracht,", "tokens": ["Du", "mu\u00dft", "mir", "die", "mich", "zur", "Welt", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auch eine Wiege bauen.", "tokens": ["Auch", "ei\u00b7ne", "Wie\u00b7ge", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Um meine Wiege la\u00df Seide nicht,", "tokens": ["Um", "mei\u00b7ne", "Wie\u00b7ge", "la\u00df", "Sei\u00b7de", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVIMP", "NE", "PTKNEG", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "La\u00df deinen Arm sich schlingen,", "tokens": ["La\u00df", "dei\u00b7nen", "Arm", "sich", "schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und nur deiner milden Augen Licht,", "tokens": ["Und", "nur", "dei\u00b7ner", "mil\u00b7den", "Au\u00b7gen", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "La\u00df zu mir niederdringen.", "tokens": ["La\u00df", "zu", "mir", "nie\u00b7der\u00b7drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und in deines keuschen Scho\u00dfes Hut,", "tokens": ["Und", "in", "dei\u00b7nes", "keu\u00b7schen", "Scho\u00b7\u00dfes", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sollst du dein Kindlein schaukeln,", "tokens": ["Sollst", "du", "dein", "Kin\u00b7dlein", "schau\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df deine Worte so mild und gut", "tokens": ["Da\u00df", "dei\u00b7ne", "Wor\u00b7te", "so", "mild", "und", "gut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie Tr\u00e4ume um es gaukeln.", "tokens": ["Wie", "Tr\u00e4u\u00b7me", "um", "es", "gau\u00b7keln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da tr\u00e4umt mir, wie ich so ganz allein,", "tokens": ["Da", "tr\u00e4umt", "mir", ",", "wie", "ich", "so", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gewohnt dir unterm Herzen,", "tokens": ["Ge\u00b7wohnt", "dir", "un\u00b7term", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie all die Leiden die Freuden dein", "tokens": ["Wie", "all", "die", "Lei\u00b7den", "die", "Freu\u00b7den", "dein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "ART", "NN", "ART", "NN", "PPOSAT"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mich freuten und mich schmerzten.", "tokens": ["Mich", "freu\u00b7ten", "und", "mich", "schmerz\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und war deine Sehnsucht ja allzugro\u00df,", "tokens": ["Und", "war", "dei\u00b7ne", "Sehn\u00b7sucht", "ja", "all\u00b7zu\u00b7gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und wu\u00dftest nicht, wem klagen,", "tokens": ["Und", "wu\u00df\u00b7test", "nicht", ",", "wem", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da weint' ich still in deinem Scho\u00df,", "tokens": ["Da", "weint'", "ich", "still", "in", "dei\u00b7nem", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und konnte dir's nicht sagen.", "tokens": ["Und", "konn\u00b7te", "dir's", "nicht", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Oft rief ich, komm o Mutter komm", "tokens": ["Oft", "rief", "ich", ",", "komm", "o", "Mut\u00b7ter", "komm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "FM", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "K\u00fchl' dich in Liebeswogen,", "tokens": ["K\u00fchl'", "dich", "in", "Lie\u00b7bes\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da f\u00fchltest du dich so still und fromm,", "tokens": ["Da", "f\u00fchl\u00b7test", "du", "dich", "so", "still", "und", "fromm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu dir hinabgezogen.", "tokens": ["Zu", "dir", "hin\u00b7ab\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Mit Unschuldsarmen hielt fest und warm", "tokens": ["Mit", "Un\u00b7schulds\u00b7ar\u00b7men", "hielt", "fest", "und", "warm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PTKVZ", "KON", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich dich in dir umschlungen,", "tokens": ["Ich", "dich", "in", "dir", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und hab' dir kindisch Sorg' und Harm", "tokens": ["Und", "hab'", "dir", "kin\u00b7disch", "Sor\u00b7g'", "und", "Harm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "In Liedern weggesungen.", "tokens": ["In", "Lie\u00b7dern", "weg\u00b7ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Was heilig in dir zu aller Stund',", "tokens": ["Was", "hei\u00b7lig", "in", "dir", "zu", "al\u00b7ler", "Stund'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das bin ich all gewesen,", "tokens": ["Das", "bin", "ich", "all", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PIAT", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O k\u00fc\u00df' mich s\u00fc\u00dfer Mund gesund,", "tokens": ["O", "k\u00fc\u00df'", "mich", "s\u00fc\u00b7\u00dfer", "Mund", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil du an mir genesen.", "tokens": ["Weil", "du", "an", "mir", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.12": {"line.1": {"text": "O Mutter halte dein Kindlein warm", "tokens": ["O", "Mut\u00b7ter", "hal\u00b7te", "dein", "Kin\u00b7dlein", "warm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VVFIN", "PPOSAT", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Welt ist kalt und helle", "tokens": ["Die", "Welt", "ist", "kalt", "und", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und leg' es sanft, bist du zu arm,", "tokens": ["Und", "leg'", "es", "sanft", ",", "bist", "du", "zu", "arm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "VAFIN", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hin an des Todes Schwelle.", "tokens": ["Hin", "an", "des", "To\u00b7des", "Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.13": {"line.1": {"text": "Leg' es in Linnen die du gewebt,", "tokens": ["Leg'", "es", "in", "Lin\u00b7nen", "die", "du", "ge\u00b7webt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "ART", "PPER", "VVPP", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Zu Blumen die du gepfl\u00fccket,", "tokens": ["Zu", "Blu\u00b7men", "die", "du", "ge\u00b7pfl\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "PPER", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Stirb mit, da\u00df wenn es die \u00c4uglein hebt,", "tokens": ["Stirb", "mit", ",", "da\u00df", "wenn", "es", "die", "\u00c4ug\u00b7lein", "hebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "KOUS", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Im Himmel es dich erblicket.", "tokens": ["Im", "Him\u00b7mel", "es", "dich", "er\u00b7bli\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PRF", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "So lallt zu dir mein frommes Herz,", "tokens": ["So", "lallt", "zu", "dir", "mein", "from\u00b7mes", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und nimmer lernt es sprechen,", "tokens": ["Und", "nim\u00b7mer", "lernt", "es", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Blickt ewig zu dir, blickt himmelw\u00e4rts,", "tokens": ["Blickt", "e\u00b7wig", "zu", "dir", ",", "blickt", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+---", "measure": "unknown.measur