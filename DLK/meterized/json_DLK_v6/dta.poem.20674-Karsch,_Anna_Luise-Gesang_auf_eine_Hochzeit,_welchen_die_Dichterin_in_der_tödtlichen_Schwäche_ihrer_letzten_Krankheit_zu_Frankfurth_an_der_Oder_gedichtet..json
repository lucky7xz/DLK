{"dta.poem.20674": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Gesang auf eine Hochzeit,  \n  welchen  \n die Dichterin in der t\u00f6dtlichen Schw\u00e4che ihrer  \n letzten Krankheit  \n zu Frankfurth an der Oder gedichtet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00df ist die Liebe der Ehen,                 ", "tokens": ["S\u00fc\u00df", "ist", "die", "Lie\u00b7be", "der", "E\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Geheiliget wird ihre Glut,", "tokens": ["Ge\u00b7hei\u00b7li\u00b7get", "wird", "ih\u00b7re", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nimmer kann sie vergehen", "tokens": ["Und", "nim\u00b7mer", "kann", "sie", "ver\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In Herzen bieder und gut.", "tokens": ["In", "Her\u00b7zen", "bie\u00b7der", "und", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Von dieser Liebe durchdrungen,", "tokens": ["Von", "die\u00b7ser", "Lie\u00b7be", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Preisw\u00fcrdige Schwester! bist Du,", "tokens": ["Preis\u00b7w\u00fcr\u00b7di\u00b7ge", "Schwes\u00b7ter", "!", "bist", "Du", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VAFIN", "PPER", "$,"], "meter": "++--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und W*ffen, der Dich errungen:", "tokens": ["Und", "W", "*", "ffen", ",", "der", "Dich", "er\u00b7run\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "XY", "VVINF", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Euch jubeln wir Fr\u00f6hlichkeit zu.", "tokens": ["Euch", "ju\u00b7beln", "wir", "Fr\u00f6h\u00b7lich\u00b7keit", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Wer kann die Fr\u00f6hlichkeit schildern,", "tokens": ["Wer", "kann", "die", "Fr\u00f6h\u00b7lich\u00b7keit", "schil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die unsere Seelen durchdringt?", "tokens": ["Die", "un\u00b7se\u00b7re", "See\u00b7len", "durch\u00b7dringt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Kein Dichter mit reizenden Bildern,", "tokens": ["Kein", "Dich\u00b7ter", "mit", "rei\u00b7zen\u00b7den", "Bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "So hoch, so herrlich er singt.", "tokens": ["So", "hoch", ",", "so", "herr\u00b7lich", "er", "singt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Wir sehn, wir wissen auf immer", "tokens": ["Wir", "sehn", ",", "wir", "wis\u00b7sen", "auf", "im\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich gl\u00fccklich an traulicher Hand,", "tokens": ["Dich", "gl\u00fcck\u00b7lich", "an", "trau\u00b7li\u00b7cher", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die Jahre verschw\u00e4chen Euch nimmer", "tokens": ["Die", "Jah\u00b7re", "ver\u00b7schw\u00e4\u00b7chen", "Euch", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Liebe rosiges Band.", "tokens": ["Der", "Lie\u00b7be", "ro\u00b7si\u00b7ges", "Band", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Du unsre Herzensverwandte,", "tokens": ["Du", "uns\u00b7re", "Her\u00b7zens\u00b7ver\u00b7wand\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Verm\u00e4hlte Schwester, Du machst", "tokens": ["Ver\u00b7m\u00e4hl\u00b7te", "Schwes\u00b7ter", ",", "Du", "machst"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Uns oft zu Onkel und Tante,", "tokens": ["Uns", "oft", "zu", "On\u00b7kel", "und", "Tan\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wir bitten, da\u00df Du nicht lachst.", "tokens": ["Wir", "bit\u00b7ten", ",", "da\u00df", "Du", "nicht", "lachst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Viel weniger da\u00df Du dar\u00fcber", "tokens": ["Viel", "we\u00b7ni\u00b7ger", "da\u00df", "Du", "da\u00b7r\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOUS", "PPER", "PAV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Stirnchen in Falten willst ziehn;", "tokens": ["Das", "Stirn\u00b7chen", "in", "Fal\u00b7ten", "willst", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Denn was ist sch\u00f6ner und lieber", "tokens": ["Denn", "was", "ist", "sch\u00f6\u00b7ner", "und", "lie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VAFIN", "ADJD", "KON", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Als Fr\u00fcchte der Ehe sehn bl\u00fchn.", "tokens": ["Als", "Fr\u00fcch\u00b7te", "der", "E\u00b7he", "sehn", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Du h\u00e4ngest das K\u00f6pfchen herunter?", "tokens": ["Du", "h\u00e4n\u00b7gest", "das", "K\u00f6pf\u00b7chen", "her\u00b7un\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Nun richt es nur wieder empor!", "tokens": ["Nun", "richt", "es", "nur", "wie\u00b7der", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sey lieblich l\u00e4chelnd, sey munter,", "tokens": ["Sey", "lieb\u00b7lich", "l\u00e4\u00b7chelnd", ",", "sey", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ADJD", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dein Br\u00e4utigam l\u00e4chelt Dir vor.", "tokens": ["Dein", "Br\u00e4u\u00b7ti\u00b7gam", "l\u00e4\u00b7chelt", "Dir", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.8": {"line.1": {"text": "Wir f\u00fchlens mit stillem Entz\u00fccken,", "tokens": ["Wir", "f\u00fch\u00b7lens", "mit", "stil\u00b7lem", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Da\u00df Er verbr\u00fcdert uns ist;", "tokens": ["Da\u00df", "Er", "ver\u00b7br\u00fc\u00b7dert", "uns", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "VAFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wir danken mit Worten und Blicken", "tokens": ["Wir", "dan\u00b7ken", "mit", "Wor\u00b7ten", "und", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dir, wenn uns Sein Brudermund k\u00fc\u00dft.", "tokens": ["Dir", ",", "wenn", "uns", "Sein", "Bru\u00b7der\u00b7mund", "k\u00fc\u00dft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.9": {"line.1": {"text": "Er zeig\u2019 uns die Brudergesinnung,", "tokens": ["Er", "zeig'", "uns", "die", "Bru\u00b7der\u00b7ge\u00b7sin\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wie Dir die Liebe, unwandelbar vest,", "tokens": ["Wie", "Dir", "die", "Lie\u00b7be", ",", "un\u00b7wan\u00b7del\u00b7bar", "vest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bis Euch des Alters Beginnung", "tokens": ["Bis", "Euch", "des", "Al\u00b7ters", "Be\u00b7gin\u00b7nung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nur mit Urenkelchen t\u00e4ndeln noch l\u00e4\u00dft.", "tokens": ["Nur", "mit", "Ur\u00b7en\u00b7kel\u00b7chen", "t\u00e4n\u00b7deln", "noch", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.10": {"line.1": {"text": "Dann ruhet Euch unter der Laube,", "tokens": ["Dann", "ru\u00b7het", "Euch", "un\u00b7ter", "der", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und sehet die Spiele der Kindelein an,", "tokens": ["Und", "se\u00b7het", "die", "Spie\u00b7le", "der", "Kin\u00b7de\u00b7lein", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und labet mit saftiger Traube", "tokens": ["Und", "la\u00b7bet", "mit", "saf\u00b7ti\u00b7ger", "Trau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Den Gaumen, der trocken zu werden begann.", "tokens": ["Den", "Gau\u00b7men", ",", "der", "tro\u00b7cken", "zu", "wer\u00b7den", "be\u00b7gann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PTKZU", "VAINF", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.11": {"line.1": {"text": "Jezt seyd Ihr so saftig wie Trauben,", "tokens": ["Jezt", "seyd", "Ihr", "so", "saf\u00b7tig", "wie", "Trau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Jezt sind Euch die Lippen so Pfirsichen weich;", "tokens": ["Jezt", "sind", "Euch", "die", "Lip\u00b7pen", "so", "Pfir\u00b7si\u00b7chen", "weich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "NN", "ADJD", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dem z\u00e4rtlichsten P\u00e4rchen der Tauben", "tokens": ["Dem", "z\u00e4rt\u00b7lichs\u00b7ten", "P\u00e4r\u00b7chen", "der", "Tau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Seyd Ihr an zarten Empfindungen gleich ", "tokens": ["Seyd", "Ihr", "an", "zar\u00b7ten", "Emp\u00b7fin\u00b7dun\u00b7gen", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "PPER", "APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}