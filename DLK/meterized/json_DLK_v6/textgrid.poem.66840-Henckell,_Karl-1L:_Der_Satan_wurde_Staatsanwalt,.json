{"textgrid.poem.66840": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Satan wurde Staatsanwalt,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Satan wurde Staatsanwalt,", "tokens": ["Der", "Sa\u00b7tan", "wur\u00b7de", "Staats\u00b7an\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Herz, das war wie Eis so kalt.", "tokens": ["Sein", "Herz", ",", "das", "war", "wie", "Eis", "so", "kalt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VAFIN", "KOKOM", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gott gab im Zorne dem Geschmei\u00df", "tokens": ["Gott", "gab", "im", "Zor\u00b7ne", "dem", "Ge\u00b7schmei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Busen einen K\u00fcbel Eis,", "tokens": ["Zum", "Bu\u00b7sen", "ei\u00b7nen", "K\u00fc\u00b7bel", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wo sonst Menschen Mitleid f\u00fchlen,", "tokens": ["Und", "wo", "sonst", "Men\u00b7schen", "Mit\u00b7leid", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da konnte man Champagner k\u00fchlen.", "tokens": ["Da", "konn\u00b7te", "man", "Cham\u00b7pag\u00b7ner", "k\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "NN", "VVINF", "$."], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Zu Magdeburg in Vorhaft sa\u00df", "tokens": ["Zu", "Mag\u00b7de\u00b7burg", "in", "Vor\u00b7haft", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Sozialist, der j\u00fcngst verga\u00df,", "tokens": ["Ein", "So\u00b7zi\u00b7a\u00b7list", ",", "der", "j\u00fcngst", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df hoch im deutschen Vaterland", "tokens": ["Da\u00df", "hoch", "im", "deut\u00b7schen", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein G\u00f6tze thront, S.M. genannt,", "tokens": ["Ein", "G\u00f6t\u00b7ze", "thront", ",", "S.", "M.", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "punct", "abbreviation", "abbreviation", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Des heiligen Namen zu betasten", "tokens": ["Des", "hei\u00b7li\u00b7gen", "Na\u00b7men", "zu", "be\u00b7tas\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Gen\u00fcgt, verbrechrisch zu belasten.", "tokens": ["Ge\u00b7n\u00fcgt", ",", "ver\u00b7brech\u00b7risch", "zu", "be\u00b7las\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zum Schutze solcher Majest\u00e4t", "tokens": ["Zum", "Schut\u00b7ze", "sol\u00b7cher", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind diesem G\u00f6tzen fr\u00fch und sp\u00e4t", "tokens": ["Sind", "die\u00b7sem", "G\u00f6t\u00b7zen", "fr\u00fch", "und", "sp\u00e4t"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDAT", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Mummelsee bis Helgoland", "tokens": ["Vom", "Mum\u00b7mel\u00b7see", "bis", "Hel\u00b7go\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Viel G\u00f6tzenw\u00e4chter vorgespannt,", "tokens": ["Viel", "G\u00f6t\u00b7zen\u00b7w\u00e4ch\u00b7ter", "vor\u00b7ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die jedem L\u00e4sterer des G\u00f6tzen", "tokens": ["Die", "je\u00b7dem", "L\u00e4s\u00b7te\u00b7rer", "des", "G\u00f6t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Messer des Gesetzes wetzen.", "tokens": ["Das", "Mes\u00b7ser", "des", "Ge\u00b7set\u00b7zes", "wet\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zu Dessau lag mit reifem Leib", "tokens": ["Zu", "Des\u00b7sau", "lag", "mit", "rei\u00b7fem", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Wochenbett ein junges Weib.", "tokens": ["Im", "Wo\u00b7chen\u00b7bett", "ein", "jun\u00b7ges", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sah die schwere Stunde kommen", "tokens": ["Sie", "sah", "die", "schwe\u00b7re", "Stun\u00b7de", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schrieb und bat so angstbeklommen", "tokens": ["Und", "schrieb", "und", "bat", "so", "angst\u00b7be\u00b7klom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den B\u00fcttelvogt: \u00bbO la\u00dft's geschehn!", "tokens": ["Den", "B\u00fct\u00b7tel\u00b7vogt", ":", "\u00bb", "O", "la\u00dft's", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "NE", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Darf ich ihn nicht noch einmal sehn?!\u00ab", "tokens": ["Darf", "ich", "ihn", "nicht", "noch", "ein\u00b7mal", "sehn", "?!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "ADV", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie fleht' und schrieb zum andern Mal", "tokens": ["Sie", "fleht'", "und", "schrieb", "zum", "an\u00b7dern", "Mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Finsternis und Seelenqual.", "tokens": ["In", "Fins\u00b7ter\u00b7nis", "und", "See\u00b7len\u00b7qual", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor ihren Augen fuhr der Tod", "tokens": ["Vor", "ih\u00b7ren", "Au\u00b7gen", "fuhr", "der", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schon auf sie zu in schwarzem Boot,", "tokens": ["Schon", "auf", "sie", "zu", "in", "schwar\u00b7zem", "Boot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und schaukelnd schwamm auf fahlem Teiche", "tokens": ["Und", "schau\u00b7kelnd", "schwamm", "auf", "fah\u00b7lem", "Tei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Wieglein mit der kleinen Leiche.", "tokens": ["Das", "Wie\u00b7glein", "mit", "der", "klei\u00b7nen", "Lei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Da, wie sie gell um Hilfe rief,", "tokens": ["Da", ",", "wie", "sie", "gell", "um", "Hil\u00b7fe", "rief", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bracht' ihr die W\u00e4rterin den Brief \u2013", "tokens": ["Bracht'", "ihr", "die", "W\u00e4r\u00b7te\u00b7rin", "den", "Brief", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Mann, zum Trost in Pein und Gram?", "tokens": ["Vom", "Mann", ",", "zum", "Trost", "in", "Pein", "und", "Gram", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O nein! Die Weihnachtsbotschaft kam", "tokens": ["O", "nein", "!", "Die", "Weih\u00b7nachts\u00b7bot\u00b7schaft", "kam"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKANT", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vom Staatsanwalt. So schlo\u00df sein Schreiben:", "tokens": ["Vom", "Staats\u00b7an\u00b7walt", ".", "So", "schlo\u00df", "sein", "Schrei\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbbedaure sehr, Ihr Mann mu\u00df bleiben!", "tokens": ["\u00bb", "be\u00b7dau\u00b7re", "sehr", ",", "Ihr", "Mann", "mu\u00df", "blei\u00b7ben", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wir lassen keinesfalls ihn los,", "tokens": ["Wir", "las\u00b7sen", "kei\u00b7nes\u00b7falls", "ihn", "los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Daf\u00fcr ist seine Schuld zu gro\u00df.", "tokens": ["Da\u00b7f\u00fcr", "ist", "sei\u00b7ne", "Schuld", "zu", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPOSAT", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man wird ihn schwer bestrafen m\u00fcssen ...\u00ab", "tokens": ["Man", "wird", "ihn", "schwer", "be\u00b7stra\u00b7fen", "m\u00fcs\u00b7sen", "...", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADJD", "VVINF", "VMFIN", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das arme Weib sank in die Kissen", "tokens": ["Das", "ar\u00b7me", "Weib", "sank", "in", "die", "Kis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "\u2013 Ein j\u00e4her Schrei durch Mark und Bein! \u2013", "tokens": ["\u2013", "Ein", "j\u00e4\u00b7her", "Schrei", "durch", "Mark", "und", "Bein", "!", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und starb. Hier steht ihr Leichenstein:", "tokens": ["Und", "starb", ".", "Hier", "steht", "ihr", "Lei\u00b7chen\u00b7stein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbim Reich der Gottesfurcht allhie", "tokens": ["\u00bb", "im", "Reich", "der", "Got\u00b7tes\u00b7furcht", "all\u00b7hie"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "APPRART", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Zeit der Schmach ward schwanger sie.", "tokens": ["Zur", "Zeit", "der", "Schmach", "ward", "schwan\u00b7ger", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VAFIN", "ADJD", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Mann fiel in des Satans Krallen,", "tokens": ["Ihr", "Mann", "fiel", "in", "des", "Sa\u00b7tans", "Kral\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da hat es Gott dem Herrn gefallen,", "tokens": ["Da", "hat", "es", "Gott", "dem", "Herrn", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und nahm sie zu sich in der Nacht", "tokens": ["Und", "nahm", "sie", "zu", "sich", "in", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der majest\u00e4tischen Niedertracht.\u00ab", "tokens": ["Der", "ma\u00b7jes\u00b7t\u00e4\u00b7ti\u00b7schen", "Nie\u00b7der\u00b7tracht", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}