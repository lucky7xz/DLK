{"textgrid.poem.65302": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Amor, ein Sprachlehrer", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amor ist ein Sprachverderber,", "tokens": ["A\u00b7mor", "ist", "ein", "Sprach\u00b7ver\u00b7der\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wortverdreher, Lautverwirrer,", "tokens": ["Wort\u00b7ver\u00b7dre\u00b7her", ",", "Laut\u00b7ver\u00b7wir\u00b7rer", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["PWAV", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der beim gro\u00dfen Thurm zu Babel", "tokens": ["Der", "beim", "gro\u00b7\u00dfen", "Thurm", "zu", "Ba\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "NN", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon die H\u00e4nd' im Spiele hatte.", "tokens": ["Schon", "die", "H\u00e4nd'", "im", "Spie\u00b7le", "hat\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn ich weine, raunt er leise", "tokens": ["Wenn", "ich", "wei\u00b7ne", ",", "raunt", "er", "lei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir in's Ohr etwas von Wonne;", "tokens": ["Mir", "in's", "Ohr", "et\u00b7was", "von", "Won\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn ich schmachte, l\u00e4\u00dft er dennoch", "tokens": ["Wenn", "ich", "schmach\u00b7te", ",", "l\u00e4\u00dft", "er", "den\u00b7noch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Reden mich von Seligkeiten.", "tokens": ["Re\u00b7den", "mich", "von", "Se\u00b7lig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "In dem lauten Schwarm der Feste", "tokens": ["In", "dem", "lau\u00b7ten", "Schwarm", "der", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Mu\u00df ich, diesem Lehrer folgend,", "tokens": ["Mu\u00df", "ich", ",", "die\u00b7sem", "Leh\u00b7rer", "fol\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Sagen, da\u00df ich einsam stehe,", "tokens": ["Sa\u00b7gen", ",", "da\u00df", "ich", "ein\u00b7sam", "ste\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und im einsam stillen Haine", "tokens": ["Und", "im", "ein\u00b7sam", "stil\u00b7len", "Hai\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Darf ich mich allein nicht nennen.", "tokens": ["Darf", "ich", "mich", "al\u00b7lein", "nicht", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Bitters\u00fc\u00df und lieblichherbe,", "tokens": ["Bit\u00b7ter\u00b7s\u00fc\u00df", "und", "lieb\u00b7lich\u00b7her\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Grausam mild und labend schmerzlich,", "tokens": ["Grau\u00b7sam", "mild", "und", "la\u00b7bend", "schmerz\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Solche Reden hat er viele", "tokens": ["Sol\u00b7che", "Re\u00b7den", "hat", "er", "vie\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Stehn in seinem W\u00f6rterbuche,", "tokens": ["Stehn", "in", "sei\u00b7nem", "W\u00f6r\u00b7ter\u00b7bu\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Das die gr\u00f6\u00dften Sprachgelehrten", "tokens": ["Das", "die", "gr\u00f6\u00df\u00b7ten", "Sprach\u00b7ge\u00b7lehr\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Mir nicht auszudeuten wagen,", "tokens": ["Mir", "nicht", "aus\u00b7zu\u00b7deu\u00b7ten", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Und mit dem ich alle Tage", "tokens": ["Und", "mit", "dem", "ich", "al\u00b7le", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRELS", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Mehr mein bi\u00dfchen Deutsch verlerne.", "tokens": ["Mehr", "mein", "bi\u00df\u00b7chen", "Deutsch", "ver\u00b7ler\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}