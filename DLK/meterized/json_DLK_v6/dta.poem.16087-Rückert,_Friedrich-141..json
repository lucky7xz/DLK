{"dta.poem.16087": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "141.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer ist begl\u00fcckt? wers w\u00e4hnt. Wer unbegl\u00fcckt? wers glaubt.", "tokens": ["Wer", "ist", "be\u00b7gl\u00fcckt", "?", "wers", "w\u00e4hnt", ".", "Wer", "un\u00b7be\u00b7gl\u00fcckt", "?", "wers", "glaubt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "VVPP", "$.", "ADV", "VVFIN", "$.", "PWS", "ADJD", "$.", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Glauben wird die Welt geschenkt dir und geraubt.", "tokens": ["Vom", "Glau\u00b7ben", "wird", "die", "Welt", "ge\u00b7schenkt", "dir", "und", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "VVFIN", "PPER", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn er den Starken l\u00e4hmt, und wenn er st\u00e4rkt den Schwachen,", "tokens": ["Wenn", "er", "den", "Star\u00b7ken", "l\u00e4hmt", ",", "und", "wenn", "er", "st\u00e4rkt", "den", "Schwa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "KON", "KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird er zum K\u00f6nig den, zum Bettler jenen machen.", "tokens": ["Wird", "er", "zum", "K\u00f6\u00b7nig", "den", ",", "zum", "Bett\u00b7ler", "je\u00b7nen", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ART", "$,", "APPRART", "NN", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Erde dienet ihm, und ist ihm unzul\u00e4nglich.", "tokens": ["Die", "Er\u00b7de", "die\u00b7net", "ihm", ",", "und", "ist", "ihm", "un\u00b7zu\u00b7l\u00e4ng\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KON", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn ihm allein ist nicht der Himmel unzug\u00e4nglich.", "tokens": ["Denn", "ihm", "al\u00b7lein", "ist", "nicht", "der", "Him\u00b7mel", "un\u00b7zu\u00b7g\u00e4ng\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VAFIN", "PTKNEG", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er tritt mit Zuversicht vor Gottes Angesicht,", "tokens": ["Er", "tritt", "mit", "Zu\u00b7ver\u00b7sicht", "vor", "Got\u00b7tes", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wei\u00df gewis, da\u00df er bestehn wird im Gericht.", "tokens": ["Und", "wei\u00df", "ge\u00b7wis", ",", "da\u00df", "er", "be\u00b7stehn", "wird", "im", "Ge\u00b7richt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVINF", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}