{"textgrid.poem.25806": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[nie sangen die V\u00f6gel so l\u00fcstern]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nie sangen die V\u00f6gel so l\u00fcstern.", "tokens": ["Nie", "san\u00b7gen", "die", "V\u00f6\u00b7gel", "so", "l\u00fcs\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sonne und Winde fl\u00fcstern", "tokens": ["Son\u00b7ne", "und", "Win\u00b7de", "fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Von weichen, wonnigen Frauen,", "tokens": ["Von", "wei\u00b7chen", ",", "won\u00b7ni\u00b7gen", "Frau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Alle B\u00e4ume hangen voll K\u00fcsse,", "tokens": ["Al\u00b7le", "B\u00e4u\u00b7me", "han\u00b7gen", "voll", "K\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Alle Lippen m\u00fcssen verlangen,", "tokens": ["Al\u00b7le", "Lip\u00b7pen", "m\u00fcs\u00b7sen", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Der Fr\u00fchling ist hungers\u00e4end", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ist", "hun\u00b7ger\u00b7s\u00e4end"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "CARD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "\u00dcber die Erde gegangen.", "tokens": ["\u00dc\u00b7ber", "die", "Er\u00b7de", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}