{"textgrid.poem.34091": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Mahnung", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrchte die schlechteste Fliege! Sie kann den edelsten Wein dir", "tokens": ["F\u00fcrch\u00b7te", "die", "schlech\u00b7tes\u00b7te", "Flie\u00b7ge", "!", "Sie", "kann", "den", "e\u00b7dels\u00b7ten", "Wein", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "PPER", "VMFIN", "ART", "ADJA", "NN", "PPER"], "meter": "+--+--+--+----+-", "measure": "dactylic.tetra.plus"}, "line.2": {"text": "Doch verderben: sie f\u00e4llt eben hinein und ers\u00e4uft!", "tokens": ["Doch", "ver\u00b7der\u00b7ben", ":", "sie", "f\u00e4llt", "e\u00b7ben", "hin\u00b7ein", "und", "er\u00b7s\u00e4uft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "PTKVZ", "KON", "VVFIN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}}}}}