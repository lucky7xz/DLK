{"textgrid.poem.53094": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Ludwig Keppler und Anna Reimer", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mann ist erst zu erheben,", "tokens": ["Der", "Mann", "ist", "erst", "zu", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ist erst thewren Lobes werth,", "tokens": ["Ist", "erst", "thew\u00b7ren", "Lo\u00b7bes", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem in diesem armen Leben", "tokens": ["Dem", "in", "die\u00b7sem", "ar\u00b7men", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist ein sittsam Weib beschert,", "tokens": ["Ist", "ein", "sitt\u00b7sam", "Weib", "be\u00b7schert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJD", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie geht vor, an Thugend-Scheine,", "tokens": ["Sie", "geht", "vor", ",", "an", "Thu\u00b7gen\u00b7dSchei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "APPR", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Auch dem besten Demant-Steine,", "tokens": ["Auch", "dem", "bes\u00b7ten", "De\u00b7mant\u00b7Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Den der Jud' aus Osten schickt.", "tokens": ["Den", "der", "Jud'", "aus", "Os\u00b7ten", "schickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Was hat sich jhr Mann zu krencken?", "tokens": ["Was", "hat", "sich", "jhr", "Mann", "zu", "kren\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PRF", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Sie wird stets auff Nahrung dencken,", "tokens": ["Sie", "wird", "stets", "auff", "Nah\u00b7rung", "den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Thun, was seine Seel erquickt.", "tokens": ["Thun", ",", "was", "sei\u00b7ne", "Seel", "er\u00b7quickt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie ist th\u00e4ttig aller enden,", "tokens": ["Sie", "ist", "th\u00e4t\u00b7tig", "al\u00b7ler", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kan mit Woll vnd Flachs vmbgehn,", "tokens": ["Kan", "mit", "Woll", "vnd", "Flachs", "vmb\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spinnt vnd wirckt mit jhren H\u00e4nden,", "tokens": ["Spinnt", "vnd", "wirckt", "mit", "jhren", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Niemand sieht sie m\u00fcssig stehn", "tokens": ["Nie\u00b7mand", "sieht", "sie", "m\u00fcs\u00b7sig", "stehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gleich dem Schiffe, das mit Wahren", "tokens": ["Gleich", "dem", "Schif\u00b7fe", ",", "das", "mit", "Wah\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weit kombt \u00fcber See gefahren,", "tokens": ["Weit", "kombt", "\u00fc\u00b7ber", "See", "ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schon des Nachts ist sie herau\u00df,", "tokens": ["Schon", "des", "Nachts", "ist", "sie", "her\u00b7au\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Spielt zusammen Magd vnd Mutter,", "tokens": ["Spielt", "zu\u00b7sam\u00b7men", "Magd", "vnd", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Gibt den Dirnen Speis', vnd Futter", "tokens": ["Gibt", "den", "Dir\u00b7nen", "Speis'", ",", "vnd", "Fut\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NE", "$,", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "T\u00e4glich durch jhr gantzes Hau\u00df.", "tokens": ["T\u00e4g\u00b7lich", "durch", "jhr", "gant\u00b7zes", "Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie denckt vmb nach einem Acker,", "tokens": ["Sie", "denckt", "vmb", "nach", "ei\u00b7nem", "A\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "K\u00e4ufft jhn an sich, pflantzet Wein,", "tokens": ["K\u00e4ufft", "jhn", "an", "sich", ",", "pflant\u00b7zet", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PRF", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihrer Arme Krafft mu\u00df wacker", "tokens": ["Ih\u00b7rer", "Ar\u00b7me", "Krafft", "mu\u00df", "wa\u00b7cker"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VMFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Etwas anzugreiffen seyn.", "tokens": ["Et\u00b7was", "an\u00b7zu\u00b7greif\u00b7fen", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVIZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie versteht, was nutz vnd frommen", "tokens": ["Sie", "ver\u00b7steht", ",", "was", "nutz", "vnd", "from\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWS", "NN", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00df aus jhrem Handel kommen,", "tokens": ["Mu\u00df", "aus", "jhrem", "Han\u00b7del", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ihre Leucht' erlischet nicht", "tokens": ["Ih\u00b7re", "Leucht'", "er\u00b7li\u00b7schet", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Fast die gantze Nacht durch spinnen,", "tokens": ["Fast", "die", "gant\u00b7ze", "Nacht", "durch", "spin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Vnd auff allen Wolstand sinnen,", "tokens": ["Vnd", "auff", "al\u00b7len", "Wol\u00b7stand", "sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sch\u00e4tzet sie vor jhre Pflicht.", "tokens": ["Sch\u00e4t\u00b7zet", "sie", "vor", "jhre", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Sonderlich mit jhrem Segen", "tokens": ["Son\u00b7der\u00b7lich", "mit", "jhrem", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "H\u00fclfft sie gern den Armen fort,", "tokens": ["H\u00fclfft", "sie", "gern", "den", "Ar\u00b7men", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie verwahrt jhr Hau\u00df f\u00fcr Regen,", "tokens": ["Sie", "ver\u00b7wahrt", "jhr", "Hau\u00df", "f\u00fcr", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vngewitter, Schnee vnd Nord,", "tokens": ["Vn\u00b7ge\u00b7wit\u00b7ter", ",", "Schnee", "vnd", "Nord", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie wei\u00df sich mit weisser Seyden", "tokens": ["Sie", "wei\u00df", "sich", "mit", "weis\u00b7ser", "Sey\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Vnd mit Purpur anzukleiden,", "tokens": ["Vnd", "mit", "Pur\u00b7pur", "an\u00b7zu\u00b7klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wei\u00df durch schweigen vnd verstand", "tokens": ["Wei\u00df", "durch", "schwei\u00b7gen", "vnd", "ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "VVINF", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Rhum auff jhren Mann zu setzen,", "tokens": ["Rhum", "auff", "jhren", "Mann", "zu", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Da\u00df man jhn mu\u00df seelig sch\u00e4tzen", "tokens": ["Da\u00df", "man", "jhn", "mu\u00df", "see\u00b7lig", "sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "VMFIN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Hin vnd wieder durch das Land.", "tokens": ["Hin", "vnd", "wie\u00b7der", "durch", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie verk\u00e4ufft dem Kr\u00e4mer Sachen,", "tokens": ["Sie", "ver\u00b7k\u00e4ufft", "dem", "Kr\u00e4\u00b7mer", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die sie selbst verfertigt hat,", "tokens": ["Die", "sie", "selbst", "ver\u00b7fer\u00b7tigt", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sauberkeit, Flei\u00df, Schmuck vnd Lachen", "tokens": ["Sau\u00b7ber\u00b7keit", ",", "Flei\u00df", ",", "Schmuck", "vnd", "La\u00b7chen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Finden allzeit vmb sie statt.", "tokens": ["Fin\u00b7den", "all\u00b7zeit", "vmb", "sie", "statt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "L\u00e4sst sie sich mit reden h\u00f6ren,", "tokens": ["L\u00e4sst", "sie", "sich", "mit", "re\u00b7den", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "VVINF", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "So sind lauter Wei\u00dfheit Lehren,", "tokens": ["So", "sind", "lau\u00b7ter", "Wei\u00df\u00b7heit", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die sie auff der Zungen f\u00fchrt,", "tokens": ["Die", "sie", "auff", "der", "Zun\u00b7gen", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sie versorgt, bestellt, vnd sihet,", "tokens": ["Sie", "ver\u00b7sorgt", ",", "be\u00b7stellt", ",", "vnd", "si\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Was im Hause nur geschihet,", "tokens": ["Was", "im", "Hau\u00b7se", "nur", "ge\u00b7schi\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Vnd wird nirgends faul gesp\u00fchrt.", "tokens": ["Vnd", "wird", "nir\u00b7gends", "faul", "ge\u00b7sp\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihre S\u00f6hne sind gediehen,", "tokens": ["Ih\u00b7re", "S\u00f6h\u00b7ne", "sind", "ge\u00b7die\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schreiben jhr die Wolfahrt zu,", "tokens": ["Schrei\u00b7ben", "jhr", "die", "Wol\u00b7fahrt", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd jhr Mann mu\u00df jmmer bl\u00fchen,", "tokens": ["Vnd", "jhr", "Mann", "mu\u00df", "jm\u00b7mer", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sagt, sie schaff' jhm Frewd vnd Ruh,", "tokens": ["Sagt", ",", "sie", "schaff'", "jhm", "Frewd", "vnd", "Ruh", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zwar durch vieler T\u00f6chter Segen", "tokens": ["Zwar", "durch", "vie\u00b7ler", "T\u00f6ch\u00b7ter", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kan man Reichthumb hinterlegen,", "tokens": ["Kan", "man", "Reicht\u00b7humb", "hin\u00b7ter\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wan sie flei\u00df- vnd z\u00fccht-ig sind;", "tokens": ["Wan", "sie", "flei\u00df", "vnd", "z\u00fccht\u00b7ig", "sind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "TRUNC", "KON", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Doch der Mutter Art vnd Thugend", "tokens": ["Doch", "der", "Mut\u00b7ter", "Art", "vnd", "Thu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Hat, was der noch zarten Jugend", "tokens": ["Hat", ",", "was", "der", "noch", "zar\u00b7ten", "Ju\u00b7gend"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "PRELS", "ART", "ADV", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Allen Lobspruch abgewinnt.", "tokens": ["Al\u00b7len", "Lob\u00b7spruch", "ab\u00b7ge\u00b7winnt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Lieblich seyn vnd sch\u00f6n von Leibe,", "tokens": ["Lieb\u00b7lich", "seyn", "vnd", "sch\u00f6n", "von", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird von Weisen nicht erkiest,", "tokens": ["Wird", "von", "Wei\u00b7sen", "nicht", "er\u00b7kiest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber wo bey einem Weibe", "tokens": ["A\u00b7ber", "wo", "bey", "ei\u00b7nem", "Wei\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gottesfurcht vnd Thugend ist,", "tokens": ["Got\u00b7tes\u00b7furcht", "vnd", "Thu\u00b7gend", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das sol erst gelobet werden", "tokens": ["Das", "sol", "erst", "ge\u00b7lo\u00b7bet", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADV", "VVPP", "VAINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "F\u00fcr den andern hier auff Erden,", "tokens": ["F\u00fcr", "den", "an\u00b7dern", "hier", "auff", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihre frucht der H\u00e4nde macht,", "tokens": ["Ih\u00b7re", "frucht", "der", "H\u00e4n\u00b7de", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df auff sie vnd jhre Kinder,", "tokens": ["Da\u00df", "auff", "sie", "vnd", "jhre", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Vnd auff Kindes-Kind nicht minder", "tokens": ["Vnd", "auff", "Kin\u00b7des\u00b7Kind", "nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "PTKNEG", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Werd' ein ewig Lob gebracht.", "tokens": ["Werd'", "ein", "e\u00b7wig", "Lob", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJD", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}