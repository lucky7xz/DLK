{"textgrid.poem.63601": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will mir meinen Freund nicht schelten lassen,", "tokens": ["Ich", "will", "mir", "mei\u00b7nen", "Freund", "nicht", "schel\u00b7ten", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den einz'gen, der mich schmeichelnd nie betrog,", "tokens": ["Den", "einz'\u00b7gen", ",", "der", "mich", "schmei\u00b7chelnd", "nie", "be\u00b7trog", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und den ich liebe, mag die Welt ihn hassen.", "tokens": ["Und", "den", "ich", "lie\u00b7be", ",", "mag", "die", "Welt", "ihn", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "VVFIN", "$,", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O Schmerz, du weiser, treuster P\u00e4dagog,", "tokens": ["O", "Schmerz", ",", "du", "wei\u00b7ser", ",", "treus\u00b7ter", "P\u00e4d\u00b7a\u00b7gog", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Noch nicht entwuchs ich deiner Zucht und Lehre,", "tokens": ["Noch", "nicht", "ent\u00b7wuchs", "ich", "dei\u00b7ner", "Zucht", "und", "Leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die mich mit rauher Hand zum Mann erzog.", "tokens": ["Die", "mich", "mit", "rau\u00b7her", "Hand", "zum", "Mann", "er\u00b7zog", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mich, der ich gern im Traum geblieben w\u00e4re,", "tokens": ["Mich", ",", "der", "ich", "gern", "im", "Traum", "ge\u00b7blie\u00b7ben", "w\u00e4\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Hast du erweckt, unsanft nach deiner Art:", "tokens": ["Hast", "du", "er\u00b7weckt", ",", "un\u00b7sanft", "nach", "dei\u00b7ner", "Art", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Steh auf, und diesen Morgenbecher leere!", "tokens": ["Steh", "auf", ",", "und", "die\u00b7sen", "Mor\u00b7gen\u00b7be\u00b7cher", "lee\u00b7re", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "PDAT", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ich trank. Da sch\u00fcttelte mich Frost. Doch ward", "tokens": ["Ich", "trank", ".", "Da", "sch\u00fct\u00b7tel\u00b7te", "mich", "Frost", ".", "Doch", "ward"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "NN", "$.", "KON", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Mein Auge wacker, diese Welt zu schauen,", "tokens": ["Mein", "Au\u00b7ge", "wa\u00b7cker", ",", "die\u00b7se", "Welt", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,", "PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "So wie sie ist. Auf Wegen steil und hart", "tokens": ["So", "wie", "sie", "ist", ".", "Auf", "We\u00b7gen", "steil", "und", "hart"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER", "VAFIN", "$.", "APPR", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Der Kraft, die du mir gabst, lernt' ich vertrauen", "tokens": ["Der", "Kraft", ",", "die", "du", "mir", "gabst", ",", "lernt'", "ich", "ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und w\u00fcnschte mir kein M\u00e4rchenfl\u00fcgelpaar,", "tokens": ["Und", "w\u00fcnschte", "mir", "kein", "M\u00e4r\u00b7chen\u00b7fl\u00fc\u00b7gel\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Zu schwanken wahngewiegt im \u00c4therblauen.", "tokens": ["Zu", "schwan\u00b7ken", "wahn\u00b7ge\u00b7wiegt", "im", "\u00c4\u00b7ther\u00b7blau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Durch schwarze Gl\u00e4ser lehrtest du mich klar", "tokens": ["Durch", "schwar\u00b7ze", "Gl\u00e4\u00b7ser", "lehr\u00b7test", "du", "mich", "klar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Den Quell des Lichtes pr\u00fcfen und erkennen,", "tokens": ["Den", "Quell", "des", "Lich\u00b7tes", "pr\u00fc\u00b7fen", "und", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Da\u00df er so herzlos wie die andre Schar,", "tokens": ["Da\u00df", "er", "so", "herz\u00b7los", "wie", "die", "and\u00b7re", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Die wir mit liebevollen Namen nennen,", "tokens": ["Die", "wir", "mit", "lie\u00b7be\u00b7vol\u00b7len", "Na\u00b7men", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-----", "measure": "unknown.measure.tri"}, "line.20": {"text": "Da\u00df die erhabnen Sterne sonder Lust", "tokens": ["Da\u00df", "die", "er\u00b7hab\u00b7nen", "Ster\u00b7ne", "son\u00b7der", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Und Mitleid \u00fcber unsern Qualen brennen.", "tokens": ["Und", "Mit\u00b7leid", "\u00fc\u00b7ber", "un\u00b7sern", "Qua\u00b7len", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Das aber tr\u00e4gt nur eine tapfre Brust,", "tokens": ["Das", "a\u00b7ber", "tr\u00e4gt", "nur", "ei\u00b7ne", "tapf\u00b7re", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Und darum ha\u00dft die Menge dein Ermahnen,", "tokens": ["Und", "da\u00b7rum", "ha\u00dft", "die", "Men\u00b7ge", "dein", "Er\u00b7mah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Den strengen Weckruf: Wolle, denn du mu\u00dft!", "tokens": ["Den", "stren\u00b7gen", "Weck\u00b7ruf", ":", "Wol\u00b7le", ",", "denn", "du", "mu\u00dft", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NE", "$,", "KON", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Du l\u00e4ssest sie den dunklen Urgrund ahnen,", "tokens": ["Du", "l\u00e4s\u00b7sest", "sie", "den", "dunk\u00b7len", "Ur\u00b7grund", "ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Aus dem des All und Eins Grundwasser quellen,", "tokens": ["Aus", "dem", "des", "All", "und", "Eins", "Grund\u00b7was\u00b7ser", "quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Geheimnis allen t\u00e4ndelnden Profanen.", "tokens": ["Ge\u00b7heim\u00b7nis", "al\u00b7len", "t\u00e4n\u00b7deln\u00b7den", "Pro\u00b7fa\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Und dennoch: wenn das Licht uns soll erhellen,", "tokens": ["Und", "den\u00b7noch", ":", "wenn", "das", "Licht", "uns", "soll", "er\u00b7hel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "KOUS", "ART", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "So m\u00fcssen wir des Brennens Angst und Pein", "tokens": ["So", "m\u00fcs\u00b7sen", "wir", "des", "Bren\u00b7nens", "Angst", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Erdulden, ohn' uns weibisch anzustellen.", "tokens": ["E\u00b7rdul\u00b7den", ",", "ohn'", "uns", "wei\u00b7bisch", "an\u00b7zu\u00b7stel\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "PPER", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Nichts, was wir nicht erk\u00e4mpft, wird unser sein;", "tokens": ["Nichts", ",", "was", "wir", "nicht", "er\u00b7k\u00e4mpft", ",", "wird", "un\u00b7ser", "sein", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "PTKNEG", "VVPP", "$,", "VAFIN", "PPOSAT", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Mit Lebenssch\u00e4tzen aus dem eignen Mark", "tokens": ["Mit", "Le\u00b7bens\u00b7sch\u00e4t\u00b7zen", "aus", "dem", "eig\u00b7nen", "Mark"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Bezahlen wir des Wissens D\u00e4mmerschein.", "tokens": ["Be\u00b7zah\u00b7len", "wir", "des", "Wis\u00b7sens", "D\u00e4m\u00b7mer\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Doch so sich l\u00e4uternd wird die Seele stark,", "tokens": ["Doch", "so", "sich", "l\u00e4u\u00b7ternd", "wird", "die", "See\u00b7le", "stark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PRF", "ADJD", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Den Glanz der h\u00f6chsten Wonnen auch zu tragen,", "tokens": ["Den", "Glanz", "der", "h\u00f6chs\u00b7ten", "Won\u00b7nen", "auch", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Daran f\u00fcrwahr kein Heldenleben karg.", "tokens": ["Da\u00b7ran", "f\u00fcr\u00b7wahr", "kein", "Hel\u00b7den\u00b7le\u00b7ben", "karg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Ahnt ihr den Tiefsinn nicht der alten Sagen,", "tokens": ["Ahnt", "ihr", "den", "Tief\u00b7sinn", "nicht", "der", "al\u00b7ten", "Sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Wie jener Heros erntet' Himmelsruh',", "tokens": ["Wie", "je\u00b7ner", "He\u00b7ros", "ern\u00b7tet'", "Him\u00b7mels\u00b7ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.39": {"text": "Der durch Lern\u00e4as S\u00fcmpfe sich geschlagen?", "tokens": ["Der", "durch", "Ler\u00b7n\u00e4\u00b7as", "S\u00fcmp\u00b7fe", "sich", "ge\u00b7schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "O mein Befreier, Freund und Meister du,", "tokens": ["O", "mein", "Be\u00b7frei\u00b7er", ",", "Freund", "und", "Meis\u00b7ter", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Der mir vom Auge nahm des Wahnes Binden,", "tokens": ["Der", "mir", "vom", "Au\u00b7ge", "nahm", "des", "Wah\u00b7nes", "Bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Ich jauchze dir in d\u00fcstern N\u00e4chten zu.", "tokens": ["Ich", "jauch\u00b7ze", "dir", "in", "d\u00fcs\u00b7tern", "N\u00e4ch\u00b7ten", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Du, wenn mir alle Taggenossen schwinden,", "tokens": ["Du", ",", "wenn", "mir", "al\u00b7le", "Tag\u00b7ge\u00b7nos\u00b7sen", "schwin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "H\u00e4ltst bei mir aus, in Schlummer singst du mich,", "tokens": ["H\u00e4ltst", "bei", "mir", "aus", ",", "in", "Schlum\u00b7mer", "singst", "du", "mich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PTKVZ", "$,", "APPR", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Und selbst im Traum mu\u00df ich dich wiederfinden.", "tokens": ["Und", "selbst", "im", "Traum", "mu\u00df", "ich", "dich", "wie\u00b7der\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Und wenn die letzte Sonne mir erblich,", "tokens": ["Und", "wenn", "die", "letz\u00b7te", "Son\u00b7ne", "mir", "er\u00b7blich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Die letzte Nacht die Fl\u00fcgel um mich breitet,", "tokens": ["Die", "letz\u00b7te", "Nacht", "die", "Fl\u00fc\u00b7gel", "um", "mich", "brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "w