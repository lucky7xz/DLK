{"textgrid.poem.60342": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Wunsch", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Urwald, in deinem Brausen", "tokens": ["Ur\u00b7wald", ",", "in", "dei\u00b7nem", "Brau\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Und ernsten D\u00e4mmerschein", "tokens": ["Und", "erns\u00b7ten", "D\u00e4m\u00b7mer\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit der Geliebten hausen", "tokens": ["Mit", "der", "Ge\u00b7lieb\u00b7ten", "hau\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "M\u00f6cht ich allein \u2013 allein!", "tokens": ["M\u00f6cht", "ich", "al\u00b7lein", "\u2013", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$(", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Von deinen schlanksten B\u00e4umen", "tokens": ["Von", "dei\u00b7nen", "schlanks\u00b7ten", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Baut ich ein H\u00fcttlein traut", "tokens": ["Baut", "ich", "ein", "H\u00fctt\u00b7lein", "traut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir aus zu Himmelsr\u00e4umen;", "tokens": ["Mir", "aus", "zu", "Him\u00b7mels\u00b7r\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APZR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O komm, du sch\u00f6ne Braut!", "tokens": ["O", "komm", ",", "du", "sch\u00f6\u00b7ne", "Braut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich legte Moosgebreite", "tokens": ["Ich", "leg\u00b7te", "Moos\u00b7ge\u00b7brei\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weich unter ihren Schritt,", "tokens": ["Weich", "un\u00b7ter", "ih\u00b7ren", "Schritt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und meine Liebe streute", "tokens": ["Und", "mei\u00b7ne", "Lie\u00b7be", "streu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich unter ihren Tritt.", "tokens": ["Ich", "un\u00b7ter", "ih\u00b7ren", "Tritt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "F\u00fcr sie das Wild erjagen,", "tokens": ["F\u00fcr", "sie", "das", "Wild", "er\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus tiefster Schlucht emp\u00f6rt!", "tokens": ["Aus", "tiefs\u00b7ter", "Schlucht", "em\u00b7p\u00f6rt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr sie den Feind erschlagen,", "tokens": ["F\u00fcr", "sie", "den", "Feind", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der unsern Frieden st\u00f6rt!", "tokens": ["Der", "un\u00b7sern", "Frie\u00b7den", "st\u00f6rt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich w\u00fcrd in Mondesn\u00e4chten,", "tokens": ["Ich", "w\u00fcrd", "in", "Mon\u00b7des\u00b7n\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Beim stillen Sternentanz,", "tokens": ["Beim", "stil\u00b7len", "Ster\u00b7nen\u00b7tanz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von wilden Liedern flechten", "tokens": ["Von", "wil\u00b7den", "Lie\u00b7dern", "flech\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um meine Braut den Kranz;", "tokens": ["Um", "mei\u00b7ne", "Braut", "den", "Kranz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und in den Abendgluten", "tokens": ["Und", "in", "den", "A\u00b7bend\u00b7glu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Fels hier oben stehn,", "tokens": ["Am", "Fels", "hier", "o\u00b7ben", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit ihr die Donnerfluten", "tokens": ["Mit", "ihr", "die", "Don\u00b7ner\u00b7flu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Abgrund st\u00fcrzen sehn;", "tokens": ["Zum", "Ab\u00b7grund", "st\u00fcr\u00b7zen", "sehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und weit hinunter blicken", "tokens": ["Und", "weit", "hin\u00b7un\u00b7ter", "bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lie\u00df' sie mein starker Arm;", "tokens": ["Lie\u00df'", "sie", "mein", "star\u00b7ker", "Arm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wie w\u00fcrd ich sie dann dr\u00fccken", "tokens": ["Wie", "w\u00fcrd", "ich", "sie", "dann", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ans Herz so fest und warm!", "tokens": ["Ans", "Herz", "so", "fest", "und", "warm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}