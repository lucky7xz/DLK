{"dta.poem.7811": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "Die widersinnige Liebe.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.99"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Ja/ Amor/ leugn\u2019 es nicht: Du bist f\u00fcrwar", "tokens": ["Ja", "/", "A\u00b7mor", "/", "leugn'", "es", "nicht", ":", "Du", "bist", "f\u00fcr\u00b7war"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$(", "NE", "$(", "VVIMP", "PPER", "PTKNEG", "$.", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und bist du sehend je: was solte seh\u2019n ein", "tokens": ["Und", "bist", "du", "se\u00b7hend", "je", ":", "was", "sol\u00b7te", "seh'n", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "ADV", "$.", "PWS", "VMFIN", "ADV", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "es schiest nur blind darein/ treibt mit den", "tokens": ["es", "schiest", "nur", "blind", "da\u00b7rein", "/", "treibt", "mit", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PAV", "$(", "VVFIN", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und r\u00e4umt dann soviel Macht/ der Himmel/", "tokens": ["Und", "r\u00e4umt", "dann", "so\u00b7viel", "Macht", "/", "der", "Him\u00b7mel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "NN", "$(", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Fehl/ oder schiesse recht/ du Schalk/ so arg als", "tokens": ["Fehl", "/", "o\u00b7der", "schies\u00b7se", "recht", "/", "du", "Schalk", "/", "so", "arg", "als"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "KON", "VVFIN", "ADJD", "$(", "PPER", "NN", "$(", "ADV", "ADJD", "KOKOM"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die flieht mich/ gegen der ich lebe Lieb-ge-", "tokens": ["Die", "flieht", "mich", "/", "ge\u00b7gen", "der", "ich", "le\u00b7be", "Lie\u00b7bge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "$(", "APPR", "PRELS", "PPER", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "und diese lauft mir nach/ die mir nit Freude", "tokens": ["und", "die\u00b7se", "lauft", "mir", "nach", "/", "die", "mir", "nit", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "PPER", "APPR", "$(", "PRELS", "PPER", "PTKNEG", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "ich liebe/ die mich hasst: und hasse/ die mich", "tokens": ["ich", "lie\u00b7be", "/", "die", "mich", "hasst", ":", "und", "has\u00b7se", "/", "die", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "PRELS", "PPER", "VVFIN", "$.", "KON", "NN", "$(", "PRELS", "PPER"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "O \u00fcmgekehrte Welt! O Flam\u0304en voller Eise!", "tokens": ["O", "\u00fcm\u00b7ge\u00b7kehr\u00b7te", "Welt", "!", "O", "Flam\u0304en", "vol\u00b7ler", "Ei\u00b7se", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "NE", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Soll/ Fillis! deine Lieb mir angenehme seyn:", "tokens": ["Soll", "/", "Fil\u00b7lis", "!", "dei\u00b7ne", "Lieb", "mir", "an\u00b7ge\u00b7neh\u00b7me", "seyn", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$(", "NE", "$.", "PPOSAT", "NN", "PPER", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so tritt mir/ in Gestalt Asterien/ herein.", "tokens": ["so", "tritt", "mir", "/", "in", "Ge\u00b7stalt", "As\u00b7te\u00b7ri\u00b7en", "/", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "APPR", "NN", "NE", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wirst du (izt bey dir mich friert/) mir", "tokens": ["So", "wirst", "du", "(", "izt", "bey", "dir", "mich", "friert", "/", ")", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "$(", "ADV", "APPR", "PPER", "PPER", "VVFIN", "$(", "$(", "PPER"], "meter": "-+--+-++-", "measure": "iambic.tetra.relaxed"}}}}}