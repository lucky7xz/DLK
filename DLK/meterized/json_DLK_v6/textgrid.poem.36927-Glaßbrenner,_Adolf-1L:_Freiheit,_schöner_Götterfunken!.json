{"textgrid.poem.36927": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freiheit, sch\u00f6ner G\u00f6tterfunken!", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freiheit, sch\u00f6ner G\u00f6tterfunken!", "tokens": ["Frei\u00b7heit", ",", "sch\u00f6\u00b7ner", "G\u00f6t\u00b7ter\u00b7fun\u00b7ken", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sang das deutsche Dichterherz.", "tokens": ["Sang", "das", "deut\u00b7sche", "Dich\u00b7ter\u00b7herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch da f\u00fchlten die Hallunken", "tokens": ["Doch", "da", "f\u00fchl\u00b7ten", "die", "Hal\u00b7lun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Schon des Feuertodes Schmerz.", "tokens": ["Schon", "des", "Feu\u00b7er\u00b7to\u00b7des", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Solcher Funke mu\u00df entflammen,", "tokens": ["Sol\u00b7cher", "Fun\u00b7ke", "mu\u00df", "ent\u00b7flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Haben weislich sie gedacht.", "tokens": ["Ha\u00b7ben", "weis\u00b7lich", "sie", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und in Tr\u00fcmmer st\u00fcrzt zusammen", "tokens": ["Und", "in", "Tr\u00fcm\u00b7mer", "st\u00fcrzt", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unsre Gr\u00f6\u00dfe, unsre Macht!", "tokens": ["Uns\u00b7re", "Gr\u00f6\u00b7\u00dfe", ",", "uns\u00b7re", "Macht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}