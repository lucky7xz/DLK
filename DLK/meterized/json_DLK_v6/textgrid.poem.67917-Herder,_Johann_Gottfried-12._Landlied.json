{"textgrid.poem.67917": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "12. Landlied", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "ScH\u00c4FERIN:", "tokens": ["ScH\u00c4\u00b7FE\u00b7RIN", ":"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Meine Sch\u00e4fchen, Morgens fr\u00fch,", "tokens": ["Mei\u00b7ne", "Sch\u00e4f\u00b7chen", ",", "Mor\u00b7gens", "fr\u00fch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00fch bis an den Abend,", "tokens": ["Fr\u00fch", "bis", "an", "den", "A\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Unter Blumen weid ich sie,", "tokens": ["Un\u00b7ter", "Blu\u00b7men", "weid", "ich", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sorg und Leid begrabend.", "tokens": ["Sorg", "und", "Leid", "be\u00b7gra\u00b7bend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Dort und hie", "tokens": ["Dort", "und", "hie"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KON", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Bl\u00e4cken sie:", "tokens": ["Bl\u00e4\u00b7cken", "sie", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Ueberall, froher Schall,", "tokens": ["Ue\u00b7be\u00b7rall", ",", "fro\u00b7her", "Schall", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Unschuld \u00fcberall!", "tokens": ["Un\u00b7schuld", "\u00fc\u00b7be\u00b7rall", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "O wie selig, frei und froh", "tokens": ["O", "wie", "se\u00b7lig", ",", "frei", "und", "froh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Lebt man auf dem Lande so.", "tokens": ["Lebt", "man", "auf", "dem", "Lan\u00b7de", "so", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "ScH\u00c4FER:", "tokens": ["ScH\u00c4\u00b7FER", ":"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Auf dem Felde Morgens fr\u00fch,", "tokens": ["Auf", "dem", "Fel\u00b7de", "Mor\u00b7gens", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00fch bis an den Abend,", "tokens": ["Fr\u00fch", "bis", "an", "den", "A\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Weid' ich meines Vaters Vieh,", "tokens": ["Weid'", "ich", "mei\u00b7nes", "Va\u00b7ters", "Vieh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sorg' und Leid begrabend:", "tokens": ["Sor\u00b7g'", "und", "Leid", "be\u00b7gra\u00b7bend", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dort und hie", "tokens": ["Dort", "und", "hie"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KON", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Bl\u00f6cken sie,", "tokens": ["Bl\u00f6\u00b7cken", "sie", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Ueberall, froher Schall,", "tokens": ["Ue\u00b7be\u00b7rall", ",", "fro\u00b7her", "Schall", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Ruhe \u00fcberall!", "tokens": ["Ru\u00b7he", "\u00fc\u00b7be\u00b7rall", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "O wie ruhig, frei und froh", "tokens": ["O", "wie", "ru\u00b7hig", ",", "frei", "und", "froh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Lebt man auf dem Lande so.", "tokens": ["Lebt", "man", "auf", "dem", "Lan\u00b7de", "so", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "BeIDE:", "tokens": ["BeI\u00b7DE", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Morgens, eh der Tag anbricht,", "tokens": ["Mor\u00b7gens", ",", "eh", "der", "Tag", "an\u00b7bricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn der Thau noch flimmert,", "tokens": ["Wenn", "der", "Thau", "noch", "flim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Fehl' ich ja mein Liebchen nicht,", "tokens": ["Fehl'", "ich", "ja", "mein", "Lieb\u00b7chen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das wie Morgen schimmert.", "tokens": ["Das", "wie", "Mor\u00b7gen", "schim\u00b7mert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "KOKOM", "NN", "VVFIN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}, "line.6": {"text": "K\u00fcssest mich,", "tokens": ["K\u00fcs\u00b7sest", "mich", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "K\u00fcsse dich,", "tokens": ["K\u00fcs\u00b7se", "dich", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Ueberall stilles Thal,", "tokens": ["Ue\u00b7be\u00b7rall", "stil\u00b7les", "Thal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Liebe \u00fcberall.", "tokens": ["Lie\u00b7be", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "O wie selig, frei und froh", "tokens": ["O", "wie", "se\u00b7lig", ",", "frei", "und", "froh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Lebt man auf dem Lande so.", "tokens": ["Lebt", "man", "auf", "dem", "Lan\u00b7de", "so", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}