{"dta.poem.14907": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der zaubrische Spielmann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Also sprach sie, und die Lieder", "tokens": ["Al\u00b7so", "sprach", "sie", ",", "und", "die", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lockten st\u00e4rker aus dem Thal,", "tokens": ["Lock\u00b7ten", "st\u00e4r\u00b7ker", "aus", "dem", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rings durch's ganze Thal hallt's wieder", "tokens": ["Rings", "durch's", "gan\u00b7ze", "Thal", "hallt's", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der Liebe Lust und Qual.", "tokens": ["Von", "der", "Lie\u00b7be", "Lust", "und", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}