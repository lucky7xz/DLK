{"dta.poem.9532": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hat das verh\u00e4ngni\u00df mir den steg zu dir verzehrt/", "tokens": ["Hat", "das", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "mir", "den", "steg", "zu", "dir", "ver\u00b7zehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "VVFIN", "PPER", "ART", "NN", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kan ich/ o G\u00f6ttin! nicht dein rein altar ber\u00fchren/", "tokens": ["Kan", "ich", "/", "o", "G\u00f6t\u00b7tin", "!", "nicht", "dein", "rein", "al\u00b7tar", "be\u00b7r\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$(", "FM", "NN", "$.", "PTKNEG", "PPOSAT", "ADJD", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Soll auf dein heiligthum ich keinen finger f\u00fchren/", "tokens": ["Soll", "auf", "dein", "hei\u00b7lig\u00b7thum", "ich", "kei\u00b7nen", "fin\u00b7ger", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "PPER", "PIAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So hat mir doch die pflicht noch keine zeit verwehrt.", "tokens": ["So", "hat", "mir", "doch", "die", "pflicht", "noch", "kei\u00b7ne", "zeit", "ver\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein geist mu\u00df opfer seyn/ mein hertze wird der herd/", "tokens": ["Mein", "geist", "mu\u00df", "op\u00b7fer", "seyn", "/", "mein", "hert\u00b7ze", "wird", "der", "herd", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "NN", "VAINF", "$(", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich thue/ was ich kan/ und was sich wil geb\u00fchren/", "tokens": ["Ich", "thue", "/", "was", "ich", "kan", "/", "und", "was", "sich", "wil", "ge\u00b7b\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWS", "PPER", "VMFIN", "$(", "KON", "PWS", "PRF", "VMFIN", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Ich wei\u00df/ du wirst itzund mehr als genug versp\u00fcren/", "tokens": ["Ich", "wei\u00df", "/", "du", "wirst", "it\u00b7zund", "mehr", "als", "ge\u00b7nug", "ver\u00b7sp\u00fc\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VAFIN", "ADV", "PIAT", "KOKOM", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was vor ein reiner dampf zu deinem throne f\u00e4hrt.", "tokens": ["Was", "vor", "ein", "rei\u00b7ner", "dampf", "zu", "dei\u00b7nem", "thro\u00b7ne", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich ehre dich allhier/ zwar ohne licht und kertzen/", "tokens": ["Ich", "eh\u00b7re", "dich", "all\u00b7hier", "/", "zwar", "oh\u00b7ne", "licht", "und", "kert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$(", "ADV", "APPR", "NN", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Durch einen heissen trieb/ aus einem reinem hertzen/", "tokens": ["Durch", "ei\u00b7nen", "heis\u00b7sen", "trieb", "/", "aus", "ei\u00b7nem", "rei\u00b7nem", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "$(", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die flamme brennet zwar itzt durch verdeckten schein/", "tokens": ["Die", "flam\u00b7me", "bren\u00b7net", "zwar", "itzt", "durch", "ver\u00b7deck\u00b7ten", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und beug ich keine knie/ so beug ich das gem\u00fcthe/", "tokens": ["Und", "beug", "ich", "kei\u00b7ne", "knie", "/", "so", "beug", "ich", "das", "ge\u00b7m\u00fc\u00b7the", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$(", "ADV", "VVFIN", "PPER", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Acht w\u00f6rter r\u00fchren mir itzunder mein gebl\u00fcte:", "tokens": ["Acht", "w\u00f6r\u00b7ter", "r\u00fch\u00b7ren", "mir", "it\u00b7zun\u00b7der", "mein", "ge\u00b7bl\u00fc\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Gottheit will geehrt/ und nicht geschauet seyn.", "tokens": ["Die", "Got\u00b7theit", "will", "ge\u00b7ehrt", "/", "und", "nicht", "ge\u00b7schau\u00b7et", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVPP", "$(", "KON", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}