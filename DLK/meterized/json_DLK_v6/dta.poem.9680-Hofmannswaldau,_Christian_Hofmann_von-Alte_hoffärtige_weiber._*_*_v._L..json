{"dta.poem.9680": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Alte hoff\u00e4rtige weiber.  \n * * v. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was n\u00fctzt es/ da\u00df ihr euch mit solchem flei\u00df und sorgen", "tokens": ["Was", "n\u00fctzt", "es", "/", "da\u00df", "ihr", "euch", "mit", "sol\u00b7chem", "flei\u00df", "und", "sor\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$(", "KOUS", "PPER", "PRF", "APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die d\u00fcrren beine deckt/ bemahlt die gelbe haut/", "tokens": ["Die", "d\u00fcr\u00b7ren", "bei\u00b7ne", "deckt", "/", "be\u00b7mahlt", "die", "gel\u00b7be", "haut", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die runtzeln machet glatt/ bespiegelt/ und beschaut/", "tokens": ["Die", "runt\u00b7zeln", "ma\u00b7chet", "glatt", "/", "be\u00b7spie\u00b7gelt", "/", "und", "be\u00b7schaut", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$(", "VVPP", "$(", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht nur die halbe nacht/ auch noch den gantzen morgen?", "tokens": ["Nicht", "nur", "die", "hal\u00b7be", "nacht", "/", "auch", "noch", "den", "gant\u00b7zen", "mor\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "ADJA", "NN", "$(", "ADV", "ADV", "ART", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denckt ihr durch dieses lieb und gunst euch zu erwecken?", "tokens": ["Denckt", "ihr", "durch", "die\u00b7ses", "lieb", "und", "gunst", "euch", "zu", "er\u00b7we\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "ADJD", "KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "O eitles narrenwerck! haut bleibt haut/ bein bleibt bein.", "tokens": ["O", "eit\u00b7les", "nar\u00b7ren\u00b7werck", "!", "haut", "bleibt", "haut", "/", "bein", "bleibt", "bein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "ADJD", "VVFIN", "ADJD", "$(", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-++-+--+", "measure": "iambic.hexa.chol"}, "line.7": {"text": "Man ken\u0303t/ was wei\u00df/ was gelb/ was glatt/ was runtzeln seyn/", "tokens": ["Man", "ke\u00f1t", "/", "was", "wei\u00df", "/", "was", "gelb", "/", "was", "glatt", "/", "was", "runt\u00b7zeln", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "PWS", "VVFIN", "$(", "PWS", "ADJD", "$(", "PWS", "ADJD", "$(", "PWS", "VVFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein stock/ der bleibt ein stock/ ob ihn gold/ seide decken.", "tokens": ["Ein", "stock", "/", "der", "bleibt", "ein", "stock", "/", "ob", "ihn", "gold", "/", "sei\u00b7de", "de\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "VVFIN", "ART", "NN", "$(", "KOUS", "PPER", "NN", "$(", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Man liebet altes geld/ nicht aber alte leiber;", "tokens": ["Man", "lie\u00b7bet", "al\u00b7tes", "geld", "/", "nicht", "a\u00b7ber", "al\u00b7te", "lei\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJA", "NN", "$(", "PTKNEG", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die welt die hasset euch/ ob ihr sie schon nicht hasst/", "tokens": ["Die", "welt", "die", "has\u00b7set", "euch", "/", "ob", "ihr", "sie", "schon", "nicht", "hasst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "PPER", "$(", "KOUS", "PPER", "PPER", "ADV", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Jhr machet ihr beschwerd/ wie sehr ihr sie umfast/", "tokens": ["Ihr", "ma\u00b7chet", "ihr", "be\u00b7schwerd", "/", "wie", "sehr", "ihr", "sie", "um\u00b7fast", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$(", "KOKOM", "ADV", "PPER", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es ist kein \u00e4rger ding/ als alt- und stoltze weiber.", "tokens": ["Es", "ist", "kein", "\u00e4r\u00b7ger", "ding", "/", "als", "al\u00b7t", "und", "stolt\u00b7ze", "wei\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "$(", "KOUS", "TRUNC", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}