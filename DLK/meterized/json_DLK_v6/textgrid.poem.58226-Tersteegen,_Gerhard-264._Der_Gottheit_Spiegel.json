{"textgrid.poem.58226": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "264. Der Gottheit Spiegel", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein lautrer Seelengrund der Gottheit Spiegel ist,", "tokens": ["Dein", "laut\u00b7rer", "See\u00b7len\u00b7grund", "der", "Got\u00b7theit", "Spie\u00b7gel", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die mind'ste Eigenheit macht seinen Glanz vergehen;", "tokens": ["Die", "min\u00b7d'\u00b7ste", "Ei\u00b7gen\u00b7heit", "macht", "sei\u00b7nen", "Glanz", "ver\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "R\u00fchr nichts, was unrein, an! Wo du recht stille bist,", "tokens": ["R\u00fchr", "nichts", ",", "was", "un\u00b7rein", ",", "an", "!", "Wo", "du", "recht", "stil\u00b7le", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "$,", "PRELS", "ADJD", "$,", "PTKVZ", "$.", "PWAV", "PPER", "ADJD", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wirst du in Gottes Licht Gott selbst bald in dir sehen.", "tokens": ["Wirst", "du", "in", "Got\u00b7tes", "Licht", "Gott", "selbst", "bald", "in", "dir", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "NN", "NN", "ADV", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}