{"dta.poem.14884": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die wunderliche Prinzessin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Weit in einem Walde droben", "tokens": ["Weit", "in", "ei\u00b7nem", "Wal\u00b7de", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwischen hoher Felsen Zinnen,", "tokens": ["Zwi\u00b7schen", "ho\u00b7her", "Fel\u00b7sen", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Steht ein altes Schlo\u00df erhoben,", "tokens": ["Steht", "ein", "al\u00b7tes", "Schlo\u00df", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wohnet eine Zaub'rin drinne.", "tokens": ["Woh\u00b7net", "ei\u00b7ne", "Zaub'\u00b7rin", "drin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Von dem Schlo\u00df, der Zaub'rin Sch\u00f6ne", "tokens": ["Von", "dem", "Schlo\u00df", ",", "der", "Zaub'\u00b7rin", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gehen wunderbare Sagen,", "tokens": ["Ge\u00b7hen", "wun\u00b7der\u00b7ba\u00b7re", "Sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Lockend schweifen fremde T\u00f6ne", "tokens": ["Lo\u00b7ckend", "schwei\u00b7fen", "frem\u00b7de", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Pl\u00f6tzlich her oft aus dem Walde.", "tokens": ["Pl\u00f6tz\u00b7lich", "her", "oft", "aus", "dem", "Wal\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "Wem sie recht das Herz getroffen,", "tokens": ["Wem", "sie", "recht", "das", "Herz", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Der mu\u00df nach dem Walde gehen,", "tokens": ["Der", "mu\u00df", "nach", "dem", "Wal\u00b7de", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Ewig diesen Kl\u00e4ngen folgend,", "tokens": ["E\u00b7wig", "die\u00b7sen", "Kl\u00e4n\u00b7gen", "fol\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und wird nimmer mehr gesehen.", "tokens": ["Und", "wird", "nim\u00b7mer", "mehr", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Tief in wundersamer Gr\u00fcne", "tokens": ["Tief", "in", "wun\u00b7der\u00b7sa\u00b7mer", "Gr\u00fc\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Steht das Schlo\u00df, schon halb verfallen,", "tokens": ["Steht", "das", "Schlo\u00df", ",", "schon", "halb", "ver\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Hell die gold'nen Zinnen gl\u00fchen,", "tokens": ["Hell", "die", "gold'\u00b7nen", "Zin\u00b7nen", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Einsam sind die weiten Hallen.", "tokens": ["Ein\u00b7sam", "sind", "die", "wei\u00b7ten", "Hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Auf des Hofes stein'gem Rasen", "tokens": ["Auf", "des", "Ho\u00b7fes", "stein'\u00b7gem", "Ra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Sitzen von der Tafelrunde", "tokens": ["Sit\u00b7zen", "von", "der", "Ta\u00b7fel\u00b7run\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "All' die Helden dort gelagert,", "tokens": ["All'", "die", "Hel\u00b7den", "dort", "ge\u00b7la\u00b7gert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Ueberdeckt mit Staub und Wunden.", "tokens": ["Ue\u00b7berd\u00b7eckt", "mit", "Staub", "und", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Heinrich liegt auf seinem L\u00f6wen,", "tokens": ["Hein\u00b7rich", "liegt", "auf", "sei\u00b7nem", "L\u00f6\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Gottfried auch, Siegfried der Scharfe,", "tokens": ["Gott\u00b7fried", "auch", ",", "Sieg\u00b7fried", "der", "Schar\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "NN", "ART", "NN", "$,"], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.23": {"text": "K\u00f6nig Alfred, eingeschlafen", "tokens": ["K\u00f6\u00b7nig", "Al\u00b7fred", ",", "ein\u00b7ge\u00b7schla\u00b7fen"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "NE", "$,", "VVPP"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.24": {"text": "Ueber seiner gold'nen Harfe.", "tokens": ["Ue\u00b7ber", "sei\u00b7ner", "gold'\u00b7nen", "Har\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Don Quixot' hoch auf der Mauer", "tokens": ["Don", "Qui\u00b7xot'", "hoch", "auf", "der", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ADJD", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.26": {"text": "Sinnend tief in n\u00e4cht'ger Stunde,", "tokens": ["Sin\u00b7nend", "tief", "in", "n\u00e4cht'\u00b7ger", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Steht ger\u00fcstet auf der Lauer", "tokens": ["Steht", "ge\u00b7r\u00fcs\u00b7tet", "auf", "der", "Lau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "VVPP", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Und bewacht die heil'ge Runde.", "tokens": ["Und", "be\u00b7wacht", "die", "heil'\u00b7ge", "Run\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}