{"textgrid.poem.44507": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Ein Finanz-Gedicht", "genre": "verse", "period": "N.A.", "pub_year": 1840, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Franke sinnt auf einen Streich.", "tokens": ["Der", "Fran\u00b7ke", "sinnt", "auf", "ei\u00b7nen", "Streich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Russe mehrt ein starkes Reich.", "tokens": ["Der", "Rus\u00b7se", "mehrt", "ein", "star\u00b7kes", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Anlehn! Ein Anlehn!", "tokens": ["Ein", "An\u00b7lehn", "!", "Ein", "An\u00b7lehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Macht alles wieder gleich.", "tokens": ["Macht", "al\u00b7les", "wie\u00b7der", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der lange Friede Segen schafft,", "tokens": ["Der", "lan\u00b7ge", "Frie\u00b7de", "Se\u00b7gen", "schafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir platzen fast vor innrer Kraft.", "tokens": ["Wir", "plat\u00b7zen", "fast", "vor", "inn\u00b7rer", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Anlehn! Ein Anlehn", "tokens": ["Ein", "An\u00b7lehn", "!", "Ein", "An\u00b7lehn"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Verd\u00fcnnt den Lebenssaft.", "tokens": ["Ver\u00b7d\u00fcnnt", "den", "Le\u00b7bens\u00b7saft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bankbanus ist nicht unser Mann,", "tokens": ["Bank\u00b7ba\u00b7nus", "ist", "nicht", "un\u00b7ser", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir nehmen einen andern an.", "tokens": ["Wir", "neh\u00b7men", "ei\u00b7nen", "an\u00b7dern", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Anlehn, ein Anlehn", "tokens": ["Ein", "An\u00b7lehn", ",", "ein", "An\u00b7lehn"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zeigt gleich, wieviel er kann.", "tokens": ["Zeigt", "gleich", ",", "wie\u00b7viel", "er", "kann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch h\u00e4lt die Lehne l\u00e4nger nicht.", "tokens": ["Doch", "h\u00e4lt", "die", "Leh\u00b7ne", "l\u00e4n\u00b7ger", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Bank und Fu\u00df, der Zinsfu\u00df bricht,", "tokens": ["Wenn", "Bank", "und", "Fu\u00df", ",", "der", "Zins\u00b7fu\u00df", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lehn an, lehn!", "tokens": ["Lehn", "an", ",", "lehn", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Dann liegen wir und lehnen nicht.", "tokens": ["Dann", "lie\u00b7gen", "wir", "und", "leh\u00b7nen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}