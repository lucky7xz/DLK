{"dta.poem.18802": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Behramgur, sagt man, hat den Reim erfunden,", "tokens": ["Behr\u00b7am\u00b7gur", ",", "sagt", "man", ",", "hat", "den", "Reim", "er\u00b7fun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PIS", "$,", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er sprach entz\u00fcckt aus reiner Seele Drang;", "tokens": ["Er", "sprach", "ent\u00b7z\u00fcckt", "aus", "rei\u00b7ner", "See\u00b7le", "Drang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dilara schnell, die Freundinn seiner Stunden,", "tokens": ["Di\u00b7la\u00b7ra", "schnell", ",", "die", "Freun\u00b7dinn", "sei\u00b7ner", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erwiederte mit gleichem Wort und Klang.", "tokens": ["Er\u00b7wie\u00b7der\u00b7te", "mit", "glei\u00b7chem", "Wort", "und", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und so, Geliebte! warst du mir beschieden", "tokens": ["Und", "so", ",", "Ge\u00b7lieb\u00b7te", "!", "warst", "du", "mir", "be\u00b7schie\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "NN", "$.", "VAFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Reims zu finden holden Lustgebrauch,", "tokens": ["Des", "Reims", "zu", "fin\u00b7den", "hol\u00b7den", "Lust\u00b7ge\u00b7brauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dass auch Behramgur ich, den Sassaniden,", "tokens": ["Dass", "auch", "Behr\u00b7am\u00b7gur", "ich", ",", "den", "Sas\u00b7sa\u00b7ni\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "PPER", "$,", "ART", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nicht mehr beneiden darf: mir ward es auch.", "tokens": ["Nicht", "mehr", "be\u00b7nei\u00b7den", "darf", ":", "mir", "ward", "es", "auch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "VMFIN", "$.", "PPER", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hast mir diess Buch geweckt, du hast\u2019s gegeben:", "tokens": ["Hast", "mir", "diess", "Buch", "ge\u00b7weckt", ",", "du", "hast's", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDAT", "NN", "VVPP", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn was ich froh, aus vollem Herzen, sprach,", "tokens": ["Denn", "was", "ich", "froh", ",", "aus", "vol\u00b7lem", "Her\u00b7zen", ",", "sprach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das klang zur\u00fcck aus deinem holden Leben,", "tokens": ["Das", "klang", "zu\u00b7r\u00fcck", "aus", "dei\u00b7nem", "hol\u00b7den", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie Blick dem Blick, so Reim dem Reime nach.", "tokens": ["Wie", "Blick", "dem", "Blick", ",", "so", "Reim", "dem", "Rei\u00b7me", "nach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "$,", "ADV", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun t\u00f6n\u2019 es fort zu dir, auch aus der Ferne", "tokens": ["Nun", "t\u00f6n'", "es", "fort", "zu", "dir", ",", "auch", "aus", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "PPER", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Wort erreicht, und schw\u00e4nde Ton und Schall", "tokens": ["Das", "Wort", "er\u00b7reicht", ",", "und", "schw\u00e4n\u00b7de", "Ton", "und", "Schall"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "KON", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist\u2019s nicht der Mantel noch ges\u00e4ter Sterne?", "tokens": ["Ist's", "nicht", "der", "Man\u00b7tel", "noch", "ge\u00b7s\u00e4\u00b7ter", "Ster\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist\u2019s nicht der Liebe hochverkl\u00e4rtes All?", "tokens": ["Ist's", "nicht", "der", "Lie\u00b7be", "hoch\u00b7ver\u00b7kl\u00e4r\u00b7tes", "All", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}