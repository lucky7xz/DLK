{"textgrid.poem.28603": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott gibt uns an Leib und Seele so viel Sch\u00e4tze, so viel Gaben;", "tokens": ["Gott", "gibt", "uns", "an", "Leib", "und", "See\u00b7le", "so", "viel", "Sch\u00e4t\u00b7ze", ",", "so", "viel", "Ga\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "ADV", "PIAT", "NN", "$,", "ADV", "PIAT", "NN", "$."], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Wil f\u00fcr Gaben, wil f\u00fcr Sch\u00e4tze blo\u00df nur unser Hertzen haben.", "tokens": ["Wil", "f\u00fcr", "Ga\u00b7ben", ",", "wil", "f\u00fcr", "Sch\u00e4t\u00b7ze", "blo\u00df", "nur", "un\u00b7ser", "Hert\u00b7zen", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "$,", "VMFIN", "APPR", "NN", "ADV", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wir zwar nemen Sch\u00e4tz und Gaben, lassen aber Schatz und Gaben", "tokens": ["Wir", "zwar", "ne\u00b7men", "Sch\u00e4tz", "und", "Ga\u00b7ben", ",", "las\u00b7sen", "a\u00b7ber", "Schatz", "und", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJA", "NN", "KON", "NN", "$,", "VVINF", "KON", "NN", "KON", "NN"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "(nicht der Sch\u00e4tz und Gaben Geber,) unsre gantze Hertzen haben.", "tokens": ["(", "nicht", "der", "Sch\u00e4tz", "und", "Ga\u00b7ben", "Ge\u00b7ber", ",", ")", "uns\u00b7re", "gant\u00b7ze", "Hert\u00b7zen", "ha\u00b7ben", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "ART", "NN", "KON", "NN", "NN", "$,", "$(", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}