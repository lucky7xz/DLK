{"textgrid.poem.51149": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1. Unser ist das Los der Epigonen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unser ist das Los der Epigonen,", "tokens": ["Un\u00b7ser", "ist", "das", "Los", "der", "E\u00b7pi\u00b7go\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die im weiten Zwischenreiche wohnen;", "tokens": ["Die", "im", "wei\u00b7ten", "Zwi\u00b7schen\u00b7rei\u00b7che", "woh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Seht, wie ihr noch einen Tropfen presset", "tokens": ["Seht", ",", "wie", "ihr", "noch", "ei\u00b7nen", "Trop\u00b7fen", "pres\u00b7set"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "ADV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aus den alten Schalen der Zitronen!", "tokens": ["Aus", "den", "al\u00b7ten", "Scha\u00b7len", "der", "Zit\u00b7ro\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Geistiges ist m\u00e4\u00dfig noch vorhanden,", "tokens": ["Geis\u00b7ti\u00b7ges", "ist", "m\u00e4\u00b7\u00dfig", "noch", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Auch des Lebens S\u00fc\u00dfe wird noch lohnen;", "tokens": ["Auch", "des", "Le\u00b7bens", "S\u00fc\u00b7\u00dfe", "wird", "noch", "loh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wasser flutet uns in breiten Str\u00f6men,", "tokens": ["Was\u00b7ser", "flu\u00b7tet", "uns", "in", "brei\u00b7ten", "Str\u00f6\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Brauchen es am wenigsten zu schonen:", "tokens": ["Brau\u00b7chen", "es", "am", "we\u00b7nigs\u00b7ten", "zu", "scho\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "PIS", "PTKZU", "VVINF", "$."], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Braut den Trank f\u00fcr lange Wintern\u00e4chte,", "tokens": ["Braut", "den", "Trank", "f\u00fcr", "lan\u00b7ge", "Win\u00b7ter\u00b7n\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Bis uns bl\u00fchen neue Lenzeskronen!", "tokens": ["Bis", "uns", "bl\u00fc\u00b7hen", "neu\u00b7e", "Len\u00b7zes\u00b7kro\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Und der Dichtung Fahrzeug mag entrinnen", "tokens": ["Und", "der", "Dich\u00b7tung", "Fahr\u00b7zeug", "mag", "ent\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VMFIN", "VVINF"], "meter": "--+-+++-+-", "measure": "anapaest.init"}, "line.12": {"text": "Dem Bereich der grausen L\u00e4strygonen!", "tokens": ["Dem", "Be\u00b7reich", "der", "grau\u00b7sen", "L\u00e4s\u00b7try\u00b7go\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}