{"dta.poem.21320": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput VI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unter dem Mantel hielt er etwas", "tokens": ["Un\u00b7ter", "dem", "Man\u00b7tel", "hielt", "er", "et\u00b7was"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Verborgen, das seltsam blinkte", "tokens": ["Ver\u00b7bor\u00b7gen", ",", "das", "selt\u00b7sam", "blink\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenn es zum Vorschein kam, und ein Beil,", "tokens": ["Wenn", "es", "zum", "Vor\u00b7schein", "kam", ",", "und", "ein", "Beil", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ein Richtbeil, zu seyn mir d\u00fcnkte.", "tokens": ["Ein", "Richt\u00b7beil", ",", "zu", "seyn", "mir", "d\u00fcnk\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKZU", "VAINF", "PPER", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Er schien von untersetzter Statur,", "tokens": ["Er", "schien", "von", "un\u00b7ter\u00b7setz\u00b7ter", "Sta\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Augen wie zwey Sterne;", "tokens": ["Die", "Au\u00b7gen", "wie", "zwey", "Ster\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er st\u00f6rte mich im Schreiben nie,", "tokens": ["Er", "st\u00f6r\u00b7te", "mich", "im", "Schrei\u00b7ben", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blieb ruhig stehn in der Ferne.", "tokens": ["Blieb", "ru\u00b7hig", "stehn", "in", "der", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Seit Jahren hatte ich nicht gesehn", "tokens": ["Seit", "Jah\u00b7ren", "hat\u00b7te", "ich", "nicht", "ge\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den sonderbaren Gesellen,", "tokens": ["Den", "son\u00b7der\u00b7ba\u00b7ren", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da fand ich ihn pl\u00f6tzlich wieder hier", "tokens": ["Da", "fand", "ich", "ihn", "pl\u00f6tz\u00b7lich", "wie\u00b7der", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "ADV", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In der stillen Mondnacht zu C\u00f6llen.", "tokens": ["In", "der", "stil\u00b7len", "Mond\u00b7nacht", "zu", "C\u00f6l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich schlenderte sinnend die Stra\u00dfen entlang,", "tokens": ["Ich", "schlen\u00b7der\u00b7te", "sin\u00b7nend", "die", "Stra\u00b7\u00dfen", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da sah ich ihn hinter mir gehen,", "tokens": ["Da", "sah", "ich", "ihn", "hin\u00b7ter", "mir", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Als ob er mein Schatten w\u00e4re, und stand", "tokens": ["Als", "ob", "er", "mein", "Schat\u00b7ten", "w\u00e4\u00b7re", ",", "und", "stand"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$,", "KON", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich still, so blieb er stehen.", "tokens": ["Ich", "still", ",", "so", "blieb", "er", "ste\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}