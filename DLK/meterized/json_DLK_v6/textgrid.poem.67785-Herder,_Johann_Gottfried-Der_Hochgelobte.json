{"textgrid.poem.67785": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Hochgelobte", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lob aller Engel! Lob der Christenheit!", "tokens": ["Lob", "al\u00b7ler", "En\u00b7gel", "!", "Lob", "der", "Chris\u00b7ten\u00b7heit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$.", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und lobte Dich in Ewigkeit,", "tokens": ["Und", "lob\u00b7te", "Dich", "in", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was Odem hat, noch w\u00e4re nicht gesungen", "tokens": ["Was", "O\u00b7dem", "hat", ",", "noch", "w\u00e4\u00b7re", "nicht", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "NE", "VAFIN", "$,", "ADV", "VAFIN", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Lob, o Herr! Du hast den Tod bezwungen,", "tokens": ["Dein", "Lob", ",", "o", "Herr", "!", "Du", "hast", "den", "Tod", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den Himmel, Herr, uns wiederbracht", "tokens": ["Den", "Him\u00b7mel", ",", "Herr", ",", "uns", "wie\u00b7der\u00b7bracht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch Deines Blutes Macht!", "tokens": ["Durch", "Dei\u00b7nes", "Blu\u00b7tes", "Macht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Seit unser Herr vom Tod erstand,", "tokens": ["Seit", "un\u00b7ser", "Herr", "vom", "Tod", "er\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Beut er uns seine Hand.", "tokens": ["Beut", "er", "uns", "sei\u00b7ne", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Ein milder K\u00f6nig! L\u00f6set uns von Pein,", "tokens": ["Ein", "mil\u00b7der", "K\u00f6\u00b7nig", "!", "L\u00f6\u00b7set", "uns", "von", "Pein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df wir sein eigen sei'n.", "tokens": ["Da\u00df", "wir", "sein", "ei\u00b7gen", "sei'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer seines Himmelreichs von Herzen nur begehrt,", "tokens": ["Wer", "sei\u00b7nes", "Him\u00b7mel\u00b7reichs", "von", "Her\u00b7zen", "nur", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "F\u00fcrwahr, dem wird's gew\u00e4hrt.", "tokens": ["F\u00fcr\u00b7wahr", ",", "dem", "wird's", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er l\u00e4\u00dft genie\u00dfen uns des Lobes,", "tokens": ["Er", "l\u00e4\u00dft", "ge\u00b7nie\u00b7\u00dfen", "uns", "des", "Lo\u00b7bes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Damit sein Gott ihn preist.", "tokens": ["Da\u00b7mit", "sein", "Gott", "ihn", "preist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer recht ihn seinen Herren hei\u00dft,", "tokens": ["Wer", "recht", "ihn", "sei\u00b7nen", "Her\u00b7ren", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wird von der S\u00fcnde frei und nimmer satt des Lobes!", "tokens": ["Wird", "von", "der", "S\u00fcn\u00b7de", "frei", "und", "nim\u00b7mer", "satt", "des", "Lo\u00b7bes", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJD", "KON", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}