{"dta.poem.10317": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Die Attacke.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Platz da, und Zieten aus dem Busch,", "tokens": ["Platz", "da", ",", "und", "Zie\u00b7ten", "aus", "dem", "Busch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Hurrah drauf in Flusch und Husch,", "tokens": ["Mit", "Hur\u00b7rah", "drauf", "in", "Flusch", "und", "Husch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PAV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und vorgebeugten Leibes rasen,", "tokens": ["Und", "vor\u00b7ge\u00b7beug\u00b7ten", "Lei\u00b7bes", "ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In einem Strich die Pferdenasen,", "tokens": ["In", "ei\u00b7nem", "Strich", "die", "Pfer\u00b7de\u00b7na\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wir zwei weit voran den Husaren,", "tokens": ["Wir", "zwei", "weit", "vo\u00b7ran", "den", "Hu\u00b7sa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "So sind wir in den Feind gefahren.", "tokens": ["So", "sind", "wir", "in", "den", "Feind", "ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die roten Jungen hinterher", "tokens": ["Die", "ro\u00b7ten", "Jun\u00b7gen", "hin\u00b7ter\u00b7her"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In todesbringender Carriere,", "tokens": ["In", "to\u00b7des\u00b7brin\u00b7gen\u00b7der", "Car\u00b7rie\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df wild die Spitzen der Chabracken", "tokens": ["Da\u00df", "wild", "die", "Spit\u00b7zen", "der", "Cha\u00b7bra\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Den Grashalm fegen wie der Wind.", "tokens": ["Den", "Gras\u00b7halm", "fe\u00b7gen", "wie", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und hussah, hep, die bunten Jacken,", "tokens": ["Und", "hus\u00b7sah", ",", "hep", ",", "die", "bun\u00b7ten", "Ja\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sind wir am Waldesrand geschwind.", "tokens": ["Sind", "wir", "am", "Wal\u00b7des\u00b7rand", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Geknatter, dann ein tolles Laufen,", "tokens": ["Ge\u00b7knat\u00b7ter", ",", "dann", "ein", "tol\u00b7les", "Lau\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wir konnten kaum mit ihnen raufen,", "tokens": ["Wir", "konn\u00b7ten", "kaum", "mit", "ih\u00b7nen", "rau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "So rissen die Gascogner aus", "tokens": ["So", "ris\u00b7sen", "die", "Gas\u00b7cog\u00b7ner", "aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Vor unserm S\u00e4belschnittgesaus.", "tokens": ["Vor", "un\u00b7serm", "S\u00e4\u00b7bel\u00b7schnitt\u00b7ge\u00b7saus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Doch hinter einer schmalen Erle", "tokens": ["Doch", "hin\u00b7ter", "ei\u00b7ner", "schma\u00b7len", "Er\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Stand einer dieser kleinen Kerle", "tokens": ["Stand", "ei\u00b7ner", "die\u00b7ser", "klei\u00b7nen", "Ker\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und macht auf mich recht schlechte Witze,", "tokens": ["Und", "macht", "auf", "mich", "recht", "schlech\u00b7te", "Wit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und scho\u00df mir ab die Helmturmspitze.", "tokens": ["Und", "scho\u00df", "mir", "ab", "die", "Helm\u00b7turm\u00b7spit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Ei, du verfluchter gelber L\u00fcmmel,", "tokens": ["Ei", ",", "du", "ver\u00b7fluch\u00b7ter", "gel\u00b7ber", "L\u00fcm\u00b7mel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.22": {"text": "Ich treffe gleich dich im Get\u00fcmmel.", "tokens": ["Ich", "tref\u00b7fe", "gleich", "dich", "im", "Ge\u00b7t\u00fcm\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.23": {"text": "Und \u201eHieb zur Erde tief\u201c, sa\u00df ihm", "tokens": ["Und", "\u201e", "Hieb", "zur", "Er\u00b7de", "tief", "\u201c", ",", "sa\u00df", "ihm"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "$(", "ADV", "APPRART", "NN", "ADJD", "$(", "$,", "VVFIN", "PPER"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.24": {"text": "Im Sch\u00e4del eine forsche Prim.", "tokens": ["Im", "Sch\u00e4\u00b7del", "ei\u00b7ne", "for\u00b7sche", "Prim", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kolonnen r\u00fcckten nun heran,", "tokens": ["Ko\u00b7lon\u00b7nen", "r\u00fcck\u00b7ten", "nun", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Auftrag war erf\u00fcllt, gethan.", "tokens": ["Der", "Auf\u00b7trag", "war", "er\u00b7f\u00fcllt", ",", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Lieutenant sammelte den Zug,", "tokens": ["Der", "Lie\u00b7u\u00b7ten\u00b7ant", "sam\u00b7mel\u00b7te", "den", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und als er durch die S\u00e4bel frug,", "tokens": ["Und", "als", "er", "durch", "die", "S\u00e4\u00b7bel", "frug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob Keiner fortblieb, Keiner fehle,", "tokens": ["Ob", "Kei\u00b7ner", "fort\u00b7blieb", ",", "Kei\u00b7ner", "feh\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da schn\u00fcrt es ihm die junge Kehle.", "tokens": ["Da", "schn\u00fcrt", "es", "ihm", "die", "jun\u00b7ge", "Keh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Denn der Trompeterschimmel b\u00e4umte,", "tokens": ["Denn", "der", "Trom\u00b7pe\u00b7ter\u00b7schim\u00b7mel", "b\u00e4um\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Den Sattel frei, und schnob und sch\u00e4umte.", "tokens": ["Den", "Sat\u00b7tel", "frei", ",", "und", "schnob", "und", "sch\u00e4um\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wir fanden seinen Reiter bald", "tokens": ["Wir", "fan\u00b7den", "sei\u00b7nen", "Rei\u00b7ter", "bald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "An Brombeerstr\u00e4uchen, tot, im Wald.", "tokens": ["An", "Brom\u00b7beer\u00b7str\u00e4u\u00b7chen", ",", "tot", ",", "im", "Wald", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ein blaurot Fleckchen zeigte nur", "tokens": ["Ein", "blau\u00b7rot", "Fleck\u00b7chen", "zeig\u00b7te", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Den Schu\u00df ins Herz, der Kugel Spur.", "tokens": ["Den", "Schu\u00df", "ins", "Herz", ",", "der", "Ku\u00b7gel", "Spur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Bei meinem Freund zum ersten Mal", "tokens": ["Bei", "mei\u00b7nem", "Freund", "zum", "ers\u00b7ten", "Mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Sah ich die Scherbe niederschnippen,", "tokens": ["Sah", "ich", "die", "Scher\u00b7be", "nie\u00b7der\u00b7schnip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und Thr\u00e4nen fielen ohne Zahl", "tokens": ["Und", "Thr\u00e4\u00b7nen", "fie\u00b7len", "oh\u00b7ne", "Zahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Dem Toten auf die bleichen Lippen.", "tokens": ["Dem", "To\u00b7ten", "auf", "die", "blei\u00b7chen", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O sch\u00e4m\u2019 dich nicht, wenn dies du liest,", "tokens": ["O", "sch\u00e4m'", "dich", "nicht", ",", "wenn", "dies", "du", "liest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PDS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df dir so leicht die Thr\u00e4ne flie\u00dft.", "tokens": ["Da\u00df", "dir", "so", "leicht", "die", "Thr\u00e4\u00b7ne", "flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Sterben tr\u00e4gst du noch die Scherbe,", "tokens": ["Im", "Ster\u00b7ben", "tr\u00e4gst", "du", "noch", "die", "Scher\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich sei, stirbst fr\u00fcher du, der Erbe,", "tokens": ["Ich", "sei", ",", "stirbst", "fr\u00fc\u00b7her", "du", ",", "der", "Er\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADV", "ADJD", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dann denk\u2019 ich an den treusten Freund,", "tokens": ["Dann", "denk'", "ich", "an", "den", "treus\u00b7ten", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den je die Sonne hat gebr\u00e4unt.", "tokens": ["Den", "je", "die", "Son\u00b7ne", "hat", "ge\u00b7br\u00e4unt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}