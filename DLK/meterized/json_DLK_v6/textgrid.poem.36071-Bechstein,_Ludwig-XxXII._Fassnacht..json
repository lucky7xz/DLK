{"textgrid.poem.36071": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "XxXII. Fassnacht.", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Faustus spricht Mephisto: \u00bbDu bist der Alte nicht mehr!", "tokens": ["Zu", "Faus\u00b7tus", "spricht", "Me\u00b7phis\u00b7to", ":", "\u00bb", "Du", "bist", "der", "Al\u00b7te", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "NE", "$.", "$(", "PPER", "VAFIN", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich f\u00fclle Dir den Becher, Du trinkst ihn nimmer leer.", "tokens": ["Ich", "f\u00fcl\u00b7le", "Dir", "den", "Be\u00b7cher", ",", "Du", "trinkst", "ihn", "nim\u00b7mer", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Lustfrucht, Dir geboten, so lockend und so reich,", "tokens": ["Die", "Lust\u00b7frucht", ",", "Dir", "ge\u00b7bo\u00b7ten", ",", "so", "lo\u00b7ckend", "und", "so", "reich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVPP", "$,", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Erfreut Dich nicht, und Todten im Grabe bist Du gleich.\u00ab", "tokens": ["Er\u00b7freut", "Dich", "nicht", ",", "und", "Tod\u00b7ten", "im", "Gra\u00b7be", "bist", "Du", "gleich", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "KON", "NN", "APPRART", "NN", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbwer sich der Freuden absagt, was braucht der meine Macht?", "tokens": ["\u00bb", "wer", "sich", "der", "Freu\u00b7den", "ab\u00b7sagt", ",", "was", "braucht", "der", "mei\u00b7ne", "Macht", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PRF", "ART", "NN", "VVFIN", "$,", "PWS", "VVFIN", "ART", "PPOSAT", "NN", "$."], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du gleichest dem Begrabnen im tiefen Bergesschacht.", "tokens": ["Du", "glei\u00b7chest", "dem", "Be\u00b7grab\u00b7nen", "im", "tie\u00b7fen", "Ber\u00b7ges\u00b7schacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Du bist ein wacher Tr\u00e4umer, und tr\u00e4umst nicht einmal recht;", "tokens": ["Du", "bist", "ein", "wa\u00b7cher", "Tr\u00e4u\u00b7mer", ",", "und", "tr\u00e4umst", "nicht", "ein\u00b7mal", "recht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Du willst der Geister Herr sein, und bist der Zweifel Knecht!\u00ab", "tokens": ["Du", "willst", "der", "Geis\u00b7ter", "Herr", "sein", ",", "und", "bist", "der", "Zwei\u00b7fel", "Knecht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "VAINF", "$,", "KON", "VAFIN", "ART", "NN", "NE", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbhast Du nicht alles, alles, was Deinen Sinn erfreut?", "tokens": ["\u00bb", "hast", "Du", "nicht", "al\u00b7les", ",", "al\u00b7les", ",", "was", "Dei\u00b7nen", "Sinn", "er\u00b7freut", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "PTKNEG", "PIS", "$,", "PIS", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Hast Ehre, Ruhm und Namen, und hast Unsterblichkeit!", "tokens": ["Hast", "Eh\u00b7re", ",", "Ruhm", "und", "Na\u00b7men", ",", "und", "hast", "U\u00b7nsterb\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NN", "KON", "NN", "$,", "KON", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Du bist ein m\u00e4cht'ger Herrscher, vor dem die Freude kniet,", "tokens": ["Du", "bist", "ein", "m\u00e4cht'\u00b7ger", "Herr\u00b7scher", ",", "vor", "dem", "die", "Freu\u00b7de", "kniet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der mit Ascetenblicken auf dieses Lustbild sieht.\u00ab", "tokens": ["Der", "mit", "A\u00b7sce\u00b7ten\u00b7bli\u00b7cken", "auf", "die\u00b7ses", "Lust\u00b7bild", "sieht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "NN", "APPR", "PDAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbwas gr\u00fcbelst Du noch einsam, wie Du zuvor gethan,", "tokens": ["\u00bb", "was", "gr\u00fc\u00b7belst", "Du", "noch", "ein\u00b7sam", ",", "wie", "Du", "zu\u00b7vor", "ge\u00b7than", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "ADJD", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Eh' Du den Weg gefunden, vertrauend uns zu nahn?", "tokens": ["Eh'", "Du", "den", "Weg", "ge\u00b7fun\u00b7den", ",", "ver\u00b7trau\u00b7end", "uns", "zu", "nahn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "VVPP", "PPER", "APPR", "ADJA", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Hast Du den Weg erkoren, folg' ihm, wohin er f\u00fchrt,", "tokens": ["Hast", "Du", "den", "Weg", "er\u00b7ko\u00b7ren", ",", "fol\u00b7g'", "ihm", ",", "wo\u00b7hin", "er", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$,", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Ist keiner doch verloren, der sich nicht selbst verliert!\u00ab", "tokens": ["Ist", "kei\u00b7ner", "doch", "ver\u00b7lo\u00b7ren", ",", "der", "sich", "nicht", "selbst", "ver\u00b7liert", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PIS", "ADV", "VVPP", "$,", "PRELS", "PRF", "PTKNEG", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbdie Freunde h\u00f6r' ich sagen, Du wollest b\u00fcssen nun;", "tokens": ["\u00bb", "die", "Freun\u00b7de", "h\u00f6r'", "ich", "sa\u00b7gen", ",", "Du", "wol\u00b7lest", "b\u00fcs\u00b7sen", "nun", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,", "PPER", "VMFIN", "VVFIN", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dich reuen Deine S\u00fcnden, Dich reu' Dein Mannesthun!", "tokens": ["Dich", "reu\u00b7en", "Dei\u00b7ne", "S\u00fcn\u00b7den", ",", "Dich", "reu'", "Dein", "Man\u00b7nest\u00b7hun", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Du wollest Dich bekehren, zu Deiner Seelen Nutz,", "tokens": ["Du", "wol\u00b7lest", "Dich", "be\u00b7keh\u00b7ren", ",", "zu", "Dei\u00b7ner", "See\u00b7len", "Nutz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dir eine Glazze scheeren, und anthun die Kapuzz?\u00ab", "tokens": ["Dir", "ei\u00b7ne", "Glaz\u00b7ze", "schee\u00b7ren", ",", "und", "an\u00b7thun", "die", "Ka\u00b7puzz", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$,", "KON", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+---+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Hohn ist ein giftger Stachel, Hohn ist ein Schlangenzahn,", "tokens": ["Hohn", "ist", "ein", "gift\u00b7ger", "Sta\u00b7chel", ",", "Hohn", "ist", "ein", "Schlan\u00b7gen\u00b7zahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der f\u00e4llt mit Natterbissen den Stolz des Mannes an.", "tokens": ["Der", "f\u00e4llt", "mit", "Nat\u00b7ter\u00b7bis\u00b7sen", "den", "Stolz", "des", "Man\u00b7nes", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbschweig, Schlange, schweig!\u00ab so t\u00f6net zum Geist sein Donnerwort;", "tokens": ["\u00bb", "schweig", ",", "Schlan\u00b7ge", ",", "schweig", "!", "\u00ab", "so", "t\u00f6\u00b7net", "zum", "Geist", "sein", "Don\u00b7ner\u00b7wort", ";"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "NN", "$,", "VVFIN", "$.", "$(", "ADV", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er st\u00fcrmt, in sich zerfallen, zu neuen Freuden fort.", "tokens": ["Er", "st\u00fcrmt", ",", "in", "sich", "zer\u00b7fal\u00b7len", ",", "zu", "neu\u00b7en", "Freu\u00b7den", "fort", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PRF", "VVINF", "$,", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Die Freunde bieten Becher, zu kosten ihren Trank,", "tokens": ["Die", "Freun\u00b7de", "bie\u00b7ten", "Be\u00b7cher", ",", "zu", "kos\u00b7ten", "ih\u00b7ren", "Trank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PTKZU", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch vom verw\u00f6hnten Zecher wird nicht der kleinste Dank.", "tokens": ["Doch", "vom", "ver\u00b7w\u00f6hn\u00b7ten", "Ze\u00b7cher", "wird", "nicht", "der", "kleins\u00b7te", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VAFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wer aller L\u00fcste Trinkhorn bis auf die Neige leert,", "tokens": ["Wer", "al\u00b7ler", "L\u00fcs\u00b7te", "Trink\u00b7horn", "bis", "auf", "die", "Nei\u00b7ge", "leert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "ADJA", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "Den mag nicht leicht erfreuen, was Andern preisenswerth.", "tokens": ["Den", "mag", "nicht", "leicht", "er\u00b7freu\u00b7en", ",", "was", "An\u00b7dern", "prei\u00b7sens\u00b7werth", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$,", "PWS", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Und Faustus spricht: \u00bbIhr Lieben, die wahre Fassnacht h\u00e4lt", "tokens": ["Und", "Faus\u00b7tus", "spricht", ":", "\u00bb", "Ihr", "Lie\u00b7ben", ",", "die", "wah\u00b7re", "Fass\u00b7nacht", "h\u00e4lt"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "$.", "$(", "PPOSAT", "ADJA", "$,", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Kein \u00e4chter Fassnachtschw\u00e4rmer im Reich der Oberwelt!", "tokens": ["Kein", "\u00e4ch\u00b7ter", "Fass\u00b7nacht\u00b7schw\u00e4r\u00b7mer", "im", "Reich", "der", "O\u00b7ber\u00b7welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er steigt ins Reich der Tiefen, er giebt Besuch der Nacht;", "tokens": ["Er", "steigt", "ins", "Reich", "der", "Tie\u00b7fen", ",", "er", "giebt", "Be\u00b7such", "der", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wohlauf, es sei auf F\u00e4ssern die Fassnacht zugebracht!\u00ab", "tokens": ["Wohl\u00b7auf", ",", "es", "sei", "auf", "F\u00e4s\u00b7sern", "die", "Fass\u00b7nacht", "zu\u00b7ge\u00b7bracht", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "APPR", "NN", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Er schlingt den Zaubermantel um die Genossen jetzt,", "tokens": ["Er", "schlingt", "den", "Zau\u00b7ber\u00b7man\u00b7tel", "um", "die", "Ge\u00b7nos\u00b7sen", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da sehn sie sich nach Salzburg gedankenschnell versetzt.", "tokens": ["Da", "sehn", "sie", "sich", "nach", "Salz\u00b7burg", "ge\u00b7dan\u00b7ken\u00b7schnell", "ver\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NE", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dort in des Bischofs Keller ruht mancher Labequell,", "tokens": ["Dort", "in", "des", "Bi\u00b7schofs", "Kel\u00b7ler", "ruht", "man\u00b7cher", "La\u00b7be\u00b7quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und sprudelt aus den F\u00e4ssern in alle Becher hell.", "tokens": ["Und", "spru\u00b7delt", "aus", "den", "F\u00e4s\u00b7sern", "in", "al\u00b7le", "Be\u00b7cher", "hell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Und wie recht frohen Muthes die Zechgesellschaft sitzt,", "tokens": ["Und", "wie", "recht", "fro\u00b7hen", "Mu\u00b7thes", "die", "Zech\u00b7ge\u00b7sell\u00b7schaft", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und mit des Weines Feuer ihr wildes Blut erhitzt,", "tokens": ["Und", "mit", "des", "Wei\u00b7nes", "Feu\u00b7er", "ihr", "wil\u00b7des", "Blut", "er\u00b7hitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da steigt der Kellermeister herab \u2013 wie der erschrickt,", "tokens": ["Da", "steigt", "der", "Kel\u00b7ler\u00b7meis\u00b7ter", "her\u00b7ab", "\u2013", "wie", "der", "er\u00b7schrickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$(", "KOKOM", "ART", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Als er die Schaar der G\u00e4ste, die fremde, hier erblickt!", "tokens": ["Als", "er", "die", "Schaar", "der", "G\u00e4s\u00b7te", ",", "die", "frem\u00b7de", ",", "hier", "er\u00b7blickt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Laut ruft er: \u00bbDiebe! Diebe!\u00ab dass das Gew\u00f6lb' erschallt;", "tokens": ["Laut", "ruft", "er", ":", "\u00bb", "Die\u00b7be", "!", "Die\u00b7be", "!", "\u00ab", "dass", "das", "Ge\u00b7w\u00f6lb'", "er\u00b7schallt", ";"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "$.", "$(", "NN", "$.", "NN", "$.", "$(", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da f\u00fchlt er sich gehalten von m\u00e4chtiger Gewalt.", "tokens": ["Da", "f\u00fchlt", "er", "sich", "ge\u00b7hal\u00b7ten", "von", "m\u00e4ch\u00b7ti\u00b7ger", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Auf einer Leiter Sprossen wird er gesetzt in Eil", "tokens": ["Auf", "ei\u00b7ner", "Lei\u00b7ter", "Spros\u00b7sen", "wird", "er", "ge\u00b7setzt", "in", "Eil"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VAFIN", "PPER", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und durch die L\u00fcfte sausend hin fliegt er, wie ein Pfeil.", "tokens": ["Und", "durch", "die", "L\u00fcf\u00b7te", "sau\u00b7send", "hin", "fliegt", "er", ",", "wie", "ein", "Pfeil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "In eines Waldes Mitten auf hohem Tannenbaum", "tokens": ["In", "ei\u00b7nes", "Wal\u00b7des", "Mit\u00b7ten", "auf", "ho\u00b7hem", "Tan\u00b7nen\u00b7baum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "L\u00e4sst Faust den Kellner sitzen, dem d\u00fcnkt's ein b\u00f6ser Traum.", "tokens": ["L\u00e4sst", "Faust", "den", "Kell\u00b7ner", "sit\u00b7zen", ",", "dem", "d\u00fcnkt's", "ein", "b\u00f6\u00b7ser", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "VVINF", "$,", "PRELS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nur dass der Traum nicht schwindet, nur dass der Tr\u00e4umer wach,", "tokens": ["Nur", "dass", "der", "Traum", "nicht", "schwin\u00b7det", ",", "nur", "dass", "der", "Tr\u00e4u\u00b7mer", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "PTKNEG", "VVFIN", "$,", "ADV", "KOUS", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und fr\u00fchen Wandrern k\u00fcndet er laut sein Ungemach. \u2013", "tokens": ["Und", "fr\u00fc\u00b7hen", "Wand\u00b7rern", "k\u00fcn\u00b7det", "er", "laut", "sein", "Un\u00b7ge\u00b7mach", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "Und mit den Freunden fr\u00f6hlich h\u00e4lt lustge Fassnacht Faust.", "tokens": ["Und", "mit", "den", "Freun\u00b7den", "fr\u00f6h\u00b7lich", "h\u00e4lt", "lust\u00b7ge", "Fass\u00b7nacht", "Faust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "VVFIN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Oft scheint ein Lebensfroher, der wild durchs Leben braust;", "tokens": ["Oft", "scheint", "ein", "Le\u00b7bens\u00b7fro\u00b7her", ",", "der", "wild", "durchs", "Le\u00b7ben", "braust", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Doch tobt gar mancher Zecher, und Lebens\u00fcberdruss", "tokens": ["Doch", "tobt", "gar", "man\u00b7cher", "Ze\u00b7cher", ",", "und", "Le\u00b7bens\u00b7\u00fc\u00b7berd\u00b7russ"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "NN", "$,", "KON", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Beut ihm den Freudenbecher als b\u00f6ser Genius.", "tokens": ["Beut", "ihm", "den", "Freu\u00b7den\u00b7be\u00b7cher", "als", "b\u00f6\u00b7ser", "Ge\u00b7nius", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KOUS", "ADJA", "NN", "$."], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}}, "stanza.14": {"line.1": {"text": "Ein Fasching ist das Leben, ein bunter Carneval,", "tokens": ["Ein", "Fa\u00b7sching", "ist", "das", "Le\u00b7ben", ",", "ein", "bun\u00b7ter", "Car\u00b7ne\u00b7val", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ein Tummelplatz der Freuden, ein Maskenzug zumal.", "tokens": ["Ein", "Tum\u00b7mel\u00b7platz", "der", "Freu\u00b7den", ",", "ein", "Mas\u00b7ken\u00b7zug", "zu\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Bis Leid wie Lust verrauscht sind, bis jede Larve sinkt,", "tokens": ["Bis", "Leid", "wie", "Lust", "ver\u00b7rauscht", "sind", ",", "bis", "je\u00b7de", "Lar\u00b7ve", "sinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "w