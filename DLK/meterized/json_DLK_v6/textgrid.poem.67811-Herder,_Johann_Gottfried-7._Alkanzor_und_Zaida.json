{"textgrid.poem.67811": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "7. Alkanzor und Zaida", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00e4uselnd wehn die Abendwinde,", "tokens": ["S\u00e4u\u00b7selnd", "wehn", "die", "Ab\u00b7end\u00b7win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00e4uselnd f\u00e4llet k\u00fchler Thau,", "tokens": ["S\u00e4u\u00b7selnd", "f\u00e4l\u00b7let", "k\u00fch\u00b7ler", "Thau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und schon kommt der Mohr Alkanzor", "tokens": ["Und", "schon", "kommt", "der", "Mohr", "Al\u00b7kan\u00b7zor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "NE"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Lichtscheu dort auf dunkler Au.", "tokens": ["Licht\u00b7scheu", "dort", "auf", "dunk\u00b7ler", "Au", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In dem Pallast wohnet Zaida,", "tokens": ["In", "dem", "Pal\u00b7last", "woh\u00b7net", "Zai\u00b7da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die so treu, er sich erkohr,", "tokens": ["Die", "so", "treu", ",", "er", "sich", "er\u00b7kohr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "$,", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie, die sch\u00f6nste junge Mohrin,", "tokens": ["Sie", ",", "die", "sch\u00f6ns\u00b7te", "jun\u00b7ge", "Moh\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er, ein edler junger Mohr.", "tokens": ["Er", ",", "ein", "ed\u00b7ler", "jun\u00b7ger", "Mohr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sehnlich harrt er nun der Stunde,", "tokens": ["Sehn\u00b7lich", "harrt", "er", "nun", "der", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die sie, ihn zu sehn, versprach,", "tokens": ["Die", "sie", ",", "ihn", "zu", "sehn", ",", "ver\u00b7sprach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wanket hin und her; nun steht er,", "tokens": ["Wan\u00b7ket", "hin", "und", "her", ";", "nun", "steht", "er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Horchet, schleichet, lauschet nach.", "tokens": ["Hor\u00b7chet", ",", "schlei\u00b7chet", ",", "lau\u00b7schet", "nach", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Furcht und Hoffen fa\u00dft ihn wechselnd,", "tokens": ["Furcht", "und", "Hof\u00b7fen", "fa\u00dft", "ihn", "wech\u00b7selnd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seufzet tief. \u2013 O tritt herf\u00fcr,", "tokens": ["Seuf\u00b7zet", "tief", ".", "\u2013", "O", "tritt", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$.", "$(", "NE", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Guter J\u00fcngling, sieh, am Fenster,", "tokens": ["Gu\u00b7ter", "J\u00fcng\u00b7ling", ",", "sieh", ",", "am", "Fens\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort erscheint dein M\u00e4dchen dir.", "tokens": ["Dort", "er\u00b7scheint", "dein", "M\u00e4d\u00b7chen", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Lieblich auf geht Mondes-Schimmer", "tokens": ["Lieb\u00b7lich", "auf", "geht", "Mon\u00b7des\u00b7Schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem verirrten Sch\u00e4fersmann,", "tokens": ["Dem", "ver\u00b7irr\u00b7ten", "Sch\u00e4\u00b7fers\u00b7mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn wie Silberglanz es aufsteigt", "tokens": ["Wenn", "wie", "Sil\u00b7ber\u00b7glanz", "es", "auf\u00b7steigt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "NN", "PPER", "VVPP"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Berg und Thale g\u00fcldend an.", "tokens": ["Berg", "und", "Tha\u00b7le", "g\u00fcl\u00b7dend", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Lieblich lacht die Pracht der Sonne", "tokens": ["Lieb\u00b7lich", "lacht", "die", "Pracht", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den verzagten Seemann an,", "tokens": ["Den", "ver\u00b7zag\u00b7ten", "See\u00b7mann", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sie grausen Sturm zertreibend", "tokens": ["Wenn", "sie", "grau\u00b7sen", "Sturm", "zer\u00b7trei\u00b7bend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00e4ttet auf der Wogen Bahn.", "tokens": ["Gl\u00e4t\u00b7tet", "auf", "der", "Wo\u00b7gen", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aber tausendmal so lieblich", "tokens": ["A\u00b7ber", "tau\u00b7send\u00b7mal", "so", "lieb\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stielt dem Liebelauscher hier", "tokens": ["Stielt", "dem", "Lie\u00b7be\u00b7lau\u00b7scher", "hier"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Halbgesehn das sch\u00f6ne M\u00e4dchen", "tokens": ["Halb\u00b7ge\u00b7sehn", "das", "sch\u00f6\u00b7ne", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die D\u00e4mmrung sich herf\u00fcr.", "tokens": ["Durch", "die", "D\u00e4mm\u00b7rung", "sich", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Auf den Zehn steht er beklommen,", "tokens": ["Auf", "den", "Zehn", "steht", "er", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Fl\u00fcstert Seufzer sanft ihr zu:", "tokens": ["Fl\u00fcs\u00b7tert", "Seuf\u00b7zer", "sanft", "ihr", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bballa mit dir, liebstes M\u00e4dchen!", "tokens": ["\u00bb", "al\u00b7la", "mit", "dir", ",", "liebs\u00b7tes", "M\u00e4d\u00b7chen", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gibst du Tod mir oder Ruh?", "tokens": ["Gibst", "du", "Tod", "mir", "o\u00b7der", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PPER", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ist sie wahr, die Schreckgeschichte,", "tokens": ["Ist", "sie", "wahr", ",", "die", "Schreck\u00b7ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mein Knabe jezt erf\u00e4hrt,", "tokens": ["Die", "mein", "Kna\u00b7be", "jezt", "er\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df man einem alten kargen", "tokens": ["Da\u00df", "man", "ei\u00b7nem", "al\u00b7ten", "kar\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reichen dich zur Braut gew\u00e4hrt?", "tokens": ["Rei\u00b7chen", "dich", "zur", "Braut", "ge\u00b7w\u00e4hrt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Da\u00df ihn jezt dein grimmer Vater", "tokens": ["Da\u00df", "ihn", "jezt", "dein", "grim\u00b7mer", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bringt von Antiquera schon,", "tokens": ["Bringt", "von", "An\u00b7ti\u00b7que\u00b7ra", "schon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist, o untreu' falsche Zaida,", "tokens": ["Ist", ",", "o", "un\u00b7treu'", "fal\u00b7sche", "Zai\u00b7da", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "FM", "FM", "FM", "FM", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist das meiner Liebe Lohn?", "tokens": ["Ist", "das", "mei\u00b7ner", "Lie\u00b7be", "Lohn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ist es wahr, so sprich mirs immer,", "tokens": ["Ist", "es", "wahr", ",", "so", "sprich", "mirs", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "ADV", "ADJD", "NE", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00e4usche l\u00e4nger nicht mein Ach,", "tokens": ["T\u00e4u\u00b7sche", "l\u00e4n\u00b7ger", "nicht", "mein", "Ach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schweige mir nicht, was ja jeder", "tokens": ["Schwei\u00b7ge", "mir", "nicht", ",", "was", "ja", "je\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "ADV", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00df und andern lispelt nach!\u00ab", "tokens": ["Wei\u00df", "und", "an\u00b7dern", "lis\u00b7pelt", "nach", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "KON", "PIS", "VVFIN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Tief erseufzt das schuld'ge M\u00e4dchen,", "tokens": ["Tief", "er\u00b7seufzt", "das", "schuld'\u00b7ge", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Thr\u00e4nen str\u00f6men sanft ihr ab:", "tokens": ["Thr\u00e4\u00b7nen", "str\u00f6\u00b7men", "sanft", "ihr", "ab", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbleider wahr, zu wahr, mein Lieber;", "tokens": ["\u00bb", "lei\u00b7der", "wahr", ",", "zu", "wahr", ",", "mein", "Lie\u00b7ber", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "$,", "PTKA", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier ist unsrer Liebe Grab!", "tokens": ["Hier", "ist", "uns\u00b7rer", "Lie\u00b7be", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Unsre Freundschaft ist verrathen,", "tokens": ["Uns\u00b7re", "Freund\u00b7schaft", "ist", "ver\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unser Bund ist schon bekannt;", "tokens": ["Un\u00b7ser", "Bund", "ist", "schon", "be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle meine Freunde w\u00fcthen,", "tokens": ["Al\u00b7le", "mei\u00b7ne", "Freun\u00b7de", "w\u00fct\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "All das Haus ist Sturm und Brand.", "tokens": ["All", "das", "Haus", "ist", "Sturm", "und", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Drohen, Schelten, Fluch ist um mich,", "tokens": ["Dro\u00b7hen", ",", "Schel\u00b7ten", ",", "Fluch", "ist", "um", "mich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "NN", "VAFIN", "APPR", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Vaters Strenge bricht mein Herz.", "tokens": ["Va\u00b7ters", "Stren\u00b7ge", "bricht", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich mu\u00df fort, o edler J\u00fcngling,", "tokens": ["Ich", "mu\u00df", "fort", ",", "o", "ed\u00b7ler", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKVZ", "$,", "FM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alla wei\u00df mit welchem Schmerz!", "tokens": ["Al\u00b7la", "wei\u00df", "mit", "wel\u00b7chem", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PWAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Alte Feindes Wunden trennten", "tokens": ["Al\u00b7te", "Fein\u00b7des", "Wun\u00b7den", "trenn\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lange dein und unser Haus;", "tokens": ["Lan\u00b7ge", "dein", "und", "un\u00b7ser", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie denn, da\u00df dein' edle Tugend", "tokens": ["Wie", "denn", ",", "da\u00df", "dein'", "ed\u00b7le", "Tu\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Allen Ha\u00df mir l\u00f6schte aus.", "tokens": ["Al\u00b7len", "Ha\u00df", "mir", "l\u00f6schte", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.16": {"line.1": {"text": "Wohl ach! wei\u00dft du, wie ich z\u00e4rtlich,", "tokens": ["Wohl", "ach", "!", "wei\u00dft", "du", ",", "wie", "ich", "z\u00e4rt\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frei von jener Stolz und Groll,", "tokens": ["Frei", "von", "je\u00b7ner", "Stolz", "und", "Groll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebte dich, ob ich vom Vater", "tokens": ["Lieb\u00b7te", "dich", ",", "ob", "ich", "vom", "Va\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleich dich nimmer hofte wohl.", "tokens": ["Gleich", "dich", "nim\u00b7mer", "hof\u00b7te", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Wohl ach! wei\u00dft du, wie so grausam", "tokens": ["Wohl", "ach", "!", "wei\u00dft", "du", ",", "wie", "so", "grau\u00b7sam"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$.", "VVFIN", "PPER", "$,", "PWAV", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Mutter mir verfuhr,", "tokens": ["Mei\u00b7ne", "Mut\u00b7ter", "mir", "ver\u00b7fuhr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ich ausstand, dich zu sehen", "tokens": ["Was", "ich", "aus\u00b7stand", ",", "dich", "zu", "se\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Abend und Fr\u00fchmorgens nur.", "tokens": ["A\u00b7bend", "und", "Fr\u00fch\u00b7mor\u00b7gens", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.18": {"line.1": {"text": "L\u00e4nger kann ich nun nicht streiten;", "tokens": ["L\u00e4n\u00b7ger", "kann", "ich", "nun", "nicht", "strei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle zwingen sie mir ab", "tokens": ["Al\u00b7le", "zwin\u00b7gen", "sie", "mir", "ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese schwache Hand, und morgen", "tokens": ["Die\u00b7se", "schwa\u00b7che", "Hand", ",", "und", "mor\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "$,", "KON", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df ich in mein Ehegrab.", "tokens": ["Mu\u00df", "ich", "in", "mein", "E\u00b7he\u00b7grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Aber denke nicht, da\u00df deine", "tokens": ["A\u00b7ber", "den\u00b7ke", "nicht", ",", "da\u00df", "dei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Treue Zaida das verlebt.", "tokens": ["Treu\u00b7e", "Zai\u00b7da", "das", "ver\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PDS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! schon sagt mein brechend Herz mir", "tokens": ["Ach", "!", "schon", "sagt", "mein", "bre\u00b7chend", "Herz", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPOSAT", "ADJD", "NN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df es nicht mehr lange bebt.", "tokens": ["Da\u00df", "es", "nicht", "mehr", "lan\u00b7ge", "bebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Lebe wohl denn, s\u00fcsser J\u00fcngling,", "tokens": ["Le\u00b7be", "wohl", "denn", ",", "s\u00fcs\u00b7ser", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu sehr leb' ich nur um dich!", "tokens": ["Zu", "sehr", "leb'", "ich", "nur", "um", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese Sch\u00e4rp', ein Abschiedszeichen,", "tokens": ["Die\u00b7se", "Sch\u00e4r\u00b7p'", ",", "ein", "Ab\u00b7schieds\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wenn du's tr\u00e4gest, denk an mich!", "tokens": ["Wenn", "du's", "tr\u00e4\u00b7gest", ",", "denk", "an", "mich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "PRELS", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Bald, Geliebter, wird ein werther", "tokens": ["Bald", ",", "Ge\u00b7lieb\u00b7ter", ",", "wird", "ein", "wert\u00b7her"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "VAFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00e4dchen lohnen deine Treu;", "tokens": ["M\u00e4d\u00b7chen", "loh\u00b7nen", "dei\u00b7ne", "Treu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sag ihr denn, da\u00df deine Zaida", "tokens": ["Sag", "ihr", "denn", ",", "da\u00df", "dei\u00b7ne", "Zai\u00b7da"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um dich fr\u00fch gestorben sey!\u00ab", "tokens": ["Um", "dich", "fr\u00fch", "ge\u00b7stor\u00b7ben", "sey", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "PRF", "ADJD", "VVPP", "VAFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "So bet\u00e4ubt, verworren go\u00df sie", "tokens": ["So", "be\u00b7t\u00e4ubt", ",", "ver\u00b7wor\u00b7ren", "go\u00df", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "ADJD", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus vor ihm der Liebe Schmerz.", "tokens": ["Aus", "vor", "ihm", "der", "Lie\u00b7be", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tief erseufzt er, rief: \u00bbO Zaida,", "tokens": ["Tief", "er\u00b7seufzt", "er", ",", "rief", ":", "\u00bb", "O", "Zai\u00b7da", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "VVFIN", "$.", "$(", "NE", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Brich, o brich nicht so mein Herz!", "tokens": ["Brich", ",", "o", "brich", "nicht", "so", "mein", "Herz", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "ADJD", "PTKNEG", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "Kanst du's denken, dich verlieren", "tokens": ["Kanst", "du's", "den\u00b7ken", ",", "dich", "ver\u00b7lie\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PIS", "VVINF", "$,", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll ich, und so seyn in Ruh?", "tokens": ["Soll", "ich", ",", "und", "so", "seyn", "in", "Ruh", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KON", "ADV", "VAINF", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieber todt zu tausendmalen,", "tokens": ["Lie\u00b7ber", "todt", "zu", "tau\u00b7send\u00b7ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Alte todt dazu!", "tokens": ["Und", "der", "Al\u00b7te", "todt", "da\u00b7zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.24": {"line.1": {"text": "Und kanst du dich denn so schimpflich", "tokens": ["Und", "kanst", "du", "dich", "denn", "so", "schimpf\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PRF", "ADV", "ADV", "ADJD"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihnen lassen? Fleuch zu mir!", "tokens": ["Ih\u00b7nen", "las\u00b7sen", "?", "Fleuch", "zu", "mir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses Herz soll f\u00fcr dich bluten,", "tokens": ["Die\u00b7ses", "Herz", "soll", "f\u00fcr", "dich", "blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VMFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser Arm soll dienen dir!\u00ab", "tokens": ["Die\u00b7ser", "Arm", "soll", "die\u00b7nen", "dir", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDAT", "NN", "VMFIN", "VVINF", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.25": {"line.1": {"text": "\u00bball umsonst, umsonst, Alkanzor,", "tokens": ["\u00bb", "all", "um\u00b7sonst", ",", "um\u00b7sonst", ",", "Al\u00b7kan\u00b7zor", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "PIAT", "ADV", "$,", "ADV", "$,", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Mauern, Wachen sind da vor,", "tokens": ["Mau\u00b7ern", ",", "Wa\u00b7chen", "sind", "da", "vor", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kaum erstahl ich diesen Blick noch,", "tokens": ["Kaum", "er\u00b7stahl", "ich", "die\u00b7sen", "Blick", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo mein M\u00e4dchen steht am Thor.", "tokens": ["Wo", "mein", "M\u00e4d\u00b7chen", "steht", "am", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.26": {"line.1": {"text": "Horch, ich h\u00f6r den Vater st\u00fcrmen,", "tokens": ["Horch", ",", "ich", "h\u00f6r", "den", "Va\u00b7ter", "st\u00fcr\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Horch, die Mutter tobt auf mich;", "tokens": ["Horch", ",", "die", "Mut\u00b7ter", "tobt", "auf", "mich", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich mu\u00df fort! Leb wohl auf ewig!", "tokens": ["Ich", "mu\u00df", "fort", "!", "Leb", "wohl", "auf", "e\u00b7wig", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKVZ", "$.", "VVIMP", "ADV", "APPR", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "G\u00fct'ger Alla leite dich!\u00ab \u2013", "tokens": ["G\u00fct'\u00b7ger", "Al\u00b7la", "lei\u00b7te", "dich", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}