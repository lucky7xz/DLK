{"dta.poem.19525": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Canal grande.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Eine gl\u00fcckgef\u00fcllte Gondel gleitet auf dem Canal grande,", "tokens": ["Ei\u00b7ne", "gl\u00fcck\u00b7ge\u00b7f\u00fcll\u00b7te", "Gon\u00b7del", "glei\u00b7tet", "auf", "dem", "Ca\u00b7nal", "gran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "ADJA", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "An Giorgione lehnt die Blonde mit dem rothen Sammtgewande.", "tokens": ["An", "Gi\u00b7or\u00b7gio\u00b7ne", "lehnt", "die", "Blon\u00b7de", "mit", "dem", "ro\u00b7then", "Sammt\u00b7ge\u00b7wan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.3": {"text": "\u201egiorgio, deiner Laute Saiten h\u00f6r' ich leise, leise klingen\u201c \u2014", "tokens": ["\u201e", "gi\u00b7or\u00b7gio", ",", "dei\u00b7ner", "Lau\u00b7te", "Sai\u00b7ten", "h\u00f6r'", "ich", "lei\u00b7se", ",", "lei\u00b7se", "klin\u00b7gen", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "$,", "PPOSAT", "NN", "NN", "VVFIN", "PPER", "ADJD", "$,", "ADJD", "VVINF", "$(", "$("], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.4": {"text": "\u201ejulia Vendramin, Erlauchte, was befiehlst du mir zu singen?\u201c", "tokens": ["\u201e", "ju\u00b7lia", "Ven\u00b7dra\u00b7min", ",", "Er\u00b7lauch\u00b7te", ",", "was", "be\u00b7fiehlst", "du", "mir", "zu", "sin\u00b7gen", "?", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NE", "NE", "$,", "VVFIN", "$,", "PWS", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "\u201enichts von sch\u00f6nen Augen, Giorgio! Solches Thema sollst du", "tokens": ["\u201e", "nichts", "von", "sch\u00f6\u00b7nen", "Au\u00b7gen", ",", "Gi\u00b7or\u00b7gio", "!", "Sol\u00b7ches", "The\u00b7ma", "sollst", "du"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PIS", "APPR", "ADJA", "NN", "$,", "NE", "$.", "PIAT", "NN", "VMFIN", "PPER"], "meter": "+-+-+-+--+--++-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Singe, wie dem Meer entstiegen diese wunderbaren Gassen!", "tokens": ["Sin\u00b7ge", ",", "wie", "dem", "Meer", "ent\u00b7stie\u00b7gen", "die\u00b7se", "wun\u00b7der\u00b7ba\u00b7ren", "Gas\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "VVFIN", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Fessle kr\u00e4nzend keine Locken, die sich ringeln los und ledig!", "tokens": ["Fess\u00b7le", "kr\u00e4n\u00b7zend", "kei\u00b7ne", "Lo\u00b7cken", ",", "die", "sich", "rin\u00b7geln", "los", "und", "le\u00b7dig", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PIAT", "NN", "$,", "PRELS", "PRF", "VVFIN", "PTKVZ", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Giorgio, singe mir von meinem unvergleichlichen Venedig!\u201c", "tokens": ["Gi\u00b7or\u00b7gio", ",", "sin\u00b7ge", "mir", "von", "mei\u00b7nem", "un\u00b7ver\u00b7gleich\u00b7li\u00b7chen", "Ve\u00b7ne\u00b7dig", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NE", "$.", "$("], "meter": "-+-+-+-+-+-+--+--", "measure": "iambic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "\u201emeine s\u00fc\u00dfe Muse will es! Es geschieht!\u201c Er pr\u00e4ludierte.", "tokens": ["\u201e", "mei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Mu\u00b7se", "will", "es", "!", "Es", "ge\u00b7schieht", "!", "\u201c", "Er", "pr\u00e4\u00b7lu\u00b7dier\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "VMFIN", "PPER", "$.", "PPER", "VVFIN", "$.", "$(", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "\u201eweiland, eh' des heil'gen Marcus Flagge dieses Meer regierte,", "tokens": ["\u201e", "wei\u00b7land", ",", "eh'", "des", "heil'\u00b7gen", "Mar\u00b7cus", "Flag\u00b7ge", "die\u00b7ses", "Meer", "re\u00b7gier\u00b7te", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NE", "NN", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Dr\u00fcben dort, wo duftverschleiert Istriens sch\u00f6ne Berge blauen,", "tokens": ["Dr\u00fc\u00b7ben", "dort", ",", "wo", "duft\u00b7ver\u00b7schlei\u00b7ert", "I\u00b7stri\u00b7ens", "sch\u00f6\u00b7ne", "Ber\u00b7ge", "blau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "VVFIN", "NE", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Sank vor ungez\u00e4hlten Jahren eine D\u00e4mm'rung voller Grauen.", "tokens": ["Sank", "vor", "un\u00b7ge\u00b7z\u00e4hl\u00b7ten", "Jah\u00b7ren", "ei\u00b7ne", "D\u00e4m\u00b7m'\u00b7rung", "vol\u00b7ler", "Grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Durch das Dunkel huschen Larven, angstgeschreckte Hunde winseln,", "tokens": ["Durch", "das", "Dun\u00b7kel", "hu\u00b7schen", "Lar\u00b7ven", ",", "angst\u00b7ge\u00b7schreck\u00b7te", "Hun\u00b7de", "win\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Schreie gellen, Stimmen warnen: \u201eL\u00f6st die B\u00f6te! Nach den", "tokens": ["Schrei\u00b7e", "gel\u00b7len", ",", "Stim\u00b7men", "war\u00b7nen", ":", "\u201e", "L\u00f6st", "die", "B\u00f6\u00b7te", "!", "Nach", "den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVPP", "$,", "NN", "VVINF", "$.", "$(", "VVFIN", "ART", "NN", "$.", "APPR", "ART"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "In den L\u00fcften haucht ein Odem, wie es in den Gr\u00e4bern", "tokens": ["In", "den", "L\u00fcf\u00b7ten", "haucht", "ein", "O\u00b7dem", ",", "wie", "es", "in", "den", "Gr\u00e4\u00b7bern"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Schaurig tagen Meer und Himmel! Aquileja brennt und lodert!", "tokens": ["Schau\u00b7rig", "ta\u00b7gen", "Meer", "und", "Him\u00b7mel", "!", "A\u00b7qui\u00b7le\u00b7ja", "brennt", "und", "lo\u00b7dert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN", "$.", "NE", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+---+-+-", "measure": "unknown.measure.septa"}}, "stanza.5": {"line.1": {"text": "Von der St\u00e4tte, wo die stillen, ungez\u00e4hmten Flammen wogen,", "tokens": ["Von", "der", "St\u00e4t\u00b7te", ",", "wo", "die", "stil\u00b7len", ",", "un\u00b7ge\u00b7z\u00e4hm\u00b7ten", "Flam\u00b7men", "wo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Kommt ein dumpfes Menschenbrausen nach dem freien Strand", "tokens": ["Kommt", "ein", "dum\u00b7pfes", "Men\u00b7schen\u00b7brau\u00b7sen", "nach", "dem", "frei\u00b7en", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Attila, die Gottesgei\u00dfel, jagt auf blutbesprengten Pfaden", "tokens": ["At\u00b7ti\u00b7la", ",", "die", "Got\u00b7tes\u00b7gei\u00b7\u00dfel", ",", "jagt", "auf", "blut\u00b7be\u00b7spreng\u00b7ten", "Pfa\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Krieger mit zerbrochnen Schwertern, Fraun mit Sch\u00e4tzen schwer", "tokens": ["Krie\u00b7ger", "mit", "zer\u00b7broch\u00b7nen", "Schwer\u00b7tern", ",", "Fraun", "mit", "Sch\u00e4t\u00b7zen", "schwer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "NE", "APPR", "NN", "ADJD"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.6": {"line.1": {"text": "Wie zum Hades Schatten wandern, ziehn zum Meere die", "tokens": ["Wie", "zum", "Ha\u00b7des", "Schat\u00b7ten", "wan\u00b7dern", ",", "ziehn", "zum", "Mee\u00b7re", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "APPRART", "NN", "ART"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Das die purpurroth gef\u00e4rbten Wolken weit hinaus beleuchten,", "tokens": ["Das", "die", "pur\u00b7pur\u00b7roth", "ge\u00b7f\u00e4rb\u00b7ten", "Wol\u00b7ken", "weit", "hin\u00b7aus", "be\u00b7leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJD", "ADJA", "NN", "ADJD", "APZR", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wittwen, Waisen schreiten jammernd, schweigend st\u00fcrzen wunde", "tokens": ["Witt\u00b7wen", ",", "Wai\u00b7sen", "schrei\u00b7ten", "jam\u00b7mernd", ",", "schwei\u00b7gend", "st\u00fcr\u00b7zen", "wun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "VVFIN", "ADJD", "$,", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Mitten im Gew\u00fchle b\u00e4umen Wagen sich und scheue Renner.", "tokens": ["Mit\u00b7ten", "im", "Ge\u00b7w\u00fch\u00b7le", "b\u00e4u\u00b7men", "Wa\u00b7gen", "sich", "und", "scheu\u00b7e", "Ren\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADJA", "NN", "PRF", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Kniee wanken, F\u00fc\u00dfe gleiten, K\u00e4stchen brechen, draus die hellen", "tokens": ["Kni\u00b7ee", "wan\u00b7ken", ",", "F\u00fc\u00b7\u00dfe", "glei\u00b7ten", ",", "K\u00e4st\u00b7chen", "bre\u00b7chen", ",", "draus", "die", "hel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$,", "NN", "VVINF", "$,", "PAV", "ART", "ADJA"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Goldnen Reife rollend springen und die wei\u00dfen Perlen quellen.", "tokens": ["Gold\u00b7nen", "Rei\u00b7fe", "rol\u00b7lend", "sprin\u00b7gen", "und", "die", "wei\u00b7\u00dfen", "Per\u00b7len", "quel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVINF", "KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Nackte K\u00fcstenkinder starren gierig auf das rings zerstreute", "tokens": ["Nack\u00b7te", "K\u00fcs\u00b7ten\u00b7kin\u00b7der", "star\u00b7ren", "gie\u00b7rig", "auf", "das", "rings", "zer\u00b7streu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "APPR", "ART", "ADV", "VVFIN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Gold, und doch betastet's keines, \u2014 Etzel's ist die ganze Beute!", "tokens": ["Gold", ",", "und", "doch", "be\u00b7tas\u00b7tet's", "kei\u00b7nes", ",", "Et\u00b7zel's", "ist", "die", "gan\u00b7ze", "Beu\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADV", "VVFIN", "PIS", "$,", "$(", "NE", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Schiffer r\u00fcsten dunkle Nachen, dr\u00fcber Wogen sch\u00e4umend schlagen,", "tokens": ["Schif\u00b7fer", "r\u00fcs\u00b7ten", "dunk\u00b7le", "Na\u00b7chen", ",", "dr\u00fc\u00b7ber", "Wo\u00b7gen", "sch\u00e4u\u00b7mend", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "$,", "PAV", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Durch die wei\u00dfe Brandung werden bleiche Fraun an Bord", "tokens": ["Durch", "die", "wei\u00b7\u00dfe", "Bran\u00b7dung", "wer\u00b7den", "blei\u00b7che", "Fraun", "an", "Bord"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Mit der Rechten an die phryg'sche M\u00fctze langt der Meerplebejer,", "tokens": ["Mit", "der", "Rech\u00b7ten", "an", "die", "phry\u00b7g'\u00b7sche", "M\u00fct\u00b7ze", "langt", "der", "Meer\u00b7ple\u00b7be\u00b7jer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+--+-+-+--+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Beut zum Sprung ins Boot die Linke dem behelmten Aquilejer.", "tokens": ["Beut", "zum", "Sprung", "ins", "Boot", "die", "Lin\u00b7ke", "dem", "be\u00b7helm\u00b7ten", "A\u00b7qui\u00b7le\u00b7jer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "APPRART", "NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.9": {"line.1": {"text": "Schon entflieht ein Schiff mit weh'nden Segeln, flatternden", "tokens": ["Schon", "ent\u00b7flieht", "ein", "Schiff", "mit", "weh'n\u00b7den", "Se\u00b7geln", ",", "flat\u00b7tern\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,", "VVINF"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Drin sich weitgetrennte Loose sonder Wahl zusammenfanden,", "tokens": ["Drin", "sich", "weit\u00b7ge\u00b7trenn\u00b7te", "Loo\u00b7se", "son\u00b7der", "Wahl", "zu\u00b7sam\u00b7men\u00b7fan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVFIN", "NE", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Unbekannte H\u00e4nde dr\u00fccken sich in angstbeklommnem Traume,", "tokens": ["Un\u00b7be\u00b7kann\u00b7te", "H\u00e4n\u00b7de", "dr\u00fc\u00b7cken", "sich", "in", "angst\u00b7be\u00b7klomm\u00b7nem", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Aquileja's Ueberbleibsel schmiegen sich in engem Raume.", "tokens": ["A\u00b7qui\u00b7le\u00b7ja's", "Ue\u00b7ber\u00b7bleib\u00b7sel", "schmie\u00b7gen", "sich", "in", "en\u00b7gem", "Rau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PRF", "A