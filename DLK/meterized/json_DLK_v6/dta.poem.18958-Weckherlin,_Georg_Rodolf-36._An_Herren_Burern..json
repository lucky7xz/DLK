{"dta.poem.18958": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "36.  \n  An Herren Burern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Vjl nennen dich (h\u00f6r ich) Han\u00df Hurer/", "tokens": ["Vjl", "nen\u00b7nen", "dich", "(", "h\u00f6r", "ich", ")", "Han\u00df", "Hu\u00b7rer", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$(", "VVFIN", "PPER", "$(", "NE", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ich aber/ weil du nu alt/ Vhrer:", "tokens": ["Ich", "a\u00b7ber", "/", "weil", "du", "nu", "alt", "/", "Vhrer", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "$(", "KOUS", "PPER", "ADV", "ADJD", "$(", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch r\u00fchmet deine alte Hur", "tokens": ["Auch", "r\u00fch\u00b7met", "dei\u00b7ne", "al\u00b7te", "Hur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein angesicht jhr Sonnen vhr;", "tokens": ["Dein", "an\u00b7ge\u00b7sicht", "jhr", "Son\u00b7nen", "vhr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil dein mund seine Z\u00e4hn offt bl\u00f6cket/", "tokens": ["Weil", "dein", "mund", "sei\u00b7ne", "Z\u00e4hn", "offt", "bl\u00f6\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den dein Na\u00df Zaiger-gleich bed\u00f6cket.", "tokens": ["Den", "dein", "Na\u00df", "Zai\u00b7ger\u00b7gleich", "be\u00b7d\u00f6\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NE", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "So \u00f6ffnend deinen weitten mund", "tokens": ["So", "\u00f6ff\u00b7nend", "dei\u00b7nen", "weit\u00b7ten", "mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "(in dem acht oder sechs z\u00e4hn stehen)", "tokens": ["(", "in", "dem", "acht", "o\u00b7der", "sechs", "z\u00e4hn", "ste\u00b7hen", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "CARD", "KON", "CARD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Kan man gar leichtlich des tags stund", "tokens": ["Kan", "man", "gar", "leicht\u00b7lich", "des", "tags", "stund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ADV", "ADJD", "ART", "NN", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Bey deiner Nasen schatten sehen.", "tokens": ["Bey", "dei\u00b7ner", "Na\u00b7sen", "schat\u00b7ten", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}