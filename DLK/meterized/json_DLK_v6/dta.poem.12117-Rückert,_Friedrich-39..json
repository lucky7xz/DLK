{"dta.poem.12117": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edu sahst die Leute nur, gestehs, von einer Seite,", "tokens": ["\u201e", "du", "sahst", "die", "Leu\u00b7te", "nur", ",", "ge\u00b7stehs", ",", "von", "ei\u00b7ner", "Sei\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "ADV", "$,", "VVPP", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der guten; sieh genau, so zeigt sich bald die zweite.\u201c", "tokens": ["Der", "gu\u00b7ten", ";", "sieh", "ge\u00b7nau", ",", "so", "zeigt", "sich", "bald", "die", "zwei\u00b7te", ".", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$.", "VVFIN", "ADJD", "$,", "ADV", "VVFIN", "PRF", "ADV", "ART", "ADJA", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mag seyn! doch war ich froh, da\u00df sie die gute hatten;", "tokens": ["Mag", "seyn", "!", "doch", "war", "ich", "froh", ",", "da\u00df", "sie", "die", "gu\u00b7te", "hat\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VAINF", "$.", "ADV", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von selber freilich ist bei jedem Lichte Schatten.", "tokens": ["Von", "sel\u00b7ber", "frei\u00b7lich", "ist", "bei", "je\u00b7dem", "Lich\u00b7te", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "VAFIN", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch selber das beweist des Lichtes St\u00e4rke ja,", "tokens": ["Doch", "sel\u00b7ber", "das", "be\u00b7weist", "des", "Lich\u00b7tes", "St\u00e4r\u00b7ke", "ja", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PDS", "VVFIN", "ART", "ADJA", "NN", "PTKANT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich vor seinem Glanz die Schatten \u00fcbersah.", "tokens": ["Da\u00df", "ich", "vor", "sei\u00b7nem", "Glanz", "die", "Schat\u00b7ten", "\u00fc\u00b7ber\u00b7sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}