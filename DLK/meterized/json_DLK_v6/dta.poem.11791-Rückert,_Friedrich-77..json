{"dta.poem.11791": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "77.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Erde dankt man nicht den Schatz, den man gegraben,", "tokens": ["Der", "Er\u00b7de", "dankt", "man", "nicht", "den", "Schatz", ",", "den", "man", "ge\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "PTKNEG", "ART", "NN", "$,", "PRELS", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Reichen nicht, was wir ihm abgewonnen haben.", "tokens": ["Dem", "Rei\u00b7chen", "nicht", ",", "was", "wir", "ihm", "ab\u00b7ge\u00b7won\u00b7nen", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$,", "PRELS", "PPER", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Man dankt auch nicht dem Meer die Perlensaat am Strand,", "tokens": ["Man", "dankt", "auch", "nicht", "dem", "Meer", "die", "Per\u00b7len\u00b7saat", "am", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch der Freigebigkeit die Gab' aus ihrer Hand.", "tokens": ["Noch", "der", "Frei\u00b7ge\u00b7big\u00b7keit", "die", "Gab'", "aus", "ih\u00b7rer", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Dort wird sich mit der M\u00fch und Schwierigkeit entschuldigen", "tokens": ["Dort", "wird", "sich", "mit", "der", "M\u00fch", "und", "Schwie\u00b7rig\u00b7keit", "ent\u00b7schul\u00b7di\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "APPR", "ART", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Der Undank, leichter hier selbst mit der Huld des Huldigen.", "tokens": ["Der", "Un\u00b7dank", ",", "leich\u00b7ter", "hier", "selbst", "mit", "der", "Huld", "des", "Hul\u00b7di\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ADV", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Dort rechnet zum Verdienst er sichs, da\u00df dirs nicht roste;", "tokens": ["Dort", "rech\u00b7net", "zum", "Ver\u00b7dienst", "er", "sichs", ",", "da\u00df", "dirs", "nicht", "ros\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPER", "PIS", "$,", "KOUS", "PIS", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier gilt ihm wenig, was er sieht da\u00df nichts dir koste.", "tokens": ["Hier", "gilt", "ihm", "we\u00b7nig", ",", "was", "er", "sieht", "da\u00df", "nichts", "dir", "kos\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "VVFIN", "KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Drum rechne nie auf Dank, du magst nun deine Gaben", "tokens": ["Drum", "rech\u00b7ne", "nie", "auf", "Dank", ",", "du", "magst", "nun", "dei\u00b7ne", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "APPR", "NN", "$,", "PPER", "VMFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Meere gleich verstreun, der Erde gleich vergraben.", "tokens": ["Dem", "Mee\u00b7re", "gleich", "ver\u00b7streun", ",", "der", "Er\u00b7de", "gleich", "ver\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch freue dich, zu sehn, da\u00df sich der Finder freut,", "tokens": ["Doch", "freu\u00b7e", "dich", ",", "zu", "sehn", ",", "da\u00df", "sich", "der", "Fin\u00b7der", "freut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PTKZU", "VVINF", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du habest aufgespart nun oder ausgestreut.", "tokens": ["Du", "ha\u00b7best", "auf\u00b7ge\u00b7spart", "nun", "o\u00b7der", "aus\u00b7ge\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}