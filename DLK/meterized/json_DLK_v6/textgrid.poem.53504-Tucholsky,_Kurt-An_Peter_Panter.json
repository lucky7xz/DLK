{"textgrid.poem.53504": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "An Peter Panter", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Peter Panter, Mitarbeiter!", "tokens": ["Pe\u00b7ter", "Pan\u00b7ter", ",", "Mit\u00b7ar\u00b7bei\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steig doch auf die hohe Leiter!", "tokens": ["Steig", "doch", "auf", "die", "ho\u00b7he", "Lei\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Singe doch von aktuellen", "tokens": ["Sin\u00b7ge", "doch", "von", "ak\u00b7tu\u00b7el\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeitgenossenzwischenf\u00e4llen!", "tokens": ["Zeit\u00b7ge\u00b7nos\u00b7senz\u00b7wi\u00b7schen\u00b7f\u00e4l\u00b7len", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df die Liebe, la\u00df die Damen", "tokens": ["La\u00df", "die", "Lie\u00b7be", ",", "la\u00df", "die", "Da\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "$,", "VVIMP", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "mit den freundlich blonden Namen;", "tokens": ["mit", "den", "freund\u00b7lich", "blon\u00b7den", "Na\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "la\u00df die bunten Busent\u00fccher \u2013", "tokens": ["la\u00df", "die", "bun\u00b7ten", "Bu\u00b7sen\u00b7t\u00fc\u00b7cher", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und vor allem: la\u00df die B\u00fccher!", "tokens": ["und", "vor", "al\u00b7lem", ":", "la\u00df", "die", "B\u00fc\u00b7cher", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "$.", "VVIMP", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df sie B\u00fccher schreiben, drucken \u2013", "tokens": ["La\u00df", "sie", "B\u00fc\u00b7cher", "schrei\u00b7ben", ",", "dru\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "VVINF", "$,", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wozu da hinuntergucken!", "tokens": ["wo\u00b7zu", "da", "hin\u00b7un\u00b7ter\u00b7gu\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Frisch! hinein ins volle Leben!", "tokens": ["Frisch", "!", "hin\u00b7ein", "ins", "vol\u00b7le", "Le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aktuell mu\u00dft du dich geben!", "tokens": ["Ak\u00b7tu\u00b7ell", "mu\u00dft", "du", "dich", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sieh mich an! Fast jede Woche", "tokens": ["Sieh", "mich", "an", "!", "Fast", "je\u00b7de", "Wo\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKVZ", "$.", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "pfeif ich auf dem Fl\u00f6tenloche:", "tokens": ["pfeif", "ich", "auf", "dem", "Fl\u00f6\u00b7ten\u00b7lo\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Reichstag, Wahlrecht, Osten, Westen,", "tokens": ["Reichs\u00b7tag", ",", "Wahl\u00b7recht", ",", "Os\u00b7ten", ",", "Wes\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Presse, Orden, Schweinem\u00e4sten \u2013!", "tokens": ["Pres\u00b7se", ",", "Or\u00b7den", ",", "Schwei\u00b7ne\u00b7m\u00e4s\u00b7ten", "\u2013", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$(", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Tanz die nationale Runde!", "tokens": ["Tanz", "die", "na\u00b7ti\u00b7o\u00b7na\u00b7le", "Run\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Kennst du das Gebot der Stunde?", "tokens": ["Kennst", "du", "das", "Ge\u00b7bot", "der", "Stun\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6cker macht das viel gewandter,", "tokens": ["H\u00f6\u00b7cker", "macht", "das", "viel", "ge\u00b7wand\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "PIAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Peter Panter, Peter Panter!", "tokens": ["Pe\u00b7ter", "Pan\u00b7ter", ",", "Pe\u00b7ter", "Pan\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Du mu\u00dft aktueller schw\u00e4tzen,", "tokens": ["Du", "mu\u00dft", "ak\u00b7tu\u00b7el\u00b7ler", "schw\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "und man wird dich h\u00f6her sch\u00e4tzen!", "tokens": ["und", "man", "wird", "dich", "h\u00f6\u00b7her", "sch\u00e4t\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lerne du im Hurraschrein:", "tokens": ["Ler\u00b7ne", "du", "im", "Hur\u00b7rasc\u00b7hrein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "man darf nicht beschaulich sein.", "tokens": ["man", "darf", "nicht", "be\u00b7schau\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}