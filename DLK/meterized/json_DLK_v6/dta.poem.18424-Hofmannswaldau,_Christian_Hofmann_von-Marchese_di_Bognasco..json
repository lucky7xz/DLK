{"dta.poem.18424": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Marchese di Bognasco.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kan dein Montmelian der feind gleich itzt besitzen/", "tokens": ["Kan", "dein", "Mont\u00b7me\u00b7li\u00b7an", "der", "feind", "gleich", "itzt", "be\u00b7sit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ART", "NN", "ADV", "ADV", "VVINF", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "So hat dein heldenmuth doch keine schuld daran;", "tokens": ["So", "hat", "dein", "hel\u00b7den\u00b7muth", "doch", "kei\u00b7ne", "schuld", "da\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "PIAT", "ADJD", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auch Hector konte selbst sein Troja nicht besch\u00fctzen/", "tokens": ["Auch", "Hec\u00b7tor", "kon\u00b7te", "selbst", "sein", "Tro\u00b7ja", "nicht", "be\u00b7sch\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VMFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wiewohl du hier viel mehr als Hector hast gethan.", "tokens": ["Wie\u00b7wohl", "du", "hier", "viel", "mehr", "als", "Hec\u00b7tor", "hast", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PIS", "KOKOM", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}