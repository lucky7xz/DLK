{"textgrid.poem.36065": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "XxVI. Alexander Magnus.", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sch\u00f6n ist's doch, den M\u00e4chtgen vertraut zur Seite stehn!", "tokens": ["Wie", "sch\u00f6n", "ist's", "doch", ",", "den", "M\u00e4cht\u00b7gen", "ver\u00b7traut", "zur", "Sei\u00b7te", "stehn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "$,", "ART", "NN", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Auf edle V\u00f6lkerhirten viel Augen freudig sehn.", "tokens": ["Auf", "ed\u00b7le", "V\u00f6l\u00b7ker\u00b7hir\u00b7ten", "viel", "Au\u00b7gen", "freu\u00b7dig", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PIAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wer ihrer Liebe theilhaft, wie wird er hoch geehrt,", "tokens": ["Wer", "ih\u00b7rer", "Lie\u00b7be", "theil\u00b7haft", ",", "wie", "wird", "er", "hoch", "ge\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADJD", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nur zeige der Planet sich stets seiner Sonne werth.", "tokens": ["Nur", "zei\u00b7ge", "der", "Pla\u00b7net", "sich", "stets", "sei\u00b7ner", "Son\u00b7ne", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein Herrscher der mit W\u00fcrde der Hoheit Scepter f\u00fchrt,", "tokens": ["Ein", "Herr\u00b7scher", "der", "mit", "W\u00fcr\u00b7de", "der", "Ho\u00b7heit", "Scep\u00b7ter", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der mit dem Arm der Weisheit des Landes Steur' regiert,", "tokens": ["Der", "mit", "dem", "Arm", "der", "Weis\u00b7heit", "des", "Lan\u00b7des", "Steur'", "re\u00b7giert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der mit dem Schwert des Rechtes das Unrecht niederbeugt,", "tokens": ["Der", "mit", "dem", "Schwert", "des", "Rech\u00b7tes", "das", "Un\u00b7recht", "nie\u00b7der\u00b7beugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der mit dem Blitz des Zornes lichtscheue That verscheucht \u2013", "tokens": ["Der", "mit", "dem", "Blitz", "des", "Zor\u00b7nes", "licht\u00b7scheu\u00b7e", "That", "ver\u00b7scheucht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "ADJA", "NN", "VVPP", "$("], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein Herrscher der das Gute, nur weil es gut ist, thut,", "tokens": ["Ein", "Herr\u00b7scher", "der", "das", "Gu\u00b7te", ",", "nur", "weil", "es", "gut", "ist", ",", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "ART", "NN", "$,", "ADV", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nicht, seiner L\u00e4nder Ruthe, hinopfert V\u00f6lkerblut,", "tokens": ["Nicht", ",", "sei\u00b7ner", "L\u00e4n\u00b7der", "Ru\u00b7the", ",", "hin\u00b7op\u00b7fert", "V\u00f6l\u00b7ker\u00b7blut", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PPOSAT", "NN", "NE", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ja, Solchem folgt der Nachruhm, und Liebe fliegt vorau,", "tokens": ["Ja", ",", "Sol\u00b7chem", "folgt", "der", "Nach\u00b7ruhm", ",", "und", "Lie\u00b7be", "fliegt", "vo\u00b7rau", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "VVFIN", "ART", "NN", "$,", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "F\u00fcrwahr und solcher Herrscher war ", "tokens": ["F\u00fcr\u00b7wahr", "und", "sol\u00b7cher", "Herr\u00b7scher", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "PIAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihm schlug ein Herz im Busen so rein, wie lautres Gold,", "tokens": ["Ihm", "schlug", "ein", "Herz", "im", "Bu\u00b7sen", "so", "rein", ",", "wie", "laut\u00b7res", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "ADV", "ADJD", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er war ein Freund der Musen und allen K\u00fcnsten hold;", "tokens": ["Er", "war", "ein", "Freund", "der", "Mu\u00b7sen", "und", "al\u00b7len", "K\u00fcns\u00b7ten", "hold", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "KON", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Zum Lorbeer, den das W\u00fcthen der Schlachten ihm errang,", "tokens": ["Zum", "Lor\u00b7beer", ",", "den", "das", "W\u00fct\u00b7hen", "der", "Schlach\u00b7ten", "ihm", "er\u00b7rang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Flocht er sich sch\u00f6nre Bl\u00fcthen durch hoher Lieder Sang.", "tokens": ["Flocht", "er", "sich", "sch\u00f6n\u00b7re", "Bl\u00fc\u00b7then", "durch", "ho\u00b7her", "Lie\u00b7der", "Sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJA", "NN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.5": {"line.1": {"text": "Kein K\u00fcnstler ging mit Unmuth aus seinem Kaiserhaus,", "tokens": ["Kein", "K\u00fcnst\u00b7ler", "ging", "mit", "Un\u00b7muth", "aus", "sei\u00b7nem", "Kai\u00b7ser\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dem nicht zum Lohn geworden ein Ehrenbl\u00fcthenstrauss.", "tokens": ["Dem", "nicht", "zum", "Lohn", "ge\u00b7wor\u00b7den", "ein", "Eh\u00b7ren\u00b7bl\u00fc\u00b7then\u00b7strauss", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "APPRART", "NN", "VAPP", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie hat er, edler ", "tokens": ["Wie", "hat", "er", ",", "ed\u00b7ler"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wie Deiner Muse, ", "tokens": ["Wie", "Dei\u00b7ner", "Mu\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Zu Innspruck weilt der Kaiser, der edle deutsche Mann;", "tokens": ["Zu", "I\u00b7nnspruck", "weilt", "der", "Kai\u00b7ser", ",", "der", "ed\u00b7le", "deut\u00b7sche", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nach Innspruck eilet Faustus zu Maximilian.", "tokens": ["Nach", "I\u00b7nnspruck", "ei\u00b7let", "Faus\u00b7tus", "zu", "Ma\u00b7xi\u00b7mi\u00b7li\u00b7an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NE", "APPR", "NE", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Schon h\u00fcllet d\u00fcstre D\u00e4mmrung den Tag in Schattenflor,", "tokens": ["Schon", "h\u00fcl\u00b7let", "d\u00fcst\u00b7re", "D\u00e4mm\u00b7rung", "den", "Tag", "in", "Schat\u00b7ten\u00b7flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVFIN", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der Mann des Zaubers wandelt zum Kaiserhaus empor.", "tokens": ["Der", "Mann", "des", "Zau\u00b7bers", "wan\u00b7delt", "zum", "Kai\u00b7ser\u00b7haus", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbviel hat der Ruf verk\u00fcndet von Euch!\u00ab der Herrscher spricht:", "tokens": ["\u00bb", "viel", "hat", "der", "Ruf", "ver\u00b7k\u00fcn\u00b7det", "von", "Euch", "!", "\u00ab", "der", "Herr\u00b7scher", "spricht", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ART", "NN", "VVFIN", "APPR", "PPER", "$.", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbdoch unsrer Lust gen\u00fcget des Volkes Sage nicht.", "tokens": ["\u00bb", "doch", "uns\u00b7rer", "Lust", "ge\u00b7n\u00fc\u00b7get", "des", "Vol\u00b7kes", "Sa\u00b7ge", "nicht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "PTKNEG", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wir m\u00f6chten selbst ein Probst\u00fcck von Euern K\u00fcnsten sehn,", "tokens": ["Wir", "m\u00f6ch\u00b7ten", "selbst", "ein", "Prob\u00b7st\u00fcck", "von", "Eu\u00b7ern", "K\u00fcns\u00b7ten", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sofern es sonder Beistand des B\u00f6sen kann geschehn!\u00ab", "tokens": ["So\u00b7fern", "es", "son\u00b7der", "Bei\u00b7stand", "des", "B\u00f6\u00b7sen", "kann", "ge\u00b7schehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "ART", "NN", "VMFIN", "VVINF", "$.", "$("], "meter": "-+-+----+-+-+", "measure": "unknown.measure.penta"}}, "stanza.8": {"line.1": {"text": "\u203anicht ", "tokens": ["\u203a", "nicht"], "token_info": ["punct", "word"], "pos": ["$(", "PTKNEG"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "\u00bbnennt es nicht Kunst, Herr Kaiser, nein, nennt es lieber ", "tokens": ["\u00bb", "nennt", "es", "nicht", "Kunst", ",", "Herr", "Kai\u00b7ser", ",", "nein", ",", "nennt", "es", "lie\u00b7ber"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "PTKNEG", "NN", "$,", "NN", "NN", "$,", "PTKANT", "$,", "VVFIN", "PPER", "ADV"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ich bin kein eitler Gaukler, der Euch gross Ding verheisst,", "tokens": ["Ich", "bin", "kein", "eit\u00b7ler", "Gauk\u00b7ler", ",", "der", "Euch", "gross", "Ding", "ver\u00b7heisst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und dann gemalte Geister im Schattenspiel Euch weisst.\u00ab", "tokens": ["Und", "dann", "ge\u00b7mal\u00b7te", "Geis\u00b7ter", "im", "Schat\u00b7ten\u00b7spiel", "Euch", "weisst", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "APPRART", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "\u00bbsoll meine Macht Euch dienen! Befehlt, Euch dient sie gern;", "tokens": ["\u00bb", "soll", "mei\u00b7ne", "Macht", "Euch", "die\u00b7nen", "!", "Be\u00b7fehlt", ",", "Euch", "dient", "sie", "gern", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$.", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ich rufe meine Geister nicht vor unw\u00fcrd'gem Herrn.", "tokens": ["Ich", "ru\u00b7fe", "mei\u00b7ne", "Geis\u00b7ter", "nicht", "vor", "un\u00b7w\u00fcrd'\u00b7gem", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Ob mir vom Guten Beistand, vom B\u00f6sen H\u00fclfe kommt?", "tokens": ["Ob", "mir", "vom", "Gu\u00b7ten", "Bei\u00b7stand", ",", "vom", "B\u00f6\u00b7sen", "H\u00fcl\u00b7fe", "kommt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "$,", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Euch gelt' es gleich, dem Schauen, nicht aber Gr\u00fcbeln frommt.\u00ab", "tokens": ["Euch", "gelt'", "es", "gleich", ",", "dem", "Schau\u00b7en", ",", "nicht", "a\u00b7ber", "Gr\u00fc\u00b7beln", "frommt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "$,", "PTKNEG", "ADV", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Der alte Kaiser ruhig des Magus Rede h\u00f6rt,", "tokens": ["Der", "al\u00b7te", "Kai\u00b7ser", "ru\u00b7hig", "des", "Ma\u00b7gus", "Re\u00b7de", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und nimmt das Wort: \u00bbSo bleibe vom Einwurf ungest\u00f6rt,", "tokens": ["Und", "nimmt", "das", "Wort", ":", "\u00bb", "So", "blei\u00b7be", "vom", "Ein\u00b7wurf", "un\u00b7ge\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$.", "$(", "ADV", "VVFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ob eine Himmelsmacht Dir, ob Andre Kraft Dir giebt,", "tokens": ["Ob", "ei\u00b7ne", "Him\u00b7mels\u00b7macht", "Dir", ",", "ob", "And\u00b7re", "Kraft", "Dir", "giebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "$,", "KOUS", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Erhelle nur die Nacht mir, die mir mein Denken tr\u00fcbt!\u00ab", "tokens": ["Er\u00b7hel\u00b7le", "nur", "die", "Nacht", "mir", ",", "die", "mir", "mein", "Den\u00b7ken", "tr\u00fcbt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPER", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "\u00bbmich floh der Schlummer oft schon, wenn ich sein Nahn erbat,", "tokens": ["\u00bb", "mich", "floh", "der", "Schlum\u00b7mer", "oft", "schon", ",", "wenn", "ich", "sein", "Nahn", "er\u00b7bat", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+----+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Und irdscher Gr\u00f6sse Trugbild vor meine Seele trat;", "tokens": ["Und", "ird\u00b7scher", "Gr\u00f6s\u00b7se", "Trug\u00b7bild", "vor", "mei\u00b7ne", "See\u00b7le", "trat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich sah die Kaiser alle vor mir vor\u00fcber gehn,", "tokens": ["Ich", "sah", "die", "Kai\u00b7ser", "al\u00b7le", "vor", "mir", "vor\u00b7\u00fc\u00b7ber", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PIS", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "All' meine hohen Ahnherrn hab' ich im Geist gesehn.\u00ab", "tokens": ["All'", "mei\u00b7ne", "ho\u00b7hen", "Ahn\u00b7herrn", "hab'", "ich", "im", "Geist", "ge\u00b7sehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$.", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "\u00bbsie haben Ruhm errungen, des Grossen viel vollbracht,", "tokens": ["\u00bb", "sie", "ha\u00b7ben", "Ruhm", "er\u00b7run\u00b7gen", ",", "des", "Gros\u00b7sen", "viel", "voll\u00b7bracht", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "NN", "VVPP", "$,", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch was war ihre St\u00e4rke gen ", "tokens": ["Doch", "was", "war", "ih\u00b7re", "St\u00e4r\u00b7ke", "gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VAFIN", "PPOSAT", "NN", "APPR"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.3": {"text": "Der sich mit J\u00fcnglingsfeuer den Erdkreis unterwarf,", "tokens": ["Der", "sich", "mit", "J\u00fcng\u00b7lings\u00b7feu\u00b7er", "den", "Erd\u00b7kreis", "un\u00b7ter\u00b7warf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mit dem der Herrscher keiner an Ruhm sich messen darf?!\u00ab", "tokens": ["Mit", "dem", "der", "Herr\u00b7scher", "kei\u00b7ner", "an", "Ruhm", "sich", "mes\u00b7sen", "darf", "?!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "PIS", "APPR", "NN", "PRF", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "\u00bbihn m\u00f6cht' ich schaun, den Grossen, o ruf' ihn mir herbei!", "tokens": ["\u00bb", "ihn", "m\u00f6cht'", "ich", "schaun", ",", "den", "Gros\u00b7sen", ",", "o", "ruf'", "ihn", "mir", "her\u00b7bei", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "VVINF", "$,", "ART", "NN", "$,", "FM", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dass ich ins Herz mir pr\u00e4ge des Helden Konterfei.", "tokens": ["Dass", "ich", "ins", "Herz", "mir", "pr\u00e4\u00b7ge", "des", "Hel\u00b7den", "Kon\u00b7ter\u00b7fei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Nachwelt ist ein Schatten, die Vorwelt Heldenglanz,", "tokens": ["Die", "Nach\u00b7welt", "ist", "ein", "Schat\u00b7ten", ",", "die", "Vor\u00b7welt", "Hel\u00b7den\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "NE", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ach, ein entfallnes Blatt nur aus der Zeiten Wunderkranz!\u00ab", "tokens": ["Ach", ",", "ein", "ent\u00b7fall\u00b7nes", "Blatt", "nur", "aus", "der", "Zei\u00b7ten", "Wun\u00b7der\u00b7kranz", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.14": {"line.1": {"text": "So Maximilianus, \u2013 und Faustus ernsthaft spricht:", "tokens": ["So", "Ma\u00b7xi\u00b7mi\u00b7li\u00b7a\u00b7nus", ",", "\u2013", "und", "Faus\u00b7tus", "ernst\u00b7haft", "spricht", ":"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "$(", "KON", "NE", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "\u00bbihr sollt den Helden schauen, seht ihn, doch redet nicht!\u00ab", "tokens": ["\u00bb", "ihr", "sollt", "den", "Hel\u00b7den", "schau\u00b7en", ",", "seht", "ihn", ",", "doch", "re\u00b7det", "nicht", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und leise Worte murmelnd er seinen Stab bewegt,", "tokens": ["Und", "lei\u00b7se", "Wor\u00b7te", "mur\u00b7melnd", "er", "sei\u00b7nen", "Stab", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Darauf sich tiefes Dunkel um beide n\u00e4chtlich legt.", "tokens": ["Da\u00b7rauf", "sich", "tie\u00b7fes", "Dun\u00b7kel", "um", "bei\u00b7de", "n\u00e4cht\u00b7lich", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "ADJA", "NN", "APPR", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.15": {"line.1": {"text": "Nun schwebt ein Heldenschatten in das Gemach herein;", "tokens": ["Nun", "schwebt", "ein", "Hel\u00b7den\u00b7schat\u00b7ten", "in", "das", "Ge\u00b7mach", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-++-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Ein Mann, umwallt von Goldhaar, doch finster blickt er drein;", "tokens": ["Ein", "Mann", ",", "um\u00b7wallt", "von", "Gold\u00b7haar", ",", "doch", "fins\u00b7ter", "blickt", "er", "drein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "NN", "$,", "ADV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "Sein Auge flammt, hinreicht ihm der Kaiser seine Hand,", "tokens": ["Sein", "Au\u00b7ge", "flammt", ",", "hin\u00b7reicht", "ihm", "der", "Kai\u00b7ser", "sei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch d\u00fcster hat der Geist sich von ihm hinweggewandt.", "tokens": ["Doch", "d\u00fcs\u00b7ter", "hat", "der", "Geist", "sich", "von", "ihm", "hin\u00b7weg\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.16": {"line.1": {"text": "Und schwindet hin; drauf gl\u00e4nzend ein sch\u00f6nes Weib sich zeigt,", "tokens": ["Und", "schwin\u00b7det", "hin", ";", "drauf", "gl\u00e4n\u00b7zend", "ein", "sch\u00f6\u00b7nes", "Weib", "sich", "zeigt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$.", "PAV", "ADJD", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die mit holdsel'gem L\u00e4cheln sich vor dem Kaiser neigt.", "tokens": ["Die", "mit", "hold\u00b7sel'\u00b7gem",