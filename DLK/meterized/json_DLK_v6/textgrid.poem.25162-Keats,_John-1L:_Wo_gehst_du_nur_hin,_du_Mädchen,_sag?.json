{"textgrid.poem.25162": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo gehst du nur hin, du M\u00e4dchen, sag?", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo gehst du nur hin, du M\u00e4dchen, sag?", "tokens": ["Wo", "gehst", "du", "nur", "hin", ",", "du", "M\u00e4d\u00b7chen", ",", "sag", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PPER", "NN", "$,", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und was tr\u00e4gst du im K\u00f6rbchen so sittig?", "tokens": ["Und", "was", "tr\u00e4gst", "du", "im", "K\u00f6rb\u00b7chen", "so", "sit\u00b7tig", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Du sauberes Kind, eil nicht so geschwind,", "tokens": ["Du", "sau\u00b7be\u00b7res", "Kind", ",", "eil", "nicht", "so", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "KOUS", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Reich mir einen Trunk, ich bitt dich!", "tokens": ["Reich", "mir", "ei\u00b7nen", "Trunk", ",", "ich", "bitt", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Ich mag deinen Anger, ich mag deinen Klee,", "tokens": ["Ich", "mag", "dei\u00b7nen", "An\u00b7ger", ",", "ich", "mag", "dei\u00b7nen", "Klee", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "$,", "PPER", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und Milch naschen mag ich unendlich;", "tokens": ["Und", "Milch", "na\u00b7schen", "mag", "ich", "un\u00b7end\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "VMFIN", "PPER", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Doch lieber mir's ist, wenn dein M\u00fcndchen mich k\u00fc\u00dft.", "tokens": ["Doch", "lie\u00b7ber", "mir's", "ist", ",", "wenn", "dein", "M\u00fcnd\u00b7chen", "mich", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "VAFIN", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Das ist ja so selbstverst\u00e4ndlich.", "tokens": ["Das", "ist", "ja", "so", "selbst\u00b7ver\u00b7st\u00e4nd\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich mag deine H\u00fcgel, deine T\u00e4ler so sehr,", "tokens": ["Ich", "mag", "dei\u00b7ne", "H\u00fc\u00b7gel", ",", "dei\u00b7ne", "T\u00e4\u00b7ler", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und ich mag deine bl\u00f6kenden Schafe \u2013", "tokens": ["Und", "ich", "mag", "dei\u00b7ne", "bl\u00f6\u00b7ken\u00b7den", "Scha\u00b7fe", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-++--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch ach, mich ins Heu zur Seite dir treu", "tokens": ["Doch", "ach", ",", "mich", "ins", "Heu", "zur", "Sei\u00b7te", "dir", "treu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "XY", "$,", "PRF", "APPRART", "NN", "APPRART", "NN", "PPER", "ADJD"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu betten zum Liebesschlafe!", "tokens": ["Zu", "bet\u00b7ten", "zum", "Lie\u00b7bes\u00b7schla\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Dein K\u00f6rbchen, das stell ich recht sorgsam beiseit,", "tokens": ["Dein", "K\u00f6rb\u00b7chen", ",", "das", "stell", "ich", "recht", "sorg\u00b7sam", "bei\u00b7seit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADJD", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Deinen Schal h\u00e4ng ich auf an der Weide,", "tokens": ["Dei\u00b7nen", "Schal", "h\u00e4ng", "ich", "auf", "an", "der", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und dann seufzen wir matt in Blumen und Blatt", "tokens": ["Und", "dann", "seuf\u00b7zen", "wir", "matt", "in", "Blu\u00b7men", "und", "Blatt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und k\u00fcssen und k\u00fcssen uns beide.", "tokens": ["Und", "k\u00fcs\u00b7sen", "und", "k\u00fcs\u00b7sen", "uns", "bei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KON", "VVFIN", "PPER", "PIS", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}