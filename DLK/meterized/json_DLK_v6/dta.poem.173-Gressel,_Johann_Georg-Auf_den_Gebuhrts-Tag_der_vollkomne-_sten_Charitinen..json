{"dta.poem.173": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Gebuhrts-Tag der vollkomne-  \n sten   Charitinen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mehr Sonne deinen Schein mit dreymal hellern Strahlen/", "tokens": ["Mehr", "Son\u00b7ne", "dei\u00b7nen", "Schein", "mit", "drey\u00b7mal", "hel\u00b7lern", "Strah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "APPR", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als wie du sonsten pflegst die Erde zu bemahlen.", "tokens": ["Als", "wie", "du", "sons\u00b7ten", "pflegst", "die", "Er\u00b7de", "zu", "be\u00b7mah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PPER", "ADV", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Blas lauter Biesams-Lufft annehmlich s\u00fcsser West", "tokens": ["Blas", "lau\u00b7ter", "Bie\u00b7sams\u00b7Lufft", "an\u00b7nehm\u00b7lich", "s\u00fcs\u00b7ser", "West"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit ", "tokens": ["Mit"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Vermehret eure Zier ihr bunt gestickten Blumen/", "tokens": ["Ver\u00b7meh\u00b7ret", "eu\u00b7re", "Zier", "ihr", "bunt", "ge\u00b7stick\u00b7ten", "Blu\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du tieffgeb\u00fcckter Klee verkehr dich in ", "tokens": ["Du", "tieff\u00b7ge\u00b7b\u00fcck\u00b7ter", "Klee", "ver\u00b7kehr", "dich", "in"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "PRF", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Jhr Dornen tragt ", "tokens": ["Ihr", "Dor\u00b7nen", "tragt"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Jhr ", "tokens": ["Ihr"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Du edle ", "tokens": ["Du", "ed\u00b7le"], "token_info": ["word", "word"], "pos": ["PPER", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Verla\u00df den klugen ", "tokens": ["Ver\u00b7la\u00df", "den", "klu\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "So dein Geruch ertheilt. Jhr Steine schwitzet Oel/", "tokens": ["So", "dein", "Ge\u00b7ruch", "er\u00b7theilt", ".", "Ihr", "Stei\u00b7ne", "schwit\u00b7zet", "O\u00b7el", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$.", "PPOSAT", "NN", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Jhr B\u00e4ume triefft mit Safft dem selbst der Balsam scheel/", "tokens": ["Ihr", "B\u00e4u\u00b7me", "triefft", "mit", "Safft", "dem", "selbst", "der", "Bal\u00b7sam", "scheel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "ART", "ADV", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jhr Wolcken fl\u00f6\u00dft herab nur Pomerantzen-N\u00e4sse/", "tokens": ["Ihr", "Wol\u00b7cken", "fl\u00f6\u00dft", "her\u00b7ab", "nur", "Po\u00b7me\u00b7rant\u00b7zen\u00b7N\u00e4s\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Jhr Diesteln gebet ab Bisamte Rauch-Gef\u00e4sse/", "tokens": ["Ihr", "Dies\u00b7teln", "ge\u00b7bet", "ab", "Bi\u00b7sam\u00b7te", "Rauch\u00b7Ge\u00b7f\u00e4s\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.15": {"text": "Jhr Fl\u00fcsse zinst Coral/ Rubin und Demant-Stein/", "tokens": ["Ihr", "Fl\u00fcs\u00b7se", "zinst", "Co\u00b7ral", "/", "Ru\u00b7bin", "und", "De\u00b7mant\u00b7Stein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NE", "$(", "NE", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Last eure Wellen nichts als puren ", "tokens": ["Last", "eu\u00b7re", "Wel\u00b7len", "nichts", "als", "pu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "PIS", "KOKOM", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Jhr B\u00e4ume tragt an statt der Bl\u00e4tter lauter Zungen/", "tokens": ["Ihr", "B\u00e4u\u00b7me", "tragt", "an", "statt", "der", "Bl\u00e4t\u00b7ter", "lau\u00b7ter", "Zun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "APPR", "ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Jhr Sterne lehret mich die s\u00fc\u00dften Schmeichelungen.", "tokens": ["Ihr", "Ster\u00b7ne", "leh\u00b7ret", "mich", "die", "s\u00fc\u00df\u00b7ten", "Schmei\u00b7che\u00b7lun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Jhr ", "tokens": ["Ihr"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.20": {"text": "Da\u00df meine Feder heut was angenehmes schafft.", "tokens": ["Da\u00df", "mei\u00b7ne", "Fe\u00b7der", "heut", "was", "an\u00b7ge\u00b7neh\u00b7mes", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PWS", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da\u00df ich dem g\u00fcldnen Licht ein t\u00fcchtigs Opffer bringe/", "tokens": ["Da\u00df", "ich", "dem", "g\u00fcld\u00b7nen", "Licht", "ein", "t\u00fcch\u00b7tigs", "Opf\u00b7fer", "brin\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df mein verstimmtes Spiel wie ", "tokens": ["Da\u00df", "mein", "ver\u00b7stimm\u00b7tes", "Spiel", "wie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "KOKOM"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Da\u00df ich das erste Licht der sch\u00f6nsten ", "tokens": ["Da\u00df", "ich", "das", "ers\u00b7te", "Licht", "der", "sch\u00f6ns\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "So wie es sich geb\u00fchrt/ mit meiner Hand bedien.", "tokens": ["So", "wie", "es", "sich", "ge\u00b7b\u00fchrt", "/", "mit", "mei\u00b7ner", "Hand", "be\u00b7di\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "PRF", "VVPP", "$(", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Bebl\u00fcmte ", "tokens": ["Be\u00b7bl\u00fcm\u00b7te"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.26": {"text": "Die denen Palmen gleich nicht welcken noch verwesen/", "tokens": ["Die", "de\u00b7nen", "Pal\u00b7men", "gleich", "nicht", "wel\u00b7cken", "noch", "ver\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "NN", "ADV", "PTKNEG", "VVINF", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wind\u2019t mir von ", "tokens": ["Wind't", "mir", "von"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.28": {"text": "So kostbahr wies verdient der ", "tokens": ["So", "kost\u00b7bahr", "wies", "ver\u00b7dient", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "VVFIN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Jhr Morgenl\u00e4nder schenckt mir eure Kostbarkeiten/", "tokens": ["Ihr", "Mor\u00b7gen\u00b7l\u00e4n\u00b7der", "schenckt", "mir", "eu\u00b7re", "Kost\u00b7bar\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Helfft mir ein Angedenck mit h\u00f6chsten Schmuck bereiten/", "tokens": ["Helfft", "mir", "ein", "An\u00b7ge\u00b7denck", "mit", "h\u00f6chs\u00b7ten", "Schmuck", "be\u00b7rei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ternat und Potosi, Moluck und Martrapan,", "tokens": ["Ter\u00b7nat", "und", "Po\u00b7to\u00b7si", ",", "Mo\u00b7luck", "und", "Mar\u00b7tra\u00b7pan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$,", "NN", "KON", "NE", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.32": {"text": "Streckt ihr/ zu ehren/ doch all\u2019 eure Kr\u00e4ffte an.", "tokens": ["Streckt", "ihr", "/", "zu", "eh\u00b7ren", "/", "doch", "all'", "eu\u00b7re", "Kr\u00e4ff\u00b7te", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PTKZU", "VVINF", "$(", "ADV", "PIAT", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Doch weicht ihr Indiens unsch\u00e4tzbahr grosse Sch\u00e4tze/", "tokens": ["Doch", "weicht", "ihr", "In\u00b7di\u00b7ens", "un\u00b7sch\u00e4tz\u00b7bahr", "gros\u00b7se", "Sch\u00e4t\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.34": {"text": "Ich wei\u00df ihr ziehet ab/ wenn ich entgegen setze", "tokens": ["Ich", "wei\u00df", "ihr", "zie\u00b7het", "ab", "/", "wenn", "ich", "ent\u00b7ge\u00b7gen", "set\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$(", "KOUS", "PPER", "PTKVZ", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Euch dieses Wunder-Bild/ den gelben Haaren weicht/", "tokens": ["Euch", "die\u00b7ses", "Wun\u00b7der\u00b7Bild", "/", "den", "gel\u00b7ben", "Haa\u00b7ren", "weicht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der Printze des Metalls/ der wie das Bley erbleicht.", "tokens": ["Der", "Print\u00b7ze", "des", "Me\u00b7talls", "/", "der", "wie", "das", "Bley", "er\u00b7bleicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ART", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die Augen stechen weg die feurgen Diamanten/", "tokens": ["Die", "Au\u00b7gen", "ste\u00b7chen", "weg", "die", "feur\u00b7gen", "Di\u00b7a\u00b7man\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Den Lippen gehn zur Seit Corallen als Trabanten/", "tokens": ["Den", "Lip\u00b7pen", "gehn", "zur", "Seit", "Co\u00b7ral\u00b7len", "als", "Tra\u00b7ban\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "NE", "KOUS", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der Hals siegt Perlen an/ die Z\u00e4hn den Helffenbein/", "tokens": ["Der", "Hals", "siegt", "Per\u00b7len", "an", "/", "die", "Z\u00e4hn", "den", "Helf\u00b7fen\u00b7bein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "PTKVZ", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und vor den Adern geht der feinste T\u00fcrckis ein.", "tokens": ["Und", "vor", "den", "A\u00b7dern", "geht", "der", "feins\u00b7te", "T\u00fcr\u00b7ckis", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Die K\u00e4hle macht besch\u00e4mt die kl\u00e4rsten Berg-Crystallen/", "tokens": ["Die", "K\u00e4h\u00b7le", "macht", "be\u00b7sch\u00e4mt", "die", "kl\u00e4rs\u00b7ten", "Ber\u00b7gCry\u00b7stal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Man sieht den rohten Wein so fein hinunter wallen/", "tokens": ["Man", "sieht", "den", "roh\u00b7ten", "Wein", "so", "fein", "hin\u00b7un\u00b7ter", "wal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Da\u00df sich die ", "tokens": ["Da\u00df", "sich", "die"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PRF", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.44": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.45": {"text": "Der Scharlach wird besch\u00e4mt vor ihren Purpur-Wangen/", "tokens": ["Der", "Schar\u00b7lach", "wird", "be\u00b7sch\u00e4mt", "vor", "ih\u00b7ren", "Pur\u00b7pur\u00b7Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Die Br\u00fcste als Albast/ ja mehr als ", "tokens": ["Die", "Br\u00fcs\u00b7te", "als", "Al\u00b7bast", "/", "ja", "mehr", "als"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "NE", "$(", "ADV", "PIAT", "KOKOM"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.47": {"text": "Vor denen Lippen bleicht Zinnober und Rubin/", "tokens": ["Vor", "de\u00b7nen", "Lip\u00b7pen", "bleicht", "Zin\u00b7no\u00b7ber", "und", "Ru\u00b7bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "VVFIN", "NN", "KON", "NE", "$("], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.48": {"text": "Jhr Athen \u00fcbersteigt ", "tokens": ["Ihr", "A\u00b7then", "\u00fc\u00b7bers\u00b7teigt"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.49": {"text": "Weil alles nun vor nichts ist gegen sie zu sch\u00e4tzen/", "tokens": ["Weil", "al\u00b7les", "nun", "vor", "nichts", "ist", "ge\u00b7gen", "sie", "zu", "sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "PIS", "VAFIN", "APPR", "PPER", "PTKZU", "VVINF"