{"dta.poem.13544": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Doch wer mag w\u00fcrdig jene Reinen loben,", "tokens": ["Doch", "wer", "mag", "w\u00fcr\u00b7dig", "je\u00b7ne", "Rei\u00b7nen", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "ADJD", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die in der Zeit hochm\u00fcth'gem Trieb und Trachten", "tokens": ["Die", "in", "der", "Zeit", "hoch\u00b7m\u00fcth'\u00b7gem", "Trieb", "und", "Trach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die heil'ge Flamme treu in sich bewachten,", "tokens": ["Die", "heil'\u00b7ge", "Flam\u00b7me", "treu", "in", "sich", "be\u00b7wach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus der die alte Sch\u00f6nheit neu erhoben!", "tokens": ["Aus", "der", "die", "al\u00b7te", "Sch\u00f6n\u00b7heit", "neu", "er\u00b7ho\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}