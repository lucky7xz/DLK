{"textgrid.poem.41670": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: In fahlen Sesseln schaut ich alte Buhlerinnen,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In fahlen Sesseln schaut ich alte Buhlerinnen,", "tokens": ["In", "fah\u00b7len", "Ses\u00b7seln", "schaut", "ich", "al\u00b7te", "Buh\u00b7le\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bleich, mit gemalten Braun, geschminkt noch im Verfall,", "tokens": ["Bleich", ",", "mit", "ge\u00b7mal\u00b7ten", "Braun", ",", "ge\u00b7schminkt", "noch", "im", "Ver\u00b7fall", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN", "$,", "VVPP", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verstellten Blicks. Ich sah von magren Ohren rinnen", "tokens": ["Ver\u00b7stell\u00b7ten", "Blicks", ".", "Ich", "sah", "von", "mag\u00b7ren", "Oh\u00b7ren", "rin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein klirrendes Geh\u00e4ng von Steinen und Metall.", "tokens": ["Ein", "klir\u00b7ren\u00b7des", "Ge\u00b7h\u00e4ng", "von", "Stei\u00b7nen", "und", "Me\u00b7tall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gesichter lippenlos, auf gr\u00fcne Tische stierend,", "tokens": ["Ge\u00b7sich\u00b7ter", "lip\u00b7pen\u00b7los", ",", "auf", "gr\u00fc\u00b7ne", "Ti\u00b7sche", "stie\u00b7rend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Lippen ohne Blut, Kinnladen ohne Zahn,", "tokens": ["Die", "Lip\u00b7pen", "oh\u00b7ne", "Blut", ",", "Kinn\u00b7la\u00b7den", "oh\u00b7ne", "Zahn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und Finger, wild verkrampft, nach Golde angstvoll gierend,", "tokens": ["Und", "Fin\u00b7ger", ",", "wild", "ver\u00b7krampft", ",", "nach", "Gol\u00b7de", "angst\u00b7voll", "gie\u00b7rend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "VVPP", "$,", "APPR", "NE", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durchw\u00fchlend Brust und Kleid in grausem Fieberwahn.", "tokens": ["Durch\u00b7w\u00fch\u00b7lend", "Brust", "und", "Kleid", "in", "grau\u00b7sem", "Fie\u00b7ber\u00b7wahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "An schmutzigem Gew\u00f6lb unz\u00e4hlge Kerzenlichter", "tokens": ["An", "schmut\u00b7zi\u00b7gem", "Ge\u00b7w\u00f6lb", "un\u00b7z\u00e4hl\u00b7ge", "Ker\u00b7zen\u00b7lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und riesge Leuchter, die ihr Flimmern bleich und wei\u00df", "tokens": ["Und", "ries\u00b7ge", "Leuch\u00b7ter", ",", "die", "ihr", "Flim\u00b7mern", "bleich", "und", "wei\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Entsandten auf die Stirn der finstren, gro\u00dfen Dichter,", "tokens": ["Ent\u00b7sand\u00b7ten", "auf", "die", "Stirn", "der", "finst\u00b7ren", ",", "gro\u00b7\u00dfen", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die stumm vergeudeten der Marter blutgen Schwei\u00df.", "tokens": ["Die", "stumm", "ver\u00b7geu\u00b7de\u00b7ten", "der", "Mar\u00b7ter", "blut\u00b7gen", "Schwei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das ist das schwarze Bild, das ich in b\u00f6sem Traume", "tokens": ["Das", "ist", "das", "schwar\u00b7ze", "Bild", ",", "das", "ich", "in", "b\u00f6\u00b7sem", "Trau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit allzu klarem Blick ersp\u00e4ht in n\u00e4chtger Zeit.", "tokens": ["Mit", "all\u00b7zu", "kla\u00b7rem", "Blick", "er\u00b7sp\u00e4ht", "in", "n\u00e4cht\u00b7ger", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PTKA", "ADJA", "NN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich selber schaute in dem grauenhaften Raume", "tokens": ["Ich", "sel\u00b7ber", "schau\u00b7te", "in", "dem", "grau\u00b7en\u00b7haf\u00b7ten", "Rau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mich aufgest\u00fctzt, erstarrt, stumm und voll tiefem Neid.", "tokens": ["Mich", "auf\u00b7ge\u00b7st\u00fctzt", ",", "er\u00b7starrt", ",", "stumm", "und", "voll", "tie\u00b7fem", "Neid", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "VVFIN", "$,", "ADJD", "KON", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Voll Neid auf dieser Schar untilgbar-z\u00e4h Verlangen,", "tokens": ["Voll", "Neid", "auf", "die\u00b7ser", "Schar", "un\u00b7tilg\u00b7ba\u00b7rz\u00e4h", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "PDAT", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf dies Vergn\u00fcgen, das die Dirnen aufrecht hielt,", "tokens": ["Auf", "dies", "Ver\u00b7gn\u00fc\u00b7gen", ",", "das", "die", "Dir\u00b7nen", "auf\u00b7recht", "hielt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie unter meinem Blick sie frech und unbefangen", "tokens": ["Wie", "un\u00b7ter", "mei\u00b7nem", "Blick", "sie", "frech", "und", "un\u00b7be\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "PPER", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Um einstge Sch\u00f6nheit und um Ehrbarkeit gespielt.", "tokens": ["Um", "einst\u00b7ge", "Sch\u00f6n\u00b7heit", "und", "um", "Ehr\u00b7bar\u00b7keit", "ge\u00b7spielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "KON", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und es erschrak mein Herz, manch Armen zu beneiden,", "tokens": ["Und", "es", "er\u00b7schrak", "mein", "Herz", ",", "manch", "Ar\u00b7men", "zu", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der gl\u00fchnden Eifers st\u00fcrzt zum Abgrund des Gerichts,", "tokens": ["Der", "gl\u00fchn\u00b7den", "Ei\u00b7fers", "st\u00fcrzt", "zum", "Ab\u00b7grund", "des", "Ge\u00b7richts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und der, von seinem Blut berauscht, die grimmsten Leiden", "tokens": ["Und", "der", ",", "von", "sei\u00b7nem", "Blut", "be\u00b7rauscht", ",", "die", "grimms\u00b7ten", "Lei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem Tode vorzieht und die H\u00f6lle selbst dem Nichts.", "tokens": ["Dem", "To\u00b7de", "vor\u00b7zieht", "und", "die", "H\u00f6l\u00b7le", "selbst", "dem", "Nichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}