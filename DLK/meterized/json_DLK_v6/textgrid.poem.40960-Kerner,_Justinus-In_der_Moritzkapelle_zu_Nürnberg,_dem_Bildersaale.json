{"textgrid.poem.40960": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "In der Moritzkapelle zu N\u00fcrnberg, dem Bildersaale", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein heiliger Mauritius!", "tokens": ["Mein", "hei\u00b7li\u00b7ger", "Mau\u00b7ri\u00b7ti\u00b7us", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "So oft in deiner Kirche Mauern", "tokens": ["So", "oft", "in", "dei\u00b7ner", "Kir\u00b7che", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von einem Bild ich scheiden mu\u00df,", "tokens": ["Von", "ei\u00b7nem", "Bild", "ich", "schei\u00b7den", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fa\u00dft mich ein sehnsuchtsvolles Trauern.", "tokens": ["Fa\u00dft", "mich", "ein", "sehn\u00b7suchts\u00b7vol\u00b7les", "Trau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gib, Heil'ger! da\u00df in dieser Stund'", "tokens": ["Gib", ",", "Heil'\u00b7ger", "!", "da\u00df", "in", "die\u00b7ser", "Stund'"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "NN", "$.", "KOUS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch endlich ich zum Sprechen wecke", "tokens": ["Doch", "end\u00b7lich", "ich", "zum", "Spre\u00b7chen", "we\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dies Frauenbild mit stummem Mund,", "tokens": ["Dies", "Frau\u00b7en\u00b7bild", "mit", "stum\u00b7mem", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dort h\u00e4ngend in mitt\u00e4g'ger Ecke.", "tokens": ["Dort", "h\u00e4n\u00b7gend", "in", "mit\u00b7t\u00e4g'\u00b7ger", "E\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O du mit Augen sanft und licht,", "tokens": ["O", "du", "mit", "Au\u00b7gen", "sanft", "und", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Altdeutsches, liebes Frauenwesen,", "tokens": ["Alt\u00b7deut\u00b7sches", ",", "lie\u00b7bes", "Frau\u00b7en\u00b7we\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Lang lauschend, ob dein Mund nicht spricht,", "tokens": ["Lang", "lau\u00b7schend", ",", "ob", "dein", "Mund", "nicht", "spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bin ich schon oft allhier gewesen.", "tokens": ["Bin", "ich", "schon", "oft", "all\u00b7hier", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dein wei\u00dfes H\u00e4ubchen, ach wie traut!", "tokens": ["Dein", "wei\u00b7\u00dfes", "H\u00e4ub\u00b7chen", ",", "ach", "wie", "traut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADV", "KOKOM", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein schwarz Gewand, wie fromm und sittig!", "tokens": ["Dein", "schwarz", "Ge\u00b7wand", ",", "wie", "fromm", "und", "sit\u00b7tig", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "PWAV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Mund, als spr\u00e4ch' er \u2013 doch kein Laut!", "tokens": ["Dein", "Mund", ",", "als", "spr\u00e4ch'", "er", "\u2013", "doch", "kein", "Laut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "VVFIN", "PPER", "$(", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O um ein einz'ges W\u00f6rtchen bitt' ich.", "tokens": ["O", "um", "ein", "einz'\u00b7ges", "W\u00f6rt\u00b7chen", "bitt'", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Bist Leben ganz, blickst an mich ja,", "tokens": ["Bist", "Le\u00b7ben", "ganz", ",", "blickst", "an", "mich", "ja", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "$,", "VVFIN", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wolltest du mir etwas sagen,", "tokens": ["Als", "woll\u00b7test", "du", "mir", "et\u00b7was", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl ein Gebet, ein Lied etwa,", "tokens": ["Wohl", "ein", "Ge\u00b7bet", ",", "ein", "Lied", "et\u00b7wa", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus alter Zeiten frommen Tagen.", "tokens": ["Aus", "al\u00b7ter", "Zei\u00b7ten", "from\u00b7men", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sprich nur! ich knie, ich bet' dir nach!", "tokens": ["Sprich", "nur", "!", "ich", "knie", ",", "ich", "bet'", "dir", "nach", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "PPER", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mauritius! jetzt la\u00df es sprechen!", "tokens": ["Mau\u00b7ri\u00b7ti\u00b7us", "!", "jetzt", "la\u00df", "es", "spre\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVIMP", "PPER", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ich bet'! bet' mit! \u2013 Kein W\u00f6rtchen, ach!", "tokens": ["Ich", "bet'", "!", "bet'", "mit", "!", "\u2013", "Kein", "W\u00f6rt\u00b7chen", ",", "ach", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "PTKVZ", "$.", "$(", "PIAT", "NN", "$,", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stumm! stumm! \u2013 Es m\u00f6cht' das Herz mir brechen!", "tokens": ["Stumm", "!", "stumm", "!", "\u2013", "Es", "m\u00f6cht'", "das", "Herz", "mir", "bre\u00b7chen", "!"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "ADJD", "$.", "$(", "PPER", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und abermals mu\u00df ich von hier,", "tokens": ["Und", "a\u00b7ber\u00b7mals", "mu\u00df", "ich", "von", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erfuhr nicht, was es mir wollt' sagen.", "tokens": ["Er\u00b7fuhr", "nicht", ",", "was", "es", "mir", "wollt'", "sa\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mauritius! das mu\u00df ich dir,", "tokens": ["Mau\u00b7ri\u00b7ti\u00b7us", "!", "das", "mu\u00df", "ich", "dir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PDS", "VMFIN", "PPER", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Schutzheil'gem dieser Kirche, klagen.", "tokens": ["Schutz\u00b7heil'\u00b7gem", "die\u00b7ser", "Kir\u00b7che", ",", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}