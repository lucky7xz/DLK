{"dta.poem.18589": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "U ber eine vornehme  D ame/ die  \n in Gasthoff zum halben Monde  \n logirte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Es zeucht der blasse Mond sonst seinen Silber ein/", "tokens": ["Es", "zeucht", "der", "blas\u00b7se", "Mond", "sonst", "sei\u00b7nen", "Sil\u00b7ber", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je n\u00e4her seinen Crey\u00df der Sonnen Gold bemahlet/", "tokens": ["Je", "n\u00e4\u00b7her", "sei\u00b7nen", "Crey\u00df", "der", "Son\u00b7nen", "Gold", "be\u00b7mah\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier aber will sein Glantz recht unvergleichlich seyn/", "tokens": ["Hier", "a\u00b7ber", "will", "sein", "Glantz", "recht", "un\u00b7ver\u00b7gleich\u00b7lich", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPOSAT", "NN", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil itzt die Sonne selbst in halben Monde strahlet.", "tokens": ["Weil", "itzt", "die", "Son\u00b7ne", "selbst", "in", "hal\u00b7ben", "Mon\u00b7de", "strah\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word"