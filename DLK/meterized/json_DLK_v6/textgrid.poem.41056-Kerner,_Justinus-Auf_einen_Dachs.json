{"textgrid.poem.41056": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen Dachs", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Armes Tier, im engen Bau", "tokens": ["Ar\u00b7mes", "Tier", ",", "im", "en\u00b7gen", "Bau"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegest du verdrossen,", "tokens": ["Lie\u00b7gest", "du", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wiesengr\u00fcn und Himmelblau", "tokens": ["Wie\u00b7sen\u00b7gr\u00fcn", "und", "Him\u00b7mel\u00b7blau"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibt dir stets verschlossen.", "tokens": ["Bleibt", "dir", "stets", "ver\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Nur in finstrer Mitternacht", "tokens": ["Nur", "in", "finst\u00b7rer", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommst herausgegangen", "tokens": ["Kommst", "her\u00b7aus\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sch\u00fcchtern: denn sie halten Wacht,", "tokens": ["Sch\u00fcch\u00b7tern", ":", "denn", "sie", "hal\u00b7ten", "Wacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie dich bald fangen.", "tokens": ["Da\u00df", "sie", "dich", "bald", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Deinem Felle nur wird Teil,", "tokens": ["Dei\u00b7nem", "Fel\u00b7le", "nur", "wird", "Teil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was dir nie geworden,", "tokens": ["Was", "dir", "nie", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VAPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Oft ein sch\u00f6nes Reiseheil", "tokens": ["Oft", "ein", "sch\u00f6\u00b7nes", "Rei\u00b7se\u00b7heil"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach den fernsten Orten.", "tokens": ["Nach", "den", "ferns\u00b7ten", "Or\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "So wie dir, also ergeht's", "tokens": ["So", "wie", "dir", ",", "al\u00b7so", "er\u00b7geht's"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "KOKOM", "PPER", "$,", "ADV", "NE"], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Mir im engen Leben,", "tokens": ["Mir", "im", "en\u00b7gen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mu\u00df an einer Stelle stets", "tokens": ["Mu\u00df", "an", "ei\u00b7ner", "Stel\u00b7le", "stets"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie du Armer kleben.", "tokens": ["Wie", "du", "Ar\u00b7mer", "kle\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Nach dem Tode tr\u00e4get man", "tokens": ["Nach", "dem", "To\u00b7de", "tr\u00e4\u00b7get", "man"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich wohl auch ins Freie,", "tokens": ["Mich", "wohl", "auch", "ins", "Frei\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Aber ach! sie scharren dann", "tokens": ["A\u00b7ber", "ach", "!", "sie", "schar\u00b7ren", "dann"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein mich flugs aufs neue.", "tokens": ["Ein", "mich", "flugs", "aufs", "neu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPRART", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}