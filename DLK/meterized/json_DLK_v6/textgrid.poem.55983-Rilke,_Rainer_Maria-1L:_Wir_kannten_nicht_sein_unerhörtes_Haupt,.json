{"textgrid.poem.55983": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir kannten nicht sein unerh\u00f6rtes Haupt,", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir kannten nicht sein unerh\u00f6rtes Haupt,", "tokens": ["Wir", "kann\u00b7ten", "nicht", "sein", "un\u00b7er\u00b7h\u00f6r\u00b7tes", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "darin die Augen\u00e4pfel reiften. Aber", "tokens": ["da\u00b7rin", "die", "Au\u00b7gen\u00b7\u00e4p\u00b7fel", "reif\u00b7ten", ".", "A\u00b7ber"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PAV", "ART", "NN", "VVFIN", "$.", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "sein Torso gl\u00fcht noch wie ein Kandelaber,", "tokens": ["sein", "Tor\u00b7so", "gl\u00fcht", "noch", "wie", "ein", "Kan\u00b7de\u00b7la\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "in dem sein Schauen, nur zur\u00fcckgeschraubt,", "tokens": ["in", "dem", "sein", "Schau\u00b7en", ",", "nur", "zu\u00b7r\u00fcck\u00b7ge\u00b7schraubt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "sich h\u00e4lt und gl\u00e4nzt. Sonst k\u00f6nnte nicht der Bug", "tokens": ["sich", "h\u00e4lt", "und", "gl\u00e4nzt", ".", "Sonst", "k\u00f6nn\u00b7te", "nicht", "der", "Bug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "VVFIN", "KON", "VVFIN", "$.", "ADV", "VMFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der Brust dich blenden, und im leisen Drehen", "tokens": ["der", "Brust", "dich", "blen\u00b7den", ",", "und", "im", "lei\u00b7sen", "Dre\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,", "KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "der Lenden k\u00f6nnte nicht ein L\u00e4cheln gehen", "tokens": ["der", "Len\u00b7den", "k\u00f6nn\u00b7te", "nicht", "ein", "L\u00e4\u00b7cheln", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "zu jener Mitte, die die Zeugung trug.", "tokens": ["zu", "je\u00b7ner", "Mit\u00b7te", ",", "die", "die", "Zeu\u00b7gung", "trug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sonst st\u00fcnde dieser Stein entstellt und kurz", "tokens": ["Sonst", "st\u00fcn\u00b7de", "die\u00b7ser", "Stein", "ent\u00b7stellt", "und", "kurz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "VVPP", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "unter der Schultern durchsichtigem Sturz", "tokens": ["un\u00b7ter", "der", "Schul\u00b7tern", "durch\u00b7sich\u00b7ti\u00b7gem", "Sturz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "und flimmerte nicht so wie Raubtierfelle;", "tokens": ["und", "flim\u00b7mer\u00b7te", "nicht", "so", "wie", "Raub\u00b7tier\u00b7fel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "KOKOM", "NN", "$."], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "und br\u00e4che nicht aus allen seinen R\u00e4ndern", "tokens": ["und", "br\u00e4\u00b7che", "nicht", "aus", "al\u00b7len", "sei\u00b7nen", "R\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "aus wie ein Stern: denn da ist keine Stelle,", "tokens": ["aus", "wie", "ein", "Stern", ":", "denn", "da", "ist", "kei\u00b7ne", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "KOKOM", "ART", "NN", "$.", "KON", "ADV", "VAFIN", "PIAT", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "die dich nicht sieht. Du mu\u00dft dein Leben \u00e4ndern.", "tokens": ["die", "dich", "nicht", "sieht", ".", "Du", "mu\u00dft", "dein", "Le\u00b7ben", "\u00e4n\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVFIN", "$.", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}