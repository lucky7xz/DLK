{"textgrid.poem.40144": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Am Krankenbett", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Auge klagte, was ihr Mund verschwieg:", "tokens": ["Ihr", "Au\u00b7ge", "klag\u00b7te", ",", "was", "ihr", "Mund", "ver\u00b7schwieg", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ihr hilflos Leid, da\u00df Andre ", "tokens": ["ihr", "hil\u00b7flos", "Leid", ",", "da\u00df", "And\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJD", "NN", "$,", "KOUS", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir seufzten tr\u00fcb; aus unsern Blicken stieg", "tokens": ["Wir", "seufz\u00b7ten", "tr\u00fcb", ";", "aus", "un\u00b7sern", "Bli\u00b7cken", "stieg"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "verm\u00e4hlt ein angstvoll F\u00fcreinanderbitten.", "tokens": ["ver\u00b7m\u00e4hlt", "ein", "angst\u00b7voll", "F\u00fc\u00b7rein\u00b7an\u00b7der\u00b7bit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und als wir so zutiefst uns angeschaut,", "tokens": ["Und", "als", "wir", "so", "zu\u00b7tiefst", "uns", "an\u00b7ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "da war auf Einmal alles Leid verwunden,", "tokens": ["da", "war", "auf", "Ein\u00b7mal", "al\u00b7les", "Leid", "ver\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "der bange Seufzer schwoll zum Liebeslaut, \u2013", "tokens": ["der", "ban\u00b7ge", "Seuf\u00b7zer", "schwoll", "zum", "Lie\u00b7bes\u00b7laut", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPRART", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "wir hatten uns vergessen: uns empfunden ...", "tokens": ["wir", "hat\u00b7ten", "uns", "ver\u00b7ges\u00b7sen", ":", "uns", "emp\u00b7fun\u00b7den", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$.", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}