{"textgrid.poem.54259": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was ist wohl der Music und ihren Schall zu gleichen?", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist wohl der Music und ihren Schall zu gleichen?", "tokens": ["Was", "ist", "wohl", "der", "Mu\u00b7sic", "und", "ih\u00b7ren", "Schall", "zu", "glei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "NN", "KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nichts auf der gantzen Welt kan ihren Werth erreichen,", "tokens": ["Nichts", "auf", "der", "gant\u00b7zen", "Welt", "kan", "ih\u00b7ren", "Werth", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Ursprung ist ihr ja vom Himmel her bestimmt,", "tokens": ["Der", "Ur\u00b7sprung", "ist", "ihr", "ja", "vom", "Him\u00b7mel", "her", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APPRART", "NN", "APZR", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie bleibt dasjenige, was gar kein Ende nimmt.", "tokens": ["Sie", "bleibt", "das\u00b7je\u00b7ni\u00b7ge", ",", "was", "gar", "kein", "En\u00b7de", "nimmt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "$,", "PRELS", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es mu\u00df doch jede Kunst einst mit der Zeit vergehen,", "tokens": ["Es", "mu\u00df", "doch", "je\u00b7de", "Kunst", "einst", "mit", "der", "Zeit", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die aber bleibt auch noch in Ewigkeit bestehen,", "tokens": ["Die", "a\u00b7ber", "bleibt", "auch", "noch", "in", "E\u00b7wig\u00b7keit", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADV", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn in dem Himmel stimmt man Lied und Sayten an,", "tokens": ["Denn", "in", "dem", "Him\u00b7mel", "stimmt", "man", "Lied", "und", "Say\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PIS", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil sie die Ewigkeit gar nicht entbehren kan.", "tokens": ["Weil", "sie", "die", "E\u00b7wig\u00b7keit", "gar", "nicht", "ent\u00b7beh\u00b7ren", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Engel m\u00fcssen ja, wenn wir die Schrifft befragen,", "tokens": ["Die", "En\u00b7gel", "m\u00fcs\u00b7sen", "ja", ",", "wenn", "wir", "die", "Schrifft", "be\u00b7fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In Salem sich dereinst mit Instrumenten tragen,", "tokens": ["In", "Sa\u00b7lem", "sich", "de\u00b7reinst", "mit", "Inst\u00b7ru\u00b7men\u00b7ten", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PRF", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sie werden, wenn man h\u00f6rt das heilig, heilig, schreyn,", "tokens": ["Sie", "wer\u00b7den", ",", "wenn", "man", "h\u00f6rt", "das", "hei\u00b7lig", ",", "hei\u00b7lig", ",", "schreyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PIS", "VVFIN", "ART", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "In ihren Sayten-Spiel gantz unvergleichlich seyn,", "tokens": ["In", "ih\u00b7ren", "Say\u00b7ten\u00b7Spiel", "gantz", "un\u00b7ver\u00b7gleich\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sie will auch unsrer Welt gantz unentbehrlich heissen,", "tokens": ["Sie", "will", "auch", "uns\u00b7rer", "Welt", "gantz", "un\u00b7ent\u00b7behr\u00b7lich", "heis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ihr Zauber-Thon kan uns viel Nutz und Vortheil weissen.", "tokens": ["Ihr", "Zau\u00b7ber\u00b7Thon", "kan", "uns", "viel", "Nutz", "und", "Vor\u00b7theil", "weis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PIAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sieht sich ein banges Hertz von K\u00fcmmerni\u00df bestrickt,", "tokens": ["Sieht", "sich", "ein", "ban\u00b7ges", "Hertz", "von", "K\u00fcm\u00b7mer\u00b7ni\u00df", "be\u00b7strickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So wird es durch Music gleich wiederum erquickt.", "tokens": ["So", "wird", "es", "durch", "Mu\u00b7sic", "gleich", "wie\u00b7de\u00b7rum", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NE", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wenn dort die Raserey den Saul sucht zu verstellen,", "tokens": ["Wenn", "dort", "die", "Ra\u00b7se\u00b7rey", "den", "Saul", "sucht", "zu", "ver\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Kan Davids Harffen-Klang den Unmuth wieder f\u00e4llen.", "tokens": ["Kan", "Da\u00b7vids", "Ha\u00b7rf\u00b7fen\u00b7Klang", "den", "Un\u00b7muth", "wie\u00b7der", "f\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "NE", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Sie r\u00fchrt die Furien, denn durch der Sayten Krafft", "tokens": ["Sie", "r\u00fchrt", "die", "Fu\u00b7ri\u00b7en", ",", "denn", "durch", "der", "Say\u00b7ten", "Krafft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Hat Orpheus seine Frau dem Pluto weggerafft.", "tokens": ["Hat", "Or\u00b7pheus", "sei\u00b7ne", "Frau", "dem", "Plu\u00b7to", "weg\u00b7ge\u00b7rafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ihr Zephir! tragt di\u00df Blatt in des von N-- H\u00e4nden,", "tokens": ["Ihr", "Ze\u00b7phir", "!", "tragt", "di\u00df", "Blatt", "in", "des", "von", "N\u00b7", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PDS", "NN", "APPR", "ART", "APPR", "TRUNC", "NN", "$,"], "meter": "meter.parsing.error", "measure": "measure.parsing.error"}, "line.2": {"text": "Er soll mir alsobald darauf die Antwort senden,", "tokens": ["Er", "soll", "mir", "al\u00b7so\u00b7bald", "da\u00b7rauf", "die", "Ant\u00b7wort", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob seine Herkunfft auch noch fest beschlossen bleibt,", "tokens": ["Ob", "sei\u00b7ne", "Her\u00b7kunfft", "auch", "noch", "fest", "be\u00b7schlos\u00b7sen", "bleibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wenn er etwan nicht das: Fiat: darunter schreibt.", "tokens": ["Und", "wenn", "er", "et\u00b7wan", "nicht", "das", ":", "Fiat", ":", "da\u00b7run\u00b7ter", "schreibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PTKNEG", "PDS", "$.", "NE", "$.", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So solt ihr mit Gewalt ihn doch nach L-- tragen,", "tokens": ["So", "solt", "ihr", "mit", "Ge\u00b7walt", "ihn", "doch", "nach", "L\u00b7", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "PPER", "ADV", "APPR", "TRUNC", "VVINF", "$,"], "meter": "meter.parsing.error", "measure": "measure.parsing.error"}, "line.6": {"text": "Aurora, bring ihm selbst des Phoebus Ro\u00df und Wagen,", "tokens": ["Au\u00b7ro\u00b7ra", ",", "bring", "ihm", "selbst", "des", "Phoe\u00b7bus", "Ro\u00df", "und", "Wa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "ART", "NE", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bedeute selbigen und sag ihm di\u00df darbey,", "tokens": ["Be\u00b7deu\u00b7te", "sel\u00b7bi\u00b7gen", "und", "sag", "ihm", "di\u00df", "dar\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "KON", "VVFIN", "PPER", "PDS", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df seine Gegenwart gantz unentbehrlich sey.", "tokens": ["Da\u00df", "sei\u00b7ne", "Ge\u00b7gen\u00b7wart", "gantz", "un\u00b7ent\u00b7behr\u00b7lich", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}