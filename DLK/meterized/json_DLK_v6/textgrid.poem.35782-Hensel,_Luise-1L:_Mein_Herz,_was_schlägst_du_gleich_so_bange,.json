{"textgrid.poem.35782": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Herz, was schl\u00e4gst du gleich so bange,", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz, was schl\u00e4gst du gleich so bange,", "tokens": ["Mein", "Herz", ",", "was", "schl\u00e4gst", "du", "gleich", "so", "ban\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn dir der Vater Tr\u00fcbsal schickt?", "tokens": ["Wenn", "dir", "der", "Va\u00b7ter", "Tr\u00fcb\u00b7sal", "schickt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sei ruhig, Herz, es w\u00e4hrt nicht lange:", "tokens": ["Sei", "ru\u00b7hig", ",", "Herz", ",", "es", "w\u00e4hrt", "nicht", "lan\u00b7ge", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bald endet Alles, was dich dr\u00fcckt.", "tokens": ["Bald", "en\u00b7det", "Al\u00b7les", ",", "was", "dich", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Noch will in dir die Welt sich regen,", "tokens": ["Noch", "will", "in", "dir", "die", "Welt", "sich", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "PPER", "ART", "NN", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die manches junge Herz beth\u00f6rt:", "tokens": ["Die", "man\u00b7ches", "jun\u00b7ge", "Herz", "be\u00b7th\u00f6rt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die mu\u00dft du in ein Grabtuch legen,", "tokens": ["Die", "mu\u00dft", "du", "in", "ein", "Grab\u00b7tuch", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gesegnen all, was ihr geh\u00f6rt.", "tokens": ["Ge\u00b7seg\u00b7nen", "all", ",", "was", "ihr", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Bald lockt sie dich mit ihren Freuden,", "tokens": ["Bald", "lockt", "sie", "dich", "mit", "ih\u00b7ren", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bald droht sie Leid und Kummer dir;", "tokens": ["Bald", "droht", "sie", "Leid", "und", "Kum\u00b7mer", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie will von deinem Gott dich scheiden,", "tokens": ["Sie", "will", "von", "dei\u00b7nem", "Gott", "dich", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und stellt dir ihre G\u00f6tzen f\u00fcr.", "tokens": ["Und", "stellt", "dir", "ih\u00b7re", "G\u00f6t\u00b7zen", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du darfst dich nicht mit ihr vereinen;", "tokens": ["Du", "darfst", "dich", "nicht", "mit", "ihr", "ver\u00b7ei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df ihre vollen Rosen stehn,", "tokens": ["La\u00df", "ih\u00b7re", "vol\u00b7len", "Ro\u00b7sen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und siehe, wie die Lilien scheinen,", "tokens": ["Und", "sie\u00b7he", ",", "wie", "die", "Li\u00b7li\u00b7en", "schei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und h\u00f6re, wie die Palmen wehn.", "tokens": ["Und", "h\u00f6\u00b7re", ",", "wie", "die", "Pal\u00b7men", "wehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So sei, mein Herz, o sei zufrieden", "tokens": ["So", "sei", ",", "mein", "Herz", ",", "o", "sei", "zu\u00b7frie\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "PPOSAT", "NN", "$,", "FM", "VAFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Allem, was der Herr dir giebt,", "tokens": ["Mit", "Al\u00b7lem", ",", "was", "der", "Herr", "dir", "giebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und denke, von der Welt geschieden,", "tokens": ["Und", "den\u00b7ke", ",", "von", "der", "Welt", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gott pr\u00fcfet dich, weil Er dich liebt.", "tokens": ["Gott", "pr\u00fc\u00b7fet", "dich", ",", "weil", "Er", "dich", "liebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja, Vater! ich will still ergeben", "tokens": ["Ja", ",", "Va\u00b7ter", "!", "ich", "will", "still", "er\u00b7ge\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "$.", "PPER", "VMFIN", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit meiner B\u00fcrde weiter gehn,", "tokens": ["Mit", "mei\u00b7ner", "B\u00fcr\u00b7de", "wei\u00b7ter", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die H\u00e4nde fromm zu Dir erheben", "tokens": ["Die", "H\u00e4n\u00b7de", "fromm", "zu", "Dir", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und nicht auf diese Erde sehn.", "tokens": ["Und", "nicht", "auf", "die\u00b7se", "Er\u00b7de", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}