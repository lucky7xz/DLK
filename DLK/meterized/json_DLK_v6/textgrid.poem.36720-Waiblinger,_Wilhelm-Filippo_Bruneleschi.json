{"textgrid.poem.36720": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Filippo Bruneleschi", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herrliche Zeiten, da einst in geselligem Bunde die K\u00fcnste", "tokens": ["Herr\u00b7li\u00b7che", "Zei\u00b7ten", ",", "da", "einst", "in", "ge\u00b7sel\u00b7li\u00b7gem", "Bun\u00b7de", "die", "K\u00fcns\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KOUS", "ADV", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Sich in Einem zum Werk Aller so th\u00e4tig vereint!", "tokens": ["Sich", "in", "Ei\u00b7nem", "zum", "Werk", "Al\u00b7ler", "so", "th\u00e4\u00b7tig", "ver\u00b7eint", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "APPRART", "NN", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Deinem Florenz zu n\u00fctzen, hat Rom dich gelehrt, und dein Lohn war", "tokens": ["Dei\u00b7nem", "Flo\u00b7renz", "zu", "n\u00fct\u00b7zen", ",", "hat", "Rom", "dich", "ge\u00b7lehrt", ",", "und", "dein", "Lohn", "war"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,", "VAFIN", "NE", "PPER", "VVPP", "$,", "KON", "PPOSAT", "NN", "VAFIN"], "meter": "+-+--+-++--+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Nachruhm, dein Pantheon hast du in die L\u00fcfte gebaut.", "tokens": ["Nach\u00b7ruhm", ",", "dein", "Pan\u00b7the\u00b7on", "hast", "du", "in", "die", "L\u00fcf\u00b7te", "ge\u00b7baut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}}}}