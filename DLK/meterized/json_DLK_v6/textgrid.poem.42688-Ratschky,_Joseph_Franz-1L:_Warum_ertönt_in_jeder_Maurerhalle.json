{"textgrid.poem.42688": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Warum ert\u00f6nt in jeder Maurerhalle", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum ert\u00f6nt in jeder Maurerhalle", "tokens": ["Wa\u00b7rum", "er\u00b7t\u00f6nt", "in", "je\u00b7der", "Mau\u00b7rer\u00b7hal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der laute Ruf des Hammers? warum ziehn", "tokens": ["Der", "lau\u00b7te", "Ruf", "des", "Ham\u00b7mers", "?", "wa\u00b7rum", "ziehn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$.", "PWAV", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erwartungsvoll die scheuen Br\u00fcder alle", "tokens": ["Er\u00b7war\u00b7tungs\u00b7voll", "die", "scheu\u00b7en", "Br\u00fc\u00b7der", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu ihren Tempeln hin?", "tokens": ["Zu", "ih\u00b7ren", "Tem\u00b7peln", "hin", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Kam wiederum mit einer Hiobskunde", "tokens": ["Kam", "wie\u00b7de\u00b7rum", "mit", "ei\u00b7ner", "Hiobs\u00b7kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein banger Schwarm verjagter Br\u00fcder an?", "tokens": ["Ein", "ban\u00b7ger", "Schwarm", "ver\u00b7jag\u00b7ter", "Br\u00fc\u00b7der", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dr\u00e4ut irgendwo dem k\u00f6niglichen Bunde", "tokens": ["Dr\u00e4ut", "ir\u00b7gend\u00b7wo", "dem", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein neuer F\u00fcrstenbann?", "tokens": ["Ein", "neu\u00b7er", "F\u00fcrs\u00b7ten\u00b7bann", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Drang abermal sich eine ungeweihte", "tokens": ["Drang", "a\u00b7ber\u00b7mal", "sich", "ei\u00b7ne", "un\u00b7ge\u00b7weih\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PRF", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zelotenschaar in einen Maurerkreis", "tokens": ["Ze\u00b7lo\u00b7ten\u00b7schaar", "in", "ei\u00b7nen", "Mau\u00b7rer\u00b7kreis"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.3": {"text": "Wuthschnaubend ein, und gab des Tempels Beute", "tokens": ["Wuth\u00b7schnau\u00b7bend", "ein", ",", "und", "gab", "des", "Tem\u00b7pels", "Beu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ergrimmten Flammen preis?", "tokens": ["Er\u00b7grimm\u00b7ten", "Flam\u00b7men", "preis", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Riss wiederum die schon besiegte Hyder", "tokens": ["Riss", "wie\u00b7de\u00b7rum", "die", "schon", "be\u00b7sieg\u00b7te", "Hy\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des M\u00f6nchthums sich aus ihrer Kluft hervor,", "tokens": ["Des", "M\u00f6ncht\u00b7hums", "sich", "aus", "ih\u00b7rer", "Kluft", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und hob zur Rache wider unsre Br\u00fcder", "tokens": ["Und", "hob", "zur", "Ra\u00b7che", "wi\u00b7der", "uns\u00b7re", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die scharfe Klau' empor?", "tokens": ["Die", "schar\u00b7fe", "Klau'", "em\u00b7por", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nein, Br\u00fcder, bannt des Unmuths tr\u00fcbe Wolke", "tokens": ["Nein", ",", "Br\u00fc\u00b7der", ",", "bannt", "des", "Un\u00b7muths", "tr\u00fc\u00b7be", "Wol\u00b7ke"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "$,", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von eurer Stirn', und jauchzet! Josephs Mund", "tokens": ["Von", "eu\u00b7rer", "Stirn'", ",", "und", "jauch\u00b7zet", "!", "Jo\u00b7se\u00b7phs", "Mund"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$.", "NE", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "That feyerlich vor seinem ganzen Volke", "tokens": ["That", "fey\u00b7er\u00b7lich", "vor", "sei\u00b7nem", "gan\u00b7zen", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Uns Schutz und Duldung kund.", "tokens": ["Uns", "Schutz", "und", "Dul\u00b7dung", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ihr sch\u00fcchternen zerstreuten Maurerhorden,", "tokens": ["Ihr", "sch\u00fcch\u00b7ter\u00b7nen", "zer\u00b7streu\u00b7ten", "Mau\u00b7rer\u00b7hor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fasst neuen Muth! die Hand des Starken schlug", "tokens": ["Fasst", "neu\u00b7en", "Muth", "!", "die", "Hand", "des", "Star\u00b7ken", "schlug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "$.", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das ehrne Joch zu Tr\u00fcmmern, das der Orden", "tokens": ["Das", "ehr\u00b7ne", "Joch", "zu", "Tr\u00fcm\u00b7mern", ",", "das", "der", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In unserm Osten trug.", "tokens": ["In", "un\u00b7serm", "Os\u00b7ten", "trug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Verk\u00fcndigt es den Br\u00fcdern jeder Zone,", "tokens": ["Ver\u00b7k\u00fcn\u00b7digt", "es", "den", "Br\u00fc\u00b7dern", "je\u00b7der", "Zo\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dass unsern Bau, auf Menschenwohl gest\u00fctzt,", "tokens": ["Dass", "un\u00b7sern", "Bau", ",", "auf", "Men\u00b7schen\u00b7wohl", "ge\u00b7st\u00fctzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der gr\u00f6sste F\u00fcrst auf Deutschlands Kaiserthrone", "tokens": ["Der", "gr\u00f6ss\u00b7te", "F\u00fcrst", "auf", "Deutschlands", "Kai\u00b7ser\u00b7thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "NE"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit seinem Schilde sch\u00fctzt.", "tokens": ["Mit", "sei\u00b7nem", "Schil\u00b7de", "sch\u00fctzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ihm danken wir's, dass um des Tempels Schwelle", "tokens": ["Ihm", "dan\u00b7ken", "wir's", ",", "dass", "um", "des", "Tem\u00b7pels", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "VAFIN", "$,", "KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht mehr ein Schwarm verkappter H\u00e4scher irrt,", "tokens": ["Nicht", "mehr", "ein", "Schwarm", "ver\u00b7kapp\u00b7ter", "H\u00e4\u00b7scher", "irrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und nun nicht mehr, wie vormals, Sch\u00fcrz' und Kelle", "tokens": ["Und", "nun", "nicht", "mehr", ",", "wie", "vor\u00b7mals", ",", "Sch\u00fcrz'", "und", "Kel\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "PTKNEG", "ADV", "$,", "PWAV", "ADV", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Hasses Losung wird.", "tokens": ["Des", "Has\u00b7ses", "Lo\u00b7sung", "wird", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Zwar sch\u00e4umen drob, voll Galle, Zions W\u00e4chter,", "tokens": ["Zwar", "sch\u00e4u\u00b7men", "drob", ",", "voll", "Gal\u00b7le", ",", "Zi\u00b7ons", "W\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "ADJD", "NN", "$,", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die, Eulen gleich, den Strahl des Lichtes scheun,", "tokens": ["Die", ",", "Eu\u00b7len", "gleich", ",", "den", "Strahl", "des", "Lich\u00b7tes", "scheun", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "NN", "ADV", "$,", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und m\u00fchn sich, uns beym P\u00f6bel als Ver\u00e4chter", "tokens": ["Und", "m\u00fchn", "sich", ",", "uns", "beym", "P\u00f6\u00b7bel", "als", "Ver\u00b7\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PRF", "$,", "PPER", "APPRART", "NN", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Gottheit zu verschreyn.", "tokens": ["Der", "Got\u00b7theit", "zu", "ver\u00b7schreyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Doch, Br\u00fcder, scheut der Bonzen niemals m\u00fcde", "tokens": ["Doch", ",", "Br\u00fc\u00b7der", ",", "scheut", "der", "Bon\u00b7zen", "nie\u00b7mals", "m\u00fc\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "VVFIN", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erbittrung nicht! sie grinse, wie sie will!", "tokens": ["Er\u00b7bit\u00b7trung", "nicht", "!", "sie", "grin\u00b7se", ",", "wie", "sie", "will", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$.", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Fiel nicht vor Josephs schrecklicher \u00c4gide", "tokens": ["Fiel", "nicht", "vor", "Jo\u00b7se\u00b7phs", "schreck\u00b7li\u00b7cher", "\u00c4\u00b7gi\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "NE", "ADJA", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Manch st\u00e4rkres Krokodill?", "tokens": ["Manch", "st\u00e4r\u00b7kres", "Kro\u00b7ko\u00b7dill", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Bleibt standhaft! zeigt, dass wir in Josephs Staaten", "tokens": ["Bleibt", "stand\u00b7haft", "!", "zeigt", ",", "dass", "wir", "in", "Jo\u00b7se\u00b7phs", "Staa\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "$.", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NE", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vor Tausenden des Schutzes w\u00fcrdig sind,", "tokens": ["Vor", "Tau\u00b7sen\u00b7den", "des", "Schut\u00b7zes", "w\u00fcr\u00b7dig", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und machet euch durch \u00e4chte Maurerthaten", "tokens": ["Und", "ma\u00b7chet", "euch", "durch", "\u00e4ch\u00b7te", "Mau\u00b7rer\u00b7tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um seine Huld verdient!", "tokens": ["Um", "sei\u00b7ne", "Huld", "ver\u00b7dient", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Beweist es laut, dass euern fesselfreyen", "tokens": ["Be\u00b7weist", "es", "laut", ",", "dass", "eu\u00b7ern", "fes\u00b7sel\u00b7fre\u00b7yen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erhabnen Blick des Lichtes Glanz umschwebt,", "tokens": ["Er\u00b7hab\u00b7nen", "Blick", "des", "Lich\u00b7tes", "Glanz", "um\u00b7schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und nach dem Tand verj\u00e4hrter Gaukeleyen", "tokens": ["Und", "nach", "dem", "Tand", "ver\u00b7j\u00e4hr\u00b7ter", "Gau\u00b7ke\u00b7le\u00b7yen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Kein heller Maurer strebt!", "tokens": ["Kein", "hel\u00b7ler", "Mau\u00b7rer", "strebt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Lasst Weisheit, Lieb' und Tugend st\u00e4ts euch leiten!", "tokens": ["Lasst", "Weis\u00b7heit", ",", "Lieb'", "und", "Tu\u00b7gend", "st\u00e4ts", "euch", "lei\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "KON", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann, Br\u00fcder, dann wird unser Bund gedeihn,", "tokens": ["Dann", ",", "Br\u00fc\u00b7der", ",", "dann", "wird", "un\u00b7ser", "Bund", "ge\u00b7deihn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und einst noch in den fernsten Afterzeiten", "tokens": ["Und", "einst", "noch", "in", "den", "ferns\u00b7ten", "Af\u00b7ter\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Menschheit Segen seyn.", "tokens": ["Der", "Menschheit", "Se\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}