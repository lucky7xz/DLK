{"textgrid.poem.64067": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Furcht und Glaube", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn zwischen uns sich weite Fernen dehnen,", "tokens": ["Wenn", "zwi\u00b7schen", "uns", "sich", "wei\u00b7te", "Fer\u00b7nen", "deh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "PRF", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein milder Strahl des Wiedersehens glimmt,", "tokens": ["Kein", "mil\u00b7der", "Strahl", "des", "Wie\u00b7der\u00b7se\u00b7hens", "glimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und in des Busens tief verschlossnem Sehnen", "tokens": ["Und", "in", "des", "Bu\u00b7sens", "tief", "ver\u00b7schloss\u00b7nem", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Herz der Hoffnung Stimme nicht vernimmt;", "tokens": ["Mein", "Herz", "der", "Hoff\u00b7nung", "Stim\u00b7me", "nicht", "ver\u00b7nimmt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dann h\u00fcllt die Furcht, die leis' in mir erwacht,", "tokens": ["Dann", "h\u00fcllt", "die", "Furcht", ",", "die", "leis'", "in", "mir", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die ganze Welt mir in der Schwermuth Nacht.", "tokens": ["Die", "gan\u00b7ze", "Welt", "mir", "in", "der", "Schwer\u00b7muth", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch, wenn ich still mein liebend Herz befrage,", "tokens": ["Doch", ",", "wenn", "ich", "still", "mein", "lie\u00b7bend", "Herz", "be\u00b7fra\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADJD", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann flieht der Zweifel, ob Du mein gekenkst.", "tokens": ["Dann", "flieht", "der", "Zwei\u00b7fel", ",", "ob", "Du", "mein", "ge\u00b7kenkst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie Geisterhauch umweht mich Deine Klage", "tokens": ["Wie", "Geis\u00b7ter\u00b7hauch", "um\u00b7weht", "mich", "Dei\u00b7ne", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Seufzern, die Du meinem Schicksal schenkst,", "tokens": ["In", "Seuf\u00b7zern", ",", "die", "Du", "mei\u00b7nem", "Schick\u00b7sal", "schenkst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und schnell verscheucht mein Glaube, ohne Wanken,", "tokens": ["Und", "schnell", "ver\u00b7scheucht", "mein", "Glau\u00b7be", ",", "oh\u00b7ne", "Wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Argwohns bange, zagende Gedanken.", "tokens": ["Des", "Arg\u00b7wohns", "ban\u00b7ge", ",", "za\u00b7gen\u00b7de", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}