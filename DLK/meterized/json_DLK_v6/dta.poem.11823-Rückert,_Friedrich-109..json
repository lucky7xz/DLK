{"dta.poem.11823": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "109.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer mit geschickter Hand die heilge Schrift abschreibt,", "tokens": ["Wer", "mit", "ge\u00b7schick\u00b7ter", "Hand", "die", "heil\u00b7ge", "Schrift", "ab\u00b7schreibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Zweifel ist da\u00df er ein fromm Gesch\u00e4ft betreibt.", "tokens": ["Kein", "Zwei\u00b7fel", "ist", "da\u00df", "er", "ein", "fromm", "Ge\u00b7sch\u00e4ft", "be\u00b7treibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "KOUS", "PPER", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn an der Abschrift kann ein Frommer sich erbaun,", "tokens": ["Denn", "an", "der", "Ab\u00b7schrift", "kann", "ein", "From\u00b7mer", "sich", "er\u00b7baun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich freuen Gottes Wort so klar vor sich zu schaun.", "tokens": ["Sich", "freu\u00b7en", "Got\u00b7tes", "Wort", "so", "klar", "vor", "sich", "zu", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "NN", "ADV", "ADJD", "APPR", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch wenn der Schreiber selbst nichts weiter thut wan schreiben,", "tokens": ["Doch", "wenn", "der", "Schrei\u00b7ber", "selbst", "nichts", "wei\u00b7ter", "thut", "wan", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "PIS", "ADV", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wird, was andern frommt, ihm selbst unfruchtbar bleiben.", "tokens": ["So", "wird", ",", "was", "an\u00b7dern", "frommt", ",", "ihm", "selbst", "un\u00b7frucht\u00b7bar", "blei\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PRELS", "PIS", "VVFIN", "$,", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und also, wenn du machst dein eignes Seyn und Leben", "tokens": ["Und", "al\u00b7so", ",", "wenn", "du", "machst", "dein", "eig\u00b7nes", "Seyn", "und", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu einem sch\u00f6nen Buch, um es der Welt zu geben;", "tokens": ["Zu", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Buch", ",", "um", "es", "der", "Welt", "zu", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KOUI", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wenn es auch alle Welt mit Lust und Andacht schaut,", "tokens": ["Wenn", "es", "auch", "al\u00b7le", "Welt", "mit", "Lust", "und", "An\u00b7dacht", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was n\u00fctzt es dir, wenn es dich selber nicht erbaut?", "tokens": ["Was", "n\u00fctzt", "es", "dir", ",", "wenn", "es", "dich", "sel\u00b7ber", "nicht", "er\u00b7baut", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "PRF", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}