{"textgrid.poem.53110": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wend' au\u00df hochbetr\u00fcbtem Hertzen", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wend' au\u00df hochbetr\u00fcbtem Hertzen", "tokens": ["Ich", "wend'", "au\u00df", "hoch\u00b7be\u00b7tr\u00fcb\u00b7tem", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein' Augen auff die Berge zu,", "tokens": ["Mein'", "Au\u00b7gen", "auff", "die", "Ber\u00b7ge", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob ich von dannen H\u00fclff vnd Ruh", "tokens": ["Ob", "ich", "von", "dan\u00b7nen", "H\u00fclff", "vnd", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu hoffen h\u00e4tt' in meinem Schmertzen.", "tokens": ["Zu", "hof\u00b7fen", "h\u00e4tt'", "in", "mei\u00b7nem", "Schmert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "K\u00f6mpt, sag ich, k\u00f6mpt mir H\u00fclffe? nein.", "tokens": ["K\u00f6mpt", ",", "sag", "ich", ",", "k\u00f6mpt", "mir", "H\u00fclf\u00b7fe", "?", "nein", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "NN", "$.", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein H\u00fclff ist Gott der Herr allein.", "tokens": ["Mein", "H\u00fclff", "ist", "Gott", "der", "Herr", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Herr des Himmels vnd der Erden,", "tokens": ["Der", "Herr", "des", "Him\u00b7mels", "vnd", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch dessen Wort die Welt mu\u00df stehn,", "tokens": ["Durch", "des\u00b7sen", "Wort", "die", "Welt", "mu\u00df", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er schaffet, da\u00df dein Fu\u00df im gehn", "tokens": ["Er", "schaf\u00b7fet", ",", "da\u00df", "dein", "Fu\u00df", "im", "gehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "APPRART", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht gleite, noch gef\u00e4llt mag werden.", "tokens": ["Nicht", "glei\u00b7te", ",", "noch", "ge\u00b7f\u00e4llt", "mag", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "$,", "ADV", "VVPP", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er h\u00fctet dein, vnd sein Gesicht", "tokens": ["Er", "h\u00fc\u00b7tet", "dein", ",", "vnd", "sein", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Entschl\u00e4fft dar\u00fcber ewig nicht.", "tokens": ["Ent\u00b7schl\u00e4fft", "da\u00b7r\u00fc\u00b7ber", "e\u00b7wig", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er ist, der Schutz helt seinen Knechten,", "tokens": ["Er", "ist", ",", "der", "Schutz", "helt", "sei\u00b7nen", "Knech\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein schlaf noch schlummern nimpt ihn ein:", "tokens": ["Kein", "schlaf", "noch", "schlum\u00b7mern", "nimpt", "ihn", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVINF", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er ist zuwieder aller Pein,", "tokens": ["Er", "ist", "zu\u00b7wie\u00b7der", "al\u00b7ler", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Schatten vber deiner Rechten,", "tokens": ["Ein", "Schat\u00b7ten", "vber", "dei\u00b7ner", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Damit die Sonne dich den Tag,", "tokens": ["Da\u00b7mit", "die", "Son\u00b7ne", "dich", "den", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Nacht der Mond nicht stechen mag.", "tokens": ["Die", "Nacht", "der", "Mond", "nicht", "ste\u00b7chen", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "ART", "NN", "PTKNEG", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Kein Vbels wird dir wiederfahren,", "tokens": ["Kein", "Vbels", "wird", "dir", "wie\u00b7der\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Der Herr wird deine Seele dir,", "tokens": ["Der", "Herr", "wird", "dei\u00b7ne", "See\u00b7le", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch Au\u00df- vnd Eingang f\u00fcr vnd f\u00fcr", "tokens": ["Auch", "Au\u00df", "vnd", "Ein\u00b7gang", "f\u00fcr", "vnd", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "TRUNC", "KON", "NN", "APPR", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In allem, was du thust, bewahren.", "tokens": ["In", "al\u00b7lem", ",", "was", "du", "thust", ",", "be\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wird dich gesegnen jederzeit,", "tokens": ["Wird", "dich", "ge\u00b7seg\u00b7nen", "je\u00b7der\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von nun an bi\u00df in Ewigkeit.", "tokens": ["Von", "nun", "an", "bi\u00df", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}