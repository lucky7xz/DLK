{"textgrid.poem.38774": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[das war es, was mir Ahndung wollte sagen]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war es, was mir Ahndung wollte sagen,", "tokens": ["Das", "war", "es", ",", "was", "mir", "Ahn\u00b7dung", "woll\u00b7te", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PWS", "PPER", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das bange Herz, das heimlich oft im Beben", "tokens": ["Das", "ban\u00b7ge", "Herz", ",", "das", "heim\u00b7lich", "oft", "im", "Be\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir eine treue Warnung hat gegeben:", "tokens": ["Mir", "ei\u00b7ne", "treu\u00b7e", "War\u00b7nung", "hat", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du sollst, du sollst noch nicht dein Letztes wagen.", "tokens": ["Du", "sollst", ",", "du", "sollst", "noch", "nicht", "dein", "Letz\u00b7tes", "wa\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Welch Kind hab' ich empfangen und getragen!", "tokens": ["Welch", "Kind", "hab'", "ich", "emp\u00b7fan\u00b7gen", "und", "ge\u00b7tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der gr\u00f6\u00dfte Schmerz f\u00fchrt schon in mir sein Leben,", "tokens": ["Der", "gr\u00f6\u00df\u00b7te", "Schmerz", "f\u00fchrt", "schon", "in", "mir", "sein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bald wird er rei\u00dfend nach dem Lichte streben,", "tokens": ["Bald", "wird", "er", "rei\u00b7\u00dfend", "nach", "dem", "Lich\u00b7te", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann wird das matte Herz von ihm zerschlagen.", "tokens": ["Dann", "wird", "das", "mat\u00b7te", "Herz", "von", "ihm", "zer\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So blute denn mit Freuden, Todeswunde,", "tokens": ["So", "blu\u00b7te", "denn", "mit", "Freu\u00b7den", ",", "To\u00b7des\u00b7wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchl' noch, o Herz, im Schmerz die lichten Blicke,", "tokens": ["F\u00fchl'", "noch", ",", "o", "Herz", ",", "im", "Schmerz", "die", "lich\u00b7ten", "Bli\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "FM", "NN", "$,", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das s\u00fc\u00dfe L\u00e4cheln, h\u00f6re noch die T\u00f6ne,", "tokens": ["Das", "s\u00fc\u00b7\u00dfe", "L\u00e4\u00b7cheln", ",", "h\u00f6\u00b7re", "noch", "die", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Durchdringt dich ganz im Tiefsten, welche Sch\u00f6ne", "tokens": ["Durch\u00b7dringt", "dich", "ganz", "im", "Tiefs\u00b7ten", ",", "wel\u00b7che", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$,", "PWAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aufstrahlt' im L\u00e4cheln, Klang, zum Liebesgl\u00fccke, \u2013", "tokens": ["Auf\u00b7strahlt'", "im", "L\u00e4\u00b7cheln", ",", "Klang", ",", "zum", "Lie\u00b7bes\u00b7gl\u00fc\u00b7cke", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "NN", "$,", "APPRART", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann f\u00fchl' dein Elend, brich zur selben Stunde!", "tokens": ["Dann", "f\u00fchl'", "dein", "E\u00b7lend", ",", "brich", "zur", "sel\u00b7ben", "Stun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}