{"textgrid.poem.65992": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: K\u00fchn wollt' auch ich nach Ruhm und Ehren fliegen,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00fchn wollt' auch ich nach Ruhm und Ehren fliegen,", "tokens": ["K\u00fchn", "wollt'", "auch", "ich", "nach", "Ruhm", "und", "Eh\u00b7ren", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Sonne nah in reinem Glanz mich wiegen,", "tokens": ["Der", "Son\u00b7ne", "nah", "in", "rei\u00b7nem", "Glanz", "mich", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo k\u00f6nigliche V\u00f6gel einsam schweben.", "tokens": ["Wo", "k\u00f6\u00b7nig\u00b7li\u00b7che", "V\u00f6\u00b7gel", "ein\u00b7sam", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nun fesselt mir ein einziger Wunsch die Schwingen:", "tokens": ["Nun", "fes\u00b7selt", "mir", "ein", "ein\u00b7zi\u00b7ger", "Wunsch", "die", "Schwin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Zu deinen F\u00fc\u00dfen sanft mein Lied zu singen", "tokens": ["Zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "sanft", "mein", "Lied", "zu", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und meine Seele ganz dir hinzugeben.", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "ganz", "dir", "hin\u00b7zu\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}