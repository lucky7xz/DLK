{"dta.poem.917": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "An DIE SCHWESTER", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Wo du gehst wird Herbst und Abend,", "tokens": ["Wo", "du", "gehst", "wird", "Herbst", "und", "A\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blaues Wild, das unter B\u00e4umen t\u00f6nt,", "tokens": ["Blau\u00b7es", "Wild", ",", "das", "un\u00b7ter", "B\u00e4u\u00b7men", "t\u00f6nt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Einsamer Weiher am Abend.", "tokens": ["Ein\u00b7sa\u00b7mer", "Wei\u00b7her", "am", "A\u00b7bend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Leise der Flug der V\u00f6gel t\u00f6nt,", "tokens": ["Lei\u00b7se", "der", "Flug", "der", "V\u00f6\u00b7gel", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Die Schwermut \u00fcber deinen Augenbogen.", "tokens": ["Die", "Schwer\u00b7mut", "\u00fc\u00b7ber", "dei\u00b7nen", "Au\u00b7gen\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein schmales L\u00e4cheln t\u00f6nt.", "tokens": ["Dein", "schma\u00b7les", "L\u00e4\u00b7cheln", "t\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gott hat deine Lider verbogen.", "tokens": ["Gott", "hat", "dei\u00b7ne", "Li\u00b7der", "ver\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Sterne suchen nachts, Karfreitagskind,", "tokens": ["Ster\u00b7ne", "su\u00b7chen", "nachts", ",", "Kar\u00b7frei\u00b7tags\u00b7kind", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Deinen Stirnenbogen.", "tokens": ["Dei\u00b7nen", "Stir\u00b7nen\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}