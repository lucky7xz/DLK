{"textgrid.poem.54183": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Heute zwischen Gestern und Morgen", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Gestern und Morgen", "tokens": ["Wie", "Ge\u00b7stern", "und", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "sich m\u00e4chtig vermischen!", "tokens": ["sich", "m\u00e4ch\u00b7tig", "ver\u00b7mi\u00b7schen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Hier ein Stuhl \u2013 da ein Stuhl \u2013", "tokens": ["Hier", "ein", "Stuhl", "\u2013", "da", "ein", "Stuhl", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$(", "ADV", "ART", "NN", "$("], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "und wir immer dazwischen!", "tokens": ["und", "wir", "im\u00b7mer", "da\u00b7zwi\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PAV", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Liebliche Veilchen im M\u00e4rz \u2013", "tokens": ["Lieb\u00b7li\u00b7che", "Veil\u00b7chen", "im", "M\u00e4rz", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Nicht mehr.", "tokens": ["Nicht", "mehr", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Proletarier-Staat mit Herz \u2013", "tokens": ["Pro\u00b7le\u00b7ta\u00b7ri\u00b7er\u00b7Staat", "mit", "Herz", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Noch nicht.", "tokens": ["Noch", "nicht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKNEG", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Noch ist es nicht so weit.", "tokens": ["Noch", "ist", "es", "nicht", "so", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Denn wir leben \u2013", "tokens": ["Denn", "wir", "le\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "denn wir leben", "tokens": ["denn", "wir", "le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "in einer \u00dcbergangszeit \u2013!", "tokens": ["in", "ei\u00b7ner", "\u00dc\u00b7ber\u00b7gangs\u00b7zeit", "\u2013", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$(", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Geplappertes A \u2013 B \u2013 C", "tokens": ["Ge\u00b7plap\u00b7per\u00b7tes", "A", "\u2013", "B", "\u2013", "C"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "NE", "$(", "XY", "$(", "XY"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "bei den alten Semestern.", "tokens": ["bei", "den", "al\u00b7ten", "Se\u00b7mes\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Fraternit\u00e9 