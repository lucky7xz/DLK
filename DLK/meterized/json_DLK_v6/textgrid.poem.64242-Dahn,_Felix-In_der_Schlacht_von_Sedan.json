{"textgrid.poem.64242": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "In der Schlacht von Sedan", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kracht aus tausend Feuerschl\u00fcnden:", "tokens": ["Es", "kracht", "aus", "tau\u00b7send", "Feu\u00b7er\u00b7schl\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Erde bebt in ihren Gr\u00fcnden:", "tokens": ["Die", "Er\u00b7de", "bebt", "in", "ih\u00b7ren", "Gr\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es jauchzt mein Herz. \u2013 Wie gro\u00df! Wie hehr!", "tokens": ["Es", "jauchzt", "mein", "Herz", ".", "\u2013", "Wie", "gro\u00df", "!", "Wie", "hehr", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "$(", "PWAV", "ADJD", "$.", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt stirb: du lebst nichts Gleiches mehr.", "tokens": ["Jetzt", "stirb", ":", "du", "lebst", "nichts", "Glei\u00b7ches", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "PPER", "VVFIN", "PIS", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}