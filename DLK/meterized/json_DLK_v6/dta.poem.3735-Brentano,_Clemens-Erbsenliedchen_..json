{"dta.poem.3735": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Erbsenliedchen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gieb mir eine Erbse.               ", "tokens": ["Gieb", "mir", "ei\u00b7ne", "Erb\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "\u201eich habe keine, \u201c", "tokens": ["\u201e", "ich", "ha\u00b7be", "kei\u00b7ne", ",", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "PIAT", "$,", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Geh zum M\u00fcller, und hol dir eine.", "tokens": ["Geh", "zum", "M\u00fcl\u00b7ler", ",", "und", "hol", "dir", "ei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NE", "$,", "KON", "VVFIN", "PPER", "ART", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "\u201eer giebt mir keine.\u201c", "tokens": ["\u201e", "er", "giebt", "mir", "kei\u00b7ne", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PIAT", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "So such dir eine.", "tokens": ["So", "such", "dir", "ei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "\u201eich finde keine.\u201c", "tokens": ["\u201e", "ich", "fin\u00b7de", "kei\u00b7ne", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PIAT", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "So blas ich dich.", "tokens": ["So", "blas", "ich", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "\u201eso wehr ich mich.", "tokens": ["\u201e", "so", "wehr", "ich", "mich", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "PPER", "PRF", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}