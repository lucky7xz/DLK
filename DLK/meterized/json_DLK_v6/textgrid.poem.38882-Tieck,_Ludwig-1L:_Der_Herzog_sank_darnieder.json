{"textgrid.poem.38882": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Herzog sank darnieder", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herzog sank darnieder", "tokens": ["Der", "Her\u00b7zog", "sank", "dar\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "PAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im wilden dunklen Hain,", "tokens": ["Im", "wil\u00b7den", "dunk\u00b7len", "Hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da nahm Held Eckart bieder", "tokens": ["Da", "nahm", "Held", "E\u00b7ckart", "bie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "NE", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihn auf die Schultern sein.", "tokens": ["Ihn", "auf", "die", "Schul\u00b7tern", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er sprach: gar viel Beschwerden", "tokens": ["Er", "sprach", ":", "gar", "viel", "Be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mach' ich dir, guter Mann;", "tokens": ["Mach'", "ich", "dir", ",", "gu\u00b7ter", "Mann", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der sagte: auf der Erden", "tokens": ["Der", "sag\u00b7te", ":", "auf", "der", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mu\u00df man gar viel bestahn.", "tokens": ["Mu\u00df", "man", "gar", "viel", "be\u00b7stahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "ADV", "VVPP", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.3": {"line.1": {"text": "Doch sollst du, sprach Burgund,", "tokens": ["Doch", "sollst", "du", ",", "sprach", "Bur\u00b7gund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "VVFIN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dich freun, bei meinem Worte,", "tokens": ["Dich", "freun", ",", "bei", "mei\u00b7nem", "Wor\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Komm ich nur erst gesund", "tokens": ["Komm", "ich", "nur", "erst", "ge\u00b7sund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zu Haus und sichern Orte.", "tokens": ["Zu", "Haus", "und", "si\u00b7chern", "Or\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Held f\u00fchlt Thr\u00e4nen hei\u00df", "tokens": ["Der", "Held", "f\u00fchlt", "Thr\u00e4\u00b7nen", "hei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auf seinen alten Wangen,", "tokens": ["Auf", "sei\u00b7nen", "al\u00b7ten", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er sprach: auf keine Weis'", "tokens": ["Er", "sprach", ":", "auf", "kei\u00b7ne", "Weis'"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Trag' ich nach Lohn Verlangen.", "tokens": ["Trag'", "ich", "nach", "Lohn", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es mehren sich die Plagen,", "tokens": ["Es", "meh\u00b7ren", "sich", "die", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sprach der Burgund in Noth;", "tokens": ["Sprach", "der", "Bur\u00b7gund", "in", "Noth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wohin willst du mich tragen?", "tokens": ["Wo\u00b7hin", "willst", "du", "mich", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du bist wohl gar der Tod? \u2013", "tokens": ["Du", "bist", "wohl", "gar", "der", "Tod", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Tod bin ich nicht genannt,", "tokens": ["Tod", "bin", "ich", "nicht", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sprach Eckart noch im Weinen,", "tokens": ["Sprach", "E\u00b7ckart", "noch", "im", "Wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du stehst in Gottes Hand,", "tokens": ["Du", "stehst", "in", "Got\u00b7tes", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sein Licht mag dich bescheinen.", "tokens": ["Sein", "Licht", "mag", "dich", "be\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ach, wohl ist mir bewu\u00dft,", "tokens": ["Ach", ",", "wohl", "ist", "mir", "be\u00b7wu\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sprach jener drauf in Reue,", "tokens": ["Sprach", "je\u00b7ner", "drauf", "in", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "PAV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df s\u00fcndvoll meine Brust,", "tokens": ["Da\u00df", "s\u00fcnd\u00b7voll", "mei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Drum zittr' ich, da\u00df er dr\u00e4ue.", "tokens": ["Drum", "zit\u00b7tr'", "ich", ",", "da\u00df", "er", "dr\u00e4u\u00b7e", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Ich hab' dem treusten Freunde", "tokens": ["Ich", "hab'", "dem", "treus\u00b7ten", "Freun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Kinder umgebracht,", "tokens": ["Die", "Kin\u00b7der", "um\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum steht er mir zum Feinde", "tokens": ["Drum", "steht", "er", "mir", "zum", "Fein\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In dieser finstern Nacht.", "tokens": ["In", "die\u00b7ser", "fins\u00b7tern", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Er war mir recht ergeben,", "tokens": ["Er", "war", "mir", "recht", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als wie der treuste Knecht,", "tokens": ["Als", "wie", "der", "treus\u00b7te", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und war im ganzen Leben", "tokens": ["Und", "war", "im", "gan\u00b7zen", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir niemals ungerecht.", "tokens": ["Mir", "nie\u00b7mals", "un\u00b7ge\u00b7recht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Die Kindlein lie\u00df ich t\u00f6dten,", "tokens": ["Die", "Kin\u00b7dlein", "lie\u00df", "ich", "t\u00f6d\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das kann er nie verzeihn,", "tokens": ["Das", "kann", "er", "nie", "ver\u00b7zeihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich f\u00fcrcht', in diesen N\u00f6then", "tokens": ["Ich", "f\u00fcrcht'", ",", "in", "die\u00b7sen", "N\u00f6\u00b7then"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Treff' ich ihn hier im Hain:", "tokens": ["Treff'", "ich", "ihn", "hier", "im", "Hain", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Das sagt mir mein Gewissen", "tokens": ["Das", "sagt", "mir", "mein", "Ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Herze innerlich,", "tokens": ["Mein", "Her\u00b7ze", "in\u00b7ner\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Kind hab ich zerrissen,", "tokens": ["Die", "Kind", "hab", "ich", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Daf\u00fcr zerrei\u00dft er mich.", "tokens": ["Da\u00b7f\u00fcr", "zer\u00b7rei\u00dft", "er", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Der Eckart sprach: empfinden", "tokens": ["Der", "E\u00b7ckart", "sprach", ":", "emp\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mu\u00df ich so schwere Last,", "tokens": ["Mu\u00df", "ich", "so", "schwe\u00b7re", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil du nicht rein von S\u00fcnden", "tokens": ["Weil", "du", "nicht", "rein", "von", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und schwer gefrevelt hast,", "tokens": ["Und", "schwer", "ge\u00b7fre\u00b7velt", "hast", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Da\u00df du den Mann wirst schauen", "tokens": ["Da\u00df", "du", "den", "Mann", "wirst", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist auch gewi\u00dflich wahr,", "tokens": ["Ist", "auch", "ge\u00b7wi\u00df\u00b7lich", "wahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch magst du mir vertrauen", "tokens": ["Doch", "magst", "du", "mir", "ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So kr\u00fcmmt er dir kein Haar.", "tokens": ["So", "kr\u00fcmmt", "er", "dir", "kein", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}