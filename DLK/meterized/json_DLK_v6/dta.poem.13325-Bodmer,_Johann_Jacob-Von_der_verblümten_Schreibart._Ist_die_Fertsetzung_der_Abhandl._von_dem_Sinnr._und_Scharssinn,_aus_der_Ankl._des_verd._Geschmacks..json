{"dta.poem.13325": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von der verbl\u00fcmten Schreibart. Ist die Fertsetzung der Abhandl. von dem Sinnr.  \n und Scharssinn, aus der Ankl. des verd. Geschmacks.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198519", "language": ["de:0.71", "da:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er kan den Augen kaum den freyen ", "tokens": ["Er", "kan", "den", "Au\u00b7gen", "kaum", "den", "frey\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Zweifel ob er auch soll dem Gesichte glauben.", "tokens": ["Aus", "Zwei\u00b7fel", "ob", "er", "auch", "soll", "dem", "Ge\u00b7sich\u00b7te", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KOUS", "PPER", "ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}