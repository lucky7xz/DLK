{"textgrid.poem.46464": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Unter Tag und Tagen,", "genre": "verse", "period": "N.A.", "pub_year": 1904, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter Tag und Tagen,", "tokens": ["Un\u00b7ter", "Tag", "und", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Tief, tief im Erdefleisch, wo kein Sonnlicht glei\u00dft,", "tokens": ["Tief", ",", "tief", "im", "E\u00b7rde\u00b7fleisch", ",", "wo", "kein", "Sonn\u00b7licht", "glei\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "APPRART", "NN", "$,", "PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Tief in eingebohrten Sch\u00e4chten, vorgetriebenen Stollen,", "tokens": ["Tief", "in", "ein\u00b7ge\u00b7bohr\u00b7ten", "Sch\u00e4ch\u00b7ten", ",", "vor\u00b7ge\u00b7trie\u00b7be\u00b7nen", "Stol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Wo die L\u00e4mpchen wandern, Glocken schrillen, Hacken schlagen:", "tokens": ["Wo", "die", "L\u00e4mp\u00b7chen", "wan\u00b7dern", ",", "Glo\u00b7cken", "schril\u00b7len", ",", "Ha\u00b7cken", "schla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "NN", "VVINF", "$,", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Da lebt die Kraft, die M\u00e4nnerm\u00fch und Kohlenschollen,", "tokens": ["Da", "lebt", "die", "Kraft", ",", "die", "M\u00e4n\u00b7ner\u00b7m\u00fch", "und", "Koh\u00b7len\u00b7schol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Minensch\u00fcsse, H\u00e4mmern, Kohlenwagenrollen", "tokens": ["Mi\u00b7nen\u00b7sch\u00fcs\u00b7se", ",", "H\u00e4m\u00b7mern", ",", "Koh\u00b7len\u00b7wa\u00b7gen\u00b7rol\u00b7len"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Zu Einem dunklen, unruhvollen Rhythmus schwei\u00dft:", "tokens": ["Zu", "Ei\u00b7nem", "dunk\u00b7len", ",", "un\u00b7ruh\u00b7vol\u00b7len", "Rhyth\u00b7mus", "schwei\u00dft", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Weltgeist!", "tokens": ["Der", "Welt\u00b7geist", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Und oben, wo im Hafen L\u00e4rm und Handel branden,", "tokens": ["Und", "o\u00b7ben", ",", "wo", "im", "Ha\u00b7fen", "L\u00e4rm", "und", "Han\u00b7del", "bran\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "APPRART", "NN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo der Handel Menschen, Geld und G\u00fcter durcheinander schmei\u00dft,", "tokens": ["Wo", "der", "Han\u00b7del", "Men\u00b7schen", ",", "Geld", "und", "G\u00fc\u00b7ter", "durch\u00b7ein\u00b7an\u00b7der", "schmei\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,", "NN", "KON", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wo Docks und Hellinge Schiffsbauten umkrampfen,", "tokens": ["Wo", "Docks", "und", "Hel\u00b7lin\u00b7ge", "Schiffs\u00b7bau\u00b7ten", "um\u00b7kramp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "NN", "VVINF", "$,"], "meter": "-+-++-++--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wo die Auslanddampfer Riesenfrachten landen, \u2013", "tokens": ["Wo", "die", "Aus\u00b7land\u00b7damp\u00b7fer", "Rie\u00b7sen\u00b7frach\u00b7ten", "lan\u00b7den", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVINF", "$,", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Oben, wo um die Gro\u00dfstadt tausend Schlote dampfen,", "tokens": ["O\u00b7ben", ",", "wo", "um", "die", "Gro\u00df\u00b7stadt", "tau\u00b7send", "Schlo\u00b7te", "damp\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "APPR", "ART", "NN", "CARD", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Wo Eisenz\u00fcge qualmend von Stadt zu St\u00e4dten stampfen:", "tokens": ["Wo", "Ei\u00b7sen\u00b7z\u00fc\u00b7ge", "qual\u00b7mend", "von", "Stadt", "zu", "St\u00e4d\u00b7ten", "stamp\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Da rast die Kraft, die Alles in Einen Arbeitswirbel rei\u00dft:", "tokens": ["Da", "rast", "die", "Kraft", ",", "die", "Al\u00b7les", "in", "Ei\u00b7nen", "Ar\u00b7beits\u00b7wir\u00b7bel", "rei\u00dft", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Der Weltgeist!", "tokens": ["Der", "Welt\u00b7geist", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Wo auf dem Flugplatz steigbereit die \u00c4roplane liegen:", "tokens": ["Wo", "auf", "dem", "Flug\u00b7platz", "steig\u00b7be\u00b7reit", "die", "\u00c4\u00b7rop\u00b7la\u00b7ne", "lie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Angespannte Dr\u00e4hte sirren, der Propeller kreist", "tokens": ["An\u00b7ge\u00b7spann\u00b7te", "Dr\u00e4h\u00b7te", "sir\u00b7ren", ",", "der", "Pro\u00b7pel\u00b7ler", "kreist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "VVINF", "$,", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Immer schneller, pl\u00f6tzlich treibt das Schraubensurren", "tokens": ["Im\u00b7mer", "schnel\u00b7ler", ",", "pl\u00f6tz\u00b7lich", "treibt", "das", "Schrau\u00b7ben\u00b7sur\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Den L\u00e4rmvogel schr\u00e4gauf \u2013 fernhoch ist er schon im Fliegen \u2013 \u2013", "tokens": ["Den", "L\u00e4rm\u00b7vo\u00b7gel", "schr\u00e4g\u00b7auf", "\u2013", "fern\u00b7hoch", "ist", "er", "schon", "im", "Flie\u00b7gen", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PTKVZ", "$(", "ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$(", "$("], "meter": "-+---+-++-+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Wo die Luftschiffe, die Riesenluft-Torpedos knurren,", "tokens": ["Wo", "die", "Luft\u00b7schif\u00b7fe", ",", "die", "Rie\u00b7sen\u00b7luft\u00b7Tor\u00b7pe\u00b7dos", "knur\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Sieghaft sicher n\u00e4herkommend st\u00e4rker, dumpfer schnurren,", "tokens": ["Sieg\u00b7haft", "si\u00b7cher", "n\u00e4\u00b7her\u00b7kom\u00b7mend", "st\u00e4r\u00b7ker", ",", "dum\u00b7pfer", "schnur\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "ADJD", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Da steigt, da fliegt, da siegt \u00fcber Erdenschwere der Unruh-Geist:", "tokens": ["Da", "steigt", ",", "da", "fliegt", ",", "da", "siegt", "\u00fc\u00b7ber", "Er\u00b7den\u00b7schwe\u00b7re", "der", "Un\u00b7ruh\u00b7Geist", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Der neue Sch\u00f6nheit, neue Tatenwege weist:", "tokens": ["Der", "neu\u00b7e", "Sch\u00f6n\u00b7heit", ",", "neu\u00b7e", "Ta\u00b7ten\u00b7we\u00b7ge", "weist", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Weltgeist!", "tokens": ["Der", "Welt\u00b7geist", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}