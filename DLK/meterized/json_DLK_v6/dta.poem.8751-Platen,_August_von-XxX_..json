{"dta.poem.8751": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxX .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist so schroff:", "tokens": ["Du", "bist", "so", "schroff", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Gibt nicht der Liebe Lust und Pein", "tokens": ["Gibt", "nicht", "der", "Lie\u00b7be", "Lust", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Reden Stoff?", "tokens": ["Zum", "Re\u00b7den", "Stoff", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wo nicht, was gilt der Lieb' ein Wo,", "tokens": ["Wo", "nicht", ",", "was", "gilt", "der", "Lieb'", "ein", "Wo", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "PWS", "VVFIN", "ART", "NN", "ART", "PWAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Wie, ein Was?", "tokens": ["Ein", "Wie", ",", "ein", "Was", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PWAV", "$,", "ART", "PWS", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Zu lieben und zu schweigen, o", "tokens": ["Zu", "lie\u00b7ben", "und", "zu", "schwei\u00b7gen", ",", "o"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,", "FM"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie lieb' ich das!", "tokens": ["Wie", "lieb'", "ich", "das", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PDS", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ich schweige, weil so kalt du scheinst,", "tokens": ["Ich", "schwei\u00b7ge", ",", "weil", "so", "kalt", "du", "scheinst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und unerweicht,", "tokens": ["Und", "un\u00b7er\u00b7weicht", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mein Auge spricht, es spricht dereinst", "tokens": ["Mein", "Au\u00b7ge", "spricht", ",", "es", "spricht", "de\u00b7reinst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Ku\u00df vielleicht.", "tokens": ["Mein", "Ku\u00df", "viel\u00b7leicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}