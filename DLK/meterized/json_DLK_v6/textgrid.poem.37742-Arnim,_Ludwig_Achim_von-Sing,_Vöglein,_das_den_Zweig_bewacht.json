{"textgrid.poem.37742": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Sing, V\u00f6glein, das den Zweig bewacht", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sing, V\u00f6glein, das den Zweig bewacht,", "tokens": ["Sing", ",", "V\u00f6\u00b7glein", ",", "das", "den", "Zweig", "be\u00b7wacht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich leg nicht an zum Schie\u00dfen!", "tokens": ["Ich", "leg", "nicht", "an", "zum", "Schie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "PTKNEG", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du singest mir von guter Nacht,", "tokens": ["Du", "sin\u00b7gest", "mir", "von", "gu\u00b7ter", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du mu\u00dft mein Liebchen gr\u00fc\u00dfen:", "tokens": ["Du", "mu\u00dft", "mein", "Lieb\u00b7chen", "gr\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O k\u00f6nnt' ich mich so singen aus,", "tokens": ["O", "k\u00f6nnt'", "ich", "mich", "so", "sin\u00b7gen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PRF", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie m\u00fc\u00dft' es einmal h\u00f6ren,", "tokens": ["Sie", "m\u00fc\u00dft'", "es", "ein\u00b7mal", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sing, Nachtigall, hier ohne Graus,", "tokens": ["Sing", ",", "Nach\u00b7ti\u00b7gall", ",", "hier", "oh\u00b7ne", "Graus", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich will dich nicht mehr st\u00f6ren.", "tokens": ["Ich", "will", "dich", "nicht", "mehr", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So weich wie deine Federlein", "tokens": ["So", "weich", "wie", "dei\u00b7ne", "Fe\u00b7der\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bin ich von s\u00fc\u00dfen Wehen,", "tokens": ["Bin", "ich", "von", "s\u00fc\u00b7\u00dfen", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich gehe in den Wald hinein,", "tokens": ["Ich", "ge\u00b7he", "in", "den", "Wald", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mag doch kein Blut mehr sehen.", "tokens": ["Mag", "doch", "kein", "Blut", "mehr", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Thr\u00e4nlein auf das Pulver f\u00e4llt,", "tokens": ["Ein", "Thr\u00e4n\u00b7lein", "auf", "das", "Pul\u00b7ver", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und l\u00f6schet alles Feuer;", "tokens": ["Und", "l\u00f6\u00b7schet", "al\u00b7les", "Feu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dir Nachtigall bin ich gesellt,", "tokens": ["Dir", "Nach\u00b7ti\u00b7gall", "bin", "ich", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und traure in der Feier.", "tokens": ["Und", "trau\u00b7re", "in", "der", "Fei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}