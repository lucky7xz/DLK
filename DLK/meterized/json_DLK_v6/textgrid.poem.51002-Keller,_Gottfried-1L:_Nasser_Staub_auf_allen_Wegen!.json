{"textgrid.poem.51002": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nasser Staub auf allen Wegen!", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nasser Staub auf allen Wegen!", "tokens": ["Nas\u00b7ser", "Staub", "auf", "al\u00b7len", "We\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dorn und Distel h\u00e4ngt voll Regen,", "tokens": ["Dorn", "und", "Dis\u00b7tel", "h\u00e4ngt", "voll", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Bach schreit wie ein Kind!", "tokens": ["Und", "der", "Bach", "schreit", "wie", "ein", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Nirgends bl\u00fcht ein Regenbogen!", "tokens": ["Nir\u00b7gends", "bl\u00fcht", "ein", "Re\u00b7gen\u00b7bo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, die Sonn ist weggezogen", "tokens": ["Ach", ",", "die", "Sonn", "ist", "weg\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Himmel taub und blind!", "tokens": ["Und", "der", "Him\u00b7mel", "taub", "und", "blind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Traurig ruhn des Waldes Lieder,", "tokens": ["Trau\u00b7rig", "ruhn", "des", "Wal\u00b7des", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle Saat liegt siech darnieder,", "tokens": ["Al\u00b7le", "Saat", "liegt", "siech", "dar\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00f6stelnd schl\u00e4ft der Wachtel Brut.", "tokens": ["Fr\u00f6s\u00b7telnd", "schl\u00e4ft", "der", "Wach\u00b7tel", "Brut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jahreshoffnung \u2013 fahler Schimmer!", "tokens": ["Jah\u00b7res\u00b7hoff\u00b7nung", "\u2013", "fah\u00b7ler", "Schim\u00b7mer", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit den Menschen steht's noch schlimmer:", "tokens": ["Mit", "den", "Men\u00b7schen", "steht's", "noch", "schlim\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kalt und tr\u00e4ge schleicht ihr Blut!", "tokens": ["Kalt", "und", "tr\u00e4\u00b7ge", "schleicht", "ihr", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Krankes Weib am Findelsteine", "tokens": ["Kran\u00b7kes", "Weib", "am", "Fin\u00b7del\u00b7stei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem S\u00e4ugling, weine! weine", "tokens": ["Mit", "dem", "S\u00e4ug\u00b7ling", ",", "wei\u00b7ne", "!", "wei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "$.", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Trostlos oder hoffnungsvoll:", "tokens": ["Trost\u00b7los", "o\u00b7der", "hoff\u00b7nungs\u00b7voll", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht im Feld und auf den B\u00e4umen \u2013", "tokens": ["Nicht", "im", "Feld", "und", "auf", "den", "B\u00e4u\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "KON", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In den Herzen mu\u00df es keimen,", "tokens": ["In", "den", "Her\u00b7zen", "mu\u00df", "es", "kei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn es besser werden soll!", "tokens": ["Wenn", "es", "bes\u00b7ser", "wer\u00b7den", "soll", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Fleh zu Gott, der ja die Saaten", "tokens": ["Fleh", "zu", "Gott", ",", "der", "ja", "die", "Saa\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das Menschenherz beraten,", "tokens": ["Und", "das", "Men\u00b7schen\u00b7herz", "be\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bete hei\u00df und immerdar,", "tokens": ["Be\u00b7te", "hei\u00df", "und", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er, unsre Not zu wenden,", "tokens": ["Da\u00df", "er", ",", "uns\u00b7re", "Not", "zu", "wen\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wolle Licht und W\u00e4rme senden", "tokens": ["Wol\u00b7le", "Licht", "und", "W\u00e4r\u00b7me", "sen\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ein gutes Menschenjahr!", "tokens": ["Und", "ein", "gu\u00b7tes", "Men\u00b7schen\u00b7jahr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}