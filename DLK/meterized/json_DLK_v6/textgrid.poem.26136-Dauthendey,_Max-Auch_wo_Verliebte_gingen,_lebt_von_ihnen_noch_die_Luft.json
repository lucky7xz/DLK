{"textgrid.poem.26136": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Auch wo Verliebte gingen, lebt von ihnen noch die Luft", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Talweg schweigt der Abendwald.", "tokens": ["Am", "Tal\u00b7weg", "schweigt", "der", "Ab\u00b7end\u00b7wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur eine Kr\u00e4he kreischt noch im Geheg,", "tokens": ["Nur", "ei\u00b7ne", "Kr\u00e4\u00b7he", "kreischt", "noch", "im", "Ge\u00b7heg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Stern ist erschienen und ruft andere bald.", "tokens": ["Ein", "Stern", "ist", "er\u00b7schie\u00b7nen", "und", "ruft", "an\u00b7de\u00b7re", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "KON", "VVFIN", "PIS", "ADV", "$."], "meter": "-+--+--++--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die Waldb\u00e4ume verloren Gestalt und Mienen.", "tokens": ["Die", "Wald\u00b7b\u00e4u\u00b7me", "ver\u00b7lo\u00b7ren", "Ge\u00b7stalt", "und", "Mie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "NN", "KON", "NN", "$."], "meter": "-++--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Am Talweg geht Geruch von manchem Maienstrauch,", "tokens": ["Am", "Tal\u00b7weg", "geht", "Ge\u00b7ruch", "von", "man\u00b7chem", "Mai\u00b7en\u00b7strauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Maiblumen trug heut' manche frohe Hand nach Haus,", "tokens": ["Maib\u00b7lu\u00b7men", "trug", "heut'", "man\u00b7che", "fro\u00b7he", "Hand", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Abend wischt am Weg den Duft nicht aus.", "tokens": ["Der", "A\u00b7bend", "wischt", "am", "Weg", "den", "Duft", "nicht", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Auch wo Verliebte gingen, lebt von ihnen noch die Luft", "tokens": ["Auch", "wo", "Ver\u00b7lieb\u00b7te", "gin\u00b7gen", ",", "lebt", "von", "ih\u00b7nen", "noch", "die", "Luft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "NN", "VVFIN", "$,", "VVFIN", "APPR", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "Inbr\u00fcnstig wie das alte Lied, das einer in den Abend ruft.", "tokens": ["In\u00b7br\u00fcns\u00b7tig", "wie", "das", "al\u00b7te", "Lied", ",", "das", "ei\u00b7ner", "in", "den", "A\u00b7bend", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.2": {"line.1": {"text": " ..."