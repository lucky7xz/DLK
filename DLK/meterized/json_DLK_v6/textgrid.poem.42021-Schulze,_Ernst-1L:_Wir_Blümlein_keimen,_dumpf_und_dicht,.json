{"textgrid.poem.42021": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir Bl\u00fcmlein keimen, dumpf und dicht,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir Bl\u00fcmlein keimen, dumpf und dicht,", "tokens": ["Wir", "Bl\u00fcm\u00b7lein", "kei\u00b7men", ",", "dumpf", "und", "dicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Wald und Thal umgeben:", "tokens": ["Von", "Wald", "und", "Thal", "um\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch eh der Kelch die H\u00fclle bricht,", "tokens": ["Doch", "eh", "der", "Kelch", "die", "H\u00fcl\u00b7le", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schaut hoffnungsvoll zum goldnen Licht", "tokens": ["Schaut", "hoff\u00b7nungs\u00b7voll", "zum", "gold\u00b7nen", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Knosp' und gr\u00fc\u00dft das junge Leben;", "tokens": ["Die", "Knosp'", "und", "gr\u00fc\u00dft", "das", "jun\u00b7ge", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und fern dahin", "tokens": ["Und", "fern", "da\u00b7hin"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "PAV"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Geht unser Sinn,", "tokens": ["Geht", "un\u00b7ser", "Sinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Wir m\u00f6chten zum Himmel uns heben.", "tokens": ["Wir", "m\u00f6ch\u00b7ten", "zum", "Him\u00b7mel", "uns", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Die Bien' entsummt, das V\u00f6glein flieht", "tokens": ["Die", "Bi\u00b7en'", "ent\u00b7summt", ",", "das", "V\u00f6\u00b7glein", "flieht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit fr\u00f6hlichem Gefieder:", "tokens": ["Mit", "fr\u00f6h\u00b7li\u00b7chem", "Ge\u00b7fie\u00b7der", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Uns h\u00e4lt das irdische Gebiet,", "tokens": ["Uns", "h\u00e4lt", "das", "ir\u00b7di\u00b7sche", "Ge\u00b7biet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wenn der frische Kelch entbl\u00fcht,", "tokens": ["Und", "wenn", "der", "fri\u00b7sche", "Kelch", "ent\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So senkt er trauernd sich hernieder;", "tokens": ["So", "senkt", "er", "trau\u00b7ernd", "sich", "her\u00b7nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Duft verrinnt", "tokens": ["Der", "Duft", "ver\u00b7rinnt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Im Fr\u00fchlingswind", "tokens": ["Im", "Fr\u00fch\u00b7lings\u00b7wind"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Und kehret uns nimmer wieder.", "tokens": ["Und", "keh\u00b7ret", "uns", "nim\u00b7mer", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Drum la\u00df zu deinem Sonnenschein,", "tokens": ["Drum", "la\u00df", "zu", "dei\u00b7nem", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du zartes Bild, uns fliehen;", "tokens": ["Du", "zar\u00b7tes", "Bild", ",", "uns", "flie\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du bist so klar, so mild, so rein,", "tokens": ["Du", "bist", "so", "klar", ",", "so", "mild", ",", "so", "rein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du pflegst uns arme Kindelein", "tokens": ["Du", "pflegst", "uns", "ar\u00b7me", "Kin\u00b7de\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gewi\u00df mit freundlichem Bem\u00fchen;", "tokens": ["Ge\u00b7wi\u00df", "mit", "freund\u00b7li\u00b7chem", "Be\u00b7m\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "An Liebesbrust", "tokens": ["An", "Lie\u00b7bes\u00b7brust"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Lebt sich's mit Lust,", "tokens": ["Lebt", "sich's", "mit", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Da ist es so s\u00fc\u00df zu verbl\u00fchen!", "tokens": ["Da", "ist", "es", "so", "s\u00fc\u00df", "zu", "ver\u00b7bl\u00fc\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}