"und heulend im Kamin sich fing?", "tokens": ["und", "heu\u00b7lend", "im", "Ka\u00b7min", "sich", "fing", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sahst du den Himmel heute fr\u00fch,", "tokens": ["Sahst", "du", "den", "Him\u00b7mel", "heu\u00b7te", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "wie Blut so rot, brandfackelgl\u00fch?!", "tokens": ["wie", "Blut", "so", "rot", ",", "brand\u00b7fa\u00b7ckel\u00b7gl\u00fch", "?!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Es liegt etwas in der Luft, mein Freund,", "tokens": ["Es", "liegt", "et\u00b7was", "in", "der", "Luft", ",", "mein", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "es liegt etwas in der Luft!", "tokens": ["es", "liegt", "et\u00b7was", "in", "der", "Luft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Es ist eine seltsame Zeit, mein Freund,", "tokens": ["Es", "ist", "ei\u00b7ne", "selt\u00b7sa\u00b7me", "Zeit", ",", "mein", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "es ist eine seltsame Zeit!", "tokens": ["es", "ist", "ei\u00b7ne", "selt\u00b7sa\u00b7me", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "ein immer toller Gehaste von Jahr zu Jahr!", "tokens": ["ein", "im\u00b7mer", "tol\u00b7ler", "Ge\u00b7has\u00b7te", "von", "Jahr", "zu", "Jahr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "nichts soll mehr bleiben, wie es war!", "tokens": ["nichts", "soll", "mehr", "blei\u00b7ben", ",", "wie", "es", "war", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "VVINF", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "nichts soll im alten Gleis mehr gehn", "tokens": ["nichts", "soll", "im", "al\u00b7ten", "Gleis", "mehr", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPRART", "ADJA", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und ruhig, fest und sicher stehn!", "tokens": ["und", "ru\u00b7hig", ",", "fest", "und", "si\u00b7cher", "stehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein jeder redet und redet drein,", "tokens": ["Ein", "je\u00b7der", "re\u00b7det", "und", "re\u00b7det", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "und jeder will der Kl\u00fcgere sein!", "tokens": ["und", "je\u00b7der", "will", "der", "Kl\u00fc\u00b7ge\u00b7re", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Der eine hofft dies, der andere das,", "tokens": ["Der", "ei\u00b7ne", "hofft", "dies", ",", "der", "an\u00b7de\u00b7re", "das", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PDS", "$,", "PRELS", "PIS", "PDS", "$,"], "meter": "-+-+--+---", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "und keiner aber wei\u00df recht: was?!", "tokens": ["und", "kei\u00b7ner", "a\u00b7ber", "wei\u00df", "recht", ":", "was", "?!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "ADV", "VVFIN", "ADJD", "$.", "PWS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Es ist eine seltsame Zeit, mein Freund,", "tokens": ["Es", "ist", "ei\u00b7ne", "selt\u00b7sa\u00b7me", "Zeit", ",", "mein", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "es ist eine seltsame Zeit!", "tokens": ["es", "ist", "ei\u00b7ne", "selt\u00b7sa\u00b7me", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Und wie es gestalten sich wird, mein Freund,", "tokens": ["Und", "wie", "es", "ge\u00b7stal\u00b7ten", "sich", "wird", ",", "mein", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "PRF", "VAFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "und wie es gestalten sich wird?", "tokens": ["und", "wie", "es", "ge\u00b7stal\u00b7ten", "sich", "wird", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "PRF", "VAFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "in welcher Richtung? in welchem Sinn?", "tokens": ["in", "wel\u00b7cher", "Rich\u00b7tung", "?", "in", "wel\u00b7chem", "Sinn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "$.", "APPR", "PWAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "ob zu Verderben? ob zu Gewinn?", "tokens": ["ob", "zu", "Ver\u00b7der\u00b7ben", "?", "ob", "zu", "Ge\u00b7winn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$.", "KOUS", "APPR", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die Jungen haben es in der Hand ...", "tokens": ["Die", "Jun\u00b7gen", "ha\u00b7ben", "es", "in", "der", "Hand", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "die Jungen mit ihrem Jugendmut,", "tokens": ["die", "Jun\u00b7gen", "mit", "ih\u00b7rem", "Ju\u00b7gend\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "mit ihrem Glauben, mit ihrer Glut!", "tokens": ["mit", "ih\u00b7rem", "Glau\u00b7ben", ",", "mit", "ih\u00b7rer", "Glut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "und wenn sie furchtlos festen Blicks", "tokens": ["und", "wenn", "sie", "furcht\u00b7los", "fes\u00b7ten", "Blicks"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "hinaussehn \u00fcber ihr kleines Heut", "tokens": ["hin\u00b7aus\u00b7sehn", "\u00fc\u00b7ber", "ihr", "klei\u00b7nes", "Heut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "und \u00fcber Parteigez\u00e4nk und Neid ...", "tokens": ["und", "\u00fc\u00b7ber", "Par\u00b7tei\u00b7ge\u00b7z\u00e4nk", "und", "Neid", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "dann, glaub ich, gestaltet sich's gut, mein Freund,", "tokens": ["dann", ",", "glaub", "ich", ",", "ge\u00b7stal\u00b7tet", "sich's", "gut", ",", "mein", "Freund", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VVPP", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "dann, glaub ich, gestaltet sich's gut!", "tokens": ["dann", ",", "glaub", "ich", ",", "ge\u00b7stal\u00b7tet", "sich's", "gut", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "