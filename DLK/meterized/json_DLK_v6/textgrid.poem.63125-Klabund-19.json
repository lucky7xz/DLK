{"textgrid.poem.63125": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "19", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies ist das Lied, das Villon sang,", "tokens": ["Dies", "ist", "das", "Lied", ",", "das", "Vil\u00b7lon", "sang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als man ihn h\u00e4ngen wollte.", "tokens": ["Als", "man", "ihn", "h\u00e4n\u00b7gen", "woll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er f\u00fchlte um den Hals den Strang,", "tokens": ["Er", "f\u00fchl\u00b7te", "um", "den", "Hals", "den", "Strang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er sang das Lied den Weg entlang,", "tokens": ["Er", "sang", "das", "Lied", "den", "Weg", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Schinderkarren rollte.", "tokens": ["Der", "Schin\u00b7der\u00b7kar\u00b7ren", "roll\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "H\u00e4ngt mich den Schurken zum Alarm", "tokens": ["H\u00e4ngt", "mich", "den", "Schur\u00b7ken", "zum", "A\u00b7larm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Nur hoch in alle Winde!", "tokens": ["Nur", "hoch", "in", "al\u00b7le", "Win\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wegweiser schlenkere mein Arm,", "tokens": ["Weg\u00b7wei\u00b7ser", "schlen\u00b7ke\u00b7re", "mein", "Arm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er weist den Weg dem schlimmen Schwarm", "tokens": ["Er", "weist", "den", "Weg", "dem", "schlim\u00b7men", "Schwarm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und manchem braunen Kinde.", "tokens": ["Und", "man\u00b7chem", "brau\u00b7nen", "Kin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Einst hat der Teufel mich gekirrt,", "tokens": ["Einst", "hat", "der", "Teu\u00b7fel", "mich", "ge\u00b7kirrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun h\u00f6r ich B\u00e4ume singen.", "tokens": ["Nun", "h\u00f6r", "ich", "B\u00e4u\u00b7me", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich f\u00fchle Gott. Mein Auge schwirrt.", "tokens": ["Ich", "f\u00fch\u00b7le", "Gott", ".", "Mein", "Au\u00b7ge", "schwirrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$.", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Leib, mein armer Leib er wird", "tokens": ["Mein", "Leib", ",", "mein", "ar\u00b7mer", "Leib", "er", "wird"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als Aveglocke schwingen.", "tokens": ["Als", "A\u00b7ve\u00b7glo\u00b7cke", "schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}