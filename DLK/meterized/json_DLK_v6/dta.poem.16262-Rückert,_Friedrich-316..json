{"dta.poem.16262": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "316.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht f\u00fcr die Menschheit nur und f\u00fcr den Geist der Welten,", "tokens": ["Nicht", "f\u00fcr", "die", "Menschheit", "nur", "und", "f\u00fcr", "den", "Geist", "der", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADV", "KON", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Du mu\u00dft auch f\u00fcr dich selbst Geschichte lassen gelten.", "tokens": ["Du", "mu\u00dft", "auch", "f\u00fcr", "dich", "selbst", "Ge\u00b7schich\u00b7te", "las\u00b7sen", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPER", "ADV", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn Gleiches ist in dir, wie in der Welt die streitet,", "tokens": ["Denn", "Glei\u00b7ches", "ist", "in", "dir", ",", "wie", "in", "der", "Welt", "die", "strei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "APPR", "PPER", "$,", "PWAV", "APPR", "ART", "NN", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Streben, das durch Kampf best\u00e4ndig vorwerts schreitet.", "tokens": ["Ein", "Stre\u00b7ben", ",", "das", "durch", "Kampf", "be\u00b7st\u00e4n\u00b7dig", "vor\u00b7werts", "schrei\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und wie die Geister, die der Zeiten Teppich weben,", "tokens": ["Und", "wie", "die", "Geis\u00b7ter", ",", "die", "der", "Zei\u00b7ten", "Tep\u00b7pich", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "PRELS", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Stets neues wirkend, doch des Alten Bild aufheben,", "tokens": ["Stets", "neu\u00b7es", "wir\u00b7kend", ",", "doch", "des", "Al\u00b7ten", "Bild", "auf\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VVPP", "$,", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und nie vergessen, wann sie sich zu h\u00f6hern Stufen", "tokens": ["Und", "nie", "ver\u00b7ges\u00b7sen", ",", "wann", "sie", "sich", "zu", "h\u00f6\u00b7hern", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "$,", "PWAV", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erhoben, was mit Flei\u00df sie auf der niedern schufen;", "tokens": ["Er\u00b7ho\u00b7ben", ",", "was", "mit", "Flei\u00df", "sie", "auf", "der", "nie\u00b7dern", "schu\u00b7fen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "APPR", "NN", "PPER", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So du auch, wenn du scheinst neuschaffend zu zerst\u00f6ren", "tokens": ["So", "du", "auch", ",", "wenn", "du", "scheinst", "neu\u00b7schaf\u00b7fend", "zu", "zer\u00b7st\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geschaffnes, f\u00fchlst es doch dir ewig angeh\u00f6ren.", "tokens": ["Ge\u00b7schaff\u00b7nes", ",", "f\u00fchlst", "es", "doch", "dir", "e\u00b7wig", "an\u00b7ge\u00b7h\u00f6\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Nur als du drinnen warst, war drin dein Thun befangen", "tokens": ["Nur", "als", "du", "drin\u00b7nen", "warst", ",", "war", "drin", "dein", "Thun", "be\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADV", "VAFIN", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun erst herausgelangt, siehst du es unbefangen.", "tokens": ["Nun", "erst", "her\u00b7aus\u00b7ge\u00b7langt", ",", "siehst", "du", "es", "un\u00b7be\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,", "VVFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Du siehst, da\u00df mit im Strom z\u00e4hlt jede Einzelwelle,", "tokens": ["Du", "siehst", ",", "da\u00df", "mit", "im", "Strom", "z\u00e4hlt", "je\u00b7de", "Ein\u00b7zel\u00b7wel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "APPR", "APPRART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auch das Gr\u00f6ste g\u00f6nnt dem Kleinsten seine Stelle.", "tokens": ["Und", "auch", "das", "Gr\u00f6s\u00b7te", "g\u00f6nnt", "dem", "Kleins\u00b7ten", "sei\u00b7ne", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Nicht missen m\u00f6chtest du auch das was du verfehltest,", "tokens": ["Nicht", "mis\u00b7sen", "m\u00f6ch\u00b7test", "du", "auch", "das", "was", "du", "ver\u00b7fehl\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "ADV", "PDS", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn es dir half dazu, da\u00df du ein Be\u00dfres w\u00e4hltest.", "tokens": ["Wenn", "es", "dir", "half", "da\u00b7zu", ",", "da\u00df", "du", "ein", "Be\u00df\u00b7res", "w\u00e4hl\u00b7test", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "PAV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}