{"textgrid.poem.25507": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn die H\u00fchner sich auf ihren Latten", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn die H\u00fchner sich auf ihren Latten", "tokens": ["Wenn", "die", "H\u00fch\u00b7ner", "sich", "auf", "ih\u00b7ren", "Lat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eine Schlafbank w\u00e4hlen f\u00fcr die Nacht,", "tokens": ["Ei\u00b7ne", "Schlaf\u00b7bank", "w\u00e4h\u00b7len", "f\u00fcr", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und die Sonn' aus meinem Schatten", "tokens": ["Und", "die", "Sonn'", "aus", "mei\u00b7nem", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen ackerlangen Riesen macht:", "tokens": ["Ei\u00b7nen", "ac\u00b7ker\u00b7lan\u00b7gen", "Rie\u00b7sen", "macht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Husch' ich in den Garten \u2013 deine Lieder", "tokens": ["Husch'", "ich", "in", "den", "Gar\u00b7ten", "\u2013", "dei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "$(", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Gehen mit in meinem Pompadour \u2013", "tokens": ["Ge\u00b7hen", "mit", "in", "mei\u00b7nem", "Pom\u00b7pa\u00b7dour", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Werfe lang ins Gras mich nieder,", "tokens": ["Wer\u00b7fe", "lang", "ins", "Gras", "mich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und vergesse Menschen und Natur.", "tokens": ["Und", "ver\u00b7ges\u00b7se", "Men\u00b7schen", "und", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Alle die Aurikeln, Nelk' und Rosen,", "tokens": ["Al\u00b7le", "die", "Au\u00b7ri\u00b7keln", ",", "Nel\u00b7k'", "und", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "$,", "NE", "KON", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.10": {"text": "Die ich sonst \u2013 wie meine Mutter, mich \u2013", "tokens": ["Die", "ich", "sonst", "\u2013", "wie", "mei\u00b7ne", "Mut\u00b7ter", ",", "mich", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ADV", "$(", "KOKOM", "PPOSAT", "NN", "$,", "PPER", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Anzusehn und liebzukosen", "tokens": ["An\u00b7zu\u00b7sehn", "und", "lieb\u00b7zu\u00b7ko\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und zu warten pflegte, missen mich.", "tokens": ["Und", "zu", "war\u00b7ten", "pfleg\u00b7te", ",", "mis\u00b7sen", "mich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Und mein L\u00e4mmchen, das ich sonst zu f\u00fcttern", "tokens": ["Und", "mein", "L\u00e4mm\u00b7chen", ",", "das", "ich", "sonst", "zu", "f\u00fct\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Ueber kein Vergn\u00fcgen je verga\u00df,", "tokens": ["Ue\u00b7ber", "kein", "Ver\u00b7gn\u00fc\u00b7gen", "je", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Bl\u00f6cket an den Gartengittern", "tokens": ["Bl\u00f6\u00b7cket", "an", "den", "Gar\u00b7ten\u00b7git\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Oft umsonst nach einer Hand voll Gras.", "tokens": ["Oft", "um\u00b7sonst", "nach", "ei\u00b7ner", "Hand", "voll", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Und mein Papchen,", "tokens": ["Und", "mein", "Pap\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "Zucker durch des K\u00e4fichs St\u00e4be gab,", "tokens": ["Zu\u00b7cker", "durch", "des", "K\u00e4\u00b7fichs", "St\u00e4\u00b7be", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.19": {"text": "Wetzt umsonst den krummen Schnabel", "tokens": ["Wetzt", "um\u00b7sonst", "den", "krum\u00b7men", "Schna\u00b7bel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "An dem glatten Ringe schaukelnd ab.", "tokens": ["An", "dem", "glat\u00b7ten", "Rin\u00b7ge", "schau\u00b7kelnd", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "Sollen meine Blumen nicht verwelken,", "tokens": ["Sol\u00b7len", "mei\u00b7ne", "Blu\u00b7men", "nicht", "ver\u00b7wel\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Lamm und Vogel schmachten? Komm o Mann!", "tokens": ["Lamm", "und", "Vo\u00b7gel", "schmach\u00b7ten", "?", "Komm", "o", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "VVFIN", "$.", "VVFIN", "FM", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "Lobe Vogel, Lamm und Nelken!", "tokens": ["Lo\u00b7be", "Vo\u00b7gel", ",", "Lamm", "und", "Nel\u00b7ken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "L\u00e4mmchen, h\u00fcpfe! Papchen, schwatze, und ihr Blumen, bl\u00fchet dann!", "tokens": ["L\u00e4mm\u00b7chen", ",", "h\u00fcp\u00b7fe", "!", "Pap\u00b7chen", ",", "schwat\u00b7ze", ",", "und", "ihr", "Blu\u00b7men", ",", "bl\u00fc\u00b7het", "dann", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$.", "NN", "$,", "VVFIN", "$,", "KON", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}