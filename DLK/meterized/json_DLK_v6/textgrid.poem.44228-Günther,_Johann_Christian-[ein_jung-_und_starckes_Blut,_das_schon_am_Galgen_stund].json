{"textgrid.poem.44228": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[ein jung- und starckes Blut, das schon am Galgen stund]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein jung- und starckes Blut, das schon am Galgen stund,", "tokens": ["Ein", "jung", "und", "star\u00b7ckes", "Blut", ",", "das", "schon", "am", "Gal\u00b7gen", "stund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "ADJA", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verblich vor Todesangst auf Wangen, Stirn und Mund;", "tokens": ["Ver\u00b7blich", "vor", "To\u00b7de\u00b7sangst", "auf", "Wan\u00b7gen", ",", "Stirn", "und", "Mund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Herze schlug vor Furcht und lies mitsamt den Haaren,", "tokens": ["Das", "Her\u00b7ze", "schlug", "vor", "Furcht", "und", "lies", "mit\u00b7samt", "den", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "APPR", "NN", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Worauf das Schr\u00f6cken sa\u00df, viel Seufzer aufw\u00e4rts fahren.", "tokens": ["Wo\u00b7rauf", "das", "Schr\u00f6\u00b7cken", "sa\u00df", ",", "viel", "Seuf\u00b7zer", "auf\u00b7w\u00e4rts", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "$,", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Indem der arme Tropf nun schon am Kloben hing", "tokens": ["In\u00b7dem", "der", "ar\u00b7me", "Tropf", "nun", "schon", "am", "Klo\u00b7ben", "hing"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und auch der rauhe Strick den fetten Hals umfing,", "tokens": ["Und", "auch", "der", "rau\u00b7he", "Strick", "den", "fet\u00b7ten", "Hals", "um\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Kam Gnade vor das Recht und lies die schwachen Sinnen,", "tokens": ["Kam", "Gna\u00b7de", "vor", "das", "Recht", "und", "lies", "die", "schwa\u00b7chen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die schon der Tod ergrif, von neuem Kraft gewinnen.", "tokens": ["Die", "schon", "der", "Tod", "er\u00b7grif", ",", "von", "neu\u00b7em", "Kraft", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da sprang der gute Kerl, ein Danckgebeth zu weihn,", "tokens": ["Da", "sprang", "der", "gu\u00b7te", "Kerl", ",", "ein", "Danck\u00b7ge\u00b7beth", "zu", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dem nechsten Tempel zu und trat mit Freuden ein,", "tokens": ["Dem", "nechs\u00b7ten", "Tem\u00b7pel", "zu", "und", "trat", "mit", "Freu\u00b7den", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "KON", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Gleich als Polylogus auf neue Kezer krachte", "tokens": ["Gleich", "als", "Po\u00b7ly\u00b7lo\u00b7gus", "auf", "neu\u00b7e", "Ke\u00b7zer", "krach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und manchen sch\u00f6nen Spruch zum Canzelm\u00e4rtrer machte.", "tokens": ["Und", "man\u00b7chen", "sch\u00f6\u00b7nen", "Spruch", "zum", "Can\u00b7zel\u00b7m\u00e4r\u00b7trer", "mach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da dies der gute Tropf mit Ungedult vernahm", "tokens": ["Da", "dies", "der", "gu\u00b7te", "Tropf", "mit", "Un\u00b7ge\u00b7dult", "ver\u00b7nahm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und ohngefehr ein Kerl von seinen Br\u00fcdern kam,", "tokens": ["Und", "ohn\u00b7ge\u00b7fehr", "ein", "Kerl", "von", "sei\u00b7nen", "Br\u00fc\u00b7dern", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So blies er diesem ein mit \u00e4ngstlichem Gesichte:", "tokens": ["So", "blies", "er", "die\u00b7sem", "ein", "mit", "\u00e4ngst\u00b7li\u00b7chem", "Ge\u00b7sich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "ART", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ach Bruder, st\u00fcnd ich doch nur jezo vor Gerichte!", "tokens": ["Ach", "Bru\u00b7der", ",", "st\u00fcnd", "ich", "doch", "nur", "je\u00b7zo", "vor", "Ge\u00b7rich\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "VVFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}