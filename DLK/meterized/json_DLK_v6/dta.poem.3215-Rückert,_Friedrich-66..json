{"dta.poem.3215": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "66.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wol g\u00f6nnen darf ich's dir, da\u00df du vor mir gegangen,", "tokens": ["Wol", "g\u00f6n\u00b7nen", "darf", "ich's", "dir", ",", "da\u00df", "du", "vor", "mir", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "PIS", "PPER", "$,", "KOUS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Nicht diesen Schmerz von mir, den ich von dir, empfangen;", "tokens": ["Nicht", "die\u00b7sen", "Schmerz", "von", "mir", ",", "den", "ich", "von", "dir", ",", "emp\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "PDAT", "NN", "APPR", "PPER", "$,", "PRELS", "PPER", "APPR", "PPER", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df du mich bleiben sah'st, und ich dich sah verscheiden;", "tokens": ["Da\u00df", "du", "mich", "blei\u00b7ben", "sah'st", ",", "und", "ich", "dich", "sah", "ver\u00b7schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VVFIN", "$,", "KON", "PPER", "PRF", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn seh'n Geliebter Tod ist mehr als eigne Leiden.", "tokens": ["Denn", "seh'n", "Ge\u00b7lieb\u00b7ter", "Tod", "ist", "mehr", "als", "eig\u00b7ne", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "VAFIN", "ADV", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}