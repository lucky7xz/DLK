{"textgrid.poem.42692": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Adria, deren elastischem blauen", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Adria, deren elastischem blauen", "tokens": ["Ad\u00b7ria", ",", "de\u00b7ren", "e\u00b7las\u00b7ti\u00b7schem", "blau\u00b7en"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PRELAT", "ADJA", "ADJA"], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Busen manch bl\u00fchendes Eyland entspriesst,", "tokens": ["Bu\u00b7sen", "manch", "bl\u00fc\u00b7hen\u00b7des", "Ey\u00b7land", "ent\u00b7spriesst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Holde schilfhaarichte Tochter des grauen", "tokens": ["Hol\u00b7de", "schilf\u00b7haa\u00b7rich\u00b7te", "Toch\u00b7ter", "des", "grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "ART", "ADJA"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Oceans, sey mir, o G\u00f6ttinn, gegr\u00fcsst!", "tokens": ["O\u00b7ceans", ",", "sey", "mir", ",", "o", "G\u00f6t\u00b7tinn", ",", "ge\u00b7gr\u00fcs\u00b7st", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "$,", "FM", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ehrfurchtsvoll nah' ich der heiligen Urne,", "tokens": ["Ehr\u00b7furchts\u00b7voll", "nah'", "ich", "der", "hei\u00b7li\u00b7gen", "Ur\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-----+--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Deren vielf\u00e4rbiger zackichter Rand,", "tokens": ["De\u00b7ren", "viel\u00b7f\u00e4r\u00b7bi\u00b7ger", "za\u00b7ckich\u00b7ter", "Rand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ringsum mit St\u00e4dten gekr\u00f6nt, die azurne", "tokens": ["Ring\u00b7sum", "mit", "St\u00e4d\u00b7ten", "ge\u00b7kr\u00f6nt", ",", "die", "a\u00b7zur\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "NN", "VVPP", "$,", "ART", "NN"], "meter": "+--+--+-+--", "measure": "dactylic.di.plus"}, "line.4": {"text": "Ebene deines Gebietes umspannt.", "tokens": ["E\u00b7be\u00b7ne", "dei\u00b7nes", "Ge\u00b7bie\u00b7tes", "um\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6n bist du, Nymphe, wenn Zephyr die Falten", "tokens": ["Sch\u00f6n", "bist", "du", ",", "Nym\u00b7phe", ",", "wenn", "Ze\u00b7phyr", "die", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "$,", "NN", "$,", "KOUS", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Deines smaragdenen Mantels durchpfl\u00fcgt,", "tokens": ["Dei\u00b7nes", "sma\u00b7rag\u00b7de\u00b7nen", "Man\u00b7tels", "durch\u00b7pfl\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Und in den flimmernden silberbestrahlten", "tokens": ["Und", "in", "den", "flim\u00b7mern\u00b7den", "sil\u00b7ber\u00b7bes\u00b7trahl\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "---+--+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Furchen die blitzende Sonne sich wiegt.", "tokens": ["Fur\u00b7chen", "die", "blit\u00b7zen\u00b7de", "Son\u00b7ne", "sich", "wiegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Lieblich ist's, wenn dein Gew\u00e4sser, sich schaukelnd,", "tokens": ["Lieb\u00b7lich", "ist's", ",", "wenn", "dein", "Ge\u00b7w\u00e4s\u00b7ser", ",", "sich", "schau\u00b7kelnd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "KOUS", "PPOSAT", "NN", "$,", "PRF", "ADJD", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sachte den Rand des Gestades benagt,", "tokens": ["Sach\u00b7te", "den", "Rand", "des", "Ge\u00b7sta\u00b7des", "be\u00b7nagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Oder, in sch\u00e4kerndem Wirbeltanz gaukelnd,", "tokens": ["O\u00b7der", ",", "in", "sch\u00e4\u00b7kern\u00b7dem", "Wir\u00b7bel\u00b7tanz", "gau\u00b7kelnd", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+--++-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Hastig ein Wellchen das andere jagt.", "tokens": ["Has\u00b7tig", "ein", "Well\u00b7chen", "das", "an\u00b7de\u00b7re", "jagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Sey mir, o Wogenbeherrscherinn, gn\u00e4dig!", "tokens": ["Sey", "mir", ",", "o", "Wo\u00b7gen\u00b7be\u00b7herr\u00b7sche\u00b7rinn", ",", "gn\u00e4\u00b7dig", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "FM", "NN", "$,", "ADJD", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sch\u00fctze die sch\u00fcchterne Barke, die bald", "tokens": ["Sch\u00fct\u00b7ze", "die", "sch\u00fcch\u00b7ter\u00b7ne", "Bar\u00b7ke", ",", "die", "bald"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PRELS", "ADV"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Fern nach dem flutenentstiegnen Venedig", "tokens": ["Fern", "nach", "dem", "flu\u00b7te\u00b7nent\u00b7stieg\u00b7nen", "Ve\u00b7ne\u00b7dig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NE"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Hin mit mir gleitet, vor \u00c4ols Gewalt!", "tokens": ["Hin", "mit", "mir", "glei\u00b7tet", ",", "vor", "\u00c4\u00b7ols", "Ge\u00b7walt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "$,", "APPR", "NN", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "Schirme mich friedlichen Z\u00f6gling der Musen,", "tokens": ["Schir\u00b7me", "mich", "fried\u00b7li\u00b7chen", "Z\u00f6g\u00b7ling", "der", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Wenn sich, von tobenden St\u00fcrmen durchbr\u00fcllt,", "tokens": ["Wenn", "sich", ",", "von", "to\u00b7ben\u00b7den", "St\u00fcr\u00b7men", "durch\u00b7br\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Abgrunder\u00f6ffnend dein g\u00e4hrender Busen", "tokens": ["Ab\u00b7grun\u00b7de\u00b7r\u00f6ff\u00b7nend", "dein", "g\u00e4h\u00b7ren\u00b7der", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Pl\u00f6tzlich bald senket, bald felsenhoch schwillt!", "tokens": ["Pl\u00f6tz\u00b7lich", "bald", "sen\u00b7ket", ",", "bald", "fel\u00b7sen\u00b7hoch", "schwillt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "$,", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.7": {"line.1": {"text": "Sch\u00fctze das Schiff, wenn mit schrecklichem Dr\u00e4uen", "tokens": ["Sch\u00fct\u00b7ze", "das", "Schiff", ",", "wenn", "mit", "schreck\u00b7li\u00b7chem", "Dr\u00e4u\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Eurus das knasternde Segel durchpfeift,", "tokens": ["Eu\u00b7rus", "das", "knas\u00b7tern\u00b7de", "Se\u00b7gel", "durch\u00b7pfeift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Und mit zerst\u00f6rendem Grimme den scheuen", "tokens": ["Und", "mit", "zer\u00b7st\u00f6\u00b7ren\u00b7dem", "Grim\u00b7me", "den", "scheu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mast ein unb\u00e4ndiger Windstoss ergreift!", "tokens": ["Mast", "ein", "un\u00b7b\u00e4n\u00b7di\u00b7ger", "Winds\u00b7toss", "er\u00b7greift", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.8": {"line.1": {"text": "Dankbar gelob' ich, alsdann dir, o traute", "tokens": ["Dank\u00b7bar", "ge\u00b7lob'", "ich", ",", "als\u00b7dann", "dir", ",", "o", "trau\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "ADV", "PPER", "$,", "FM", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "G\u00f6ttinn, ein festliches Loblied zu weihn:", "tokens": ["G\u00f6t\u00b7tinn", ",", "ein", "fest\u00b7li\u00b7ches", "Lob\u00b7lied", "zu", "weihn", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Lauschend vernehm' es in Osten der laute,", "tokens": ["Lau\u00b7schend", "ver\u00b7nehm'", "es", "in", "Os\u00b7ten", "der", "lau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "NN", "ART", "VVFIN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Ister, in Westen der wallende Rhein!", "tokens": ["Is\u00b7ter", ",", "in", "Wes\u00b7ten", "der", "wal\u00b7len\u00b7de", "Rhein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "NN", "ART", "ADJA", "NE", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}