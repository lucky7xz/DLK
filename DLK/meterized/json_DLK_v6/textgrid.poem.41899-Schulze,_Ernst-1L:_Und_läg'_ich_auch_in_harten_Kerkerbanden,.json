{"textgrid.poem.41899": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und l\u00e4g' ich auch in harten Kerkerbanden,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und l\u00e4g' ich auch in harten Kerkerbanden,", "tokens": ["Und", "l\u00e4g'", "ich", "auch", "in", "har\u00b7ten", "Ker\u00b7ker\u00b7ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umg\u00e4b' auch rings die Nacht mich \u00f6d' und leer,", "tokens": ["Um\u00b7g\u00e4b'", "auch", "rings", "die", "Nacht", "mich", "\u00f6d'", "und", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und irrt' ich auch in weit entfernten Landen", "tokens": ["Und", "irrt'", "ich", "auch", "in", "weit", "ent\u00b7fern\u00b7ten", "Lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch Gluth und Frost, durch W\u00fcsteney und Meer,", "tokens": ["Durch", "Gluth", "und", "Frost", ",", "durch", "W\u00fcs\u00b7te\u00b7ney", "und", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verfolgt, bedroht, verlassen, unverstanden,", "tokens": ["Ver\u00b7folgt", ",", "be\u00b7droht", ",", "ver\u00b7las\u00b7sen", ",", "un\u00b7ver\u00b7stan\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "$,", "VVPP", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In Sturm und Noth, mit mattem Fu\u00df, umher,", "tokens": ["In", "Sturm", "und", "Noth", ",", "mit", "mat\u00b7tem", "Fu\u00df", ",", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "ADJA", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch w\u00fcrde nie dein Bild sich von mir trennen,", "tokens": ["Doch", "w\u00fcr\u00b7de", "nie", "dein", "Bild", "sich", "von", "mir", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPOSAT", "NN", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dein w\u00fcrd' ich seyn und dich noch sterbend nennen.", "tokens": ["Dein", "w\u00fcrd'", "ich", "seyn", "und", "dich", "noch", "ster\u00b7bend", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPER", "VAINF", "KON", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn wie belebt das ungezwung'ne Eisen", "tokens": ["Denn", "wie", "be\u00b7lebt", "das", "un\u00b7ge\u00b7zwung'\u00b7ne", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VVPP", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich folgsam naht dem fesselnden Magnet,", "tokens": ["Sich", "folg\u00b7sam", "naht", "dem", "fes\u00b7seln\u00b7den", "Mag\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie ewig treu in wandellosen Kreisen", "tokens": ["Wie", "e\u00b7wig", "treu", "in", "wan\u00b7del\u00b7lo\u00b7sen", "Krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Erde sich um's Licht der Sonne dreht,", "tokens": ["Die", "Er\u00b7de", "sich", "um's", "Licht", "der", "Son\u00b7ne", "dreht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "NE", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie ohne Rast auf n\u00e4cht'gen Pilgerreisen", "tokens": ["Wie", "oh\u00b7ne", "Rast", "auf", "n\u00e4cht'\u00b7gen", "Pil\u00b7ger\u00b7rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von Meer zu Meer die Schaar der Sterne geht,", "tokens": ["Von", "Meer", "zu", "Meer", "die", "Schaar", "der", "Ster\u00b7ne", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So ward auch ich in dunkeln Schicksalsstunden", "tokens": ["So", "ward", "auch", "ich", "in", "dun\u00b7keln", "Schick\u00b7sals\u00b7stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit finsterm Zwang an deinen Pfad gebunden.", "tokens": ["Mit", "fins\u00b7term", "Zwang", "an", "dei\u00b7nen", "Pfad", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und magst du auch mich stolz und kalt verlassen,", "tokens": ["Und", "magst", "du", "auch", "mich", "stolz", "und", "kalt", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nimmer Trost und Freude mir verleihn,", "tokens": ["Und", "nim\u00b7mer", "Trost", "und", "Freu\u00b7de", "mir", "ver\u00b7leihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mag liebend einst ein Andrer dich umfassen,", "tokens": ["Mag", "lie\u00b7bend", "einst", "ein", "A\u00b7ndrer", "dich", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wilder Schmerz mein Innerstes entzweyn,", "tokens": ["Und", "wil\u00b7der", "Schmerz", "mein", "In\u00b7ners\u00b7tes", "ent\u00b7zweyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und k\u00f6nntest du auch je mich feindlich hassen", "tokens": ["Und", "k\u00f6nn\u00b7test", "du", "auch", "je", "mich", "feind\u00b7lich", "has\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und deines Siegs und meiner Noth dich freun,", "tokens": ["Und", "dei\u00b7nes", "Siegs", "und", "mei\u00b7ner", "Noth", "dich", "freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du zw\u00e4ngst mich leicht, in ungeheuren Leiden,", "tokens": ["Du", "zw\u00e4ngst", "mich", "leicht", ",", "in", "un\u00b7ge\u00b7heu\u00b7ren", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Vom Leben wohl, doch nicht von dir zu scheiden.", "tokens": ["Vom", "Le\u00b7ben", "wohl", ",", "doch", "nicht", "von", "dir", "zu", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "ADV", "PTKNEG", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ach, Alles, was verkn\u00fcpft mit deinem Leben,", "tokens": ["Ach", ",", "Al\u00b7les", ",", "was", "ver\u00b7kn\u00fcpft", "mit", "dei\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIS", "$,", "PWS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur Kunde mir von deiner N\u00e4he giebt,", "tokens": ["Nur", "Kun\u00b7de", "mir", "von", "dei\u00b7ner", "N\u00e4\u00b7he", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der leichte Flor, der deine Brust umgeben,", "tokens": ["Der", "leich\u00b7te", "Flor", ",", "der", "dei\u00b7ne", "Brust", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Werk, woran die zarte Hand sich \u00fcbt,", "tokens": ["Das", "Werk", ",", "wo\u00b7ran", "die", "zar\u00b7te", "Hand", "sich", "\u00fcbt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Saiten, die von deinem Finger beben,", "tokens": ["Die", "Sai\u00b7ten", ",", "die", "von", "dei\u00b7nem", "Fin\u00b7ger", "be\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Blumen, die dein Auge w\u00e4hlt und liebt,", "tokens": ["Die", "Blu\u00b7men", ",", "die", "dein", "Au\u00b7ge", "w\u00e4hlt", "und", "liebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Raum, die Luft, das Licht, das dich umfangen,", "tokens": ["Der", "Raum", ",", "die", "Luft", ",", "das", "Licht", ",", "das", "dich", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Weckt Lieb' und Lust und Schmerz mir und Verlangen.", "tokens": ["Weckt", "Lieb'", "und", "Lust", "und", "Schmerz", "mir", "und", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "KON", "NN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und seh' ich dann dich selber vor mir stehen,", "tokens": ["Und", "seh'", "ich", "dann", "dich", "sel\u00b7ber", "vor", "mir", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Monde gleich an dunkler Wolken Rand,", "tokens": ["Dem", "Mon\u00b7de", "gleich", "an", "dunk\u00b7ler", "Wol\u00b7ken", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "L\u00e4\u00dft freundlich mir dein klarer Blick sich sehen,", "tokens": ["L\u00e4\u00dft", "freund\u00b7lich", "mir", "dein", "kla\u00b7rer", "Blick", "sich", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PPOSAT", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ber\u00fchrt nur leis' im Nahn mich deine Hand,", "tokens": ["Be\u00b7r\u00fchrt", "nur", "leis'", "im", "Nahn", "mich", "dei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPRART", "NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "F\u00fchl' ich von fern nur deines Mundes Wehen,", "tokens": ["F\u00fchl'", "ich", "von", "fern", "nur", "dei\u00b7nes", "Mun\u00b7des", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJD", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Streift fl\u00fcchtig nur dein Arm mich, dein Gewand,", "tokens": ["Streift", "fl\u00fcch\u00b7tig", "nur", "dein", "Arm", "mich", ",", "dein", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "PPOSAT", "NN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dann ringen schnell im wunderbaren Spiele", "tokens": ["Dann", "rin\u00b7gen", "schnell", "im", "wun\u00b7der\u00b7ba\u00b7ren", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Durch meine Brust verworrene Gef\u00fchle.", "tokens": ["Durch", "mei\u00b7ne", "Brust", "ver\u00b7wor\u00b7re\u00b7ne", "Ge\u00b7f\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wie still am Rand der wilden Felsenquelle", "tokens": ["Wie", "still", "am", "Rand", "der", "wil\u00b7den", "Fel\u00b7sen\u00b7quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In linder Luft die stolze Rose bl\u00fcht,", "tokens": ["In", "lin\u00b7der", "Luft", "die", "stol\u00b7ze", "Ro\u00b7se", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Inde\u00df ihr Bild im Strom der raschen Welle", "tokens": ["In\u00b7de\u00df", "ihr", "Bild", "im", "Strom", "der", "ra\u00b7schen", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Unruhig schwankt und auf und nieder flieht:", "tokens": ["Un\u00b7ru\u00b7hig", "schwankt", "und", "auf", "und", "nie\u00b7der", "flieht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "APPR", "KON", "PTKVZ", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "So strahlst auch du in wundersel'ger Helle,", "tokens": ["So", "strahlst", "auch", "du", "in", "wun\u00b7der\u00b7sel'\u00b7ger", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit klarem Sinn und friedlichem Gem\u00fcth;", "tokens": ["Mit", "kla\u00b7rem", "Sinn", "und", "fried\u00b7li\u00b7chem", "Ge\u00b7m\u00fcth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+