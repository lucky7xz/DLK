 "measure": "iambic.penta"}, "line.4": {"text": "Nichts an mir ist, nichts in mir selbst mehr mein,", "tokens": ["Nichts", "an", "mir", "ist", ",", "nichts", "in", "mir", "selbst", "mehr", "mein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPER", "VAFIN", "$,", "PIS", "APPR", "PPER", "ADV", "ADV", "PPOSAT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Flammen f\u00fchl' ich durch die Brust mir rinnen,", "tokens": ["Und", "Flam\u00b7men", "f\u00fchl'", "ich", "durch", "die", "Brust", "mir", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und k\u00e4mpfe wild mit Zweifel, Trug und Schein;", "tokens": ["Und", "k\u00e4mp\u00b7fe", "wild", "mit", "Zwei\u00b7fel", ",", "Trug", "und", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mit Licht und Nacht in wandelbaren Wogen", "tokens": ["Mit", "Licht", "und", "Nacht", "in", "wan\u00b7del\u00b7ba\u00b7ren", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "H\u00e4lt Lust und Leid mir Blick und Geist umzogen.", "tokens": ["H\u00e4lt", "Lust", "und", "Leid", "mir", "Blick", "und", "Geist", "um\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PPER", "NN", "KON", "NN", "VVIZU", "$."], "met