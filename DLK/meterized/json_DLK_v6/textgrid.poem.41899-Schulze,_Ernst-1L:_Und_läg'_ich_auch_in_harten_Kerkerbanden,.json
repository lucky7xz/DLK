ambic.tetra.relaxed"}, "line.8": {"text": "Gern w\u00fcrd' er ihr, der Feindlichen, es geben.", "tokens": ["Gern", "w\u00fcrd'", "er", "ihr", ",", "der", "Feind\u00b7li\u00b7chen", ",", "es", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "ART", "NN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Und bin ich auch von ew'ger Qual zerrissen,", "tokens": ["Und", "bin", "ich", "auch", "von", "ew'\u00b7ger", "Qual", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vergeh' ich auch im rastlos wilden Streit,", "tokens": ["Ver\u00b7geh'", "ich", "auch", "im", "rast\u00b7los", "wil\u00b7den", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nichts will mein Herz von Rath und Rettung wissen,", "tokens": ["Nichts", "will", "mein", "Herz", "von", "Rath", "und", "Ret\u00b7tung", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn nicht das Gl\u00fcck die volle Gunst mir beut,", "tokens": ["Wenn", "nicht", "das", "Gl\u00fcck", "die", "vol\u00b7le", "Gunst", "mir", "beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn mit dem Schmerz m\u00fc\u00dft' ich das Leben missen,", "tokens": ["Denn", "mit", "dem", "Schmerz", "m\u00fc\u00dft'", "ich", "das", "Le\u00b7ben", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem Liebe nur Licht, Kraft und Athem leiht.", "tokens": ["Dem", "Lie\u00b7be", "nur", "Licht", ",", "Kraft", "und", "A\u00b7them", "leiht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+++-+-+", "measure": "zehnsilber"}, "line.7": {"text": "Eh langsam mir Gef\u00fchl und Sehnsucht sterben,", "tokens": ["Eh", "lang\u00b7sam", "mir", "Ge\u00b7f\u00fchl", "und", "Sehn\u00b7sucht", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mag lieber rasch mich Kampf und Sturm verderben!", "tokens": ["Mag", "lie\u00b7ber", "rasch", "mich", "Kampf", "und", "Sturm", "ver\u00b7der\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "O w\u00e4rst du doch als K\u00f6nigin geboren", "tokens": ["O", "w\u00e4rst", "du", "doch", "als", "K\u00f6\u00b7ni\u00b7gin", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADV", "KOUS", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und h\u00e4ttest mich aus deines Volkes Zahl", "tokens": ["Und", "h\u00e4t\u00b7test", "mich", "aus", "dei\u00b7nes", "Vol\u00b7kes", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum niedrigsten der Diener dir erkohren,", "tokens": ["Zum", "nied\u00b7rigs\u00b7ten", "der", "Die\u00b7ner", "dir", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Den Becher dir zu f\u00fcllen nur beym Mahl!", "tokens": ["Den", "Be\u00b7cher", "dir", "zu", "f\u00fcl\u00b7len", "nur", "beym", "Mahl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wohl hab' ich jetzt die Freyheit l\u00e4ngst verloren,", "tokens": ["Wohl", "hab'", "ich", "jetzt", "die", "Frey\u00b7heit", "l\u00e4ngst", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und ach, doch ist mein Loos nicht deine Wahl!", "tokens": ["Und", "ach", ",", "doch", "ist", "mein", "Loos", "nicht", "dei\u00b7ne", "Wahl", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und mu\u00df auch ich mich ganz den Deinen nennen,", "tokens": ["Und", "mu\u00df", "auch", "ich", "mich", "ganz", "den", "Dei\u00b7nen", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PPER", "PRF", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Du willst mir nie den s\u00fc\u00dfen Namen g\u00f6nnen!", "tokens": ["Du", "willst", "mir", "nie", "den", "s\u00fc\u00b7\u00dfen", "Na\u00b7men", "g\u00f6n\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "O nimm es hin, dies jugendliche Leben,", "tokens": ["O", "nimm", "es", "hin", ",", "dies", "ju\u00b7gend\u00b7li\u00b7che", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dies Herz, das sonst so k\u00fchn, so fr\u00f6hlich schlug,", "tokens": ["Dies", "Herz", ",", "das", "sonst", "so", "k\u00fchn", ",", "so", "fr\u00f6h\u00b7lich", "schlug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den treuen Sinn, des Willens edles Streben,", "tokens": ["Den", "treu\u00b7en", "Sinn", ",", "des", "Wil\u00b7lens", "ed\u00b7les", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Geist, der nie ein schn\u00f6des Band ertrug!", "tokens": ["Den", "Geist", ",", "der", "nie", "ein", "schn\u00f6\u00b7des", "Band", "er\u00b7trug", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mein Hoffen selbst, ich will auch das dir geben;", "tokens": ["Mein", "Hof\u00b7fen", "selbst", ",", "ich", "will", "auch", "das", "dir", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,", "PPER", "VMFIN", "ADV", "ART", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcr dich ist nichts mir lieb und werth genug!", "tokens": ["F\u00fcr", "dich", "ist", "nichts", "mir", "lieb", "und", "werth", "ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIS", "PPER", "ADJD", "KON", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O da\u00df mein Herz doch einmal nur erriethe,", "tokens": ["O", "da\u00df", "mein", "Herz", "doch", "ein\u00b7mal", "nur", "er\u00b7rie\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPOSAT", "NN", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nicht schein' auch dir ganz werthlos, was ich biete!", "tokens": ["Nicht", "schein'", "auch", "dir", "ganz", "wert\u00b7hlos", ",", "was", "ich", "bie\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ADV", "PPER", "ADV", "ADJD", "$,", "PWS", "PPER", "VVFIN", "$."], "m