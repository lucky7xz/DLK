{"dta.poem.15871": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "76.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verbannung immer ist die allerkleinste Reise,", "tokens": ["Ver\u00b7ban\u00b7nung", "im\u00b7mer", "ist", "die", "al\u00b7ler\u00b7kleins\u00b7te", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verbannung aus dem Bann geweihter Zauberkreise.", "tokens": ["Ver\u00b7ban\u00b7nung", "aus", "dem", "Bann", "ge\u00b7weih\u00b7ter", "Zau\u00b7ber\u00b7krei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie fest der Zauberbann ist um dein Haus gezogen,", "tokens": ["Wie", "fest", "der", "Zau\u00b7ber\u00b7bann", "ist", "um", "dein", "Haus", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das merkst du dann erst wann du bist heraus gezogen.", "tokens": ["Das", "merkst", "du", "dann", "erst", "wann", "du", "bist", "he\u00b7raus", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "PWAV", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mit der Entfernung nimmt nicht ab, nimmt zu der Zug,", "tokens": ["Mit", "der", "Ent\u00b7fer\u00b7nung", "nimmt", "nicht", "ab", ",", "nimmt", "zu", "der", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und zieht in deinen Kreis zur\u00fcck dich bald genug.", "tokens": ["Und", "zieht", "in", "dei\u00b7nen", "Kreis", "zu\u00b7r\u00fcck", "dich", "bald", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}