{"textgrid.poem.64463": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ist T\u00f6ffels Frau nicht klug, ja fast gelehrt?", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist T\u00f6ffels Frau nicht klug, ja fast gelehrt?", "tokens": ["Ist", "T\u00f6f\u00b7fels", "Frau", "nicht", "klug", ",", "ja", "fast", "ge\u00b7lehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "NN", "PTKNEG", "ADJD", "$,", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und denkt ihr Mann nicht elend und verkehrt?", "tokens": ["Und", "denkt", "ihr", "Mann", "nicht", "e\u00b7lend", "und", "ver\u00b7kehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Jeder spricht: Sie kann ja nichts daf\u00fcr!", "tokens": ["Ein", "Je\u00b7der", "spricht", ":", "Sie", "kann", "ja", "nichts", "da\u00b7f\u00fcr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$.", "PPER", "VMFIN", "ADV", "PIS", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "W\u00fcnscht T\u00f6ffel nicht sich sehnlich einen Erben?", "tokens": ["W\u00fcnscht", "T\u00f6f\u00b7fel", "nicht", "sich", "sehn\u00b7lich", "ei\u00b7nen", "Er\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKNEG", "PRF", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wird mit ihm nicht T\u00f6ffels Stamm ersterben?", "tokens": ["Und", "wird", "mit", "ihm", "nicht", "T\u00f6f\u00b7fels", "Stamm", "ers\u00b7ter\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPER", "PTKNEG", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Jeder spricht: Da liegt die Schuld an ihr!", "tokens": ["Ein", "Je\u00b7der", "spricht", ":", "Da", "liegt", "die", "Schuld", "an", "ihr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$.", "ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}