{"dta.poem.15995": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "49.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Untern bilden sich nach ihrer Obern Bilde,", "tokens": ["Die", "Un\u00b7tern", "bil\u00b7den", "sich", "nach", "ih\u00b7rer", "O\u00b7bern", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu Dumpfheit oder Sinn, zu Herbheit oder Milde.", "tokens": ["Zu", "Dum\u00b7pf\u00b7heit", "o\u00b7der", "Sinn", ",", "zu", "Herb\u00b7heit", "o\u00b7der", "Mil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Weisen haben dis zur unbedingten Huldigung", "tokens": ["Die", "Wei\u00b7sen", "ha\u00b7ben", "dis", "zur", "un\u00b7be\u00b7ding\u00b7ten", "Hul\u00b7di\u00b7gung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PDS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der F\u00fcrsten nicht gesagt, noch zu des Volks Entschuldigung.", "tokens": ["Der", "F\u00fcrs\u00b7ten", "nicht", "ge\u00b7sagt", ",", "noch", "zu", "des", "Volks", "Ent\u00b7schul\u00b7di\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "$,", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Denn schlecht nicht m\u00fcssen seyn, die schlechtes Muster haben,", "tokens": ["Denn", "schlecht", "nicht", "m\u00fcs\u00b7sen", "seyn", ",", "die", "schlech\u00b7tes", "Mus\u00b7ter", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "VMFIN", "VAINF", "$,", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch doppelt s\u00fcndigen, die b\u00f6ses Beispiel gaben.", "tokens": ["Doch", "dop\u00b7pelt", "s\u00fcn\u00b7di\u00b7gen", ",", "die", "b\u00f6\u00b7ses", "Bei\u00b7spiel", "ga\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}