{"textgrid.poem.59230": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Dichter", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo treues Wollen, redlich Streben", "tokens": ["Wo", "treu\u00b7es", "Wol\u00b7len", ",", "red\u00b7lich", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "$,", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und rechten Sinn der Rechte sp\u00fcrt,", "tokens": ["Und", "rech\u00b7ten", "Sinn", "der", "Rech\u00b7te", "sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das mu\u00df die Seele ihm erheben,", "tokens": ["Das", "mu\u00df", "die", "See\u00b7le", "ihm", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das hat mich jedesmal ger\u00fchrt.", "tokens": ["Das", "hat", "mich", "je\u00b7des\u00b7mal", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Reich des Glaubens ist geendet,", "tokens": ["Das", "Reich", "des", "Glau\u00b7bens", "ist", "ge\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zerst\u00f6rt die alte Herrlichkeit,", "tokens": ["Zer\u00b7st\u00f6rt", "die", "al\u00b7te", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sch\u00f6nheit weinend abgewendet,", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "wei\u00b7nend", "ab\u00b7ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So gnadenlos ist unsre Zeit.", "tokens": ["So", "gna\u00b7den\u00b7los", "ist", "uns\u00b7re", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O Einfalt, gut in frommen Herzen,", "tokens": ["O", "Ein\u00b7falt", ",", "gut", "in", "from\u00b7men", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du z\u00fcchtig sch\u00f6ne Gottesbraut!", "tokens": ["Du", "z\u00fcch\u00b7tig", "sch\u00f6\u00b7ne", "Got\u00b7tes\u00b7braut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dich schlugen sie mit frechen Scherzen,", "tokens": ["Dich", "schlu\u00b7gen", "sie", "mit", "fre\u00b7chen", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil dir vor ihrer Klugheit graut.", "tokens": ["Weil", "dir", "vor", "ih\u00b7rer", "Klug\u00b7heit", "graut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wo findst du nun ein Haus, vertrieben,", "tokens": ["Wo", "findst", "du", "nun", "ein", "Haus", ",", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo man dir deine Wunder l\u00e4\u00dft,", "tokens": ["Wo", "man", "dir", "dei\u00b7ne", "Wun\u00b7der", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das treue Tun, das sch\u00f6ne Lieben,", "tokens": ["Das", "treu\u00b7e", "Tun", ",", "das", "sch\u00f6\u00b7ne", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Lebens fromm vergn\u00fcglich Fest?", "tokens": ["Des", "Le\u00b7bens", "fromm", "ver\u00b7gn\u00fcg\u00b7lich", "Fest", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo findest du den alten Garten,", "tokens": ["Wo", "fin\u00b7dest", "du", "den", "al\u00b7ten", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Spielzeug, wunderbares Kind,", "tokens": ["Dein", "Spiel\u00b7zeug", ",", "wun\u00b7der\u00b7ba\u00b7res", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sterne heil'ge Redensarten,", "tokens": ["Der", "Ster\u00b7ne", "heil'\u00b7ge", "Re\u00b7dens\u00b7ar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Morgenrot, den frischen Wind?", "tokens": ["Das", "Mor\u00b7gen\u00b7rot", ",", "den", "fri\u00b7schen", "Wind", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie hat die Sonne sch\u00f6n geschienen!", "tokens": ["Wie", "hat", "die", "Son\u00b7ne", "sch\u00f6n", "ge\u00b7schie\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun ist so alt und schwach die Zeit;", "tokens": ["Nun", "ist", "so", "alt", "und", "schwach", "die", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie stehst so jung du unter ihnen,", "tokens": ["Wie", "stehst", "so", "jung", "du", "un\u00b7ter", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADJD", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie wird mein Herz mir stark und weit!", "tokens": ["Wie", "wird", "mein", "Herz", "mir", "stark", "und", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Dichter kann nicht mit verarmen;", "tokens": ["Der", "Dich\u00b7ter", "kann", "nicht", "mit", "ver\u00b7ar\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn alles um ihn her zerf\u00e4llt,", "tokens": ["Wenn", "al\u00b7les", "um", "ihn", "her", "zer\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "APZR", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hebt ihn ein g\u00f6ttliches Erbarmen \u2013", "tokens": ["Hebt", "ihn", "ein", "g\u00f6tt\u00b7li\u00b7ches", "Er\u00b7bar\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Dichter ist das Herz der Welt.", "tokens": ["Der", "Dich\u00b7ter", "ist", "das", "Herz", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Den bl\u00f6den Willen aller Wesen,", "tokens": ["Den", "bl\u00f6\u00b7den", "Wil\u00b7len", "al\u00b7ler", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Irdischen des Herren Spur,", "tokens": ["Im", "Ir\u00b7di\u00b7schen", "des", "Her\u00b7ren", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Soll er durch Liebeskraft erl\u00f6sen,", "tokens": ["Soll", "er", "durch", "Lie\u00b7bes\u00b7kraft", "er\u00b7l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der sch\u00f6ne Liebling der Natur.", "tokens": ["Der", "sch\u00f6\u00b7ne", "Lieb\u00b7ling", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Drum hat ihm Gott das Wort gegeben,", "tokens": ["Drum", "hat", "ihm", "Gott", "das", "Wort", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das k\u00fchn das Dunkelste benennt,", "tokens": ["Das", "k\u00fchn", "das", "Dun\u00b7kels\u00b7te", "be\u00b7nennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den frommen Ernst im reichen Leben,", "tokens": ["Den", "from\u00b7men", "Ernst", "im", "rei\u00b7chen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Freudigkeit, die keiner kennt.", "tokens": ["Die", "Freu\u00b7dig\u00b7keit", ",", "die", "kei\u00b7ner", "kennt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Da soll er singen frei auf Erden,", "tokens": ["Da", "soll", "er", "sin\u00b7gen", "frei", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Lust und Not auf Gott vertraun,", "tokens": ["In", "Lust", "und", "Not", "auf", "Gott", "ver\u00b7traun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df aller Herzen freier werden,", "tokens": ["Da\u00df", "al\u00b7ler", "Her\u00b7zen", "frei\u00b7er", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Eratmend in die Kl\u00e4nge schaun.", "tokens": ["E\u00b7rat\u00b7mend", "in", "die", "Kl\u00e4n\u00b7ge", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Der Ehre sei er recht zum Horte,", "tokens": ["Der", "Eh\u00b7re", "sei", "er", "recht", "zum", "Hor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schande leucht er ins Gesicht!", "tokens": ["Der", "Schan\u00b7de", "leucht", "er", "ins", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Viel Wunderkraft ist in dem Worte,", "tokens": ["Viel", "Wun\u00b7der\u00b7kraft", "ist", "in", "dem", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das hell aus reinem Herzen bricht.", "tokens": ["Das", "hell", "aus", "rei\u00b7nem", "Her\u00b7zen", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Vor Eitelkeit soll er vor allen", "tokens": ["Vor", "Ei\u00b7tel\u00b7keit", "soll", "er", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PPER", "APPR", "PIAT"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Streng h\u00fcten sein unschuld'ges Herz,", "tokens": ["Streng", "h\u00fc\u00b7ten", "sein", "un\u00b7schuld'\u00b7ges", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Falschen nimmer sich gefallen,", "tokens": ["Im", "Fal\u00b7schen", "nim\u00b7mer", "sich", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um eitel Witz und blanken Scherz.", "tokens": ["Um", "ei\u00b7tel", "Witz", "und", "blan\u00b7ken", "Scherz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Oh, la\u00dft unedle M\u00fche fahren,", "tokens": ["Oh", ",", "la\u00dft", "u\u00b7ned\u00b7le", "M\u00fc\u00b7he", "fah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O klingelt, glei\u00dft und spielet nicht", "tokens": ["O", "klin\u00b7gelt", ",", "glei\u00dft", "und", "spie\u00b7let", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Licht und Gnad, so ihr erfahren,", "tokens": ["Mit", "Licht", "und", "Gnad", ",", "so", "ihr", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur S\u00fcnde macht ihr das Gedicht!", "tokens": ["Zur", "S\u00fcn\u00b7de", "macht", "ihr", "das", "Ge\u00b7dicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Den lieben Gott la\u00df in dir walten,", "tokens": ["Den", "lie\u00b7ben", "Gott", "la\u00df", "in", "dir", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Aus frischer Brust nur treulich sing!", "tokens": ["Aus", "fri\u00b7scher", "Brust", "nur", "treu\u00b7lich", "sing", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was wahr in dir, wird sich gestalten,", "tokens": ["Was", "wahr", "in", "dir", ",", "wird", "sich", "ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "PPER", "$,", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das andre ist erb\u00e4rmlich Ding. \u2013", "tokens": ["Das", "and\u00b7re", "ist", "er\u00b7b\u00e4rm\u00b7lich", "Ding", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "PIS", "VAFIN", "ADJD", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Den Morgen seh ich ferne scheinen,", "tokens": ["Den", "Mor\u00b7gen", "seh", "ich", "fer\u00b7ne", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Str\u00f6me ziehn im gr\u00fcnen Grund,", "tokens": ["Die", "Str\u00f6\u00b7me", "ziehn", "im", "gr\u00fc\u00b7nen", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir ist so wohl! \u2013 die's ehrlich meinen,", "tokens": ["Mir", "ist", "so", "wohl", "!", "\u2013", "die's", "ehr\u00b7lich", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$.", "$(", "PDS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die gr\u00fc\u00df ich all aus Herzensgrund!", "tokens": ["Die", "gr\u00fc