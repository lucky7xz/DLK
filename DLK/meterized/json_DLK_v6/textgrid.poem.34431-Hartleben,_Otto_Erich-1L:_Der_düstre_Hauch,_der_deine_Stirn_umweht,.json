{"textgrid.poem.34431": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der d\u00fcstre Hauch, der deine Stirn umweht,", "genre": "verse", "period": "N.A.", "pub_year": 1885, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der d\u00fcstre Hauch, der deine Stirn umweht,", "tokens": ["Der", "d\u00fcst\u00b7re", "Hauch", ",", "der", "dei\u00b7ne", "Stirn", "um\u00b7weht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der stille Zug, der um Erbarmen fleht,", "tokens": ["der", "stil\u00b7le", "Zug", ",", "der", "um", "Er\u00b7bar\u00b7men", "fleht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "das Zucken, das die Lippen dir bewegt,", "tokens": ["das", "Zu\u00b7cken", ",", "das", "die", "Lip\u00b7pen", "dir", "be\u00b7wegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "hat wunderbar die Seele mir erregt. \u2013", "tokens": ["hat", "wun\u00b7der\u00b7bar", "die", "See\u00b7le", "mir", "er\u00b7regt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dein Antlitz spricht von kaum verwehtem Leid,", "tokens": ["Dein", "Ant\u00b7litz", "spricht", "von", "kaum", "ver\u00b7weh\u00b7tem", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "verlornem Sinnen ist dein Blick geweiht,", "tokens": ["ver\u00b7lor\u00b7nem", "Sin\u00b7nen", "ist", "dein", "Blick", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "wie welken Blumen, die die H\u00e4upter neigen,", "tokens": ["wie", "wel\u00b7ken", "Blu\u00b7men", ",", "die", "die", "H\u00e4up\u00b7ter", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PWAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "scheint dir der Erde Sonnenlust zu schweigen.", "tokens": ["scheint", "dir", "der", "Er\u00b7de", "Son\u00b7nen\u00b7lust", "zu", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und doch! \u2013 Ein Etwas flammt noch von Verlangen,", "tokens": ["Und", "doch", "!", "\u2013", "Ein", "Et\u00b7was", "flammt", "noch", "von", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "$(", "ART", "ADV", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ein Etwas will an diesem Leben hangen,", "tokens": ["ein", "Et\u00b7was", "will", "an", "die\u00b7sem", "Le\u00b7ben", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VMFIN", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "ein Etwas dr\u00e4ngt nach ungefundner Lust \u2013", "tokens": ["ein", "Et\u00b7was", "dr\u00e4ngt", "nach", "un\u00b7ge\u00b7fund\u00b7ner", "Lust", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "o f\u00e4ndest du das Gl\u00fcck an meiner Brust!", "tokens": ["o", "f\u00e4n\u00b7dest", "du", "das", "Gl\u00fcck", "an", "mei\u00b7ner", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}