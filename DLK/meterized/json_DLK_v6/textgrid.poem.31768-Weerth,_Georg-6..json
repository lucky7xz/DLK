{"textgrid.poem.31768": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So sprachen wir wohl; und Soherr, mein Freund,", "tokens": ["So", "spra\u00b7chen", "wir", "wohl", ";", "und", "So\u00b7herr", ",", "mein", "Freund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "KON", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Viel k\u00f6stliche Sp\u00e4\u00dfe machte.", "tokens": ["Viel", "k\u00f6st\u00b7li\u00b7che", "Sp\u00e4\u00b7\u00dfe", "mach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der junge Herr Morgen verschiedenemal", "tokens": ["Der", "jun\u00b7ge", "Herr", "Mor\u00b7gen", "ver\u00b7schie\u00b7de\u00b7ne\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "NE"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Seine herzlichen Tautr\u00e4nen lachte.", "tokens": ["Sei\u00b7ne", "herz\u00b7li\u00b7chen", "Tau\u00b7tr\u00e4\u00b7nen", "lach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--++-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Und ein L\u00fcftchen wehte von R\u00fcdesheim", "tokens": ["Und", "ein", "L\u00fcft\u00b7chen", "weh\u00b7te", "von", "R\u00fc\u00b7des\u00b7heim"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NE"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und kr\u00e4uselte \u00fcber die Wellen", "tokens": ["Und", "kr\u00e4u\u00b7sel\u00b7te", "\u00fc\u00b7ber", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und k\u00fc\u00dfte am Strande des Herbstes Blum", "tokens": ["Und", "k\u00fc\u00df\u00b7te", "am", "Stran\u00b7de", "des", "Herbs\u00b7tes", "Blum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "NE"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und die Trauben, die dunklen und hellen,", "tokens": ["Und", "die", "Trau\u00b7ben", ",", "die", "dunk\u00b7len", "und", "hel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "KON", "VVINF", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Und schwang sich bergan, und es t\u00f6nte leis", "tokens": ["Und", "schwang", "sich", "ber\u00b7gan", ",", "und", "es", "t\u00f6n\u00b7te", "leis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die \u00c4olsharfe wieder \u2013", "tokens": ["Die", "\u00c4\u00b7ols\u00b7har\u00b7fe", "wie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und es war mir, als s\u00e4nge der Geister Chor", "tokens": ["Und", "es", "war", "mir", ",", "als", "s\u00e4n\u00b7ge", "der", "Geis\u00b7ter", "Chor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "$,", "KOUS", "NN", "ART", "NN", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Ein Lied aus dem \u00bbBuche der Lieder\u00ab.", "tokens": ["Ein", "Lied", "aus", "dem", "\u00bb", "Bu\u00b7che", "der", "Lie\u00b7der", "\u00ab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ART", "$(", "NN", "ART", "NN", "$(", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Aus deinem Buche, du kranker Schwan,", "tokens": ["Aus", "dei\u00b7nem", "Bu\u00b7che", ",", "du", "kran\u00b7ker", "Schwan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der du mu\u00dftest die Tage verbringen", "tokens": ["Der", "du", "mu\u00df\u00b7test", "die", "Ta\u00b7ge", "ver\u00b7brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VMFIN", "ART", "NN", "VVINF"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Im Exile, indes der Heimat H\u00f6hn", "tokens": ["Im", "E\u00b7xi\u00b7le", ",", "in\u00b7des", "der", "Hei\u00b7mat", "H\u00f6hn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "ADV", "ART", "NN", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Von deinem Ruhme klingen. \u2013", "tokens": ["Von", "dei\u00b7nem", "Ruh\u00b7me", "klin\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch Herr Soherr sprach: \u00bbIch glaube, es ist", "tokens": ["Doch", "Herr", "So\u00b7herr", "sprach", ":", "\u00bb", "Ich", "glau\u00b7be", ",", "es", "ist"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "NN", "VVFIN", "$.", "$(", "PPER", "VVFIN", "$,", "PPER", "VAFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Am besten, wir steigen zu Tale", "tokens": ["Am", "bes\u00b7ten", ",", "wir", "stei\u00b7gen", "zu", "Ta\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "$,", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und fr\u00fchst\u00fccken Austern und Kaviar,", "tokens": ["Und", "fr\u00fch\u00b7st\u00fc\u00b7cken", "Aus\u00b7tern", "und", "Ka\u00b7vi\u00b7ar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Oder K\u00e4se, oder Lachs, oder Aale.", "tokens": ["O\u00b7der", "K\u00e4\u00b7se", ",", "o\u00b7der", "Lachs", ",", "o\u00b7der", "Aa\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KON", "NN", "$,", "KON", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Ich gebe Ihnen ein gutes Glas", "tokens": ["Ich", "ge\u00b7be", "Ih\u00b7nen", "ein", "gu\u00b7tes", "Glas"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von einer verst\u00e4ndigen Sorte.\u00ab \u2013", "tokens": ["Von", "ei\u00b7ner", "ver\u00b7st\u00e4n\u00b7di\u00b7gen", "Sor\u00b7te", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sprach's. \u2013 Und ich erkannte den tiefen Sinn", "tokens": ["Sprach'", "s.", "\u2013", "Und", "ich", "er\u00b7kann\u00b7te", "den", "tie\u00b7fen", "Sinn"], "token_info": ["word", "abbreviation", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["XY", "XY", "$(", "KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Dieser h\u00f6chst gewichtigen Worte.", "tokens": ["Die\u00b7ser", "h\u00f6chst", "ge\u00b7wich\u00b7ti\u00b7gen", "Wor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Und der Keller erschlo\u00df sich. Und balde war'n", "tokens": ["Und", "der", "Kel\u00b7ler", "er\u00b7schlo\u00df", "sich", ".", "Und", "bal\u00b7de", "wa\u00b7r'n"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "$.", "KON", "ADV", "VVINF"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Wir in sehr erfreulicher Andacht;", "tokens": ["Wir", "in", "sehr", "er\u00b7freu\u00b7li\u00b7cher", "An\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und nicht an Herrn Engels und nicht an K\u00f6ln,", "tokens": ["Und", "nicht", "an", "Herrn", "En\u00b7gels", "und", "nicht", "an", "K\u00f6ln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "NE", "KON", "PTKNEG", "APPR", "NE", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.4": {"text": "Sondern nur an den Wein jedermann dacht.", "tokens": ["Son\u00b7dern", "nur", "an", "den", "Wein", "je\u00b7der\u00b7mann", "dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "PIS", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.8": {"line.1": {"text": "Und sangen: O Jerum, Jerum, Je!", "tokens": ["Und", "san\u00b7gen", ":", "O", "Je\u00b7rum", ",", "Je\u00b7rum", ",", "Je", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NE", "NE", "$,", "NE", "$,", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und lagen uns in den Armen.", "tokens": ["Und", "la\u00b7gen", "uns", "in", "den", "Ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hosianna! \u2013 Da flogen die T\u00fcren auf,", "tokens": ["Ho\u00b7si\u00b7an\u00b7na", "!", "\u2013", "Da", "flo\u00b7gen", "die", "T\u00fc\u00b7ren", "auf", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "$(", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und herein traten zwei Gendarmen. \u2013 \u2013", "tokens": ["Und", "her\u00b7ein", "tra\u00b7ten", "zwei", "Gen\u00b7dar\u00b7men", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PTKVZ", "VVFIN", "CARD", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}