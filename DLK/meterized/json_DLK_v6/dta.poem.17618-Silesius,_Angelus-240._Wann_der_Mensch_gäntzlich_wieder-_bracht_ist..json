{"dta.poem.17618": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "240.  Wann der Mensch g\u00e4ntzlich wieder-  \n bracht ist.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ist der Mensch zu Gott vollkommlich wieder-", "tokens": ["Wenn", "ist", "der", "Mensch", "zu", "Gott", "voll\u00b7komm\u00b7lich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "ART", "NN", "APPR", "NN", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "bracht?", "tokens": ["bracht", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Wenn er das Muster ist darnach jhn Gott gemacht.", "tokens": ["Wenn", "er", "das", "Mus\u00b7ter", "ist", "dar\u00b7nach", "jhn", "Gott", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "PAV", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}