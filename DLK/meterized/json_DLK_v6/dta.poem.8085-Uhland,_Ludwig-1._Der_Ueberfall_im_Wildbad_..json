{"dta.poem.8085": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1.  Der Ueberfall im Wildbad .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "In sch\u00f6nen Sommertagen, wann lau die L\u00fcfte wehn,", "tokens": ["In", "sch\u00f6\u00b7nen", "Som\u00b7mer\u00b7ta\u00b7gen", ",", "wann", "lau", "die", "L\u00fcf\u00b7te", "wehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die W\u00e4lder lustig gr\u00fcnen, die G\u00e4rten bl\u00fchend stehn,", "tokens": ["Die", "W\u00e4l\u00b7der", "lus\u00b7tig", "gr\u00fc\u00b7nen", ",", "die", "G\u00e4r\u00b7ten", "bl\u00fc\u00b7hend", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da ritt aus Stuttgarts Thoren ein Held von stolzer Art,", "tokens": ["Da", "ritt", "aus", "Stutt\u00b7garts", "Tho\u00b7ren", "ein", "Held", "von", "stol\u00b7zer", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Graf Eberhard der Greiner, der alte Rauschebart.", "tokens": ["Graf", "E\u00b7ber\u00b7hard", "der", "Grei\u00b7ner", ",", "der", "al\u00b7te", "Rau\u00b7sche\u00b7bart", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Mit wenig Edelknechten zieht er in\u2019s Land hinaus,", "tokens": ["Mit", "we\u00b7nig", "E\u00b7del\u00b7knech\u00b7ten", "zieht", "er", "in's", "Land", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "APZR", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er tr\u00e4gt nicht Helm noch Panzer, nicht geht\u2019s auf blut\u2019gen Strau\u00df,", "tokens": ["Er", "tr\u00e4gt", "nicht", "Helm", "noch", "Pan\u00b7zer", ",", "nicht", "geht's", "auf", "blut'\u00b7gen", "Strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "ADV", "NN", "$,", "PTKNEG", "VVFIN", "APPR", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "In\u2019s Wildbad will er reiten, wo hei\u00df ein Quell entspringt,", "tokens": ["In's", "Wild\u00b7bad", "will", "er", "rei\u00b7ten", ",", "wo", "hei\u00df", "ein", "Quell", "ent\u00b7springt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VMFIN", "PPER", "VVFIN", "$,", "PWAV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der Sieche heilt und kr\u00e4ftigt, der Greise wieder j\u00fcngt.", "tokens": ["Der", "Sie\u00b7che", "heilt", "und", "kr\u00e4f\u00b7tigt", ",", "der", "Grei\u00b7se", "wie\u00b7der", "j\u00fcngt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Zu Hirschau bei dem Abte, da kehrt der Ritter ein", "tokens": ["Zu", "Hirsc\u00b7hau", "bei", "dem", "Ab\u00b7te", ",", "da", "kehrt", "der", "Rit\u00b7ter", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "ART"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Und trinkt bei Orgelschalle den k\u00fchlen Klosterwein.", "tokens": ["Und", "trinkt", "bei", "Or\u00b7gel\u00b7schal\u00b7le", "den", "k\u00fch\u00b7len", "Klos\u00b7ter\u00b7wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dann gehts durch Tannenw\u00e4lder in\u2019s gr\u00fcne Thal gesprengt,", "tokens": ["Dann", "gehts", "durch", "Tan\u00b7nen\u00b7w\u00e4l\u00b7der", "in's", "gr\u00fc\u00b7ne", "Thal", "ge\u00b7sprengt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wo durch ihr Felsenbette die Enz sich rauschend dr\u00e4ngt.", "tokens": ["Wo", "durch", "ihr", "Fel\u00b7sen\u00b7bet\u00b7te", "die", "Enz", "sich", "rau\u00b7schend", "dr\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "ART", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Zu Wildbad an dem Markte, da steht ein stattlich Haus,", "tokens": ["Zu", "Wild\u00b7bad", "an", "dem", "Mark\u00b7te", ",", "da", "steht", "ein", "statt\u00b7lich", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es h\u00e4ngt daran zum Zeichen ein blanker Spie\u00df heraus,", "tokens": ["Es", "h\u00e4ngt", "da\u00b7ran", "zum", "Zei\u00b7chen", "ein", "blan\u00b7ker", "Spie\u00df", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPRART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dort steigt der Graf vom Rosse, dort h\u00e4lt er gute Rast,", "tokens": ["Dort", "steigt", "der", "Graf", "vom", "Ros\u00b7se", ",", "dort", "h\u00e4lt", "er", "gu\u00b7te", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Den Quell besucht er t\u00e4glich, der ritterliche Gast.", "tokens": ["Den", "Quell", "be\u00b7sucht", "er", "t\u00e4g\u00b7lich", ",", "der", "rit\u00b7ter\u00b7li\u00b7che", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Wann er sich dann entkleidet und wenig ausgeruht", "tokens": ["Wann", "er", "sich", "dann", "ent\u00b7klei\u00b7det", "und", "we\u00b7nig", "aus\u00b7ge\u00b7ruht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVPP", "KON", "PIS", "VVFIN"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Und sein Gebet gesprochen, so steigt er in die Flut;", "tokens": ["Und", "sein", "Ge\u00b7bet", "ge\u00b7spro\u00b7chen", ",", "so", "steigt", "er", "in", "die", "Flut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er setzt sich stets zur Stelle, wo aus dem Felsenspalt", "tokens": ["Er", "setzt", "sich", "stets", "zur", "Stel\u00b7le", ",", "wo", "aus", "dem", "Fel\u00b7sen\u00b7spalt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPRART", "NN", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Am hei\u00dfesten und vollsten der edle Sprudel wallt.", "tokens": ["Am", "hei\u00b7\u00dfes\u00b7ten", "und", "volls\u00b7ten", "der", "ed\u00b7le", "Spru\u00b7del", "wallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "KON", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Ein angescho\u00dfner Eber, der sich die Wunde wusch,", "tokens": ["Ein", "an\u00b7ge\u00b7scho\u00df\u00b7ner", "E\u00b7ber", ",", "der", "sich", "die", "Wun\u00b7de", "wusch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Verrieth voreinst den J\u00e4gern den Quell in Kluft und Busch,", "tokens": ["Ver\u00b7rieth", "vor\u00b7e\u00b7inst", "den", "J\u00e4\u00b7gern", "den", "Quell", "in", "Kluft", "und", "Busch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nun ist\u2019s dem alten Recken ein lieber Zeitvertreib,", "tokens": ["Nun", "ist's", "dem", "al\u00b7ten", "Re\u00b7cken", "ein", "lie\u00b7ber", "Zeit\u00b7ver\u00b7treib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Zu waschen und zu strecken den narbenvollen Leib.", "tokens": ["Zu", "wa\u00b7schen", "und", "zu", "stre\u00b7cken", "den", "nar\u00b7ben\u00b7vol\u00b7len", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Da k\u00f6mmt einsmals gesprungen sein j\u00fcngster Edelknab\u2019:", "tokens": ["Da", "k\u00f6mmt", "eins\u00b7mals", "ge\u00b7sprun\u00b7gen", "sein", "j\u00fcngs\u00b7ter", "E\u00b7del\u00b7knab'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "VVPP", "VAINF", "ADJA", "NN", "$."], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u201eherr Graf! es zieht ein Haufe das obre Thal herab.", "tokens": ["\u201e", "herr", "Graf", "!", "es", "zieht", "ein", "Hau\u00b7fe", "das", "ob\u00b7re", "Thal", "her\u00b7ab", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NE", "$.", "PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sie tragen schwere Kolben, der Hauptmann f\u00fchrt im Schild", "tokens": ["Sie", "tra\u00b7gen", "schwe\u00b7re", "Kol\u00b7ben", ",", "der", "Haupt\u00b7mann", "f\u00fchrt", "im", "Schild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ein R\u00f6slein roth von Golde und einen Eber wild.\u201c", "tokens": ["Ein", "R\u00f6s\u00b7lein", "roth", "von", "Gol\u00b7de", "und", "ei\u00b7nen", "E\u00b7ber", "wild", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "KON", "ART", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "\u201emein Sohn! das sind die Schlegler, die schlagen kr\u00e4ftig drein, \u2014", "tokens": ["\u201e", "mein", "Sohn", "!", "das", "sind", "die", "Schleg\u00b7ler", ",", "die", "schla\u00b7gen", "kr\u00e4f\u00b7tig", "drein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$.", "PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "ADJD", "PTKVZ", "$,", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gib mir den Leibrock, Junge! \u2014 das ist der Eberstein,", "tokens": ["Gib", "mir", "den", "Lei\u00b7brock", ",", "Jun\u00b7ge", "!", "das", "ist", "der", "E\u00b7bers\u00b7tein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "NN", "$.", "$(", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Ich kenne wohl den Eber, er hat so grimmen Zorn,", "tokens": ["Ich", "ken\u00b7ne", "wohl", "den", "E\u00b7ber", ",", "er", "hat", "so", "grim\u00b7men", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ich kenne wohl die Rose, sie f\u00fchrt so scharfen Dorn.\u201c", "tokens": ["Ich", "ken\u00b7ne", "wohl", "die", "Ro\u00b7se", ",", "sie", "f\u00fchrt", "so", "schar\u00b7fen", "Dorn", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "PPER", "VVFIN", "ADV", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Da k\u00f6mmt ein armer Hirte in athemlosem Lauf:", "tokens": ["Da", "k\u00f6mmt", "ein", "ar\u00b7mer", "Hir\u00b7te", "in", "at\u00b7hem\u00b7lo\u00b7sem", "Lauf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u201eherr Graf! es zieht \u2019ne Rotte das untre Thal herauf.", "tokens": ["\u201e", "herr", "Graf", "!", "es", "zieht", "'ne", "Rot\u00b7te", "das", "un\u00b7tre", "Thal", "her\u00b7auf", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NE", "$.", "PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Hauptmann f\u00fchrt drei Beile sein R\u00fcstzeug gl\u00e4nzt und glei\u00dft,", "tokens": ["Der", "Haupt\u00b7mann", "f\u00fchrt", "drei", "Bei\u00b7le", "sein", "R\u00fcst\u00b7zeug", "gl\u00e4nzt", "und", "glei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "CARD", "NN", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da\u00df mir\u2019s, wie Wetterleuchten, noch in den Augen bei\u00dft.\u201c", "tokens": ["Da\u00df", "mir's", ",", "wie", "Wet\u00b7ter\u00b7leuch\u00b7ten", ",", "noch", "in", "den", "Au\u00b7gen", "bei\u00dft", ".", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NE", "$,", "PWAV", "NN", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-++-+-+", "measure": "unknown.measure.septa"}}, "stanza.10": {"line.1": {"text": "\u201edas ist der Wunnensteiner, ", "tokens": ["\u201e", "das", "ist", "der", "Wun\u00b7nen\u00b7stei\u00b7ner", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gib mir den Mantel, Knabe! \u2014 der Glanz ist mir bekannt,", "tokens": ["Gib", "mir", "den", "Man\u00b7tel", ",", "Kna\u00b7be", "!", "der", "Glanz", "ist", "mir", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "NN", "$.", "$(", "ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.3": {"text": "Er bringt mir wenig Wonne, die Beile hauen gut, \u2014", "tokens": ["Er", "bringt", "mir", "we\u00b7nig", "Won\u00b7ne", ",", "die", "Bei\u00b7le", "hau\u00b7en", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$,", "ART", "NN", "VVFIN", "ADJD", "$,", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Bind mir das Schwerdt zur Seite! \u2014 der Wolf, der lechzt nach", "tokens": ["Bind", "mir", "das", "Schwerdt", "zur", "Sei\u00b7te", "!", "der", "Wolf", ",", "der", "lechzt", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPRART", "NN", "$.", "$(", "ART", "NE", "$,", "PRELS", "VVFIN", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Blut.", "tokens": ["Blut", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.11": {"line.1": {"text": "Ein M\u00e4gdlein mag man schrecken, das sich im Bade schmiegt,", "tokens": ["Ein", "M\u00e4gd\u00b7lein", "mag", "man", "schre\u00b7cken", ",", "das", "sich", "im", "Ba\u00b7de", "schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "VVINF", "$,", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das ist ein lustig Necken, das Niemand Schaden f\u00fcgt,", "tokens": ["Das", "ist", "ein", "lus\u00b7tig", "Ne\u00b7cken", ",", "das", "Nie\u00b7mand", "Scha\u00b7den", "f\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN", "$,", "PRELS", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wird aber \u00fcberfallen ein alter Kriegesheld,", "tokens": ["Wird", "a\u00b7ber", "\u00fc\u00b7berf\u00b7al\u00b7len", "ein", "al\u00b7ter", "Krie\u00b7ge\u00b7sheld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dann gilt\u2019s, wenn nicht sein Leben, doch schweres L\u00f6segeld.\u201c", "tokens": ["Dann", "gilt's", ",", "wenn", "nicht", "sein", "Le\u00b7ben", ",", "doch", "schwe\u00b7res", "L\u00f6\u00b7se\u00b7geld", ".", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PTKNEG", "PPOSAT", "NN", "$,", "ADV", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "Da spricht der arme Hirte: \u201ede\u00df mag noch werden Rath,", "tokens": ["Da", "spricht", "der", "ar\u00b7me", "Hir\u00b7te", ":", "\u201e", "de\u00df", "mag", "noch", "wer\u00b7den", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "$(", "PDS", "VMFIN", "ADV", "VAFIN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich wei\u00df geheime Wege, die noch kein Mensch betrat,", "tokens": ["Ich", "wei\u00df", "ge\u00b7hei\u00b7me", "We\u00b7ge", ",", "die", "noch", "kein", "Mensch", "be\u00b7trat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,", "PRELS", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Kein Ro\u00df mag sie ersteigen, nur Geissen klettern dort,", "tokens": ["Kein", "Ro\u00df", "mag", "sie", "er\u00b7stei\u00b7gen", ",", "nur", "Geis\u00b7sen", "klet\u00b7tern", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVPP", "$,", "ADV", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wollt Ihr sogleich mir folgen, ich bring\u2019 Euch sicher fort.\u201c", "tokens": ["Wollt", "Ihr", "sog\u00b7leich", "mir", "fol\u00b7gen", ",", "ich", "bring'", "Euch", "si\u00b7cher", "fort", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,", "PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "$("], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}}, "stanza.13": {"line.1": {"text": "Sie klimmen durch das Dickicht den steilsten Berg hinan,", "tokens": ["Sie", "klim\u00b7men", "durch", "das", "Di\u00b7ckicht", "den", "steils\u00b7ten", "Berg", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mit seinem guten Schwerdte haut oft der Graf sich Bahn.", "tokens": ["Mit", "sei\u00b7nem", "gu\u00b7ten", "Schwerd\u00b7te", "haut", "oft", "der", "Graf", "sich", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "PRF", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie herb das Fliehen schmecke, noch hatt\u2019 er\u2019s nie vermerkt,", "tokens": ["Wie", "herb", "das", "Flie\u00b7hen", "schme\u00b7cke", ",", "noch", "hatt'", "er's", "nie", "ver\u00b7merkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$,", "ADV", "VAFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Viel lieber m\u00f6cht\u2019 er fechten, das Bad hat ihn gest\u00e4rkt.", "tokens": ["Viel", "lie\u00b7ber", "m\u00f6cht'", "er", "fech\u00b7ten", ",", "das", "Bad", "hat", "ihn", "ge\u00b7st\u00e4rkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVINF", "$,", "PRELS", "NE", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.14": {"line.1": {"text": "In hei\u00dfer Mittagsstunde bergunter und bergauf!", "tokens": ["In", "hei\u00b7\u00dfer", "Mit\u00b7tags\u00b7stun\u00b7de", "ber\u00b7gun\u00b7ter", "und", "ber\u00b7gauf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "KON", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Schon mu\u00df der Graf sich lehnen auf seines Schwerdtes Knauf.", "tokens": ["Schon", "mu\u00df", "der", "Graf", "sich", "leh\u00b7nen", "auf", "sei\u00b7nes", "Schwerd\u00b7tes", "Knauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PRF", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Darob erbarmt\u2019s den Hirten des alten, hohen Herrn,", "tokens": ["Da\u00b7rob", "er\u00b7barmt's", "den", "Hir\u00b7ten", "des", "al\u00b7ten", ",", "ho\u00b7hen", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er nimmt ihn auf den R\u00fccken: \u201eich thu\u2019s von Herzen gern.\u201c", "tokens": ["Er", "nimmt", "ihn", "auf", "den", "R\u00fc\u00b7cken", ":", "\u201e", "ich", "thu's", "von", "Her\u00b7zen", "gern", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$(", "PPER", "VVFIN", "APPR", "NN", "ADV", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.15": {"line.1": {"text": "Da denkt der alte Greiner: \u201ees thut doch wahrlich gut,", "tokens": ["Da", "denkt", "der", "al\u00b7te", "Grei\u00b7ner", ":", "\u201e", "es", "thut", "doch", "wahr\u00b7lich", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "$(", "PPER", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So s\u00e4nftlich seyn getragen von einem treuen Blut;", "tokens": ["So", "s\u00e4nft\u00b7lich", "seyn", "ge\u00b7tra\u00b7gen", "von", "ei\u00b7nem", "treu\u00b7en", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "In F\u00e4hrden und in N\u00f6then zeigt erst das Volk sich \u00e4cht,", "tokens": ["In", "F\u00e4hr\u00b7den", "und", "in", "N\u00f6\u00b7then", "zeigt", "erst", "das", "Volk", "sich", "\u00e4cht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVFIN", "ADV", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Drum soll man nie zertreten sein altes, gutes Recht.\u201c", "tokens": ["Drum", "soll", "man", "nie", "zer\u00b7tre\u00b7ten", "sein", "al\u00b7tes", ",", "gu\u00b7tes", "Recht", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PAV", "VMFIN", "PIS", "ADV", "VVFIN", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.16": {"line.1": {"text": "Als drauf der Graf gerettet zu Stuttgart sitzt im Saal,", "tokens": ["Als", "drauf", "der", "Graf", "ge\u00b7ret\u00b7tet", "zu", "Stutt\u00b7gart", "sitzt", "im", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "ART", "NN", "VVPP", "APPR", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hei\u00dft er \u2019ne M\u00fcnze pr\u00e4gen als ein Ged\u00e4chtni\u00dfmal,", "tokens": ["Hei\u00dft", "er", "'ne", "M\u00fcn\u00b7ze", "pr\u00e4\u00b7gen", "als", "ein", "Ge\u00b7d\u00e4cht\u00b7ni\u00df\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "KOKOM", "ART", "NN", "$,"], "meter": "+--+-+-++-+-+", "measure": "iambic.septa.invert"}, "line.3": {"text": "Er gibt dem treuen Hirten manch blankes St\u00fcck davon,", "tokens": ["Er", "gibt", "dem", "treu\u00b7en", "Hir\u00b7ten", "manch", "blan\u00b7kes", "St\u00fcck", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PIAT", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Auch manchem Herrn vom Schlegel verehrt er eins zum Hohn.", "tokens": ["Auch", "man\u00b7chem", "Herrn", "vom", "Schle\u00b7gel", "ver\u00b7ehrt", "er", "eins", "zum", "Hohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPRART", "NN", "VVFIN", "PPER", "PIS", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.17": {"line.1": {"text": "Dann schickt er t\u00fccht\u2019ge Maurer in\u2019s Wildbad alsofort,", "tokens": ["Dann", "schickt", "er", "t\u00fccht'\u00b7ge", "Mau\u00b7rer", "in's", "Wild\u00b7bad", "al\u00b7so\u00b7fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die sollen Mauern f\u00fchren rings um den offnen Ort,", "tokens": ["Die", "sol\u00b7len", "Mau\u00b7ern", "f\u00fch\u00b7ren", "rings", "um", "den", "off\u00b7nen", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Damit in k\u00fcnft\u2019gen Sommern sich jeder greise Mann,", "tokens": ["Da\u00b7mit", "in", "k\u00fcnft'\u00b7gen", "Som\u00b7mern", "sich", "je\u00b7der", "grei\u00b7se", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ADJA", "NN", "PRF", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Von Feinden ungef\u00e4hrdet, im Bade j\u00fcngen kann.", "tokens": ["Von", "Fein\u00b7den", "un\u00b7ge\u00b7f\u00e4hr\u00b7det", ",", "im", "Ba\u00b7de", "j\u00fcn\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}