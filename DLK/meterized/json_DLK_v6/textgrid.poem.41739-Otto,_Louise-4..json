{"textgrid.poem.41739": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und mich umklang es wie brausender Sturm!", "tokens": ["Und", "mich", "um\u00b7klang", "es", "wie", "brau\u00b7sen\u00b7der", "Sturm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie Orgelklang h\u00f6rt ich's t\u00f6nen,", "tokens": ["Wie", "Or\u00b7gel\u00b7klang", "h\u00f6rt", "ich's", "t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PIS", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und laut in den innersten Tiefen der Seele", "tokens": ["Und", "laut", "in", "den", "in\u00b7ners\u00b7ten", "Tie\u00b7fen", "der", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Vernahm ich ein feierlich Wort,", "tokens": ["Ver\u00b7nahm", "ich", "ein", "fei\u00b7er\u00b7lich", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Das wie ein Echo von diesen W\u00e4nden", "tokens": ["Das", "wie", "ein", "E\u00b7cho", "von", "die\u00b7sen", "W\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "KOKOM", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mir wieder und wieder erklang,", "tokens": ["Mir", "wie\u00b7der", "und", "wie\u00b7der", "er\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "ADV", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Es war das Wort aus dem alten Gesang", "tokens": ["Es", "war", "das", "Wort", "aus", "dem", "al\u00b7ten", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Des mutigen M\u00f6nches vergangener Zeit,", "tokens": ["Des", "mu\u00b7ti\u00b7gen", "M\u00f6n\u00b7ches", "ver\u00b7gan\u00b7ge\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Der mit diesem Lied seine Zelle geweiht.", "tokens": ["Der", "mit", "die\u00b7sem", "Lied", "sei\u00b7ne", "Zel\u00b7le", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PDAT", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "\u00bbund ob die Welt voll Teufel w\u00e4r'", "tokens": ["\u00bb", "und", "ob", "die", "Welt", "voll", "Teu\u00b7fel", "w\u00e4r'"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "KOUS", "ART", "NN", "ADJD", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und wollt uns gar verschlingen,", "tokens": ["Und", "wollt", "uns", "gar", "ver\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "So f\u00fcrchten wir uns nicht so sehr,", "tokens": ["So", "f\u00fcrch\u00b7ten", "wir", "uns", "nicht", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Es mu\u00df uns doch gelingen!\u00ab", "tokens": ["Es", "mu\u00df", "uns", "doch", "ge\u00b7lin\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ja, Luther hat vor Menschen nicht gezittert", "tokens": ["Ja", ",", "Lu\u00b7ther", "hat", "vor", "Men\u00b7schen", "nicht", "ge\u00b7zit\u00b7tert"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NE", "VAFIN", "APPR", "NN", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nicht vor einer Welt voll Teufel!", "tokens": ["Und", "nicht", "vor", "ei\u00b7ner", "Welt", "voll", "Teu\u00b7fel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kam dennoch der Versucher, ein zagender Zweifel", "tokens": ["Kam", "den\u00b7noch", "der", "Ver\u00b7su\u00b7cher", ",", "ein", "za\u00b7gen\u00b7der", "Zwei\u00b7fel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Da fa\u00dfte der k\u00fchne Mann das Tintenfa\u00df", "tokens": ["Da", "fa\u00df\u00b7te", "der", "k\u00fch\u00b7ne", "Mann", "das", "Tin\u00b7ten\u00b7fa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und warf's dem D\u00e4mon siegvoll hin.", "tokens": ["Und", "wa\u00b7rf's", "dem", "D\u00e4\u00b7mon", "sieg\u00b7voll", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "So hat er ", "tokens": ["So", "hat", "er"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "So protestieren wir: schleudern die Tintenflut", "tokens": ["So", "pro\u00b7tes\u00b7tie\u00b7ren", "wir", ":", "schleu\u00b7dern", "die", "Tin\u00b7ten\u00b7flut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "VVFIN", "ART", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Unsrer Begeistrung Str\u00f6me", "tokens": ["Uns\u00b7rer", "Be\u00b7geis\u00b7trung", "Str\u00f6\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "Gegen die Fr\u00f6mmler und Pharis\u00e4er", "tokens": ["Ge\u00b7gen", "die", "Fr\u00f6mm\u00b7ler", "und", "Pha\u00b7ri\u00b7s\u00e4\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "Gegen all die Philister und Spukgestalten", "tokens": ["Ge\u00b7gen", "all", "die", "Phi\u00b7lis\u00b7ter", "und", "Spuk\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ART", "NN", "KON", "NN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.11": {"text": "Die's mit dem Teufel halten,", "tokens": ["Die's", "mit", "dem", "Teu\u00b7fel", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.12": {"text": "Der wider Recht und Pflicht", "tokens": ["Der", "wi\u00b7der", "Recht", "und", "Pflicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Und wider Freiheit ficht.", "tokens": ["Und", "wi\u00b7der", "Frei\u00b7heit", "ficht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Wir werden nimmer die Waffen strecken", "tokens": ["Wir", "wer\u00b7den", "nim\u00b7mer", "die", "Waf\u00b7fen", "stre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Bis alle Feinde rings vernichtet", "tokens": ["Bis", "al\u00b7le", "Fein\u00b7de", "rings", "ver\u00b7nich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und alles Dunkel aufgelichtet.", "tokens": ["Und", "al\u00b7les", "Dun\u00b7kel", "auf\u00b7ge\u00b7lich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Gilt's nicht zu handeln \u2013 gilt es doch zu schreiben.", "tokens": ["Gilt's", "nicht", "zu", "han\u00b7deln", "\u2013", "gilt", "es", "doch", "zu", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "PTKZU", "VVINF", "$(", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Es soll das Wort den L\u00fcgengeist vertreiben:", "tokens": ["Es", "soll", "das", "Wort", "den", "L\u00fc\u00b7gen\u00b7geist", "ver\u00b7trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}}}}}