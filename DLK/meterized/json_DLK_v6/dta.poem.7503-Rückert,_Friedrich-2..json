{"dta.poem.7503": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Flamme w\u00e4chst vom Zug der Luft, und mehrt den Zug;", "tokens": ["Die", "Flam\u00b7me", "w\u00e4chst", "vom", "Zug", "der", "Luft", ",", "und", "mehrt", "den", "Zug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So h\u00e4lt sich Leidenschaft durch Leidenschaft im Flug.", "tokens": ["So", "h\u00e4lt", "sich", "Lei\u00b7den\u00b7schaft", "durch", "Lei\u00b7den\u00b7schaft", "im", "Flug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Feuer sch\u00fcrt der Wind, und l\u00f6scht das Feuer wieder;", "tokens": ["Das", "Feu\u00b7er", "sch\u00fcrt", "der", "Wind", ",", "und", "l\u00f6scht", "das", "Feu\u00b7er", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So k\u00e4mpfet Leidenschaft die Leidenschaft danieder.", "tokens": ["So", "k\u00e4mp\u00b7fet", "Lei\u00b7den\u00b7schaft", "die", "Lei\u00b7den\u00b7schaft", "da\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie still die Lampe brennt am windbeschirmten Ort,", "tokens": ["Wie", "still", "die", "Lam\u00b7pe", "brennt", "am", "wind\u00b7be\u00b7schirm\u00b7ten", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ein beruhigt Herz in Andacht fort und fort.", "tokens": ["So", "ein", "be\u00b7ru\u00b7higt", "Herz", "in", "An\u00b7dacht", "fort", "und", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VVPP", "NN", "APPR", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}