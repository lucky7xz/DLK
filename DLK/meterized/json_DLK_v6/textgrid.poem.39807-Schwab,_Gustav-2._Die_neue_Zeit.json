{"textgrid.poem.39807": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "2. Die neue Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seltnes ward von uns erlebet,", "tokens": ["Selt\u00b7nes", "ward", "von", "uns", "er\u00b7le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einer von den gro\u00dfen Tagen;", "tokens": ["Ei\u00b7ner", "von", "den", "gro\u00b7\u00dfen", "Ta\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, die Weltuhr hat geschlagen,", "tokens": ["Ja", ",", "die", "Wel\u00b7tuhr", "hat", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df die Mitternacht erbebet.", "tokens": ["Da\u00df", "die", "Mit\u00b7ter\u00b7nacht", "er\u00b7be\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Funkelnd gl\u00e4nzten die Gestirne", "tokens": ["Fun\u00b7kelnd", "gl\u00e4nz\u00b7ten", "die", "Ge\u00b7stir\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einem neuen Tag entgegen,", "tokens": ["Ei\u00b7nem", "neu\u00b7en", "Tag", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf der Erde keimte Segen,", "tokens": ["Auf", "der", "Er\u00b7de", "ke\u00b7im\u00b7te", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und der Mensch erhub die Stirne.", "tokens": ["Und", "der", "Mensch", "er\u00b7hub", "die", "Stir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Morgenwolken rot und blutig", "tokens": ["Mor\u00b7gen\u00b7wol\u00b7ken", "rot", "und", "blu\u00b7tig"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kamen drauf herangezogen,", "tokens": ["Ka\u00b7men", "drauf", "her\u00b7an\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nebel kamen aufgeflogen,", "tokens": ["Ne\u00b7bel", "ka\u00b7men", "auf\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch das Herz blieb fest und mutig.", "tokens": ["Doch", "das", "Herz", "blieb", "fest", "und", "mu\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bis der Stral vom Himmel z\u00fcckte,", "tokens": ["Bis", "der", "Stral", "vom", "Him\u00b7mel", "z\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis die St\u00fcrme heulten w\u00fctend,", "tokens": ["Bis", "die", "St\u00fcr\u00b7me", "heul\u00b7ten", "w\u00fc\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die alte Nacht sich br\u00fctend", "tokens": ["Und", "die", "al\u00b7te", "Nacht", "sich", "br\u00fc\u00b7tend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "PRF", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf die m\u00fcden H\u00e4upter dr\u00fcckte.", "tokens": ["Auf", "die", "m\u00fc\u00b7den", "H\u00e4up\u00b7ter", "dr\u00fcck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und es zagten alle Frommen,", "tokens": ["Und", "es", "zag\u00b7ten", "al\u00b7le", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es seufzte der Gerechte:", "tokens": ["Und", "es", "seufz\u00b7te", "der", "Ge\u00b7rech\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbsoll vergehen die\u00df Geschlechte,", "tokens": ["\u00bb", "soll", "ver\u00b7ge\u00b7hen", "die\u00df", "Ge\u00b7schlech\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "VVINF", "PDS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch bevor die Sonn' ist kommen?\u00ab", "tokens": ["Noch", "be\u00b7vor", "die", "Sonn'", "ist", "kom\u00b7men", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VAFIN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sieh, da t\u00f6net eine Stimme,", "tokens": ["Sieh", ",", "da", "t\u00f6\u00b7net", "ei\u00b7ne", "Stim\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Macht sich Bahn zu aller Herzen,", "tokens": ["Macht", "sich", "Bahn", "zu", "al\u00b7ler", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die Seufzer, durch die Schmerzen,", "tokens": ["Durch", "die", "Seuf\u00b7zer", ",", "durch", "die", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch das Element im Grimme:", "tokens": ["Durch", "das", "E\u00b7le\u00b7ment", "im", "Grim\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbeinst geschieht des Himmels Wille,", "tokens": ["\u00bb", "einst", "ge\u00b7schieht", "des", "Him\u00b7mels", "Wil\u00b7le", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr geht unter All' im Ringen,", "tokens": ["Ihr", "geht", "un\u00b7ter", "All'", "im", "Rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber Er wird es vollbringen.", "tokens": ["A\u00b7ber", "Er", "wird", "es", "voll\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Und die Weltuhr steht nicht stille.", "tokens": ["Und", "die", "Wel\u00b7tuhr", "steht", "nicht", "stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wollt ihr in die R\u00e4der fahren?", "tokens": ["Wollt", "ihr", "in", "die", "R\u00e4\u00b7der", "fah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wollt ihr am Gewichte zerren?", "tokens": ["Wollt", "ihr", "am", "Ge\u00b7wich\u00b7te", "zer\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wi\u00dft ihr's nicht? vor Gott dem Herren", "tokens": ["Wi\u00dft", "ih\u00b7r's", "nicht", "?", "vor", "Gott", "dem", "Her\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$.", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist ein Tag gleich tausend Jahren!\u00ab", "tokens": ["Ist", "ein", "Tag", "gleich", "tau\u00b7send", "Jah\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "CARD", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}