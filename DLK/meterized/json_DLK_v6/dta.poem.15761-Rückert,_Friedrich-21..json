{"dta.poem.15761": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In meinem Innern ganz ist dis Gedicht vorhanden,", "tokens": ["In", "mei\u00b7nem", "In\u00b7nern", "ganz", "ist", "dis", "Ge\u00b7dicht", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VAFIN", "PDS", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das in Bruchst\u00fccken nur ist \u00e4u\u00dferlich entstanden.", "tokens": ["Das", "in", "Bruchs\u00b7t\u00fc\u00b7cken", "nur", "ist", "\u00e4u\u00b7\u00dfer\u00b7lich", "ent\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "ADV", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In jedem Bruchst\u00fcck bricht ein St\u00fcckchen Glanz hervor", "tokens": ["In", "je\u00b7dem", "Bruchs\u00b7t\u00fcck", "bricht", "ein", "St\u00fcck\u00b7chen", "Glanz", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alswie vom Angesicht des Liebchens hinterm Flor.", "tokens": ["Als\u00b7wie", "vom", "An\u00b7ge\u00b7sicht", "des", "Lieb\u00b7chens", "hin\u00b7term", "Flor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denk dir das Ganze, wenn ein Liebender du bist!", "tokens": ["Denk", "dir", "das", "Gan\u00b7ze", ",", "wenn", "ein", "Lie\u00b7ben\u00b7der", "du", "bist", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "KOUS", "ART", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch sch\u00f6ner magst du dir es denken als es ist.", "tokens": ["Noch", "sch\u00f6\u00b7ner", "magst", "du", "dir", "es", "den\u00b7ken", "als", "es", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "PPER", "PPER", "VVINF", "KOKOM", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}