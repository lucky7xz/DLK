{"dta.poem.13369": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Abhandlung  \n von der Schreibart in Miltons  \n verlohrnen Paradiese.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198519", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach nur ein Blick von ihr, nur eine von den Stunden,", "tokens": ["Ach", "nur", "ein", "Blick", "von", "ihr", ",", "nur", "ei\u00b7ne", "von", "den", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ART", "NN", "APPR", "PPER", "$,", "ADV", "ART", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die zwischen ihr und mir oft unvermerckt verschwunden!", "tokens": ["Die", "zwi\u00b7schen", "ihr", "und", "mir", "oft", "un\u00b7ver\u00b7merckt", "ver\u00b7schwun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "KON", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}