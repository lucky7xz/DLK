{"textgrid.poem.55612": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ziehn die Schafe von der Wiese,", "tokens": ["Ziehn", "die", "Scha\u00b7fe", "von", "der", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegt sie da, ein reines Gr\u00fcn,", "tokens": ["Liegt", "sie", "da", ",", "ein", "rei\u00b7nes", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber bald zum Paradiese", "tokens": ["A\u00b7ber", "bald", "zum", "Pa\u00b7ra\u00b7die\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird sie bunt gebl\u00fcmt erbl\u00fchn.", "tokens": ["Wird", "sie", "bunt", "ge\u00b7bl\u00fcmt", "er\u00b7bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hoffnung breitet lichte Schleier", "tokens": ["Hoff\u00b7nung", "brei\u00b7tet", "lich\u00b7te", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nebelhaft vor unsern Blick:", "tokens": ["Ne\u00b7bel\u00b7haft", "vor", "un\u00b7sern", "Blick", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wunscherf\u00fcllung, Sonnenfeier,", "tokens": ["Wun\u00b7scher\u00b7f\u00fcl\u00b7lung", ",", "Son\u00b7nen\u00b7fei\u00b7er", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wolkenteilung bring uns Gl\u00fcck.", "tokens": ["Wol\u00b7ken\u00b7tei\u00b7lung", "bring", "uns", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}