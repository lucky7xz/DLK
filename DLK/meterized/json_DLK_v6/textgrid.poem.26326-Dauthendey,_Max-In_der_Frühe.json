{"textgrid.poem.26326": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "In der Fr\u00fche", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00dfe wei\u00dfe Malvenbl\u00fcten, frischbetaute,", "tokens": ["Gro\u00b7\u00dfe", "wei\u00b7\u00dfe", "Mal\u00b7ven\u00b7bl\u00fc\u00b7ten", ",", "frischbe\u00b7tau\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sah ich in der Fr\u00fche, da das Taglicht graute,", "tokens": ["Sah", "ich", "in", "der", "Fr\u00fc\u00b7he", ",", "da", "das", "Tag\u00b7licht", "grau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "In dem Garten, und es schliefen noch die Laute.", "tokens": ["In", "dem", "Gar\u00b7ten", ",", "und", "es", "schlie\u00b7fen", "noch", "die", "Lau\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Jede runde Bl\u00fcte leuchtete und brachte", "tokens": ["Je\u00b7de", "run\u00b7de", "Bl\u00fc\u00b7te", "leuch\u00b7te\u00b7te", "und", "brach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Hellen Schmelz dem Himmel, der erwachte,", "tokens": ["Hel\u00b7len", "Schmelz", "dem", "Him\u00b7mel", ",", "der", "er\u00b7wach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Als das Gartendunkel noch der Nacht gedachte.", "tokens": ["Als", "das", "Gar\u00b7ten\u00b7dun\u00b7kel", "noch", "der", "Nacht", "ge\u00b7dach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "In der Ferne stand ein blauer Berg gehoben,", "tokens": ["In", "der", "Fer\u00b7ne", "stand", "ein", "blau\u00b7er", "Berg", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Lange Wolken sich am freien Gipfel schoben,", "tokens": ["Lan\u00b7ge", "Wol\u00b7ken", "sich", "am", "frei\u00b7en", "Gip\u00b7fel", "scho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Und vom Lichte lag dort d\u00fcnne Spur gewoben.", "tokens": ["Und", "vom", "Lich\u00b7te", "lag", "dort", "d\u00fcn\u00b7ne", "Spur", "ge\u00b7wo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ADV", "VVFIN", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Und ich dachte: Bl\u00fcten, Berg und Licht, sie wissen,", "tokens": ["Und", "ich", "dach\u00b7te", ":", "Bl\u00fc\u00b7ten", ",", "Berg", "und", "Licht", ",", "sie", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "NN", "$,", "NN", "KON", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Da\u00df sie heut am hellen Tage nichts vermissen,", "tokens": ["Da\u00df", "sie", "heut", "am", "hel\u00b7len", "Ta\u00b7ge", "nichts", "ver\u00b7mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "ADJA", "NN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Und nur ich, nur ich bin heimatlos, zerrissen.", "tokens": ["Und", "nur", "ich", ",", "nur", "ich", "bin", "hei\u00b7mat\u00b7los", ",", "zer\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "PPER", "$,", "ADV", "PPER", "VAFIN", "ADJD", "$,", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}