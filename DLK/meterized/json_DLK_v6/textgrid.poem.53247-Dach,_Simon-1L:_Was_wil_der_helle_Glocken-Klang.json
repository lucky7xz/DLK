{"textgrid.poem.53247": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was wil der helle Glocken-Klang", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was wil der helle Glocken-Klang", "tokens": ["Was", "wil", "der", "hel\u00b7le", "Glo\u00b7cken\u00b7Klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In allen dreyen St\u00e4dten?", "tokens": ["In", "al\u00b7len", "drey\u00b7en", "St\u00e4d\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bedeut er Trauren oder Danck,", "tokens": ["Be\u00b7deut", "er", "Trau\u00b7ren", "o\u00b7der", "Danck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wil, wir sollen behten?", "tokens": ["Und", "wil", ",", "wir", "sol\u00b7len", "beh\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er ist zu Freuden angestellt,", "tokens": ["Er", "ist", "zu", "Freu\u00b7den", "an\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum jauchtzen au\u00dferkohren,", "tokens": ["Zum", "jaucht\u00b7zen", "au\u00b7\u00dfer\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Weil Du, O F\u00fcrsten-Kind, der Welt", "tokens": ["Weil", "Du", ",", "O", "F\u00fcrs\u00b7ten\u00b7Kind", ",", "der", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "NE", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und uns bist heut gebohren.", "tokens": ["Und", "uns", "bist", "heut", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Fallt alle nieder auff die Knie,", "tokens": ["Fallt", "al\u00b7le", "nie\u00b7der", "auff", "die", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr liebt ja ewer Leben,", "tokens": ["Ihr", "liebt", "ja", "e\u00b7wer", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und danckt dem H\u00f6chsten spat und fr\u00fch,", "tokens": ["Und", "danckt", "dem", "H\u00f6chs\u00b7ten", "spat", "und", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Ihn sein Pfand gegeben.", "tokens": ["Der", "Ihn", "sein", "Pfand", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Warumb der wahren Andacht Glut", "tokens": ["Wa\u00b7rumb", "der", "wah\u00b7ren", "An\u00b7dacht", "Glut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So lang ihn angeschrien,", "tokens": ["So", "lang", "ihn", "an\u00b7ge\u00b7schri\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das hat Er aus bewegtem Muht", "tokens": ["Das", "hat", "Er", "aus", "be\u00b7weg\u00b7tem", "Muht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Uns reichlich jetzt verliehen.", "tokens": ["Uns", "reich\u00b7lich", "jetzt", "ver\u00b7lie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Bohten sollen nicht verziehn,", "tokens": ["Die", "Boh\u00b7ten", "sol\u00b7len", "nicht", "ver\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auff, reiset unverdrossen,", "tokens": ["Auff", ",", "rei\u00b7set", "un\u00b7ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Fort, bringt es eilends nach Berlin,", "tokens": ["Fort", ",", "bringt", "es", "ei\u00b7lends", "nach", "Ber\u00b7lin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Doch allererst nach Crossen,", "tokens": ["Doch", "al\u00b7le\u00b7rerst", "nach", "Cros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sagt es dem Hag' imgleichen an,", "tokens": ["Sagt", "es", "dem", "Hag'", "im\u00b7glei\u00b7chen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch Churland mu\u00df es wissen,", "tokens": ["Auch", "Chur\u00b7land", "mu\u00df", "es", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Elbe werd' es kunt gethan,", "tokens": ["Der", "El\u00b7be", "werd'", "es", "kunt", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie wird sich froh' ergiessen,", "tokens": ["Sie", "wird", "sich", "froh'", "er\u00b7gies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Oder-Strom erfrewet seyn,", "tokens": ["Der", "O\u00b7der\u00b7Strom", "er\u00b7fre\u00b7wet", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Spree und Ruhr imgleichen,", "tokens": ["Die", "Spree", "und", "Ruhr", "im\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr allen wird der edle Rhein", "tokens": ["F\u00fcr", "al\u00b7len", "wird", "der", "ed\u00b7le", "Rhein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Freuden keinem weichen.", "tokens": ["In", "Freu\u00b7den", "kei\u00b7nem", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was? h\u00f6r' ich nicht der Music Schall", "tokens": ["Was", "?", "h\u00f6r'", "ich", "nicht", "der", "Mu\u00b7sic", "Schall"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "$.", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Fern von der gr\u00fcnen Br\u00fccken", "tokens": ["Fern", "von", "der", "gr\u00fc\u00b7nen", "Br\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sich mengen in den Doner-Knall", "tokens": ["Sich", "men\u00b7gen", "in", "den", "Do\u00b7ner\u00b7Knall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aus allen groben St\u00fccken?", "tokens": ["Aus", "al\u00b7len", "gro\u00b7ben", "St\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wo h\u00e4lt sich jetzund die Armee,", "tokens": ["Wo", "h\u00e4lt", "sich", "je\u00b7tzund", "die", "Ar\u00b7mee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die hie bi\u00dfher gelegen?", "tokens": ["Die", "hie", "bi\u00df\u00b7her", "ge\u00b7le\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt thu' sie keinem Menschen Weh,", "tokens": ["Jetzt", "thu'", "sie", "kei\u00b7nem", "Men\u00b7schen", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mars z\u00fccke keinen Degen,", "tokens": ["Mars", "z\u00fc\u00b7cke", "kei\u00b7nen", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der L\u00f6w sey jetzt ein stilles Schaff,", "tokens": ["Der", "L\u00f6w", "sey", "jetzt", "ein", "stil\u00b7les", "Schaff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Adler eine Taube,", "tokens": ["Der", "Ad\u00b7ler", "ei\u00b7ne", "Tau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Du F\u00fcrst von Weimar, Du Herr Graff,", "tokens": ["Du", "F\u00fcrst", "von", "Wei\u00b7mar", ",", "Du", "Herr", "Graff", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NE", "$,", "PPER", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Kr\u00f6nt ewer Heer mit Laube.", "tokens": ["Kr\u00f6nt", "e\u00b7wer", "Heer", "mit", "Lau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sparr, D\u00f6rffling, G\u00f6rtzke, Kannenberg", "tokens": ["Sparr", ",", "D\u00f6rff\u00b7ling", ",", "G\u00f6rtz\u00b7ke", ",", "Kan\u00b7nen\u00b7berg"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und alle tapffre Helden,", "tokens": ["Und", "al\u00b7le", "tapf\u00b7fre", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von deren Muht, Verdienst und St\u00e4rck", "tokens": ["Von", "de\u00b7ren", "Muht", ",", "Ver\u00b7dienst", "und", "St\u00e4rck"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht Zeit ist nun zu melden,", "tokens": ["Nicht", "Zeit", "ist", "nun", "zu", "mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VAFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Macht jetzund Anstand mit dem Streit,", "tokens": ["Macht", "je\u00b7tzund", "An\u00b7stand", "mit", "dem", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Lasst keinen Menschen t\u00f6dten,", "tokens": ["Lasst", "kei\u00b7nen", "Men\u00b7schen", "t\u00f6d\u0