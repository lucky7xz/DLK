"tokens": ["Der", "S\u00fcd\u00b7wind", "steckt", "in", "sei\u00b7ner", "Klufft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und macht kein Wasser tr\u00fcbe,", "tokens": ["Und", "macht", "kein", "Was\u00b7ser", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der weite Raum der warmen Lufft", "tokens": ["Der", "wei\u00b7te", "Raum", "der", "war\u00b7men", "Lufft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Erregt nur Schertz und Liebe.", "tokens": ["Er\u00b7regt", "nur", "Schertz", "und", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und die\u00df zu Ehren Dir, O Kind,", "tokens": ["Und", "die\u00df", "zu", "Eh\u00b7ren", "Dir", ",", "O", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDS", "APPR", "NN", "PPER", "$,", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du k\u00f6mmst zu gutter Stunde,", "tokens": ["Du", "k\u00f6mmst", "zu", "gut\u00b7ter", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In der Natur und Himmel sind", "tokens": ["In", "der", "Na\u00b7tur", "und", "Him\u00b7mel", "sind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN",