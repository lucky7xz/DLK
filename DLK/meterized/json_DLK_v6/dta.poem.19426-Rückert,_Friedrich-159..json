{"dta.poem.19426": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "159.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist gegangen und wir gehn dir alle nach;", "tokens": ["Du", "bist", "ge\u00b7gan\u00b7gen", "und", "wir", "gehn", "dir", "al\u00b7le", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "PPER", "VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du giengst zur Ruh und wir sind noch ein Weilchen wach.", "tokens": ["Du", "giengst", "zur", "Ruh", "und", "wir", "sind", "noch", "ein", "Weil\u00b7chen", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "PPER", "VAFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Vielmehr wir schlafen noch, du bist vom Traum erwacht;", "tokens": ["Viel\u00b7mehr", "wir", "schla\u00b7fen", "noch", ",", "du", "bist", "vom", "Traum", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Leben, Spreu und Wind, o schwerer Traum der Nacht!", "tokens": ["O", "Le\u00b7ben", ",", "Spreu", "und", "Wind", ",", "o", "schwe\u00b7rer", "Traum", "der", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "KON", "NN", "$,", "FM", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was ists, das weiter wir hier zu besorgen haben,", "tokens": ["Was", "ists", ",", "das", "wei\u00b7ter", "wir", "hier", "zu", "be\u00b7sor\u00b7gen", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "ADV", "PPER", "ADV", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als eins das andere anst\u00e4ndig zu begraben!", "tokens": ["Als", "eins", "das", "an\u00b7de\u00b7re", "an\u00b7st\u00e4n\u00b7dig", "zu", "be\u00b7gra\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}