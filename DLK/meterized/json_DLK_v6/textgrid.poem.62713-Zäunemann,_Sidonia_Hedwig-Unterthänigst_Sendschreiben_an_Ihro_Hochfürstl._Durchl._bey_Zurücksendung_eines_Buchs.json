{"textgrid.poem.62713": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Unterth\u00e4nigst Sendschreiben an Ihro Hochf\u00fcrstl. Durchl. bey Zur\u00fccksendung eines Buchs", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil mich Dein Gnaden-Strahl,", "tokens": ["Weil", "mich", "Dein", "Gna\u00b7den\u00b7Strahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und deine F\u00fcrsten-Huld so viel so oftermahl", "tokens": ["Und", "dei\u00b7ne", "F\u00fcrs\u00b7ten\u00b7Huld", "so", "viel", "so", "of\u00b7ter\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Begl\u00fcckt und froh gemacht. Und weil zu jeden Stunden", "tokens": ["Be\u00b7gl\u00fcckt", "und", "froh", "ge\u00b7macht", ".", "Und", "weil", "zu", "je\u00b7den", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "ADJD", "VVPP", "$.", "KON", "KOUS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Vers an ", "tokens": ["Mein", "Vers", "an"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "So ist mein Geist getrost, und glaubt, da\u00df auch die\u00df Blat", "tokens": ["So", "ist", "mein", "Geist", "ge\u00b7trost", ",", "und", "glaubt", ",", "da\u00df", "auch", "die\u00df", "Blat"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,", "KON", "VVFIN", "$,", "KOUS", "ADV", "PDS", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein gleiches F\u00fcrsten-Gl\u00fcck von dir zu hoffen hat.", "tokens": ["Ein", "glei\u00b7ches", "F\u00fcrs\u00b7ten\u00b7G\u00b7l\u00fcck", "von", "dir", "zu", "hof\u00b7fen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ist kr\u00e4ftig, meinen Kiel zum Dichten anzutreiben:", "tokens": ["Ist", "kr\u00e4f\u00b7tig", ",", "mei\u00b7nen", "Kiel", "zum", "Dich\u00b7ten", "an\u00b7zu\u00b7trei\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PPOSAT", "NN", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Ehrfurcht aber folgt doch jeder Zeile nach.", "tokens": ["Die", "Ehr\u00b7furcht", "a\u00b7ber", "folgt", "doch", "je\u00b7der", "Zei\u00b7le", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nur ich betaure sehr, da\u00df ich so matt und schwach,", "tokens": ["Nur", "ich", "be\u00b7tau\u00b7re", "sehr", ",", "da\u00df", "ich", "so", "matt", "und", "schwach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zum hohen Dichten bin. Dein Purpur fordert Gaben,", "tokens": ["Zum", "ho\u00b7hen", "Dich\u00b7ten", "bin", ".", "Dein", "Pur\u00b7pur", "for\u00b7dert", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "$.", "PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Geist, Feuer, Nettigkeit. O m\u00f6cht ich dieses haben,", "tokens": ["Geist", ",", "Feu\u00b7er", ",", "Net\u00b7tig\u00b7keit", ".", "O", "m\u00f6cht", "ich", "die\u00b7ses", "ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$.", "NE", "VMFIN", "PPER", "PDS", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So stimmte ich mit Flei\u00df die Dichter-Fl\u00f6te an!", "tokens": ["So", "stimm\u00b7te", "ich", "mit", "Flei\u00df", "die", "Dich\u00b7ter\u00b7Fl\u00f6\u00b7te", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So aber schreibt mein Kiel, so gut er jetzo kan.", "tokens": ["So", "a\u00b7ber", "schreibt", "mein", "Kiel", ",", "so", "gut", "er", "jet\u00b7zo", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "ADJD", "PPER", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Du hast, ", "tokens": ["Du", "hast", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Woraus nach Bienen-Art ich solte Hong ziehen.", "tokens": ["Wo\u00b7raus", "nach", "Bie\u00b7nen\u00b7Art", "ich", "sol\u00b7te", "Hong", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Du gabst mir auch zugleich das holde F\u00fcrsten-Wort:", "tokens": ["Du", "gabst", "mir", "auch", "zu\u00b7gleich", "das", "hol\u00b7de", "F\u00fcrs\u00b7ten\u00b7Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Da\u00df, wenn ichs durchgesehn, so solte ich so fort", "tokens": ["Da\u00df", ",", "wenn", "ichs", "durch\u00b7ge\u00b7sehn", ",", "so", "sol\u00b7te", "ich", "so", "fort"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "PIS", "VVINF", "$,", "ADV", "VMFIN", "PPER", "ADV", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wie mirs gefallen hat. Wie aber soll ichs wagen?", "tokens": ["Wie", "mirs", "ge\u00b7fal\u00b7len", "hat", ".", "Wie", "a\u00b7ber", "soll", "ichs", "wa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVPP", "VAFIN", "$.", "PWAV", "ADV", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich bin kein Philosoph, auch kein Theologus,", "tokens": ["Ich", "bin", "kein", "Phi\u00b7lo\u00b7soph", ",", "auch", "kein", "Theo\u00b7lo\u00b7gus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "ADV", "PIAT", "NN", "$,"], "meter": "-+--++--+-+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Wer davon sprechen soll, und reden will, der mu\u00df", "tokens": ["Wer", "da\u00b7von", "spre\u00b7chen", "soll", ",", "und", "re\u00b7den", "will", ",", "der", "mu\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PAV", "VVINF", "VMFIN", "$,", "KON", "VVINF", "VMFIN", "$,", "ART", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Physic, Philosophie, und heilge Schrift verstehen,", "tokens": ["Phy\u00b7sic", ",", "Phi\u00b7lo\u00b7so\u00b7phie", ",", "und", "heil\u00b7ge", "Schrift", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Sonst denkt und schlie\u00dft er falsch und w\u00fcrd es nur verdrehen.", "tokens": ["Sonst", "denkt", "und", "schlie\u00dft", "er", "falsch", "und", "w\u00fcrd", "es", "nur", "ver\u00b7dre\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "ADJD", "KON", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Damit ich aber ", "tokens": ["Da\u00b7mit", "ich", "a\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "Ey, so entdeck ich Dir dar\u00fcber meinen Sinn:", "tokens": ["Ey", ",", "so", "ent\u00b7deck", "ich", "Dir", "da\u00b7r\u00fc\u00b7ber", "mei\u00b7nen", "Sinn", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "PAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Es ist in diesem Buch ganz angenehm zu lesen,", "tokens": ["Es", "ist", "in", "die\u00b7sem", "Buch", "ganz", "an\u00b7ge\u00b7nehm", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und ist mir manchesmahl ein Zeitvertreib gewesen,", "tokens": ["Und", "ist", "mir", "man\u00b7ches\u00b7mahl", "ein", "Zeit\u00b7ver\u00b7treib", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der mich vergn\u00fcget hat. Allein ich glaube auch,", "tokens": ["Der", "mich", "ver\u00b7gn\u00fc\u00b7get", "hat", ".", "Al\u00b7lein", "ich", "glau\u00b7be", "auch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "VAFIN", "$.", "ADV", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Da\u00df wohl ein Criticus nach der Gelehrten Brauch", "tokens": ["Da\u00df", "wohl", "ein", "Cri\u00b7ti\u00b7cus", "nach", "der", "Ge\u00b7lehr\u00b7ten", "Brauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Hier critisiren kan, er w\u00fcrde schon was finden,", "tokens": ["Hier", "cri\u00b7ti\u00b7si\u00b7ren", "kan", ",", "er", "w\u00fcr\u00b7de", "schon", "was", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$,", "PPER", "VAFIN", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Das Stof und Anla\u00df g\u00e4b. Wer wei\u00df, ob R \u2013 \u2013 Gr\u00fcnden", "tokens": ["Das", "Stof", "und", "An\u00b7la\u00df", "g\u00e4b", ".", "Wer", "wei\u00df", ",", "ob", "R", "\u2013", "\u2013", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "word"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$.", "PWS", "VVFIN", "$,", "KOUS", "NE", "$(", "$(", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Ein grosser Philosoph nicht widersprechen wird?", "tokens": ["Ein", "gros\u00b7ser", "Phi\u00b7lo\u00b7soph", "nicht", "wi\u00b7der\u00b7spre\u00b7chen", "wird", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wer wei\u00df, ob nicht manchmahl ein weiser Seelen-Hirt", "tokens": ["Wer", "wei\u00df", ",", "ob", "nicht", "manch\u00b7mahl", "ein", "wei\u00b7ser", "See\u00b7len\u00b7Hirt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PTKNEG", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Etwas darwider sagt? und diese Meinung heget:", "tokens": ["Et\u00b7was", "dar\u00b7wi\u00b7der", "sagt", "?", "und", "die\u00b7se", "Mei\u00b7nung", "he\u00b7get", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VVFIN", "$.", "KON", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Er habe die\u00df und das nicht gr\u00fcndlich ausgeleget,", "tokens": ["Er", "ha\u00b7be", "die\u00df", "und", "das", "nicht", "gr\u00fcnd\u00b7lich", "aus\u00b7ge\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDS", "KON", "PDS", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Noch weislich angebracht. ", "tokens": ["Noch", "weis\u00b7lich", "an\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.36": {"text": "Mein Kiel ", "tokens": ["Mein", "Kiel"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.37": {"text": "Und sucht so, wie mein Herz, dir davon Dank zu sagen,", "tokens": ["Und", "sucht", "so", ",", "wie", "mein", "Herz", ",", "dir", "da\u00b7von", "Dank", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOKOM", "PPOSAT", "NN", "$,", "PPER", "PAV", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Man wird Dirs unversehrt vor deine H\u00e4nde tragen.", "tokens": ["Man", "wird", "Dirs", "un\u00b7ver\u00b7sehrt", "vor", "dei\u00b7ne", "H\u00e4n\u00b7de", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "NN", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ich aber w\u00fcnsche ", "tokens": ["Ich", "a\u00b7ber", "w\u00fcn\u00b7sche"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.40": {"text": "Ein stetig F\u00fcrsten-Gl\u00fcck und tausend s\u00fcsse Lust.", "tokens": ["Ein", "ste\u00b7tig", "F\u00fcrs\u00b7ten\u00b7G\u00b7l\u00fcck", "und", "tau\u00b7send", "s\u00fcs\u00b7se", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "KON", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Doch will ich diesen Wunsch zugleich in Demuth setzen;", "tokens": ["Doch", "will", "ich", "die\u00b7sen", "Wunsch", "zu\u00b7gleich", "in", "De\u00b7muth", "set\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PDAT", "NN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Du wollst mich deiner Gnad noch ferner w\u00fcrdig sch\u00e4tzen.", "tokens": ["Du", "wollst", "mich", "dei\u00b7ner", "Gnad", "noch", "fer\u00b7ner", "w\u00fcr\u00b7dig", "sch\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}