{"textgrid.poem.49654": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Vesuv", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Vesuv, indem er speit, mit nichten", "tokens": ["Der", "Ve\u00b7suv", ",", "in\u00b7dem", "er", "speit", ",", "mit", "nich\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "APPR", "PIS"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Darf man gegen ihn die Klagen richten,", "tokens": ["Darf", "man", "ge\u00b7gen", "ihn", "die", "Kla\u00b7gen", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Insofern ja die Besonderheit", "tokens": ["In\u00b7so\u00b7fern", "ja", "die", "Be\u00b7son\u00b7der\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Darin liegt, da\u00df er mitunter speit.", "tokens": ["Da\u00b7rin", "liegt", ",", "da\u00df", "er", "mi\u00b7tun\u00b7ter", "speit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Halten Sie den Vorwurf f\u00fcr ersprie\u00dflich?", "tokens": ["Hal\u00b7ten", "Sie", "den", "Vor\u00b7wurf", "f\u00fcr", "er\u00b7sprie\u00df\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn man schon Vulkan ist, mu\u00df man schlie\u00dflich;", "tokens": ["Wenn", "man", "schon", "Vul\u00b7kan", "ist", ",", "mu\u00df", "man", "schlie\u00df\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "NE", "VAFIN", "$,", "VMFIN", "PIS", "ADJD", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Und man regnet Asche oder speit,", "tokens": ["Und", "man", "reg\u00b7net", "A\u00b7sche", "o\u00b7der", "speit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ob die Menschheit auch betroffen schreit.", "tokens": ["Ob", "die", "Menschheit", "auch", "be\u00b7trof\u00b7fen", "schreit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVPP", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Aber dieses scheint gesagt zu werden", "tokens": ["A\u00b7ber", "die\u00b7ses", "scheint", "ge\u00b7sagt", "zu", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "VVPP", "PTKZU", "VAINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Doch am Platze: wenn sich auf der Erden", "tokens": ["Doch", "am", "Plat\u00b7ze", ":", "wenn", "sich", "auf", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "$.", "KOUS", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "So was zubegibt, wie der Vesuv,", "tokens": ["So", "was", "zu\u00b7be\u00b7gibt", ",", "wie", "der", "Ve\u00b7suv", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+--++--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Trifft der Tadel den, der ihn erschuf.", "tokens": ["Trifft", "der", "Ta\u00b7del", "den", ",", "der", "ihn", "er\u00b7schuf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Und man fragt mit Recht den Himmelsvater,", "tokens": ["Und", "man", "fragt", "mit", "Recht", "den", "Him\u00b7mels\u00b7va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ob es sch\u00f6n ist, wenn sich aus dem Krater", "tokens": ["Ob", "es", "sch\u00f6n", "ist", ",", "wenn", "sich", "aus", "dem", "Kra\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "KOUS", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "So viel Ungl\u00fcck auf die T\u00e4ler st\u00fcrzt,", "tokens": ["So", "viel", "Un\u00b7gl\u00fcck", "auf", "die", "T\u00e4\u00b7ler", "st\u00fcrzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Manchem auch die Lebenszeit verk\u00fcrzt.", "tokens": ["Man\u00b7chem", "auch", "die", "Le\u00b7bens\u00b7zeit", "ver\u00b7k\u00fcrzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Weiter fr\u00e4gt der sonst im Glauben Schwache:", "tokens": ["Wei\u00b7ter", "fr\u00e4gt", "der", "sonst", "im", "Glau\u00b7ben", "Schwa\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADV", "APPRART", "NN", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00e4llt noch \u00fcberhaupt kein Spatz vom Dache?", "tokens": ["F\u00e4llt", "noch", "\u00fc\u00b7ber\u00b7haupt", "kein", "Spatz", "vom", "Da\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Oder hatte dieser Bibelsatz", "tokens": ["O\u00b7der", "hat\u00b7te", "die\u00b7ser", "Bi\u00b7bel\u00b7satz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PDAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Geltung nur f\u00fcr einen fr\u00fchern Spatz?", "tokens": ["Gel\u00b7tung", "nur", "f\u00fcr", "ei\u00b7nen", "fr\u00fc\u00b7hern", "Spatz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Diese \u2013 sagen wir \u2013 Unstimmigkeiten", "tokens": ["Die\u00b7se", "\u2013", "sa\u00b7gen", "wir", "\u2013", "Un\u00b7stim\u00b7mig\u00b7kei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["PDAT", "$(", "VVFIN", "PPER", "$(", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "K\u00f6nnen b\u00f6se Zweifel uns bereiten.", "tokens": ["K\u00f6n\u00b7nen", "b\u00f6\u00b7se", "Zwei\u00b7fel", "uns", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "War es zu verhindern, d\u00e4chte man,", "tokens": ["War", "es", "zu", "ver\u00b7hin\u00b7dern", ",", "d\u00e4ch\u00b7te", "man", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKZU", "VVINF", "$,", "VVFIN", "PIS", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Warum speit dann der Vesuvvulkan?", "tokens": ["Wa\u00b7rum", "speit", "dann", "der", "Ve\u00b7suv\u00b7vul\u00b7kan", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Mir nat\u00fcrlich scheint noch viel verd\u00e4chtig;", "tokens": ["Mir", "na\u00b7t\u00fcr\u00b7lich", "scheint", "noch", "viel", "ver\u00b7d\u00e4ch\u00b7tig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der Vesuv ist lang schon niedertr\u00e4chtig.", "tokens": ["Der", "Ve\u00b7suv", "ist", "lang", "schon", "nie\u00b7der\u00b7tr\u00e4ch\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADV", "ADJD", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Damals schien es eine G\u00f6tterschar", "tokens": ["Da\u00b7mals", "schien", "es", "ei\u00b7ne", "G\u00f6t\u00b7ter\u00b7schar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Bei Pompeji, die so freundlich war.", "tokens": ["Bei", "Pom\u00b7pe\u00b7ji", ",", "die", "so", "freund\u00b7lich", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Damals bat der Mensch in Aschenregen", "tokens": ["Da\u00b7mals", "bat", "der", "Mensch", "in", "A\u00b7schen\u00b7re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Jupiter um den besondern Segen.", "tokens": ["Ju\u00b7pi\u00b7ter", "um", "den", "be\u00b7son\u00b7dern", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Heute bittet man Gott Zebaoth", "tokens": ["Heu\u00b7te", "bit\u00b7tet", "man", "Gott", "Ze\u00b7bao\u00b7th"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "NN", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Um die Rettung aus der bittern Not.", "tokens": ["Um", "die", "Ret\u00b7tung", "aus", "der", "bit\u00b7tern", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Also sieht man, da\u00df die Glauben wechseln,", "tokens": ["Al\u00b7so", "sieht", "man", ",", "da\u00df", "die", "Glau\u00b7ben", "wech\u00b7seln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "An die G\u00f6tter, die das Unheil drechseln.", "tokens": ["An", "die", "G\u00f6t\u00b7ter", ",", "die", "das", "Un\u00b7heil", "drech\u00b7seln", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Der Vesuv jedoch bleibt auf dem Platz,", "tokens": ["Der", "Ve\u00b7suv", "je\u00b7doch", "bleibt", "auf", "dem", "Platz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und vom Dache f\u00e4llt noch mancher Spatz.", "tokens": ["Und", "vom", "Da\u00b7che", "f\u00e4llt", "noch", "man\u00b7cher", "Spatz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}