{"textgrid.poem.30779": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "93.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die mich wi\u00dflich neiden,", "tokens": ["Die", "mich", "wi\u00df\u00b7lich", "nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Kan ich noch wol leiden;", "tokens": ["Kan", "ich", "noch", "wol", "lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "\u00dcbel kan ich meiden,", "tokens": ["\u00dc\u00b7bel", "kan", "ich", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Die mich heimlich neiden.", "tokens": ["Die", "mich", "heim\u00b7lich", "nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}