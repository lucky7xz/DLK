{"dta.poem.9397": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Zugabe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Djs nim aus lieber hand/ mein Freund/ was ich", "tokens": ["Djs", "nim", "aus", "lie\u00b7ber", "hand", "/", "mein", "Freund", "/", "was", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADV", "APPR", "ADJA", "NN", "$(", "PPOSAT", "NN", "$(", "PWS", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "geschrieben", "tokens": ["ge\u00b7schrie\u00b7ben"], "token_info": ["word"], "pos": ["VVPP"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "nach Buchners edlen Art/ weil du sie pflegst zu lieben/", "tokens": ["nach", "Buch\u00b7ners", "ed\u00b7len", "Art", "/", "weil", "du", "sie", "pflegst", "zu", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$(", "KOUS", "PPER", "PPER", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und selbst dem Buntzler Schwan in allem folgest", "tokens": ["und", "selbst", "dem", "Buntz\u00b7ler", "Schwan", "in", "al\u00b7lem", "fol\u00b7gest"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "NN", "APPR", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "nach", "tokens": ["nach"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "und nim\u0303st es wohl in acht/ was vormals uns gebrach", "tokens": ["und", "nim\u0303st", "es", "wohl", "in", "acht", "/", "was", "vor\u00b7mals", "uns", "ge\u00b7brach"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "CARD", "$(", "PWS", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In deutscher Poesie. Dein K\u00f6hler pflegt zu lieben", "tokens": ["In", "deut\u00b7scher", "Poe\u00b7sie", ".", "Dein", "K\u00f6h\u00b7ler", "pflegt", "zu", "lie\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "PPOSAT", "NE", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "und Major selbsten Dich/ so vormahls angetrieben", "tokens": ["und", "Ma\u00b7jor", "selbs\u00b7ten", "Dich", "/", "so", "vor\u00b7mahls", "an\u00b7ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "PPER", "$(", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Den niemahls lassen Geist zur wissenschafft und", "tokens": ["Den", "nie\u00b7mahls", "las\u00b7sen", "Geist", "zur", "wis\u00b7sen\u00b7schafft", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVINF", "NN", "APPRART", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "kunst/", "tokens": ["kunst", "/"], "token_info": ["word", "punct"], "pos": ["ADV", "$("], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Die dich zum Meister macht und bringet Ehr und", "tokens": ["Die", "dich", "zum", "Meis\u00b7ter", "macht", "und", "brin\u00b7get", "Ehr", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "KON", "VVFIN", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Gunst", "tokens": ["Gunst"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Bey Hochgelehrter Schaar. Entschl\u00e4gstu dich der", "tokens": ["Bey", "Hoch\u00b7ge\u00b7lehr\u00b7ter", "Schaar", ".", "Ent\u00b7schl\u00e4gs\u00b7tu", "dich", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Ehren", "tokens": ["Eh\u00b7ren"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "und wiltu nicht dein Lob mit diesem Titul mehren?", "tokens": ["und", "wil\u00b7tu", "nicht", "dein", "Lob", "mit", "die\u00b7sem", "Ti\u00b7tul", "meh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wie das? mein Freund/ wie so! was geht dich an der", "tokens": ["Wie", "das", "?", "mein", "Freund", "/", "wie", "so", "!", "was", "geht", "dich", "an", "der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PDS", "$.", "PPOSAT", "NN", "$(", "KOKOM", "ADV", "$.", "PWS", "VVFIN", "PRF", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Neid/", "tokens": ["Neid", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.18": {"text": "Der Groben unverstand/ der solches dier verbent;", "tokens": ["Der", "Gro\u00b7ben", "un\u00b7ver\u00b7stand", "/", "der", "sol\u00b7ches", "dier", "ver\u00b7bent", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "ART", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Nauf! nauf und acht es nicht! Es sol dein Lob auf gehe\u0303/", "tokens": ["Nauf", "!", "nauf", "und", "acht", "es", "nicht", "!", "Es", "sol", "dein", "Lob", "auf", "geh\u1ebd", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PTKVZ", "KON", "CARD", "PPER", "PTKNEG", "$.", "PPER", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wo unser Zenith selbst und Nadir pflegt zu stehen/", "tokens": ["Wo", "un\u00b7ser", "Ze\u00b7nith", "selbst", "und", "Na\u00b7dir", "pflegt", "zu", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "KON", "PPER", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Dier/ Neid/ zu spott und hohn; Nun schlie\u00df noch", "tokens": ["Dier", "/", "Neid", "/", "zu", "spott", "und", "hohn", ";", "Nun", "schlie\u00df", "noch"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "NN", "$(", "PTKA", "ADJD", "KON", "PTKVZ", "$.", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "ferner mich", "tokens": ["fer\u00b7ner", "mich"], "token_info": ["word", "word"], "pos": ["ADV", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "In deine Freundschafft ein/ und deinen Neid zer-", "tokens": ["In", "dei\u00b7ne", "Freund\u00b7schafft", "ein", "/", "und", "dei\u00b7nen", "Neid", "zer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "$(", "KON", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "brich.", "tokens": ["brich", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-", "measure": "single.down"}}}}}