{"textgrid.poem.38202": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Ach was hilft ein Bl\u00fcmelein", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sterben ist eine harte Bu\u00df,", "tokens": ["Ster\u00b7ben", "ist", "ei\u00b7ne", "har\u00b7te", "Bu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wei\u00df wohl da\u00df ich sterben mu\u00df,", "tokens": ["Wei\u00df", "wohl", "da\u00df", "ich", "ster\u00b7ben", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein R\u00f6slein rosenroth", "tokens": ["Und", "ein", "R\u00f6s\u00b7lein", "ro\u00b7sen\u00b7roth"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Pflanzt mein Schatz nach meinem Tod.", "tokens": ["Pflanzt", "mein", "Schatz", "nach", "mei\u00b7nem", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn ich mal gestorben bin,", "tokens": ["Wenn", "ich", "mal", "ge\u00b7stor\u00b7ben", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo begrabt man mich denn hin?", "tokens": ["Wo", "be\u00b7grabt", "man", "mich", "denn", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "PRF", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schau nur in den Kirchhof nein,", "tokens": ["Schau", "nur", "in", "den", "Kirch\u00b7hof", "nein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "PTKANT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da wird noch ein Pl\u00e4zlein seyn!", "tokens": ["Da", "wird", "noch", "ein", "Pl\u00e4z\u00b7lein", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wachsen sch\u00f6ne Bl\u00fcmlein drauf,", "tokens": ["Wach\u00b7sen", "sch\u00f6\u00b7ne", "Bl\u00fcm\u00b7lein", "drauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Geben dir ein sch\u00f6nen Straus.", "tokens": ["Ge\u00b7ben", "dir", "ein", "sch\u00f6\u00b7nen", "Straus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Ach was hilft ein R\u00f6slein roth,", "tokens": ["Ach", "was", "hilft", "ein", "R\u00f6s\u00b7lein", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn es bl\u00fcht nach Liebes Tod!", "tokens": ["Wenn", "es", "bl\u00fcht", "nach", "Lie\u00b7bes", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dort hinein, und nicht hinaus,", "tokens": ["Dort", "hin\u00b7ein", ",", "und", "nicht", "hin\u00b7aus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "KON", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4gt man mich ins Grabeshaus,", "tokens": ["Tr\u00e4gt", "man", "mich", "ins", "Gra\u00b7bes\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Habs gesehen in der Nacht,", "tokens": ["Habs", "ge\u00b7se\u00b7hen", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hats ein Traum mir kund gemacht.", "tokens": ["Hats", "ein", "Traum", "mir", "kund", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "PTKVZ", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Auf den Kirchhof wollt ich gehn,", "tokens": ["Auf", "den", "Kirch\u00b7hof", "wollt", "ich", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Th\u00e4t das Grab schon offen stehn,", "tokens": ["Th\u00e4t", "das", "Grab", "schon", "of\u00b7fen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach das Grab war schon gebaut,", "tokens": ["Ach", "das", "Grab", "war", "schon", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hab es traurig angeschaut.", "tokens": ["Hab", "es", "trau\u00b7rig", "an\u00b7ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "War wohl sieben Klafter tief,", "tokens": ["War", "wohl", "sie\u00b7ben", "Klaf\u00b7ter", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "CARD", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Drinnen lag ich schon und schlief,", "tokens": ["Drin\u00b7nen", "lag", "ich", "schon", "und", "schlief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als die Glock hat ausgebrau\u00dft,", "tokens": ["Als", "die", "Glock", "hat", "aus\u00b7ge\u00b7brau\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gingen unsre Freund nach Haus.", "tokens": ["Gin\u00b7gen", "uns\u00b7re", "Freund", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sterben ist ein harte Pein,", "tokens": ["Ster\u00b7ben", "ist", "ein", "har\u00b7te", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenns zwey Herzallerliebste seyn,", "tokens": ["Wenns", "zwey", "Her\u00b7zal\u00b7ler\u00b7liebs\u00b7te", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die des Todes Sichel scheidt,", "tokens": ["Die", "des", "To\u00b7des", "Si\u00b7chel", "scheidt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach das ist das gr\u00f6\u00dfte Leid.", "tokens": ["Ach", "das", "ist", "das", "gr\u00f6\u00df\u00b7te", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Denn was hilft ein Bl\u00fcmelein,", "tokens": ["Denn", "was", "hilft", "ein", "Bl\u00fc\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn es hei\u00dft ins Grab hinein;", "tokens": ["Wenn", "es", "hei\u00dft", "ins", "Grab", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach was hilft ein R\u00f6slein roth,", "tokens": ["Ach", "was", "hilft", "ein", "R\u00f6s\u00b7lein", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn es bl\u00fcht nach Liebes Tod.", "tokens": ["Wenn", "es", "bl\u00fcht", "nach", "Lie\u00b7bes", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}