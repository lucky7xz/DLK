{"textgrid.poem.60254": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An einen Jugendfreund", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Lebens holder Zauber ging vor\u00fcber.", "tokens": ["Des", "Le\u00b7bens", "hol\u00b7der", "Zau\u00b7ber", "ging", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich klage, da\u00df die Jugend mir verloren;", "tokens": ["Ich", "kla\u00b7ge", ",", "da\u00df", "die", "Ju\u00b7gend", "mir", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch eines macht mir noch die Klage tr\u00fcber:", "tokens": ["Doch", "ei\u00b7nes", "macht", "mir", "noch", "die", "Kla\u00b7ge", "tr\u00fc\u00b7ber", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Treue brach, die du mir einst geschworen.", "tokens": ["Die", "Treu\u00b7e", "brach", ",", "die", "du", "mir", "einst", "ge\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nicht meint ich, da\u00df vor uns das teure Erbe", "tokens": ["Nicht", "meint", "ich", ",", "da\u00df", "vor", "uns", "das", "teu\u00b7re", "Er\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "KOUS", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verblichner Jugend \u2013 ihre Freundschaft sterbe.", "tokens": ["Ver\u00b7blich\u00b7ner", "Ju\u00b7gend", "\u2013", "ih\u00b7re", "Freund\u00b7schaft", "ster\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du eiltest im Vergessen! ungeduldig", "tokens": ["Du", "eil\u00b7test", "im", "Ver\u00b7ges\u00b7sen", "!", "un\u00b7ge\u00b7dul\u00b7dig"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Warfst du dem Tod aus deiner Brust entgegen,", "tokens": ["Warfst", "du", "dem", "Tod", "aus", "dei\u00b7ner", "Brust", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was du nur allzubald dem herben schuldig,", "tokens": ["Was", "du", "nur", "all\u00b7zu\u00b7bald", "dem", "her\u00b7ben", "schul\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "ART", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenns einmal aus ist mit des Herzens Schl\u00e4gen.", "tokens": ["Wenns", "ein\u00b7mal", "aus", "ist", "mit", "des", "Her\u00b7zens", "Schl\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "VAFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nicht wolltest du die Treu im Busen halten", "tokens": ["Nicht", "woll\u00b7test", "du", "die", "Treu", "im", "Bu\u00b7sen", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VMFIN", "PPER", "ART", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bis an der Gruft gebieterisch Erkalten.", "tokens": ["Bis", "an", "der", "Gruft", "ge\u00b7bie\u00b7te\u00b7risch", "Er\u00b7kal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn du tief schlummerst unter deinem H\u00fcgel,", "tokens": ["Wenn", "du", "tief", "schlum\u00b7merst", "un\u00b7ter", "dei\u00b7nem", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nichts mehr erf\u00e4hrst vom holden Lenzerwachen,", "tokens": ["Nichts", "mehr", "er\u00b7f\u00e4hrst", "vom", "hol\u00b7den", "Len\u00b7zer\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie laue Winde dann mit leichtem Fl\u00fcgel", "tokens": ["Wie", "lau\u00b7e", "Win\u00b7de", "dann", "mit", "leich\u00b7tem", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Rosenglut am Strauch lebendig fachen,", "tokens": ["Die", "Ro\u00b7seng\u00b7lut", "am", "Strauch", "le\u00b7ben\u00b7dig", "fa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie s\u00fc\u00df dann singen in den gr\u00fcnen Hallen", "tokens": ["Wie", "s\u00fc\u00df", "dann", "sin\u00b7gen", "in", "den", "gr\u00fc\u00b7nen", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von Rosenduft berauschte Nachtigallen:", "tokens": ["Von", "Ro\u00b7sen\u00b7duft", "be\u00b7rauschte", "Nach\u00b7ti\u00b7gal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Dann w\u00e4re fr\u00fch genug der Freund vergessen,", "tokens": ["Dann", "w\u00e4\u00b7re", "fr\u00fch", "ge\u00b7nug", "der", "Freund", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den du geliebt in deinen Jugendtagen,", "tokens": ["Den", "du", "ge\u00b7liebt", "in", "dei\u00b7nen", "Ju\u00b7gend\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des volles Herz gleich gl\u00fchend, unermessen,", "tokens": ["Des", "vol\u00b7les", "Herz", "gleich", "gl\u00fc\u00b7hend", ",", "un\u00b7er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Jugendideal und dir geschlagen.", "tokens": ["Dem", "Ju\u00b7gen\u00b7di\u00b7de\u00b7al", "und", "dir", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Er hielt den Traum umarmet und dein Lieben,", "tokens": ["Er", "hielt", "den", "Traum", "um\u00b7ar\u00b7met", "und", "dein", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "KON", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und beides sah er m\u00e4rchenhaft zerstieben.", "tokens": ["Und", "bei\u00b7des", "sah", "er", "m\u00e4r\u00b7chen\u00b7haft", "zer\u00b7stie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Gleichwie Nachtl\u00fcfte wehn in Bl\u00fctenhagen,", "tokens": ["Gleich\u00b7wie", "Nacht\u00b7l\u00fcf\u00b7te", "wehn", "in", "Bl\u00fc\u00b7ten\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Wehm\u00fctig s\u00e4useln, doch kein Blatt entf\u00fchren;", "tokens": ["Weh\u00b7m\u00fc\u00b7tig", "s\u00e4u\u00b7seln", ",", "doch", "kein", "Blatt", "ent\u00b7f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Nachtigallen durch Geb\u00fcsche klagen,", "tokens": ["Wie", "Nach\u00b7ti\u00b7gal\u00b7len", "durch", "Ge\u00b7b\u00fc\u00b7sche", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch keine Rose je zu Tode r\u00fchren:", "tokens": ["Doch", "kei\u00b7ne", "Ro\u00b7se", "je", "zu", "To\u00b7de", "r\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So sollte dieses Lied mit seinem Trauern", "tokens": ["So", "soll\u00b7te", "die\u00b7ses", "Lied", "mit", "sei\u00b7nem", "Trau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PDAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Durch deine reiche Freudenbl\u00fcte schauern.", "tokens": ["Durch", "dei\u00b7ne", "rei\u00b7che", "Freu\u00b7den\u00b7bl\u00fc\u00b7te", "schau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Jedoch umsonst, da\u00df ich dem Lied geb\u00f6te,", "tokens": ["Je\u00b7doch", "um\u00b7sonst", ",", "da\u00df", "ich", "dem", "Lied", "ge\u00b7b\u00f6\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es will nicht ahmen leiser L\u00fcfte Zittern", "tokens": ["Es", "will", "nicht", "ah\u00b7men", "lei\u00b7ser", "L\u00fcf\u00b7te", "Zit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nicht im Hain das klagende Gefl\u00f6te;", "tokens": ["Und", "nicht", "im", "Hain", "das", "kla\u00b7gen\u00b7de", "Ge\u00b7fl\u00f6\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein rauher Klang will deine Freude sch\u00fcttern.", "tokens": ["Sein", "rau\u00b7her", "Klang", "will", "dei\u00b7ne", "Freu\u00b7de", "sch\u00fct\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hat doch der Frost, der mir von dir gekommen,", "tokens": ["Hat", "doch", "der", "Frost", ",", "der", "mir", "von", "dir", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von meinem Herbstgr\u00fcn auch viel fortgenommen.", "tokens": ["Von", "mei\u00b7nem", "Herbst\u00b7gr\u00fcn", "auch", "viel", "fort\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}}, "stanza.7": {"line.1": {"text": "Das mu\u00df die sanften Klaget\u00f6ne sch\u00e4rfen,", "tokens": ["Das", "mu\u00df", "die", "sanf\u00b7ten", "Kla\u00b7ge\u00b7t\u00f6\u00b7ne", "sch\u00e4r\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seh ich den Freund, mir einst vor allen teuer,", "tokens": ["Seh", "ich", "den", "Freund", ",", "mir", "einst", "vor", "al\u00b7len", "teu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Mein Herz in frohem \u00dcbermut verwerfen;", "tokens": ["Mein", "Herz", "in", "fro\u00b7hem", "\u00dc\u00b7ber\u00b7mut", "ver\u00b7wer\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und z\u00fcnden mu\u00df des Stolzes z\u00fcrnend Feuer.", "tokens": ["Und", "z\u00fcn\u00b7den", "mu\u00df", "des", "Stol\u00b7zes", "z\u00fcr\u00b7nend", "Feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "ART", "NN", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dies Herz war oft von Gottes Flammen helle,", "tokens": ["Dies", "Herz", "war", "oft", "von", "Got\u00b7tes", "Flam\u00b7men", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "ADV", "APPR", "NN", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nicht der Verwerfung Staub ist seine Stelle.", "tokens": ["Nicht", "der", "Ver\u00b7wer\u00b7fung", "Staub", "ist", "sei\u00b7ne", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ich kann es meiner Klage nicht verwehren,", "tokens": ["Ich", "kann", "es", "mei\u00b7ner", "Kla\u00b7ge", "nicht", "ver\u00b7weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df sie dich f\u00fchre l\u00e4ngstverla\u00dfne Pfade,", "tokens": ["Da\u00df", "sie", "dich", "f\u00fch\u00b7re", "l\u00e4ngst\u00b7ver\u00b7la\u00df\u00b7ne", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und da\u00df sie dich, vielleicht auch deine Z\u00e4hren,", "tokens": ["Und", "da\u00df", "sie", "dich", ",", "viel\u00b7leicht", "auch", "dei\u00b7ne", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "$,", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu einem tr\u00fcben Abschiedsfeste lade;", "tokens": ["Zu", "ei\u00b7nem", "tr\u00fc\u00b7ben", "Ab\u00b7schieds\u00b7fes\u00b7te", "la\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denn unsre Freundschaft will ich nun bestatten", "tokens": ["Denn", "uns\u00b7re", "Freund\u00b7schaft", "will", "ich", "nun", "be\u00b7stat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "po