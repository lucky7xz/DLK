{"textgrid.poem.53639": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Der kleine Ge\u00dfler und der gro\u00dfe Grosz", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "R\u00f6sselmann:", "tokens": ["R\u00f6s\u00b7sel\u00b7mann", ":"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Das d\u00fcrft Ihr nicht, das darf der Kaiser nicht,", "tokens": ["Das", "d\u00fcrft", "Ihr", "nicht", ",", "das", "darf", "der", "Kai\u00b7ser", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "$,", "PDS", "VMFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das widerstreitet unsern Freiheitsbriefen!", "tokens": ["Das", "wi\u00b7der\u00b7strei\u00b7tet", "un\u00b7sern", "Frei\u00b7heits\u00b7brie\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ge\u00dfler:", "tokens": ["Ge\u00df\u00b7ler", ":"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Wo sind sie? Hat der Kaiser sie best\u00e4tigt?", "tokens": ["Wo", "sind", "sie", "?", "Hat", "der", "Kai\u00b7ser", "sie", "be\u00b7st\u00e4\u00b7tigt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er hat sie nicht best\u00e4tigt! Diese Gunst", "tokens": ["Er", "hat", "sie", "nicht", "be\u00b7st\u00e4\u00b7tigt", "!", "Die\u00b7se", "Gunst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$.", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mu\u00df erst erworben werden durch Gehorsam.", "tokens": ["Mu\u00df", "erst", "er\u00b7wor\u00b7ben", "wer\u00b7den", "durch", "Ge\u00b7hor\u00b7sam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVPP", "VAINF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Rebellen seid ihr alle gegen Kaisers Gericht", "tokens": ["Re\u00b7bel\u00b7len", "seid", "ihr", "al\u00b7le", "ge\u00b7gen", "Kai\u00b7sers", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "PIS", "APPR", "NN", "NN"], "meter": "---+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "und n\u00e4hrt verwegene Emp\u00f6rung.", "tokens": ["und", "n\u00e4hrt", "ver\u00b7we\u00b7ge\u00b7ne", "Em\u00b7p\u00f6\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ich kenn euch alle, ich durchschau euch ganz:", "tokens": ["Ich", "kenn", "euch", "al\u00b7le", ",", "ich", "durch\u00b7schau", "euch", "ganz", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$,", "PPER", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Den nehm ich jetzt heraus aus eurer Mitte,", "tokens": ["Den", "nehm", "ich", "jetzt", "he\u00b7raus", "aus", "eu\u00b7rer", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Doch alle seid ihr teilhaft seiner Schuld.", "tokens": ["Doch", "al\u00b7le", "seid", "ihr", "teil\u00b7haft", "sei\u00b7ner", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wer klug ist, lerne schweigen und gehorchen.", "tokens": ["Wer", "klug", "ist", ",", "ler\u00b7ne", "schwei\u00b7gen", "und", "ge\u00b7hor\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VVFIN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}