{"dta.poem.20356": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An Mirtillen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mirtille stellte sich/ als ob sie nicht verstanden/", "tokens": ["Mir\u00b7til\u00b7le", "stell\u00b7te", "sich", "/", "als", "ob", "sie", "nicht", "ver\u00b7stan\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "$(", "KOKOM", "KOUS", "PPER", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ich ihr vorgesagt von meinen liebes-banden.", "tokens": ["Was", "ich", "ihr", "vor\u00b7ge\u00b7sagt", "von", "mei\u00b7nen", "lie\u00b7bes\u00b7ban\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun wei\u00df ich wie es kommt/ da\u00df sie mich nicht versteht:", "tokens": ["Nun", "wei\u00df", "ich", "wie", "es", "kommt", "/", "da\u00df", "sie", "mich", "nicht", "ver\u00b7steht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PWAV", "PPER", "VVFIN", "$(", "KOUS", "PPER", "PRF", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie kennt die sprache nicht/ die aus dem munde geht.", "tokens": ["Sie", "kennt", "die", "spra\u00b7che", "nicht", "/", "die", "aus", "dem", "mun\u00b7de", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "PTKNEG", "$(", "ART", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer mit ihr reden will/ der mu\u00df die sitten brechen/", "tokens": ["Wer", "mit", "ihr", "re\u00b7den", "will", "/", "der", "mu\u00df", "die", "sit\u00b7ten", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVINF", "VMFIN", "$(", "ART", "VMFIN", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und mit den h\u00e4nden nur/ nicht mit den lippen sprechen.", "tokens": ["Und", "mit", "den", "h\u00e4n\u00b7den", "nur", "/", "nicht", "mit", "den", "lip\u00b7pen", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "ADV", "$(", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}