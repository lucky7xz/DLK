{"dta.poem.2710": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Fremdlinge alhier/  \n  Betrachtet bey Absterben Fr. M. S. v. L. g.  \n K. v. F. den 27. Mart. 1678.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach Leben voller M\u00fch'/ voll Schrecken/ voller Thr\u00e4nen!", "tokens": ["Ach", "Le\u00b7ben", "vol\u00b7ler", "M\u00fch'", "/", "voll", "Schre\u00b7cken", "/", "vol\u00b7ler", "Thr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "NN", "ADJA", "NN", "$(", "ADJD", "NN", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gebrechlicher als Gla\u00df/ und fl\u00fcchtiger als Wind!", "tokens": ["Ge\u00b7brech\u00b7li\u00b7cher", "als", "Gla\u00df", "/", "und", "fl\u00fcch\u00b7ti\u00b7ger", "als", "Wind", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "NN", "$(", "KON", "ADJA", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit Jrrthum \u00fcberh\u00e4ufft/ durchbeitzt mit herbem Sehnen", "tokens": ["Mit", "Jrr\u00b7thum", "\u00fc\u00b7berh\u00b7\u00e4ufft", "/", "durch\u00b7beitzt", "mit", "her\u00b7bem", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$(", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In dessen Unbestand sich kein Bestand nicht findt.", "tokens": ["In", "des\u00b7sen", "Un\u00b7be\u00b7stand", "sich", "kein", "Be\u00b7stand", "nicht", "findt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PRF", "PIAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie reitzest du uns nicht? Wie kanst du uns nicht blenden?", "tokens": ["Wie", "reit\u00b7zest", "du", "uns", "nicht", "?", "Wie", "kanst", "du", "uns", "nicht", "blen\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "PTKNEG", "$.", "PWAV", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da doch dein lauter Nichts dein erster Anfang heist.", "tokens": ["Da", "doch", "dein", "lau\u00b7ter", "Nichts", "dein", "ers\u00b7ter", "An\u00b7fang", "heist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "PIAT", "PIS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du schwindest/ wenn du w\u00e4chst/ wie Schatten untern H\u00e4nden/", "tokens": ["Du", "schwin\u00b7dest", "/", "wenn", "du", "w\u00e4chst", "/", "wie", "Schat\u00b7ten", "un\u00b7tern", "H\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "VVFIN", "$(", "KOKOM", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bist/ wenn du gleich erh\u00f6ht/ ein scharffer Rauch der beist.", "tokens": ["Bist", "/", "wenn", "du", "gleich", "er\u00b7h\u00f6ht", "/", "ein", "scharf\u00b7fer", "Rauch", "der", "beist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "KOUS", "PPER", "ADV", "VVPP", "$(", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Den Narren scheinst du s\u00fc\u00df\u2019 und weisen Leuten bitter/", "tokens": ["Den", "Nar\u00b7ren", "scheinst", "du", "s\u00fc\u00df'", "und", "wei\u00b7sen", "Leu\u00b7ten", "bit\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der jene/ so dich liebt/ der kennt dich nicht einmal.", "tokens": ["Der", "je\u00b7ne", "/", "so", "dich", "liebt", "/", "der", "kennt", "dich", "nicht", "ein\u00b7mal", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "$(", "ADV", "PPER", "VVFIN", "$(", "ART", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wer dich verachten lernt/ verstehet deine G\u00fcter/", "tokens": ["Wer", "dich", "ver\u00b7ach\u00b7ten", "lernt", "/", "ver\u00b7ste\u00b7het", "dei\u00b7ne", "G\u00fc\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "VVFIN", "$(", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und wer dir glauben will/ der traut auf Frucht und Qual.", "tokens": ["Und", "wer", "dir", "glau\u00b7ben", "will", "/", "der", "traut", "auf", "Frucht", "und", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVINF", "VMFIN", "$(", "ART", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du siehst so sch\u00f6ne nicht/ als du dich kanst vorstellen/", "tokens": ["Du", "siehst", "so", "sch\u00f6\u00b7ne", "nicht", "/", "als", "du", "dich", "kanst", "vor\u00b7stel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "PTKNEG", "$(", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ja vielen zeigst du dich ", "tokens": ["Ja", "vie\u00b7len", "zeigst", "du", "dich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "PIS", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Nur da\u00df sie s\u00fcndigen und fahren zu der H\u00f6llen.", "tokens": ["Nur", "da\u00df", "sie", "s\u00fcn\u00b7di\u00b7gen", "und", "fah\u00b7ren", "zu", "der", "H\u00f6l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVINF", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Viel raubst du unverhofft eh\u2019 sie noch schwach und kranck/", "tokens": ["Viel", "raubst", "du", "un\u00b7ver\u00b7hofft", "eh'", "sie", "noch", "schwach", "und", "kranck", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Blo\u00df da\u00df du kanst den Raum der Busse so beschneiden/", "tokens": ["Blo\u00df", "da\u00df", "du", "kanst", "den", "Raum", "der", "Bus\u00b7se", "so", "be\u00b7schnei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VMFIN", "ART", "NN", "ART", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Je mehr du uns verheischt/ je weniger du schenckst.", "tokens": ["Je", "mehr", "du", "uns", "ver\u00b7heischt", "/", "je", "we\u00b7ni\u00b7ger", "du", "schenckst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "VVFIN", "$(", "ADV", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Dein gr\u00f6stes Wolthun ist ein lang und schmertzlich leiden/", "tokens": ["Dein", "gr\u00f6s\u00b7tes", "Wolt\u00b7hun", "ist", "ein", "lang", "und", "schmertz\u00b7lich", "lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "ADJD", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Daran zum Ausschlag du noch neue Martern h\u00e4ngst.", "tokens": ["Da\u00b7ran", "zum", "Aus\u00b7schlag", "du", "noch", "neu\u00b7e", "Mar\u00b7tern", "h\u00e4ngst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "NN", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Das bist du/ Leben/ nun. So sitzen wir zu Miethe", "tokens": ["Das", "bist", "du", "/", "Le\u00b7ben", "/", "nun", ".", "So", "sit\u00b7zen", "wir", "zu", "Mie\u00b7the"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "$(", "NN", "$(", "ADV", "$.", "ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und wissen nicht die Zeit wenn Gott uns/ ziehet! sagt.", "tokens": ["Und", "wis\u00b7sen", "nicht", "die", "Zeit", "wenn", "Gott", "uns", "/", "zie\u00b7het", "!", "sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "KOUS", "NN", "PPER", "$(", "VVFIN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wenn seine Stimm erschallt: Mensch geh au\u00df dem Gebiethe/", "tokens": ["Wenn", "sei\u00b7ne", "Stimm", "er\u00b7schallt", ":", "Mensch", "geh", "au\u00df", "dem", "Ge\u00b7bie\u00b7the", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "$.", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sein Stunden-Gla\u00df ist au\u00df/ de\u00df Lebens Frist vertagt.", "tokens": ["Sein", "Stun\u00b7den\u00b7Gla\u00df", "ist", "au\u00df", "/", "de\u00df", "Le\u00b7bens", "Frist", "ver\u00b7tagt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "$(", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wer wolte sich alsdenn von Mesech nicht begeben?", "tokens": ["Wer", "wol\u00b7te", "sich", "als\u00b7denn", "von", "Me\u00b7sech", "nicht", "be\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PRF", "ADV", "APPR", "NE", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und l\u00e4nger Frembdling noch bey seinen H\u00fcrden seyn?", "tokens": ["Und", "l\u00e4n\u00b7ger", "Frembd\u00b7ling", "noch", "bey", "sei\u00b7nen", "H\u00fcr\u00b7den", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wer wolte weiter dann bey Kedars H\u00fctten leben?", "tokens": ["Wer", "wol\u00b7te", "wei\u00b7ter", "dann", "bey", "Ke\u00b7dars", "H\u00fct\u00b7ten", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "APPR", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Umzirckt von Krieg und Streit gedr\u00fcckt von Angst und Pein.", "tokens": ["Um\u00b7zirckt", "von", "Krieg", "und", "Streit", "ge\u00b7dr\u00fcckt", "von", "Angst", "und", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Es scheint dem Menschen schwer bey wildem Volck zu wohnen/", "tokens": ["Es", "scheint", "dem", "Men\u00b7schen", "schwer", "bey", "wil\u00b7dem", "Volck", "zu", "woh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Das gar an Grausamkeit den Thieren gleiche geht.", "tokens": ["Das", "gar", "an", "Grau\u00b7sam\u00b7keit", "den", "Thie\u00b7ren", "glei\u00b7che", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "NN", "ART", "NN", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und wer will Nachbar seyn bey Scyth und Nasamonen/", "tokens": ["Und", "wer", "will", "Nach\u00b7bar", "seyn", "bey", "Scyth", "und", "Na\u00b7sa\u00b7mo\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "NN", "VAINF", "APPR", "NE", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da keiner weder Gott noch die Natur versteht.", "tokens": ["Da", "kei\u00b7ner", "we\u00b7der", "Gott", "noch", "die", "Na\u00b7tur", "ver\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "KON", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Allein viel schmertzlicher wird es der Seele fallen", "tokens": ["Al\u00b7lein", "viel", "schmertz\u00b7li\u00b7cher", "wird", "es", "der", "See\u00b7le", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "PPER", "ART", "NN", "VVINF"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.34": {"text": "Wenn sie so lang allhier im Leibe bleiben mu\u00df/", "tokens": ["Wenn", "sie", "so", "lang", "all\u00b7hier", "im", "Lei\u00b7be", "blei\u00b7ben", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ADV", "APPRART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und unter Fleisch und Blut bey M\u00f6rd- und R\u00e4ubern wallen/", "tokens": ["Und", "un\u00b7ter", "Fleisch", "und", "Blut", "bey", "M\u00f6rd", "und", "R\u00e4u\u00b7bern", "wal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "APPR", "TRUNC", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da ihr macht t\u00e4glich Weh der S\u00fcnden Uberflu\u00df.", "tokens": ["Da", "ihr", "macht", "t\u00e4g\u00b7lich", "Weh", "der", "S\u00fcn\u00b7den", "U\u00b7ber\u00b7flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Der ist ein feiger Mensch/ der/ wenn er satt gelebet/", "tokens": ["Der", "ist", "ein", "fei\u00b7ger", "Mensch", "/", "der", "/", "wenn", "er", "satt", "ge\u00b7le\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$(", "ART", "$(", "KOUS", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Zuletzt noch klagen will die abgek\u00fcrtzte Zeit:", "tokens": ["Zu\u00b7letzt", "noch", "kla\u00b7gen", "will", "die", "ab\u00b7ge\u00b7k\u00fcrtz\u00b7te", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der Viehisch/ welcher frech dem Himmel widerstrebet/", "tokens": ["Der", "Vie\u00b7hisch", "/", "wel\u00b7cher", "frech", "dem", "Him\u00b7mel", "wi\u00b7der\u00b7stre\u00b7bet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "ADJD", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und sich nur weltzen will im Schlam der Eitelkeit.", "tokens": ["Und", "sich", "nur", "welt\u00b7zen", "will", "im", "Schlam", "der", "Ei\u00b7tel\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADV", "VVINF", "VMFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wer hier stets Wirth seyn will/ kan dort nicht B\u00fcrger werden/", "tokens": ["Wer", "hier", "stets", "Wirth", "seyn", "will", "/", "kan", "dort", "nicht", "B\u00fcr\u00b7ger", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "NN", "VAINF", "VMFIN", "$(", "VMFIN", "ADV", "PTKNEG", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Wir m\u00fcssen au\u00df der See de\u00df Lebens an den Port.", "tokens": ["Wir", "m\u00fcs\u00b7sen", "au\u00df", "der", "See", "de\u00df", "Le\u00b7bens", "an", "den", "Port", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "ART", "NN", "APPR", "ART", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Das Elend das wir bau\u2019n auf diesem Rund der Erden", "tokens": ["Das", "E\u00b7lend", "das", "wir", "bau'n", "auf", "die\u00b7sem", "Rund", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "APPR", "PDAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Ermahnt/ und rufft uns zu: Fleuch/ Frembdling/ eile fort!", "tokens": ["Er\u00b7mahnt", "/", "und", "rufft", "uns", "zu", ":", "Fleuch", "/", "Frembd\u