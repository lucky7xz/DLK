{"textgrid.poem.42836": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Das Kartenspiel", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vier M\u00e4nner zogen sich zur\u00fcck,", "tokens": ["Vier", "M\u00e4n\u00b7ner", "zo\u00b7gen", "sich", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schlossen sich ein, und drei", "tokens": ["Schlos\u00b7sen", "sich", "ein", ",", "und", "drei"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PRF", "PTKVZ", "$,", "KON", "CARD"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Von ihnen versuchten ihr Gl\u00fcck,", "tokens": ["Von", "ih\u00b7nen", "ver\u00b7such\u00b7ten", "ihr", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Spielten Karten.", "tokens": ["Spiel\u00b7ten", "Kar\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Drau\u00dfen im Garten", "tokens": ["Drau\u00b7\u00dfen", "im", "Gar\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Bl\u00fchte der Mai.", "tokens": ["Bl\u00fch\u00b7te", "der", "Mai", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Im schw\u00fclen Zimmer sa\u00dfen die", "tokens": ["Im", "schw\u00fc\u00b7len", "Zim\u00b7mer", "sa\u00b7\u00dfen", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "M\u00e4nner bei ihren Karten.", "tokens": ["M\u00e4n\u00b7ner", "bei", "ih\u00b7ren", "Kar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "Ihre Weiber lie\u00dfen sie", "tokens": ["Ih\u00b7re", "Wei\u00b7ber", "lie\u00b7\u00dfen", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Drau\u00dfen weinen und warten.", "tokens": ["Drau\u00b7\u00dfen", "wei\u00b7nen", "und", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KON", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Und spielten Spiel um Spiel zu dritt,", "tokens": ["Und", "spiel\u00b7ten", "Spiel", "um", "Spiel", "zu", "dritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "NN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und jeder schwitzte.", "tokens": ["Und", "je\u00b7der", "schwitz\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der vierte Mann sah zu, kibit \u2013", "tokens": ["Der", "vier\u00b7te", "Mann", "sah", "zu", ",", "ki\u00b7bit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,", "FM.la", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kibitzte.", "tokens": ["Ki\u00b7bitz\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Geld hin \u2013 Geld her \u2013 Geld her \u2013 Geld hin \u2013", "tokens": ["Geld", "hin", "\u2013", "Geld", "her", "\u2013", "Geld", "her", "\u2013", "Geld", "hin", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$(", "NN", "PTKVZ", "$(", "NN", "PTKVZ", "$(", "NN", "PTKVZ", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Verlust \u2013 Gewinn \u2013", "tokens": ["Ver\u00b7lust", "\u2013", "Ge\u00b7winn", "\u2013"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nach Kartengemisch.", "tokens": ["Nach", "Kar\u00b7ten\u00b7ge\u00b7misch", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Es wurde gebucht,", "tokens": ["Es", "wur\u00b7de", "ge\u00b7bucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Gereizt und geflucht.", "tokens": ["Ge\u00b7reizt", "und", "ge\u00b7flucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KON", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Man schlug auf den Tisch.", "tokens": ["Man", "schlug", "auf", "den", "Tisch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Man witzelte seicht.", "tokens": ["Man", "wit\u00b7zel\u00b7te", "seicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "H\u00e4tte Pikdame statt Karozehn", "tokens": ["H\u00e4t\u00b7te", "Pik\u00b7da\u00b7me", "statt", "Ka\u00b7ro\u00b7zehn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "APPR", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Den Buben genommen,", "tokens": ["Den", "Bu\u00b7ben", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Dann w\u00e4re vielleicht", "tokens": ["Dann", "w\u00e4\u00b7re", "viel\u00b7leicht"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Alles anders gekommen.", "tokens": ["Al\u00b7les", "an\u00b7ders", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.4": {"line.1": {"text": "Und noch einmal und noch und noch,", "tokens": ["Und", "noch", "ein\u00b7mal", "und", "noch", "und", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "KON", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verbissen und besessen. \u2013", "tokens": ["Ver\u00b7bis\u00b7sen", "und", "be\u00b7ses\u00b7sen", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVPP", "KON", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein L\u00fcftchen kam durchs Schl\u00fcsselloch,", "tokens": ["Ein", "L\u00fcft\u00b7chen", "kam", "durchs", "Schl\u00fcs\u00b7sel\u00b7loch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Roch nach verbranntem Essen.", "tokens": ["Roch", "nach", "ver\u00b7brann\u00b7tem", "Es\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der K\u00f6nig fiel.", "tokens": ["Der", "K\u00f6\u00b7nig", "fiel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Das letzte Spiel,", "tokens": ["Das", "letz\u00b7te", "Spiel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Das allerletzte Spiel begann.", "tokens": ["Das", "al\u00b7ler\u00b7letz\u00b7te", "Spiel", "be\u00b7gann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wieder stach die Karozehn.", "tokens": ["Und", "wie\u00b7der", "stach", "die", "Ka\u00b7ro\u00b7zehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der vierte Mann,", "tokens": ["Der", "vier\u00b7te", "Mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der nichts getan als zugesehn,", "tokens": ["Der", "nichts", "ge\u00b7tan", "als", "zu\u00b7ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVPP", "KOKOM", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gewann.", "tokens": ["Ge\u00b7wann", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.6": {"line.1": {"text": "Vier g\u00e4hnende M\u00e4nner gingen", "tokens": ["Vier", "g\u00e4h\u00b7nen\u00b7de", "M\u00e4n\u00b7ner", "gin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hinaus ins Morgengraun.", "tokens": ["Hin\u00b7aus", "ins", "Mor\u00b7gen\u00b7graun", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drau\u00dfen hingen", "tokens": ["Drau\u00b7\u00dfen", "hin\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Am Gartenzaun", "tokens": ["Am", "Gar\u00b7ten\u00b7zaun"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Vier vertrocknete Fraun.", "tokens": ["Vier", "ver\u00b7trock\u00b7ne\u00b7te", "Fraun", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}