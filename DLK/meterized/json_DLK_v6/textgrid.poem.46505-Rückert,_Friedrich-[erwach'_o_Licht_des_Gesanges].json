"Dazu in der Nacht hat man Kerzen,", "tokens": ["Da\u00b7zu", "in", "der", "Nacht", "hat", "man", "Ker\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ART", "NN", "VAFIN", "PIS", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wenn aus ist Sonnenschein.", "tokens": ["Wenn", "aus", "ist", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Den Schein der Sonn' ersetzen,", "tokens": ["Den", "Schein", "der", "Sonn'", "er\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Kerze, kannst du nicht;", "tokens": ["O", "Ker\u00b7ze", ",", "kannst", "du", "nicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch kann das Auge sich letzen", "tokens": ["Doch", "kann", "das", "Au\u00b7ge", "sich", "let\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN", "PRF", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "An keinem anderen Licht.", "tokens": ["An", "kei\u00b7nem", "an\u00b7de\u00b7ren", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Ich zag' ums Herz, wie lang es", "tokens": ["Ich", "zag'", "ums", "Herz", ",", "wie", "lang", "es"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PWAV", "ADJD", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist ohne Freudenschwung;", "tokens": ["Ist", "oh\u00b7ne", "Freu\u00b7den\u00b7schwung", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erwach' o Licht des Gesanges,", "tokens": ["Er\u00b7wach'", "o", "Licht", "des", "Ge\u00b7san\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "O Licht der Beseligung!", "tokens": ["O", "Licht", "der", "Be\u00b7se\u00b7li\u00b7gung", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Wach', holden Ueberschwanges,", "tokens": ["Wach'", ",", "hol\u00b7den", "Ue\u00b7ber\u00b7schwan\u00b7ges", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Licht der Erinnerung,", "tokens": ["O", "Licht", "der", "E\u00b7rin\u00b7ne\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Bis ich beschwichtigten Dranges", "tokens": ["Bis", "ich", "be\u00b7schwich\u00b7tig\u00b7ten", "Dran\u00b7ges"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schlaf' ein in D\u00e4mmerung!", "tokens": ["Schlaf'", "ein", "in", "D\u00e4m\u00b7me\u00b7rung", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}}}}