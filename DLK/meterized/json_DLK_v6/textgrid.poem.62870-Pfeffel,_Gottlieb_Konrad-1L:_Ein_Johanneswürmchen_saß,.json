{"textgrid.poem.62870": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Johannesw\u00fcrmchen sa\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Johannesw\u00fcrmchen sa\u00df,", "tokens": ["Ein", "Jo\u00b7han\u00b7nes\u00b7w\u00fcrm\u00b7chen", "sa\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seines Demantscheins", "tokens": ["Sei\u00b7nes", "De\u00b7mant\u00b7scheins"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Unbewu\u00dft, im weichen Gras", "tokens": ["Un\u00b7be\u00b7wu\u00dft", ",", "im", "wei\u00b7chen", "Gras"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eines Bardenhains.", "tokens": ["Ei\u00b7nes", "Bar\u00b7den\u00b7hains", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Leise schlich aus faulem Moos", "tokens": ["Lei\u00b7se", "schlich", "aus", "fau\u00b7lem", "Moos"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich ein Ungeth\u00fcm,", "tokens": ["Sich", "ein", "Un\u00b7ge\u00b7th\u00fcm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Eine Kr\u00f6te, her und scho\u00df", "tokens": ["Ei\u00b7ne", "Kr\u00f6\u00b7te", ",", "her", "und", "scho\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PTKVZ", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "All ihr Gift nach ihm.", "tokens": ["All", "ihr", "Gift", "nach", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ach was hab ich dir gethan?", "tokens": ["Ach", "was", "hab", "ich", "dir", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rief der Wurm ihr zu.", "tokens": ["Rief", "der", "Wurm", "ihr", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ey, fuhr ihn das Unthier an,", "tokens": ["Ey", ",", "fuhr", "ihn", "das", "Un\u00b7thier", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Warum gl\u00e4nzest du?", "tokens": ["Wa\u00b7rum", "gl\u00e4n\u00b7zest", "du", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}