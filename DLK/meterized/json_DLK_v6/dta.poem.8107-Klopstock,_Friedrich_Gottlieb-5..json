{"dta.poem.8107": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aussaat, o wie reif schimmerst du her! Laut ruft im Gefild", "tokens": ["Aus\u00b7saat", ",", "o", "wie", "reif", "schim\u00b7merst", "du", "her", "!", "Laut", "ruft", "im", "Ge\u00b7fild"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "FM", "KOKOM", "ADJD", "VVFIN", "PPER", "PTKVZ", "$.", "APPR", "VVFIN", "APPRART", "NN"], "meter": "-+-+++-+-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "Die Heerschaar zu der Erndte! Selige, die, Glanz zu Glanz,", "tokens": ["Die", "Heer\u00b7schaar", "zu", "der", "Ernd\u00b7te", "!", "Se\u00b7li\u00b7ge", ",", "die", ",", "Glanz", "zu", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$.", "NN", "$,", "PRELS", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Der Vollender sammelt, wie nimmt", "tokens": ["Der", "Voll\u00b7en\u00b7der", "sam\u00b7melt", ",", "wie", "nimmt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des neuen Aeoons Herrlichkeit euch auf!", "tokens": ["Des", "neu\u00b7en", "A\u00b7e\u00b7oons", "Herr\u00b7lich\u00b7keit", "euch", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}