{"textgrid.poem.55259": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Breit wie lang", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer bescheiden ist, mu\u00df dulden,", "tokens": ["Wer", "be\u00b7schei\u00b7den", "ist", ",", "mu\u00df", "dul\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wer frech ist, der mu\u00df leiden;", "tokens": ["Und", "wer", "frech", "ist", ",", "der", "mu\u00df", "lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "VAFIN", "$,", "ART", "VMFIN", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Also wirst du gleich verschulden,", "tokens": ["Al\u00b7so", "wirst", "du", "gleich", "ver\u00b7schul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob du frech seist, ob bescheiden.", "tokens": ["Ob", "du", "frech", "seist", ",", "ob", "be\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "KOUS", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}