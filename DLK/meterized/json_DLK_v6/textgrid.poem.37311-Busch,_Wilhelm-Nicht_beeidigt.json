{"textgrid.poem.37311": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Nicht beeidigt", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du gelobt sein, so verzichte", "tokens": ["Willst", "du", "ge\u00b7lobt", "sein", ",", "so", "ver\u00b7zich\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "VVPP", "VAINF", "$,", "ADV", "VVFIN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Auf kindlich bl\u00f6des Wesen.", "tokens": ["Auf", "kind\u00b7lich", "bl\u00f6\u00b7des", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Entschlie\u00df dich, deine himmlischen Gedichte", "tokens": ["Ent\u00b7schlie\u00df", "dich", ",", "dei\u00b7ne", "himm\u00b7li\u00b7schen", "Ge\u00b7dich\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Leuten vorzulesen.", "tokens": ["Den", "Leu\u00b7ten", "vor\u00b7zu\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Welt ist h\u00f6flich und gesellig,", "tokens": ["Die", "Welt", "ist", "h\u00f6f\u00b7lich", "und", "ge\u00b7sel\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und eh man dich beleidigt,", "tokens": ["Und", "eh", "man", "dich", "be\u00b7lei\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sagt wohl ein jeder leicht, was dir gef\u00e4llig,", "tokens": ["Sagt", "wohl", "ein", "je\u00b7der", "leicht", ",", "was", "dir", "ge\u00b7f\u00e4l\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "PIS", "ADJD", "$,", "PWS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn keiner ist beeidigt.", "tokens": ["Denn", "kei\u00b7ner", "ist", "be\u00b7ei\u00b7digt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}