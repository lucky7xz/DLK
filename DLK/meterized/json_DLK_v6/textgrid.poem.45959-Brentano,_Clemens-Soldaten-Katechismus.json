{"textgrid.poem.45959": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Soldaten-Katechismus", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bist matt und m\u00fcd, so sing ein Lied,", "tokens": ["Bist", "matt", "und", "m\u00fcd", ",", "so", "sing", "ein", "Lied", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Herzenslust, das st\u00e4rkt die Brust.", "tokens": ["Aus", "Her\u00b7zens\u00b7lust", ",", "das", "st\u00e4rkt", "die", "Brust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In h\u00f6chster Qual fluch' wohl einmal,", "tokens": ["In", "h\u00f6chs\u00b7ter", "Qual", "fluch'", "wohl", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In hei\u00dfem Streit Gott dir's verzeiht.", "tokens": ["In", "hei\u00b7\u00dfem", "Streit", "Gott", "dir's", "ver\u00b7zeiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Geh in die Schanz froh wie zum Tanz,", "tokens": ["Geh", "in", "die", "Schanz", "froh", "wie", "zum", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADJD", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Heil giebt der Tod, das Leben Not.", "tokens": ["Heil", "giebt", "der", "Tod", ",", "das", "Le\u00b7ben", "Not", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Gefangen sein ist gro\u00dfe Pein,", "tokens": ["Ge\u00b7fan\u00b7gen", "sein", "ist", "gro\u00b7\u00dfe", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel besser ficht bis 's Aug dir bricht.", "tokens": ["Viel", "bes\u00b7ser", "ficht", "bis", "'s", "Aug", "dir", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "PPER", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Scheint grausam dir dein Offizier,", "tokens": ["Scheint", "grau\u00b7sam", "dir", "dein", "Of\u00b7fi\u00b7zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bedenke hart ist Krieges Art.", "tokens": ["Be\u00b7den\u00b7ke", "hart", "ist", "Krie\u00b7ges", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VAFIN", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der B\u00fcrger schw\u00e4tzt, der Prahler wetzt,", "tokens": ["Der", "B\u00fcr\u00b7ger", "schw\u00e4tzt", ",", "der", "Prah\u00b7ler", "wetzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Krieger ficht, Mensch richte nicht.", "tokens": ["Der", "Krie\u00b7ger", "ficht", ",", "Mensch", "rich\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nicht r\u00e4sonier', wie man dich f\u00fchr',", "tokens": ["Nicht", "r\u00e4\u00b7so\u00b7nier'", ",", "wie", "man", "dich", "f\u00fchr'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "PWAV", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist im Plan, man giebt ihn an.", "tokens": ["Du", "bist", "im", "Plan", ",", "man", "giebt", "ihn", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$,", "PIS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Montur ist eng, Ordnung gestreng,", "tokens": ["Mon\u00b7tur", "ist", "eng", ",", "Ord\u00b7nung", "ge\u00b7streng", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,", "NN", "ADJD", "$,"], "meter": "+-+++--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "F\u00fcr alles steht, der vor dir geht.", "tokens": ["F\u00fcr", "al\u00b7les", "steht", ",", "der", "vor", "dir", "geht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Halt trocken rein so Schlo\u00df als Stein,", "tokens": ["Halt", "tro\u00b7cken", "rein", "so", "Schlo\u00df", "als", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "ADV", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Leicht ist's geputzt und viel es nutzt.", "tokens": ["Leicht", "ist's", "ge\u00b7putzt", "und", "viel", "es", "nutzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "VVPP", "KON", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Bad', wasche dich, wenn's schicket sich,", "tokens": ["Bad'", ",", "wa\u00b7sche", "dich", ",", "wenn's", "schi\u00b7cket", "sich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "$,", "PIS", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gesund dich's h\u00e4lt, und kost't kein Geld.", "tokens": ["Ge\u00b7sund", "dich's", "h\u00e4lt", ",", "und", "kost't", "kein", "Geld", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "$,", "KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Wo du quartiert, hilf gern dem Wirt,", "tokens": ["Wo", "du", "quar\u00b7tiert", ",", "hilf", "gern", "dem", "Wirt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVIMP", "ADV", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Dann tut er dir mehr als Geb\u00fchr.", "tokens": ["Dann", "tut", "er", "dir", "mehr", "als", "Ge\u00b7b\u00fchr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Du bist Soldat, die Kriegestat", "tokens": ["Du", "bist", "Sol\u00b7dat", ",", "die", "Krie\u00b7ge\u00b7stat"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sei dein Genu\u00df aus Will und Mu\u00df.", "tokens": ["Sei", "dein", "Ge\u00b7nu\u00df", "aus", "Will", "und", "Mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "NE", "KON", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Hart ist die Nu\u00df, doch bei\u00dft das Mu\u00df", "tokens": ["Hart", "ist", "die", "Nu\u00df", ",", "doch", "bei\u00dft", "das", "Mu\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Kern heraus, das sei dein Schmaus!", "tokens": ["Den", "Kern", "he\u00b7raus", ",", "das", "sei", "dein", "Schmaus", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Sei treu der Fahn' stets zugetan,", "tokens": ["Sei", "treu", "der", "Fahn'", "stets", "zu\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du schworst bei ihr, nicht desertier'.", "tokens": ["Du", "schworst", "bei", "ihr", ",", "nicht", "de\u00b7ser\u00b7tier'", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "$,", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.15": {"line.1": {"text": "Mit Magd und Weib nicht Mutwill treib,", "tokens": ["Mit", "Magd", "und", "Weib", "nicht", "Mut\u00b7will", "treib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die dich gebar auch beides war.", "tokens": ["Die", "dich", "ge\u00b7bar", "auch", "bei\u00b7des", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADV", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Getreue Lieb' nur Einer gieb,", "tokens": ["Ge\u00b7treu\u00b7e", "Lieb'", "nur", "Ei\u00b7ner", "gieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das st\u00e4rkt in Schlacht und Todesnacht.", "tokens": ["Das", "st\u00e4rkt", "in", "Schlacht", "und", "To\u00b7des\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Wer alle Tag' treibt neuen Scherz,", "tokens": ["Wer", "al\u00b7le", "Tag'", "treibt", "neu\u00b7en", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat statt dem Herz 'neu Taubenschlag.", "tokens": ["Hat", "statt", "dem", "Herz", "'neu", "Tau\u00b7ben\u00b7schlag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Trink nicht zuviel beim W\u00fcrfelspiel", "tokens": ["Trink", "nicht", "zu\u00b7viel", "beim", "W\u00fcr\u00b7fel\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das giebt b\u00f6s Wort und bringt in Mord.", "tokens": ["Das", "giebt", "b\u00f6s", "Wort", "und", "bringt", "in", "Mord", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "NN", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Halt auf die Ehr', doch \u00fcberh\u00f6r'", "tokens": ["Halt", "auf", "die", "Ehr'", ",", "doch", "\u00fc\u00b7ber\u00b7h\u00f6r'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Wort, das leicht vom Munde streicht.", "tokens": ["Ein", "Wort", ",", "das", "leicht", "vom", "Mun\u00b7de", "streicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Hart ist die Zeit, such' keinen Streit,", "tokens": ["Hart", "ist", "die", "Zeit", ",", "such'", "kei\u00b7nen", "Streit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wo der Feind im Feld erscheint.", "tokens": ["Als", "wo", "der", "Feind", "im", "Feld", "er\u00b7scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.21": {"line.1": {"text": "Schneid kein Gesicht dem Schw\u00e4chern nicht,", "tokens": ["Schneid", "kein", "Ge\u00b7sicht", "dem", "Schw\u00e4\u00b7chern", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Schwacher ist doch auch ein Christ.", "tokens": ["Ein", "Schwa\u00b7cher", "ist", "doch", "auch", "ein", "Christ", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.22": {"line.1": {"text": "Verleumd' geschwind kein armes Kind,", "tokens": ["Ver\u00b7leumd'", "ge\u00b7schwind", "kein", "ar\u00b7mes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer B\u00f6ses spricht, sich selber sticht.", "tokens": ["Wer", "B\u00f6\u00b7ses", "spricht", ",", "sich", "sel\u00b7ber", "sticht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VVFIN", "$,", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.23": {"line.1": {"text": "Die Landwehr ehr', ihr Dienst ist schwer,", "tokens": ["Die", "Land\u00b7wehr", "ehr'", ",", "ihr", "Dienst", "ist", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4\u00dft Hof und Haus und hilft dir aus.", "tokens": ["L\u00e4\u00dft", "Hof", "und", "Haus", "und", "hilft", "dir", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.24": {"line.1": {"text": "In Feindes Land \u00fcb' keine Schand,", "tokens": ["In", "Fein\u00b7des", "Land", "\u00fcb'", "kei\u00b7ne", "Schand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das merkt er sich und sch\u00fctzet dich.", "tokens": ["Das", "merkt", "er", "sich", "und", "sch\u00fct\u00b7zet", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PRF", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.25": {"line.1": {"text": "Doch trau' auch nicht auf jed Gesicht,", "tokens": ["Doch", "trau'", "auch", "nicht", "auf", "jed", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Sei streng und mild, ein edles Bild.", "tokens": ["Sei", "streng", "und", "mild", ",", "ein", "ed\u00b7les", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "ADJD", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.26": {"line.1": {"text": "Wer als dein Feind gesund erscheint,", "tokens": ["Wer", "als", "dein", "Feind", "ge\u00b7sund", "er\u00b7schein