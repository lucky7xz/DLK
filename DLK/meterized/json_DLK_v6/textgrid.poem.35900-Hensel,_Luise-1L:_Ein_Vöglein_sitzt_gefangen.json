{"textgrid.poem.35900": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein V\u00f6glein sitzt gefangen", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein V\u00f6glein sitzt gefangen", "tokens": ["Ein", "V\u00f6\u00b7glein", "sitzt", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im engen Kerker sein", "tokens": ["Im", "en\u00b7gen", "Ker\u00b7ker", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schl\u00e4gt die Eisenstangen", "tokens": ["Und", "schl\u00e4gt", "die", "Ei\u00b7sen\u00b7stan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit wunden Fl\u00fcgelein.", "tokens": ["Mit", "wun\u00b7den", "Fl\u00fc\u00b7ge\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es weht zu ihm her\u00fcber", "tokens": ["Es", "weht", "zu", "ihm", "her\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der frohen Br\u00fcder Lied;", "tokens": ["Der", "fro\u00b7hen", "Br\u00fc\u00b7der", "Lied", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald nah, bald fern vor\u00fcber", "tokens": ["Bald", "nah", ",", "bald", "fern", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Manch freier Vogel zieht.", "tokens": ["Manch", "frei\u00b7er", "Vo\u00b7gel", "zieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie m\u00f6cht' es auch so gerne", "tokens": ["Wie", "m\u00f6cht'", "es", "auch", "so", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch blaue L\u00fcfte ziehn;", "tokens": ["Durch", "blau\u00b7e", "L\u00fcf\u00b7te", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie blickt es in die Ferne", "tokens": ["Wie", "blickt", "es", "in", "die", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach frischer W\u00e4lder Gr\u00fcn.", "tokens": ["Nach", "fri\u00b7scher", "W\u00e4l\u00b7der", "Gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie fr\u00f6hlich wollt' es loben", "tokens": ["Wie", "fr\u00f6h\u00b7lich", "wollt'", "es", "lo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit s\u00fc\u00dfem Sang und Klang", "tokens": ["Mit", "s\u00fc\u00b7\u00dfem", "Sang", "und", "Klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Herrn im Himmel droben", "tokens": ["Den", "Herrn", "im", "Him\u00b7mel", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein kurzes Leben lang. \u2013", "tokens": ["Sein", "kur\u00b7zes", "Le\u00b7ben", "lang", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Arm V\u00f6glein mag nicht singen,", "tokens": ["Arm", "V\u00f6\u00b7glein", "mag", "nicht", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist traurig und allein,", "tokens": ["Ist", "trau\u00b7rig", "und", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Thut auf und nieder springen", "tokens": ["Thut", "auf", "und", "nie\u00b7der", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "KON", "PTKVZ", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im engen Kerker sein.", "tokens": ["Im", "en\u00b7gen", "Ker\u00b7ker", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und singt es einmal leise", "tokens": ["Und", "singt", "es", "ein\u00b7mal", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit krankem Schn\u00e4belein,", "tokens": ["Mit", "kran\u00b7kem", "Schn\u00e4\u00b7be\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So klingt nur Trauerweise", "tokens": ["So", "klingt", "nur", "Trau\u00b7er\u00b7wei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tief aus dem Herzen sein. \u2013", "tokens": ["Tief", "aus", "dem", "Her\u00b7zen", "sein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VAINF", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Ach, V\u00f6glein, dein Verlangen,", "tokens": ["Ach", ",", "V\u00f6\u00b7glein", ",", "dein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das f\u00fchl' ich all mit dir;", "tokens": ["Das", "f\u00fchl'", "ich", "all", "mit", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Sehnen und dein Bangen", "tokens": ["Dein", "Seh\u00b7nen", "und", "dein", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Brennt auch im Busen mir.", "tokens": ["Brennt", "auch", "im", "Bu\u00b7sen", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Mich zieht nach S\u00fcdens Auen", "tokens": ["Mich", "zieht", "nach", "S\u00fc\u00b7dens", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie dich der Sehnsucht Schmerz:", "tokens": ["Wie", "dich", "der", "Sehn\u00b7sucht", "Schmerz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Bruder m\u00f6cht' ich schauen,", "tokens": ["Den", "Bru\u00b7der", "m\u00f6cht'", "ich", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihm sinken an das Herz.", "tokens": ["Ihm", "sin\u00b7ken", "an", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Mit ihm dann wollt' ich loben", "tokens": ["Mit", "ihm", "dann", "wollt'", "ich", "lo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In s\u00fc\u00dfem Sang und Klang", "tokens": ["In", "s\u00fc\u00b7\u00dfem", "Sang", "und", "Klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Herrn im Himmel droben", "tokens": ["Den", "Herrn", "im", "Him\u00b7mel", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein ganzes Leben lang.", "tokens": ["Mein", "gan\u00b7zes", "Le\u00b7ben", "lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}