{"textgrid.poem.28054": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist ein deutscher Reim? Deutsch kan hier iederman;", "tokens": ["Was", "ist", "ein", "deut\u00b7scher", "Reim", "?", "Deutsch", "kan", "hier", "ie\u00b7der\u00b7man", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$.", "NN", "VMFIN", "ADV", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum ist mir lieb, da\u00df ich kan auch, was ieder kan.", "tokens": ["Drum", "ist", "mir", "lieb", ",", "da\u00df", "ich", "kan", "auch", ",", "was", "ie\u00b7der", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "VMFIN", "ADV", "$,", "PRELS", "PIS", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch kan mein Reim noch was, das Zoilus nicht kan,", "tokens": ["Doch", "kan", "mein", "Reim", "noch", "was", ",", "das", "Zoi\u00b7lus", "nicht", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "ADV", "PIS", "$,", "PRELS", "NE", "PTKNEG", "VMFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Da\u00df meinen Reim, wie ihr, besticht nicht Iedermann.", "tokens": ["Da\u00df", "mei\u00b7nen", "Reim", ",", "wie", "ihr", ",", "be\u00b7sticht", "nicht", "Ie\u00b7der\u00b7mann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PWAV", "PPER", "$,", "VVFIN", "PTKNEG", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}