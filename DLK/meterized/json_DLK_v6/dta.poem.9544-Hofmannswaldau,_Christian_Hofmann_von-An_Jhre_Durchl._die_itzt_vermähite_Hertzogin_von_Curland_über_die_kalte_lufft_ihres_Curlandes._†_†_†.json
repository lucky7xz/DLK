{"dta.poem.9544": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Jhre Durchl. die itzt verm\u00e4hite Hertzogin  \n von Curland/ \u00fcber die kalte lufft ihres Curlandes.  \n \u2020 \u2020 \u2020", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht f\u00fcrchte dich/ Durchlauchtigste Princesse/", "tokens": ["Nicht", "f\u00fcrch\u00b7te", "dich", "/", "Durch\u00b7lauch\u00b7tigs\u00b7te", "Prin\u00b7ces\u00b7se", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$(", "ADJA", "NN", "$("], "meter": "-+--++--++-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vor Curlands kalten mitter-nacht;", "tokens": ["Vor", "Cur\u00b7lands", "kal\u00b7ten", "mit\u00b7ter\u00b7nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Printz/ der dich verehrt nach deiner gr\u00f6sse/", "tokens": ["Sein", "Printz", "/", "der", "dich", "ver\u00b7ehrt", "nach", "dei\u00b7ner", "gr\u00f6s\u00b7se", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist schon auff deine lust bedacht.", "tokens": ["Ist", "schon", "auff", "dei\u00b7ne", "lust", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein auge selbst mit seinen liebligkeiten", "tokens": ["Dein", "au\u00b7ge", "selbst", "mit", "sei\u00b7nen", "lieb\u00b7lig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wird sich das land nach deinem wunsch bereiten;", "tokens": ["Wird", "sich", "das", "land", "nach", "dei\u00b7nem", "wunsch", "be\u00b7rei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und kennst du nicht (die warheit mag hier schertzen)", "tokens": ["Und", "kennst", "du", "nicht", "(", "die", "war\u00b7heit", "mag", "hier", "schert\u00b7zen", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$(", "ART", "NN", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der kalten l\u00e4nder eigenschafft?", "tokens": ["Der", "kal\u00b7ten", "l\u00e4n\u00b7der", "ei\u00b7gen\u00b7schafft", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die liebe hat dort rechte krafft:", "tokens": ["Die", "lie\u00b7be", "hat", "dort", "rech\u00b7te", "krafft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Je k\u00e4lter dort die lufft/ je heisser sind die hertzen.", "tokens": ["Je", "k\u00e4l\u00b7ter", "dort", "die", "lufft", "/", "je", "heis\u00b7ser", "sind", "die", "hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ART", "NN", "$(", "ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}