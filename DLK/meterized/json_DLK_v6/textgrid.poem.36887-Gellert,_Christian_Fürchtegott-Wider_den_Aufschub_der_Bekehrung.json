{"textgrid.poem.36887": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Wider den Aufschub der Bekehrung", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du die Bu\u00dfe noch, die Gott gebeut, verschieben:", "tokens": ["Willst", "du", "die", "Bu\u00b7\u00dfe", "noch", ",", "die", "Gott", "ge\u00b7beut", ",", "ver\u00b7schie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "$,", "ART", "NN", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sch\u00e4ndest du sein Wort, und mu\u00dft dich selbst nicht lieben.", "tokens": ["So", "sch\u00e4n\u00b7dest", "du", "sein", "Wort", ",", "und", "mu\u00dft", "dich", "selbst", "nicht", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KON", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist deine Besserung nicht deiner Seele Gl\u00fcck?", "tokens": ["Ist", "dei\u00b7ne", "Bes\u00b7se\u00b7rung", "nicht", "dei\u00b7ner", "See\u00b7le", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wer verschiebt sein Heil gern einen Augenblick?", "tokens": ["Und", "wer", "ver\u00b7schiebt", "sein", "Heil", "gern", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPOSAT", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Allein wie schwer ist's nicht, sein eigen Herz bek\u00e4mpfen,", "tokens": ["Al\u00b7lein", "wie", "schwer", "ist's", "nicht", ",", "sein", "ei\u00b7gen", "Herz", "be\u00b7k\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADJD", "VAFIN", "PTKNEG", "$,", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Begierden widerstehn, und seine L\u00fcste d\u00e4mpfen?", "tokens": ["Be\u00b7gier\u00b7den", "wi\u00b7der\u00b7stehn", ",", "und", "sei\u00b7ne", "L\u00fcs\u00b7te", "d\u00e4mp\u00b7fen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja, S\u00fcnder, es ist schwer; allein zu deiner Ruh", "tokens": ["Ja", ",", "S\u00fcn\u00b7der", ",", "es", "ist", "schwer", ";", "al\u00b7lein", "zu", "dei\u00b7ner", "Ruh"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "$,", "PPER", "VAFIN", "ADJD", "$.", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist dies der einzge Weg. Und dem entsagest du?", "tokens": ["Ist", "dies", "der", "einz\u00b7ge", "Weg", ".", "Und", "dem", "ent\u00b7sa\u00b7gest", "du", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "ADJA", "NN", "$.", "KON", "ART", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ist deine Pflicht von Gott, wie kannst du sie vergessen?", "tokens": ["Ist", "dei\u00b7ne", "Pflicht", "von", "Gott", ",", "wie", "kannst", "du", "sie", "ver\u00b7ges\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "NN", "$,", "PWAV", "VMFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach deinen Kr\u00e4ften selbst hat er sie abgemessen.", "tokens": ["Nach", "dei\u00b7nen", "Kr\u00e4f\u00b7ten", "selbst", "hat", "er", "sie", "ab\u00b7ge\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Was weigerst du dich noch? Ist Gott denn ein Tyrann,", "tokens": ["Was", "wei\u00b7gerst", "du", "dich", "noch", "?", "Ist", "Gott", "denn", "ein", "Ty\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "$.", "VAFIN", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der mehr von mir verlangt, als ich ihm leisten kann?", "tokens": ["Der", "mehr", "von", "mir", "ver\u00b7langt", ",", "als", "ich", "ihm", "leis\u00b7ten", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sprich selbst: gewinnet Gott, wenn ich ihm kindlich diene,", "tokens": ["Sprich", "selbst", ":", "ge\u00b7win\u00b7net", "Gott", ",", "wenn", "ich", "ihm", "kind\u00b7lich", "die\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "VVFIN", "NN", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und, seiner wert zu sein, im Glauben mich erk\u00fchne?", "tokens": ["Und", ",", "sei\u00b7ner", "wert", "zu", "sein", ",", "im", "Glau\u00b7ben", "mich", "er\u00b7k\u00fch\u00b7ne", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPOSAT", "ADJD", "PTKZU", "VAINF", "$,", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn du die Tugend \u00fcbst, die Gott, dein Herr gebeut,", "tokens": ["Wenn", "du", "die", "Tu\u00b7gend", "\u00fcbst", ",", "die", "Gott", ",", "dein", "Herr", "ge\u00b7beut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "ART", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wem dienst du? Ringst du nicht nach deiner Seligkeit?", "tokens": ["Wem", "dienst", "du", "?", "Ringst", "du", "nicht", "nach", "dei\u00b7ner", "Se\u00b7lig\u00b7keit", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "NN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was weigerst du dich noch, das Laster zu verlassen?", "tokens": ["Was", "wei\u00b7gerst", "du", "dich", "noch", ",", "das", "Las\u00b7ter", "zu", "ver\u00b7las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil es dein Ungl\u00fcck ist, befiehlt es Gott zu hassen.", "tokens": ["Weil", "es", "dein", "Un\u00b7gl\u00fcck", "ist", ",", "be\u00b7fiehlt", "es", "Gott", "zu", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$,", "VVFIN", "PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was weigerst du dich noch, der Tugend Freund zu sein?", "tokens": ["Was", "wei\u00b7gerst", "du", "dich", "noch", ",", "der", "Tu\u00b7gend", "Freund", "zu", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "$,", "ART", "NN", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil sie dich gl\u00fccklich macht, befiehlt sie Gott allein.", "tokens": ["Weil", "sie", "dich", "gl\u00fcck\u00b7lich", "macht", ",", "be\u00b7fiehlt", "sie", "Gott", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$,", "VVFIN", "PPER", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Gott beut die Kraft dir an, das Gute zu vollbringen.", "tokens": ["Gott", "beut", "die", "Kraft", "dir", "an", ",", "das", "Gu\u00b7te", "zu", "voll\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Soll er durch Allmacht dich, ihm zu gehorchen, zwingen?", "tokens": ["Soll", "er", "durch", "All\u00b7macht", "dich", ",", "ihm", "zu", "ge\u00b7hor\u00b7chen", ",", "zwin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er gab dir die Vernunft; und du verleugnest sie?", "tokens": ["Er", "gab", "dir", "die", "Ver\u00b7nunft", ";", "und", "du", "ver\u00b7leug\u00b7nest", "sie", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$.", "KON", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er sendet dir sein Wort; und du gehorchst ihm nie?", "tokens": ["Er", "sen\u00b7det", "dir", "sein", "Wort", ";", "und", "du", "ge\u00b7horchst", "ihm", "nie", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Sprich nicht: Gott kennt mein Herz; ich hab es ihm verhei\u00dfen,", "tokens": ["Sprich", "nicht", ":", "Gott", "kennt", "mein", "Herz", ";", "ich", "hab", "es", "ihm", "ver\u00b7hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "$.", "NN", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich noch dereinst, mich bald vom Laster loszurei\u00dfen;", "tokens": ["Mich", "noch", "de\u00b7reinst", ",", "mich", "bald", "vom", "Las\u00b7ter", "los\u00b7zu\u00b7rei\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$,", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Itzt ist dies Werk zu schwer. Doch diese Schwierigkeit,", "tokens": ["Itzt", "ist", "dies", "Werk", "zu", "schwer", ".", "Doch", "die\u00b7se", "Schwie\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "NN", "PTKA", "ADJD", "$.", "KON", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die heute dich erschreckt, w\u00e4chst sie nicht durch die Zeit?", "tokens": ["Die", "heu\u00b7te", "dich", "er\u00b7schreckt", ",", "w\u00e4chst", "sie", "nicht", "durch", "die", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Je \u00f6fter du vollbringst, was Fleisch und Blut befohlen,", "tokens": ["Je", "\u00f6f\u00b7ter", "du", "voll\u00b7bringst", ",", "was", "Fleisch", "und", "Blut", "be\u00b7foh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,", "PWS", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je st\u00e4rker wird der Hang, die Tat zu wiederholen.", "tokens": ["Je", "st\u00e4r\u00b7ker", "wird", "der", "Hang", ",", "die", "Tat", "zu", "wie\u00b7der\u00b7ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Scheust du dich heute nicht, des H\u00f6chsten Feind zu sein:", "tokens": ["Scheust", "du", "dich", "heu\u00b7te", "nicht", ",", "des", "H\u00f6chs\u00b7ten", "Feind", "zu", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "$,", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Um wie viel weniger wirst du dich morgen scheun!", "tokens": ["Um", "wie", "viel", "we\u00b7ni\u00b7ger", "wirst", "du", "dich", "mor\u00b7gen", "scheun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "KOKOM", "ADV", "ADV", "VAFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Ist denn die Bu\u00df ein Werk von wenig Augenblicken?", "tokens": ["Ist", "denn", "die", "Bu\u00df", "ein", "Werk", "von", "we\u00b7nig", "Au\u00b7gen\u00b7bli\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kann dich kein schneller Tod der Welt noch heut entr\u00fccken?", "tokens": ["Kann", "dich", "kein", "schnel\u00b7ler", "Tod", "der", "Welt", "noch", "heut", "ent\u00b7r\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "ADJA", "NN", "ART", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist ein Geschrei zu Gott, ein Wunsch nach Besserung,", "tokens": ["Ist", "ein", "Ge\u00b7schrei", "zu", "Gott", ",", "ein", "Wunsch", "nach", "Bes\u00b7se\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.4": {"text": "Und Angst und Missetat, die wahre Heiligung?", "tokens": ["Und", "Angst", "und", "Mis\u00b7se\u00b7tat", ",", "die", "wah\u00b7re", "Hei\u00b7li\u00b7gung", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Ist's gnug zur Seligkeit, des Gl\u00fcckes der Erl\u00f6sten,", "tokens": ["Ist's", "gnug", "zur", "Se\u00b7lig\u00b7keit", ",", "des", "Gl\u00fc\u00b7ckes", "der", "Er\u00b7l\u00f6s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn uns der Tod ergreift, sich sicher zu getr\u00f6sten;", "tokens": ["Wenn", "uns", "der", "Tod", "er\u00b7greift", ",", "sich", "si\u00b7cher", "zu", "ge\u00b7tr\u00f6s\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist das Bekenntnis gnug, da\u00df uns die S\u00fcnde reut:", "tokens": ["Ist", "das", "Be\u00b7kennt\u00b7nis", "gnug", ",", "da\u00df", "uns", "die", "S\u00fcn\u00b7de", "reut", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So ist kein leichter Werk, als deine Seligkeit.", "tokens": ["So", "ist", "kein", "leich\u00b7ter", "Werk", ",", "als", "dei\u00b7ne", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Doch fordert Gott von uns die Reinigkeit der Seelen;", "tokens": ["Doch", "for\u00b7dert", "Gott", "von", "uns", "die", "Rei\u00b7nig\u00b7keit", "der", "See\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist keine Seele rein, der Glaub und Liebe fehlen;", "tokens": ["Ist", "kei\u00b7ne", "See\u00b7le", "rein", ",", "der", "Glaub", "und", "Lie\u00b7be", "feh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADJD", "$,", "ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist dieses