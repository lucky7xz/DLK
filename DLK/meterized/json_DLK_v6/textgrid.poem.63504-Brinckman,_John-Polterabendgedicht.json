{"textgrid.poem.63504": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "Polterabendgedicht", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nee, wenn een nah Berlin hen m\u00f6t", "tokens": ["Nee", ",", "wenn", "e\u00b7en", "nah", "Ber\u00b7lin", "hen", "m\u00f6t"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "ADJD", "NE", "NE", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "dor achter ut Land M\u00e4kelb\u00f6rg", "tokens": ["dor", "ach\u00b7ter", "ut", "Land", "M\u00e4\u00b7kel\u00b7b\u00f6rg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "un dat gor up sin eegen F\u00f6t,", "tokens": ["un", "dat", "gor", "up", "sin", "e\u00b7e\u00b7gen", "F\u00f6t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "wat w\u00f6hlt een denn f\u00f6r Sand doch d\u00f6rch!", "tokens": ["wat", "w\u00f6hlt", "e\u00b7en", "denn", "f\u00f6r", "Sand", "doch", "d\u00f6rch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Nee, wat mi dat f\u00f6r Sweet hett kost't!", "tokens": ["Nee", ",", "wat", "mi", "dat", "f\u00f6r", "Sweet", "hett", "kost't", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Tok\u00fcnftig f\u00fchr ick mit de Post.", "tokens": ["To\u00b7k\u00fcnf\u00b7tig", "f\u00fchr", "ick", "mit", "de", "Post", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn up de dumm oll Iserbahn", "tokens": ["Denn", "up", "de", "dumm", "oll", "I\u00b7ser\u00b7bahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "NE", "ADJD", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "dor kann'ck mi gor nich to verstahn.", "tokens": ["dor", "kann'ck", "mi", "gor", "nich", "to", "ver\u00b7stahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Na Gott si Dank! nu b\u00fcn ick dor", "tokens": ["Na", "Gott", "si", "Dank", "!", "nu", "b\u00fcn", "ick", "dor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "un, meen ick ok, to rechter Tit.", "tokens": ["un", ",", "meen", "ick", "ok", ",", "to", "rech\u00b7ter", "Tit", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "$,", "VVFIN", "PPER", "NE", "$,", "ADV", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Denn pultert ward doch h\u00fct, nich wohr?", "tokens": ["Denn", "pul\u00b7tert", "ward", "doch", "h\u00fct", ",", "nich", "wohr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ADV", "ADJD", "$,", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Un morrn, nich wohr? denn ward irst frigt.", "tokens": ["Un", "morrn", ",", "nich", "wohr", "?", "denn", "ward", "irst", "frigt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PTKNEG", "ADJD", "$.", "ADV", "VAFIN", "ADV", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "H\u00fct s\u00fcnd wi noch Mamsell, l\u00fctt Brut,", "tokens": ["H\u00fct", "s\u00fcnd", "wi", "noch", "Mam\u00b7sell", ",", "l\u00fctt", "Brut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "KOKOM", "ADV", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "man morrn denn s\u00fcht dat anners ut,", "tokens": ["man", "morrn", "denn", "s\u00fcht", "dat", "an\u00b7ners", "ut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "KON", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "denn ward se jo 'ne l\u00fctt Madam;", "tokens": ["denn", "ward", "se", "jo", "'ne", "l\u00fctt", "Ma\u00b7dam", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "denn is dat all'n annern Kram.", "tokens": ["denn", "is", "dat", "all'n", "an\u00b7nern", "Kram", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "ART", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dat Nest is doch all farrig makt", "tokens": ["Dat", "Nest", "is", "doch", "all", "far\u00b7rig", "makt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "ADV", "PIAT", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "recht n\u00fcdlich all un warm un still,", "tokens": ["recht", "n\u00fcd\u00b7lich", "all", "un", "warm", "un", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIAT", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wu Mann un Fru sick girn in strakt", "tokens": ["wu", "Mann", "un", "Fru", "sick", "girn", "in", "strakt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "un in de Eck sitt Arm in Arm,", "tokens": ["un", "in", "de", "Eck", "sitt", "Arm", "in", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "APPR", "NE", "NE", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "un all doch, wat to Hochtit h\u00fcrt,", "tokens": ["un", "all", "doch", ",", "wat", "to", "Hoch\u00b7tit", "h\u00fcrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "PIAT", "ADV", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "P\u00f6tt, K\u00e4tel, Schapens, Pann un B\u00fcrt,", "tokens": ["P\u00f6tt", ",", "K\u00e4\u00b7tel", ",", "Scha\u00b7pens", ",", "Pann", "un", "B\u00fcrt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "NN", "$,", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schapp, Stohl un Disch un Bedd un Weeg,", "tokens": ["Schapp", ",", "Stohl", "un", "Disch", "un", "Bedd", "un", "Weeg", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "FM", "FM", "FM", "FM", "FM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "as sick dat h\u00fcrt, all in de Reeg.", "tokens": ["as", "sick", "dat", "h\u00fcrt", ",", "all", "in", "de", "Reeg", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "PIAT", "APPR", "NE", "NE", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Na, na, dat reegt von s\u00fclst sick all.", "tokens": ["Na", ",", "na", ",", "dat", "reegt", "von", "s\u00fclst", "sick", "all", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$,", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "PIAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dat Gl\u00fcck dat blifft, de Leew de gifft.", "tokens": ["Dat", "Gl\u00fcck", "dat", "blifft", ",", "de", "Leew", "de", "gifft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "FM.da", "FM.da", "FM.da", "FM.da", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man wenn sick dat nich \u00e4nnern schall,", "tokens": ["Man", "wenn", "sick", "dat", "nich", "\u00e4n\u00b7nern", "schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOUS", "NE", "ART", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "is't Best, wenn't unverropen blifft.", "tokens": ["is't", "Best", ",", "wenn't", "un\u00b7ver\u00b7ro\u00b7pen", "blifft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "$,", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "De Leew hett Brut un Br\u00fcjam jo,", "tokens": ["De", "Leew", "hett", "Brut", "un", "Br\u00fc\u00b7jam", "jo", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "FM", "FM", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "man dor h\u00fcrt, weet ji, mihr noch to,", "tokens": ["man", "dor", "h\u00fcrt", ",", "weet", "ji", ",", "mihr", "noch", "to", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVFIN", "$,", "FM.la", "FM.la", "$,", "ADV", "ADV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "un dat, dat heww ick ut juch s\u00f6cht", "tokens": ["un", "dat", ",", "dat", "heww", "ick", "ut", "juch", "s\u00f6cht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "un in de Kiep hier mit juch br\u00f6cht.", "tokens": ["un", "in", "de", "Kiep", "hier", "mit", "juch", "br\u00f6cht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "APPR", "NE", "NE", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Dat's Solt un Brot. \u2013 Dat leewe Brot", "tokens": ["Dat's", "Solt", "un", "Brot", ".", "\u2013", "Dat", "lee\u00b7we", "Brot"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["NE", "VMFIN", "FM", "NN", "$.", "$(", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "dat m\u00f6t toirst in't Hus herin.", "tokens": ["dat", "m\u00f6t", "to\u00b7irst", "in't", "Hus", "he\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn Not dat is de Leew ehr Dod,", "tokens": ["Denn", "Not", "dat", "is", "de", "Leew", "ehr", "Dod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "FM", "FM", "FM", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "un wenn se s\u00fcnst ok hexen k\u00fcnn.", "tokens": ["un", "wenn", "se", "s\u00fcnst", "ok", "he\u00b7xen", "k\u00fcnn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Man trurig is dat Brot ahn Solt,", "tokens": ["Man", "tru\u00b7rig", "is", "dat", "Brot", "ahn", "Solt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "FM", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "un w\u00fcrrt ji hunnert Johr ok olt.", "tokens": ["un", "w\u00fcrrt", "ji", "hun\u00b7nert", "Johr", "ok", "olt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Man dumm is, wen dat nich versteiht", "tokens": ["Man", "dumm", "is", ",", "wen", "dat", "nich", "ver\u00b7steiht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "ADJD", "FM", "$,", "PWS", "ART", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "un sick dat s\u00fclst versolten deit.", "tokens": ["un", "sick", "dat", "s\u00fclst", "ver\u00b7sol\u00b7ten", "deit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Na, smuck l\u00fctt Brut, warr man nich rot!", "tokens": ["Na", ",", "smuck", "l\u00fctt", "Brut", ",", "warr", "man", "nich", "rot", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "FM", "FM", "FM", "$,", "PWAV", "PIS", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dat helpt nich, un wat k\u00fcmmt, dat gellt,", "tokens": ["Dat", "helpt", "nich", ",", "un", "wat", "k\u00fcmmt", ",", "dat", "gellt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "$,", "FM", "FM", "VVFIN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "un wat in't Blot liggt, liggt in't Blot,", "tokens": ["un", "wat", "in't", "Blot", "liggt", ",", "liggt", "in't", "Blot", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "NN", "VVFIN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "dat geiht nich anners in de Welt,", "tokens": ["dat", "geiht", "nich", "an\u00b7ners", "in", "de", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "ADV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "sit Adam up Eva ehr Geheet", "tokens": ["sit", "A\u00b7dam", "up", "E\u00b7va", "ehr", "Ge\u00b7heet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.6": {"text": "so n\u00fcrig in den Appel bet.", "tokens": ["so", "n\u00fc\u00b7rig", "in", "den", "Ap\u00b7pel", "bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.7": {"text": "Un kik, ut twee, dor warrn sacht dree,", "tokens": ["Un", "kik", ",", "ut", "twee", ",", "dor", "warrn", "sacht", "dree", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "FM", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "dat is dat Hochtitsabc.", "tokens": ["dat", "is", "dat", "Hoch\u00b7tits\u00b7abc", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.nl", "FM.nl", "FM.nl", "FM.nl", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}