{"textgrid.poem.60552": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dort auf dem Kirchhofkrcuzo sang", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort auf dem Kirchhofkrcuzo sang", "tokens": ["Dort", "auf", "dem", "Kirch\u00b7hof\u00b7krcu\u00b7zo", "sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Vogel einsam; aber bald", "tokens": ["Ein", "Vo\u00b7gel", "ein\u00b7sam", ";", "a\u00b7ber", "bald"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NE", "ADJD", "$.", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erhob er sich und schwang", "tokens": ["Er\u00b7hob", "er", "sich", "und", "schwang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zur\u00fcck sich in den gr\u00fcnen Wald.", "tokens": ["Zu\u00b7r\u00fcck", "sich", "in", "den", "gr\u00fc\u00b7nen", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie fr\u00fcher aus dem Fr\u00fchlingschor", "tokens": ["Wie", "fr\u00fc\u00b7her", "aus", "dem", "Fr\u00fch\u00b7ling\u00b7schor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schallt nun sein Lied so frei und wild;", "tokens": ["Schallt", "nun", "sein", "Lied", "so", "frei", "und", "wild", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein V\u00f6glein noch verlor", "tokens": ["Kein", "V\u00f6\u00b7glein", "noch", "ver\u00b7lor"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Stimm am lieben Kreuzesbild.", "tokens": ["Die", "Stimm", "am", "lie\u00b7ben", "Kreu\u00b7zes\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}