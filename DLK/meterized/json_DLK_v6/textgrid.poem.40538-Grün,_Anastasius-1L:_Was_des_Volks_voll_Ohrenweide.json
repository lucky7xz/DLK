{"textgrid.poem.40538": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was des Volks voll Ohrenweide", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was des Volks voll Ohrenweide", "tokens": ["Was", "des", "Volks", "voll", "Oh\u00b7ren\u00b7wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf Neapels Molo steht,", "tokens": ["Auf", "Nea\u00b7pels", "Mo\u00b7lo", "steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um den Mann im Narrenkleide,", "tokens": ["Um", "den", "Mann", "im", "Nar\u00b7ren\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Himmelw\u00e4rts sein Aug' verdreht!", "tokens": ["Him\u00b7mel\u00b7w\u00e4rts", "sein", "Aug'", "ver\u00b7dreht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie aus der Tritonen Schlunde", "tokens": ["Wie", "aus", "der", "Tri\u00b7to\u00b7nen", "Schlun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Dort am Marktplatz Well' auf Well',", "tokens": ["Dort", "am", "Markt\u00b7platz", "Well'", "auf", "Well'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprudelt aus verzerrtem Munde", "tokens": ["Spru\u00b7delt", "aus", "ver\u00b7zerr\u00b7tem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pl\u00e4tschernd ihm der Verse Quell:", "tokens": ["Pl\u00e4t\u00b7schernd", "ihm", "der", "Ver\u00b7se", "Quell", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Und wie Brunneneimer fangen", "tokens": ["Und", "wie", "Brun\u00b7ne\u00b7nei\u00b7mer", "fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine S\u00f6hne, Lazarus,", "tokens": ["Dei\u00b7ne", "S\u00f6h\u00b7ne", ",", "La\u00b7za\u00b7rus", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Ritter, Zaubrer, Schlangen,", "tokens": ["Sei\u00b7ne", "Rit\u00b7ter", ",", "Zaub\u00b7rer", ",", "Schlan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Feen und Drachen vollen Gu\u00df!", "tokens": ["Feen", "und", "Dra\u00b7chen", "vol\u00b7len", "Gu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch mein Herz, fast will's ihn neiden,", "tokens": ["Doch", "mein", "Herz", ",", "fast", "will's", "ihn", "nei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADV", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fc\u00dft ihn Bruder in Apoll!", "tokens": ["Gr\u00fc\u00dft", "ihn", "Bru\u00b7der", "in", "A\u00b7poll", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "NE", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Ist's Ein Quell nicht, der in Beiden,", "tokens": ["Ist's", "Ein", "Quell", "nicht", ",", "der", "in", "Bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKNEG", "$,", "PRELS", "APPR", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur verschiedne Bahnen quoll?", "tokens": ["Nur", "ver\u00b7schied\u00b7ne", "Bah\u00b7nen", "quoll", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie die Sch\u00f6nheit seiner Glieder", "tokens": ["Wie", "die", "Sch\u00f6n\u00b7heit", "sei\u00b7ner", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die Lumpen des Gewands,", "tokens": ["Durch", "die", "Lum\u00b7pen", "des", "Ge\u00b7wands", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So durch Fetzen seiner Lieder", "tokens": ["So", "durch", "Fet\u00b7zen", "sei\u00b7ner", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leuchtet hell des Gottes Glanz.", "tokens": ["Leuch\u00b7tet", "hell", "des", "Got\u00b7tes", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "W\u00e4hrend auf dem Polsterthrone", "tokens": ["W\u00e4h\u00b7rend", "auf", "dem", "Pols\u00b7ter\u00b7thro\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seines Munds Hanswurst sich dehnt,", "tokens": ["Sei\u00b7nes", "Munds", "Hans\u00b7wurst", "sich", "dehnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und als echter Lazarone", "tokens": ["Und", "als", "ech\u00b7ter", "La\u00b7za\u00b7ro\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Maccaronensold ersehnt;", "tokens": ["Mac\u00b7ca\u00b7ro\u00b7nen\u00b7sold", "er\u00b7sehnt", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Seh' ich um die Stirn' ihm rinnen", "tokens": ["Seh'", "ich", "um", "die", "Stirn'", "ihm", "rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jovis Wetterleuchten bald,", "tokens": ["Jo\u00b7vis", "Wet\u00b7ter\u00b7leuch\u00b7ten", "bald", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Blick' als Adler minnen", "tokens": ["Sei\u00b7ne", "Blick'", "als", "Ad\u00b7ler", "min\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KOUS", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem sch\u00f6nsten Lorbeerwald.", "tokens": ["Mit", "dem", "sch\u00f6ns\u00b7ten", "Lor\u00b7beer\u00b7wald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Voll von Helden, Wundern, Sagen", "tokens": ["Voll", "von", "Hel\u00b7den", ",", "Wun\u00b7dern", ",", "Sa\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADJD", "APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieht er rings die wei\u00dfe See", "tokens": ["Sieht", "er", "rings", "die", "wei\u00b7\u00dfe", "See"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gleich dem Buche aufgeschlagen", "tokens": ["Gleich", "dem", "Bu\u00b7che", "auf\u00b7ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einer Riesenepopee.", "tokens": ["Ei\u00b7ner", "Rie\u00b7se\u00b7ne\u00b7po\u00b7pee", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und des Golfs Gestade dehnen", "tokens": ["Und", "des", "Golfs", "Ge\u00b7sta\u00b7de", "deh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fcthenvoll sich um die Fluth", "tokens": ["Bl\u00fc\u00b7then\u00b7voll", "sich", "um", "die", "Fluth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein Kranz, der, es zu kr\u00f6nen,", "tokens": ["Wie", "ein", "Kranz", ",", "der", ",", "es", "zu", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf dem Buch des Meisters ruht.", "tokens": ["Auf", "dem", "Buch", "des", "Meis\u00b7ters", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Der Vesuv dort scheint ein Dichter,", "tokens": ["Der", "Ve\u00b7suv", "dort", "scheint", "ein", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Ganz von Christi Thr\u00e4n' ergl\u00fcht,", "tokens": ["Ganz", "von", "Chris\u00b7ti", "Thr\u00e4n'", "er\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem aus trunknem Mund ein lichter", "tokens": ["Dem", "aus", "trunk\u00b7nem", "Mund", "ein", "lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flammendithyrambus spr\u00fcht!", "tokens": ["Flam\u00b7men\u00b7dit\u00b7hy\u00b7ram\u00b7bus", "spr\u00fcht", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Lieder, Bilder, Reim' umklingen", "tokens": ["Lie\u00b7der", ",", "Bil\u00b7der", ",", "Reim'", "um\u00b7klin\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um und um dich, mein Poet,", "tokens": ["Um", "und", "um", "dich", ",", "mein", "Po\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "KON", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Brauchst vom Blatt nur abzusingen,", "tokens": ["Brauchst", "vom", "Blatt", "nur", "ab\u00b7zu\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADV", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was schon rings geschrieben steht.", "tokens": ["Was", "schon", "rings", "ge\u00b7schrie\u00b7ben", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Jedes spr\u00f6den Reimes Hallen", "tokens": ["Je\u00b7des", "spr\u00f6\u00b7den", "Rei\u00b7mes", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Macht des Meeres Rauschen gut:", "tokens": ["Macht", "des", "Mee\u00b7res", "Rau\u00b7schen", "gut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch auch Perlen, dir entfallen,", "tokens": ["Doch", "auch", "Per\u00b7len", ",", "dir", "ent\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schnell verschlingt sie, ach, die Fluth!", "tokens": ["Schnell", "ver\u00b7schlingt", "sie", ",", "ach", ",", "die", "Fluth", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "ITJ", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Lauschend h\u00e4lt dich Volk umfangen,", "tokens": ["Lau\u00b7schend", "h\u00e4lt", "dich", "Volk", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Elend in dem hohlen Blick,", "tokens": ["E\u00b7lend", "in", "dem", "hoh\u00b7len", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hungers Furchen in den Wangen,", "tokens": ["Hun\u00b7gers", "Fur\u00b7chen", "in", "den", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Last der Knechtschaft im Genick.", "tokens": ["Last", "der", "Knecht\u00b7schaft", "im", "Ge\u00b7nick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Um jed' Antlitz um die Wette", "tokens": ["Um", "jed'", "Ant\u00b7litz", "um", "die", "Wet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Breitet L\u00e4cheln jetzt sich aus,", "tokens": ["Brei\u00b7tet", "L\u00e4\u00b7cheln", "jetzt", "sich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das aus seinem Furchenbette", "tokens": ["Das", "aus", "sei\u00b7nem", "Fur\u00b7chen\u00b7bet\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selbst den Hunger wirft hinaus!", "tokens": ["Selbst", "den", "Hun\u00b7ger", "wirft", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "O wie gut die\u00df heil'ge L\u00e4cheln", "tokens": ["O", "wie", "gut", "die\u00df", "heil'\u00b7ge", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "PDS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem zerlumpten Bettler steht,", "tokens": ["Dem", "zer\u00b7lump\u00b7ten", "Bett\u00b7ler", "steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie vom Mast der Flagge F\u00e4cheln", "tokens": ["Wie", "vom", "Mast", "der", "Flag\u00b7ge", "F\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"