{"textgrid.poem.36643": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Siehe doch die Stadt der Gr\u00e4ber", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehe doch die Stadt der Gr\u00e4ber", "tokens": ["Sie\u00b7he", "doch", "die", "Stadt", "der", "Gr\u00e4\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In bacchantischer Entz\u00fcckung!", "tokens": ["In", "bac\u00b7chan\u00b7ti\u00b7scher", "Ent\u00b7z\u00fc\u00b7ckung", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rom verj\u00fcngt sich, Kindertage", "tokens": ["Rom", "ver\u00b7j\u00fcngt", "sich", ",", "Kin\u00b7der\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "PRF", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebt es wieder, und ich folgte", "tokens": ["Lebt", "es", "wie\u00b7der", ",", "und", "ich", "folg\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KON", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht dem Strome dieser Freude,", "tokens": ["Nicht", "dem", "Stro\u00b7me", "die\u00b7ser", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die in allen Stra\u00dfen w\u00fcthet,", "tokens": ["Die", "in", "al\u00b7len", "Stra\u00b7\u00dfen", "w\u00fct\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00fcrfe keinen Feuerbrand", "tokens": ["W\u00fcr\u00b7fe", "kei\u00b7nen", "Feu\u00b7er\u00b7brand"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In die allgemeine Flamme?", "tokens": ["In", "die", "all\u00b7ge\u00b7mei\u00b7ne", "Flam\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Einsam stehn die alten Tempel", "tokens": ["Ein\u00b7sam", "stehn", "die", "al\u00b7ten", "Tem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um den Palatin, verlassen", "tokens": ["Um", "den", "Pa\u00b7la\u00b7tin", ",", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KOUI", "ART", "NN", "$,", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Von dem m\u00e4chtigen Geschlechte,", "tokens": ["Von", "dem", "m\u00e4ch\u00b7ti\u00b7gen", "Ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das sie einst verehrt, verlassen", "tokens": ["Das", "sie", "einst", "ver\u00b7ehrt", ",", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "PPER", "ADV", "VVPP", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Von der Mitwelt selbst; dem Corso", "tokens": ["Von", "der", "Mit\u00b7welt", "selbst", ";", "dem", "Cor\u00b7so"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "$.", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00e4lzt aus dem Vulkan der Freude", "tokens": ["W\u00e4lzt", "aus", "dem", "Vul\u00b7kan", "der", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Sich die wilde Str\u00f6mung zu,", "tokens": ["Sich", "die", "wil\u00b7de", "Str\u00f6\u00b7mung", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Schwellend durch gedr\u00e4ngte Gassen.", "tokens": ["Schwel\u00b7lend", "durch", "ge\u00b7dr\u00e4ng\u00b7te", "Gas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Drum hinweg mit Ernst und Trauer,", "tokens": ["Drum", "hin\u00b7weg", "mit", "Ernst", "und", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APZR", "APPR", "NE", "KON", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Selbst den ehrbarsten Gedanken", "tokens": ["Selbst", "den", "ehr\u00b7bars\u00b7ten", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Nennt man heut' nur Grille; la\u00dft mich", "tokens": ["Nennt", "man", "heut'", "nur", "Gril\u00b7le", ";", "la\u00dft", "mich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "NN", "$.", "VVIMP", "PPER"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Frisch ins taumelnde Gewimmel,", "tokens": ["Frisch", "ins", "tau\u00b7meln\u00b7de", "Ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Frisch ins brausende Gewoge;", "tokens": ["Frisch", "ins", "brau\u00b7sen\u00b7de", "Ge\u00b7wo\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie man sonst der Narren lachte,", "tokens": ["Wie", "man", "sonst", "der", "Nar\u00b7ren", "lach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Lacht man heut' mit vollem Recht", "tokens": ["Lacht", "man", "heut'", "mit", "vol\u00b7lem", "Recht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Eines trockenen Verst\u00e4nd'gen!", "tokens": ["Ei\u00b7nes", "tro\u00b7cke\u00b7nen", "Ver\u00b7st\u00e4n\u00b7d'\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.4": {"line.1": {"text": "F\u00fcrchte nur, dich zu verlieren;", "tokens": ["F\u00fcrch\u00b7te", "nur", ",", "dich", "zu", "ver\u00b7lie\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie im Meer ein Regentropfen,", "tokens": ["Wie", "im", "Meer", "ein", "Re\u00b7gen\u00b7trop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So vergehst du hier, und keiner", "tokens": ["So", "ver\u00b7gehst", "du", "hier", ",", "und", "kei\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KON", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fragt nach deinem Rang und Wissen,", "tokens": ["Fragt", "nach", "dei\u00b7nem", "Rang", "und", "Wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aller Bande der Gewohnheit", "tokens": ["Al\u00b7ler", "Ban\u00b7de", "der", "Ge\u00b7wohn\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist der Mensch nun los, die Willk\u00fcr", "tokens": ["Ist", "der", "Mensch", "nun", "los", ",", "die", "Will\u00b7k\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADV", "PTKVZ", "$,", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Wird Gesetz, und l\u00fcstet dich's,", "tokens": ["Wird", "Ge\u00b7setz", ",", "und", "l\u00fcs\u00b7tet", "dich's", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "KON", "VVFIN", "PIS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kannst du auf dem Kopfe gehen.", "tokens": ["Kannst", "du", "auf", "dem", "Kop\u00b7fe", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Armuth gibt's nicht mehr und Reichthum.", "tokens": ["Ar\u00b7muth", "gibt's", "nicht", "mehr", "und", "Reicht\u00b7hum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "ADV", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine Maske deckt sie beide,", "tokens": ["Ei\u00b7ne", "Mas\u00b7ke", "deckt", "sie", "bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und geduldig nimmst du jeden,", "tokens": ["Und", "ge\u00b7dul\u00b7dig", "nimmst", "du", "je\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie er scheint; Gesicht und H\u00fclle,", "tokens": ["Wie", "er", "scheint", ";", "Ge\u00b7sicht", "und", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$.", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wort und die Geberde tauschen", "tokens": ["Wort", "und", "die", "Ge\u00b7ber\u00b7de", "tau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ART", "NN", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Die Geschlechter selbst, das Alter", "tokens": ["Die", "Ge\u00b7schlech\u00b7ter", "selbst", ",", "das", "Al\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "L\u00e4chelt dich in Locken an,", "tokens": ["L\u00e4\u00b7chelt", "dich", "in", "Lo\u00b7cken", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Jugend geht an Kr\u00fccken.", "tokens": ["Und", "die", "Ju\u00b7gend", "geht", "an", "Kr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Was die Welt im Ernst getrieben,", "tokens": ["Was", "die", "Welt", "im", "Ernst", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und was Geist und Hand besch\u00e4ftigt,", "tokens": ["Und", "was", "Geist", "und", "Hand", "be\u00b7sch\u00e4f\u00b7tigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur zum Scheine, nur zum Scherze", "tokens": ["Nur", "zum", "Schei\u00b7ne", ",", "nur", "zum", "Scher\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4gt man Alles dir vor Augen,", "tokens": ["Tr\u00e4gt", "man", "Al\u00b7les", "dir", "vor", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PIS", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hier der G\u00e4rtner seine Blumen,", "tokens": ["Hier", "der", "G\u00e4rt\u00b7ner", "sei\u00b7ne", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der Gelehrte seine B\u00fccher,", "tokens": ["Der", "Ge\u00b7lehr\u00b7te", "sei\u00b7ne", "B\u00fc\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Seine Medicin der Arzt,", "tokens": ["Sei\u00b7ne", "Me\u00b7di\u00b7cin", "der", "Arzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und der Landmann seine Fr\u00fcchte.", "tokens": ["Und", "der", "Land\u00b7mann", "sei\u00b7ne", "Fr\u00fcch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aus der Erde fernsten Strecken", "tokens": ["Aus", "der", "Er\u00b7de", "ferns\u00b7ten", "Stre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommen bunte V\u00f6lkertrachten,", "tokens": ["Kom\u00b7men", "bun\u00b7te", "V\u00f6l\u00b7ker\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mahomskinder, Mohrenprinzen,", "tokens": ["Ma\u00b7homs\u00b7kin\u00b7der", ",", "Moh\u00b7ren\u00b7prin\u00b7zen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aethiopische Gesichter,", "tokens": ["A\u00b7e\u00b7thio\u00b7pi\u00b7sche", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und um ganz dich zu verwirren,", "tokens": ["Und", "um", "ganz", "dich", "zu", "ver\u00b7wir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "ADV", "PPER", "PTKZU", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Schickt das Reich der Fabel Gnomen;", "tokens": ["Schickt", "das", "Reich", "der", "Fa\u00b7bel", "Gno\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Widerstehe, wenn du kannst,", "tokens": ["Wi\u00b7der\u00b7ste\u00b7he", ",", "wenn", "du", "kannst", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Allerliebsten jungen Feen.", "tokens": ["Al\u00b7ler\u00b7liebs\u00b7ten", "jun\u00b7gen", "Feen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Von den fliehenden Gestalten", "tokens": ["Von", "den", "flie\u00b7hen\u00b7den", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gl\u00fcckt es keine dir zu fesseln;", "tokens": ["Gl\u00fcckt", "es", "kei\u00b7ne", "dir", "zu", "fes\u00b7seln", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese m\u00f6chtest du verfolgen,", "tokens": ["Die\u00b7se", "m\u00f6ch\u00b7test", "du", "ver\u00b7fol\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jene lockt dich an. Vergebens!", "tokens": ["Je\u00b7ne", "lockt", "dich", "an", ".", "Ver\u00b7ge\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wesenlose Schattenbilder,", "tokens": ["We\u00b7sen\u00b7lo\u00b7se", "Schat\u00b7ten\u00b7bil\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwinden sie hinweg, geh\u00f6ren", "tokens": ["Schwin\u00b7den", "sie", "hin\u00b7weg", ",", "ge\u00b7h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "PTKVZ", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur sich selber an, und du", "tokens": ["Nur", "sich", "sel\u00b7ber", "an", ",", "und", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PRF", "ADV", "PTKVZ", "$,", "KON", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Bist allein zur\u00fcckgeblieben.", "tokens": ["Bist", "al\u00b7lein", "zu\u00b7r\u00fcck\u00b7ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und des eignen Lebens denk' ich,", "tokens": ["Und", "des", "eig\u00b7nen", "Le\u00b7bens", "denk'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jener Zeit, da ihre Bilder", "tokens": ["Je\u00b7ner", "Zeit", ",", "da", "ih\u00b7re", "Bil\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir die Welt, und seine Tiefen", "tokens": ["Mir", "die", "Welt", ",", "und", "sei\u00b7ne", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das Gem\u00fcth, da mir die Menschheit", "tokens": ["Das", "Ge\u00b7m\u00fcth", ",", "da", "mir", "die", "Menschheit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Ihre Thaten aufgeschlossen,", "tokens": ["Ih\u00b7re", "Tha\u00b7ten", "auf\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da vom Reiche der Lebend'gen", "tokens": ["Da", "vom", "Rei\u00b7che", "der", "Le\u00b7ben\u00b7d'\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NE", "ART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "So viel herrliches sich stolz", "tokens": ["So", "viel", "herr\u00b7li\u00b7ches", "sich", "stolz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "PRF", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Im Gem\u00fcthe mir gesammelt.", "tokens": ["Im", "Ge\u00b7m\u00fc\u00b7the", "mir", "ge\u00b7sam\u00b7melt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Da der Mensch und alle Dinge", "tokens": ["Da", "der", "Mensch", "und", "al\u00b7le", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "KON", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So phantastisch noch im Dufte", "tokens": ["So", "phan\u00b7tas\u00b7tisch", "noch", "im", "Duf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Mir erschienen, da sie alle", "tokens": ["Mir", "er\u00b7schie\u00b7nen", ",", "da", "sie", "al\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "KOUS", "PPER", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch sich glichen, da die Masken", "tokens": ["Noch", "sich", "gli\u00b7chen", ",", "da", "die", "Mas\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PRF", "VVINF", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mich get\u00e4uscht, da ich nach allen", "tokens": ["Mich", "ge\u00b7t\u00e4uscht", ",", "da", "ich", "nach", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVPP", "$,", "KOUS", "PPER", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit verme\u00dfnem Wahn gegriffen,", "tokens": ["Mit", "ver\u00b7me\u00df\u00b7nem", "Wahn", "ge\u00b7grif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und von tausenden mir nichts", "tokens": ["Und", "von", "tau\u00b7sen\u00b7den", "mir", "nichts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "PPER", "PIS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Als mein eignes Selbst geblieben.", "tokens": ["Als", "mein", "eig\u00b7nes", "Selbst", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}