{"textgrid.poem.41807": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hei\u00dft ", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hei\u00dft ", "tokens": ["Hei\u00dft"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Weil wir ein h\u00f6chstes Ziel doch nie erreichen,", "tokens": ["Weil", "wir", "ein", "h\u00f6chs\u00b7tes", "Ziel", "doch", "nie", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die M\u00fch' zu scheuen, zu ihm aufzusteigen", "tokens": ["Die", "M\u00fch'", "zu", "scheu\u00b7en", ",", "zu", "ihm", "auf\u00b7zu\u00b7stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf steilem Pfad umringt von Warnungsmalen?", "tokens": ["Auf", "stei\u00b7lem", "Pfad", "um\u00b7ringt", "von", "War\u00b7nungs\u00b7ma\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Sonne winkt mit ihren goldnen Strahlen", "tokens": ["Die", "Son\u00b7ne", "winkt", "mit", "ih\u00b7ren", "gold\u00b7nen", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur H\u00f6he wo die Alltagsnebel weichen \u2013", "tokens": ["Zur", "H\u00f6\u00b7he", "wo", "die", "All\u00b7tags\u00b7ne\u00b7bel", "wei\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PWAV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dort wird die Welt im andern Licht sich zeigen", "tokens": ["Dort", "wird", "die", "Welt", "im", "an\u00b7dern", "Licht", "sich", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hei\u00dft Deine Losung: ", "tokens": ["Hei\u00dft", "Dei\u00b7ne", "Lo\u00b7sung", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Ist sie Dir fest und treu ins Herz geschrieben,", "tokens": ["Ist", "sie", "Dir", "fest", "und", "treu", "ins", "Herz", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKVZ", "KON", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So la\u00df nicht ab vom mut'gen Aufw\u00e4rtsstreben,", "tokens": ["So", "la\u00df", "nicht", "ab", "vom", "mut'\u00b7gen", "Auf\u00b7w\u00e4rts\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PTKNEG", "PTKVZ", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bist Du auch noch so fern vom Ziel geblieben!", "tokens": ["Bist", "Du", "auch", "noch", "so", "fern", "vom", "Ziel", "ge\u00b7blie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Versuche denn, von Menschenlieb getrieben,", "tokens": ["Ver\u00b7su\u00b7che", "denn", ",", "von", "Men\u00b7schen\u00b7lieb", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}