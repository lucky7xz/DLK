{"dta.poem.2761": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein anders.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Best\u00e4ndigkeit/ wenn alles bricht/", "tokens": ["Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "/", "wenn", "al\u00b7les", "bricht", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hemmt doch getreue Liebe nicht/", "tokens": ["Hemmt", "doch", "ge\u00b7treu\u00b7e", "Lie\u00b7be", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "PTKNEG", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sie schenckt zu letzte Kronen.", "tokens": ["Sie", "schenckt", "zu", "letz\u00b7te", "Kro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer Tugend zu dem Grundstein legt/", "tokens": ["Wer", "Tu\u00b7gend", "zu", "dem", "Grund\u00b7stein", "legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den will sie auch belohnen/", "tokens": ["Den", "will", "sie", "auch", "be\u00b7loh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die bleibt mir in das Hertz gepr\u00e4gt.", "tokens": ["Die", "bleibt", "mir", "in", "das", "Hertz", "ge\u00b7pr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die ists/ die eintzig mich erfreut/", "tokens": ["Die", "ists", "/", "die", "eint\u00b7zig", "mich", "er\u00b7freut", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$(", "ART", "ADJD", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Best\u00e4ndigkeit.", "tokens": ["Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Best\u00e4ndigkeit/ ach sch\u00f6ner Sieg!", "tokens": ["Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "/", "ach", "sch\u00f6\u00b7ner", "Sieg", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "XY", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wann der Begierden Kampff und Krieg", "tokens": ["Wann", "der", "Be\u00b7gier\u00b7den", "Kampff", "und", "Krieg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Mu\u00df g\u00e4ntzlich sich ergeben!", "tokens": ["Mu\u00df", "g\u00e4ntz\u00b7lich", "sich", "er\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du Herrscherin bezwingst den Tod/", "tokens": ["Du", "Herr\u00b7sche\u00b7rin", "be\u00b7zwingst", "den", "Tod", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wenn schon unser Leben", "tokens": ["Und", "wenn", "schon", "un\u00b7ser", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ger\u00e4th in Kummer/ Angst und Noth/", "tokens": ["Ge\u00b7r\u00e4\u00b7th", "in", "Kum\u00b7mer", "/", "Angst", "und", "Noth", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Stellst du doch uns zu der Seit/", "tokens": ["Stellst", "du", "doch", "uns", "zu", "der", "Seit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Best\u00e4ndigkeit.", "tokens": ["Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Best\u00e4ndigkeit/ vertrauter Schatz/", "tokens": ["Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "/", "ver\u00b7trau\u00b7ter", "Schatz", "/"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Soll stets in meiner Seele Platz/", "tokens": ["Soll", "stets", "in", "mei\u00b7ner", "See\u00b7le", "Platz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Oberhand behalten.", "tokens": ["Und", "O\u00b7ber\u00b7hand", "be\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Himmel/ der sonst alles f\u00fcgt/", "tokens": ["Den", "Him\u00b7mel", "/", "der", "sonst", "al\u00b7les", "f\u00fcgt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADV", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den la\u00df ich dr\u00fcber walten.", "tokens": ["Den", "la\u00df", "ich", "dr\u00fc\u00b7ber", "wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVIMP", "PPER", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nichts hat mich auf der Welt vergn\u00fcgt/", "tokens": ["Nichts", "hat", "mich", "auf", "der", "Welt", "ver\u00b7gn\u00fcgt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als du/ du Hoffnung meiner Zeit/", "tokens": ["Als", "du", "/", "du", "Hoff\u00b7nung", "mei\u00b7ner", "Zeit", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "PPER", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Best\u00e4ndigkeit/", "tokens": ["Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Best\u00e4ndigkeit/ die macht mich fro/", "tokens": ["Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "/", "die", "macht", "mich", "fro", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun ich dich/ werthste/ sehe so", "tokens": ["Nun", "ich", "dich", "/", "werths\u00b7te", "/", "se\u00b7he", "so"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "PRF", "$(", "VVFIN", "$(", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dergleichen Kleinod tragen.", "tokens": ["Derg\u00b7lei\u00b7chen", "Klei\u00b7nod", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was eintzig unsre Liebe ziert/", "tokens": ["Was", "eint\u00b7zig", "uns\u00b7re", "Lie\u00b7be", "ziert", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Davon der Neid mu\u00df sagen/", "tokens": ["Da\u00b7von", "der", "Neid", "mu\u00df", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist da\u00df sie Sieges-Palmen f\u00fchrt/", "tokens": ["Ist", "da\u00df", "sie", "Sie\u00b7ges\u00b7Pal\u00b7men", "f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOUS", "PPER", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die durch viel Proben eingeweyht/", "tokens": ["Die", "durch", "viel", "Pro\u00b7ben", "ein\u00b7ge\u00b7weyht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Best\u00e4ndigkeit.", "tokens": ["Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Best\u00e4ndigkeit/ mein h\u00f6chstes Gut", "tokens": ["Be\u00b7st\u00e4n\u00b7dig\u00b