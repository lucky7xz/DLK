{"textgrid.poem.64316": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "36. Das R\u00fcsthaus", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Herz im Leibe thut mir weh,", "tokens": ["Das", "Herz", "im", "Lei\u00b7be", "thut", "mir", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ich der V\u00e4ter R\u00fcstung seh;", "tokens": ["Wenn", "ich", "der", "V\u00e4\u00b7ter", "R\u00fcs\u00b7tung", "seh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich seh zugleich mit nassem Blick", "tokens": ["Ich", "seh", "zu\u00b7gleich", "mit", "nas\u00b7sem", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In unsrer V\u00e4ter Zeit zur\u00fcck!", "tokens": ["In", "uns\u00b7rer", "V\u00e4\u00b7ter", "Zeit", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich greife gleich nach Schwert und Speer,", "tokens": ["Ich", "grei\u00b7fe", "gleich", "nach", "Schwert", "und", "Speer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch Speer und Schwert sind mir zu schwer!", "tokens": ["Doch", "Speer", "und", "Schwert", "sind", "mir", "zu", "schwer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich lege traurig ungespannt", "tokens": ["Ich", "le\u00b7ge", "trau\u00b7rig", "un\u00b7ge\u00b7spannt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Bogen aus der schwachen Hand.", "tokens": ["Den", "Bo\u00b7gen", "aus", "der", "schwa\u00b7chen", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Des Panzers und des Helmes Last,", "tokens": ["Des", "Pan\u00b7zers", "und", "des", "Hel\u00b7mes", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schild, den ihre Hand gefa\u00dft,", "tokens": ["Der", "Schild", ",", "den", "ih\u00b7re", "Hand", "ge\u00b7fa\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des breiten Beiles langer Schaft", "tokens": ["Des", "brei\u00b7ten", "Bei\u00b7les", "lan\u00b7ger", "Schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zeugt von der V\u00e4ter Riesen-Kraft!", "tokens": ["Zeugt", "von", "der", "V\u00e4\u00b7ter", "Rie\u00b7sen\u00b7Kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Geschwenkt von eines Helden Arm", "tokens": ["Ge\u00b7schwenkt", "von", "ei\u00b7nes", "Hel\u00b7den", "Arm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat dieses Panner manchen Schwarm", "tokens": ["Hat", "die\u00b7ses", "Pan\u00b7ner", "man\u00b7chen", "Schwarm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDAT", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der stolzen Feind', in mancher Schlacht", "tokens": ["Der", "stol\u00b7zen", "Feind'", ",", "in", "man\u00b7cher", "Schlacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie scheues Wildpret weggejagt.", "tokens": ["Wie", "scheu\u00b7es", "Wild\u00b7pret", "weg\u00b7ge\u00b7jagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie flohn und warfen aus der Faust", "tokens": ["Sie", "flohn", "und", "war\u00b7fen", "aus", "der", "Faust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fahnen, vom Gew\u00fchl zerzaust;", "tokens": ["Die", "Fah\u00b7nen", ",", "vom", "Ge\u00b7w\u00fchl", "zer\u00b7zaust", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die sammelte des Siegers Hand", "tokens": ["Die", "sam\u00b7mel\u00b7te", "des", "Sie\u00b7gers", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Und hing sie auf an diese Wand.", "tokens": ["Und", "hing", "sie", "auf", "an", "die\u00b7se", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Viel andre Beute h\u00e4nget noch", "tokens": ["Viel", "and\u00b7re", "Beu\u00b7te", "h\u00e4n\u00b7get", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und zeugt vom abgeworfnen Joch,", "tokens": ["Und", "zeugt", "vom", "ab\u00b7ge\u00b7worf\u00b7nen", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von der Burgunder Heeres- Macht", "tokens": ["Von", "der", "Bur\u00b7gun\u00b7der", "Hee\u00b7res", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "TRUNC", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00dcbermut und eitler Pracht!", "tokens": ["Und", "\u00dc\u00b7ber\u00b7mut", "und", "eit\u00b7ler", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Mit diesen Stricken wollten sie", "tokens": ["Mit", "die\u00b7sen", "Stri\u00b7cken", "woll\u00b7ten", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schweizer H\u00e4nde binden fr\u00fch,", "tokens": ["Der", "Schwei\u00b7zer", "H\u00e4n\u00b7de", "bin\u00b7den", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und eh' die Sonne sank ins Thal", "tokens": ["Und", "eh'", "die", "Son\u00b7ne", "sank", "ins", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beschien sie noch der Stolzen Fall!", "tokens": ["Be\u00b7schien", "sie", "noch", "der", "Stol\u00b7zen", "Fall", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "So focht der V\u00e4ter Helden-Mut,", "tokens": ["So", "focht", "der", "V\u00e4\u00b7ter", "Hel\u00b7den\u00b7Mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es flo\u00df f\u00fcr uns ihr teures Blut!", "tokens": ["Es", "flo\u00df", "f\u00fcr", "uns", "ihr", "teu\u00b7res", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sind des Enkel-Dankes wert,", "tokens": ["Sie", "sind", "des", "En\u00b7kel\u00b7Dan\u00b7kes", "wert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl dem, der sie durch Thaten ehrt!", "tokens": ["Wohl", "dem", ",", "der", "sie", "durch", "Tha\u00b7ten", "ehrt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}