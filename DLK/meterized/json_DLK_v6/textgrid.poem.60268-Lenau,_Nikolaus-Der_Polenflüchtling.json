{"textgrid.poem.60268": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der Polenfl\u00fcchtling", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.85", "ca:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im quellenarmen W\u00fcstenland", "tokens": ["Im", "quel\u00b7len\u00b7ar\u00b7men", "W\u00fcs\u00b7ten\u00b7land"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Arabischer Nomaden", "tokens": ["A\u00b7ra\u00b7bi\u00b7scher", "No\u00b7ma\u00b7den"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Irrt, ohne Ziel und Vaterland,", "tokens": ["Irrt", ",", "oh\u00b7ne", "Ziel", "und", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf windverwehten Pfaden", "tokens": ["Auf", "wind\u00b7ver\u00b7weh\u00b7ten", "Pfa\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Polenheld und grollet still,", "tokens": ["Ein", "Po\u00b7le\u00b7nheld", "und", "grol\u00b7let", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df noch sein Herz nicht brechen will.", "tokens": ["Da\u00df", "noch", "sein", "Herz", "nicht", "bre\u00b7chen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Sonn auf ihn herunterspr\u00fcht", "tokens": ["Die", "Sonn", "auf", "ihn", "her\u00b7un\u00b7ter\u00b7spr\u00fcht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die hei\u00dfen Mittagsbr\u00e4nde,", "tokens": ["Die", "hei\u00b7\u00dfen", "Mit\u00b7tags\u00b7br\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von ihrem Flammenkusse gl\u00fcht", "tokens": ["Von", "ih\u00b7rem", "Flam\u00b7men\u00b7kus\u00b7se", "gl\u00fcht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Schwert an seiner Lende.", "tokens": ["Das", "Schwert", "an", "sei\u00b7ner", "Len\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Will wecken ihm den tapfern Stahl", "tokens": ["Will", "we\u00b7cken", "ihm", "den", "tap\u00b7fern", "Stahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zur Racheglut der Sonnenstrahl?", "tokens": ["Zur", "Ra\u00b7che\u00b7glut", "der", "Son\u00b7nen\u00b7strahl", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sein Leib neigt sich dem Boden zu", "tokens": ["Sein", "Leib", "neigt", "sich", "dem", "Bo\u00b7den", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ART", "NN", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit d\u00fcrstendem Ermatten;", "tokens": ["Mit", "d\u00fcrs\u00b7ten\u00b7dem", "Er\u00b7mat\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der s\u00e4nke gern zu k\u00fchler Ruh", "tokens": ["Der", "s\u00e4n\u00b7ke", "gern", "zu", "k\u00fch\u00b7ler", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In seinen eignen Schatten,", "tokens": ["In", "sei\u00b7nen", "eig\u00b7nen", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der tr\u00e4nke gern vor d\u00fcrrer Glut", "tokens": ["Der", "tr\u00e4n\u00b7ke", "gern", "vor", "d\u00fcr\u00b7rer", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schier seine eigne Tr\u00e4nenflut.", "tokens": ["Schier", "sei\u00b7ne", "eig\u00b7ne", "Tr\u00e4\u00b7nen\u00b7flut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch solche Qual sein ", "tokens": ["Doch", "sol\u00b7che", "Qual", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Weils tr\u00e4gt ein tiefers Kr\u00e4nken.", "tokens": ["Weils", "tr\u00e4gt", "ein", "tie\u00b7fers", "Kr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er schreitet fort, von Schmerz gest\u00e4rkt,", "tokens": ["Er", "schrei\u00b7tet", "fort", ",", "von", "Schmerz", "ge\u00b7st\u00e4rkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Schlachtenangedenken.", "tokens": ["Vom", "Schlach\u00b7ten\u00b7an\u00b7ge\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Manchmal sein Mund Kosziusko! ruft,", "tokens": ["Manch\u00b7mal", "sein", "Mund", "Kos\u00b7zius\u00b7ko", "!", "ruft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NE", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und tr\u00e4umend haut er in die Luft.", "tokens": ["Und", "tr\u00e4u\u00b7mend", "haut", "er", "in", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Als nun der Abend K\u00fchlung bringt,", "tokens": ["Als", "nun", "der", "A\u00b7bend", "K\u00fch\u00b7lung", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steht er an gr\u00fcner Stelle;", "tokens": ["Steht", "er", "an", "gr\u00fc\u00b7ner", "Stel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Ein s\u00fc\u00dfes Lied des Mitleids singt", "tokens": ["Ein", "s\u00fc\u00b7\u00dfes", "Lied", "des", "Mit\u00b7leids", "singt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entgegen ihm die Quelle,", "tokens": ["Ent\u00b7ge\u00b7gen", "ihm", "die", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und s\u00e4uselnd weht das Gras ihn an:", "tokens": ["Und", "s\u00e4u\u00b7selnd", "weht", "das", "Gras", "ihn", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O schlummre hier, du armer Mann!", "tokens": ["O", "schlumm\u00b7re", "hier", ",", "du", "ar\u00b7mer", "Mann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er sinkt, er schl\u00e4ft. Der fremde Baum", "tokens": ["Er", "sinkt", ",", "er", "schl\u00e4ft", ".", "Der", "frem\u00b7de", "Baum"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Einfl\u00fcstert ihn gelinde", "tokens": ["Ein\u00b7fl\u00fcs\u00b7tert", "ihn", "ge\u00b7lin\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In einen sch\u00f6nen Heldentraum;", "tokens": ["In", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Hel\u00b7den\u00b7traum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wellen und die Winde", "tokens": ["Die", "Wel\u00b7len", "und", "die", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Umrauschen ihn wie Schlachtengang,", "tokens": ["Um\u00b7rau\u00b7schen", "ihn", "wie", "Schlach\u00b7ten\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Umrauschen ihn wie Siegsgesang.", "tokens": ["Um\u00b7rau\u00b7schen", "ihn", "wie", "Siegs\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dort kommt im Osten voll und klar", "tokens": ["Dort", "kommt", "im", "Os\u00b7ten", "voll", "und", "klar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herauf des Mondes Schimmern;", "tokens": ["Her\u00b7auf", "des", "Mon\u00b7des", "Schim\u00b7mern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von einer Beduinenschar", "tokens": ["Von", "ei\u00b7ner", "Be\u00b7du\u00b7i\u00b7nen\u00b7schar"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die blanken S\u00e4bel flimmern", "tokens": ["Die", "blan\u00b7ken", "S\u00e4\u00b7bel", "flim\u00b7mern"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Weithin im \u00f6den Mondrevier,", "tokens": ["Weit\u00b7hin", "im", "\u00f6\u00b7den", "Mon\u00b7dre\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Wildnis n\u00e4chtlich helle Zier.", "tokens": ["Der", "Wild\u00b7nis", "n\u00e4cht\u00b7lich", "hel\u00b7le", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Stets lauter t\u00f6nt der Hufentanz", "tokens": ["Stets", "lau\u00b7ter", "t\u00f6nt", "der", "Hu\u00b7fen\u00b7tanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von windverwandten Fliehern,", "tokens": ["Von", "wind\u00b7ver\u00b7wand\u00b7ten", "Flie\u00b7hern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die hei\u00dfgejagt im Mondenglanz", "tokens": ["Die", "hei\u00df\u00b7ge\u00b7jagt", "im", "Mon\u00b7den\u00b7glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Quell entgegenwiehern.", "tokens": ["Dem", "Quell", "ent\u00b7ge\u00b7gen\u00b7wie\u00b7hern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Reiter rufen in die Nacht;", "tokens": ["Die", "Rei\u00b7ter", "ru\u00b7fen", "in", "die", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch nicht der Polenheld erwacht.", "tokens": ["Doch", "nicht", "der", "Po\u00b7le\u00b7nheld", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Sie lassen, frisch und froh gelaunt,", "tokens": ["Sie", "las\u00b7sen", ",", "frisch", "und", "froh", "ge\u00b7launt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ross' im Quelle trinken,", "tokens": ["Die", "Ross'", "im", "Quel\u00b7le", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und pl\u00f6tzlich schauen sie erstaunt", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "schau\u00b7en", "sie", "er\u00b7staunt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Schwert im Grase blinken,", "tokens": ["Ein", "Schwert", "im", "Gra\u00b7se", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und zitternd spielt das k\u00fchle Licht", "tokens": ["Und", "zit\u00b7ternd", "spielt", "das", "k\u00fch\u00b7le", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf einem bleichen Angesicht.", "tokens": ["Auf", "ei\u00b7nem", "blei\u00b7chen", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Sie lagern um den Fremden stumm,", "tokens": ["Sie", "la\u00b7gern", "um", "den", "Frem\u00b7den", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn aufzuwecken bange;", "tokens": ["Ihn", "auf\u00b7zu\u00b7we\u00b7cken", "ban\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie sehn der Narben Heiligtum", "tokens": ["Sie", "sehn", "der", "Nar\u00b7ben", "Hei\u00b7lig\u00b7tum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf blasser Stirn und Wange;", "tokens": ["Auf", "blas\u00b7ser", "Stirn", "und", "Wan\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem W\u00fcstensohn zu Herzen geht", "tokens": ["Dem", "W\u00fcs\u00b7ten\u00b7sohn", "zu", "Her\u00b7zen", "geht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Ungl\u00fccks stille Majest\u00e4t.", "tokens": ["Des", "Un\u00b7gl\u00fccks", "stil\u00b7le", "Ma\u00b7jes\u00b7t\u00e4t", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "ia