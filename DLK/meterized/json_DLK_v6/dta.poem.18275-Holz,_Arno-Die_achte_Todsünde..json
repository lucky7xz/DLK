{"dta.poem.18275": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Die achte Tods\u00fcnde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.85", "en:0.14"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Uns w\u00fcthend, darf uns rasend machen,", "tokens": ["Uns", "w\u00fct\u00b7hend", ",", "darf", "uns", "ra\u00b7send", "ma\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir stecken's schlie\u00dflich ruhig ein,", "tokens": ["Wir", "ste\u00b7cken's", "schlie\u00df\u00b7lich", "ru\u00b7hig", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer wird denn immer: \u201eKreuzigt!\u201c schrein?", "tokens": ["Wer", "wird", "denn", "im\u00b7mer", ":", "\u201e", "Kreu\u00b7zigt", "!", "\u201c", "schrein", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "$.", "$(", "VVFIN", "$.", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur Eins wird man ihm nie verknusen,", "tokens": ["Nur", "Eins", "wird", "man", "ihm", "nie", "ver\u00b7knu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PIS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Und g\u00e4b's statt neun selbst neunzig Musen:", "tokens": ["Und", "g\u00e4b's", "statt", "neun", "selbst", "neun\u00b7zig", "Mu\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "CARD", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn er in Reimen w\u00e4ssrig thr\u00e4nt,", "tokens": ["Wenn", "er", "in", "Rei\u00b7men", "w\u00e4ss\u00b7rig", "thr\u00e4nt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Inde\u00df sein armer Leser g\u00e4hnt!", "tokens": ["In\u00b7de\u00df", "sein", "ar\u00b7mer", "Le\u00b7ser", "g\u00e4hnt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Drum, wer uns langweilt oder ledert,", "tokens": ["Drum", ",", "wer", "uns", "lang\u00b7weilt", "o\u00b7der", "le\u00b7dert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Verdient, da\u00df man ihn theert und federt!", "tokens": ["Ver\u00b7di\u00b7ent", ",", "da\u00df", "man", "ihn", "theert", "und", "fe\u00b7dert", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PIS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}