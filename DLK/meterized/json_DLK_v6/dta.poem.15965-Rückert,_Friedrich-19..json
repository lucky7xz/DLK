{"dta.poem.15965": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Gl\u00fcck, das pl\u00f6tzlich kam, wird pl\u00f6tzlich wieder gehn;", "tokens": ["Ein", "Gl\u00fcck", ",", "das", "pl\u00f6tz\u00b7lich", "kam", ",", "wird", "pl\u00f6tz\u00b7lich", "wie\u00b7der", "gehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,", "VAFIN", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das langsamer gereift, wird l\u00e4nger es bestehn?", "tokens": ["Das", "lang\u00b7sa\u00b7mer", "ge\u00b7reift", ",", "wird", "l\u00e4n\u00b7ger", "es", "be\u00b7stehn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VAFIN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nein! ohne Dauer ist hier jede Bl\u00fct' im Garten,", "tokens": ["Nein", "!", "oh\u00b7ne", "Dau\u00b7er", "ist", "hier", "je\u00b7de", "Bl\u00fct'", "im", "Gar\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "APPR", "NN", "VAFIN", "ADV", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und unverwelklich bl\u00fcht nur das, was wir erwarten.", "tokens": ["Und", "un\u00b7ver\u00b7wel\u00b7klich", "bl\u00fcht", "nur", "das", ",", "was", "wir", "er\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "PDS", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "La\u00df jedes Gl\u00fcck verbl\u00fchn, wenn dir nur eines bleibt,", "tokens": ["La\u00df", "je\u00b7des", "Gl\u00fcck", "ver\u00b7bl\u00fchn", ",", "wenn", "dir", "nur", "ei\u00b7nes", "bleibt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "NN", "VVINF", "$,", "KOUS", "PPER", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Hoffnung, die am Zweig stets neue Knospen treibt.", "tokens": ["Die", "Hoff\u00b7nung", ",", "die", "am", "Zweig", "stets", "neu\u00b7e", "Knos\u00b7pen", "treibt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}