{"textgrid.poem.53407": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[wenn wir nun krafftlos ungestalt]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir nun krafftlos ungestalt", "tokens": ["Wenn", "wir", "nun", "kraff\u00b7tlos", "un\u00b7ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von aller Welt verlassen liegen,", "tokens": ["Von", "al\u00b7ler", "Welt", "ver\u00b7las\u00b7sen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Tod mus \u00fcber uns Gewalt", "tokens": ["Der", "Tod", "mus", "\u00fc\u00b7ber", "uns", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPR", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ohn einig' H\u00fclff und Rettung kriegen.", "tokens": ["Ohn", "ei\u00b7nig'", "H\u00fclff", "und", "Ret\u00b7tung", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hin ist die Welt sampt ihrer Lust,", "tokens": ["Hin", "ist", "die", "Welt", "sampt", "ih\u00b7rer", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sinnen Kr\u00e4ffte m\u00fcssen brechen,", "tokens": ["Der", "Sin\u00b7nen", "Kr\u00e4ff\u00b7te", "m\u00fcs\u00b7sen", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wir r\u00f6cheln mit der vollen Brust,", "tokens": ["Wir", "r\u00f6\u00b7cheln", "mit", "der", "vol\u00b7len", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Zunge kann kein Wort mehr sprechen.", "tokens": ["Die", "Zun\u00b7ge", "kann", "kein", "Wort", "mehr", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es endet sich des Lebens Lauff", "tokens": ["Es", "en\u00b7det", "sich", "des", "Le\u00b7bens", "Lauff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sampt dieser Erden eiteln Dingen,", "tokens": ["Sampt", "die\u00b7ser", "Er\u00b7den", "ei\u00b7teln", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die finstre Grabstat thut sich auff", "tokens": ["Die", "finst\u00b7re", "Grab\u00b7stat", "thut", "sich", "auff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd sucht uns tieff hinein zuschlingen.", "tokens": ["Vnd", "sucht", "uns", "tieff", "hin\u00b7ein", "zu\u00b7schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So meinet nicht, es sey gethan,", "tokens": ["So", "mei\u00b7net", "nicht", ",", "es", "sey", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vmb uns und unser gantzes Wesen,", "tokens": ["Vmb", "uns", "und", "un\u00b7ser", "gant\u00b7zes", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die\u00df war der blinden Heyden Wahn", "tokens": ["Die\u00df", "war", "der", "blin\u00b7den", "Hey\u00b7den", "Wahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die nie von Glauben was gelesen.", "tokens": ["Die", "nie", "von", "Glau\u00b7ben", "was", "ge\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "PWS", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wir ziehen nur das irdisch aus", "tokens": ["Wir", "zie\u00b7hen", "nur", "das", "ir\u00b7disch", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd legen ab der S\u00fcnden Leiden,", "tokens": ["Vnd", "le\u00b7gen", "ab", "der", "S\u00fcn\u00b7den", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Damit das sch\u00f6ne Himmels-Hau\u00df", "tokens": ["Da\u00b7mit", "das", "sch\u00f6\u00b7ne", "Him\u00b7mels\u00b7Hau\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vns k\u00f6nne herrlich \u00fcberkleiden.", "tokens": ["Vns", "k\u00f6n\u00b7ne", "herr\u00b7lich", "\u00fc\u00b7berk\u00b7lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Vns die wir jetzt schon himmlisch sind", "tokens": ["Vns", "die", "wir", "jetzt", "schon", "himm\u00b7lisch", "sind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "PPER", "ADV", "ADV", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd auff was zeitlich ist nicht sehen,", "tokens": ["Vnd", "auff", "was", "zeit\u00b7lich", "ist", "nicht", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRELS", "ADJD", "VAFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn die\u00df ist Schatten, Rauch und Wind", "tokens": ["Denn", "die\u00df", "ist", "Schat\u00b7ten", ",", "Rauch", "und", "Wind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und l\u00e4sset sich wie Staub verwehen.", "tokens": ["Und", "l\u00e4s\u00b7set", "sich", "wie", "Staub", "ver\u00b7we\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nein sondern auff die Ewigheit", "tokens": ["Nein", "son\u00b7dern", "auff", "die", "E\u00b7wig\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ohn untterla\u00df die Hertzen lencken,", "tokens": ["Ohn", "unt\u00b7ter\u00b7la\u00df", "die", "Hert\u00b7zen", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da Gott ohn Weise, Ma\u00df und Zeit", "tokens": ["Da", "Gott", "ohn", "Wei\u00b7se", ",", "Ma\u00df", "und", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich uns wird zu geniessen schencken.", "tokens": ["Sich", "uns", "wird", "zu", "ge\u00b7nies\u00b7sen", "schen\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPER", "VAFIN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "So sollt ihr nun den Sinn allein", "tokens": ["So", "sollt", "ihr", "nun", "den", "Sinn", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinauff wo Christus wohnet kehren,", "tokens": ["Hin\u00b7auff", "wo", "Chris\u00b7tus", "woh\u00b7net", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "NE", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der unsrer Schatten Liecht wird seyn", "tokens": ["Der", "uns\u00b7rer", "Schat\u00b7ten", "Liecht", "wird", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "NN", "VAFIN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und unsern schn\u00f6den Leib verkl\u00e4ren.", "tokens": ["Und", "un\u00b7sern", "schn\u00f6\u00b7den", "Leib", "ver\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Gebt keiner leichten Wollust stat", "tokens": ["Gebt", "kei\u00b7ner", "leich\u00b7ten", "Wol\u00b7lust", "stat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lasst euch die S\u00fcnde nicht beth\u00f6ren,", "tokens": ["Lasst", "euch", "die", "S\u00fcn\u00b7de", "nicht", "be\u00b7th\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Weil dort nur Unschuld Stelle hat", "tokens": ["Weil", "dort", "nur", "Un\u00b7schuld", "Stel\u00b7le", "hat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "NN", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die S\u00e4w und Hund hinaus geh\u00f6ren.", "tokens": ["Die", "S\u00e4w", "und", "Hund", "hin\u00b7aus", "ge\u00b7h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APZR", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}