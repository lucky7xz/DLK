{"dta.poem.580": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Allerhand sch\u00f6ne Rarit\u00e4ten und sch\u00f6ne  \n Spiele - Wercke/  \n Welche auf dem  \n Binnisch - und Gr\u00e4ntzischen  \n Hochzeit-Feste vorgestellet  \n der Teutsche Welsche Mann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Hier k\u00f6nnt ihr weiter sehn der ", "tokens": ["Hier", "k\u00f6nnt", "ihr", "wei\u00b7ter", "sehn", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVINF", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Spitzen Unterhembd/ denn ", "tokens": ["Und", "Spit\u00b7zen", "Un\u00b7ter\u00b7hembd", "/", "denn"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "NN", "NN", "$(", "KON"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dort l\u00e4ufft aus ", "tokens": ["Dort", "l\u00e4ufft", "aus"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ"], "meter": "+--", "measure": "dactylic.init"}, "line.4": {"text": "Ein Sperling steht im Stall an einer dicken Ketten/", "tokens": ["Ein", "Sper\u00b7ling", "steht", "im", "Stall", "an", "ei\u00b7ner", "di\u00b7cken", "Ket\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hier f\u00e4hrt ein Wage \u00fcbers Meer/", "tokens": ["Hier", "f\u00e4hrt", "ein", "Wa\u00b7ge", "\u00fc\u00b7bers", "Meer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Schiff kommt \u00fcbern Hartz-Wald her.", "tokens": ["Ein", "Schiff", "kommt", "\u00fc\u00b7bern", "Hartz\u00b7Wald", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Sch\u00f6ne Rarit\u00e4t/ sch\u00f6ne Spiele-Werckerc.", "tokens": ["Sch\u00f6\u00b7ne", "Ra\u00b7ri\u00b7t\u00e4t", "/", "sch\u00f6\u00b7ne", "Spie\u00b7le\u00b7\u00b7Wer\u00b7ckerc", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}}}}