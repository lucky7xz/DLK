{"dta.poem.8226": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Nelken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Das ich der Liebsten schickte;", "tokens": ["Das", "ich", "der", "Liebs\u00b7ten", "schick\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht lie\u00df ich sagen ihr, von wem,", "tokens": ["Nicht", "lie\u00df", "ich", "sa\u00b7gen", "ihr", ",", "von", "wem", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "VVFIN", "PPER", "$,", "APPR", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wer die Blumen pfl\u00fcckte.", "tokens": ["Und", "wer", "die", "Blu\u00b7men", "pfl\u00fcck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch als ich Abends kam zu Tanz", "tokens": ["Doch", "als", "ich", "A\u00b7bends", "kam", "zu", "Tanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und that verstohlen und sachte,", "tokens": ["Und", "that", "ver\u00b7stoh\u00b7len", "und", "sach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da trug sie die Nelken am Busenlatz,", "tokens": ["Da", "trug", "sie", "die", "Nel\u00b7ken", "am", "Bu\u00b7sen\u00b7latz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und schaute mich an und lachte.", "tokens": ["Und", "schau\u00b7te", "mich", "an", "und", "lach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}