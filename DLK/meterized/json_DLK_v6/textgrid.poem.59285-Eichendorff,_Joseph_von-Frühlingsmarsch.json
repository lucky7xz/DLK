{"textgrid.poem.59285": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsmarsch", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch \u00fcber euren Sorgen", "tokens": ["Hoch", "\u00fc\u00b7ber", "eu\u00b7ren", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sah ich vom Berg ins Land", "tokens": ["Sah", "ich", "vom", "Berg", "ins", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Voll tausend guter Morgen,", "tokens": ["Voll", "tau\u00b7send", "gu\u00b7ter", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Welt in Bl\u00fcten stand.", "tokens": ["Die", "Welt", "in", "Bl\u00fc\u00b7ten", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was zagt ihr tr\u00e4g und bl\u00f6de?", "tokens": ["Was", "zagt", "ihr", "tr\u00e4g", "und", "bl\u00f6\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "KON", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was sch\u00f6n ist, wird doch dein!", "tokens": ["Was", "sch\u00f6n", "ist", ",", "wird", "doch", "dein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VAFIN", "ADV", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Welt tut nur so spr\u00f6de", "tokens": ["Die", "Welt", "tut", "nur", "so", "spr\u00f6\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und will erobert sein.", "tokens": ["Und", "will", "er\u00b7o\u00b7bert", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "La\u00dft die Trompeten laden,", "tokens": ["La\u00dft", "die", "Trom\u00b7pe\u00b7ten", "la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Durchs Land die Trommeln gehn,", "tokens": ["Durchs", "Land", "die", "Trom\u00b7meln", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es wimmeln Kameraden,", "tokens": ["Es", "wim\u00b7meln", "Ka\u00b7me\u00b7ra\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo rechte Banner wehn.", "tokens": ["Wo", "rech\u00b7te", "Ban\u00b7ner", "wehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wir ziehn durch die Provinzen,", "tokens": ["Wir", "ziehn", "durch", "die", "Pro\u00b7vin\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da funkelt manches Schlo\u00df,", "tokens": ["Da", "fun\u00b7kelt", "man\u00b7ches", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sch\u00f6n Lieb, hol dich vom Zwinger", "tokens": ["Sch\u00f6n", "Lieb", ",", "hol", "dich", "vom", "Zwin\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und schwing dich mit aufs Ro\u00df!", "tokens": ["Und", "schwing", "dich", "mit", "aufs", "Ro\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wenn das Bl\u00fchen endet:", "tokens": ["Und", "wenn", "das", "Bl\u00fc\u00b7hen", "en\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch taumelnd sprengen wir,", "tokens": ["Noch", "tau\u00b7melnd", "spren\u00b7gen", "wir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Abendrot geblendet,", "tokens": ["Vom", "A\u00b7ben\u00b7drot", "ge\u00b7blen\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ins letzte Nachtquartier.", "tokens": ["Ins", "letz\u00b7te", "Nacht\u00b7quar\u00b7tier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}