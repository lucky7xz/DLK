{"dta.poem.2867": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "68.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich rathe dir, wenn eng ist deines Gartens Raum,", "tokens": ["Ich", "ra\u00b7the", "dir", ",", "wenn", "eng", "ist", "dei\u00b7nes", "Gar\u00b7tens", "Raum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "ADJD", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zuerst zu pflanzen drin fruchttragend einen Baum;", "tokens": ["Zu\u00b7erst", "zu", "pflan\u00b7zen", "drin", "frucht\u00b7tra\u00b7gend", "ei\u00b7nen", "Baum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dann aber, wenn noch Raum daneben ist, daneben", "tokens": ["Dann", "a\u00b7ber", ",", "wenn", "noch", "Raum", "da\u00b7ne\u00b7ben", "ist", ",", "da\u00b7ne\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "ADV", "NN", "PAV", "VAFIN", "$,", "PAV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu pflanzen einen Baum, der auch mag Schatten geben.", "tokens": ["Zu", "pflan\u00b7zen", "ei\u00b7nen", "Baum", ",", "der", "auch", "mag", "Schat\u00b7ten", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$,", "PRELS", "ADV", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sei nur zufrieden, wenn der eine dir den Schatten,", "tokens": ["Sei", "nur", "zu\u00b7frie\u00b7den", ",", "wenn", "der", "ei\u00b7ne", "dir", "den", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "KOUS", "ART", "ART", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der andre gibt die Frucht, so wirst du nie ermatten.", "tokens": ["Der", "and\u00b7re", "gibt", "die", "Frucht", ",", "so", "wirst", "du", "nie", "er\u00b7mat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch dann bist du begl\u00fcckt, wenn dir den engen Raum", "tokens": ["Doch", "dann", "bist", "du", "be\u00b7gl\u00fcckt", ",", "wenn", "dir", "den", "en\u00b7gen", "Raum"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVPP", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Herzens f\u00fcllet ein Zugleich-Frucht-Schatten-Baum.", "tokens": ["Des", "Her\u00b7zens", "f\u00fcl\u00b7let", "ein", "Zu\u00b7gleich\u00b7Frucht\u00b7Schat\u00b7ten\u00b7Baum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}