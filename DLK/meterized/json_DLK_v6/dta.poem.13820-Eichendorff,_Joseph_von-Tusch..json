{"dta.poem.13820": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Tusch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00e4ngt die Sonne an zu stechen,", "tokens": ["F\u00e4ngt", "die", "Son\u00b7ne", "an", "zu", "ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tapfer schie\u00dfen Gras und Kr\u00e4uter", "tokens": ["Tap\u00b7fer", "schie\u00b7\u00dfen", "Gras", "und", "Kr\u00e4u\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die B\u00e4ume schlagen aus:", "tokens": ["Und", "die", "B\u00e4u\u00b7me", "schla\u00b7gen", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df des Feinds Gewalt zerbrechen,", "tokens": ["Mu\u00df", "des", "Feinds", "Ge\u00b7walt", "zer\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimmt der Winter schnell Rei\u00dfaus,", "tokens": ["Nimmt", "der", "Win\u00b7ter", "schnell", "Rei\u00df\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Erd' und Himmel gl\u00e4nzen heiter;", "tokens": ["Erd'", "und", "Him\u00b7mel", "gl\u00e4n\u00b7zen", "hei\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und wir Musikanten fahren,", "tokens": ["Und", "wir", "Mu\u00b7si\u00b7kan\u00b7ten", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lustig auf dem Flu\u00df hinunter,", "tokens": ["Lus\u00b7tig", "auf", "dem", "Flu\u00df", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Trommeln, pfeifen, blasen, geigen", "tokens": ["Trom\u00b7meln", ",", "pfei\u00b7fen", ",", "bla\u00b7sen", ",", "gei\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "VVINF", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und die H\u00f6rner klingen munter.", "tokens": ["Und", "die", "H\u00f6r\u00b7ner", "klin\u00b7gen", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}