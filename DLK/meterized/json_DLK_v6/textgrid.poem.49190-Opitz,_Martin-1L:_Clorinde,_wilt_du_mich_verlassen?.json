{"textgrid.poem.49190": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Clorinde, wilt du mich verlassen?", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Clorinde, wilt du mich verlassen?", "tokens": ["Clo\u00b7rin\u00b7de", ",", "wilt", "du", "mich", "ver\u00b7las\u00b7sen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Halt an und eile nicht so sehr;", "tokens": ["Halt", "an", "und", "ei\u00b7le", "nicht", "so", "sehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "KON", "VVFIN", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Lieben ist ein halbes Hassen,", "tokens": ["Dein", "Lie\u00b7ben", "ist", "ein", "hal\u00b7bes", "Has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat wenig Glut, Rauch desto mehr.", "tokens": ["Hat", "we\u00b7nig", "Glut", ",", "Rauch", "des\u00b7to", "mehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,", "NN", "ADV", "ADV", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Getreue Liebe kan nicht wancken,", "tokens": ["Ge\u00b7treu\u00b7e", "Lie\u00b7be", "kan", "nicht", "wan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie liegt zu Ancker jederzeit,", "tokens": ["Sie", "liegt", "zu", "An\u00b7cker", "je\u00b7der\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat ihre Sinnen und Gedancken", "tokens": ["Hat", "ih\u00b7re", "Sin\u00b7nen", "und", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gegr\u00fcndet auff Bestendigkeit.", "tokens": ["Ge\u00b7gr\u00fcn\u00b7det", "auff", "Bes\u00b7ten\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Was wilt du dich umb etwas gr\u00e4men", "tokens": ["Was", "wilt", "du", "dich", "umb", "et\u00b7was", "gr\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "PRF", "APPR", "PIS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das nichts ist als ein blosser Wahn,", "tokens": ["Das", "nichts", "ist", "als", "ein", "blos\u00b7ser", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und du mir nicht kanst wieder nemen", "tokens": ["Und", "du", "mir", "nicht", "kanst", "wie\u00b7der", "ne\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PPER", "PTKNEG", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich gleichfals dir nicht geben kan.", "tokens": ["Ich", "gleich\u00b7fals", "dir", "nicht", "ge\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Kein Mensch ist nur ihm selbst gebohren,", "tokens": ["Kein", "Mensch", "ist", "nur", "ihm", "selbst", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was du bist, wei\u00df ich, da\u00df ich bin;", "tokens": ["Was", "du", "bist", ",", "wei\u00df", "ich", ",", "da\u00df", "ich", "bin", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$,", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was fort ist, wird nicht mehr verlohren,", "tokens": ["Was", "fort", "ist", ",", "wird", "nicht", "mehr", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKVZ", "VAFIN", "$,", "VAFIN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Darumb gedencke, hin sey hin.", "tokens": ["Da\u00b7rumb", "ge\u00b7den\u00b7cke", ",", "hin", "sey", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "ADV", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Komm her und la\u00df dich ferner lieben", "tokens": ["Komm", "her", "und", "la\u00df", "dich", "fer\u00b7ner", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "KON", "VVIMP", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weil deine Jugend Sch\u00f6nheit hat;", "tokens": ["Weil", "dei\u00b7ne", "Ju\u00b7gend", "Sch\u00f6n\u00b7heit", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bleib, bistu ein mal schon geblieben,", "tokens": ["Bleib", ",", "bis\u00b7tu", "ein", "mal", "schon", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ART", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Flucht ist jetzt doch gar zu spat.", "tokens": ["Die", "Flucht", "ist", "jetzt", "doch", "gar", "zu", "spat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}