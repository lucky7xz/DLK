{"textgrid.poem.55471": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "52", "genre": "verse", "period": "N.A.", "pub_year": 1820, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwar die vierundzwanzig Ritter", "tokens": ["Zwar", "die", "vie\u00b7rund\u00b7zwan\u00b7zig", "Rit\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ehren wir in allen F\u00e4llen;", "tokens": ["Eh\u00b7ren", "wir", "in", "al\u00b7len", "F\u00e4l\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch auch Fr\u00e4ulein sind nicht bitter,", "tokens": ["Doch", "auch", "Fr\u00e4u\u00b7lein", "sind", "nicht", "bit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn sie sich dazwischenstellen.", "tokens": ["Wenn", "sie", "sich", "da\u00b7zwi\u00b7schen\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Heute lasset mich beachten", "tokens": ["Heu\u00b7te", "las\u00b7set", "mich", "be\u00b7ach\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Solche lieblichsten Vereine,", "tokens": ["Sol\u00b7che", "lieb\u00b7lichs\u00b7ten", "Ver\u00b7ei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Wenn sie bunte Reihe machten,", "tokens": ["Wenn", "sie", "bun\u00b7te", "Rei\u00b7he", "mach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "K\u00e4men s\u00e4mtlich angezogen", "tokens": ["K\u00e4\u00b7men", "s\u00e4mt\u00b7lich", "an\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser St\u00e4mme frohe Lichter,", "tokens": ["Die\u00b7ser", "St\u00e4m\u00b7me", "fro\u00b7he", "Lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wurden K\u00f6nige gewogen", "tokens": ["Wur\u00b7den", "K\u00f6\u00b7ni\u00b7ge", "ge\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und begr\u00fc\u00dften sie die Dichter.", "tokens": ["Und", "be\u00b7gr\u00fc\u00df\u00b7ten", "sie", "die", "Dich\u00b7ter."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Und besonders aber ", "tokens": ["Und", "be\u00b7son\u00b7ders", "a\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Welche wir zu segnen kamen;", "tokens": ["Wel\u00b7che", "wir", "zu", "seg\u00b7nen", "ka\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Freunde nennen sie die Kleine,", "tokens": ["Freun\u00b7de", "nen\u00b7nen", "sie", "die", "Klei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie verdient gar viele Namen.", "tokens": ["Sie", "ver\u00b7dient", "gar", "vie\u00b7le", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}