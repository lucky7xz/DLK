{"textgrid.poem.36431": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Siebentes Sonett", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der M\u00fcllerbursche schiebt hinauf zur M\u00fchle", "tokens": ["Der", "M\u00fcl\u00b7ler\u00b7bur\u00b7sche", "schiebt", "hin\u00b7auf", "zur", "M\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf seinem Karren einen M\u00fchlenstein.", "tokens": ["Auf", "sei\u00b7nem", "Kar\u00b7ren", "ei\u00b7nen", "M\u00fch\u00b7len\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und in die \u00d6ffnung schob er glatt hinein", "tokens": ["Und", "in", "die", "\u00d6ff\u00b7nung", "schob", "er", "glatt", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sein steifes Glied und schaffte so sich K\u00fchle.", "tokens": ["Sein", "stei\u00b7fes", "Glied", "und", "schaff\u00b7te", "so", "sich", "K\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "VVFIN", "ADV", "PRF", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die blonde M\u00fcll'rin sieht's im Sonnenschein.", "tokens": ["Die", "blon\u00b7de", "M\u00fcll'\u00b7rin", "sieht's", "im", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und trotz der unertr\u00e4glich dumpfen Schw\u00fcle", "tokens": ["Und", "trotz", "der", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "dum\u00b7pfen", "Schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "L\u00e4uft sie hinab, da\u00df pr\u00fcfend sie's bef\u00fchle:", "tokens": ["L\u00e4uft", "sie", "hin\u00b7ab", ",", "da\u00df", "pr\u00fc\u00b7fend", "sie's", "be\u00b7f\u00fch\u00b7le", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "ADJD", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie fa\u00dft und f\u00fchlt, es ist von Fleisch und Bein.", "tokens": ["Sie", "fa\u00dft", "und", "f\u00fchlt", ",", "es", "ist", "von", "Fleisch", "und", "Bein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbna h\u00f6r', mein Junge\u00ab, ruft sie sehr brutal,", "tokens": ["\u00bb", "na", "h\u00f6r'", ",", "mein", "Jun\u00b7ge", "\u00ab", ",", "ruft", "sie", "sehr", "bru\u00b7tal", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "VVFIN", "$,", "PPOSAT", "NN", "$(", "$,", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbwas soll die Schweinerei mit deinem Schweif? .. !", "tokens": ["\u00bb", "was", "soll", "die", "Schwei\u00b7ne\u00b7rei", "mit", "dei\u00b7nem", "Schweif", "?", "..", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PWS", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$.", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist das die Pr\u00fcfung, die ich dir befahl.", "tokens": ["Ist", "das", "die", "Pr\u00fc\u00b7fung", ",", "die", "ich", "dir", "be\u00b7fahl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ob du auch w\u00fcrdig w\u00e4rest f\u00fcr mein Bett?\u00ab", "tokens": ["Ob", "du", "auch", "w\u00fcr\u00b7dig", "w\u00e4\u00b7rest", "f\u00fcr", "mein", "Bett", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch er zeigt nur die Inschrift um den Reif.", "tokens": ["Doch", "er", "zeigt", "nur", "die", "In\u00b7schrift", "um", "den", "Reif", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und ach, sie liest ger\u00fchrt: Elisabeth ... !", "tokens": ["Und", "ach", ",", "sie", "liest", "ge\u00b7r\u00fchrt", ":", "E\u00b7lis\u00b7a\u00b7beth", "...", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "XY", "$,", "PPER", "VVFIN", "VVPP", "$.", "NE", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}