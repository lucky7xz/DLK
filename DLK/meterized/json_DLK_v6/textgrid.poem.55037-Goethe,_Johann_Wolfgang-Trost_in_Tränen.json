{"textgrid.poem.55037": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Trost in Tr\u00e4nen", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie kommt's, da\u00df du so traurig bist,", "tokens": ["Wie", "kommt's", ",", "da\u00df", "du", "so", "trau\u00b7rig", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da alles froh erscheint?", "tokens": ["Da", "al\u00b7les", "froh", "er\u00b7scheint", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man sieht dir's an den Augen an,", "tokens": ["Man", "sieht", "dir's", "an", "den", "Au\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIS", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gewi\u00df, du hast geweint.", "tokens": ["Ge\u00b7wi\u00df", ",", "du", "hast", "ge\u00b7weint", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbund hab ich einsam auch geweint,", "tokens": ["\u00bb", "und", "hab", "ich", "ein\u00b7sam", "auch", "ge\u00b7weint", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "PPER", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So ist's mein eigner Schmerz,", "tokens": ["So", "ist's", "mein", "eig\u00b7ner", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Tr\u00e4nen flie\u00dfen gar so s\u00fc\u00df,", "tokens": ["Und", "Tr\u00e4\u00b7nen", "flie\u00b7\u00dfen", "gar", "so", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erleichtern mir das Herz.\u00ab", "tokens": ["Er\u00b7leich\u00b7tern", "mir", "das", "Herz", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die frohen Freunde laden dich,", "tokens": ["Die", "fro\u00b7hen", "Freun\u00b7de", "la\u00b7den", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O komm an unsre Brust!", "tokens": ["O", "komm", "an", "uns\u00b7re", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und was du auch verloren hast,", "tokens": ["Und", "was", "du", "auch", "ver\u00b7lo\u00b7ren", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vertraue den Verlust.", "tokens": ["Ver\u00b7trau\u00b7e", "den", "Ver\u00b7lust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbihr l\u00e4rmt und rauscht und ahnet nicht,", "tokens": ["\u00bb", "ihr", "l\u00e4rmt", "und", "rauscht", "und", "ah\u00b7net", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was mich, den Armen, qu\u00e4lt.", "tokens": ["Was", "mich", ",", "den", "Ar\u00b7men", ",", "qu\u00e4lt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "$,", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach nein, verloren hab ich's nicht,", "tokens": ["Ach", "nein", ",", "ver\u00b7lo\u00b7ren", "hab", "ich's", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$,", "VVPP", "VAFIN", "PIS", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sosehr es mir auch fehlt.\u00ab", "tokens": ["So\u00b7sehr", "es", "mir", "auch", "fehlt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So raffe denn dich eilig auf,", "tokens": ["So", "raf\u00b7fe", "denn", "dich", "ei\u00b7lig", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist ein junges Blut.", "tokens": ["Du", "bist", "ein", "jun\u00b7ges", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In deinen Jahren hat man Kraft", "tokens": ["In", "dei\u00b7nen", "Jah\u00b7ren", "hat", "man", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PIS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und zum Erwerben Mut.", "tokens": ["Und", "zum", "Er\u00b7wer\u00b7ben", "Mut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbach nein, erwerben kann ich's nicht,", "tokens": ["\u00bb", "ach", "nein", ",", "er\u00b7wer\u00b7ben", "kann", "ich's", "nicht", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "PTKANT", "$,", "VVINF", "VMFIN", "PIS", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es steht mir gar zu fern.", "tokens": ["Es", "steht", "mir", "gar", "zu", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es weilt so hoch, es blinkt so sch\u00f6n,", "tokens": ["Es", "weilt", "so", "hoch", ",", "es", "blinkt", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie droben jener Stern.\u00ab", "tokens": ["Wie", "dro\u00b7ben", "je\u00b7ner", "Stern", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADV", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Sterne, die begehrt man nicht,", "tokens": ["Die", "Ster\u00b7ne", ",", "die", "be\u00b7gehrt", "man", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PIS", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man freut sich ihrer Pracht,", "tokens": ["Man", "freut", "sich", "ih\u00b7rer", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mit Entz\u00fccken blickt man auf", "tokens": ["Und", "mit", "Ent\u00b7z\u00fc\u00b7cken", "blickt", "man", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "PIS", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In jeder heitern Nacht.", "tokens": ["In", "je\u00b7der", "hei\u00b7tern", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbund mit Entz\u00fccken blick ich auf,", "tokens": ["\u00bb", "und", "mit", "Ent\u00b7z\u00fc\u00b7cken", "blick", "ich", "auf", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So manchen lieben Tag;", "tokens": ["So", "man\u00b7chen", "lie\u00b7ben", "Tag", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verweinen la\u00dft die N\u00e4chte mich,", "tokens": ["Ver\u00b7wei\u00b7nen", "la\u00dft", "die", "N\u00e4ch\u00b7te", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Solang ich weinen mag.\u00ab", "tokens": ["So\u00b7lang", "ich", "wei\u00b7nen", "mag.", "\u00ab"], "token_info": ["word", "word", "word", "abbreviation", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}}}}}