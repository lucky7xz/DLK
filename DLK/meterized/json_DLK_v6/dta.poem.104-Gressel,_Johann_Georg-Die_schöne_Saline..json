{"dta.poem.104": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die sch\u00f6ne  Saline.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcngst da Dianens Glantz durch dunckle Wolcken brach/", "tokens": ["F\u00fcngst", "da", "Di\u00b7a\u00b7nens", "Glantz", "durch", "dunck\u00b7le", "Wol\u00b7cken", "brach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NE", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da sich das Sternen-Heer schon allbereits zur Wach\u2019", "tokens": ["Da", "sich", "das", "Ster\u00b7nen\u00b7Heer", "schon", "all\u00b7be\u00b7reits", "zur", "Wach'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf seinem Sammel-Platz dem blauen Himmel zeigte/", "tokens": ["Auf", "sei\u00b7nem", "Sam\u00b7mel\u00b7Platz", "dem", "blau\u00b7en", "Him\u00b7mel", "zeig\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Da sahe ich ein Kind von Englischer ", "tokens": ["Da", "sa\u00b7he", "ich", "ein", "Kind", "von", "Eng\u00b7li\u00b7scher"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "An dessen Ausbildung die g\u00fctige Natur", "tokens": ["An", "des\u00b7sen", "Aus\u00b7bil\u00b7dung", "die", "g\u00fc\u00b7ti\u00b7ge", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "All\u2019 ihre Krafft gewandt es g\u00f6ttlich auszuzieren;", "tokens": ["All'", "ih\u00b7re", "Krafft", "ge\u00b7wandt", "es", "g\u00f6tt\u00b7lich", "aus\u00b7zu\u00b7zie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPOSAT", "NN", "VVPP", "PPER", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vor ihn mu\u00df ", "tokens": ["Vor", "ihn", "mu\u00df"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPER", "VMFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Die blaue ", "tokens": ["Die", "blau\u00b7e"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Calliope mu\u00df ihm an heller Stimme weichen.", "tokens": ["Cal\u00b7li\u00b7o\u00b7pe", "mu\u00df", "ihm", "an", "hel\u00b7ler", "Stim\u00b7me", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Eratens leichte Hand spielt kein so nett Clavier", "tokens": ["E\u00b7ra\u00b7tens", "leich\u00b7te", "Hand", "spielt", "kein", "so", "nett", "Cla\u00b7vier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "VVFIN", "PIAT", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als diese ", "tokens": ["Als", "die\u00b7se"], "token_info": ["word", "word"], "pos": ["KOUS", "PDS"], "meter": "+--", "measure": "dactylic.init"}, "line.15": {"text": "Sich vor denselben wies mit schwartz geputzten Haaren/", "tokens": ["Sich", "vor", "den\u00b7sel\u00b7ben", "wies", "mit", "schwartz", "ge\u00b7putz\u00b7ten", "Haa\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PDS", "VVFIN", "APPR", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die auf der weissen Haut sich mit Zinnober paaren/", "tokens": ["Die", "auf", "der", "weis\u00b7sen", "Haut", "sich", "mit", "Zin\u00b7no\u00b7ber", "paa\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "PRF", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das auf dem Liljen-Feld der Wangen sich gesetzt.", "tokens": ["Das", "auf", "dem", "Lil\u00b7jen\u00b7Feld", "der", "Wan\u00b7gen", "sich", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der Br\u00fcste Schnee Geb\u00fcrg mit ", "tokens": ["Der", "Br\u00fcs\u00b7te", "Schnee", "Ge\u00b7b\u00fcrg", "mit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Wie weisser ", "tokens": ["Wie", "weis\u00b7ser"], "token_info": ["word", "word"], "pos": ["PWAV", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.20": {"text": "Hier kan der schwache Kiehl das Wunder nicht erg\u00e4ntzen;", "tokens": ["Hier", "kan", "der", "schwa\u00b7che", "Kiehl", "das", "Wun\u00b7der", "nicht", "er\u00b7g\u00e4nt\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da meine r\u00fcde Faust schon allzuweit gepfl\u00fcgt;", "tokens": ["Da", "mei\u00b7ne", "r\u00fc\u00b7de", "Faust", "schon", "all\u00b7zu\u00b7weit", "ge\u00b7pfl\u00fcgt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Gnug/ da\u00df ihr hoher Strahl ein steinern Hertz besiegt/", "tokens": ["Gnug", "/", "da\u00df", "ihr", "ho\u00b7her", "Strahl", "ein", "stei\u00b7nern", "Hertz", "be\u00b7siegt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Gnug/ da\u00df der S\u00e4yten-Schall den frohen Geist entz\u00fccket/", "tokens": ["Gnug", "/", "da\u00df", "der", "S\u00e4y\u00b7ten\u00b7Schall", "den", "fro\u00b7hen", "Geist", "ent\u00b7z\u00fc\u00b7cket", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Gnug/ da\u00df die holde Stimm der Freyheit Gold entr\u00fccket/", "tokens": ["Gnug", "/", "da\u00df", "die", "hol\u00b7de", "Stimm", "der", "Frey\u00b7heit", "Gold", "ent\u00b7r\u00fc\u00b7cket", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und an unz\u00e4hligen die Sclaven-Fesseln legt/", "tokens": ["Und", "an", "un\u00b7z\u00e4h\u00b7li\u00b7gen", "die", "Scla\u00b7ven\u00b7Fes\u00b7seln", "legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die statt der Unlust-Last nur s\u00fcsse Lust erregt/", "tokens": ["Die", "statt", "der", "Un\u00b7lust\u00b7Last", "nur", "s\u00fcs\u00b7se", "Lust", "er\u00b7regt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Weil man vor Sclaven Kost die pure Wollust isset/", "tokens": ["Weil", "man", "vor", "Scla\u00b7ven", "Kost", "die", "pu\u00b7re", "Wol\u00b7lust", "is\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-++", "measure": "unknown.measure.septa"}, "line.28": {"text": "Wenn sich ihr holder Blick mit unsern Augen k\u00fcsset.", "tokens": ["Wenn", "sich", "ihr", "hol\u00b7der", "Blick", "mit", "un\u00b7sern", "Au\u00b7gen", "k\u00fcs\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-++", "measure": "unknown.measure.septa"}}}}}