{"dta.poem.8582": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Noch immer,", "tokens": ["Noch", "im\u00b7mer", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "durch den br\u00fctenden Sommer,", "tokens": ["durch", "den", "br\u00fc\u00b7ten\u00b7den", "Som\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "singen die Lerchen.", "tokens": ["sin\u00b7gen", "die", "Ler\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Meine blinkende Sichel", "tokens": ["Mei\u00b7ne", "blin\u00b7ken\u00b7de", "Si\u00b7chel"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "zischt durchs Korn.", "tokens": ["zischt", "durchs", "Korn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Im roten Kopftuch", "tokens": ["Im", "ro\u00b7ten", "Kopf\u00b7tuch"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "hinter mir", "tokens": ["hin\u00b7ter", "mir"], "token_info": ["word", "word"], "pos": ["APPR", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "m\u00fcht sich mein Weib und sammelt die Aehren.", "tokens": ["m\u00fcht", "sich", "mein", "Weib", "und", "sam\u00b7melt", "die", "A\u00b7eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PPOSAT", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Mit nackten Beinchen", "tokens": ["Mit", "nack\u00b7ten", "Be\u00b7in\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "und kleinen, braunen F\u00e4usten, die Blumen halten,", "tokens": ["und", "klei\u00b7nen", ",", "brau\u00b7nen", "F\u00e4us\u00b7ten", ",", "die", "Blu\u00b7men", "hal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "ADJA", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "liegt, lacht und strampelt", "tokens": ["liegt", ",", "lacht", "und", "stram\u00b7pelt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "unser Gl\u00fcck.", "tokens": ["un\u00b7ser", "Gl\u00fcck", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}