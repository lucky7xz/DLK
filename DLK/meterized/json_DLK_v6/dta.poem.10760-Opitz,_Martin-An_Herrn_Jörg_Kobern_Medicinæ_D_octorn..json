{"dta.poem.10760": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn J\u00f6rg Kobern   Medicin\u00e6  \n  D octorn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Wen\u0303 Bawren so da dienen sollen", "tokens": ["We\u00f1", "Baw\u00b7ren", "so", "da", "die\u00b7nen", "sol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Selbst Edellente werden wollen:", "tokens": ["Selbst", "E\u00b7del\u00b7len\u00b7te", "wer\u00b7den", "wol\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn aller Zipffel peltzer Geister", "tokens": ["Wenn", "al\u00b7ler", "Zipf\u00b7fel", "pelt\u00b7zer", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich achten mehr als B\u00fcrgermeister.", "tokens": ["Sich", "ach\u00b7ten", "mehr", "als", "B\u00fcr\u00b7ger\u00b7meis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wen\u0303 Schuster/ Gerber/ ander Bengel", "tokens": ["We\u00f1", "Schus\u00b7ter", "/", "Ger\u00b7ber", "/", "an\u00b7der", "Ben\u00b7gel"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "NN", "$(", "NE", "$(", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Getrieben durch de\u00df Satans Engel/", "tokens": ["Ge\u00b7trie\u00b7ben", "durch", "de\u00df", "Sa\u00b7tans", "En\u00b7gel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sich Gottes sachen vnderstehen/", "tokens": ["Sich", "Got\u00b7tes", "sa\u00b7chen", "vn\u00b7der\u00b7ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit newem Glauben schwanger gehen/", "tokens": ["Mit", "ne\u00b7wem", "Glau\u00b7ben", "schwan\u00b7ger", "ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVINF", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Wen\u0303 Fischer werden Advocaten/", "tokens": ["We\u00f1", "Fi\u00b7scher", "wer\u00b7den", "Ad\u00b7vo\u00b7ca\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Vnd ohne Recht zum Rechten rathen/", "tokens": ["Vnd", "oh\u00b7ne", "Recht", "zum", "Rech\u00b7ten", "ra\u00b7then", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wenn grobe Schmide bey dem bober/", "tokens": ["Wenn", "gro\u00b7be", "Schmi\u00b7de", "bey", "dem", "bo\u00b7ber", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Das wolten thun was ", "tokens": ["Das", "wol\u00b7ten", "thun", "was"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "VVINF", "PWS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Wen alles sollt gehn vbernhauffen/", "tokens": ["Wen", "al\u00b7les", "sollt", "gehn", "vbern\u00b7hauf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VMFIN", "VVINF", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "So mu\u00df die Welt zum Thor au\u00dflauffen.", "tokens": ["So", "mu\u00df", "die", "Welt", "zum", "Thor", "au\u00df\u00b7lauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}