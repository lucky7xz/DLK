{"textgrid.poem.60604": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der Erl\u00f6ser", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich irr um traurige Cypressen,", "tokens": ["Ich", "irr", "um", "trau\u00b7ri\u00b7ge", "Cyp\u00b7res\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Am leichenvollen Golgatha:", "tokens": ["Am", "lei\u00b7chen\u00b7vol\u00b7len", "Gol\u00b7ga\u00b7tha", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie kann ich schweigen und vergessen,", "tokens": ["Wie", "kann", "ich", "schwei\u00b7gen", "und", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVINF", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was hier zu meinem Heil geschah?", "tokens": ["Was", "hier", "zu", "mei\u00b7nem", "Heil", "ge\u00b7schah", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn nicht das Blut von tausend Rindern", "tokens": ["Denn", "nicht", "das", "Blut", "von", "tau\u00b7send", "Rin\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ART", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ward hier vergossen, sondern Blut,", "tokens": ["Ward", "hier", "ver\u00b7gos\u00b7sen", ",", "son\u00b7dern", "Blut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das ganzen Welten Gutes thut,", "tokens": ["Das", "gan\u00b7zen", "Wel\u00b7ten", "Gu\u00b7tes", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Mittlers zwischen Gott und S\u00fcndern.", "tokens": ["Des", "Mitt\u00b7lers", "zwi\u00b7schen", "Gott", "und", "S\u00fcn\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich will, ich mu\u00df von Jesu singen!", "tokens": ["Ich", "will", ",", "ich", "mu\u00df", "von", "Je\u00b7su", "sin\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Liebe kam er auf die Welt.", "tokens": ["Aus", "Lie\u00b7be", "kam", "er", "auf", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wahrheit flog mit g\u00fcldnen Schwingen", "tokens": ["Die", "Wahr\u00b7heit", "flog", "mit", "g\u00fcld\u00b7nen", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm g\u00f6ttlich strahlend beygesellt:", "tokens": ["Ihm", "g\u00f6tt\u00b7lich", "strah\u00b7lend", "bey\u00b7ge\u00b7sellt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als Finsterni\u00df der dicksten Schatten", "tokens": ["Als", "Fins\u00b7ter\u00b7ni\u00df", "der", "dicks\u00b7ten", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Noch \u00fcber allen V\u00f6lkern lag,", "tokens": ["Noch", "\u00fc\u00b7ber", "al\u00b7len", "V\u00f6l\u00b7kern", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und auch die Weisen keinen Tag,", "tokens": ["Und", "auch", "die", "Wei\u00b7sen", "kei\u00b7nen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Kaum eine schwache D\u00e4mmrung hatten.", "tokens": ["Kaum", "ei\u00b7ne", "schwa\u00b7che", "D\u00e4mm\u00b7rung", "hat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr V\u00f6lker, in Jud\u00e4ens Gr\u00e4nzen", "tokens": ["Ihr", "V\u00f6l\u00b7ker", ",", "in", "Ju\u00b7d\u00e4\u00b7ens", "Gr\u00e4n\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erscheint ein wunderbares Licht!", "tokens": ["Er\u00b7scheint", "ein", "wun\u00b7der\u00b7ba\u00b7res", "Licht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Jordans wei\u00dfe Fluthen gl\u00e4nzen,", "tokens": ["Des", "Jor\u00b7dans", "wei\u00b7\u00dfe", "Flut\u00b7hen", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie von der Sonnen Angesicht.", "tokens": ["Wie", "von", "der", "Son\u00b7nen", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sehe Cedern sich verg\u00fclden,", "tokens": ["Ich", "se\u00b7he", "Ce\u00b7dern", "sich", "ver\u00b7g\u00fcl\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Cedern auf dem Libanon!", "tokens": ["Die", "Ce\u00b7dern", "auf", "dem", "Li\u00b7ba\u00b7non", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der neue Morgen schimmert schon", "tokens": ["Der", "neu\u00b7e", "Mor\u00b7gen", "schim\u00b7mert", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den allerdunkelsten Gefilden.", "tokens": ["Den", "al\u00b7ler\u00b7dun\u00b7kels\u00b7ten", "Ge\u00b7fil\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Gott k\u00f6mmt vom Himmel, euch zu lehren:", "tokens": ["Gott", "k\u00f6mmt", "vom", "Him\u00b7mel", ",", "euch", "zu", "leh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seht, wie vor ihm die Erde schweigt!", "tokens": ["Seht", ",", "wie", "vor", "ihm", "die", "Er\u00b7de", "schweigt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Heiden dr\u00e4ngen sich, zu h\u00f6ren,", "tokens": ["Die", "Hei\u00b7den", "dr\u00e4n\u00b7gen", "sich", ",", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da sich der gro\u00dfe Lehrer zeigt.", "tokens": ["Da", "sich", "der", "gro\u00b7\u00dfe", "Leh\u00b7rer", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er lehret uns die Gottheit kennen,", "tokens": ["Er", "leh\u00b7ret", "uns", "die", "Got\u00b7theit", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ladet uns zum neuen Bund:", "tokens": ["Und", "la\u00b7det", "uns", "zum", "neu\u00b7en", "Bund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Durch ihn darf unser scheuer Mund", "tokens": ["Durch", "ihn", "darf", "un\u00b7ser", "scheu\u00b7er", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gott wieder unsern Vater nennen.", "tokens": ["Gott", "wie\u00b7der", "un\u00b7sern", "Va\u00b7ter", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da unser schuldiges Geschlechte", "tokens": ["Da", "un\u00b7ser", "schul\u00b7di\u00b7ges", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Tode heimgefallen war:", "tokens": ["Dem", "To\u00b7de", "heim\u00b7ge\u00b7fal\u00b7len", "war", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Stellt sich der einzige Gerechte", "tokens": ["Stellt", "sich", "der", "ein\u00b7zi\u00b7ge", "Ge\u00b7rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Zum Opfer der Vers\u00f6hnung dar.", "tokens": ["Zum", "Op\u00b7fer", "der", "Ver\u00b7s\u00f6h\u00b7nung", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verlohren waren Adams Kinder!", "tokens": ["Ver\u00b7loh\u00b7ren", "wa\u00b7ren", "A\u00b7dams", "Kin\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NE", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Sohn des Gottes Zebaoth", "tokens": ["Der", "Sohn", "des", "Got\u00b7tes", "Ze\u00b7bao\u00b7th"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Erniedrigt sich zum Kreuzestod,", "tokens": ["Er\u00b7nied\u00b7rigt", "sich", "zum", "Kreu\u00b7ze\u00b7stod", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und stirbt f\u00fcr abgefallne S\u00fcnder.", "tokens": ["Und", "stirbt", "f\u00fcr", "ab\u00b7ge\u00b7fall\u00b7ne", "S\u00fcn\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er stirbt! Und war aus Gott gebohren!", "tokens": ["Er", "stirbt", "!", "Und", "war", "aus", "Gott", "ge\u00b7boh\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weg, Zweifel, der mir Jesum raubt!", "tokens": ["Weg", ",", "Zwei\u00b7fel", ",", "der", "mir", "Je\u00b7sum", "raubt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie grimmig zischt vor meinen Ohren", "tokens": ["Wie", "grim\u00b7mig", "zischt", "vor", "mei\u00b7nen", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Natter schwellend um dein Haupt!", "tokens": ["Die", "Nat\u00b7ter", "schwel\u00b7lend", "um", "dein", "Haupt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bete, Herr, vor dir im Staube!", "tokens": ["Ich", "be\u00b7te", ",", "Herr", ",", "vor", "dir", "im", "Stau\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du redest, und ein himmlisch Licht", "tokens": ["Du", "re\u00b7dest", ",", "und", "ein", "himm\u00b7lisch", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Strahlt sieghaft mir ins Angesicht:", "tokens": ["Strahlt", "sieg\u00b7haft", "mir", "ins", "An\u00b7ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du redest, und, o Gott, ich glaube!", "tokens": ["Du", "re\u00b7dest", ",", "und", ",", "o", "Gott", ",", "ich", "glau\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "$,", "FM", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie? Der f\u00fcr mich am Kreuz erbla\u00dfte,", "tokens": ["Wie", "?", "Der", "f\u00fcr", "mich", "am", "Kreuz", "er\u00b7bla\u00df\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "ART", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er\u00f6ffnete des Tauben Ohr,", "tokens": ["Er\u00b7\u00f6ff\u00b7ne\u00b7te", "des", "Tau\u00b7ben", "Ohr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Rief, die des Todes Arm umfa\u00dfte,", "tokens": ["Rief", ",", "die", "des", "To\u00b7des", "Arm", "um\u00b7fa\u00df\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Allm\u00e4chtig aus dem Grab hervor,", "tokens": ["All\u00b7m\u00e4ch\u00b7tig", "aus", "dem", "Grab", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "That \u00fcber menschliches Verm\u00f6gen;", "tokens": ["That", "\u00fc\u00b7ber", "menschli\u00b7ches", "Ver\u00b7m\u00f6\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und dieser sollte Mensch allein,", "tokens": ["Und", "die\u00b7ser", "soll\u00b7te", "Mensch", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht Gott, nicht mein Erl\u00f6ser seyn,", "tokens": ["Nicht", "Gott", ",", "nicht", "mein", "Er\u00b7l\u00f6\u00b7ser", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und hundert Wunderwerke l\u00f6gen?", "tokens": ["Und", "hun\u00b7dert", "Wun\u00b7der\u00b7wer\u00b7ke", "l\u00f6\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Er ists, er kann sich nicht verhehlen,", "tokens": ["Er", "ists", ",", "er", "kann", "sich", "nicht", "ver\u00b7heh\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VMFIN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er ist es, Gott von Ewigkeit!", "tokens": ["Er", "ist", "es", ",", "Gott", "von", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich schw\u00f6r es bey den gro\u00dfen Seelen,", "tokens": ["Ich", "schw\u00f6r", "es", "bey", "den", "gro\u00b7\u00dfen", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den M\u00e4rtyrern der alten Zeit,", "tokens": ["Den", "M\u00e4r\u00b7ty\u00b7rern", "der", "al\u00b7ten", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die sich nach diesem Jesu nannten,", "tokens": ["Die", "sich", "nach", "die\u00b7sem", "Je\u00b7su", "nann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PDAT", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und mit erhabnem Heldenmuth", "tokens": ["Und", "mit", "er\u00b7hab\u00b7nem", "Hel\u00b7den\u00b7muth"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auch auf der Folter, in der Glut,", "tokens": ["Auch", "auf", "der", "Fol\u00b7ter", ",", "in", "der", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verfolgter Christen Gott bekannten!", "tokens": ["Ver\u00b7folg\u00b7ter", "Chris\u00b7ten", "Gott", "be\u00b7kann\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Verehrt, verehrt ihn, alle Lande!", "tokens": ["Ver\u00b7ehrt", ",", "ver\u00b7ehrt", "ihn", ",", "al\u00b7le", "Lan\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "PPER", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Jesus, der im Grabe liegt,", "tokens": ["Der", "Je\u00b7sus", ",", "der", "im", "Gra\u00b7be", "liegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zerbricht des Todes ehrne Bande,", "tokens": ["Zer\u00b7bricht", "des", "To\u00b7des", "ehr\u00b7ne", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lebt ewig, und sein Glaube siegt.", "tokens": ["Lebt", "e\u00b7wig", ",", "und", "sein", "Glau\u00b7be", "siegt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Glaube, diese zarte Pflanze,", "tokens": ["Sein", "Glau\u00b7be", ",", "die\u00b7se", "zar\u00b7te", "Pflan\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gr\u00fcnt aus verstr\u00f6mtem Blut hervor,", "tokens": ["Gr\u00fcnt", "aus", "ver\u00b7str\u00f6m\u00b7tem", "Blut", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und hebt im Sturm das Haupt empor,", "tokens": ["Und", "hebt", "im", "Sturm", "das", "Haupt", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit immer ungeschw\u00e4chtem Glanze.", "tokens": ["Mit", "im\u00b7mer", "un\u00b7ge\u00b7schw\u00e4ch\u00b7tem", "Glan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Was lehnen w\u00fcthende Nerone", "tokens": ["Was", "leh\u00b7nen", "w\u00fct\u00b7hen\u00b7de", "Ne\u00b7ro\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADJA", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Sich wider den Me\u00dfias auf?", "tokens": ["Sich", "wi\u00b7der", "den", "Me\u00b7\u00dfias", "auf", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihr Ungeheuer auf dem Throne;", "tokens": ["Ihr", "Un\u00b7ge\u00b7heu\u00b7er", "auf", "dem", "Thro\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tyrannen, sammelt euch zu Hauf!", "tokens": ["Ty\u00b7ran\u00b7nen", ",", "sam\u00b7melt", "euch", "zu", "Hauf", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo seyd ihr? Doch sie sind verschwunden;", "tokens": ["Wo", "seyd", "ihr", "?", "Doch", "sie", "sind", "ver\u00b7schwun\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und alle Heiden m\u00fcssen sehn,", "tokens": ["Und", "al\u00b7le", "Hei\u00b7den", "m\u00fcs\u00b7sen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df Menschen Gott nicht widerstehn,", "tokens": ["Da\u00df", "Men\u00b7schen", "Gott", "nicht", "wi\u00b7der\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und unser Jesus \u00fcberwunden.", "tokens": ["Und", "un\u00b7ser", "Je\u00b7sus", "\u00fc\u00b7berw\u00b7un\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NE", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}