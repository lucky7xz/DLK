{"textgrid.poem.60554": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der Rekrut", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wehe, wehe dem Rekruten!", "tokens": ["We\u00b7he", ",", "we\u00b7he", "dem", "Rek\u00b7ru\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "J\u00e4mmerliche Weltstatuten!", "tokens": ["J\u00e4m\u00b7mer\u00b7li\u00b7che", "Welt\u00b7sta\u00b7tu\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenig Schlaf auf hartem Kissen,", "tokens": ["We\u00b7nig", "Schlaf", "auf", "har\u00b7tem", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wasser nur auf karge Bissen;", "tokens": ["Was\u00b7ser", "nur", "auf", "kar\u00b7ge", "Bis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+++-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "In so sch\u00f6nen Fr\u00fchlingstagen", "tokens": ["In", "so", "sch\u00f6\u00b7nen", "Fr\u00fch\u00b7lings\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "M\u00f6rderliche Waffen tragen;", "tokens": ["M\u00f6r\u00b7der\u00b7li\u00b7che", "Waf\u00b7fen", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ohne Lust und Liebe springen,", "tokens": ["Oh\u00b7ne", "Lust", "und", "Lie\u00b7be", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie des Drillmanns Worte klingen,", "tokens": ["Wie", "des", "Drill\u00b7manns", "Wor\u00b7te", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "\u00dcber Hecken, Bach und Graben;", "tokens": ["\u00dc\u00b7ber", "He\u00b7cken", ",", "Bach", "und", "Gra\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NE", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Schreiten, trippeln, schwenken, traben,", "tokens": ["Schrei\u00b7ten", ",", "trip\u00b7peln", ",", "schwen\u00b7ken", ",", "tra\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Stillstehn pl\u00f6tzlich ohne Ruck,", "tokens": ["Still\u00b7stehn", "pl\u00f6tz\u00b7lich", "oh\u00b7ne", "Ruck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Und an mir vorbei mit allem Guten", "tokens": ["Und", "an", "mir", "vor\u00b7bei", "mit", "al\u00b7lem", "Gu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ADV", "APPR", "PIS", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Rauscht das Leben, wie des Stromes Fluten", "tokens": ["Rauscht", "das", "Le\u00b7ben", ",", "wie", "des", "Stro\u00b7mes", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PWAV", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Dort am Br\u00fcckennepomuk.", "tokens": ["Dort", "am", "Br\u00fc\u00b7cken\u00b7ne\u00b7po\u00b7muk", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}