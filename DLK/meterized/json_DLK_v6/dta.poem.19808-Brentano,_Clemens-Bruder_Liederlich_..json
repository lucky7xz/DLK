{"dta.poem.19808": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Bruder Liederlich .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bruder Liederlich,               ", "tokens": ["Bru\u00b7der", "Lie\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Was saufst dich so voll?", "tokens": ["Was", "saufst", "dich", "so", "voll", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "O du mein Gott,", "tokens": ["O", "du", "mein", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Was schmeckts mir so wohl.", "tokens": ["Was", "schmeckts", "mir", "so", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Am Montag", "tokens": ["Am", "Mon\u00b7tag"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Mu\u00df versoffen seyn,", "tokens": ["Mu\u00df", "ver\u00b7sof\u00b7fen", "seyn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "VVPP", "VAINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Was Sonntag", "tokens": ["Was", "Sonn\u00b7tag"], "token_info": ["word", "word"], "pos": ["PWS", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Uebrig war vom Wein.", "tokens": ["Ueb\u00b7rig", "war", "vom", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Am Dienstag", "tokens": ["Am", "Diens\u00b7tag"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Schlafen wir bis neun,", "tokens": ["Schla\u00b7fen", "wir", "bis", "neun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "CARD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ihr liebe Br\u00fcder", "tokens": ["Ihr", "lie\u00b7be", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "F\u00fchrt mich zum Wein.", "tokens": ["F\u00fchrt", "mich", "zum", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Am Mittwoch", "tokens": ["Am", "Mitt\u00b7woch"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Ist mitten in der Wochen,", "tokens": ["Ist", "mit\u00b7ten", "in", "der", "Wo\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Haben wir das Fleisch gefressen,", "tokens": ["Ha\u00b7ben", "wir", "das", "Fleisch", "ge\u00b7fres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fre\u00df der Meister die Knochen.", "tokens": ["Fre\u00df", "der", "Meis\u00b7ter", "die", "Kno\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "Am Donnerstag", "tokens": ["Am", "Don\u00b7ners\u00b7tag"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Stehn wir auf um vier,", "tokens": ["Stehn", "wir", "auf", "um", "vier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "CARD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ihr lieben Br\u00fcder,", "tokens": ["Ihr", "lie\u00b7ben", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Kommt mit zum Bier,", "tokens": ["Kommt", "mit", "zum", "Bier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Am Freytag", "tokens": ["Am", "Frey\u00b7tag"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Gehen wir ins Bad,", "tokens": ["Ge\u00b7hen", "wir", "ins", "Bad", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Alle Lumperey", "tokens": ["Al\u00b7le", "Lum\u00b7pe\u00b7rey"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Waschen wir ab.", "tokens": ["Wa\u00b7schen", "wir", "ab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Am Samstag", "tokens": ["Am", "Sams\u00b7tag"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Da wollen wir schaffen,", "tokens": ["Da", "wol\u00b7len", "wir", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Spricht der Meister:", "tokens": ["Spricht", "der", "Meis\u00b7ter", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "K\u00f6nnts bleiben lassen.", "tokens": ["K\u00f6nnts", "blei\u00b7ben", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Am Sonntag", "tokens": ["Am", "Sonn\u00b7tag"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Vor dem Essen", "tokens": ["Vor", "dem", "Es\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Spricht der Meister:", "tokens": ["Spricht", "der", "Meis\u00b7ter", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "\u201ejezt wollen wir rechnen.", "tokens": ["\u201e", "jezt", "wol\u00b7len", "wir", "rech\u00b7nen", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "\u201edie ganze Woche", "tokens": ["\u201e", "die", "gan\u00b7ze", "Wo\u00b7che"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "\u201ehast du gelumpt,", "tokens": ["\u201e", "hast", "du", "ge\u00b7lumpt", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "\u201ehast du gesoffen,", "tokens": ["\u201e", "hast", "du", "ge\u00b7sof\u00b7fen", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "\u201enull f\u00fcr Null geht auf.", "tokens": ["\u201e", "null", "f\u00fcr", "Null", "geht", "auf", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "NE", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Nun will ich nicht mehr leben,", "tokens": ["Nun", "will", "ich", "nicht", "mehr", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit dir Geselle mein.", "tokens": ["Mit", "dir", "Ge\u00b7sel\u00b7le", "mein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Urlaub will ich dir geben,", "tokens": ["Ur\u00b7laub", "will", "ich", "dir", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weil du nicht bleibst daheim.", "tokens": ["Weil", "du", "nicht", "bleibst", "da\u00b7heim", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du hast die sieben Tag", "tokens": ["Du", "hast", "die", "sie\u00b7ben", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Gefeiert mit ", "tokens": ["Ge\u00b7fei\u00b7ert", "mit"], "token_info": ["word", "word"], "pos": ["VVPP", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "So ich nicht leiden mag.", "tokens": ["So", "ich", "nicht", "lei\u00b7den", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "PPER", "PTKNEG", "VVINF", "VMFIN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Gar willig und mit Freuden", "tokens": ["Gar", "wil\u00b7lig", "und", "mit", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Will ich jezt ziehn davon,", "tokens": ["Will", "ich", "jezt", "ziehn", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVFIN", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Will solche Krauter meiden,", "tokens": ["Will", "sol\u00b7che", "Krau\u00b7ter", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dies also machen thun,", "tokens": ["Dies", "al\u00b7so", "ma\u00b7chen", "thun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O Kraut, o Meister Kraut,", "tokens": ["O", "Kraut", ",", "o", "Meis\u00b7ter", "Kraut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Des Tags soll zweymal fressen,", "tokens": ["Des", "Tags", "soll", "zwey\u00b7mal", "fres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In meine zarte Haut.", "tokens": ["In", "mei\u00b7ne", "zar\u00b7te", "Haut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Egyptisch soll dich plagen", "tokens": ["E\u00b7gypt\u00b7isch", "soll", "dich", "pla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Der Sonn und Mondenschein,", "tokens": ["Der", "Sonn", "und", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein B\u00fcndel schwer zu tragen", "tokens": ["Ein", "B\u00fcn\u00b7del", "schwer", "zu", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Soll dir Gesellschaft seyn,", "tokens": ["Soll", "dir", "Ge\u00b7sell\u00b7schaft", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VAINF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Dazu ein schlimmer Weg,", "tokens": ["Da\u00b7zu", "ein", "schlim\u00b7mer", "Weg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Darauf du jezt sollst wandern,", "tokens": ["Da\u00b7rauf", "du", "jezt", "sollst", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "VMFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bis \u00fcber die Schuh im Dreck.", "tokens": ["Bis", "\u00fc\u00b7ber", "die", "Schuh", "im", "Dreck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Wie bist du so vermessen,", "tokens": ["Wie", "bist", "du", "so", "ver\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "H\u00f6r zu du Krauter mein,", "tokens": ["H\u00f6r", "zu", "du", "Krau\u00b7ter", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "NN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du giebst zwar wohl zu fressen,", "tokens": ["Du", "giebst", "zwar", "wohl", "zu", "fres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Viel Supp und wenig Fleisch,", "tokens": ["Viel", "Supp", "und", "we\u00b7nig", "Fleisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und alle Tag zwey Kraut,", "tokens": ["Und", "al\u00b7le", "Tag", "zwey", "Kraut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "CARD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das macht in einem Jahre", "tokens": ["Das", "macht", "in", "ei\u00b7nem", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sieben hundert dreissig Kraut.", "tokens": ["Sie\u00b7ben", "hun\u00b7dert", "dreis\u00b7sig", "Kraut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "CARD", "CARD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Was soll ich dir belohnen,", "tokens": ["Was", "soll", "ich", "dir", "be\u00b7loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn du's verdienest nicht?", "tokens": ["Wenn", "du's", "ver\u00b7die\u00b7nest", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Buckel thust du schonen,", "tokens": ["Den", "Bu\u00b7ckel", "thust", "du", "scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "DastDass dir nicht Weh geschieht;               ", "tokens": ["Dast", "Dass", "dir", "nicht", "Weh", "ge\u00b7schieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PTKNEG", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Thust alle Stund ein Schlag,", "tokens": ["Thust", "al\u00b7le", "Stund", "ein", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die Hand magst nicht aufheben,", "tokens": ["Die", "Hand", "magst", "nicht", "auf\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.7": {"text": "Drum ich dich nimmer mag.", "tokens": ["Drum", "ich", "dich", "nim\u00b7mer", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["PAV", "PPER", "PRF", "ADV", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.15": {"line.1": {"text": "Die Frau hat mich geliebet,", "tokens": ["Die", "Frau", "hat", "mich", "ge\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und auch die Tochter dein,", "tokens": ["Und", "auch", "die", "Toch\u00b7ter", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Abschied mich betr\u00fcbet,", "tokens": ["Der", "Ab\u00b7schied", "mich", "be\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bringt mich in schwere Pein,", "tokens": ["Bringt", "mich", "in", "schwe\u00b7re", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Macht mir mein Herz verwundt,", "tokens": ["Macht", "mir", "mein", "Herz", "ver\u00b7wundt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Wann ich an sie gedenke,", "tokens": ["Wann", "ich", "an", "sie", "ge\u00b7den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.7": {"text": "Und ihren rothen Mund.", "tokens": ["Und", "ih\u00b7ren", "ro\u00b7then", "Mund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Mein Weib kann dir nicht helfen,", "tokens": ["Mein", "Weib", "kann", "dir", "nicht", "hel\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weil sie nicht Meister ist,", "tokens": ["Weil", "sie", "nicht", "Meis\u00b7ter", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df nur die Lieb verwelken,", "tokens": ["La\u00df", "nur", "die", "Lieb", "ver\u00b7wel\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Wann abgereiset bist,", "tokens": ["Wann", "ab\u00b7ge\u00b7rei\u00b7set", "bist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Geh, nimm dein Kleid an Leib,", "tokens": ["Geh", ",", "nimm", "dein", "Kleid", "an", "Leib", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und