{"textgrid.poem.32395": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "73. Auf ebendieselbe", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Chloe, halte deinen Blick", "tokens": ["O", "Chloe", ",", "hal\u00b7te", "dei\u00b7nen", "Blick"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Von diesem Schalke ja zur\u00fcck!", "tokens": ["Von", "die\u00b7sem", "Schal\u00b7ke", "ja", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gesetzt, er w\u00e4r' auch ohne Leben:", "tokens": ["Ge\u00b7setzt", ",", "er", "w\u00e4r'", "auch", "oh\u00b7ne", "Le\u00b7ben", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VAFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was er nicht hat, das kann dein Blick ihm geben.", "tokens": ["Was", "er", "nicht", "hat", ",", "das", "kann", "dein", "Blick", "ihm", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VAFIN", "$,", "PDS", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}