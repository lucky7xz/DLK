{"dta.poem.3816": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Hochzeit-Briefgen.  \n Jm Nahmen eines andern.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Schlaf, Neuverlobtes Paar, in allers\u00fc\u00dfter", "tokens": ["Schlaf", ",", "Neu\u00b7ver\u00b7lob\u00b7tes", "Paar", ",", "in", "al\u00b7ler\u00b7s\u00fc\u00df\u00b7ter"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ruh,", "tokens": ["Ruh", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Und da es itzo kalt, deckt euch fein flei\u00dfig zu,", "tokens": ["Und", "da", "es", "it\u00b7zo", "kalt", ",", "deckt", "euch", "fein", "flei\u00b7\u00dfig", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADJD", "$,", "VVFIN", "PPER", "ADJD", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Verwahrt an eurer Th\u00fcr, so Riegel, Schlo\u00df, als", "tokens": ["Ver\u00b7wahrt", "an", "eu\u00b7rer", "Th\u00fcr", ",", "so", "Rie\u00b7gel", ",", "Schlo\u00df", ",", "als"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "ADV", "NN", "$,", "NN", "$,", "KOUS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "B\u00e4nder,", "tokens": ["B\u00e4n\u00b7der", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Erwuchert immerfort die reichsten Liebes-Pf\u00e4nder;", "tokens": ["Er\u00b7wuc\u00b7hert", "im\u00b7mer\u00b7fort", "die", "reichs\u00b7ten", "Lie\u00b7bes\u00b7P\u00b7f\u00e4n\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jhr spahret keine M\u00fch, so kan ich prophezeyn,", "tokens": ["Ihr", "spah\u00b7ret", "kei\u00b7ne", "M\u00fch", ",", "so", "kan", "ich", "pro\u00b7phe\u00b7zeyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Frucht von eurem Flei\u00df wird bald zu sp\u00fchren", "tokens": ["Die", "Frucht", "von", "eu\u00b7rem", "Flei\u00df", "wird", "bald", "zu", "sp\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "seyn.", "tokens": ["seyn", "."], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Inzwischen will auch ich ein schlechtes Angedencken,", "tokens": ["I\u00b7nzwi\u00b7schen", "will", "auch", "ich", "ein", "schlech\u00b7tes", "An\u00b7ge\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie man es hier gewohnt, zu eurem Feste schencken.", "tokens": ["Wie", "man", "es", "hier", "ge\u00b7wohnt", ",", "zu", "eu\u00b7rem", "Fes\u00b7te", "schen\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ADV", "VVPP", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ist gleich dasselbe arm, so ist mein Wunsch doch reich,", "tokens": ["Ist", "gleich", "das\u00b7sel\u00b7be", "arm", ",", "so", "ist", "mein", "Wunsch", "doch", "reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PDAT", "ADJD", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Schlaft wohl, ", "tokens": ["Schlaft", "wohl", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "mehret euch.", "tokens": ["meh\u00b7ret", "euch", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}