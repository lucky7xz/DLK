{"dta.poem.3123": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "168.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verzeiht, was ich gefehlt, ich hab' es gut gemeint,", "tokens": ["Ver\u00b7zeiht", ",", "was", "ich", "ge\u00b7fehlt", ",", "ich", "hab'", "es", "gut", "ge\u00b7meint", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "VVPP", "$,", "PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich euch nichts verhehlt, was meinem Geist erscheint.", "tokens": ["Da\u00df", "ich", "euch", "nichts", "ver\u00b7hehlt", ",", "was", "mei\u00b7nem", "Geist", "er\u00b7scheint", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIS", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ihr m\u00f6gt es anders sehn, im eignen Licht erwacht;", "tokens": ["Ihr", "m\u00f6gt", "es", "an\u00b7ders", "sehn", ",", "im", "eig\u00b7nen", "Licht", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich freue mich, wenn ihr nur auf die Augen macht.", "tokens": ["Ich", "freu\u00b7e", "mich", ",", "wenn", "ihr", "nur", "auf", "die", "Au\u00b7gen", "macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}