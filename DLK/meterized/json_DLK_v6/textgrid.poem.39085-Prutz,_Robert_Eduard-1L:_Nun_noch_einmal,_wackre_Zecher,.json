{"textgrid.poem.39085": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun noch einmal, wackre Zecher,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun noch einmal, wackre Zecher,", "tokens": ["Nun", "noch", "ein\u00b7mal", ",", "wack\u00b7re", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "f\u00fcllet einmal noch die Becher,", "tokens": ["f\u00fcl\u00b7let", "ein\u00b7mal", "noch", "die", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "f\u00fcllt sie sch\u00e4umend bis zum Rand:", "tokens": ["f\u00fcllt", "sie", "sch\u00e4u\u00b7mend", "bis", "zum", "Rand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser letzte Becher allen,", "tokens": ["Die\u00b7ser", "letz\u00b7te", "Be\u00b7cher", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "die als Opfer sind gefallen", "tokens": ["die", "als", "Op\u00b7fer", "sind", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "KOUS", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "f\u00fcr das hei\u00dfgeliebte Vaterland!", "tokens": ["f\u00fcr", "das", "hei\u00df\u00b7ge\u00b7lieb\u00b7te", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Selig, die den Tod gefunden", "tokens": ["Se\u00b7lig", ",", "die", "den", "Tod", "ge\u00b7fun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "unter Leichen, unter Wunden", "tokens": ["un\u00b7ter", "Lei\u00b7chen", ",", "un\u00b7ter", "Wun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "auf dem offnen Siegesplan!", "tokens": ["auf", "dem", "off\u00b7nen", "Sie\u00b7ges\u00b7plan", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese wohl sind zu beneiden,", "tokens": ["Die\u00b7se", "wohl", "sind", "zu", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "denn sie durften hoffend scheiden,", "tokens": ["denn", "sie", "durf\u00b7ten", "hof\u00b7fend", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "eh' die Schmach sie unsrer Tage sahn.", "tokens": ["eh'", "die", "Schmach", "sie", "uns\u00b7rer", "Ta\u00b7ge", "sahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Doch mit schwerverhaltner Tr\u00e4ne", "tokens": ["Doch", "mit", "schwer\u00b7ver\u00b7halt\u00b7ner", "Tr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wir gedenken auch an jene,", "tokens": ["wir", "ge\u00b7den\u00b7ken", "auch", "an", "je\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PDS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "die der Kerker uns entrafft;", "tokens": ["die", "der", "Ker\u00b7ker", "uns", "en\u00b7trafft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "die wie Blumen tun im Sande,", "tokens": ["die", "wie", "Blu\u00b7men", "tun", "im", "San\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "NN", "VVINF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "welkten in dem Druck der Bande,", "tokens": ["welk\u00b7ten", "in", "dem", "Druck", "der", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "in dem Elend der Gefangenschaft!", "tokens": ["in", "dem", "E\u00b7lend", "der", "Ge\u00b7fan\u00b7gen\u00b7schaft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Oder die in \u00f6der Ferne,", "tokens": ["O\u00b7der", "die", "in", "\u00f6\u00b7der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "unter fremdem, kaltem Sterne,", "tokens": ["un\u00b7ter", "frem\u00b7dem", ",", "kal\u00b7tem", "Ster\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "einsam starben und verbannt!", "tokens": ["ein\u00b7sam", "star\u00b7ben", "und", "ver\u00b7bannt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die in Sehnsucht sich verzehrten,", "tokens": ["Die", "in", "Sehn\u00b7sucht", "sich", "ver\u00b7zehr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "die den letzten Blick noch kehrten", "tokens": ["die", "den", "letz\u00b7ten", "Blick", "noch", "kehr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "nach dem teuren, undankbaren Land! \u2013", "tokens": ["nach", "dem", "teu\u00b7ren", ",", "un\u00b7dank\u00b7ba\u00b7ren", "Land", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Schweb, o schweb mit leisen Fl\u00fcgeln,", "tokens": ["Schweb", ",", "o", "schweb", "mit", "lei\u00b7sen", "Fl\u00fc\u00b7geln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "schweb, o Lied, zu allen H\u00fcgeln,", "tokens": ["schweb", ",", "o", "Lied", ",", "zu", "al\u00b7len", "H\u00fc\u00b7geln", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wo der Teuren Asche ruht:", "tokens": ["wo", "der", "Teu\u00b7ren", "A\u00b7sche", "ruht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie auch sind f\u00fcr uns gestorben,", "tokens": ["Sie", "auch", "sind", "f\u00fcr", "uns", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "sie auch haben mitgeworben", "tokens": ["sie", "auch", "ha\u00b7ben", "mit\u00b7ge\u00b7wor\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "f\u00fcr des Lebens allerh\u00f6chstes Gut.", "tokens": ["f\u00fcr", "des", "Le\u00b7bens", "al\u00b7ler\u00b7h\u00f6chs\u00b7tes", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Sag, da\u00df, wie in ihren Tagen,", "tokens": ["Sag", ",", "da\u00df", ",", "wie", "in", "ih\u00b7ren", "Ta\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "$,", "PWAV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "auch noch jetzt die Herzen schlagen", "tokens": ["auch", "noch", "jetzt", "die", "Her\u00b7zen", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "f\u00fcr die Freiheit stolz und hei\u00df;", "tokens": ["f\u00fcr", "die", "Frei\u00b7heit", "stolz", "und", "hei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "sag, da\u00df, \u00e4hnlich ihrer Tugend,", "tokens": ["sag", ",", "da\u00df", ",", "\u00e4hn\u00b7lich", "ih\u00b7rer", "Tu\u00b7gend", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "$,", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "auch noch heute Deutschlands Jugend", "tokens": ["auch", "noch", "heu\u00b7te", "Deutschlands", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "f\u00fcr das Vaterland zu sterben wei\u00df! \u2013", "tokens": ["f\u00fcr", "das", "Va\u00b7ter\u00b7land", "zu", "ster\u00b7ben", "wei\u00df", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Horch, welch Brausen in den L\u00fcften!", "tokens": ["Horch", ",", "welch", "Brau\u00b7sen", "in", "den", "L\u00fcf\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus den Gr\u00e4bern, aus den Gr\u00fcften", "tokens": ["Aus", "den", "Gr\u00e4\u00b7bern", ",", "aus", "den", "Gr\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Flammen schlagen hell hervor:", "tokens": ["Flam\u00b7men", "schla\u00b7gen", "hell", "her\u00b7vor", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kampfplatz ist die Welt geworden,", "tokens": ["Kampf\u00b7platz", "ist", "die", "Welt", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schrecken fa\u00dft die feilen Horden,", "tokens": ["Schre\u00b7cken", "fa\u00dft", "die", "fei\u00b7len", "Hor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und der Freiheit Sonne schwebt empor!", "tokens": ["und", "der", "Frei\u00b7heit", "Son\u00b7ne", "schwebt", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}