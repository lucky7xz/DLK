{"textgrid.poem.33361": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Empfindungen in dem neu angelegten Lustgarten Sr. Excellenz des Grafen von Kobenzl", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Garten hat mich je vergn\u00fcgt,", "tokens": ["Kein", "Gar\u00b7ten", "hat", "mich", "je", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo jedes Spr\u00f6\u00dfchen, das kaum aufgeschossen,", "tokens": ["Wo", "je\u00b7des", "Spr\u00f6\u00df\u00b7chen", ",", "das", "kaum", "auf\u00b7ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch schon in festen Windeln liegt,", "tokens": ["Auch", "schon", "in", "fes\u00b7ten", "Win\u00b7deln", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Blumen nur auf Schnirkelbeeten sprossen;", "tokens": ["Und", "Blu\u00b7men", "nur", "auf", "Schnir\u00b7kel\u00b7bee\u00b7ten", "spros\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wo man mit einer Schnur Alleen mi\u00dft;", "tokens": ["Wo", "man", "mit", "ei\u00b7ner", "Schnur", "Al\u00b7leen", "mi\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und jedes B\u00e4umchen ein Verschnitt'ner ist;", "tokens": ["Und", "je\u00b7des", "B\u00e4um\u00b7chen", "ein", "Ver\u00b7schnitt'\u00b7ner", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wo man das Wasser tanzen lehret,", "tokens": ["Wo", "man", "das", "Was\u00b7ser", "tan\u00b7zen", "leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und ihm den Lauf durch Marmorbecken wehret,", "tokens": ["Und", "ihm", "den", "Lauf", "durch", "Mar\u00b7mor\u00b7be\u00b7cken", "weh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wo man statt Blumen Kiesel s\u00e4't,", "tokens": ["Wo", "man", "statt", "Blu\u00b7men", "Kie\u00b7sel", "s\u00e4't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und die Natur im Fischbeinrocke geht;", "tokens": ["Und", "die", "Na\u00b7tur", "im", "Fischbein\u00b7ro\u00b7cke", "geht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Wo nur die Kunst allein regieret,", "tokens": ["Wo", "nur", "die", "Kunst", "al\u00b7lein", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und ihre Meisterin am G\u00e4ngelbande f\u00fchret. \u2013", "tokens": ["Und", "ih\u00b7re", "Meis\u00b7te\u00b7rin", "am", "G\u00e4n\u00b7gel\u00b7ban\u00b7de", "f\u00fch\u00b7ret", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nicht so der Ort, Erlauchter, den dein Ruf", "tokens": ["Nicht", "so", "der", "Ort", ",", "Er\u00b7lauch\u00b7ter", ",", "den", "dein", "Ruf"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "NN", "$,", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Zu einem zweiten Eden schuf.", "tokens": ["Zu", "ei\u00b7nem", "zwei\u00b7ten", "E\u00b7den", "schuf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Hier dient die Kunst gleich einem Knechte,", "tokens": ["Hier", "dient", "die", "Kunst", "gleich", "ei\u00b7nem", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und l\u00e4sset der Natur die Rechte;", "tokens": ["Und", "l\u00e4s\u00b7set", "der", "Na\u00b7tur", "die", "Rech\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Sie lie\u00df ihr jeden Reiz, den ihr der Sch\u00f6pfer gab,", "tokens": ["Sie", "lie\u00df", "ihr", "je\u00b7den", "Reiz", ",", "den", "ihr", "der", "Sch\u00f6p\u00b7fer", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und fegte nur, was sie entstaltet, ab.", "tokens": ["Und", "feg\u00b7te", "nur", ",", "was", "sie", "ent\u00b7stal\u00b7tet", ",", "ab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PRELS", "PPER", "VVPP", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "O freue dich, Natur, dein Liebling wollte,", "tokens": ["O", "freu\u00b7e", "dich", ",", "Na\u00b7tur", ",", "dein", "Lieb\u00b7ling", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "NN", "$,", "PPOSAT", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Da\u00df man, nachdem die Kunst das Ihrige gethan,", "tokens": ["Da\u00df", "man", ",", "nach\u00b7dem", "die", "Kunst", "das", "Ih\u00b7ri\u00b7ge", "ge\u00b7than", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "KOUS", "ART", "NN", "ART", "PPOSS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Nur dich allein noch sehen sollte.", "tokens": ["Nur", "dich", "al\u00b7lein", "noch", "se\u00b7hen", "soll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Und wenn die Kunst \u2013 hier eine eb'ne Bahn,", "tokens": ["Und", "wenn", "die", "Kunst", "\u2013", "hier", "ei\u00b7ne", "eb'\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$(", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Dort einen Teich mit kleinen Wasserf\u00e4llen,", "tokens": ["Dort", "ei\u00b7nen", "Teich", "mit", "klei\u00b7nen", "Was\u00b7ser\u00b7f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Da eine Grotte, dort bequeme Ruhestellen,", "tokens": ["Da", "ei\u00b7ne", "Grot\u00b7te", ",", "dort", "be\u00b7que\u00b7me", "Ru\u00b7he\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und hier ein Baumbouquet dir abgewann,", "tokens": ["Und", "hier", "ein", "Baum\u00b7bou\u00b7quet", "dir", "ab\u00b7ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Sieh, so verbarg sie doch bescheiden vor dem Blicke", "tokens": ["Sieh", ",", "so", "ver\u00b7barg", "sie", "doch", "be\u00b7schei\u00b7den", "vor", "dem", "Bli\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Des Forschers sich, und lie\u00df nur dich allein zur\u00fccke.", "tokens": ["Des", "For\u00b7schers", "sich", ",", "und", "lie\u00df", "nur", "dich", "al\u00b7lein", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "$,", "KON", "VVFIN", "ADV", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Was dir dein Liebling zur Versch\u00f6n'rung gab,", "tokens": ["Was", "dir", "dein", "Lieb\u00b7ling", "zur", "Ver\u00b7sch\u00f6n'\u00b7rung", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Die schattichten verschlungnen Pfade,", "tokens": ["Die", "schat\u00b7tich\u00b7ten", "ver\u00b7schlung\u00b7nen", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.30": {"text": "Um jeden Teich' die lachenden Gestade,", "tokens": ["Um", "je\u00b7den", "Teich'", "die", "la\u00b7chen\u00b7den", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Gel\u00e4nder, wo ein Weidenstab", "tokens": ["Ge\u00b7l\u00e4n\u00b7der", ",", "wo", "ein", "Wei\u00b7den\u00b7stab"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Sich in den andern schlingt, die Br\u00fccke, deren St\u00fctze", "tokens": ["Sich", "in", "den", "an\u00b7dern", "schlingt", ",", "die", "Br\u00fc\u00b7cke", ",", "de\u00b7ren", "St\u00fct\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PRF", "APPR", "ART", "ADJA", "VVFIN", "$,", "ART", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Lebend'ge B\u00e4ume sind, die weichen Rasensitze,", "tokens": ["Le\u00b7ben\u00b7d'\u00b7ge", "B\u00e4u\u00b7me", "sind", ",", "die", "wei\u00b7chen", "Ra\u00b7sen\u00b7sit\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.34": {"text": "Den Reiz der Aussicht, den er ihnen gab,", "tokens": ["Den", "Reiz", "der", "Aus\u00b7sicht", ",", "den", "er", "ih\u00b7nen", "gab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Dies alles \u2013 sah er dir nur ab.", "tokens": ["Dies", "al\u00b7les", "\u2013", "sah", "er", "dir", "nur", "ab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "$(", "VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Nicht Reize neuer Kunst, nein, nur die deinen", "tokens": ["Nicht", "Rei\u00b7ze", "neu\u00b7er", "Kunst", ",", "nein", ",", "nur", "die", "dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "NN", "ADJA", "NN", "$,", "PTKANT", "$,", "ADV", "ART", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Wollt' er auf seinen H\u00fcgeln sich vereinen.", "tokens": ["Wollt'", "er", "auf", "sei\u00b7nen", "H\u00fc\u00b7geln", "sich", "ver\u00b7ei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "So liebt er dich, und du liebst ihn,", "tokens": ["So", "liebt", "er", "dich", ",", "und", "du", "liebst", "ihn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "KON", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Und schenkst ihm jede deiner stillen Freuden,", "tokens": ["Und", "schenkst", "ihm", "je\u00b7de", "dei\u00b7ner", "stil\u00b7len", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Die t\u00e4glich ihn in deine Arme zieh'n,", "tokens": ["Die", "t\u00e4g\u00b7lich", "ihn", "in", "dei\u00b7ne", "Ar\u00b7me", "zieh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Und l\u00e4ssest ihn von dir nie unerquicket scheiden.", "tokens": ["Und", "l\u00e4s\u00b7sest", "ihn", "von", "dir", "nie", "un\u00b7er\u00b7quic\u00b7ket", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Dich, Gl\u00fcckliche! die du in dessen H\u00e4nden bist,", "tokens": ["Dich", ",", "Gl\u00fcck\u00b7li\u00b7che", "!", "die", "du", "in", "des\u00b7sen", "H\u00e4n\u00b7den", "bist", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$.", "PRELS", "PPER", "APPR", "PRELAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Der dich zugleich versch\u00f6nert \u2013 und genie\u00dft.", "tokens": ["Der", "dich", "zu\u00b7gleich", "ver\u00b7sch\u00f6\u00b7nert", "\u2013", "und", "ge\u00b7nie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$(", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}