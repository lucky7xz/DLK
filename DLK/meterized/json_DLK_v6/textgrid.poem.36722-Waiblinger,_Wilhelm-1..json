{"textgrid.poem.36722": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es giebt Seelen, doch wen'ge, die, reiner als andre, vom Urquell", "tokens": ["Es", "giebt", "See\u00b7len", ",", "doch", "wen'\u00b7ge", ",", "die", ",", "rei\u00b7ner", "als", "and\u00b7re", ",", "vom", "Ur\u00b7quell"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "ADV", "VVFIN", "$,", "PRELS", "$,", "ADJA", "KOUS", "PIS", "$,", "APPRART", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Sich, vom unendlichen Grund alles Lebend'gen, gel\u00f6st.", "tokens": ["Sich", ",", "vom", "un\u00b7end\u00b7li\u00b7chen", "Grund", "al\u00b7les", "Le\u00b7ben\u00b7d'\u00b7gen", ",", "ge\u00b7l\u00f6st", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "$,", "APPRART", "ADJA", "NN", "PIAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Jedes R\u00e4thsel der Welt es scheint in ihnen gefunden,", "tokens": ["Je\u00b7des", "R\u00e4th\u00b7sel", "der", "Welt", "es", "scheint", "in", "ih\u00b7nen", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "PPER", "VVFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Jeglicher Widerstreit hold und entz\u00fcckend vers\u00f6hnt.", "tokens": ["Jeg\u00b7li\u00b7cher", "Wi\u00b7der\u00b7streit", "hold", "und", "ent\u00b7z\u00fc\u00b7ckend", "ver\u00b7s\u00f6hnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.5": {"text": "Nimmer tr\u00fcbt sich in ihnen die \u00fcbernat\u00fcrliche Klarheit,", "tokens": ["Nim\u00b7mer", "tr\u00fcbt", "sich", "in", "ih\u00b7nen", "die", "\u00fc\u00b7bern\u00b7a\u00b7t\u00fcr\u00b7li\u00b7che", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Und doch sind sie wohl nie sich ihrer Allmacht bewu\u00dft.", "tokens": ["Und", "doch", "sind", "sie", "wohl", "nie", "sich", "ih\u00b7rer", "All\u00b7macht", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ADV", "PRF", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+---+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Keines Zweifels erzitternder Hauch regt die liebliche Tiefe", "tokens": ["Kei\u00b7nes", "Zwei\u00b7fels", "er\u00b7zit\u00b7tern\u00b7der", "Hauch", "regt", "die", "lieb\u00b7li\u00b7che", "Tie\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+--++-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Ihres Innern, es ruht stille der Himmel auf ihm.", "tokens": ["Ih\u00b7res", "In\u00b7nern", ",", "es", "ruht", "stil\u00b7le", "der", "Him\u00b7mel", "auf", "ihm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "+-+--++--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Aehnlich sind sie dem Herrn, der die ungemessenen Kr\u00e4fte", "tokens": ["A\u00b7ehn\u00b7lich", "sind", "sie", "dem", "Herrn", ",", "der", "die", "un\u00b7ge\u00b7mes\u00b7se\u00b7nen", "Kr\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "---+--+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Seiner Natur oft im Bild bl\u00fchender Rosen verh\u00fcllt.", "tokens": ["Sei\u00b7ner", "Na\u00b7tur", "oft", "im", "Bild", "bl\u00fc\u00b7hen\u00b7der", "Ro\u00b7sen", "ver\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPRART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Ja sie schaffen wie er! Nicht im Wirbel des Sturms, in des Fr\u00fchlings", "tokens": ["Ja", "sie", "schaf\u00b7fen", "wie", "er", "!", "Nicht", "im", "Wir\u00b7bel", "des", "Sturms", ",", "in", "des", "Fr\u00fch\u00b7lings"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VVFIN", "KOKOM", "PPER", "$.", "PTKNEG", "APPRART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Sanft holdseliger Lust spro\u00dft und erschlie\u00dft sich der Keim,", "tokens": ["Sanft", "hold\u00b7se\u00b7li\u00b7ger", "Lust", "spro\u00dft", "und", "er\u00b7schlie\u00dft", "sich", "der", "Keim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Der sich zur F\u00fclle der Frucht in frischer Gesundheit erschwellet.", "tokens": ["Der", "sich", "zur", "F\u00fcl\u00b7le", "der", "Frucht", "in", "fri\u00b7scher", "Ge\u00b7sund\u00b7heit", "er\u00b7schwel\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Nur in der Zephire Wehn reift sie vollendet heran.", "tokens": ["Nur", "in", "der", "Ze\u00b7phi\u00b7re", "Wehn", "reift", "sie", "voll\u00b7en\u00b7det", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVFIN", "PPER", "VVPP", "PTKVZ", "$."], "meter": "-+--+-++--+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "So ihr ruhiges Wirken. Wie all' ihr Wesen nur Einheit,", "tokens": ["So", "ihr", "ru\u00b7hi\u00b7ges", "Wir\u00b7ken", ".", "Wie", "all'", "ihr", "We\u00b7sen", "nur", "Ein\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$.", "PWAV", "PIS", "PPOSAT", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Wie selbst die fl\u00fcchtige Welt ihnen harmonisch erscheint,", "tokens": ["Wie", "selbst", "die", "fl\u00fcch\u00b7ti\u00b7ge", "Welt", "ih\u00b7nen", "har\u00b7mo\u00b7nisch", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "So am geheimen Punkt, aus dem in vollkommenem Gleichmaa\u00df", "tokens": ["So", "am", "ge\u00b7hei\u00b7men", "Punkt", ",", "aus", "dem", "in", "voll\u00b7kom\u00b7me\u00b7nem", "Gleich\u00b7maa\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,", "APPR", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Sich der entwickelte Stoff rein und gesondert belebt,", "tokens": ["Sich", "der", "ent\u00b7wi\u00b7ckel\u00b7te", "Stoff", "rein", "und", "ge\u00b7son\u00b7dert", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "ADJD", "KON", "VVPP", "VVPP", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "So das erstehende Werk erfassen sie auch, und bescheiden", "tokens": ["So", "das", "er\u00b7ste\u00b7hen\u00b7de", "Werk", "er\u00b7fas\u00b7sen", "sie", "auch", ",", "und", "be\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,", "KON", "ADJA"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "Zeigt es sich jeglichem Blick, aber es reizt nicht, es ist.", "tokens": ["Zeigt", "es", "sich", "jeg\u00b7li\u00b7chem", "Blick", ",", "a\u00b7ber", "es", "reizt", "nicht", ",", "es", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PIAT", "NN", "$,", "KON", "PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VAFIN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Nicht im \u00fcppig erquellenden Werden, im schmachtenden Welken,", "tokens": ["Nicht", "im", "\u00fcp\u00b7pig", "er\u00b7quel\u00b7len\u00b7den", "Wer\u00b7den", ",", "im", "schmach\u00b7ten\u00b7den", "Wel\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "ADJD", "VVINF", "VAFIN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Stellen sie's eben wo's ist, wo es entfaltet ist, dar.", "tokens": ["Stel\u00b7len", "sie's", "e\u00b7ben", "wo's", "ist", ",", "wo", "es", "ent\u00b7fal\u00b7tet", "ist", ",", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "NE", "ADV", "NE", "VAFIN", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$,", "PTKVZ", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Drum ist ihr Werk das H\u00f6chste: doch jene Sch\u00f6pfung der Einheit", "tokens": ["Drum", "ist", "ihr", "Werk", "das", "H\u00f6chs\u00b7te", ":", "doch", "je\u00b7ne", "Sch\u00f6p\u00b7fung", "der", "Ein\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPOSAT", "NN", "ART", "ADJA", "$.", "ADV", "PDAT", "NN", "ART", "NN"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Nennet man sch\u00f6n, die Idee, die sie beseligend weckt,", "tokens": ["Nen\u00b7net", "man", "sch\u00f6n", ",", "die", "I\u00b7dee", ",", "die", "sie", "be\u00b7se\u00b7li\u00b7gend", "weckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "VVFIN", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.19": {"text": "Nennt man Sch\u00f6nheit, und so, o Raffael Sanzio, bist du", "tokens": ["Nennt", "man", "Sch\u00f6n\u00b7heit", ",", "und", "so", ",", "o", "Raf\u00b7fael", "San\u00b7zio", ",", "bist", "du"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIS", "NN", "$,", "KON", "ADV", "$,", "FM", "NE", "NE", "$,", "VAFIN", "PPER"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.20": {"text": "Der vollendetste mir, weil du der sch\u00f6nste mir bist.", "tokens": ["Der", "voll\u00b7en\u00b7dets\u00b7te", "mir", ",", "weil", "du", "der", "sch\u00f6ns\u00b7te", "mir", "bist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "$,", "KOUS", "PPER", "ART", "ADJA", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}