{"dta.poem.5488": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Lob GOttes bey Betrachtung seiner  \n Wercke.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Quell aller sch\u00f6nen Creaturen!", "tokens": ["Quell", "al\u00b7ler", "sch\u00f6\u00b7nen", "Crea\u00b7tu\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Ich seh von deiner weisen Macht", "tokens": ["Ich", "seh", "von", "dei\u00b7ner", "wei\u00b7sen", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An allen Orten Wunder-Spuren,", "tokens": ["An", "al\u00b7len", "Or\u00b7ten", "Wun\u00b7der\u00b7Spu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Indem ich, wo ich geh\u2019 und stehe,", "tokens": ["In\u00b7dem", "ich", ",", "wo", "ich", "geh'", "und", "ste\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wohin ich meine Blicke drehe,", "tokens": ["Wo\u00b7hin", "ich", "mei\u00b7ne", "Bli\u00b7cke", "dre\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Am Himmel an der Sonnen Glut,", "tokens": ["Am", "Him\u00b7mel", "an", "der", "Son\u00b7nen", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf Erden in der Luft und Fluth,", "tokens": ["Auf", "Er\u00b7den", "in", "der", "Luft", "und", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Ausbruch deiner Liebe sehe;", "tokens": ["Den", "Aus\u00b7bruch", "dei\u00b7ner", "Lie\u00b7be", "se\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Daher ich, weil in deinen Wercken", "tokens": ["Da\u00b7her", "ich", ",", "weil", "in", "dei\u00b7nen", "Wer\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Du gegenw\u00e4rtig zu bemercken;", "tokens": ["Du", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "zu", "be\u00b7mer\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "In ihnen billig dich erh\u00f6he.", "tokens": ["In", "ih\u00b7nen", "bil\u00b7lig", "dich", "er\u00b7h\u00f6\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}