", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PRF", "ADV", "$(", "VVPP", "KON", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Wie schickt sich wohl dein weh zu ihren anmuths-reisen?", "tokens": ["Wie", "schickt", "sich", "wohl", "dein", "weh", "zu", "ih\u00b7ren", "an\u00b7muths\u00b7rei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ADV", "PPOSAT", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Und ihre grausamkeit zu deiner seelen-pein?", "tokens": ["Und", "ih\u00b7re", "grau\u00b7sam\u00b7keit", "zu", "dei\u00b7ner", "see\u00b7len\u00b7pein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPOSAT", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Armseelger/ glaube nicht/ Melinde werd dich lieben/", "tokens": ["Arm\u00b7seel\u00b7ger", "/", "glau\u00b7be", "nicht", "/", "Me\u00b7lin\u00b7de", "werd", "dich", "lie\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "PTKNEG", "$(", "NE", "VAFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Nein/ nein/ nur mache dich von solchem wahn befreyt/", "tokens": ["Nein", "/", "nein", "/", "nur", "ma\u00b7che", "dich", "von", "sol\u00b7chem", "wahn", "be\u00b7freyt", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PTKANT", "$(", "ADV", "VVFIN", "PRF", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Sie weis von deiner noth/ sie kennet dein bet