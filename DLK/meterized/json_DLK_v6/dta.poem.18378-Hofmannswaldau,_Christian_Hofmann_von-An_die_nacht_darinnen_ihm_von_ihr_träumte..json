{"dta.poem.18378": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An die nacht/ darinnen ihm von ihr  \n tr\u00e4umte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seyd tausendmahl begr\u00fcsst/ ihr tunckle-reiche schatten/", "tokens": ["Seyd", "tau\u00b7send\u00b7mahl", "be\u00b7gr\u00fcs\u00b7st", "/", "ihr", "tun\u00b7ck\u00b7le\u00b7rei\u00b7che", "schat\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "VVFIN", "$(", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der himmel kr\u00f6ne dich/ du gold-gestirnte Nacht/", "tokens": ["Der", "him\u00b7mel", "kr\u00f6\u00b7ne", "dich", "/", "du", "gold\u00b7ge\u00b7stirn\u00b7te", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil du mein helles licht mir wieder wilt erstatten/", "tokens": ["Weil", "du", "mein", "hel\u00b7les", "licht", "mir", "wie\u00b7der", "wilt", "er\u00b7stat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das mir ein schn\u00f6der tag so finster hat gemacht/", "tokens": ["Das", "mir", "ein", "schn\u00f6\u00b7der", "tag", "so", "fins\u00b7ter", "hat", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "ADJA", "NN", "ADV", "ADJD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja wohl mit recht ist der ein schn\u00f6der tag zu nennen/", "tokens": ["Ja", "wohl", "mit", "recht", "ist", "der", "ein", "schn\u00f6\u00b7der", "tag", "zu", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "APPR", "ADJD", "VAFIN", "ART", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der meinen leitstern mir/ Melinden/ hat entf\u00fchrt/", "tokens": ["Der", "mei\u00b7nen", "leits\u00b7tern", "mir", "/", "Me\u00b7lin\u00b7den", "/", "hat", "ent\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "$(", "NE", "$(", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der meine gluth erstickt/ darin ich pflag zu brennen", "tokens": ["Der", "mei\u00b7ne", "gluth", "er\u00b7stickt", "/", "da\u00b7rin", "ich", "pflag", "zu", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$(", "PAV", "PPER", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und allen schmuck versehrt/ womit ich ausgeziert.", "tokens": ["Und", "al\u00b7len", "schmuck", "ver\u00b7sehrt", "/", "wo\u00b7mit", "ich", "aus\u00b7ge\u00b7ziert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$(", "PWAV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch was der tag verbrach/ wilt du/ o nacht! ersetzen/", "tokens": ["Doch", "was", "der", "tag", "ver\u00b7brach", "/", "wilt", "du", "/", "o", "nacht", "!", "er\u00b7set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$(", "VMFIN", "PPER", "$(", "FM", "NN", "$.", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Er ri\u00df Melindem weg/ du aber bringst sie mir/", "tokens": ["Er", "ri\u00df", "Me\u00b7lin\u00b7dem", "weg", "/", "du", "a\u00b7ber", "bringst", "sie", "mir", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "PTKVZ", "$(", "PPER", "ADV", "VVFIN", "PPER", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und da er als ein feind zerst\u00f6rte mein erg\u00f6tzen/", "tokens": ["Und", "da", "er", "als", "ein", "feind", "zer\u00b7st\u00f6r\u00b7te", "mein", "er\u00b7g\u00f6t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "KOUS", "ART", "NN", "VVFIN", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So stellstu mirs im traum noch eins so lieblich f\u00fcr;", "tokens": ["So", "stell\u00b7stu", "mirs", "im", "traum", "noch", "eins", "so", "lieb\u00b7lich", "f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPRART", "NN", "ADV", "PIS", "ADV", "ADJD", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ach angenehmster traum/ der mir mein leben zeiget/", "tokens": ["Ach", "an\u00b7ge\u00b7nehms\u00b7ter", "traum", "/", "der", "mir", "mein", "le\u00b7ben", "zei\u00b7get", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$(", "PRELS", "PPER", "PPOSAT", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der mich/ und was ich bin mit stiller lust umbschliest/", "tokens": ["Der", "mich", "/", "und", "was", "ich", "bin", "mit", "stil\u00b7ler", "lust", "umbsc\u00b7hliest", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$(", "KON", "PWS", "PPER", "VAFIN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der die vergn\u00fcgung selbst auf meine Scheitel neiget/", "tokens": ["Der", "die", "ver\u00b7gn\u00fc\u00b7gung", "selbst", "auf", "mei\u00b7ne", "Schei\u00b7tel", "nei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und mein verg\u00e4lltes gl\u00fcck so z\u00e4rtlich mir vers\u00fcst/", "tokens": ["Und", "mein", "ver\u00b7g\u00e4ll\u00b7tes", "gl\u00fcck", "so", "z\u00e4rt\u00b7lich", "mir", "ver\u00b7s\u00fcst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie artig bringst du mir im schatten doch getragen/", "tokens": ["Wie", "ar\u00b7tig", "bringst", "du", "mir", "im", "schat\u00b7ten", "doch", "ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PPER", "APPRART", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Melindens lieblichkeit und sternen-gleiche pracht.", "tokens": ["Me\u00b7lin\u00b7dens", "lieb\u00b7lich\u00b7keit", "und", "ster\u00b7nen\u00b7glei\u00b7che", "pracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die der vorhaste tag mir gleichwol will versagen/", "tokens": ["Die", "der", "vor\u00b7has\u00b7te", "tag", "mir", "gleich\u00b7wol", "will", "ver\u00b7sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Weil er auf sonsten nichts/ als meinen todt bedacht;", "tokens": ["Weil", "er", "auf", "sons\u00b7ten", "nichts", "/", "als", "mei\u00b7nen", "todt", "be\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "PIS", "$(", "KOUS", "PPOSAT", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie offters pfleg ich nicht Melinden zu erblicken/", "tokens": ["Wie", "off\u00b7ters", "pfleg", "ich", "nicht", "Me\u00b7lin\u00b7den", "zu", "er\u00b7bli\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PTKNEG", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wie pr\u00e4chtig scheint mir nicht ihr holdes angesicht/", "tokens": ["Wie", "pr\u00e4ch\u00b7tig", "scheint", "mir", "nicht", "ihr", "hol\u00b7des", "an\u00b7ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wie h\u00e4ufig pflegt sie doch die glut auf mich zu schicken/", "tokens": ["Wie", "h\u00e4u\u00b7fig", "pflegt", "sie", "doch", "die", "glut", "auf", "mich", "zu", "schi\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die mit verliebten glantz aus ihren augen bricht/", "tokens": ["Die", "mit", "ver\u00b7lieb\u00b7ten", "glantz", "aus", "ih\u00b7ren", "au\u00b7gen", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wie freundlich stelt sie sich mit fingen/ spielen/ lachen/", "tokens": ["Wie", "freund\u00b7lich", "stelt", "sie", "sich", "mit", "fin\u00b7gen", "/", "spie\u00b7len", "/", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PRF", "APPR", "VVFIN", "$(", "VVINF", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und tret ich endlich auch zu ihr was n\u00e4her hin/", "tokens": ["Und", "tret", "ich", "end\u00b7lich", "auch", "zu", "ihr", "was", "n\u00e4\u00b7her", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "PWS", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So sp\u00fcr ich allererst die rechte wunder-sachen/", "tokens": ["So", "sp\u00fcr", "ich", "al\u00b7le\u00b7rerst", "die", "rech\u00b7te", "wun\u00b7der\u00b7sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die durch verborgne krafft bezaubern muth und sinn.", "tokens": ["Die", "durch", "ver\u00b7borg\u00b7ne", "krafft", "be\u00b7zau\u00b7bern", "muth", "und", "sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wir doppeln ungescheut die keuschen liebes-flammen/", "tokens": ["Wir", "dop\u00b7peln", "un\u00b7ge\u00b7scheut", "die", "keu\u00b7schen", "lie\u00b7bes\u00b7flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die weder ha\u00df noch neid durch ihren gifft befleckt/", "tokens": ["Die", "we\u00b7der", "ha\u00df", "noch", "neid", "durch", "ih\u00b7ren", "gifft", "be\u00b7fleckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KON", "VVIMP", "ADV", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und schliessen hand und mund durch einen kus zusammen/", "tokens": ["Und", "schlies\u00b7sen", "hand", "und", "mund", "durch", "ei\u00b7nen", "kus", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der lieblich ist wie sie/ und nach den himmel schmeckt/", "tokens": ["Der", "lieb\u00b7lich", "ist", "wie", "sie", "/", "und", "nach", "den", "him\u00b7mel", "schmeckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "KOKOM", "PPER", "$(", "KON", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ach w\u00e4r es m\u00f6glich doch dis immer zu geniessen/", "tokens": ["Ach", "w\u00e4r", "es", "m\u00f6g\u00b7lich", "doch", "dis", "im\u00b7mer", "zu", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "ADJD", "ADV", "PDS", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Was mir ein sanffter traum so lieblich f\u00fcrgestelt/", "tokens": ["Was", "mir", "ein", "sanff\u00b7ter", "traum", "so", "lieb\u00b7lich", "f\u00fcr\u00b7ge\u00b7stelt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Gewis ich wolt im traum mein leben gar beschliessen/", "tokens": ["Ge\u00b7wis", "ich", "wolt", "im", "traum", "mein", "le\u00b7ben", "gar", "be\u00b7schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VMFIN", "APPRART", "NN", "PPOSAT", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Das ohne diesen trost kaum kaum sich noch erh\u00e4lt.", "tokens": ["Das", "oh\u00b7ne", "die\u00b7sen", "trost", "kaum", "kaum", "sich", "noch", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PDAT", "NN", "ADV", "ADV", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Was aber Phantasie; ihr taumlende gedancken/", "tokens": ["Was", "a\u00b7ber", "Phan\u00b7ta\u00b7sie", ";", "ihr", "taum\u00b7len\u00b7de", "ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "$.", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wo laufft ihr endlich hin? wie sehr verirrt ihr euch!", "tokens": ["Wo", "laufft", "ihr", "end\u00b7lich", "hin", "?", "wie", "sehr", "ver\u00b7irrt", "ihr", "euch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "PWAV", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wie th\u00f6rigt sucht ihr doch in Morpheus blinden schrancken/", "tokens": ["Wie", "th\u00f6\u00b7rigt", "sucht", "ihr", "doch", "in", "Mor\u00b7pheus", "blin\u00b7den", "schran\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "NE", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Den ursprung eurer lust und euer freuden-reich.", "tokens": ["Den", "ur\u00b7sprung", "eu\u00b7rer", "lust", "und", "eu\u00b7er", "freu\u00b7den\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Was sind die tr\u00e4ume doch? ein irrwisch unsrer hertzen/", "tokens": ["Was", "sind", "die", "tr\u00e4u\u00b7me", "doch", "?", "ein", "irr\u00b7wisch", "uns\u00b7rer", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "VVFIN", "ADV", "$.", "ART", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Der mit verlognem schein uns von der warheit f\u00fchrt/", "tokens": ["Der", "mit", "ver\u00b7log\u00b7nem", "schein", "uns", "von", "der", "war\u00b7heit", "f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ein falsches schatten-werck zur mehrung unsrer schmertzen/", "tokens": ["Ein", "fal\u00b7sches", "schat\u00b7ten\u00b7\u00b7werck", "zur", "meh\u00b7rung", "uns\u00b7rer", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Zu blenden die vernunfft die unser thun regiert.", "tokens": ["Zu", "blen\u00b7den", "die", "ver\u00b7nunfft", "die", "un\u00b7ser", "thun", "re\u00b7giert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "ART", "PPOSAT", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Ein zunder/ welcher uns je mehr und mehr entz\u00fcndet/", "tokens": ["E