{"textgrid.poem.51236": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der sonnige Duft, Septemberluft,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der sonnige Duft, Septemberluft,", "tokens": ["Der", "son\u00b7ni\u00b7ge", "Duft", ",", "Sep\u00b7tem\u00b7ber\u00b7luft", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Sie wehten ein M\u00fccklein mir aufs Buch,", "tokens": ["Sie", "weh\u00b7ten", "ein", "M\u00fcck\u00b7lein", "mir", "aufs", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das suchte sich die Ruhegruft", "tokens": ["Das", "such\u00b7te", "sich", "die", "Ru\u00b7he\u00b7gruft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fern vom Wald sein Leichentuch.", "tokens": ["Und", "fern", "vom", "Wald", "sein", "Lei\u00b7chen\u00b7tuch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vier Fl\u00fcgelein von Seiden fein", "tokens": ["Vier", "Fl\u00fc\u00b7ge\u00b7lein", "von", "Sei\u00b7den", "fein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Trug's auf dem R\u00fccken zart,", "tokens": ["Trug's", "auf", "dem", "R\u00fc\u00b7cken", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Drin man in Regenbogenschein", "tokens": ["Drin", "man", "in", "Re\u00b7gen\u00b7bo\u00b7gen\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Spielendes Licht gewahrt'.", "tokens": ["Spie\u00b7len\u00b7des", "Licht", "ge\u00b7wahrt'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Hellgr\u00fcn das schlanke Leibchen war,", "tokens": ["Hell\u00b7gr\u00fcn", "das", "schlan\u00b7ke", "Leib\u00b7chen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Hellgr\u00fcn der F\u00fc\u00dfchen dreifach Paar,", "tokens": ["Hell\u00b7gr\u00fcn", "der", "F\u00fc\u00df\u00b7chen", "drei\u00b7fach", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und auf dem K\u00f6pfchen wundersam", "tokens": ["Und", "auf", "dem", "K\u00f6pf\u00b7chen", "wun\u00b7der\u00b7sam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sa\u00df ein Federb\u00fcschchen stramm;", "tokens": ["Sa\u00df", "ein", "Fe\u00b7der\u00b7b\u00fcschchen", "stramm", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Die \u00c4uglein wie ein goldnes Erz", "tokens": ["Die", "\u00c4ug\u00b7lein", "wie", "ein", "gold\u00b7nes", "Erz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Gl\u00e4nzten mir in das tiefste Herz.", "tokens": ["Gl\u00e4nz\u00b7ten", "mir", "in", "das", "tiefs\u00b7te", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Dies zierliche und manierliche Wesen", "tokens": ["Dies", "zier\u00b7li\u00b7che", "und", "ma\u00b7nier\u00b7li\u00b7che", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Hatt sich zu Gruft und Leichentuch", "tokens": ["Hatt", "sich", "zu", "Gruft", "und", "Lei\u00b7chen\u00b7tuch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Das gl\u00e4nzende Papier erlesen,", "tokens": ["Das", "gl\u00e4n\u00b7zen\u00b7de", "Pa\u00b7pier", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Darin ich las, ein dichterliches Buch.", "tokens": ["Da\u00b7rin", "ich", "las", ",", "ein", "dich\u00b7ter\u00b7li\u00b7ches", "Buch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "So lie\u00df den Band ich aufgeschlagen", "tokens": ["So", "lie\u00df", "den", "Band", "ich", "auf\u00b7ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und sah erstaunt dem Sterben zu,", "tokens": ["Und", "sah", "er\u00b7staunt", "dem", "Ster\u00b7ben", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Wie langsam, langsam ohne Klagen", "tokens": ["Wie", "lang\u00b7sam", ",", "lang\u00b7sam", "oh\u00b7ne", "Kla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Das Tierlein kam zu seiner Ruh.", "tokens": ["Das", "Tier\u00b7lein", "kam", "zu", "sei\u00b7ner", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Drei Tage ging es m\u00fcd und matt", "tokens": ["Drei", "Ta\u00b7ge", "ging", "es", "m\u00fcd", "und", "matt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Umher auf dem Papiere;", "tokens": ["Um\u00b7her", "auf", "dem", "Pa\u00b7pie\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Die Fl\u00fcgelein von Seide fein,", "tokens": ["Die", "Fl\u00fc\u00b7ge\u00b7lein", "von", "Sei\u00b7de", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Sie gl\u00e4nzten alle viere.", "tokens": ["Sie", "gl\u00e4nz\u00b7ten", "al\u00b7le", "vie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Am vierten Tage stand es still", "tokens": ["Am", "vier\u00b7ten", "Ta\u00b7ge", "stand", "es", "still"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Gerade auf dem W\u00f6rtlein \u00bbwill!\u00ab", "tokens": ["Ge\u00b7ra\u00b7de", "auf", "dem", "W\u00f6rt\u00b7lein", "\u00bb", "will", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$(", "VMFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Gar tapfer stand's auf selbem Raum,", "tokens": ["Gar", "tap\u00b7fer", "stan\u00b7d's", "auf", "sel\u00b7bem", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.30": {"text": "Hob je ein F\u00fc\u00dfchen wie im Traum;", "tokens": ["Hob", "je", "ein", "F\u00fc\u00df\u00b7chen", "wie", "im", "Traum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Am f\u00fcnften Tage legt' es sich,", "tokens": ["Am", "f\u00fcnf\u00b7ten", "Ta\u00b7ge", "legt'", "es", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Doch noch am sechsten regt' es sich;", "tokens": ["Doch", "noch", "am", "sechs\u00b7ten", "regt'", "es", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Am siebten endlich siegt' der Tod,", "tokens": ["Am", "sieb\u00b7ten", "end\u00b7lich", "siegt'", "der", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Da war zu Ende seine Not.", "tokens": ["Da", "war", "zu", "En\u00b7de", "sei\u00b7ne", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Nun ruht im Buch sein leicht Gebein.", "tokens": ["Nun", "ruht", "im", "Buch", "sein", "leicht", "Ge\u00b7bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "M\u00f6g uns sein Frieden eigen sein!", "tokens": ["M\u00f6g", "uns", "sein", "Frie\u00b7den", "ei\u00b7gen", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}