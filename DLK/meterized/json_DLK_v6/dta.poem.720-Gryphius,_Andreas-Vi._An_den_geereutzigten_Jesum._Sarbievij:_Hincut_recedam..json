{"dta.poem.720": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Vi.  An den geereutzigten Jesum.  \n  Sarbievij: Hincut recedam.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer wil ich gantz nicht weg! la\u00df alle Schwerdter klingen. ", "tokens": ["Hjer", "wil", "ich", "gantz", "nicht", "weg", "!", "la\u00df", "al\u00b7le", "Schwerd\u00b7ter", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$.", "VVIMP", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Greiff Spies vnd Sebel an brauch aller waffen macht", "tokens": ["Greiff", "Spies", "vnd", "Se\u00b7bel", "an", "brauch", "al\u00b7ler", "waf\u00b7fen", "macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "KON", "NN", "APPR", "NN", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd ", "tokens": ["Vnd"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Mich soll von diesem ", "tokens": ["Mich", "soll", "von", "die\u00b7sem"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PDAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Hier will ich wenn mich ach vnd angst vnd leid vmbringen", "tokens": ["Hier", "will", "ich", "wenn", "mich", "ach", "vnd", "angst", "vnd", "leid", "vm\u00b7brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "KOUS", "PPER", "XY", "KON", "NN", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn Erd\u2019 vnd Meer auffreist/ ja wenn der Do\u00f1er macht/", "tokens": ["Wenn", "Erd'", "vnd", "Meer", "auf\u00b7freist", "/", "ja", "wenn", "der", "Do\u00f1er", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVPP", "$(", "ADV", "KOUS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Mit dunckelrotem Blitz auff meinem Kopffe kracht/ ", "tokens": ["Mit", "dun\u00b7ckel\u00b7ro\u00b7tem", "Blitz", "auff", "mei\u00b7nem", "Kopf\u00b7fe", "kracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ja wenn der ", "tokens": ["Ja", "wenn", "der"], "token_info": ["word", "word", "word"], "pos": ["PTKANT", "KOUS", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Weil mir die Brust noch klopfft/ auch weder dort noch ", "tokens": ["Weil", "mir", "die", "Brust", "noch", "klopfft", "/", "auch", "we\u00b7der", "dort", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$(", "ADV", "KON", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "hier/", "tokens": ["hier", "/"], "token_info": ["word", "punct"], "pos": ["ADV", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Vnd nun vnd ewig soll mich reissen nichts von dir.", "tokens": ["Vnd", "nun", "vnd", "e\u00b7wig", "soll", "mich", "reis\u00b7sen", "nichts", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADJD", "VMFIN", "PPER", "VVFIN", "PIS", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hier wil ich/ wenn ich soll/ den matten Geist auffgeben.", "tokens": ["Hier", "wil", "ich", "/", "wenn", "ich", "soll", "/", "den", "mat\u00b7ten", "Geist", "auff\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$(", "KOUS", "PPER", "VMFIN", "$(", "ART", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du aber der du hoch am holtz stehst auffgericht;", "tokens": ["Du", "a\u00b7ber", "der", "du", "hoch", "am", "holtz", "stehst", "auff\u00b7ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Herr ", "tokens": ["Herr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Vnd hei\u00df durch deinen Todt im tod mich ewig leben.", "tokens": ["Vnd", "hei\u00df", "durch", "dei\u00b7nen", "Todt", "im", "tod", "mich", "e\u00b7wig", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "APPRART", "NN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}