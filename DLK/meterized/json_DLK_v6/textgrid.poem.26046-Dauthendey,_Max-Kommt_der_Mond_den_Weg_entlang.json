{"textgrid.poem.26046": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Kommt der Mond den Weg entlang", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt der Mond den Weg entlang,", "tokens": ["Kommt", "der", "Mond", "den", "Weg", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Legt seine Wang' an meine Wang',", "tokens": ["Legt", "sei\u00b7ne", "Wang'", "an", "mei\u00b7ne", "Wang'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Redet mir ins Herz hinein:", "tokens": ["Re\u00b7det", "mir", "ins", "Herz", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre Lippen sind sengend wie Feuerstein,", "tokens": ["Ih\u00b7re", "Lip\u00b7pen", "sind", "sen\u00b7gend", "wie", "Feu\u00b7ers\u00b7tein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KOKOM", "NN", "$,"], "meter": "+-+-++--+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Ihre Augen sind selige Inseln im Meer,", "tokens": ["Ih\u00b7re", "Au\u00b7gen", "sind", "se\u00b7li\u00b7ge", "In\u00b7seln", "im", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Bang wie die Nacht ist ihr Herz", "tokens": ["Bang", "wie", "die", "Nacht", "ist", "ihr", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Und an Wollust schwer.", "tokens": ["Und", "an", "Wol\u00b7lust", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbo Mond, sprich nicht weiter, ich sterbe schon.\u00ab", "tokens": ["\u00bb", "o", "Mond", ",", "sprich", "nicht", "wei\u00b7ter", ",", "ich", "ster\u00b7be", "schon", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "NN", "$,", "ADV", "PTKNEG", "PTKVZ", "$,", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und die Sehnsucht trug mich halb tot davon.", "tokens": ["Und", "die", "Sehn\u00b7sucht", "trug", "mich", "halb", "tot", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADJD", "ADJD", "PAV", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}