{"dta.poem.6795": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von Spiegeln und Fern-  \n Gl\u00e4sern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bey andern Spiegeln nun von ungeraden Fl\u00e4chen,", "tokens": ["Bey", "an\u00b7dern", "Spie\u00b7geln", "nun", "von", "un\u00b7ge\u00b7ra\u00b7den", "Fl\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieht man die Strahlen sich gantz unterschiedlich brechen.", "tokens": ["Sieht", "man", "die", "Strah\u00b7len", "sich", "gantz", "un\u00b7ter\u00b7schied\u00b7lich", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "PRF", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie kommen nicht zur\u00fcck, wohin sie erst getrieben", "tokens": ["Sie", "kom\u00b7men", "nicht", "zu\u00b7r\u00fcck", ",", "wo\u00b7hin", "sie", "erst", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,", "PWAV", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vom Vorwurff, den wir sehn, weil Raum dazwischen blieben.", "tokens": ["Vom", "Vor\u00b7wurff", ",", "den", "wir", "sehn", ",", "weil", "Raum", "da\u00b7zwi\u00b7schen", "blie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,", "KOUS", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es zieht das Licht durch sie sich bald zusammen;", "tokens": ["Es", "zieht", "das", "Licht", "durch", "sie", "sich", "bald", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bald dehnet es sich aus, wodurch verwirrte Striche,", "tokens": ["Bald", "deh\u00b7net", "es", "sich", "aus", ",", "wo\u00b7durch", "ver\u00b7wirr\u00b7te", "Stri\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "PWAV", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wovon kein einziger dem Vorbild gliche,", "tokens": ["Wo\u00b7von", "kein", "ein\u00b7zi\u00b7ger", "dem", "Vor\u00b7bild", "gli\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und wunderlich gemischte Z\u00fcge stammen.", "tokens": ["Und", "wun\u00b7der\u00b7lich", "ge\u00b7mischte", "Z\u00fc\u00b7ge", "stam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Die Winckel sind verwirrt, die Linien und Ecken,", "tokens": ["Die", "Win\u00b7ckel", "sind", "ver\u00b7wirrt", ",", "die", "Li\u00b7ni\u00b7en", "und", "E\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die sonder Ordnungen sich durch einander strecken,", "tokens": ["Die", "son\u00b7der", "Ord\u00b7nun\u00b7gen", "sich", "durch", "ein\u00b7an\u00b7der", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Formiren uns viel eitle Phantaseyen,", "tokens": ["For\u00b7mi\u00b7ren", "uns", "viel", "eit\u00b7le", "Phan\u00b7ta\u00b7se\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Und alles ist verr\u00fccket und verst\u00f6rt,", "tokens": ["Und", "al\u00b7les", "ist", "ver\u00b7r\u00fc\u00b7cket", "und", "ver\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}