{"textgrid.poem.42293": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "L\u00fcckenb\u00fc\u00dfer", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Harfner hatt' ein Harfenspiel", "tokens": ["Ein", "Harf\u00b7ner", "hatt'", "ein", "Har\u00b7fen\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr seine Hand ersonnen.", "tokens": ["F\u00fcr", "sei\u00b7ne", "Hand", "er\u00b7son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drauf hatt' er s\u00fc\u00dfen Lobes viel", "tokens": ["Drauf", "hatt'", "er", "s\u00fc\u00b7\u00dfen", "Lo\u00b7bes", "viel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Land' umher gewonnen.", "tokens": ["Im", "Land'", "um\u00b7her", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Keck stahl das Harfenspiel ein Schwarm", "tokens": ["Keck", "stahl", "das", "Har\u00b7fen\u00b7spiel", "ein", "Schwarm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Affen gleichen J\u00fcngern,", "tokens": ["Von", "Af\u00b7fen", "glei\u00b7chen", "J\u00fcn\u00b7gern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und qu\u00e4lte sich, da\u00df Gott erbarm!", "tokens": ["Und", "qu\u00e4l\u00b7te", "sich", ",", "da\u00df", "Gott", "er\u00b7barm", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "KOUS", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Harfner nachzufingern.", "tokens": ["Dem", "Harf\u00b7ner", "nach\u00b7zu\u00b7fin\u00b7gern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Viel Gl\u00fcck, viel Gl\u00fcck zum Ehrenschmaus,", "tokens": ["Viel", "Gl\u00fcck", ",", "viel", "Gl\u00fcck", "zum", "Eh\u00b7ren\u00b7schmaus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr ruhmbefli\u00dfnen J\u00fcnger!", "tokens": ["Ihr", "ruhm\u00b7be\u00b7fli\u00df\u00b7nen", "J\u00fcn\u00b7ger", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Harfe machts allein nicht aus,", "tokens": ["Die", "Har\u00b7fe", "machts", "al\u00b7lein", "nicht", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stehlt ihm auch Hand und Finger!", "tokens": ["Stehlt", "ihm", "auch", "Hand", "und", "Fin\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}