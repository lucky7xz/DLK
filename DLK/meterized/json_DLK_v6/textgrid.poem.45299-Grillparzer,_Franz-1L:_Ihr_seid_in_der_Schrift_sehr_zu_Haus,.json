{"textgrid.poem.45299": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr seid in der Schrift sehr zu Haus,", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr seid in der Schrift sehr zu Haus,", "tokens": ["Ihr", "seid", "in", "der", "Schrift", "sehr", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Wenn euch euer Aug \u00e4rgert, so rei\u00dft ihrs aus,", "tokens": ["Wenn", "euch", "eu\u00b7er", "Aug", "\u00e4r\u00b7gert", ",", "so", "rei\u00dft", "ihrs", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "ADV", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Somit wird der \u00c4rger wohl vergehen,", "tokens": ["So\u00b7mit", "wird", "der", "\u00c4r\u00b7ger", "wohl", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nur mit dem \u00c4rger zugleich das Sehen.", "tokens": ["Nur", "mit", "dem", "\u00c4r\u00b7ger", "zu\u00b7gleich", "das", "Se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}