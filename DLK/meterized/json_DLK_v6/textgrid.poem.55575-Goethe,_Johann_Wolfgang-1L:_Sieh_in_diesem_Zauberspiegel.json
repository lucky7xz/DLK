{"textgrid.poem.55575": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sieh in diesem Zauberspiegel", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh in diesem Zauberspiegel", "tokens": ["Sieh", "in", "die\u00b7sem", "Zau\u00b7ber\u00b7spie\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen Traum, wie lieb und gut", "tokens": ["Ei\u00b7nen", "Traum", ",", "wie", "lieb", "und", "gut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter ihres Gottes Fl\u00fcgel", "tokens": ["Un\u00b7ter", "ih\u00b7res", "Got\u00b7tes", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsre Freundin leidend ruht.", "tokens": ["Uns\u00b7re", "Freun\u00b7din", "lei\u00b7dend", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schaue, wie sie sich hin\u00fcber", "tokens": ["Schau\u00b7e", ",", "wie", "sie", "sich", "hin\u00b7\u00fc\u00b7ber"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "PRF", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus des Lebens Woge stritt;", "tokens": ["Aus", "des", "Le\u00b7bens", "Wo\u00b7ge", "stritt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh dein Bild ihr gegen\u00fcber", "tokens": ["Sieh", "dein", "Bild", "ihr", "ge\u00b7gen\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den Gott, der f\u00fcr euch litt.", "tokens": ["Und", "den", "Gott", ",", "der", "f\u00fcr", "euch", "litt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "F\u00fchle, was ich in dem Weben", "tokens": ["F\u00fch\u00b7le", ",", "was", "ich", "in", "dem", "We\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser Himmelsluft gef\u00fchlt,", "tokens": ["Die\u00b7ser", "Him\u00b7mel\u00b7sluft", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als mit ungeduld'gem Streben", "tokens": ["Als", "mit", "un\u00b7ge\u00b7duld'\u00b7gem", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich die Zeichnung hingew\u00fchlt.", "tokens": ["Ich", "die", "Zeich\u00b7nung", "hin\u00b7ge\u00b7w\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}