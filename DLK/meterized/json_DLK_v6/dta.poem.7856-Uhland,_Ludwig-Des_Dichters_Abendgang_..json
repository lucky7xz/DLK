{"dta.poem.7856": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Des Dichters Abendgang .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Ergehst du dich im Abendlicht, \u2014", "tokens": ["Er\u00b7gehst", "du", "dich", "im", "A\u00b7bend\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist die Zeit der Dichterwonne \u2014", "tokens": ["Das", "ist", "die", "Zeit", "der", "Dich\u00b7ter\u00b7won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So wende stets dein Angesicht", "tokens": ["So", "wen\u00b7de", "stets", "dein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Glanze der gesunknen Sonne!", "tokens": ["Zum", "Glan\u00b7ze", "der", "ge\u00b7sun\u00b7knen", "Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In hoher Feier schwebt dein Geist,", "tokens": ["In", "ho\u00b7her", "Fei\u00b7er", "schwebt", "dein", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du schauest in des Tempels Hallen,", "tokens": ["Du", "schau\u00b7est", "in", "des", "Tem\u00b7pels", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wo alles Heil\u2019ge sich erschleu\u00dft", "tokens": ["Wo", "al\u00b7les", "Heil'\u00b7ge", "sich", "er\u00b7schleu\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN", "PRF", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und himmlische Gebilde wallen.", "tokens": ["Und", "himm\u00b7li\u00b7sche", "Ge\u00b7bil\u00b7de", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wann aber um das Heiligthum", "tokens": ["Wann", "a\u00b7ber", "um", "das", "Hei\u00b7lig\u00b7thum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die dunkeln Wolken niederrollen:", "tokens": ["Die", "dun\u00b7keln", "Wol\u00b7ken", "nie\u00b7der\u00b7rol\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann ist\u2019s vollbracht, du kehrest um,", "tokens": ["Dann", "ist's", "voll\u00b7bracht", ",", "du", "keh\u00b7rest", "um", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beseligt von dem Wundervollen.", "tokens": ["Be\u00b7se\u00b7ligt", "von", "dem", "Wun\u00b7der\u00b7vol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In stiller R\u00fchrung wirst du gehn,", "tokens": ["In", "stil\u00b7ler", "R\u00fch\u00b7rung", "wirst", "du", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du tr\u00e4gst in dir des Liedes Segen;", "tokens": ["Du", "tr\u00e4gst", "in", "dir", "des", "Lie\u00b7des", "Se\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das Lichte, das du dort gesehn,", "tokens": ["Das", "Lich\u00b7te", ",", "das", "du", "dort", "ge\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Umgl\u00e4nzt dich mild auf finstern Wegen.", "tokens": ["Um\u00b7gl\u00e4nzt", "dich", "mild", "auf", "fins\u00b7tern", "We\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}