{"textgrid.poem.66251": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Grablied des 1787. Jahres", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gehab dich wohl, du liebes Jahr,", "tokens": ["Ge\u00b7hab", "dich", "wohl", ",", "du", "lie\u00b7bes", "Jahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit deinen Monden wandelbar,", "tokens": ["Mit", "dei\u00b7nen", "Mon\u00b7den", "wan\u00b7del\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit deinen Tagen, Sohn der Zeit,", "tokens": ["Mit", "dei\u00b7nen", "Ta\u00b7gen", ",", "Sohn", "der", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zieh friedlich in die Ewigkeit.", "tokens": ["Zieh", "fried\u00b7lich", "in", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Blicker in dem Himmel sah", "tokens": ["Der", "Bli\u00b7cker", "in", "dem", "Him\u00b7mel", "sah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das B\u00f6se, das in dir geschah;", "tokens": ["Das", "B\u00f6\u00b7se", ",", "das", "in", "dir", "ge\u00b7schah", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch sah er auch die goldne Saat", "tokens": ["Doch", "sah", "er", "auch", "die", "gold\u00b7ne", "Saat"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von mancher Geist- und Herzensthat.", "tokens": ["Von", "man\u00b7cher", "Geist", "und", "Her\u00b7zenst\u00b7hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er sah des Christen Widerstand,", "tokens": ["Er", "sah", "des", "Chris\u00b7ten", "Wi\u00b7der\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit dem er Zweifel \u00fcberwand;", "tokens": ["Mit", "dem", "er", "Zwei\u00b7fel", "\u00fc\u00b7ber\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er sah des Glaubens hohe Macht,", "tokens": ["Er", "sah", "des", "Glau\u00b7bens", "ho\u00b7he", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Satans H\u00f6llengrimm verlacht;", "tokens": ["Die", "Sa\u00b7tans", "H\u00f6l\u00b7len\u00b7grimm", "ver\u00b7lacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sah in der stummen Siedelei", "tokens": ["Sah", "in", "der", "stum\u00b7men", "Sie\u00b7de\u00b7lei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Mann, der ohne Heuchelei", "tokens": ["Den", "Mann", ",", "der", "oh\u00b7ne", "Heu\u00b7che\u00b7lei"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ger\u00e4uschlos manche That gethan,", "tokens": ["Ge\u00b7r\u00e4uschlos", "man\u00b7che", "That", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Ruhm und Gold nicht lohnen kann.", "tokens": ["Die", "Ruhm", "und", "Gold", "nicht", "loh\u00b7nen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So manche Z\u00e4hre tilgtest du,", "tokens": ["So", "man\u00b7che", "Z\u00e4h\u00b7re", "tilg\u00b7test", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So manchem K\u00e4mpfer gabst du Ruh';", "tokens": ["So", "man\u00b7chem", "K\u00e4mp\u00b7fer", "gabst", "du", "Ruh'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So manchem hast du tiefgef\u00fchlt", "tokens": ["So", "man\u00b7chem", "hast", "du", "tief\u00b7ge\u00b7f\u00fchlt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Schwei\u00df im Todeskampf gek\u00fchlt.", "tokens": ["Den", "Schwei\u00df", "im", "To\u00b7des\u00b7kampf", "ge\u00b7k\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Auch schl\u00fcpftest du ins Kerkergrab", "tokens": ["Auch", "schl\u00fcpf\u00b7test", "du", "ins", "Ker\u00b7ker\u00b7grab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und streiftest manche Fesseln ab;", "tokens": ["Und", "streif\u00b7test", "man\u00b7che", "Fes\u00b7seln", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch ", "tokens": ["Auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Mit Thr\u00e4nen hab' ich sie betr\u00e4uft.", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "hab'", "ich", "sie", "be\u00b7tr\u00e4uft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "So nimm denn diesen Thr\u00e4nenku\u00df,", "tokens": ["So", "nimm", "denn", "die\u00b7sen", "Thr\u00e4\u00b7nen\u00b7ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O du des Jahres Genius!", "tokens": ["O", "du", "des", "Jah\u00b7res", "Ge\u00b7nius", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Tilg unsre Schuld aus deinem Buch.", "tokens": ["Tilg", "uns\u00b7re", "Schuld", "aus", "dei\u00b7nem", "Buch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erfleh uns Segen nur, nicht Fluch.", "tokens": ["Er\u00b7fleh", "uns", "Se\u00b7gen", "nur", ",", "nicht", "Fluch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADV", "$,", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}