{"textgrid.poem.36894": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Ermunterung, die Schrift zu lesen", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soll dein verderbtes Herz zur Heiligung genesen,", "tokens": ["Soll", "dein", "ver\u00b7derb\u00b7tes", "Herz", "zur", "Hei\u00b7li\u00b7gung", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Christ, so vers\u00e4ume nicht, das Wort des Herrn zu lesen;", "tokens": ["Christ", ",", "so", "ver\u00b7s\u00e4u\u00b7me", "nicht", ",", "das", "Wort", "des", "Herrn", "zu", "le\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PTKNEG", "$,", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Bedenke, da\u00df dies Wort das Heil der ganzen Welt,", "tokens": ["Be\u00b7den\u00b7ke", ",", "da\u00df", "dies", "Wort", "das", "Heil", "der", "gan\u00b7zen", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PDS", "NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Rat der Seligkeit, den Geist aus Gott enth\u00e4lt.", "tokens": ["Den", "Rat", "der", "Se\u00b7lig\u00b7keit", ",", "den", "Geist", "aus", "Gott", "ent\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Merk auf, als ob dir Gott, dein Gott, gerufen h\u00e4tte;", "tokens": ["Merk", "auf", ",", "als", "ob", "dir", "Gott", ",", "dein", "Gott", ",", "ge\u00b7ru\u00b7fen", "h\u00e4t\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "KOKOM", "KOUS", "PPER", "NN", "$,", "PPOSAT", "NN", "$,", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Merk auf, als ob er selbst zu dir vom Himmel redte!", "tokens": ["Merk", "auf", ",", "als", "ob", "er", "selbst", "zu", "dir", "vom", "Him\u00b7mel", "red\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "KOKOM", "KOUS", "PPER", "ADV", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So lies; mit Ehrfurcht lies, mit Lust und mit Vertraun,", "tokens": ["So", "lies", ";", "mit", "Ehr\u00b7furcht", "lies", ",", "mit", "Lust", "und", "mit", "Ver\u00b7traun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "APPR", "NN", "VVFIN", "$,", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und mit dem frommen Ernst, in Gott dich zu erbaun.", "tokens": ["Und", "mit", "dem", "from\u00b7men", "Ernst", ",", "in", "Gott", "dich", "zu", "er\u00b7baun", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sprich fromm: O Gott! vor dem ich meine H\u00e4nde falte,", "tokens": ["Sprich", "fromm", ":", "O", "Gott", "!", "vor", "dem", "ich", "mei\u00b7ne", "H\u00e4n\u00b7de", "fal\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "$.", "NE", "NN", "$.", "APPR", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gib, da\u00df ich dein Gebot f\u00fcr dein Wort ewig halte;", "tokens": ["Gib", ",", "da\u00df", "ich", "dein", "Ge\u00b7bot", "f\u00fcr", "dein", "Wort", "e\u00b7wig", "hal\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und la\u00df mich deinen Rat empfindungsvoll verstehn,", "tokens": ["Und", "la\u00df", "mich", "dei\u00b7nen", "Rat", "emp\u00b7fin\u00b7dungs\u00b7voll", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Wunder am Gesetz, am Wort vom Kreuze sehn!", "tokens": ["Die", "Wun\u00b7der", "am", "Ge\u00b7setz", ",", "am", "Wort", "vom", "Kreu\u00b7ze", "sehn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er, aller Wahrheit Gott, kann dich nicht irren lassen.", "tokens": ["Er", ",", "al\u00b7ler", "Wahr\u00b7heit", "Gott", ",", "kann", "dich", "nicht", "ir\u00b7ren", "las\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PIAT", "NN", "NN", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lies, Christ, sein heilig Buch, lies oft; du wirst es fassen,", "tokens": ["Lies", ",", "Christ", ",", "sein", "hei\u00b7lig", "Buch", ",", "lies", "oft", ";", "du", "wirst", "es", "fas\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PPOSAT", "ADJD", "NN", "$,", "VVFIN", "ADV", "$.", "PPER", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So viel dein Heil verlangt. Gott ist's, der Weisheit gibt,", "tokens": ["So", "viel", "dein", "Heil", "ver\u00b7langt", ".", "Gott", "ist's", ",", "der", "Weis\u00b7heit", "gibt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVPP", "$.", "NN", "VAFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn man sie redlich sucht und aus Gewissen liebt.", "tokens": ["Wenn", "man", "sie", "red\u00b7lich", "sucht", "und", "aus", "Ge\u00b7wis\u00b7sen", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADJD", "VVFIN", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Lies, frei von Leidenschaft und ledig von Gesch\u00e4ften,", "tokens": ["Lies", ",", "frei", "von", "Lei\u00b7den\u00b7schaft", "und", "le\u00b7dig", "von", "Ge\u00b7sch\u00e4f\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "APPR", "NN", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sammle deinen Geist mit allen seinen Kr\u00e4ften.", "tokens": ["Und", "samm\u00b7le", "dei\u00b7nen", "Geist", "mit", "al\u00b7len", "sei\u00b7nen", "Kr\u00e4f\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der beste Teil des Tags, des Morgens Heiterkeit,", "tokens": ["Der", "bes\u00b7te", "Teil", "des", "Tags", ",", "des", "Mor\u00b7gens", "Hei\u00b7ter\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "ART", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dann der Tag des Herrn, der sei der Schrift geweiht.", "tokens": ["Und", "dann", "der", "Tag", "des", "Herrn", ",", "der", "sei", "der", "Schrift", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "$,", "PRELS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "R\u00fchrt dich ein starker Spruch: so ruf ihn, dir zum Gl\u00fccke,", "tokens": ["R\u00fchrt", "dich", "ein", "star\u00b7ker", "Spruch", ":", "so", "ruf", "ihn", ",", "dir", "zum", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$.", "ADV", "NN", "PPER", "$,", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Tags oft in dein Herz, im stillen oft, zur\u00fccke;", "tokens": ["Des", "Tags", "oft", "in", "dein", "Herz", ",", "im", "stil\u00b7len", "oft", ",", "zu\u00b7r\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,", "APPRART", "ADJA", "ADV", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Empfinde seinen Geist, und st\u00e4rke dich durch ihn", "tokens": ["Emp\u00b7fin\u00b7de", "sei\u00b7nen", "Geist", ",", "und", "st\u00e4r\u00b7ke", "dich", "durch", "ihn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PRF", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zum wahren Edelmut, das Gute zu vollziehn.", "tokens": ["Zum", "wah\u00b7ren", "E\u00b7del\u00b7mut", ",", "das", "Gu\u00b7te", "zu", "voll\u00b7ziehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Um tugendhaft zu sein, dazu sind wir auf Erden.", "tokens": ["Um", "tu\u00b7gend\u00b7haft", "zu", "sein", ",", "da\u00b7zu", "sind", "wir", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PTKZU", "VAINF", "$,", "PAV", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Tu, was die Schrift gebeut; dann wirst du inne werden,", "tokens": ["Tu", ",", "was", "die", "Schrift", "ge\u00b7beut", ";", "dann", "wirst", "du", "in\u00b7ne", "wer\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "PTKVZ", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Lehre sei von Gott, die dir verk\u00fcndigt ist,", "tokens": ["Die", "Leh\u00b7re", "sei", "von", "Gott", ",", "die", "dir", "ver\u00b7k\u00fcn\u00b7digt", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dann das Wort verstehn, dem du gehorsam bist.", "tokens": ["Und", "dann", "das", "Wort", "ver\u00b7stehn", ",", "dem", "du", "ge\u00b7hor\u00b7sam", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Spricht sie geheimnisvoll: so la\u00df dich dies nicht schrecken.", "tokens": ["Spricht", "sie", "ge\u00b7heim\u00b7nis\u00b7voll", ":", "so", "la\u00df", "dich", "dies", "nicht", "schre\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$.", "ADV", "VVIMP", "PPER", "PDS", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein endlicher Verstand kann Gott nie ganz entdecken;", "tokens": ["Ein", "end\u00b7li\u00b7cher", "Ver\u00b7stand", "kann", "Gott", "nie", "ganz", "ent\u00b7de\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gott bleibt unendlich hoch. Wenn er sich dir erkl\u00e4rt:", "tokens": ["Gott", "bleibt", "un\u00b7end\u00b7lich", "hoch", ".", "Wenn", "er", "sich", "dir", "er\u00b7kl\u00e4rt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "ADJD", "$.", "KOUS", "PPER", "PRF", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So glaube, was er spricht, nicht was dein Witz begehrt.", "tokens": ["So", "glau\u00b7be", ",", "was", "er", "spricht", ",", "nicht", "was", "dein", "Witz", "be\u00b7gehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "PTKNEG", "PWS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Sich seines schwachen Lichts bei Gottes Licht nicht sch\u00e4men,", "tokens": ["Sich", "sei\u00b7nes", "schwa\u00b7chen", "Lichts", "bei", "Got\u00b7tes", "Licht", "nicht", "sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "ADJA", "NN", "APPR", "NN", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist Ruhm; und die Vernunft alsdann gefangen nehmen,", "tokens": ["Ist", "Ruhm", ";", "und", "die", "Ver\u00b7nunft", "als\u00b7dann", "ge\u00b7fan\u00b7gen", "neh\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$.", "KON", "ART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn Gott sich offenbart, ist der Gesch\u00f6pfe Pflicht;", "tokens": ["Wenn", "Gott", "sich", "of\u00b7fen\u00b7bart", ",", "ist", "der", "Ge\u00b7sch\u00f6p\u00b7fe", "Pflicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "ADJD", "$,", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und weise Demut ist's, das glauben, was Gott spricht.", "tokens": ["Und", "wei\u00b7se", "De\u00b7mut", "ist's", ",", "das", "glau\u00b7ben", ",", "was", "Gott", "spricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "$,", "PDS", "VVINF", "$,", "PRELS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Drum la\u00df dich, frommer Christ, durch keine Zweifel kr\u00e4nken.", "tokens": ["Drum", "la\u00df", "dich", ",", "from\u00b7mer", "Christ", ",", "durch", "kei\u00b7ne", "Zwei\u00b7fel", "kr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier bist du Kind; doch dort wird Gott mehr Licht dir schenken.", "tokens": ["Hier", "bist", "du", "Kind", ";", "doch", "dort", "wird", "Gott", "mehr", "Licht", "dir", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$.", "ADV", "ADV", "VAFIN", "NN", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dort w\u00e4chst mit deinem Gl\u00fcck dein Licht in Ewigkeit;", "tokens": ["Dort", "w\u00e4chst", "mit", "dei\u00b7nem", "Gl\u00fcck", "dein", "Licht", "in", "E\u00b7wig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dort ist die Zeit des Schauns, und hier des Glaubens Zeit.", "tokens": ["Dort", "ist", "die", "Zeit", "des", "Schauns", ",", "und", "hier", "des", "Glau\u00b7bens", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,", "KON", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Verehre stets die Schrift; und siehst du Dunkelheiten:", "tokens": ["Ver\u00b7eh\u00b7re", "stets", "die", "Schrift", ";", "und", "siehst", "du", "Dun\u00b7kel\u00b7hei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$.", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So la\u00df dich deinen Freund, der mehr als du sieht, leiten.", "tokens": ["So", "la\u00df", "dich", "dei\u00b7nen", "Freund", ",", "der", "mehr", "als", "du", "sieht", ",", "lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "NN", "$,", "PRELS", "ADV", "KOUS", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein forschernder Verstand, der sich der Schrift geweiht,", "tokens": ["Ein", "for\u00b7schern\u00b7der", "Ver\u00b7stand", ",", "der", "sich", "der", "Schrift", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Ein angefochtnes Herz, hebt manche Dunkelheit.", "tokens": ["Ein", "an\u00b7ge\u00b7focht\u00b7nes", "Herz", ",", "hebt", "man\u00b7che", "Dun\u00b7kel\u00b7heit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Halt fest an Gottes Wort; es ist dein Gl\u00fcck auf Erden,", "tokens": ["Halt", "fest", "an", "Got\u00b7tes", "Wort", ";", "es", "ist", "dein", "Gl\u00fcck", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPR", "NN", "NN", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wird, so wahr Gott ist, dein Gl\u00fcck im Himmel werden.", "tokens": ["Und", "wird", ",", "so", "wahr", "Gott", "ist", ",", "dein", "Gl\u00fcck", "im", "Him\u00b7mel", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "ADV", "ADJD", "NN", "VAFIN", "$,", "PPOSAT", "NN", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verachte christlich gro\u00df des Bibelfeindes Spott;", "tokens": ["Ver\u00b7ach\u00b7te", "christ\u00b7lich", "gro\u00df", "des", "Bi\u00b7bel\u00b7fein\u00b7des", "Spott", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Lehre, die er schm\u00e4ht, bleibt doch das Wort aus Gott.", "tokens": ["Die", "Leh\u00b7re", ",", "die", "er", "schm\u00e4ht", ",", "bleibt", "doch", "das", "Wort", "aus", "Gott", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}