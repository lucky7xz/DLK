{"dta.poem.13329": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von der verbl\u00fcmten Schreibart. Ist die Fertsetzung der Abhandl. von dem Sinnr.  \n und Scharssinn, aus der Ankl. des verd. Geschmacks.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198519", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es bl\u00fchete bereits", "tokens": ["Es", "bl\u00fc\u00b7he\u00b7te", "be\u00b7reits"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Silber-weisse Schnee- die g\u00fcldne Crocus-Blume", "tokens": ["Die", "Sil\u00b7ber\u00b7weis\u00b7se", "Schnee", "die", "g\u00fcld\u00b7ne", "Cro\u00b7cus\u00b7Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die letzte hebt der g\u00fcldnen ", "tokens": ["Die", "letz\u00b7te", "hebt", "der", "g\u00fcld\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fast ohne Stiel gleich an der Erd herf\u00fcr.", "tokens": ["Fast", "oh\u00b7ne", "Stiel", "gleich", "an", "der", "Erd", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}