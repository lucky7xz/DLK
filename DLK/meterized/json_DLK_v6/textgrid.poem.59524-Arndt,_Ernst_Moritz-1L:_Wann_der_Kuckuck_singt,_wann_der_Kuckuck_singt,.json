{"textgrid.poem.59524": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wann der Kuckuck singt, wann der Kuckuck singt,", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann der Kuckuck singt, wann der Kuckuck singt,", "tokens": ["Wann", "der", "Ku\u00b7ckuck", "singt", ",", "wann", "der", "Ku\u00b7ckuck", "singt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ist Hochzeit im gr\u00fcnen Wald,", "tokens": ["Ist", "Hoch\u00b7zeit", "im", "gr\u00fc\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und es tanzt und springt, und es spielt und klingt", "tokens": ["Und", "es", "tanzt", "und", "springt", ",", "und", "es", "spielt", "und", "klingt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "$,", "KON", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Liebe mit s\u00fc\u00dfer Gewalt;", "tokens": ["Die", "Lie\u00b7be", "mit", "s\u00fc\u00b7\u00dfer", "Ge\u00b7walt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Die Knaben und M\u00e4dchen zu zweien", "tokens": ["Die", "Kna\u00b7ben", "und", "M\u00e4d\u00b7chen", "zu", "zwei\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "PTKZU", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Sie wandeln im fr\u00f6hlichen Maien", "tokens": ["Sie", "wan\u00b7deln", "im", "fr\u00f6h\u00b7li\u00b7chen", "Mai\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Zum gr\u00fcnen, gr\u00fcnen Wald.", "tokens": ["Zum", "gr\u00fc\u00b7nen", ",", "gr\u00fc\u00b7nen", "Wald", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und mit Heisahei! und abermal Hei!", "tokens": ["Und", "mit", "Hei\u00b7sa\u00b7hei", "!", "und", "a\u00b7ber\u00b7mal", "Hei", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$.", "KON", "ADV", "NN", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Liebe sie spielet frisch,", "tokens": ["Die", "Lie\u00b7be", "sie", "spie\u00b7let", "frisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nach dem Tanze f\u00fchrt sie je zwei und zwei", "tokens": ["Nach", "dem", "Tan\u00b7ze", "f\u00fchrt", "sie", "je", "zwei", "und", "zwei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "CARD", "KON", "CARD"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Die Spieler ins Schattengeb\u00fcsch;", "tokens": ["Die", "Spie\u00b7ler", "ins", "Schat\u00b7ten\u00b7ge\u00b7b\u00fcsch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Da streut sie auf schwellenden Moosen", "tokens": ["Da", "streut", "sie", "auf", "schwel\u00b7len\u00b7den", "Moo\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Ein Bettchen von Veilchen und Rosen", "tokens": ["Ein", "Bett\u00b7chen", "von", "Veil\u00b7chen", "und", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Im gr\u00fcnen, gr\u00fcnen Wald.", "tokens": ["Im", "gr\u00fc\u00b7nen", ",", "gr\u00fc\u00b7nen", "Wald", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sei nicht bange, M\u00e4del, es mu\u00df so sein,", "tokens": ["Sei", "nicht", "ban\u00b7ge", ",", "M\u00e4\u00b7del", ",", "es", "mu\u00df", "so", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "$,", "NN", "$,", "PPER", "VMFIN", "ADV", "VAINF", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die Liebe, sie brauchet Gewalt,", "tokens": ["Die", "Lie\u00b7be", ",", "sie", "brau\u00b7chet", "Ge\u00b7walt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "F\u00e4hrt gern mit Donnern und Blitzen drein,", "tokens": ["F\u00e4hrt", "gern", "mit", "Don\u00b7nern", "und", "Blit\u00b7zen", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und lustig zur Hochzeit schallt.", "tokens": ["Und", "lus\u00b7tig", "zur", "Hoch\u00b7zeit", "schallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dein Bl\u00fcmchen magst nimmer du retten,", "tokens": ["Dein", "Bl\u00fcm\u00b7chen", "magst", "nim\u00b7mer", "du", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Drum freu' dich der blumigen Betten", "tokens": ["Drum", "freu'", "dich", "der", "blu\u00b7mi\u00b7gen", "Bet\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Im gr\u00fcnen, gr\u00fcnen Wald.", "tokens": ["Im", "gr\u00fc\u00b7nen", ",", "gr\u00fc\u00b7nen", "Wald", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Auf! mein Kuckuck, klinge und singe laut!", "tokens": ["Auf", "!", "mein", "Ku\u00b7ckuck", ",", "klin\u00b7ge", "und", "sin\u00b7ge", "laut", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "PPOSAT", "NN", "$,", "VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Es ist Hochzeit im gr\u00fcnen Wald.", "tokens": ["Es", "ist", "Hoch\u00b7zeit", "im", "gr\u00fc\u00b7nen", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf! mein Himmel, Br\u00e4utigam kling' und Braut", "tokens": ["Auf", "!", "mein", "Him\u00b7mel", ",", "Br\u00e4u\u00b7ti\u00b7gam", "kling'", "und", "Braut"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$.", "PPOSAT", "NN", "$,", "NE", "VVFIN", "KON", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und donnre der Lust Gewalt!", "tokens": ["Und", "donn\u00b7re", "der", "Lust", "Ge\u00b7walt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Auf! Schwestern, und schlinget den Reihen", "tokens": ["Auf", "!", "Schwes\u00b7tern", ",", "und", "schlin\u00b7get", "den", "Rei\u00b7hen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$.", "NN", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Zu zweien, zu zweien, zu zweien", "tokens": ["Zu", "zwei\u00b7en", ",", "zu", "zwei\u00b7en", ",", "zu", "zwei\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Im gr\u00fcnen, gr\u00fcnen Wald!", "tokens": ["Im", "gr\u00fc\u00b7nen", ",", "gr\u00fc\u00b7nen", "Wald", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}