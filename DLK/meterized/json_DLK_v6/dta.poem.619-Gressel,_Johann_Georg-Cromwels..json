{"dta.poem.619": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Cromwels.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Vor den sich Engelland in Demuth hat geneigt/", "tokens": ["Vor", "den", "sich", "En\u00b7gel\u00b7land", "in", "De\u00b7muth", "hat", "ge\u00b7neigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PRF", "NE", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Vor welchen ", "tokens": ["Vor", "wel\u00b7chen"], "token_info": ["word", "word"], "pos": ["APPR", "PWAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Des Asche lieget hier wo todte Aeser liegen/", "tokens": ["Des", "A\u00b7sche", "lie\u00b7get", "hier", "wo", "tod\u00b7te", "A\u00b7e\u00b7ser", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PWAV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Den nie das Schwerdt besiegt/ den must das Feur bestegen.", "tokens": ["Den", "nie", "das", "Schwerdt", "be\u00b7siegt", "/", "den", "must", "das", "Feur", "bes\u00b7te\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "$(", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}