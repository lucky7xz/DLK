{"textgrid.poem.25764": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seht nur umher die Gegend an,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht nur umher die Gegend an,", "tokens": ["Seht", "nur", "um\u00b7her", "die", "Ge\u00b7gend", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sch\u00f6n ist sie, wie sch\u00f6n!", "tokens": ["Wie", "sch\u00f6n", "ist", "sie", ",", "wie", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O! wer das nicht empfinden kann,", "tokens": ["O", "!", "wer", "das", "nicht", "emp\u00b7fin\u00b7den", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWS", "PDS", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mag immer wieder gehn.", "tokens": ["Mag", "im\u00b7mer", "wie\u00b7der", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er w\u00e4re doch f\u00fcr uns kein Mann,", "tokens": ["Er", "w\u00e4\u00b7re", "doch", "f\u00fcr", "uns", "kein", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und w\u00fcrd' uns nicht verstehn.", "tokens": ["Und", "w\u00fcrd'", "uns", "nicht", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sey fr\u00f6hlich, oder sey betr\u00fcbt,", "tokens": ["Sey", "fr\u00f6h\u00b7lich", ",", "o\u00b7der", "sey", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "KON", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Komm sp\u00e4t hieher, komm fr\u00fch:", "tokens": ["Komm", "sp\u00e4t", "hie\u00b7her", ",", "komm", "fr\u00fch", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PAV", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wenn sonst dein Auge Sch\u00f6nheit liebt,", "tokens": ["Wenn", "sonst", "dein", "Au\u00b7ge", "Sch\u00f6n\u00b7heit", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So heb' es auf, und sieh!", "tokens": ["So", "heb'", "es", "auf", ",", "und", "sieh", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVIMP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Das Vorrecht, zu erfreun, vergibt", "tokens": ["Das", "Vor\u00b7recht", ",", "zu", "er\u00b7freun", ",", "ver\u00b7gibt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PTKZU", "VVINF", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sich diese H\u00f6he nie.", "tokens": ["Sich", "die\u00b7se", "H\u00f6\u00b7he", "nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PDAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Die reinste Freud' auf dieser Welt", "tokens": ["Die", "reins\u00b7te", "Freud'", "auf", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Gew\u00e4hrt uns die Natur.", "tokens": ["Ge\u00b7w\u00e4hrt", "uns", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Dem Herzen, welchem sie gef\u00e4llt,", "tokens": ["Dem", "Her\u00b7zen", ",", "wel\u00b7chem", "sie", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Vertraun wir ohne Schwur.", "tokens": ["Ver\u00b7traun", "wir", "oh\u00b7ne", "Schwur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Doch wer sich heuchelnd nur so stellt,", "tokens": ["Doch", "wer", "sich", "heu\u00b7chelnd", "nur", "so", "stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "ADJD", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Der mache F\u00fcrsten Cour.", "tokens": ["Der", "ma\u00b7che", "F\u00fcrs\u00b7ten", "Cour", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Kann Liebe jemals ohne Pein,", "tokens": ["Kann", "Lie\u00b7be", "je\u00b7mals", "oh\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Kann Freundschaft ohne Schmerz,", "tokens": ["Kann", "Freund\u00b7schaft", "oh\u00b7ne", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Kann Reichthum ohne Sorge seyn,", "tokens": ["Kann", "Reicht\u00b7hum", "oh\u00b7ne", "Sor\u00b7ge", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "APPR", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Ohn' Angst ein Mutterherz?", "tokens": ["Ohn'", "Angst", "ein", "Mut\u00b7ter\u00b7herz", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Ach! Hefen f\u00fchrt der beste Wein,", "tokens": ["Ach", "!", "He\u00b7fen", "f\u00fchrt", "der", "bes\u00b7te", "Wein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Und Gift das reinste Erz.", "tokens": ["Und", "Gift", "das", "reins\u00b7te", "Erz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Doch sehen wir noch tausendmal", "tokens": ["Doch", "se\u00b7hen", "wir", "noch", "tau\u00b7send\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Die Sonn' hier untergehn,", "tokens": ["Die", "Sonn'", "hier", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "So sagen wir zum letzten Strahl':", "tokens": ["So", "sa\u00b7gen", "wir", "zum", "letz\u00b7ten", "Strahl'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Leb' wohl, auf Wiedersehn!", "tokens": ["Leb'", "wohl", ",", "auf", "Wie\u00b7der\u00b7sehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Und k\u00f6nnen ohne Reu' und Qual", "tokens": ["Und", "k\u00f6n\u00b7nen", "oh\u00b7ne", "Reu'", "und", "Qual"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Wie sie zur Ruhe gehn.", "tokens": ["Wie", "sie", "zur", "Ru\u00b7he", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}