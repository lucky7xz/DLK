{"dta.poem.17051": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "152.  Der Him\u0303lische auf Erden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wer reines Hertzens ist/ und Z\u00fcchtig in Geberden/", "tokens": ["Wer", "rei\u00b7nes", "Hert\u00b7zens", "ist", "/", "und", "Z\u00fcch\u00b7tig", "in", "Ge\u00b7ber\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VAFIN", "$(", "KON", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hochverliebt in GOtt/ ist Him\u0304lisch auf der Erden.", "tokens": ["Und", "hoch\u00b7ver\u00b7liebt", "in", "Gott", "/", "ist", "Him\u0304\u00b7lisch", "auf", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$(", "VAFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}