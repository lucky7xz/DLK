{"textgrid.poem.39907": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als sie funfzehn Jahr gestanden,", "tokens": ["Als", "sie", "funf\u00b7zehn", "Jahr", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "CARD", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sah'n schon alle Steine grau,", "tokens": ["Sah'n", "schon", "al\u00b7le", "Stei\u00b7ne", "grau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vieles hatten \u00fcberstanden", "tokens": ["Vie\u00b7les", "hat\u00b7ten", "\u00fc\u00b7bers\u00b7tan\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcrst zumal und F\u00fcrstenbau.", "tokens": ["F\u00fcrst", "zu\u00b7mal", "und", "F\u00fcrs\u00b7ten\u00b7bau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn das span'sche Kriegsgewitter", "tokens": ["Denn", "das", "span'\u00b7sche", "Kriegs\u00b7ge\u00b7wit\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War gezogen durch das Land,", "tokens": ["War", "ge\u00b7zo\u00b7gen", "durch", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch am Thor die steinern' Ritter", "tokens": ["Doch", "am", "Thor", "die", "stei\u00b7nern'", "Rit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hielten unbezwungnen Stand.", "tokens": ["Hiel\u00b7ten", "un\u00b7be\u00b7zwung\u00b7nen", "Stand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und die Linde vor den Thoren", "tokens": ["Und", "die", "Lin\u00b7de", "vor", "den", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NE", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rauschte freudiglich darein,", "tokens": ["Rauschte", "freu\u00b7dig\u00b7lich", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als von F\u00fcrstenhand erkoren,", "tokens": ["Als", "von", "F\u00fcrs\u00b7ten\u00b7hand", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freie W\u00e4chterin zu sein.", "tokens": ["Frei\u00b7e", "W\u00e4ch\u00b7te\u00b7rin", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Rauscht' und bl\u00fchte funfzehn Jahre,", "tokens": ["Rauscht'", "und", "bl\u00fch\u00b7te", "funf\u00b7zehn", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis ein Winter wieder kam,", "tokens": ["Bis", "ein", "Win\u00b7ter", "wie\u00b7der", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der den Herzog auf der Bahre", "tokens": ["Der", "den", "Her\u00b7zog", "auf", "der", "Bah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem treuen Schlosse nahm.", "tokens": ["Von", "dem", "treu\u00b7en", "Schlos\u00b7se", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Mit der welken Bl\u00e4tter Zittern", "tokens": ["Mit", "der", "wel\u00b7ken", "Bl\u00e4t\u00b7ter", "Zit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fl\u00fcsterte sein Baum darein,", "tokens": ["Fl\u00fcs\u00b7ter\u00b7te", "sein", "Baum", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das edle Paar von Rittern", "tokens": ["Und", "das", "ed\u00b7le", "Paar", "von", "Rit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jetzo schien es erst von Stein.", "tokens": ["Jet\u00b7zo", "schien", "es", "erst", "von", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Lehrer viel und Sch\u00fcler wallen", "tokens": ["Leh\u00b7rer", "viel", "und", "Sch\u00fc\u00b7ler", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die Stra\u00dfen schleichend bang,", "tokens": ["Durch", "die", "Stra\u00b7\u00dfen", "schlei\u00b7chend", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus den S\u00e4len, aus den Hallen", "tokens": ["Aus", "den", "S\u00e4\u00b7len", ",", "aus", "den", "Hal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00f6nt ein frommer Sterbgesang.", "tokens": ["T\u00f6nt", "ein", "from\u00b7mer", "Sterb\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch die graue Landesveste", "tokens": ["Doch", "die", "grau\u00b7e", "Lan\u00b7des\u00b7ves\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeuget noch von ihrem Herrn,", "tokens": ["Zeu\u00b7get", "noch", "von", "ih\u00b7rem", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4tten gleich die fremden G\u00e4ste", "tokens": ["H\u00e4t\u00b7ten", "gleich", "die", "frem\u00b7den", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie zerst\u00f6ret gar zu gern", "tokens": ["Sie", "zer\u00b7st\u00f6\u00b7ret", "gar", "zu", "gern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKA", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und der Baum der bl\u00fcht noch immer", "tokens": ["Und", "der", "Baum", "der", "bl\u00fcht", "noch", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "VVFIN", "ADV", "ADV"], "meter": "--+-+-++", "measure": "anapaest.init"}, "line.2": {"text": "Seit manch hundert Sommern gut,", "tokens": ["Seit", "manch", "hun\u00b7dert", "Som\u00b7mern", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "CARD", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ziert mit gr\u00fcner Zweige Schimmer", "tokens": ["Ziert", "mit", "gr\u00fc\u00b7ner", "Zwei\u00b7ge", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Manchen freien Musenhut.", "tokens": ["Man\u00b7chen", "frei\u00b7en", "Mu\u00b7sen\u00b7hut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Horch, sie rauscht im Abendwinde,", "tokens": ["Horch", ",", "sie", "rauscht", "im", "Ab\u00b7end\u00b7win\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wandle, Herzog, durch dein Schlo\u00df,", "tokens": ["Wand\u00b7le", ",", "Her\u00b7zog", ",", "durch", "dein", "Schlo\u00df", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Komm' und pfl\u00fcck' von deiner Linde", "tokens": ["Komm'", "und", "pfl\u00fcck", "von", "dei\u00b7ner", "Lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen frischen Bl\u00fctenspro\u00df!", "tokens": ["Ei\u00b7nen", "fri\u00b7schen", "Bl\u00fc\u00b7tens\u00b7pro\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}