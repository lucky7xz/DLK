{"dta.poem.7507": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Alswie der Mensch, so ist sein Gott, so ist sein Glaube,", "tokens": ["Als\u00b7wie", "der", "Mensch", ",", "so", "ist", "sein", "Gott", ",", "so", "ist", "sein", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus geist'gem Aether bald, und bald aus Erdenstaube.", "tokens": ["Aus", "geist'\u00b7gem", "A\u00b7e\u00b7ther", "bald", ",", "und", "bald", "aus", "Er\u00b7dens\u00b7tau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$,", "KON", "ADV", "APPR", "NN", "$."], "meter": "-+----+-+-+-+-", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Doch doppelt ist der Gott, der Glaube doppelt auch,", "tokens": ["Doch", "dop\u00b7pelt", "ist", "der", "Gott", ",", "der", "Glau\u00b7be", "dop\u00b7pelt", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier selbstentglommner Trieb, dort \u00fcberkommner Brauch.", "tokens": ["Hier", "selbs\u00b7tent\u00b7glomm\u00b7ner", "Trieb", ",", "dort", "\u00fc\u00b7ber\u00b7komm\u00b7ner", "Brauch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Eigenste wird ganz nie frei vom Angenommnen,", "tokens": ["Das", "Ei\u00b7gens\u00b7te", "wird", "ganz", "nie", "frei", "vom", "An\u00b7ge\u00b7nomm\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Doch \u00fcbt die Eigenheit ihr Recht am Ueberkommnen.", "tokens": ["Doch", "\u00fcbt", "die", "Ei\u00b7gen\u00b7heit", "ihr", "Recht", "am", "Ue\u00b7ber\u00b7komm\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Man rei\u00dft das Haus nicht ein, das V\u00e4ter uns gebaut,", "tokens": ["Man", "rei\u00dft", "das", "Haus", "nicht", "ein", ",", "das", "V\u00e4\u00b7ter", "uns", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$,", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch richtet man sich's ein, wie man's am liebsten schaut.", "tokens": ["Doch", "rich\u00b7tet", "man", "sich's", "ein", ",", "wie", "man's", "am", "liebs\u00b7ten", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PIS", "PTKVZ", "$,", "PWAV", "PIS", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und r\u00e4umt man nicht hinweg ehrw\u00fcrd'ge Ahnenbilder,", "tokens": ["Und", "r\u00e4umt", "man", "nicht", "hin\u00b7weg", "ehr\u00b7w\u00fcrd'\u00b7ge", "Ah\u00b7nen\u00b7bil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKNEG", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch Deutung macht man sie und durch Umgebung milder.", "tokens": ["Durch", "Deu\u00b7tung", "macht", "man", "sie", "und", "durch", "Um\u00b7ge\u00b7bung", "mil\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "PPER", "KON", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Des Glaubens Bilder sind unendlich umzudeuten,", "tokens": ["Des", "Glau\u00b7bens", "Bil\u00b7der", "sind", "un\u00b7end\u00b7lich", "um\u00b7zu\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das macht so brauchbar sie bei so verschiednen Leuten.", "tokens": ["Das", "macht", "so", "brauch\u00b7bar", "sie", "bei", "so", "ver\u00b7schied\u00b7nen", "Leu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "PPER", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}}}}