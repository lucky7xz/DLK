{"textgrid.poem.40254": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vor meinem Lager liegt der helle", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor meinem Lager liegt der helle", "tokens": ["Vor", "mei\u00b7nem", "La\u00b7ger", "liegt", "der", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mondschein auf der Diele;", "tokens": ["Mond\u00b7schein", "auf", "der", "Die\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "mir war, als fiele", "tokens": ["mir", "war", ",", "als", "fie\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "auf die Schwelle", "tokens": ["auf", "die", "Schwel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "das Fr\u00fchlicht schon,", "tokens": ["das", "Fr\u00fch\u00b7licht", "schon", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "mein Auge zweifelt noch.", "tokens": ["mein", "Au\u00b7ge", "zwei\u00b7felt", "noch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und ich hebe mein Haupt und sehe,", "tokens": ["Und", "ich", "he\u00b7be", "mein", "Haupt", "und", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "sehe den hellen Mond", "tokens": ["se\u00b7he", "den", "hel\u00b7len", "Mond"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "in seiner H\u00f6he", "tokens": ["in", "sei\u00b7ner", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "gl\u00e4nzen. Und ich senke,", "tokens": ["gl\u00e4n\u00b7zen", ".", "Und", "ich", "sen\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVINF", "$.", "KON", "PPER", "VVFIN", "$,"], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.5": {"text": "senke mein Haupt \u2013 und denke", "tokens": ["sen\u00b7ke", "mein", "Haupt", "\u2013", "und", "den\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "KON", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "an meine Heimat ...", "tokens": ["an", "mei\u00b7ne", "Hei\u00b7mat", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}}}}}