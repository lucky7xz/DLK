{"dta.poem.1629": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der Sch\u00fctze.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Bogen ist mein Leben/", "tokens": ["Ein", "Bo\u00b7gen", "ist", "mein", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Thun der Pfeil/ die Sehne mein Entschlu\u00df/", "tokens": ["Mein", "Thun", "der", "Pfeil", "/", "die", "Seh\u00b7ne", "mein", "Ent\u00b7schlu\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$(", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Ehre soll das Ziel/ ich will den Sch\u00fctzen geben/", "tokens": ["Die", "Eh\u00b7re", "soll", "das", "Ziel", "/", "ich", "will", "den", "Sch\u00fct\u00b7zen", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "$(", "PPER", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nach bester M\u00f6gligkeit richt ich den Schu\u00df/", "tokens": ["Nach", "bes\u00b7ter", "M\u00f6g\u00b7lig\u00b7keit", "richt", "ich", "den", "Schu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Trifft er nicht nach Verlangen an/", "tokens": ["Trifft", "er", "nicht", "nach", "Ver\u00b7lan\u00b7gen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "NN", "PTKVZ", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "So tr\u00f6sten mich Gewissen und Gedult/", "tokens": ["So", "tr\u00f6s\u00b7ten", "mich", "Ge\u00b7wis\u00b7sen", "und", "Ge\u00b7dult", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dem Gl\u00fccke bleibt die Schuld:", "tokens": ["Dem", "Gl\u00fc\u00b7cke", "bleibt", "die", "Schuld", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Mein Absehn geht auffs rechte Ziel/", "tokens": ["Mein", "Ab\u00b7sehn", "geht", "auffs", "rech\u00b7te", "Ziel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Bogen breche wenn er will/", "tokens": ["Der", "Bo\u00b7gen", "bre\u00b7che", "wenn", "er", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "KOUS", "PPER", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ich spann ihn/ weil ich Athem holen kan.", "tokens": ["Ich", "spann", "ihn", "/", "weil", "ich", "A\u00b7them", "ho\u00b7len", "kan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "KOUS", "PPER", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}