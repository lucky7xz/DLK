{"textgrid.poem.54406": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "17. Gedichte", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hiermit beschwer ich dich, mein Herze, du sollst sagen,", "tokens": ["Hier\u00b7mit", "be\u00b7schwer", "ich", "dich", ",", "mein", "Her\u00b7ze", ",", "du", "sollst", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PRF", "$,", "PPOSAT", "VVFIN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was mich im Schlafe st\u00f6rt, und meinen Geist besiegt.", "tokens": ["Was", "mich", "im", "Schla\u00b7fe", "st\u00f6rt", ",", "und", "mei\u00b7nen", "Geist", "be\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich kann mich wachend zwar nicht \u00fcber dich beklagen,", "tokens": ["Ich", "kann", "mich", "wa\u00b7chend", "zwar", "nicht", "\u00fc\u00b7ber", "dich", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "ADV", "PTKNEG", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil die Empfindung stets der Einsicht unterliegt;", "tokens": ["Weil", "die", "Emp\u00b7fin\u00b7dung", "stets", "der", "Ein\u00b7sicht", "un\u00b7ter\u00b7liegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Allein so bald der Schlaf die matten Glieder strecket,", "tokens": ["Al\u00b7lein", "so", "bald", "der", "Schlaf", "die", "mat\u00b7ten", "Glie\u00b7der", "stre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und mich zum Tr\u00e4umen bringt, so weis ich selber nicht,", "tokens": ["Und", "mich", "zum", "Tr\u00e4u\u00b7men", "bringt", ",", "so", "weis", "ich", "sel\u00b7ber", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN", "$,", "ADV", "PTKVZ", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer mich zur Z\u00e4rtlichkeit und Schmeichelung erwecket.", "tokens": ["Wer", "mich", "zur", "Z\u00e4rt\u00b7lich\u00b7keit", "und", "Schmei\u00b7che\u00b7lung", "er\u00b7we\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mein Herze, rede doch, gieb mir hierinn ein Licht.", "tokens": ["Mein", "Her\u00b7ze", ",", "re\u00b7de", "doch", ",", "gieb", "mir", "hie\u00b7rinn", "ein", "Licht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "$,", "VVFIN", "ADV", "$,", "VVIMP", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Was hab ich dir gethan, und was ist mein Verbrechen,", "tokens": ["Was", "hab", "ich", "dir", "ge\u00b7than", ",", "und", "was", "ist", "mein", "Ver\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "VVPP", "$,", "KON", "PWS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df du dich gegen mich so fromm und fremde stellst?", "tokens": ["Da\u00df", "du", "dich", "ge\u00b7gen", "mich", "so", "fromm", "und", "frem\u00b7de", "stellst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "ADV", "ADJD", "KON", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ach willst du denn nicht mehr, wie sonst, vertraulich sprechen:", "tokens": ["Ach", "willst", "du", "denn", "nicht", "mehr", ",", "wie", "sonst", ",", "ver\u00b7trau\u00b7lich", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "$,", "PWAV", "ADV", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Entdeck das Urtheil frey, das du itzt von mir f\u00e4llst.", "tokens": ["Ent\u00b7deck", "das", "Ur\u00b7theil", "frey", ",", "das", "du", "itzt", "von", "mir", "f\u00e4llst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So klagte Doris j\u00fcngst; drauf schwanden Furcht und Kummer;", "tokens": ["So", "klag\u00b7te", "Do\u00b7ris", "j\u00fcngst", ";", "drauf", "schwan\u00b7den", "Furcht", "und", "Kum\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "$.", "PAV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Schlaf befiel sie gleich den ersten Augenblick;", "tokens": ["Der", "Schlaf", "be\u00b7fiel", "sie", "gleich", "den", "ers\u00b7ten", "Au\u00b7gen\u00b7blick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sie lag in s\u00fcsser Ruh; im angenehmsten Schlummer,", "tokens": ["Sie", "lag", "in", "s\u00fcs\u00b7ser", "Ruh", ";", "im", "an\u00b7ge\u00b7nehms\u00b7ten", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$.", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und dachte nicht auf das was sie gest\u00f6rt, zur\u00fcck.", "tokens": ["Und", "dach\u00b7te", "nicht", "auf", "das", "was", "sie", "ge\u00b7st\u00f6rt", ",", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "ART", "PRELS", "PPER", "VVPP", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Doch eh sie es geglaubt, vergn\u00fcgte sie ein Schatten;", "tokens": ["Doch", "eh", "sie", "es", "ge\u00b7glaubt", ",", "ver\u00b7gn\u00fcg\u00b7te", "sie", "ein", "Schat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVPP", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sie rief voll Ungeduld; Ach \u2013 \u2013 bist du da?", "tokens": ["Sie", "rief", "voll", "Un\u00b7ge\u00b7duld", ";", "Ach", "\u2013", "\u2013", "bist", "du", "da", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "$.", "ITJ", "$(", "$(", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "So wie die Taube girrt nach ihrem treuen Gatten;", "tokens": ["So", "wie", "die", "Tau\u00b7be", "girrt", "nach", "ih\u00b7rem", "treu\u00b7en", "Gat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und Doris wuste nicht, wie ihr vor Lust geschah.", "tokens": ["Und", "Do\u00b7ris", "wus\u00b7te", "nicht", ",", "wie", "ihr", "vor", "Lust", "ge\u00b7schah", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Sie schlug die Augen auf; sie dachte hin und wieder,", "tokens": ["Sie", "schlug", "die", "Au\u00b7gen", "auf", ";", "sie", "dach\u00b7te", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "PTKVZ", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Sie ward halb ausser sich, und die bewegte Brust", "tokens": ["Sie", "ward", "halb", "aus\u00b7ser", "sich", ",", "und", "die", "be\u00b7weg\u00b7te", "Brust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PRF", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Nahm Lieb und Schrecken ein. Es drang durch alle Glieder", "tokens": ["Nahm", "Lieb", "und", "Schre\u00b7cken", "ein", ".", "Es", "drang", "durch", "al\u00b7le", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der Vorschmack s\u00fcsser Qual, die nie empfundne Lust.", "tokens": ["Der", "Vor\u00b7schmack", "s\u00fcs\u00b7ser", "Qual", ",", "die", "nie", "emp\u00b7fund\u00b7ne", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Nun, sprach sie, kann ich erst den sch\u00f6nen Ursprung wissen,", "tokens": ["Nun", ",", "sprach", "sie", ",", "kann", "ich", "erst", "den", "sch\u00f6\u00b7nen", "Ur\u00b7sprung", "wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Daraus mein Leiden flie\u00dft, und der mir doch gef\u00e4llt.", "tokens": ["Da\u00b7raus", "mein", "Lei\u00b7den", "flie\u00dft", ",", "und", "der", "mir", "doch", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VVFIN", "$,", "KON", "ART", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ach holder Schatten komm, ich will dich feurig k\u00fcssen,", "tokens": ["Ach", "hol\u00b7der", "Schat\u00b7ten", "komm", ",", "ich", "will", "dich", "feu\u00b7rig", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "VVFIN", "$,", "PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Bis mir das Schicksal auch den K\u00f6rper zugesellt.", "tokens": ["Bis", "mir", "das", "Schick\u00b7sal", "auch", "den", "K\u00f6r\u00b7per", "zu\u00b7ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}