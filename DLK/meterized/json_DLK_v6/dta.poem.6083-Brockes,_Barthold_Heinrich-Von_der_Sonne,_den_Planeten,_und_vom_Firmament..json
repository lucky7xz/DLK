{"dta.poem.6083": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit welchem Wunder-Glantz ist eine Nacht geschm\u00fcckt,", "tokens": ["Mit", "wel\u00b7chem", "Wun\u00b7der\u00b7Glantz", "ist", "ei\u00b7ne", "Nacht", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ihre Klarheit uns ein stiller Himmel weiset,", "tokens": ["Wenn", "ih\u00b7re", "Klar\u00b7heit", "uns", "ein", "stil\u00b7ler", "Him\u00b7mel", "wei\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo man die stete Pracht mit steter Lust erblickt!", "tokens": ["Wo", "man", "die", "ste\u00b7te", "Pracht", "mit", "ste\u00b7ter", "Lust", "er\u00b7blickt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Welch Anmuth, wenn der Blick den hohen Raum durchreiset,", "tokens": ["Welch", "An\u00b7muth", ",", "wenn", "der", "Blick", "den", "ho\u00b7hen", "Raum", "durc\u00b7hrei\u00b7set", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In jenem sch\u00f6nen Creis, den man das Firmament", "tokens": ["In", "je\u00b7nem", "sch\u00f6\u00b7nen", "Creis", ",", "den", "man", "das", "Fir\u00b7ma\u00b7ment"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "PRELS", "PIS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gemeiniglich benennt:", "tokens": ["Ge\u00b7mei\u00b7nig\u00b7lich", "be\u00b7nennt", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der seiner Lichter Gold des Tags erbleichen sieht,", "tokens": ["Der", "sei\u00b7ner", "Lich\u00b7ter", "Gold", "des", "Tags", "er\u00b7blei\u00b7chen", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Um bald darauf im Dunckeln", "tokens": ["Um", "bald", "da\u00b7rauf", "im", "Dun\u00b7ckeln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "PAV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Noch herrlicher zu funckeln.", "tokens": ["Noch", "herr\u00b7li\u00b7cher", "zu", "fun\u00b7ckeln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Man siehet, wie die Lufft, als lauter Funcken, gl\u00fcht:", "tokens": ["Man", "sie\u00b7het", ",", "wie", "die", "Lufft", ",", "als", "lau\u00b7ter", "Fun\u00b7cken", ",", "gl\u00fcht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "ART", "NN", "$,", "KOUS", "PIAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die feurige Figur der Sterne,", "tokens": ["Die", "feu\u00b7ri\u00b7ge", "Fi\u00b7gur", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "Ohn\u2019 in der ungeme\u00dfnen Ferne", "tokens": ["Ohn'", "in", "der", "un\u00b7ge\u00b7me\u00df\u00b7nen", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Von ihrem Schimmer zu verliehren,", "tokens": ["Von", "ih\u00b7rem", "Schim\u00b7mer", "zu", "ver\u00b7lieh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ist noch annehmlicher und sanffter zu versp\u00fcren.", "tokens": ["Ist", "noch", "an\u00b7nehm\u00b7li\u00b7cher", "und", "sanff\u00b7ter", "zu", "ver\u00b7sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sie lassen offt, nach Tagen, welche sch\u00f6n,", "tokens": ["Sie", "las\u00b7sen", "offt", ",", "nach", "Ta\u00b7gen", ",", "wel\u00b7che", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Uns N\u00e4chte, die noch sch\u00f6ner, sehn.", "tokens": ["Uns", "N\u00e4ch\u00b7te", ",", "die", "noch", "sch\u00f6\u00b7ner", ",", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "ADV", "ADJD", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}