{"textgrid.poem.28452": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "67.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man weist uns Herre Christ in dieser Zeiten Jammer", "tokens": ["Man", "weist", "uns", "Her\u00b7re", "Christ", "in", "die\u00b7ser", "Zei\u00b7ten", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "NN", "NN", "APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald da, bald dort herum zur W\u00fcsten, zu der Kammer,", "tokens": ["Bald", "da", ",", "bald", "dort", "he\u00b7rum", "zur", "W\u00fcs\u00b7ten", ",", "zu", "der", "Kam\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "APZR", "APPRART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als seystu dort und da; wir aber glaubens nicht;", "tokens": ["Als", "seys\u00b7tu", "dort", "und", "da", ";", "wir", "a\u00b7ber", "glau\u00b7bens", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "KON", "ADV", "$.", "PPER", "ADV", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir glauben aber fest und hoffen dein Gericht.", "tokens": ["Wir", "glau\u00b7ben", "a\u00b7ber", "fest", "und", "hof\u00b7fen", "dein", "Ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-