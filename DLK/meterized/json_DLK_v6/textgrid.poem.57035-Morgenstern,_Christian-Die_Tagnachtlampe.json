{"textgrid.poem.57035": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Die Tagnachtlampe", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.85", "da:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Korf erfindet eine Tagnachtlampe,", "tokens": ["Korf", "er\u00b7fin\u00b7det", "ei\u00b7ne", "Tag\u00b7nacht\u00b7lam\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "die, sobald sie angedreht,", "tokens": ["die", ",", "so\u00b7bald", "sie", "an\u00b7ge\u00b7dreht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "selbst den hellsten Tag", "tokens": ["selbst", "den", "hells\u00b7ten", "Tag"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "in Nacht verwandelt.", "tokens": ["in", "Nacht", "ver\u00b7wan\u00b7delt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Als er sie vor des Kongresses Rampe", "tokens": ["Als", "er", "sie", "vor", "des", "Kon\u00b7gres\u00b7ses", "Ram\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "APPR", "ART", "NN", "NE"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "demonstriert, vermag", "tokens": ["de\u00b7monst\u00b7riert", ",", "ver\u00b7mag"], "token_info": ["word", "punct", "word"], "pos": ["VVFIN", "$,", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "niemand, der sein Fach versteht,", "tokens": ["nie\u00b7mand", ",", "der", "sein", "Fach", "ver\u00b7steht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "zu verkennen, da\u00df es sich hier handelt \u2013", "tokens": ["zu", "ver\u00b7ken\u00b7nen", ",", "da\u00df", "es", "sich", "hier", "han\u00b7delt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "(finster wird's am hellerlichten Tag,", "tokens": ["(", "fins\u00b7ter", "wird's", "am", "hel\u00b7ler\u00b7lich\u00b7ten", "Tag", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "und ein Beifallssturm das Haus durchweht)", "tokens": ["und", "ein", "Bei\u00b7falls\u00b7sturm", "das", "Haus", "durch\u00b7weht", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "(und man ruft dem Diener Mampe:", "tokens": ["(", "und", "man", "ruft", "dem", "Die\u00b7ner", "Mam\u00b7pe", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PIS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bblicht anz\u00fcnden!\u00ab) \u2013 da\u00df es sich hier handelt", "tokens": ["\u00bb", "licht", "an\u00b7z\u00fcn\u00b7den", "!", "\u00ab", ")", "\u2013", "da\u00df", "es", "sich", "hier", "han\u00b7delt"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "NN", "VVIZU", "$.", "$(", "$(", "$(", "KOUS", "PPER", "PRF", "ADV", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "um das Faktum: da\u00df gedachte Lampe,", "tokens": ["um", "das", "Fak\u00b7tum", ":", "da\u00df", "ge\u00b7dach\u00b7te", "Lam\u00b7pe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "KOUS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "in der Tat, wenn angedreht,", "tokens": ["in", "der", "Tat", ",", "wenn", "an\u00b7ge\u00b7dreht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "selbst den hellsten Tag", "tokens": ["selbst", "den", "hells\u00b7ten", "Tag"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "in Nacht verwandelt.", "tokens": ["in", "Nacht", "ver\u00b7wan\u00b7delt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}