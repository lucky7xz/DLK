{"dta.poem.11800": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "86.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dem Storch ward lang das Bein, um durch den Sumpf zu waten;", "tokens": ["Dem", "Storch", "ward", "lang", "das", "Bein", ",", "um", "durch", "den", "Sumpf", "zu", "wa\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ART", "NN", "$,", "KOUI", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die es zum Schwimmen braucht, der Gans ists kurz gerathen.", "tokens": ["Die", "es", "zum", "Schwim\u00b7men", "braucht", ",", "der", "Gans", "ists", "kurz", "ge\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie braucht das Ruder, um die Fl\u00e4che zu durchgleiten,", "tokens": ["Sie", "braucht", "das", "Ru\u00b7der", ",", "um", "die", "Fl\u00e4\u00b7che", "zu", "durch\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUI", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Stelze nicht, um wo sie schwimmen kann, zu schreiten.", "tokens": ["Die", "Stel\u00b7ze", "nicht", ",", "um", "wo", "sie", "schwim\u00b7men", "kann", ",", "zu", "schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$,", "KOUI", "PWAV", "PPER", "VVINF", "VMFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Verschiednes Werkzeug wohnt Verschiednen dazu bei,", "tokens": ["Ver\u00b7schied\u00b7nes", "Werk\u00b7zeug", "wohnt", "Ver\u00b7schied\u00b7nen", "da\u00b7zu", "bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "NN", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df manichfaches Spiel im einen Spielraum sei.", "tokens": ["Da\u00df", "ma\u00b7nich\u00b7fa\u00b7ches", "Spiel", "im", "ei\u00b7nen", "Spiel\u00b7raum", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPRART", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}