{"textgrid.poem.46945": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir haben lang' mit stummem Schmacherr\u00f6ten", "tokens": ["Wir", "ha\u00b7ben", "lang'", "mit", "stum\u00b7mem", "Schma\u00b7cher\u00b7r\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geblickt auf uns und unsres Landes Schande,", "tokens": ["Ge\u00b7blickt", "auf", "uns", "und", "uns\u00b7res", "Lan\u00b7des", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPER", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu dir aufhebend unsres Armes Bande:", "tokens": ["Zu", "dir", "auf\u00b7he\u00b7bend", "uns\u00b7res", "Ar\u00b7mes", "Ban\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "\u00bbwie lang', Herr, willst du sie noch fester l\u00f6ten?\u00ab", "tokens": ["\u00bb", "wie", "lang'", ",", "Herr", ",", "willst", "du", "sie", "noch", "fes\u00b7ter", "l\u00f6\u00b7ten", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOKOM", "ADV", "$,", "NN", "$,", "VMFIN", "PPER", "PPER", "ADV", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Jetzt willst du dich, o Retter in den N\u00f6ten,", "tokens": ["Jetzt", "willst", "du", "dich", ",", "o", "Ret\u00b7ter", "in", "den", "N\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "$,", "FM", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Erbarmen wieder \u00fcber deinem Lande;", "tokens": ["Er\u00b7bar\u00b7men", "wie\u00b7der", "\u00fc\u00b7ber", "dei\u00b7nem", "Lan\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Rettung kommt, sie kommt im St\u00e4dtebrande", "tokens": ["Die", "Ret\u00b7tung", "kommt", ",", "sie", "kommt", "im", "St\u00e4d\u00b7te\u00b7bran\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Von dir, sie kommt in blut'gen Morgenr\u00f6ten.", "tokens": ["Von", "dir", ",", "sie", "kommt", "in", "blut'\u00b7gen", "Mor\u00b7gen\u00b7r\u00f6\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "O Herr, vom Schweren kann nur Schweres l\u00f6sen,", "tokens": ["O", "Herr", ",", "vom", "Schwe\u00b7ren", "kann", "nur", "Schwe\u00b7res", "l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPRART", "NN", "VMFIN", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und wir sind schwergeb\u00fcckt in unserm Staube;", "tokens": ["Und", "wir", "sind", "schwer\u00b7ge\u00b7b\u00fcckt", "in", "un\u00b7serm", "Stau\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "O eile du die Kraft uns einzufl\u00f6\u00dfen", "tokens": ["O", "ei\u00b7le", "du", "die", "Kraft", "uns", "ein\u00b7zu\u00b7fl\u00f6\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Zum Auferstehn! La\u00df nicht dem Sturm zum Raube", "tokens": ["Zum", "Auf\u00b7er\u00b7stehn", "!", "La\u00df", "nicht", "dem", "Sturm", "zum", "Rau\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "VVIMP", "PTKNEG", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Uns werden in der Rettung Sturmget\u00f6sen;", "tokens": ["Uns", "wer\u00b7den", "in", "der", "Ret\u00b7tung", "Sturm\u00b7ge\u00b7t\u00f6\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Panier sei Hoffnung, unser Schild dein Glaube!", "tokens": ["Pa\u00b7nier", "sei", "Hoff\u00b7nung", ",", "un\u00b7ser", "Schild", "dein", "Glau\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}