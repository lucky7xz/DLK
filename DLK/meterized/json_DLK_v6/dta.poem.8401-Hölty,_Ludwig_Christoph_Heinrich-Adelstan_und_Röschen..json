{"dta.poem.8401": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Adelstan und R\u00f6schen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Und alles wurde froh,", "tokens": ["Und", "al\u00b7les", "wur\u00b7de", "froh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Als Ritter Veit von Adelstan", "tokens": ["Als", "Rit\u00b7ter", "Veit", "von", "A\u00b7del\u00b7stan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "NE", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der K\u00f6nigsstadt entfloh.", "tokens": ["Der", "K\u00f6\u00b7nigs\u00b7stadt", "ent\u00b7floh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von Geigern und Kastraten fern", "tokens": ["Von", "Gei\u00b7gern", "und", "Kas\u00b7tra\u00b7ten", "fern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADJD"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und vom Redutentanz,", "tokens": ["Und", "vom", "Re\u00b7du\u00b7ten\u00b7tanz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vertauscht' er seinen goldnen Stern", "tokens": ["Ver\u00b7tauscht'", "er", "sei\u00b7nen", "gold\u00b7nen", "Stern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit einem Sch\u00e4ferkranz.", "tokens": ["Mit", "ei\u00b7nem", "Sch\u00e4\u00b7fer\u00b7kranz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Schooss der Au, der Wiesenklee", "tokens": ["Der", "Scho\u00b7oss", "der", "Au", ",", "der", "Wie\u00b7sen\u00b7klee"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verlieh ihm s\u00fcssre Rast,", "tokens": ["Ver\u00b7lieh", "ihm", "s\u00fcss\u00b7re", "Rast", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als Himmelbett' und Kanapee", "tokens": ["Als", "Him\u00b7mel\u00b7bett'", "und", "Ka\u00b7na\u00b7pee"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im f\u00fcrstlichen Palast.", "tokens": ["Im", "f\u00fcrst\u00b7li\u00b7chen", "Pa\u00b7last", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er irrte t\u00e4glich durch den Hain,", "tokens": ["Er", "irr\u00b7te", "t\u00e4g\u00b7lich", "durch", "den", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit einer Brust voll Ruh,", "tokens": ["Mit", "ei\u00b7ner", "Brust", "voll", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und sah dem Spiel' und sah dem Reihn", "tokens": ["Und", "sah", "dem", "Spiel'", "und", "sah", "dem", "Reihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der D\u00f6rferinnen zu;", "tokens": ["Der", "D\u00f6r\u00b7fe\u00b7rin\u00b7nen", "zu", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sah unter niederm H\u00fcttendach", "tokens": ["Sah", "un\u00b7ter", "nie\u00b7derm", "H\u00fct\u00b7ten\u00b7dach"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sch\u00e4ferinnen Preis:", "tokens": ["Der", "Sch\u00e4\u00b7fe\u00b7rin\u00b7nen", "Preis", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und pl\u00f6zlich schlug sein Herzensschlag", "tokens": ["Und", "pl\u00f6z\u00b7lich", "schlug", "sein", "Her\u00b7zens\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wol noch einmal so heiss.", "tokens": ["Wol", "noch", "ein\u00b7mal", "so", "heiss", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Sie wurden drauf gar bald vertraut;", "tokens": ["Sie", "wur\u00b7den", "drauf", "gar", "bald", "ver\u00b7traut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was Wunder doch! Er war", "tokens": ["Was", "Wun\u00b7der", "doch", "!", "Er", "war"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "NN", "ADV", "$.", "PPER", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein Mann von Welt und wohlgebaut,", "tokens": ["Ein", "Mann", "von", "Welt", "und", "wohl\u00b7ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und R\u00f6schen achzehn Jahr.", "tokens": ["Und", "R\u00f6\u00b7schen", "ach\u00b7zehn", "Jahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie gab, durch manchen Thr\u00e4nenguss", "tokens": ["Sie", "gab", ",", "durch", "man\u00b7chen", "Thr\u00e4\u00b7nen\u00b7guss"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erweichet, ihm Geh\u00f6r;", "tokens": ["Er\u00b7wei\u00b7chet", ",", "ihm", "Ge\u00b7h\u00f6r", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zuerst bekam er einen Kuss", "tokens": ["Zu\u00b7erst", "be\u00b7kam", "er", "ei\u00b7nen", "Kuss"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zulezt noch etwas mehr.", "tokens": ["Zu\u00b7lezt", "noch", "et\u00b7was", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Izt wurde, nach des Hofes Brauch,", "tokens": ["Izt", "wur\u00b7de", ",", "nach", "des", "Ho\u00b7fes", "Brauch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sein Busen pl\u00f6zlich lau:", "tokens": ["Sein", "Bu\u00b7sen", "pl\u00f6z\u00b7lich", "lau", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er sass nicht mehr am Schlehenstrauch", "tokens": ["Er", "sass", "nicht", "mehr", "am", "Schle\u00b7hen\u00b7strauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit R\u00f6schen auf der Au.", "tokens": ["Mit", "R\u00f6\u00b7schen", "auf", "der", "Au", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Des Dorfes und des M\u00e4dchens satt,", "tokens": ["Des", "Dor\u00b7fes", "und", "des", "M\u00e4d\u00b7chens", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Warf er sich auf sein Ross,", "tokens": ["Warf", "er", "sich", "auf", "sein", "Ross", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Flog wieder in die K\u00f6nigsstadt,", "tokens": ["Flog", "wie\u00b7der", "in", "die", "K\u00f6\u00b7nigs\u00b7stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und in sein Marmorschloss.", "tokens": ["Und", "in", "sein", "Mar\u00b7mor\u00b7schloss", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hier taumelt' er von Ball zu Ball,", "tokens": ["Hier", "tau\u00b7melt'", "er", "von", "Ball", "zu", "Ball", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vergass der Rasenbank,", "tokens": ["Ver\u00b7gass", "der", "Ra\u00b7sen\u00b7bank", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo beim Get\u00f6n der Nachtigall", "tokens": ["Wo", "beim", "Ge\u00b7t\u00f6n", "der", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sein M\u00e4dchen ihn umschlang.", "tokens": ["Sein", "M\u00e4d\u00b7chen", "ihn", "um\u00b7schlang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und R\u00f6schen, die auf Wiesengr\u00fcn", "tokens": ["Und", "R\u00f6\u00b7schen", ",", "die", "auf", "Wie\u00b7sen\u00b7gr\u00fcn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Haselschatten sass,", "tokens": ["Im", "Ha\u00b7sel\u00b7schat\u00b7ten", "sass", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sah Mann und Ross vor\u00fcberfliehn,", "tokens": ["Sah", "Mann", "und", "Ross", "vor\u00b7\u00fc\u00b7berf\u00b7liehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wurde todtenblass.", "tokens": ["Und", "wur\u00b7de", "tod\u00b7ten\u00b7blass", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Adelstan! ich armes Blut!", "tokens": ["Mein", "A\u00b7del\u00b7stan", "!", "ich", "ar\u00b7mes", "Blut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er sah und h\u00f6rte nicht,", "tokens": ["Er", "sah", "und", "h\u00f6r\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und dr\u00fcckte sich den Reisehut", "tokens": ["Und", "dr\u00fcck\u00b7te", "sich", "den", "Rei\u00b7se\u00b7hut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nur tiefer ins Gesicht.", "tokens": ["Nur", "tie\u00b7fer", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sie zupft', auf ihren Hirtenstab", "tokens": ["Sie", "zupft'", ",", "auf", "ih\u00b7ren", "Hir\u00b7ten\u00b7stab"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gelehnt, am Busenband,", "tokens": ["Ge\u00b7lehnt", ",", "am", "Bu\u00b7sen\u00b7band", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis er dem Ross die Spornen gab,", "tokens": ["Bis", "er", "dem", "Ross", "die", "Spor\u00b7nen", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihrem Aug' entschwand;", "tokens": ["Und", "ih\u00b7rem", "Aug'", "ent\u00b7schwand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und schluchzt', und warf sich in das Gras,", "tokens": ["Und", "schluchzt'", ",", "und", "warf", "sich", "in", "das", "Gras", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verbarg sich ins Gestr\u00e4uch,", "tokens": ["Ver\u00b7barg", "sich", "ins", "Ge\u00b7str\u00e4uch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Weint ihren sch\u00f6nen Busen nass,", "tokens": ["Weint", "ih\u00b7ren", "sch\u00f6\u00b7nen", "Bu\u00b7sen", "nass", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und ihre Wangen bleich.", "tokens": ["Und", "ih\u00b7re", "Wan\u00b7gen", "bleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Kein Tanz, kein Spiel behagt' ihr mehr.", "tokens": ["Kein", "Tanz", ",", "kein", "Spiel", "be\u00b7hagt'", "ihr", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Abendroth, kein West;", "tokens": ["Kein", "A\u00b7ben\u00b7droth", ",", "kein", "West", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das D\u00f6rfchen d\u00fcnkt ihr freudenleer,", "tokens": ["Das", "D\u00f6rf\u00b7chen", "d\u00fcnkt", "ihr", "freu\u00b7den\u00b7leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Flur ein Otternnest.", "tokens": ["Die", "Flur", "ein", "Ot\u00b7tern\u00b7nest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein melancholisch Heimchen zirpt", "tokens": ["Ein", "me\u00b7lan\u00b7cho\u00b7lisch", "Heim\u00b7chen", "zirpt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Vor ihrer Kammerth\u00fcr;", "tokens": ["Vor", "ih\u00b7rer", "Kam\u00b7mert\u00b7h\u00fcr", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das 