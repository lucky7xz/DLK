{"textgrid.poem.32388": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "66. An die Dorilis", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein H\u00fcndchen, Dorilis, ist z\u00e4rtlich, t\u00e4ndelnd, rein:", "tokens": ["Dein", "H\u00fcnd\u00b7chen", ",", "Do\u00b7ri\u00b7lis", ",", "ist", "z\u00e4rt\u00b7lich", ",", "t\u00e4n\u00b7delnd", ",", "rein", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NE", "$,", "VAFIN", "ADJD", "$,", "VVPP", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du es also leckst, soll das mich wundern? nein!", "tokens": ["Da\u00df", "du", "es", "al\u00b7so", "leckst", ",", "soll", "das", "mich", "wun\u00b7dern", "?", "nein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,", "VMFIN", "ART", "PPER", "VVINF", "$.", "PTKANT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein dein H\u00fcndchen lecket dich:", "tokens": ["Al\u00b7lein", "dein", "H\u00fcnd\u00b7chen", "le\u00b7cket", "dich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dieses wundert mich.", "tokens": ["Und", "die\u00b7ses", "wun\u00b7dert", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}