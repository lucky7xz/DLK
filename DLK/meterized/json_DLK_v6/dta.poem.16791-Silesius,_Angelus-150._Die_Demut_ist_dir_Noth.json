{"dta.poem.16791": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "150.  Die Demut ist dir Noth/", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh nur fein unter dich: du fleuchst den Blitz der Zeit/", "tokens": ["Sieh", "nur", "fein", "un\u00b7ter", "dich", ":", "du", "fleuchst", "den", "Blitz", "der", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "APPR", "PPER", "$.", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was meinstu dann zu schaun in Blitz der Ewigkeit?", "tokens": ["Was", "meins\u00b7tu", "dann", "zu", "schaun", "in", "Blitz", "der", "E\u00b7wig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PTKZU", "VVINF", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}