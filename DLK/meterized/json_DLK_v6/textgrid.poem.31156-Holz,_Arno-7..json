{"textgrid.poem.31156": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonnengluthen, Abendschatten", "tokens": ["Son\u00b7nen\u00b7glu\u00b7then", ",", "A\u00b7bend\u00b7schat\u00b7ten"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wechselten im alten Gleise,", "tokens": ["Wech\u00b7sel\u00b7ten", "im", "al\u00b7ten", "Glei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und auch dir, dem Qualenmatten,", "tokens": ["Und", "auch", "dir", ",", "dem", "Qua\u00b7len\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00f6nt ins Ohr die gleiche Weise:", "tokens": ["T\u00f6nt", "ins", "Ohr", "die", "glei\u00b7che", "Wei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ging das Gestern, kommt das heute", "tokens": ["Ging", "das", "Ge\u00b7stern", ",", "kommt", "das", "heu\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "PDS", "ADV"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Und am Ende auch das Morgen,", "tokens": ["Und", "am", "En\u00b7de", "auch", "das", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch in alle drei als Beute", "tokens": ["Doch", "in", "al\u00b7le", "drei", "als", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "CARD", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Theilen gierig sich die Sorgen.", "tokens": ["Thei\u00b7len", "gie\u00b7rig", "sich", "die", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sonnengluthen, Abendschatten", "tokens": ["Son\u00b7nen\u00b7glu\u00b7then", ",", "A\u00b7bend\u00b7schat\u00b7ten"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6nnen nicht von selber enden,", "tokens": ["K\u00f6n\u00b7nen", "nicht", "von", "sel\u00b7ber", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPR", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber dir, den Lebenssatten,", "tokens": ["A\u00b7ber", "dir", ",", "den", "Le\u00b7bens\u00b7sat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist's verg\u00f6nnt, sein Loos zu wenden.", "tokens": ["Ist's", "ver\u00b7g\u00f6nnt", ",", "sein", "Loos", "zu", "wen\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht umsonst sei dir gegeben,", "tokens": ["Nicht", "um\u00b7sonst", "sei", "dir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Was Natur den andern schuldig:", "tokens": ["Was", "Na\u00b7tur", "den", "an\u00b7dern", "schul\u00b7dig", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ART", "ADJA", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Drum so ende du dein Leben,", "tokens": ["Drum", "so", "en\u00b7de", "du", "dein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Oder trag es still geduldig!", "tokens": ["O\u00b7der", "trag", "es", "still", "ge\u00b7dul\u00b7dig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}