{"textgrid.poem.38805": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Muthwill und Spiel und Ku\u00df stehn mir zur Seiten,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Muthwill und Spiel und Ku\u00df stehn mir zur Seiten,", "tokens": ["Mut\u00b7hwill", "und", "Spiel", "und", "Ku\u00df", "stehn", "mir", "zur", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KON", "NN", "KON", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mit Erstaunen sehn sie, da\u00df ich schreibe,", "tokens": ["Und", "mit", "Er\u00b7stau\u00b7nen", "sehn", "sie", ",", "da\u00df", "ich", "schrei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie fragen kindisch: was soll das bedeuten?", "tokens": ["Sie", "fra\u00b7gen", "kin\u00b7disch", ":", "was", "soll", "das", "be\u00b7deu\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "PWS", "VMFIN", "PDS", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich sage ihnen, wie entfernt ich bleibe", "tokens": ["Ich", "sa\u00b7ge", "ih\u00b7nen", ",", "wie", "ent\u00b7fernt", "ich", "blei\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ADJD", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von dir noch immer, s\u00fc\u00dfe Braut; geleiten", "tokens": ["Von", "dir", "noch", "im\u00b7mer", ",", "s\u00fc\u00b7\u00dfe", "Braut", ";", "ge\u00b7lei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "PPER", "ADV", "ADV", "$,", "ADJA", "NN", "$.", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Will ich dich hin, so spricht die Hoffnung, treibe", "tokens": ["Will", "ich", "dich", "hin", ",", "so", "spricht", "die", "Hoff\u00b7nung", ",", "trei\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "PRF", "PTKVZ", "$,", "ADV", "VVFIN", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Den Scherz nur fort und andre die hier plaudern.", "tokens": ["Den", "Scherz", "nur", "fort", "und", "and\u00b7re", "die", "hier", "plau\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "KON", "PIS", "ART", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein Wink, sie gehn und d\u00fcrfen nicht mehr zaudern.", "tokens": ["Ein", "Wink", ",", "sie", "gehn", "und", "d\u00fcr\u00b7fen", "nicht", "mehr", "zau\u00b7dern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVINF", "KON", "VMFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit dir allein, allein mit meinen Thr\u00e4nen,", "tokens": ["Mit", "dir", "al\u00b7lein", ",", "al\u00b7lein", "mit", "mei\u00b7nen", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "$,", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Hoffnung, milde Tr\u00f6sterin, nun eile", "tokens": ["O", "Hoff\u00b7nung", ",", "mil\u00b7de", "Tr\u00f6s\u00b7te\u00b7rin", ",", "nun", "ei\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit mir, es fliegt voran mein br\u00fcnstig Sehnen,", "tokens": ["Mit", "mir", ",", "es", "fliegt", "vo\u00b7ran", "mein", "br\u00fcns\u00b7tig", "Seh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "VVFIN", "ADV", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O mach, da\u00df ich im Wink die Ferne theile,", "tokens": ["O", "mach", ",", "da\u00df", "ich", "im", "Wink", "die", "Fer\u00b7ne", "thei\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df ich mich nicht mehr darf verlassen w\u00e4hnen!", "tokens": ["Da\u00df", "ich", "mich", "nicht", "mehr", "darf", "ver\u00b7las\u00b7sen", "w\u00e4h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "ADV", "VMFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie l\u00e4chelt dann so still und spricht: verweile,", "tokens": ["Sie", "l\u00e4\u00b7chelt", "dann", "so", "still", "und", "spricht", ":", "ver\u00b7wei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "KON", "VVFIN", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nicht zu ihr sollst du fliehen, sie kehrt zur\u00fccke,", "tokens": ["Nicht", "zu", "ihr", "sollst", "du", "flie\u00b7hen", ",", "sie", "kehrt", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "VMFIN", "PPER", "VVINF", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "In vorger Heimath winket dir dein Gl\u00fccke.", "tokens": ["In", "vor\u00b7ger", "Hei\u00b7math", "win\u00b7ket", "dir", "dein", "Gl\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So geht sie fort, und aus dem Boden spriessen", "tokens": ["So", "geht", "sie", "fort", ",", "und", "aus", "dem", "Bo\u00b7den", "spries\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur Schmerzen wo gewandelt hat ihr Fu\u00df,", "tokens": ["Nur", "Schmer\u00b7zen", "wo", "ge\u00b7wan\u00b7delt", "hat", "ihr", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PWAV", "VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die zarten Bl\u00fcthen mu\u00df ich dann begie\u00dfen", "tokens": ["Die", "zar\u00b7ten", "Bl\u00fc\u00b7then", "mu\u00df", "ich", "dann", "be\u00b7gie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Thr\u00e4nen, und in Seufzern str\u00f6mt ihr Flu\u00df; \u2013", "tokens": ["Mit", "Thr\u00e4\u00b7nen", ",", "und", "in", "Seuf\u00b7zern", "str\u00f6mt", "ihr", "Flu\u00df", ";", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$,", "KON", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich denke dich, und wie sie immer flie\u00dfen", "tokens": ["Ich", "den\u00b7ke", "dich", ",", "und", "wie", "sie", "im\u00b7mer", "flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "PWAV", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Steigt vor mir auf dein Blick, dein Wink, dein Ku\u00df,", "tokens": ["Steigt", "vor", "mir", "auf", "dein", "Blick", ",", "dein", "Wink", ",", "dein", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du wandelst auf den Schmerzen hin und wieder,", "tokens": ["Du", "wan\u00b7delst", "auf", "den", "Schmer\u00b7zen", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es schwingt sich wie Musik der Bau der Glieder.", "tokens": ["Es", "schwingt", "sich", "wie", "Mu\u00b7sik", "der", "Bau", "der", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KOKOM", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du l\u00e4chelst und vom rosenrothen Munde", "tokens": ["Du", "l\u00e4\u00b7chelst", "und", "vom", "ro\u00b7sen\u00b7ro\u00b7then", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gehn Worte und ein wonnetrunkner Laut,", "tokens": ["Gehn", "Wor\u00b7te", "und", "ein", "won\u00b7ne\u00b7trunk\u00b7ner", "Laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du redest von der sch\u00f6nen einzgen Stunde", "tokens": ["Du", "re\u00b7dest", "von", "der", "sch\u00f6\u00b7nen", "einz\u00b7gen", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als du geworden meiner Liebe Braut,", "tokens": ["Als", "du", "ge\u00b7wor\u00b7den", "mei\u00b7ner", "Lie\u00b7be", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAPP", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als unter Freud' und Schmerz im s\u00fc\u00dfen Bunde", "tokens": ["Als", "un\u00b7ter", "Freud'", "und", "Schmerz", "im", "s\u00fc\u00b7\u00dfen", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "KON", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dein Herz dem meinigen sich ganz vertraut,", "tokens": ["Dein", "Herz", "dem", "mei\u00b7ni\u00b7gen", "sich", "ganz", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "PPOSS", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Als wir nicht wu\u00dften mehr ob Thr\u00e4nen, Lust,", "tokens": ["Als", "wir", "nicht", "wu\u00df\u00b7ten", "mehr", "ob", "Thr\u00e4\u00b7nen", ",", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "ADV", "KOUS", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Fest druckte Mund an Mund und Brust an Brust.", "tokens": ["Fest", "druck\u00b7te", "Mund", "an", "Mund", "und", "Brust", "an", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Geheimni\u00df s\u00fc\u00df in allen K\u00fcssen webte,", "tokens": ["Ge\u00b7heim\u00b7ni\u00df", "s\u00fc\u00df", "in", "al\u00b7len", "K\u00fcs\u00b7sen", "web\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das sich so zart in unsre Lippen senkte,", "tokens": ["Das", "sich", "so", "zart", "in", "uns\u00b7re", "Lip\u00b7pen", "senk\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Liebes-Geist auf unsern Zungen strebte,", "tokens": ["Ein", "Lie\u00b7bes\u00b7Geist", "auf", "un\u00b7sern", "Zun\u00b7gen", "streb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der in dem Balsam jedes Wort ertr\u00e4nkte,", "tokens": ["Der", "in", "dem", "Bal\u00b7sam", "je\u00b7des", "Wort", "er\u00b7tr\u00e4nk\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was nicht als Lieb' in unsrer Seele lebte,", "tokens": ["Was", "nicht", "als", "Lieb'", "in", "uns\u00b7rer", "See\u00b7le", "leb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "KOUS", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was zweifelnd ja noch unsern Geist bewegte,", "tokens": ["Was", "zwei\u00b7felnd", "ja", "noch", "un\u00b7sern", "Geist", "be\u00b7weg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das wurde nun im tiefsten Meer verlohren,", "tokens": ["Das", "wur\u00b7de", "nun", "im", "tiefs\u00b7ten", "Meer", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und aus den Wogen Venus selbst gebohren.", "tokens": ["Und", "aus", "den", "Wo\u00b7gen", "Ve\u00b7nus", "selbst", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Seitdem beherrscht Liebe nur unsre Sinne,", "tokens": ["Seit\u00b7dem", "be\u00b7herrscht", "Lie\u00b7be", "nur", "uns\u00b7re", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Von Liebe klingt in uns das rothe Blut,", "tokens": ["Von", "Lie\u00b7be", "klingt", "in", "uns", "das", "ro\u00b7the", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So hat bezwungen uns die Kraft der Minne", "tokens": ["So", "hat", "be\u00b7zwun\u00b7gen", "uns", "die", "Kraft", "der", "Min\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ewigen Triumph der trunkne Muth", "tokens": ["Da\u00df", "e\u00b7wi\u00b7gen", "Tri\u00b7umph", "der", "trunk\u00b7ne", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nur feiern will, da\u00df nichts uns zum Gewinne,", "tokens": ["Nur", "fei\u00b7ern", "will", ",", "da\u00df", "nichts", "uns", "zum", "Ge\u00b7win\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$,", "KOUS", "PIS", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measu