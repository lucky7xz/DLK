{"dta.poem.13717": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Schie\u00dft zu, trefft, Pfeile, die durch's Dunkel schwirren!", "tokens": ["Schie\u00dft", "zu", ",", "trefft", ",", "Pfei\u00b7le", ",", "die", "durch's", "Dun\u00b7kel", "schwir\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "$,", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ruhvoll um Klippen \u00fcber'm t\u00fcckschen Grunde", "tokens": ["Ruh\u00b7voll", "um", "Klip\u00b7pen", "\u00fc\u00b7ber'm", "t\u00fcck\u00b7schen", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "APPRART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Lenk' ich mein Schiff, wohin die Sterne winken.", "tokens": ["Lenk'", "ich", "mein", "Schiff", ",", "wo\u00b7hin", "die", "Ster\u00b7ne", "win\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}