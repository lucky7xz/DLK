{"textgrid.poem.36366": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der heilige Dulder", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein K\u00e4mpfer mir bekannt,", "tokens": ["Es", "ist", "ein", "K\u00e4mp\u00b7fer", "mir", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der nie hat Blut vergossen;", "tokens": ["Der", "nie", "hat", "Blut", "ver\u00b7gos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Fromm ist er und ein Hirt genannt,", "tokens": ["Fromm", "ist", "er", "und", "ein", "Hirt", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "KON", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur Segen kommt von seiner Hand", "tokens": ["Nur", "Se\u00b7gen", "kommt", "von", "sei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und himmlisch Heil geflossen.", "tokens": ["Und", "himm\u00b7lisch", "Heil", "ge\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dem Geier, der die L\u00fcfte mi\u00dft,", "tokens": ["Dem", "Gei\u00b7er", ",", "der", "die", "L\u00fcf\u00b7te", "mi\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er hat ihm widerstanden;", "tokens": ["Er", "hat", "ihm", "wi\u00b7der\u00b7stan\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er kennt des Tigers blut'ge List,", "tokens": ["Er", "kennt", "des", "Ti\u00b7gers", "blut'\u00b7ge", "List", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Drachen, der im Abgrund ist,", "tokens": ["Den", "Dra\u00b7chen", ",", "der", "im", "Ab\u00b7grund", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Frei wandelt er in Banden.", "tokens": ["Frei", "wan\u00b7delt", "er", "in", "Ban\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hatt' ihn des Argen Trug bet\u00f6rt,", "tokens": ["Hatt'", "ihn", "des", "Ar\u00b7gen", "Trug", "be\u00b7t\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verbergend seine Werke;", "tokens": ["Ver\u00b7ber\u00b7gend", "sei\u00b7ne", "Wer\u00b7ke", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schnell hat er sich von dem gekehrt,", "tokens": ["Schnell", "hat", "er", "sich", "von", "dem", "ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PRF", "APPR", "ART", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seitdem ist ihm die Kraft gemehrt,", "tokens": ["Seit\u00b7dem", "ist", "ihm", "die", "Kraft", "ge\u00b7mehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Da\u00df er die Br\u00fcder st\u00e4rke.", "tokens": ["Da\u00df", "er", "die", "Br\u00fc\u00b7der", "st\u00e4r\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Herrscher schimmern sonder Zahl", "tokens": ["Die", "Herr\u00b7scher", "schim\u00b7mern", "son\u00b7der", "Zahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Stolz auf ihren Thronen;", "tokens": ["Mit", "Stolz", "auf", "ih\u00b7ren", "Thro\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch fa\u00dft sie grimme Todesqual,", "tokens": ["Doch", "fa\u00dft", "sie", "grim\u00b7me", "To\u00b7des\u00b7qual", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie m\u00fcssen fort zum grausen Mahl,", "tokens": ["Sie", "m\u00fcs\u00b7sen", "fort", "zum", "grau\u00b7sen", "Mahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKVZ", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zerbrochen sind die Kronen.", "tokens": ["Zer\u00b7bro\u00b7chen", "sind", "die", "Kro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Im Schlaf begraben liegt die Welt,", "tokens": ["Im", "Schlaf", "be\u00b7gra\u00b7ben", "liegt", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und unten braust die H\u00f6lle;", "tokens": ["Und", "un\u00b7ten", "braust", "die", "H\u00f6l\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Himmel ist des Hirten Zelt,", "tokens": ["Im", "Him\u00b7mel", "ist", "des", "Hir\u00b7ten", "Zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den keine ird'sche Fessel h\u00e4lt,", "tokens": ["Den", "kei\u00b7ne", "ird'\u00b7sche", "Fes\u00b7sel", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er eilt hinauf zur Stelle.", "tokens": ["Er", "eilt", "hin\u00b7auf", "zur", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Geheiligt und verkl\u00e4rt schon hier", "tokens": ["Ge\u00b7hei\u00b7ligt", "und", "ver\u00b7kl\u00e4rt", "schon", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schwebt er hinauf zum Lichte;", "tokens": ["Schwebt", "er", "hin\u00b7auf", "zum", "Lich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein Blut weiht er Erl\u00f6ser, Dir!", "tokens": ["Sein", "Blut", "weiht", "er", "Er\u00b7l\u00f6\u00b7ser", ",", "Dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "NN", "$,", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er sieht des Himmels Liebeszier", "tokens": ["Er", "sieht", "des", "Him\u00b7mels", "Lie\u00b7bes\u00b7zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In g\u00f6ttlichem Gesichte.", "tokens": ["In", "g\u00f6tt\u00b7li\u00b7chem", "Ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ihn hindert nicht des Feindes Hohn", "tokens": ["Ihn", "hin\u00b7dert", "nicht", "des", "Fein\u00b7des", "Hohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Glauben sich zu g\u00fcrten.", "tokens": ["Im", "Glau\u00b7ben", "sich", "zu", "g\u00fcr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er ist's, des Lichtes wahrer Sohn,", "tokens": ["Er", "ist's", ",", "des", "Lich\u00b7tes", "wah\u00b7rer", "Sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Werft Herrscher! euch von eurem Thron", "tokens": ["Werft", "Herr\u00b7scher", "!", "euch", "von", "eu\u00b7rem", "Thron"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "$.", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu beten vor dem Hirten.", "tokens": ["Zu", "be\u00b7ten", "vor", "dem", "Hir\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}