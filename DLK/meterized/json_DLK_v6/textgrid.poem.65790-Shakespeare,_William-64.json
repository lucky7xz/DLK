{"textgrid.poem.65790": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "64", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sah ich der Alten stolze Wunderpracht", "tokens": ["Sah", "ich", "der", "Al\u00b7ten", "stol\u00b7ze", "Wun\u00b7der\u00b7pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch W\u00fctrichshand der Zeit gest\u00fcrzt verwittern,", "tokens": ["Durch", "W\u00fct\u00b7richs\u00b7hand", "der", "Zeit", "ge\u00b7st\u00fcrzt", "ver\u00b7wit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Erde hohe T\u00fcrme gleichgemacht,", "tokens": ["Der", "Er\u00b7de", "ho\u00b7he", "T\u00fcr\u00b7me", "gleich\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Unsterblich Erz vor Menschenwut erzittern:", "tokens": ["U\u00b7nsterb\u00b7lich", "Erz", "vor", "Men\u00b7schen\u00b7wut", "er\u00b7zit\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Sah ich die gierige See am K\u00f6nigreich", "tokens": ["Sah", "ich", "die", "gie\u00b7ri\u00b7ge", "See", "am", "K\u00f6\u00b7nig\u00b7reich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Der Meeresk\u00fcsten \u00fcberflutend zehren,", "tokens": ["Der", "Mee\u00b7res\u00b7k\u00fcs\u00b7ten", "\u00fc\u00b7berf\u00b7lu\u00b7tend", "zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Feste dann, an Wassersch\u00e4tzen reich,", "tokens": ["Die", "Fes\u00b7te", "dann", ",", "an", "Was\u00b7ser\u00b7sch\u00e4t\u00b7zen", "reich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "F\u00fclle mit Raub, und Raub mit F\u00fclle mehren:", "tokens": ["F\u00fcl\u00b7le", "mit", "Raub", ",", "und", "Raub", "mit", "F\u00fcl\u00b7le", "meh\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "KON", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "Wenn ich dies Wandelleben \u00fcbersah,", "tokens": ["Wenn", "ich", "dies", "Wan\u00b7del\u00b7le\u00b7ben", "\u00fc\u00b7ber\u00b7sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ja Leben selbst zum Untergang getrieben,", "tokens": ["Ja", "Le\u00b7ben", "selbst", "zum", "Un\u00b7ter\u00b7gang", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Kam unter Tr\u00fcmmern mir dies Gr\u00fcbeln nah:", "tokens": ["Kam", "un\u00b7ter", "Tr\u00fcm\u00b7mern", "mir", "dies", "Gr\u00fc\u00b7beln", "nah", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "PPER", "PDS", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Einst kommt auch Zeit und fordert deinen Lieben. \u2013", "tokens": ["Einst", "kommt", "auch", "Zeit", "und", "for\u00b7dert", "dei\u00b7nen", "Lie\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "KON", "VVFIN", "PPOSAT", "ADJA", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Solch ein Gedank' ist wie ein Tod; er treibt", "tokens": ["Solch", "ein", "Ge\u00b7dank'", "ist", "wie", "ein", "Tod", ";", "er", "treibt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Zum Weinen, da\u00df du hast, was dir nicht bleibt.", "tokens": ["Zum", "Wei\u00b7nen", ",", "da\u00df", "du", "hast", ",", "was", "dir", "nicht", "bleibt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "VAFIN", "$,", "PWS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}