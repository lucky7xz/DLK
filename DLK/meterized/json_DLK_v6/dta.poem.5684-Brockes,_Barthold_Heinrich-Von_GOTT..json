{"dta.poem.5684": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von GOTT.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man sieht den Jrrthum nicht, nicht Falschheit, recht mit", "tokens": ["Man", "sieht", "den", "Jrr\u00b7thum", "nicht", ",", "nicht", "Falschheit", ",", "recht", "mit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "PTKNEG", "$,", "PTKNEG", "NN", "$,", "ADV", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Klarheit,", "tokens": ["Klar\u00b7heit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Man halte sie denn zu der Wahrheit.", "tokens": ["Man", "hal\u00b7te", "sie", "denn", "zu", "der", "Wahr\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Setzt man der Blindheit nicht entgegen das Gesicht;", "tokens": ["Setzt", "man", "der", "Blind\u00b7heit", "nicht", "ent\u00b7ge\u00b7gen", "das", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "So ist auch keine Blindheit nicht.", "tokens": ["So", "ist", "auch", "kei\u00b7ne", "Blind\u00b7heit", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Gr\u00e4nzen, welche endlich, und allein", "tokens": ["Die", "Gr\u00e4n\u00b7zen", ",", "wel\u00b7che", "end\u00b7lich", ",", "und", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "$,", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Durch das ", "tokens": ["Durch", "das"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Da\u00df sie von Jhm entstanden seyn.", "tokens": ["Da\u00df", "sie", "von", "Jhm", "ent\u00b7stan\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zu einer ersteren Jdee, die allgemein,", "tokens": ["Zu", "ei\u00b7ner", "ers\u00b7te\u00b7ren", "Jdee", ",", "die", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Und \u00fcber alles ist, mu\u00df man nothwendig steigen.", "tokens": ["Und", "\u00fc\u00b7ber", "al\u00b7les", "ist", ",", "mu\u00df", "man", "noth\u00b7wen\u00b7dig", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VAFIN", "$,", "VMFIN", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Zur einz\u2019gen Regel mu\u00df man diese blo\u00df erlesen,", "tokens": ["Zur", "einz'\u00b7gen", "Re\u00b7gel", "mu\u00df", "man", "die\u00b7se", "blo\u00df", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VMFIN", "PIS", "PDAT", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der alles folgt, vor welcher nichts gewesen.", "tokens": ["Der", "al\u00b7les", "folgt", ",", "vor", "wel\u00b7cher", "nichts", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "APPR", "PRELS", "PIS", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Durch sie sind alle Ding\u2019 erkannt, und alle weisen,", "tokens": ["Durch", "sie", "sind", "al\u00b7le", "Ding'", "er\u00b7kannt", ",", "und", "al\u00b7le", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIAT", "NN", "VVPP", "$,", "KON", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df sie von GOTTES Gr\u00f6ss\u2019 und Herrlichkeit, allein", "tokens": ["Da\u00df", "sie", "von", "GoT\u00b7TES", "Gr\u00f6ss'", "und", "Herr\u00b7lich\u00b7keit", ",", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "APPR", "NE", "NN", "KON", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Verkleinte Z\u00fcge seyn.", "tokens": ["Ver\u00b7klein\u00b7te", "Z\u00fc\u00b7ge", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}