{"textgrid.poem.42237": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Minona", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df sie mich k\u00fcssen, die knospende Blume, den Kelch meiner Trunkenheit!", "tokens": ["La\u00df", "sie", "mich", "k\u00fcs\u00b7sen", ",", "die", "knos\u00b7pen\u00b7de", "Blu\u00b7me", ",", "den", "Kelch", "mei\u00b7ner", "Trun\u00b7ken\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "VVINF", "$,", "ART", "ADJA", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+--+--+--+--+-+", "measure": "dactylic.tetra.plus"}, "line.2": {"text": "Wenn meiner Lippen fiebernde Glut dir die Glieder durchzittert hat,", "tokens": ["Wenn", "mei\u00b7ner", "Lip\u00b7pen", "fie\u00b7bern\u00b7de", "Glut", "dir", "die", "Glie\u00b7der", "durch\u00b7zit\u00b7tert", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN", "PPER", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+--+--+--+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Dann erst wirst du mir Weib, und ein m\u00e4chtig Erinnern", "tokens": ["Dann", "erst", "wirst", "du", "mir", "Weib", ",", "und", "ein", "m\u00e4ch\u00b7tig", "E\u00b7rin\u00b7nern"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PPER", "NN", "$,", "KON", "ART", "ADJD", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Schwellt meine Segel gl\u00fcckseligen Inseln entgegen.", "tokens": ["Schwellt", "mei\u00b7ne", "Se\u00b7gel", "gl\u00fcck\u00b7se\u00b7li\u00b7gen", "In\u00b7seln", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}}}}}