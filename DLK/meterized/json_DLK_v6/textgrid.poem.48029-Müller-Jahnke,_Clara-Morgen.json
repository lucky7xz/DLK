{"textgrid.poem.48029": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Morgen", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der erste rosige D\u00e4mmerschein", "tokens": ["Der", "ers\u00b7te", "ro\u00b7si\u00b7ge", "D\u00e4m\u00b7mer\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "schwimmt drau\u00dfen in grauer Luft,", "tokens": ["schwimmt", "drau\u00b7\u00dfen", "in", "grau\u00b7er", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "durchs offene Fenster der Fr\u00fchwind weht", "tokens": ["durchs", "of\u00b7fe\u00b7ne", "Fens\u00b7ter", "der", "Fr\u00fch\u00b7wind", "weht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "uns w\u00fcrzigen Fliederduft.", "tokens": ["uns", "w\u00fcr\u00b7zi\u00b7gen", "Flie\u00b7der\u00b7duft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Zerronnen ist all im schimmernden Licht", "tokens": ["Zer\u00b7ron\u00b7nen", "ist", "all", "im", "schim\u00b7mern\u00b7den", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PIAT", "APPRART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "der finstere Traum der Nacht . . .", "tokens": ["der", "fins\u00b7te\u00b7re", "Traum", "der", "Nacht", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$.", "$.", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "an deiner wogenden Brust bin ich", "tokens": ["an", "dei\u00b7ner", "wo\u00b7gen\u00b7den", "Brust", "bin", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAFIN", "PPER"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "zum Leben aufgewacht!", "tokens": ["zum", "Le\u00b7ben", "auf\u00b7ge\u00b7wacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}