{"textgrid.poem.31153": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Zum 2. September", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Tag, an dem in leuchtender Wehr", "tokens": ["O", "Tag", ",", "an", "dem", "in", "leuch\u00b7ten\u00b7der", "Wehr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "ART", "APPR", "ADJA", "NN"], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Noch immer schwarzweissroth", "tokens": ["Noch", "im\u00b7mer", "schwarz\u00b7weiss\u00b7roth"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die deutsche Flagge von Fels zu Meer", "tokens": ["Die", "deut\u00b7sche", "Flag\u00b7ge", "von", "Fels", "zu", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "APPR", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nord-, ost- und westw\u00e4rts loht:", "tokens": ["Nord", ",", "ost", "und", "west\u00b7w\u00e4rts", "loht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "$,", "TRUNC", "KON", "ADV", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "In Einigkeit verbunden", "tokens": ["In", "Ei\u00b7nig\u00b7keit", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch die heilige Schaar, die an Dir verblich,", "tokens": ["Durch", "die", "hei\u00b7li\u00b7ge", "Schaar", ",", "die", "an", "Dir", "ver\u00b7blich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PPER", "ADJD", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "O Tag voll Blut und Wunden,", "tokens": ["O", "Tag", "voll", "Blut", "und", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wir gr\u00fcssen Dich! Wir gr\u00fcssen Dich!", "tokens": ["Wir", "gr\u00fcs\u00b7sen", "Dich", "!", "Wir", "gr\u00fcs\u00b7sen", "Dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn oft noch wird Dein Morgenwind", "tokens": ["Denn", "oft", "noch", "wird", "Dein", "Mor\u00b7gen\u00b7wind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch die Reiser an unsern Helmen wehn", "tokens": ["Durch", "die", "Rei\u00b7ser", "an", "un\u00b7sern", "Hel\u00b7men", "wehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und manche Mutter mit ihrem Kind", "tokens": ["Und", "man\u00b7che", "Mut\u00b7ter", "mit", "ih\u00b7rem", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Lautweinend am Wegrand stehn.", "tokens": ["Laut\u00b7wei\u00b7nend", "am", "Weg\u00b7rand", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nur Waffen h\u00f6rt man schmieden", "tokens": ["Nur", "Waf\u00b7fen", "h\u00f6rt", "man", "schmie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vom Bodensee bis an den Belt;", "tokens": ["Vom", "Bo\u00b7den\u00b7see", "bis", "an", "den", "Belt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Traum vom ewigen Frieden,", "tokens": ["Den", "Traum", "vom", "e\u00b7wi\u00b7gen", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "L\u00fcgen straft ihn die heutige Welt!", "tokens": ["L\u00fc\u00b7gen", "straft", "ihn", "die", "heu\u00b7ti\u00b7ge", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Zeit, die Eisen und Blut verschweisst,", "tokens": ["Die", "Zeit", ",", "die", "Ei\u00b7sen", "und", "Blut", "ver\u00b7schweisst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir ahnen sie l\u00e4ngst vor den Th\u00fcren stehn:", "tokens": ["Wir", "ah\u00b7nen", "sie", "l\u00e4ngst", "vor", "den", "Th\u00fc\u00b7ren", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die Trommel, die wirbelnd die Luft zerreisst,", "tokens": ["Die", "Trom\u00b7mel", ",", "die", "wir\u00b7belnd", "die", "Luft", "zer\u00b7reisst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kann schon morgen durch unsere Reihen gehn.", "tokens": ["Kann", "schon", "mor\u00b7gen", "durch", "un\u00b7se\u00b7re", "Rei\u00b7hen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Dann werden auf deutschem Herde", "tokens": ["Dann", "wer\u00b7den", "auf", "deut\u00b7schem", "Her\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+----+-", "measure": "dactylic.init"}, "line.6": {"text": "Die alten Gluten noch einmal gl\u00fchn", "tokens": ["Die", "al\u00b7ten", "Glu\u00b7ten", "noch", "ein\u00b7mal", "gl\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "VVINF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und roth auf franz\u00f6sischer Erde", "tokens": ["Und", "roth", "auf", "fran\u00b7z\u00f6\u00b7si\u00b7scher", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Um junge Gr\u00e4ber Rosen bl\u00fchn.", "tokens": ["Um", "jun\u00b7ge", "Gr\u00e4\u00b7ber", "Ro\u00b7sen", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nicht die Welt zu knechten ist unsre Begier,", "tokens": ["Nicht", "die", "Welt", "zu", "knech\u00b7ten", "ist", "uns\u00b7re", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "PTKZU", "VVINF", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Brandfackeln zu werfen in fremdes Gl\u00fcck:", "tokens": ["Brand\u00b7fa\u00b7ckeln", "zu", "wer\u00b7fen", "in", "frem\u00b7des", "Gl\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ein schw\u00e4bischer Bauer ist kein Baschkir", "tokens": ["Ein", "schw\u00e4\u00b7bi\u00b7scher", "Bau\u00b7er", "ist", "kein", "Baschkir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und ein pommerscher Landwehrmann kein Kalm\u00fcck!", "tokens": ["Und", "ein", "pom\u00b7mer\u00b7scher", "Land\u00b7wehr\u00b7mann", "kein", "Kal\u00b7m\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PIAT", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Was thut's, wenn der Ruhm unsre Siege", "tokens": ["Was", "thut's", ",", "wenn", "der", "Ruhm", "uns\u00b7re", "Sie\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Auf seine th\u00f6nernen Tafeln schreibt?", "tokens": ["Auf", "sei\u00b7ne", "th\u00f6\u00b7ner\u00b7nen", "Ta\u00b7feln", "schreibt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Sie gelten dem Weib an der Wiege", "tokens": ["Sie", "gel\u00b7ten", "dem", "Weib", "an", "der", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Und dem Sch\u00e4fer, der seine Schafe treibt!", "tokens": ["Und", "dem", "Sch\u00e4\u00b7fer", ",", "der", "sei\u00b7ne", "Scha\u00b7fe", "treibt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Doch weh, wenn die Kraft, die einst Kronen zerbrach,", "tokens": ["Doch", "weh", ",", "wenn", "die", "Kraft", ",", "die", "einst", "Kro\u00b7nen", "zer\u00b7brach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "ART", "NN", "$,", "PRELS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Nicht l\u00e4nger mehr unsre Schwerter umspr\u00fcht", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "mehr", "uns\u00b7re", "Schwer\u00b7ter", "um\u00b7spr\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und die alte Zeit der alten Schmach", "tokens": ["Und", "die", "al\u00b7te", "Zeit", "der", "al\u00b7ten", "Schmach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "In unsre Stirnen ihr Schandmahl gl\u00fcht!", "tokens": ["In", "uns\u00b7re", "Stir\u00b7nen", "ihr", "Schand\u00b7mahl", "gl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wenn Franzosen, Russen und Czechen", "tokens": ["Wenn", "Fran\u00b7zo\u00b7sen", ",", "Rus\u00b7sen", "und", "Cze\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Ihre Fangarme um unser Land gekrallt \u2013", "tokens": ["Ih\u00b7re", "Fang\u00b7ar\u00b7me", "um", "un\u00b7ser", "Land", "ge\u00b7krallt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Doch schon zu denken daran, ist Verbrechen,", "tokens": ["Doch", "schon", "zu", "den\u00b7ken", "da\u00b7ran", ",", "ist", "Ver\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKZU", "VVINF", "PAV", "$,", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nach blitzt ja die Wacht auf dem Niederwald!", "tokens": ["Nach", "blitzt", "ja", "die", "Wacht", "auf", "dem", "Nie\u00b7der\u00b7wald", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NE", "$."], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Drum, Du Tag, an dem in leuchtender Wehr", "tokens": ["Drum", ",", "Du", "Tag", ",", "an", "dem", "in", "leuch\u00b7ten\u00b7der", "Wehr"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "PPER", "NN", "$,", "APPR", "ART", "APPR", "ADJA", "NN"], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Noch immer schwarzweissroth", "tokens": ["Noch", "im\u00b7mer", "schwarz\u00b7weiss\u00b7roth"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die deutsche Flagge von Fels zu Meer", "tokens": ["Die", "deut\u00b7sche", "Flag\u00b7ge", "von", "Fels", "zu", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "APPR", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nord-, ost- und westw\u00e4rts loht:", "tokens": ["Nord", ",", "ost", "und", "west\u00b7w\u00e4rts", "loht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "$,", "TRUNC", "KON", "ADV", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "In Einigkeit verbunden", "tokens": ["In", "Ei\u00b7nig\u00b7keit", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch die heilige Schaar, die an Dir verblich,", "tokens": ["Durch", "die", "hei\u00b7li\u00b7ge", "Schaar", ",", "die", "an", "Dir", "ver\u00b7blich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PPER", "ADJD", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "O Tag voll Blut und Wunden,", "tokens": ["O", "Tag", "voll", "Blut", "und", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wir gr\u00fcssen Dich! Wir gr\u00fcssen Dich!", "tokens": ["Wir", "gr\u00fcs\u00b7sen", "Dich", "!", "Wir", "gr\u00fcs\u00b7sen", "Dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}