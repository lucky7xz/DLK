{"textgrid.poem.31651": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "23. An Gott den H. Geist", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein geheimer Herzen-Schatz / meine helle Sinnen-Sonne!", "tokens": ["Mein", "ge\u00b7hei\u00b7mer", "Her\u00b7zen\u00b7Schatz", "/", "mei\u00b7ne", "hel\u00b7le", "Sin\u00b7nen\u00b7Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "tr\u00f6ste und bestrahle mich. Ohne deine Gnaden-Wonne /", "tokens": ["tr\u00f6s\u00b7te", "und", "be\u00b7strah\u00b7le", "mich", ".", "Oh\u00b7ne", "dei\u00b7ne", "Gna\u00b7den\u00b7Won\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "KON", "VVFIN", "PPER", "$.", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "bleibt es dunkler Vngl\u00fccks-Schatten. Von dem Mond / der Erden-Lust /", "tokens": ["bleibt", "es", "dunk\u00b7ler", "Vn\u00b7gl\u00fccks\u00b7Schat\u00b7ten", ".", "Von", "dem", "Mond", "/", "der", "Er\u00b7den\u00b7Lust", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$.", "APPR", "ART", "NN", "$(", "ART", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "hab' ich schlechte Herz-Erquickung / wann du mir nicht bist bewust.", "tokens": ["hab'", "ich", "schlech\u00b7te", "Her\u00b7zEr\u00b7qui\u00b7ckung", "/", "wann", "du", "mir", "nicht", "bist", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "NN", "$(", "PWAV", "PPER", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Spiel das sondre Sinnen-Spiel / auf dem Schauplatz der Gedanken;", "tokens": ["Spiel", "das", "sond\u00b7re", "Sin\u00b7nen\u00b7Spiel", "/", "auf", "dem", "Schau\u00b7platz", "der", "Ge\u00b7dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$(", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "mach die Geist-Rageten gehn / da\u00df sie Sternenhoch auffanken:", "tokens": ["mach", "die", "Geist\u00b7Ra\u00b7ge\u00b7ten", "gehn", "/", "da\u00df", "sie", "Ster\u00b7nen\u00b7hoch", "auf\u00b7fan\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$(", "KOUS", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "da\u00df man m\u00f6g den Allmachts- Donner und den Jubel-Schall verstehn.", "tokens": ["da\u00df", "man", "m\u00f6g", "den", "All\u00b7machts", "Don\u00b7ner", "und", "den", "Ju\u00b7bel\u00b7Schall", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "ART", "TRUNC", "NN", "KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "La\u00df / von An-zu Angesicht / bald / was jetzt im Geist / mich sehn.", "tokens": ["La\u00df", "/", "von", "An\u00b7zu", "An\u00b7ge\u00b7sicht", "/", "bald", "/", "was", "jetzt", "im", "Geist", "/", "mich", "sehn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$(", "APPR", "NE", "NN", "$(", "ADV", "$(", "PWS", "ADV", "APPRART", "NN", "$(", "PPER", "VVINF", "$."], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}