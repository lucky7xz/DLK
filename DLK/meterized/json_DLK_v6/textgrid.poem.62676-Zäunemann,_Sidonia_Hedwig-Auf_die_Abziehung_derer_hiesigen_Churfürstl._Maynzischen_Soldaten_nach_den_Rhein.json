{"textgrid.poem.62676": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Abziehung derer hiesigen Churf\u00fcrstl. Maynzischen Soldaten nach den Rhein", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was schallet jezt vor ein Geschrey", "tokens": ["Was", "schal\u00b7let", "jezt", "vor", "ein", "Ge\u00b7schrey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Martis hocherhabnem Munde?", "tokens": ["Aus", "Mar\u00b7tis", "ho\u00b7cher\u00b7hab\u00b7nem", "Mun\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es heist: Es kommt die Zeit herbey,", "tokens": ["Es", "heist", ":", "Es", "kommt", "die", "Zeit", "her\u00b7bey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es nahet sich nunmehr die Stunde,", "tokens": ["Es", "na\u00b7het", "sich", "nun\u00b7mehr", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da mein geliebtes Kinder-Chor", "tokens": ["Da", "mein", "ge\u00b7lieb\u00b7tes", "Kin\u00b7der\u00b7Chor"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In sch\u00f6nster Ordnung trit hervor.", "tokens": ["In", "sch\u00f6ns\u00b7ter", "Ord\u00b7nung", "trit", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf! auf! ihr m\u00fc\u00dft durch Ubung derer Waffen,", "tokens": ["Auf", "!", "auf", "!", "ihr", "m\u00fc\u00dft", "durch", "U\u00b7bung", "de\u00b7rer", "Waf\u00b7fen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "PTKVZ", "$.", "PPER", "VMFIN", "APPR", "NN", "PDS", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Euch Ruhm und Ehr, auch Gl\u00fcck und Beute schaffen.", "tokens": ["Euch", "Ruhm", "und", "Ehr", ",", "auch", "Gl\u00fcck", "und", "Beu\u00b7te", "schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "$,", "ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie lang wolt ihr in Mauren seyn?", "tokens": ["Wie", "lang", "wolt", "ihr", "in", "Mau\u00b7ren", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was schlummert ihr in euren Th\u00fcrmen,", "tokens": ["Was", "schlum\u00b7mert", "ihr", "in", "eu\u00b7ren", "Th\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und sperrt euch in Baraquen ein?", "tokens": ["Und", "sperrt", "euch", "in", "Ba\u00b7ra\u00b7quen", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf in das Feld! ihr m\u00fcsset st\u00fcrmen!", "tokens": ["Auf", "in", "das", "Feld", "!", "ihr", "m\u00fcs\u00b7set", "st\u00fcr\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$.", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gelegenheit zum Krieg ist da,", "tokens": ["Ge\u00b7le\u00b7gen\u00b7heit", "zum", "Krieg", "ist", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gelegenheit zum Sieg ist nah.", "tokens": ["Ge\u00b7le\u00b7gen\u00b7heit", "zum", "Sieg", "ist", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Drum auf! und geht zum Rhein und nach Pohlen,", "tokens": ["Drum", "auf", "!", "und", "geht", "zum", "Rhein", "und", "nach", "Poh\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$.", "KON", "VVFIN", "APPRART", "NE", "KON", "APPR", "NE", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und wo es Noth. Denn Mars hats so befohlen.", "tokens": ["Und", "wo", "es", "Noth", ".", "Denn", "Mars", "hats", "so", "be\u00b7foh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "NN", "$.", "KON", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Kaum war di\u00df strenge Wort gesagt;", "tokens": ["Kaum", "war", "di\u00df", "stren\u00b7ge", "Wort", "ge\u00b7sagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So hat es ", "tokens": ["So", "hat", "es"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Worauf Die Helden sich befragt:", "tokens": ["Wo\u00b7rauf", "Die", "Hel\u00b7den", "sich", "be\u00b7fragt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie? Wo? und wenn sie m\u00f6chten schlagen?", "tokens": ["Wie", "?", "Wo", "?", "und", "wenn", "sie", "m\u00f6ch\u00b7ten", "schla\u00b7gen", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PWAV", "$.", "KON", "KOUS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wohin der Lauf wohl k\u00f6nte gehn?", "tokens": ["Wo\u00b7hin", "der", "Lauf", "wohl", "k\u00f6n\u00b7te", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wo sie etwan m\u00fc\u00dften stehn", "tokens": ["Und", "wo", "sie", "et\u00b7wan", "m\u00fc\u00df\u00b7ten", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenns vor dem Feind vieleicht zum streiten k\u00e4me:", "tokens": ["Wenns", "vor", "dem", "Feind", "vie\u00b7leicht", "zum", "strei\u00b7ten", "k\u00e4\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wer wohl den Sieg mit sich zur\u00fccke n\u00e4hme?", "tokens": ["Wer", "wohl", "den", "Sieg", "mit", "sich", "zu\u00b7r\u00fc\u00b7cke", "n\u00e4h\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der eine sprach: Es geht ins Reich;", "tokens": ["Der", "ei\u00b7ne", "sprach", ":", "Es", "geht", "ins", "Reich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$.", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der andre sagte, nein nach Pohlen.", "tokens": ["Der", "and\u00b7re", "sag\u00b7te", ",", "nein", "nach", "Poh\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PTKANT", "APPR", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der sprach, mein Herze wird nicht weich", "tokens": ["Der", "sprach", ",", "mein", "Her\u00b7ze", "wird", "nicht", "weich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "PPOSAT", "VVFIN", "VAFIN", "PTKNEG", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Ruhm in Schlesien zu hohlen.", "tokens": ["Den", "Ruhm", "in", "Schle\u00b7si\u00b7en", "zu", "hoh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der w\u00fcnschte sich am Rhein zu gehn,", "tokens": ["Der", "w\u00fcnschte", "sich", "am", "Rhein", "zu", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPRART", "NE", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Der f\u00fcnfte wolte Welschland sehn;", "tokens": ["Der", "f\u00fcnf\u00b7te", "wol\u00b7te", "Wel\u00b7schland", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der sehnte sich nach denen Moscowittern;", "tokens": ["Der", "sehn\u00b7te", "sich", "nach", "de\u00b7nen", "Mo\u00b7sco\u00b7wit\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "PRELS", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Der wolle vor Franzosen nicht erzittern.", "tokens": ["Der", "wol\u00b7le", "vor", "Fran\u00b7zo\u00b7sen", "nicht", "er\u00b7zit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der eine sprach, ich freue mich,", "tokens": ["Der", "ei\u00b7ne", "sprach", ",", "ich", "freu\u00b7e", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch manche Oerter zu marschieren;", "tokens": ["Durch", "man\u00b7che", "O\u00b7er\u00b7ter", "zu", "mar\u00b7schie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da will ich M\u00e4dgen m\u00e4chtiglich,", "tokens": ["Da", "will", "ich", "M\u00e4d\u00b7gen", "m\u00e4ch\u00b7tig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und auch den Bauers-Mann vexiren.", "tokens": ["Und", "auch", "den", "Bau\u00b7er\u00b7sMann", "ve\u00b7xi\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der andre sprach, jetzt geht es an,", "tokens": ["Der", "and\u00b7re", "sprach", ",", "jetzt", "geht", "es", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich mir Vortheil machen kan.", "tokens": ["Da\u00df", "ich", "mir", "Vor\u00b7theil", "ma\u00b7chen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Jezt ist es Zeit den Beutel und die Flicken", "tokens": ["Jezt", "ist", "es", "Zeit", "den", "Beu\u00b7tel", "und", "die", "Fli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit Gold und Geld und waren auszuspicken.", "tokens": ["Mit", "Gold", "und", "Geld", "und", "wa\u00b7ren", "aus\u00b7zu\u00b7spi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "VAFIN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Der dritte sagte: das ist gut,", "tokens": ["Der", "drit\u00b7te", "sag\u00b7te", ":", "das", "ist", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$.", "PDS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun will ich mir bald die Gamaschen,", "tokens": ["Nun", "will", "ich", "mir", "bald", "die", "Ga\u00b7ma\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Mit meiner Feinde Schwei\u00df und Blut,", "tokens": ["Mit", "mei\u00b7ner", "Fein\u00b7de", "Schwei\u00df", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu meiner gr\u00f6\u00dften Ehre waschen.", "tokens": ["Zu", "mei\u00b7ner", "gr\u00f6\u00df\u00b7ten", "Eh\u00b7re", "wa\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der vierte sprach: Ich fall dir bey,", "tokens": ["Der", "vier\u00b7te", "sprach", ":", "Ich", "fall", "dir", "bey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$.", "PPER", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und warte auf ein Feld-Geschrey,", "tokens": ["Und", "war\u00b7te", "auf", "ein", "Feld\u00b7Ge\u00b7schrey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df ich die Hand zum Kampfe kan erheben;", "tokens": ["Da\u00df", "ich", "die", "Hand", "zum", "Kamp\u00b7fe", "kan", "er\u00b7he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der stolze Feind soll zittern und erbeben.", "tokens": ["Der", "stol\u00b7ze", "Feind", "soll", "zit\u00b7tern", "und", "er\u00b7be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Der Aufbruch war nunmehro nah;", "tokens": ["Der", "Auf\u00b7bruch", "war", "nun\u00b7meh\u00b7ro", "nah", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man h\u00f6rte nichts als von Marschiren.", "tokens": ["Man", "h\u00f6r\u00b7te", "nichts", "als", "von", "Mar\u00b7schi\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIS", "KOKOM", "APPR", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Helden ruften hier und da:", "tokens": ["Die", "Hel\u00b7den", "ruf\u00b7ten", "hier", "und", "da", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir m\u00fcssen Martis Wort vollf\u00fchren.", "tokens": ["Wir", "m\u00fcs\u00b7sen", "Mar\u00b7tis", "Wort", "voll\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nun lagern wir uns in das Feld:", "tokens": ["Nun", "la\u00b7gern", "wir", "uns", "in", "das", "Feld", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein jeder zeig sich als ein Held,", "tokens": ["Ein", "je\u00b7der", "zeig", "sich", "als", "ein", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und endige der stolzen Feinde Schnarchen;", "tokens": ["Und", "en\u00b7di\u00b7ge", "der", "stol\u00b7zen", "Fein\u00b7de", "Schnar\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So ehren uns die F\u00fcrsten und Monarchen.", "tokens": ["So", "eh\u00b7ren", "uns", "die", "F\u00fcrs\u00b7ten", "und", "Mon\u00b7ar\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Mercur kam auch mit Mavors Wort,", "tokens": ["Mer\u00b7cur", "kam", "auch", "mit", "Ma\u00b7vors", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Erfurt und zum Petersberge.", "tokens": ["Na