{"dta.poem.20434": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Wettstreit der tulpen/ nelcke/ rose/  \n lilie/ cypre\u00df/ narci\u00df und  \n hyacinth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mir weissen lilie geb\u00fchret kron und thron/", "tokens": ["Mir", "weis\u00b7sen", "li\u00b7lie", "ge\u00b7b\u00fch\u00b7ret", "kron", "und", "thron", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "NN", "KON", "NE", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mein silber das ist wei\u00df und rein von aller fchmincke/", "tokens": ["Mein", "sil\u00b7ber", "das", "ist", "wei\u00df", "und", "rein", "von", "al\u00b7ler", "fchmin\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "KON", "PDS", "VAFIN", "ADJD", "KON", "ADJD", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die rosen \u00e4hnlich sieht/ die tulipan versincke/", "tokens": ["Die", "ro\u00b7sen", "\u00e4hn\u00b7lich", "sieht", "/", "die", "tu\u00b7li\u00b7pan", "ver\u00b7sin\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "VVFIN", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und lege sich ins grab/ das sag ich dir zu hohn.", "tokens": ["Und", "le\u00b7ge", "sich", "ins", "grab", "/", "das", "sag", "ich", "dir", "zu", "hohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "$(", "PDS", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man hat sonst weiter nichts als augen-lust davon/", "tokens": ["Man", "hat", "sonst", "wei\u00b7ter", "nichts", "als", "au\u00b7gen\u00b7lust", "da\u00b7von", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADV", "PIS", "KOKOM", "NN", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn ich den perlen thau aus meinen kelche trincke/", "tokens": ["Wenn", "ich", "den", "per\u00b7len", "thau", "aus", "mei\u00b7nen", "kel\u00b7che", "trin\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So seht ihr blumen erst in was vor zierd ich blincke/", "tokens": ["So", "seht", "ihr", "blu\u00b7men", "erst", "in", "was", "vor", "zierd", "ich", "blin\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ADV", "APPR", "PRELS", "APPR", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mir dient der westen wind des lentzens liebster sohn/", "tokens": ["Mir", "dient", "der", "wes\u00b7ten", "wind", "des", "lent\u00b7zens", "liebs\u00b7ter", "sohn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "F\u00fcr geile hertzen ist der rose milch und blut.", "tokens": ["F\u00fcr", "gei\u00b7le", "hert\u00b7zen", "ist", "der", "ro\u00b7se", "milch", "und", "blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich bin die keuscheit blum und unbefleckt an zierde.", "tokens": ["Ich", "bin", "die", "keu\u00b7scheit", "blum", "und", "un\u00b7be\u00b7fleckt", "an", "zier\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NE", "KON", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ihr andern alle seyd beschmutzet von begierde.", "tokens": ["Ihr", "an\u00b7dern", "al\u00b7le", "seyd", "be\u00b7schmut\u00b7zet", "von", "be\u00b7gier\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "PIAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und mag noch eine seyn die mir den einspruch thut?", "tokens": ["Und", "mag", "noch", "ei\u00b7ne", "seyn", "die", "mir", "den", "ein\u00b7spruch", "thut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ART", "VAINF", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Weg ros\u2019 und tulipan/ weg nelck ich hab erreichet/", "tokens": ["Weg", "ros'", "und", "tu\u00b7li\u00b7pan", "/", "weg", "nelck", "ich", "hab", "er\u00b7rei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "NE", "$(", "ADV", "VVFIN", "PPER", "VAFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df keine blume sich mit meiner w\u00fcrde gleichet.", "tokens": ["Da\u00df", "kei\u00b7ne", "blu\u00b7me", "sich", "mit", "mei\u00b7ner", "w\u00fcr\u00b7de", "glei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "VVFIN", "PRF", "APPR", "PPOSAT", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}