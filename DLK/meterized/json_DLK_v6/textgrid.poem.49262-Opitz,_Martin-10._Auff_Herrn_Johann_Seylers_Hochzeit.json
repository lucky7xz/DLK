{"textgrid.poem.49262": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "10. Auff Herrn Johann Seylers Hochzeit", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonn' hat sich verkrochen,", "tokens": ["Die", "Sonn'", "hat", "sich", "ver\u00b7kro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Tag ist gantz dahin,", "tokens": ["Der", "Tag", "ist", "gantz", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Mond' ist angebrochen,", "tokens": ["Der", "Mond'", "ist", "an\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Arbeit-Tr\u00f6sterin,", "tokens": ["Die", "Ar\u00b7beit\u00b7Tr\u00f6s\u00b7te\u00b7rin", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Nacht, hat angeleget", "tokens": ["Die", "Nacht", ",", "hat", "an\u00b7ge\u00b7le\u00b7get"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr schwartzes Trauerkleyd,", "tokens": ["Ihr", "schwart\u00b7zes", "Trau\u00b7er\u00b7kleyd", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kein Gra\u00df ist, das sich reget,", "tokens": ["Kein", "Gra\u00df", "ist", ",", "das", "sich", "re\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kein Baum nicht weit und breit.", "tokens": ["Kein", "Baum", "nicht", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKNEG", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Welt ist schon zu Bette,", "tokens": ["Die", "Welt", "ist", "schon", "zu", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und hat die Augen zu,", "tokens": ["Und", "hat", "die", "Au\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir schlaffen in die Wette,", "tokens": ["Wir", "schlaf\u00b7fen", "in", "die", "Wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Meer liegt auch in Ruh;", "tokens": ["Das", "Meer", "liegt", "auch", "in", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nur zweene Geister wachen,", "tokens": ["Nur", "zwee\u00b7ne", "Geis\u00b7ter", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Krieg- und Liebesgott", "tokens": ["Der", "Krieg", "und", "Lie\u00b7bes\u00b7gott"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bestellen ihre Sachen", "tokens": ["Be\u00b7stel\u00b7len", "ih\u00b7re", "Sa\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In dem wir sind als tod.", "tokens": ["In", "dem", "wir", "sind", "als", "tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VAFIN", "KOUS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wann uns gar sanffte tr\u00e4umet,", "tokens": ["Wann", "uns", "gar", "sanff\u00b7te", "tr\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und alle sicher seyn,", "tokens": ["Und", "al\u00b7le", "si\u00b7cher", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr keiner, derer s\u00e4umet,", "tokens": ["Ihr", "kei\u00b7ner", ",", "de\u00b7rer", "s\u00e4u\u00b7met", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nimpt seine Schantzen ein.", "tokens": ["Nimpt", "sei\u00b7ne", "Schant\u00b7zen", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Soldaten die verlangen", "tokens": ["Sol\u00b7da\u00b7ten", "die", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nach Blute f\u00fcr und f\u00fcr;", "tokens": ["Nach", "Blu\u00b7te", "f\u00fcr", "und", "f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Buhler ligt gefangen", "tokens": ["Der", "Buh\u00b7ler", "ligt", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "F\u00fcr seiner Liebsten Th\u00fcr.", "tokens": ["F\u00fcr", "sei\u00b7ner", "Liebs\u00b7ten", "Th\u00fcr."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Mars mu\u00df sein L\u00e4ger schlagen", "tokens": ["Mars", "mu\u00df", "sein", "L\u00e4\u00b7ger", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hier unters grosse Dach,", "tokens": ["Hier", "un\u00b7ters", "gros\u00b7se", "Dach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch Hitz' und K\u00e4lt' ertragen,", "tokens": ["Auch", "Hitz'", "und", "K\u00e4lt'", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Trinckt offtmals au\u00df der Bach;", "tokens": ["Trinckt", "offt\u00b7mals", "au\u00df", "der", "Bach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So mu\u00df sich auch gewehnen", "tokens": ["So", "mu\u00df", "sich", "auch", "ge\u00b7weh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PRF", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Buhler, lescht vor Wein", "tokens": ["Ein", "Buh\u00b7ler", ",", "lescht", "vor", "Wein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit vielen heissen Threnen", "tokens": ["Mit", "vie\u00b7len", "heis\u00b7sen", "Thre\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den Durst der Liebespein.", "tokens": ["Den", "Durst", "der", "Lie\u00b7be\u00b7spein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Man sieht zu jedermalen", "tokens": ["Man", "sieht", "zu", "je\u00b7der\u00b7ma\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bey Nachte heller seyn", "tokens": ["Bey", "Nach\u00b7te", "hel\u00b7ler", "seyn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "VAINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "De\u00df Feuers liechte Strahlen,", "tokens": ["De\u00df", "Feu\u00b7ers", "liech\u00b7te", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als bey der Sonnen Schein;", "tokens": ["Als", "bey", "der", "Son\u00b7nen", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auch damals legt die Liebe", "tokens": ["Auch", "da\u00b7mals", "legt", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dem Feuer besser zu,", "tokens": ["Dem", "Feu\u00b7er", "bes\u00b7ser", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wann alles gleich ist tr\u00fcbe", "tokens": ["Wann", "al\u00b7les", "gleich", "ist", "tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ADV", "VAFIN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und kr\u00e4nckt uns ohne Ruh.", "tokens": ["Und", "kr\u00e4nckt", "uns", "oh\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So wird auch sonst gelesen,", "tokens": ["So", "wird", "auch", "sonst", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df Venus bey der Nacht", "tokens": ["Da\u00df", "Ve\u00b7nus", "bey", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "De\u00df Kindes sey genesen", "tokens": ["De\u00df", "Kin\u00b7des", "sey", "ge\u00b7ne\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und es zur Welt gebracht.", "tokens": ["Und", "es", "zur", "Welt", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drumb wil sie, da\u00df in gleichen", "tokens": ["Drumb", "wil", "sie", ",", "da\u00df", "in", "glei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "$,", "KOUS", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der, welcher lieben wil,", "tokens": ["Der", ",", "wel\u00b7cher", "lie\u00b7ben", "wil", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bey stiller Nacht soll streichen", "tokens": ["Bey", "stil\u00b7ler", "Nacht", "soll", "strei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auff sein gew\u00fcndschtes Ziel.", "tokens": ["Auff", "sein", "ge\u00b7w\u00fcnd\u00b7schtes", "Ziel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Herr Seyler, dieser Sachen", "tokens": ["Herr", "Sey\u00b7ler", ",", "die\u00b7ser", "Sa\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NE", "$,", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Seyd ihr nun gantz befreyt;", "tokens": ["Seyd", "ihr", "nun", "gantz", "be\u00b7freyt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr d\u00f6rfft alleine wachen", "tokens": ["Ihr", "d\u00f6rfft", "al\u00b7lei\u00b7ne", "wa\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach Lust und Fr\u00f6ligkeit", "tokens": ["Nach", "Lust", "und", "Fr\u00f6\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und fahrt in guten Stande", "tokens": ["Und", "fahrt", "in", "gu\u00b7ten", "Stan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Am sichern Hafen an.", "tokens": ["Am", "si\u00b7chern", "Ha\u00b7fen", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.7": {"text": "Wol dem, der so zu Lande", "tokens": ["Wol", "dem", ",", "der", "so", "zu", "Lan\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "ADV", "APPR", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Mit Gl\u00fccke kommen kan.", "tokens": ["Mit", "Gl\u00fc\u00b7cke", "kom\u00b7men", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}