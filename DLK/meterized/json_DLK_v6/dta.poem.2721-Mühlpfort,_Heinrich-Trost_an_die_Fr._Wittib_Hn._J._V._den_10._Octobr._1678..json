{"dta.poem.2721": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trost an die Fr. Wittib Hn. J. V. den  10.  \n Octobr. 1678.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer kan den Wunder-Schlu\u00df des H\u00f6chsten satt er-\ngr\u00fcnden?", "tokens": ["Wer", "kan", "den", "Wun\u00b7der\u00b7Schlu\u00df", "des", "H\u00f6chs\u00b7ten", "satt", "er", "gr\u00fcn\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "ART", "NN", "APPR", "TRUNC", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein Allmacht legt sich selber aus", "tokens": ["Sein", "All\u00b7macht", "legt", "sich", "sel\u00b7ber", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So bald sie spricht: Bestell dein Hau\u00df", "tokens": ["So", "bald", "sie", "spricht", ":", "Be\u00b7stell", "dein", "Hau\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$.", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Mensch man wird dich todt im Sarche morgen finden.", "tokens": ["O", "Mensch", "man", "wird", "dich", "todt", "im", "Sar\u00b7che", "mor\u00b7gen", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PIS", "VAFIN", "PPER", "ADJD", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da ist das Ziel verkehrt/ der Vorsatz gantz verr\u00fcckt", "tokens": ["Da", "ist", "das", "Ziel", "ver\u00b7kehrt", "/", "der", "Vor\u00b7satz", "gantz", "ver\u00b7r\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$(", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und alles bieibet nach worzu wir uns geschickt.", "tokens": ["Und", "al\u00b7les", "bi\u00b7ei\u00b7bet", "nach", "wor\u00b7zu", "wir", "uns", "ge\u00b7schickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "PWAV", "PPER", "PPER", "VVPP", "$."], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Herr ", "tokens": ["Herr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Wie vormals auff den Marckt zu ziehn/", "tokens": ["Wie", "vor\u00b7mals", "auff", "den", "Marckt", "zu", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df seine Handlung mochte bl\u00fchn", "tokens": ["Da\u00df", "sei\u00b7ne", "Hand\u00b7lung", "moch\u00b7te", "bl\u00fchn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und ihm k\u00e4m aus der H\u00f6h der Seegen zugeflossen/", "tokens": ["Und", "ihm", "k\u00e4m", "aus", "der", "H\u00f6h", "der", "See\u00b7gen", "zu\u00b7ge\u00b7flos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Der Leipzig wolte sehn und wie die Messe lauff", "tokens": ["Der", "Leip\u00b7zig", "wol\u00b7te", "sehn", "und", "wie", "die", "Mes\u00b7se", "lauff"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VMFIN", "VVINF", "KON", "PWAV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Gibt eben diesen Tag des Lebens Geister auff.", "tokens": ["Gibt", "e\u00b7ben", "die\u00b7sen", "Tag", "des", "Le\u00b7bens", "Geis\u00b7ter", "auff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PDAT", "NN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nun wirst du/ ", "tokens": ["Nun", "wirst", "du", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Du legst noch Zoll noch das Geleit/", "tokens": ["Du", "legst", "noch", "Zoll", "noch", "das", "Ge\u00b7leit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Die Fracht geht in die Ewigkeit/", "tokens": ["Die", "Fracht", "geht", "in", "die", "E\u00b7wig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Da wird ans h\u00f6chste Gut dein Heiland dich anweisen.", "tokens": ["Da", "wird", "ans", "h\u00f6chs\u00b7te", "Gut", "dein", "Hei\u00b7land", "dich", "an\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "ADJA", "NN", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Denn ist es wol gethan/ wenn er mit dir ", "tokens": ["Denn", "ist", "es", "wol", "ge\u00b7than", "/", "wenn", "er", "mit", "dir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$(", "KOUS", "PPER", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und deiner S\u00fcnden Rest mit seinem Blut abf\u00fchrt.", "tokens": ["Und", "dei\u00b7ner", "S\u00fcn\u00b7den", "Rest", "mit", "sei\u00b7nem", "Blut", "ab\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Denn dieses hast du auch bey dir genau ermessen.", "tokens": ["Denn", "die\u00b7ses", "hast", "du", "auch", "bey", "dir", "ge\u00b7nau", "er\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPER", "ADV", "APPR", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Wenn jetzt ein Kauffmann reisen mu\u00df/", "tokens": ["Wenn", "jetzt", "ein", "Kauf\u00b7fmann", "rei\u00b7sen", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Eh er noch fortsetzt seinen Fu\u00df/", "tokens": ["Eh", "er", "noch", "fort\u00b7setzt", "sei\u00b7nen", "Fu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.22": {"text": "So wird er/ wie der Weg/ zu forschen nicht vergessen.", "tokens": ["So", "wird", "er", "/", "wie", "der", "Weg", "/", "zu", "for\u00b7schen", "nicht", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$(", "KOKOM", "ART", "NN", "$(", "PTKZU", "VVINF", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ob ihn Gefahr bedreut und Krieges-Volck erschreckt?", "tokens": ["Ob", "ihn", "Ge\u00b7fahr", "be\u00b7dreut", "und", "Krie\u00b7ge\u00b7sVolck", "er\u00b7schreckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ob wo ein heimlich Feind in dem Verborgnen steckt?", "tokens": ["Ob", "wo", "ein", "heim\u00b7lich", "Feind", "in", "dem", "Ver\u00b7borg\u00b7nen", "steckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "ART", "ADJD", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Di\u00df konte dir zur Zeit noch keinen Kummer machen/", "tokens": ["Di\u00df", "kon\u00b7te", "dir", "zur", "Zeit", "noch", "kei\u00b7nen", "Kum\u00b7mer", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "APPRART", "NN", "ADV", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die Strassen waren dir bekand/", "tokens": ["Die", "Stras\u00b7sen", "wa\u00b7ren", "dir", "be\u00b7kand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "So wohnte Sicherheit im Land.", "tokens": ["So", "wohn\u00b7te", "Si\u00b7cher\u00b7heit", "im", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Und jeder zog frey aus mit seinem Gut und Sachen.", "tokens": ["Und", "je\u00b7der", "zog", "frey", "aus", "mit", "sei\u00b7nem", "Gut", "und", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADJD", "APPR", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Da\u00df wenn nach Menschen Art man seine Rechnung stellt", "tokens": ["Da\u00df", "wenn", "nach", "Men\u00b7schen", "Art", "man", "sei\u00b7ne", "Rech\u00b7nung", "stellt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPR", "NN", "NN", "PIS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Dich jetzt abreisenden nichts mehr zur\u00fccke h\u00e4lt.", "tokens": ["Dich", "jetzt", "ab\u00b7rei\u00b7sen\u00b7den", "nichts", "mehr", "zu\u00b7r\u00fc\u00b7cke", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PIS", "ADV", "ADV", "VVFIN", "$."], "meter": "+-++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.31": {"text": "Allein du wustest nicht/ wie Gottes Wege gehen/", "tokens": ["Al\u00b7lein", "du", "wus\u00b7test", "nicht", "/", "wie", "Got\u00b7tes", "We\u00b7ge", "ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PTKNEG", "$(", "KOKOM", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der \u00fcber uns so bald gebeuth/", "tokens": ["Der", "\u00fc\u00b7ber", "uns", "so", "bald", "ge\u00b7beuth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ADV", "ADV", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Der abgemessen unsre Zeit", "tokens": ["Der", "ab\u00b7ge\u00b7mes\u00b7sen", "uns\u00b7re", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Und schon das Ziel gesetzt in den gestirnten H\u00f6hen.", "tokens": ["Und", "schon", "das", "Ziel", "ge\u00b7setzt", "in", "den", "ge\u00b7stirn\u00b7ten", "H\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Dein Name konte dir di\u00df deutlich bilden ein/", "tokens": ["Dein", "Na\u00b7me", "kon\u00b7te", "dir", "di\u00df", "deut\u00b7lich", "bil\u00b7den", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PDS", "ADJD", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da\u00df Menschen auff der Welt als wie die Vogel seyn.", "tokens": ["Da\u00df", "Men\u00b7schen", "auff", "der", "Welt", "als", "wie", "die", "Vo\u00b7gel", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "KOUS", "KOKOM", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Menn jetzt der Steller ihm hat seinen Herd gebauet", "tokens": ["Menn", "jetzt", "der", "Stel\u00b7ler", "ihm", "hat", "sei\u00b7nen", "Herd", "ge\u00b7bau\u00b7et"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "NN", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.38": {"text": "Mit gr\u00fcnen Rasen rings umbsteckt/", "tokens": ["Mit", "gr\u00fc\u00b7nen", "Ra\u00b7sen", "rings", "umbs\u00b7teckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Und sich mit breiten Zweigen deckt", "tokens": ["Und", "sich", "mit", "brei\u00b7ten", "Zwei\u00b7gen", "deckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Und listig durch die Nacht de\u00df frischen Laubes schauet/", "tokens": ["Und", "lis\u00b7tig", "durch", "die", "Nacht", "de\u00df", "fri\u00b7schen", "Lau\u00b7bes", "schau\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "So lockt er durch die Pfeiff und durch den Schreyer an/", "tokens": ["So", "lockt", "er", "durch", "die", "Pfeiff", "und", "durch", "den", "Schre\u00b7yer", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ob er das albre Volck ins Netze bringen kan.", "tokens": ["Ob", "er", "das", "alb\u00b7re", "Volck", "ins", "Net\u00b7ze", "brin\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Wird nun der Vogel sich frisch in die L\u00fcffte schwingen/", "tokens": ["Wird", "nun", "der", "Vo\u00b7gel", "sich", "frisch", "in", "die", "L\u00fcff\u00b7te", "schwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NE", "PRF", "ADJD", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.44": {"text": "So ist sein Berg-Flug gantz begl\u00fcckt/", "tokens": ["So", "ist", "sein", "Ber\u00b7gFlug", "gantz", "be\u00b7gl\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Und er dem Netz und Garn entr\u00fcckt:", "tokens": ["Und", "er", "dem", "Netz", "und", "Garn", "ent\u00b7r\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "H\u00f6rt aber er beth\u00f6rt die in dem Kefich singen/", "tokens": ["H\u00f6rt", "a\u00b7ber", "er", "be\u00b7th\u00f6rt", "die", "in", "dem", "Ke\u00b7fich", "sin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PPER", "VVFIN", "ART", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Und nahet sich herzu/ so f\u00e4nget ihn der Strick", "tokens": ["Und", "na\u00b7het", "sich", "her\u00b7zu", "/", "so", "f\u00e4n\u00b7get", "ihn", "der", "Strick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADV", "$(", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Denn w\u00fcrgt der Vogler ihn in einem Augenblick.", "tokens": ["Denn", "w\u00fcrgt", "der", "Vog\u00b7ler", "ihn", "in", "ei\u00b7nem", "Au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Nicht anders gehts mit uns/ der Tod legt seine Garne/", "tokens": ["Nicht", "an\u00b7ders", "gehts", "mit", "uns", "/", "der", "Tod", "legt", "sei\u00b7ne", "Gar\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "APPR", "PPER", "$(", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Zeigt uns der Wollust Honigseim/", "tokens": ["Zeigt", "uns", "der", "Wol\u00b7lust", "Ho\u00b7ni\u00b7gseim", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Den Reichthum/ unsrer Seelen Leim", "tokens": ["Den", "Reicht\u00b7hum", "/", "uns\u00b7rer", "See\u00b7len", "Leim"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Und da\u00df der arm