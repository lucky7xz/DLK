{"textgrid.poem.46957": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Erstes Grab", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Ottensen auf der Wiese", "tokens": ["Zu", "Ot\u00b7ten\u00b7sen", "auf", "der", "Wie\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ist eine gemeinsame Gruft;", "tokens": ["Ist", "ei\u00b7ne", "ge\u00b7mein\u00b7sa\u00b7me", "Gruft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "So traurig ist keine wie diese", "tokens": ["So", "trau\u00b7rig", "ist", "kei\u00b7ne", "wie", "die\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PIAT", "KOKOM", "PDS"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wohl unter des Himmels Luft.", "tokens": ["Wohl", "un\u00b7ter", "des", "Him\u00b7mels", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Darinnen liegt begraben", "tokens": ["Da\u00b7rin\u00b7nen", "liegt", "be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein ganzes Volksgeschlecht,", "tokens": ["Ein", "gan\u00b7zes", "Volks\u00b7ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "V\u00e4ter, M\u00fctter, Br\u00fcder, T\u00f6chter, Kinder, Knaben,", "tokens": ["V\u00e4\u00b7ter", ",", "M\u00fct\u00b7ter", ",", "Br\u00fc\u00b7der", ",", "T\u00f6ch\u00b7ter", ",", "Kin\u00b7der", ",", "Kna\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Zusammen Herr und Knecht.", "tokens": ["Zu\u00b7sam\u00b7men", "Herr", "und", "Knecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die rufen weh zum Himmel", "tokens": ["Die", "ru\u00b7fen", "weh", "zum", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus ihrer stummen Gruft,", "tokens": ["Aus", "ih\u00b7rer", "stum\u00b7men", "Gruft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und werden's rufen zum Himmel,", "tokens": ["Und", "wer\u00b7den's", "ru\u00b7fen", "zum", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wenn die Trommet' einst ruft.", "tokens": ["Wenn", "die", "Trom\u00b7met'", "einst", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Wir haben gewohnt in Frieden", "tokens": ["Wir", "ha\u00b7ben", "ge\u00b7wohnt", "in", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zu Hamburg in der Stadt,", "tokens": ["Zu", "Ham\u00b7burg", "in", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis uns daraus vertrieben", "tokens": ["Bis", "uns", "da\u00b7raus", "ver\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "PAV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein fremder W\u00fctrich hat.", "tokens": ["Ein", "frem\u00b7der", "W\u00fct\u00b7rich", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er hat uns ausgesto\u00dfen", "tokens": ["Er", "hat", "uns", "aus\u00b7ge\u00b7sto\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Winter zur Stadt hinaus,", "tokens": ["Im", "Win\u00b7ter", "zur", "Stadt", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "APZR", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die hungernden, nackenden, blo\u00dfen,", "tokens": ["Die", "hun\u00b7gern\u00b7den", ",", "na\u00b7cken\u00b7den", ",", "blo\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVIZU", "$,", "ADV", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wo finden wir Dach und Haus?", "tokens": ["Wo", "fin\u00b7den", "wir", "Dach", "und", "Haus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Wo finden wir Kost und Kleider,", "tokens": ["Wo", "fin\u00b7den", "wir", "Kost", "und", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wir zwanzigtausend an Zahl? \u2013", "tokens": ["Wir", "zwan\u00b7zig\u00b7tau\u00b7send", "an", "Zahl", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "CARD", "APPR", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die andern schleppten sich weiter,", "tokens": ["Die", "an\u00b7dern", "schlepp\u00b7ten", "sich", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wir blieben hier zumal.", "tokens": ["Wir", "blie\u00b7ben", "hier", "zu\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die andern nahmen die Britten", "tokens": ["Die", "an\u00b7dern", "nah\u00b7men", "die", "Brit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und andre die D\u00e4nen auf;", "tokens": ["Und", "and\u00b7re", "die", "D\u00e4\u00b7nen", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wir brachten mit m\u00fcden Schritten", "tokens": ["Wir", "brach\u00b7ten", "mit", "m\u00fc\u00b7den", "Schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bis hieher unsren Lauf.", "tokens": ["Bis", "hie\u00b7her", "un\u00b7sren", "Lauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wir konnten nicht weiter keuchen,", "tokens": ["Wir", "konn\u00b7ten", "nicht", "wei\u00b7ter", "keu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ersch\u00f6pft war unsere Kraft;", "tokens": ["Er\u00b7sch\u00f6pft", "war", "un\u00b7se\u00b7re", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Frost, Hunger, Elend und Seuchen,", "tokens": ["Frost", ",", "Hun\u00b7ger", ",", "E\u00b7lend", "und", "Seu\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Sie haben uns hingerafft.", "tokens": ["Sie", "ha\u00b7ben", "uns", "hin\u00b7ge\u00b7rafft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Ein ungeheuerer Kn\u00e4uel,", "tokens": ["Ein", "un\u00b7ge\u00b7heu\u00b7e\u00b7rer", "Kn\u00e4u\u00b7el", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zw\u00f6lfhundert oder mehr;", "tokens": ["Zw\u00f6lf\u00b7hun\u00b7dert", "o\u00b7der", "mehr", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es zieht sich \u00fcber den Greuel", "tokens": ["Es", "zieht", "sich", "\u00fc\u00b7ber", "den", "Greu\u00b7el"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein d\u00fcnner Rasen her.", "tokens": ["Ein", "d\u00fcn\u00b7ner", "Ra\u00b7sen", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Der deckt nun unsre Bl\u00f6\u00dfe,", "tokens": ["Der", "deckt", "nun", "uns\u00b7re", "Bl\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Obdach er uns gab;", "tokens": ["Ein", "Ob\u00b7dach", "er", "uns", "gab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Man merkt des Jammers Gr\u00f6\u00dfe", "tokens": ["Man", "merkt", "des", "Jam\u00b7mers", "Gr\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht an dem kleinen Grab.", "tokens": ["Nicht", "an", "dem", "klei\u00b7nen", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}