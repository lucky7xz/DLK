{"dta.poem.8732": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Xi .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcgen im verbund'nen Klange?", "tokens": ["F\u00fc\u00b7gen", "im", "ver\u00b7bun\u00b7d'\u00b7nen", "Klan\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ich mit meinem d\u00fcstern Drange,", "tokens": ["Ich", "mit", "mei\u00b7nem", "d\u00fcs\u00b7tern", "Dran\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du in deiner Jugendsch\u00f6ne?", "tokens": ["Du", "in", "dei\u00b7ner", "Ju\u00b7gend\u00b7sch\u00f6\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heiter schl\u00fcrfst du leichte Stunden,", "tokens": ["Hei\u00b7ter", "schl\u00fcrfst", "du", "leich\u00b7te", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dem es nie vergebens tagte:", "tokens": ["Dem", "es", "nie", "ver\u00b7ge\u00b7bens", "tag\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich ersehne das Versagte,", "tokens": ["Ich", "er\u00b7seh\u00b7ne", "das", "Ver\u00b7sag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und beweine, was verschwunden.", "tokens": ["Und", "be\u00b7wei\u00b7ne", ",", "was", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Du, zu deines M\u00e4dchens Laren", "tokens": ["Du", ",", "zu", "dei\u00b7nes", "M\u00e4d\u00b7chens", "La\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommst du n\u00e4chtlich oft gegangen,", "tokens": ["Kommst", "du", "n\u00e4cht\u00b7lich", "oft", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schmiegst dich an die zarten Wangen,", "tokens": ["Schmiegst", "dich", "an", "die", "zar\u00b7ten", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fchlst in ihren seidnen Haaren:", "tokens": ["W\u00fchlst", "in", "ih\u00b7ren", "seid\u00b7nen", "Haa\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00e4hrend ich, der im Gem\u00fcthe", "tokens": ["W\u00e4h\u00b7rend", "ich", ",", "der", "im", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf den Wink der Gunst verzichtet,", "tokens": ["Auf", "den", "Wink", "der", "Gunst", "ver\u00b7zich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "B\u00fccher vor mir aufgeschichtet,", "tokens": ["B\u00fc\u00b7cher", "vor", "mir", "auf\u00b7ge\u00b7schich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ueber'm Rauch der Lampe br\u00fcte.", "tokens": ["Ue\u00b7ber'm", "Rauch", "der", "Lam\u00b7pe", "br\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Freund, es war ein eitles W\u00e4hnen,", "tokens": ["Freund", ",", "es", "war", "ein", "eit\u00b7les", "W\u00e4h\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df sich unsre Geister f\u00e4nden,", "tokens": ["Da\u00df", "sich", "uns\u00b7re", "Geis\u00b7ter", "f\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsre Blicke sich verst\u00e4nden,", "tokens": ["Uns\u00b7re", "Bli\u00b7cke", "sich", "ver\u00b7st\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich vermischten unsre Thr\u00e4nen:", "tokens": ["Sich", "ver\u00b7mischten", "uns\u00b7re", "Thr\u00e4\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "PPOSAT", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "La\u00df mich denn allein, vers\u00e4ume", "tokens": ["La\u00df", "mich", "denn", "al\u00b7lein", ",", "ver\u00b7s\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht um mich die goldnen Tage,", "tokens": ["Nicht", "um", "mich", "die", "gold\u00b7nen", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUI", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kehre wieder zum Gelage,", "tokens": ["Keh\u00b7re", "wie\u00b7der", "zum", "Ge\u00b7la\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und vergi\u00df den Mann der Tr\u00e4ume!", "tokens": ["Und", "ver\u00b7gi\u00df", "den", "Mann", "der", "Tr\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}