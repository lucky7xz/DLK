{"textgrid.poem.52451": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "33. Berenices Weihe", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf ihre Wiege streuten die Grazien", "tokens": ["Auf", "ih\u00b7re", "Wie\u00b7ge", "streu\u00b7ten", "die", "Gra\u00b7zi\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Bl\u00fctenknospen s\u00fc\u00dfer, versch\u00e4mter Huld;", "tokens": ["Die", "Bl\u00fc\u00b7ten\u00b7knos\u00b7pen", "s\u00fc\u00b7\u00dfer", ",", "ver\u00b7sch\u00e4m\u00b7ter", "Huld", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Unschuld und des Frohsinns Rosen,", "tokens": ["Der", "Un\u00b7schuld", "und", "des", "Froh\u00b7sinns", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schwimmend in zarter Empfindung Dufte.", "tokens": ["Schwim\u00b7mend", "in", "zar\u00b7ter", "Emp\u00b7fin\u00b7dung", "Duf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Der Schwestern j\u00fcngste dr\u00fcckte den dunkeln Kranz", "tokens": ["Der", "Schwes\u00b7tern", "j\u00fcngs\u00b7te", "dr\u00fcck\u00b7te", "den", "dun\u00b7keln", "Kranz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von Sinnviolen sanfter Melancholie", "tokens": ["Von", "Sinn\u00b7vi\u00b7o\u00b7len", "sanf\u00b7ter", "Me\u00b7lan\u00b7cho\u00b7lie"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihr auf die Stirne, traurig l\u00e4chelnd", "tokens": ["Ihr", "auf", "die", "Stir\u00b7ne", ",", "trau\u00b7rig", "l\u00e4\u00b7chelnd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "ADJD", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mit dem z\u00e4rtlichsten Blick der Weihe.", "tokens": ["Und", "mit", "dem", "z\u00e4rt\u00b7lichs\u00b7ten", "Blick", "der", "Wei\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}