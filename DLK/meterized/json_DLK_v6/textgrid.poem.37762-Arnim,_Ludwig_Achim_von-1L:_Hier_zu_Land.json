{"textgrid.poem.37762": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier zu Land", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier zu Land", "tokens": ["Hier", "zu", "Land"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Gilt die Hand,", "tokens": ["Gilt", "die", "Hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Die mit Kunst", "tokens": ["Die", "mit", "Kunst"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Lohnt die Gunst", "tokens": ["Lohnt", "die", "Gunst"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "S\u00fcndger Frauen,", "tokens": ["S\u00fcnd\u00b7ger", "Frau\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Da\u00df sie schauen", "tokens": ["Da\u00df", "sie", "schau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Sich im Bild", "tokens": ["Sich", "im", "Bild"], "token_info": ["word", "word", "word"], "pos": ["PRF", "APPRART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Heilig mild.", "tokens": ["Hei\u00b7lig", "mild", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "\u00bbraphael,", "tokens": ["\u00bb", "ra\u00b7phael", ","], "token_info": ["punct", "word", "punct"], "pos": ["$(", "NE", "$,"], "meter": "--", "measure": "unknown.measure.zero"}, "line.10": {"text": "Gut Gesell,", "tokens": ["Gut", "Ge\u00b7sell", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Mahle mich,", "tokens": ["Mah\u00b7le", "mich", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Ich bitt' Dich, stich", "tokens": ["Ich", "bitt'", "Dich", ",", "stich"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Mark Anton,", "tokens": ["Mark", "An\u00b7ton", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Lieb ist Lohn,", "tokens": ["Lieb", "ist", "Lohn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Und ein Ku\u00df,", "tokens": ["Und", "ein", "Ku\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.16": {"text": "Helfen mu\u00df.\u00ab", "tokens": ["Hel\u00b7fen", "mu\u00df", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NN", "VMFIN", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Andre Staaten", "tokens": ["And\u00b7re", "Staa\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "Andre Saaten,", "tokens": ["And\u00b7re", "Saa\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Andre St\u00e4dtchen", "tokens": ["And\u00b7re", "St\u00e4dt\u00b7chen"], "token_info"