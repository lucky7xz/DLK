{"textgrid.poem.55258": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Ein andres", "genre": "verse", "period": "N.A.", "pub_year": 1812, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mu\u00dft nicht widerstehn dem Schicksal,", "tokens": ["Mu\u00dft", "nicht", "wi\u00b7der\u00b7stehn", "dem", "Schick\u00b7sal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber mu\u00dft es auch nicht fliehen!", "tokens": ["A\u00b7ber", "mu\u00dft", "es", "auch", "nicht", "flie\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wirst du ihm entgegengehen,", "tokens": ["Wirst", "du", "ihm", "ent\u00b7ge\u00b7gen\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird's dich freundlich nach sich ziehen.", "tokens": ["Wird's", "dich", "freund\u00b7lich", "nach", "sich", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}