{"textgrid.poem.38207": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Hessisch", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich kam zur Stube rein,", "tokens": ["Als", "ich", "kam", "zur", "Stu\u00b7be", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da ist gut wohnen!", "tokens": ["Da", "ist", "gut", "woh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ich hab so lang drau\u00dfen gestanden,", "tokens": ["Ich", "hab", "so", "lang", "drau\u00b7\u00dfen", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da\u00df Gott erbarm!", "tokens": ["Da\u00df", "Gott", "er\u00b7barm", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ich seh dies an deinem Hut,", "tokens": ["Ich", "seh", "dies", "an", "dei\u00b7nem", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie dein Hut tr\u00f6pflen thut,", "tokens": ["Wie", "dein", "Hut", "tr\u00f6pf\u00b7len", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Von Regen ist er na\u00df,", "tokens": ["Von", "Re\u00b7gen", "ist", "er", "na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von wegen meinem Schatz.", "tokens": ["Von", "we\u00b7gen", "mei\u00b7nem", "Schatz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich gieng wohl \u00fcber Berg und Thal,", "tokens": ["Ich", "gieng", "wohl", "\u00fc\u00b7ber", "Berg", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "W\u00e4r mir kein Weg zu schmal,", "tokens": ["W\u00e4r", "mir", "kein", "Weg", "zu", "schmal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu meinem Sch\u00e4tzchen wollt ich gehn,", "tokens": ["Zu", "mei\u00b7nem", "Sch\u00e4tz\u00b7chen", "wollt", "ich", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Alle Wochen siebenmal.", "tokens": ["Al\u00b7le", "Wo\u00b7chen", "sie\u00b7ben\u00b7mal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dort steht ein sch\u00f6ner Lorbeerbaum,", "tokens": ["Dort", "steht", "ein", "sch\u00f6\u00b7ner", "Lor\u00b7beer\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der steht sch\u00f6n da,", "tokens": ["Der", "steht", "sch\u00f6n", "da", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und ein sch\u00f6ner Reutersbub,", "tokens": ["Und", "ein", "sch\u00f6\u00b7ner", "Reu\u00b7ters\u00b7bub", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der steht mir an.", "tokens": ["Der", "steht", "mir", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Herz mich ein wenig, k\u00fc\u00df mich ein wenig,", "tokens": ["Herz", "mich", "ein", "we\u00b7nig", ",", "k\u00fc\u00df", "mich", "ein", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "PIS", "$,", "VVFIN", "PPER", "ART", "PIS", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Hab mich ein wenig lieb,", "tokens": ["Hab", "mich", "ein", "we\u00b7nig", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "PIS", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenns auch regnet oder schneit,", "tokens": ["Wenns", "auch", "reg\u00b7net", "o\u00b7der", "schneit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenns unser Herz nur erfreut.", "tokens": ["Wenns", "un\u00b7ser", "Herz", "nur", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}