{"dta.poem.21887": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "XlIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Der Orseus drang", "tokens": ["Der", "Or\u00b7seus", "drang"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "durch seiner Seiten zwang", "tokens": ["durch", "sei\u00b7ner", "Sei\u00b7ten", "zwang"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "in Pluto H\u00f6llen schlund.", "tokens": ["in", "Plu\u00b7to", "H\u00f6l\u00b7len", "schlund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mir ist es auch gelungen/", "tokens": ["Mir", "ist", "es", "auch", "ge\u00b7lun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "da\u00df ich zum schwarzen Grund", "tokens": ["da\u00df", "ich", "zum", "schwar\u00b7zen", "Grund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "durch meine Leyer hingedrungen.", "tokens": ["durch", "mei\u00b7ne", "Le\u00b7yer", "hin\u00b7ge\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}