{"textgrid.poem.24075": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Amor-Vampyr", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im hellen Herbstwald auf buntem Laub", "tokens": ["Im", "hel\u00b7len", "Herbst\u00b7wald", "auf", "bun\u00b7tem", "Laub"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Waren wir wie Kinder und k\u00fc\u00dften uns", "tokens": ["Wa\u00b7ren", "wir", "wie", "Kin\u00b7der", "und", "k\u00fc\u00df\u00b7ten", "uns"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "KOKOM", "NN", "KON", "VVFIN", "PPER"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Unschuldig in linder Liebe.", "tokens": ["Un\u00b7schul\u00b7dig", "in", "lin\u00b7der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Bubenm\u00e4del, Bubenm\u00e4del,", "tokens": ["Bu\u00b7ben\u00b7m\u00e4\u00b7del", ",", "Bu\u00b7ben\u00b7m\u00e4\u00b7del", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie lachten deine Augen, die hellen, braunen,", "tokens": ["Wie", "lach\u00b7ten", "dei\u00b7ne", "Au\u00b7gen", ",", "die", "hel\u00b7len", ",", "brau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie lag dein liebes K\u00f6pfchen so leicht auf dem Laube,", "tokens": ["Wie", "lag", "dein", "lie\u00b7bes", "K\u00f6pf\u00b7chen", "so", "leicht", "auf", "dem", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und leicht auch lagen meine Lippen auf deinen.", "tokens": ["Und", "leicht", "auch", "la\u00b7gen", "mei\u00b7ne", "Lip\u00b7pen", "auf", "dei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Aber die Nacht kam auf Katzenpfoten,", "tokens": ["A\u00b7ber", "die", "Nacht", "kam", "auf", "Kat\u00b7zen\u00b7pfo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Die schwarze, schwere, schweigende Nacht,", "tokens": ["Die", "schwar\u00b7ze", ",", "schwe\u00b7re", ",", "schwei\u00b7gen\u00b7de", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und schw\u00fcl wars im Zimmer.", "tokens": ["Und", "schw\u00fcl", "wars", "im", "Zim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das gelbe Licht der schwebenden Lampe lag", "tokens": ["Das", "gel\u00b7be", "Licht", "der", "schwe\u00b7ben\u00b7den", "Lam\u00b7pe", "lag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Wie leuchtender, feuchter Nebel \u00fcber dem Raum,", "tokens": ["Wie", "leuch\u00b7ten\u00b7der", ",", "feuch\u00b7ter", "Ne\u00b7bel", "\u00fc\u00b7ber", "dem", "Raum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und deine Augen fragten \u00e4ngstlich aus dem gelben D\u00e4mmer.", "tokens": ["Und", "dei\u00b7ne", "Au\u00b7gen", "frag\u00b7ten", "\u00e4ngst\u00b7lich", "aus", "dem", "gel\u00b7ben", "D\u00e4m\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "Braune, br\u00fctende, unselige Augen.", "tokens": ["Brau\u00b7ne", ",", "br\u00fc\u00b7ten\u00b7de", ",", "un\u00b7se\u00b7li\u00b7ge", "Au\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "In ihnen braute, tief unten, tief,", "tokens": ["In", "ih\u00b7nen", "brau\u00b7te", ",", "tief", "un\u00b7ten", ",", "tief", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "ADJD", "ADV", "$,", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Brodelnder giftiger Gischt.", "tokens": ["Bro\u00b7deln\u00b7der", "gif\u00b7ti\u00b7ger", "Gischt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Oh du, du, du!", "tokens": ["Oh", "du", ",", "du", ",", "du", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "PPER", "$,", "PPER", "$,", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Und \u00fcber dich hin warf mich die Wut der Liebe.", "tokens": ["Und", "\u00fc\u00b7ber", "dich", "hin", "warf", "mich", "die", "Wut", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "VVFIN", "PRF", "ART", "NN", "ART", "NN", "$."], "meter": "-+--++--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Und unsre Lippen lasteten aufeinander,", "tokens": ["Und", "uns\u00b7re", "Lip\u00b7pen", "las\u00b7te\u00b7ten", "auf\u00b7ein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie alle schmerzlichen, sehnsuchtschmachtenden S\u00fcnden zweier Sterne,", "tokens": ["Wie", "al\u00b7le", "schmerz\u00b7li\u00b7chen", ",", "sehn\u00b7sucht\u00b7schmach\u00b7ten\u00b7den", "S\u00fcn\u00b7den", "zwei\u00b7er", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVINF", "$,", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Die sich im wirbelnden Weltall treffen", "tokens": ["Die", "sich", "im", "wir\u00b7beln\u00b7den", "Wel\u00b7tall", "tref\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPRART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und klagegellend sich umklammern.", "tokens": ["Und", "kla\u00b7ge\u00b7gel\u00b7lend", "sich", "um\u00b7klam\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Oh du, du, du!", "tokens": ["Oh", "du", ",", "du", ",", "du", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "PPER", "$,", "PPER", "$,", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Und meine Augen gruben sich in deine,", "tokens": ["Und", "mei\u00b7ne", "Au\u00b7gen", "gru\u00b7ben", "sich", "in", "dei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und meine Arme wanden sich um deinen Leib wie Raubtierpranken;", "tokens": ["Und", "mei\u00b7ne", "Ar\u00b7me", "wan\u00b7den", "sich", "um", "dei\u00b7nen", "Leib", "wie", "Raub\u00b7tier\u00b7pran\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.3": {"text": "Und es st\u00f6hnte deine Brust,", "tokens": ["Und", "es", "st\u00f6hn\u00b7te", "dei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und deine Augen irrten wie verflogene Tauben.", "tokens": ["Und", "dei\u00b7ne", "Au\u00b7gen", "irr\u00b7ten", "wie", "ver\u00b7flo\u00b7ge\u00b7ne", "Tau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Sie suchten den hellen Herbstwald", "tokens": ["Sie", "such\u00b7ten", "den", "hel\u00b7len", "Herbst\u00b7wald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und die Kindheit unsrer Liebe", "tokens": ["Und", "die", "Kind\u00b7heit", "uns\u00b7rer", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Im bunten Laube.", "tokens": ["Im", "bun\u00b7ten", "Lau\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Und fanden nicht und wurden schmerzenstarr", "tokens": ["Und", "fan\u00b7den", "nicht", "und", "wur\u00b7den", "schmer\u00b7zen\u00b7starr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "KON", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und h\u00f6llebr\u00fcnstig hei\u00df und hackten in mein Herz", "tokens": ["Und", "h\u00f6l\u00b7le\u00b7br\u00fcns\u00b7tig", "hei\u00df", "und", "hack\u00b7ten", "in", "mein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie schwarze Adlerschn\u00e4bel.", "tokens": ["Wie", "schwar\u00b7ze", "Ad\u00b7ler\u00b7schn\u00e4\u00b7bel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Oh du!", "tokens": ["Oh", "du", "!"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.13": {"line.1": {"text": "Oh du!", "tokens": ["Oh", "du", "!"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.14": {"line.1": {"text": "Matt sank mein Haupt dir in den Schoo\u00df.", "tokens": ["Matt", "sank", "mein", "Haupt", "dir", "in", "den", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Du bebtest.", "tokens": ["Du", "beb\u00b7test", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.15": {"line.1": {"text": "Dann sprachst du leise wirre Worte und weintest.", "tokens": ["Dann", "sprachst", "du", "lei\u00b7se", "wir\u00b7re", "Wor\u00b7te", "und", "wein\u00b7test", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.16": {"line.1": {"text": "Und deine Augen wurden wieder hell.", "tokens": ["Und", "dei\u00b7ne", "Au\u00b7gen", "wur\u00b7den", "wie\u00b7der", "hell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.17": {"line.1": {"text": "Wei\u00dft du es wohl, was zwischen uns geschehn?", "tokens": ["Wei\u00dft", "du", "es", "wohl", ",", "was", "zwi\u00b7schen", "uns", "ge\u00b7schehn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$,", "PRELS", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.18": {"line.1": {"text": "Der Ha\u00df hat uns gepaart in wildem Kampf,", "tokens": ["Der", "Ha\u00df", "hat", "uns", "ge\u00b7paart", "in", "wil\u00b7dem", "Kampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Ha\u00df von Mann zu Weib und Weib zu Mann,", "tokens": ["Der", "Ha\u00df", "von", "Mann", "zu", "Weib", "und", "Weib", "zu", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die hei\u00dfe Gier, sich einzusaugen das fremde Herz", "tokens": ["Die", "hei\u00b7\u00dfe", "Gier", ",", "sich", "ein\u00b7zu\u00b7sau\u00b7gen", "das", "frem\u00b7de", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRF", "VVIZU", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und jeden Tropfen Blutes und jeden Atemzug.", "tokens": ["Und", "je\u00b7den", "Trop\u00b7fen", "Blu\u00b7tes", "und", "je\u00b7den", "A\u00b7tem\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.19": {"line.1": {"text": "Mein Herz und dein Herz haben sich geschaut im Kampfe,", "tokens": ["Mein", "Herz", "und", "dein", "Herz", "ha\u00b7ben", "sich", "ge\u00b7schaut", "im", "Kamp\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VAFIN", "PRF", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und k\u00e4mpfend sich durchdringend sind sie in Eins geflossen.", "tokens": ["Und", "k\u00e4mp\u00b7fend", "sich", "durch\u00b7drin\u00b7gend", "sind", "sie", "in", "Eins", "ge\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "VVPP", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+---+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.20": {"line.1": {"text": "Du bist nun ich, doppelt ist meine Seele.", "tokens": ["Du", "bist", "nun", "ich", ",", "dop\u00b7pelt", "ist", "mei\u00b7ne", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.21": {"line.1": {"text": "Wird sie je leben", "tokens": ["Wird", "sie", "je", "le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "K\u00f6nnen ohne dich?", "tokens": ["K\u00f6n\u00b7nen", "oh\u00b7ne", "dich", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}