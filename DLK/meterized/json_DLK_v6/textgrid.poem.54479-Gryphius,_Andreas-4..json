{"textgrid.poem.54479": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der festen Himmelburg die Schuld' vnd Gri i verschlossen/", "tokens": ["Der", "fes\u00b7ten", "Him\u00b7mel\u00b7burg", "die", "Schuld'", "vnd", "Gri", "i", "ver\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "KON", "NE", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ist Hutt vnd Riegel frey/ dort seh ich Jesum stehn", "tokens": ["Ist", "Hutt", "vnd", "Rie\u00b7gel", "frey", "/", "dort", "seh", "ich", "Je\u00b7sum", "stehn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "KON", "NN", "ADJD", "$(", "ADV", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An Gottes rechter Hand/ gro\u00dfm\u00e4chtig hoch vnd sch\u00f6n/", "tokens": ["An", "Got\u00b7tes", "rech\u00b7ter", "Hand", "/", "gro\u00df\u00b7m\u00e4ch\u00b7tig", "hoch", "vnd", "sch\u00f6n", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$(", "ADJD", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht wie auff Golgatha mit vielem Blutt begossen/", "tokens": ["Nicht", "wie", "auff", "Gol\u00b7ga\u00b7tha", "mit", "vie\u00b7lem", "Blutt", "be\u00b7gos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOKOM", "APPR", "NE", "APPR", "PIS", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich schaue neben Ihm die seiner Schmach genossen/", "tokens": ["Ich", "schau\u00b7e", "ne\u00b7ben", "Ihm", "die", "sei\u00b7ner", "Schmach", "ge\u00b7nos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie herrlich schau ich sie/ wie jauchtzend vmb ihn gehn/", "tokens": ["Wie", "herr\u00b7lich", "schau", "ich", "sie", "/", "wie", "jaucht\u00b7zend", "vmb", "ihn", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PPER", "$(", "PWAV", "ADJD", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vor Angst itzt Freudenvoll/ Welt spotte/ spey vnd h\u00f6hn:", "tokens": ["Vor", "Angst", "itzt", "Freu\u00b7den\u00b7voll", "/", "Welt", "spot\u00b7te", "/", "spey", "vnd", "h\u00f6hn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "$(", "NN", "VVFIN", "$(", "NE", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die keine Noth vnd Schmach vnd Folter hier verdrossen", "tokens": ["Die", "kei\u00b7ne", "Noth", "vnd", "Schmach", "vnd", "Fol\u00b7ter", "hier", "ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "KON", "NN", "KON", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sind au\u00df der Angst zu Trost/ durch Tod zu Gott/ gef\u00fchrt/", "tokens": ["Sind", "au\u00df", "der", "Angst", "zu", "Trost", "/", "durch", "Tod", "zu", "Gott", "/", "ge\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "APPR", "NN", "$(", "APPR", "NN", "APPR", "NN", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vnd mit der Ehren-Kron von Christus Hand geziehrt.", "tokens": ["Vnd", "mit", "der", "Eh\u00b7ren\u00b7Kron", "von", "Chris\u00b7tus", "Hand", "ge\u00b7ziehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Trotzt Feinde! jagt mich au\u00df! versperrt mir eure Th\u00fcren!", "tokens": ["Trotzt", "Fein\u00b7de", "!", "jagt", "mich", "au\u00df", "!", "ver\u00b7sperrt", "mir", "eu\u00b7re", "Th\u00fc\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$.", "VVFIN", "PPER", "PTKVZ", "$.", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn jenes Thor auffgeht/ werfft diesen C\u00f6rper eyn!", "tokens": ["Wenn", "je\u00b7nes", "Thor", "auff\u00b7geht", "/", "werfft", "die\u00b7sen", "C\u00f6r\u00b7per", "eyn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VVFIN", "$(", "VVFIN", "PDAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zubrecht was irrdisch ist/ di\u00df Fleisch/ die Arm vnd Bein!", "tokens": ["Zu\u00b7brecht", "was", "irr\u00b7disch", "ist", "/", "di\u00df", "Fleisch", "/", "die", "Arm", "vnd", "Bein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "ADJD", "VAFIN", "$(", "PDS", "NN", "$(", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich kan den Schatz doch nicht/ der darinn ist/ verlieren.", "tokens": ["Ich", "kan", "den", "Schatz", "doch", "nicht", "/", "der", "da\u00b7rinn", "ist", "/", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "PTKNEG", "$(", "ART", "PAV", "VAFIN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}