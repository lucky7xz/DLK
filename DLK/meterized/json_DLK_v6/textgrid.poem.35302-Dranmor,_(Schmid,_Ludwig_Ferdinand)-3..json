{"textgrid.poem.35302": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Thr\u00e4nen, die um mich geweint,", "tokens": ["Thr\u00e4\u00b7nen", ",", "die", "um", "mich", "ge\u00b7weint", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Abschiedsschw\u00fcre lieber Kinder,", "tokens": ["Ab\u00b7schieds\u00b7schw\u00fc\u00b7re", "lie\u00b7ber", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seid ihr auch nicht ernst gemeint,", "tokens": ["Seid", "ihr", "auch", "nicht", "ernst", "ge\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr ersch\u00fcttert mich nicht minder;", "tokens": ["Ihr", "er\u00b7sch\u00fct\u00b7tert", "mich", "nicht", "min\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn f\u00fcr das, was ich vergangen,", "tokens": ["Denn", "f\u00fcr", "das", ",", "was", "ich", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDS", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "R\u00e4cht sich meine Phantasie,", "tokens": ["R\u00e4cht", "sich", "mei\u00b7ne", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ein Gl\u00fcck, das ich empfangen,", "tokens": ["Und", "ein", "Gl\u00fcck", ",", "das", "ich", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das vergess' ich nie.", "tokens": ["Das", "ver\u00b7ge\u00b7ss'", "ich", "nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Keuscher Lippen zarter Ku\u00df,", "tokens": ["Keu\u00b7scher", "Lip\u00b7pen", "zar\u00b7ter", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kleiner H\u00e4nde freundlich Dr\u00fccken,", "tokens": ["Klei\u00b7ner", "H\u00e4n\u00b7de", "freund\u00b7lich", "Dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An den sch\u00fcchternsten Genu\u00df", "tokens": ["An", "den", "sch\u00fcch\u00b7terns\u00b7ten", "Ge\u00b7nu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--++--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Denk' ich heute mit Entz\u00fccken.", "tokens": ["Denk'", "ich", "heu\u00b7te", "mit", "Ent\u00b7z\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zauber einst geliebter Z\u00fcge,", "tokens": ["Zau\u00b7ber", "einst", "ge\u00b7lieb\u00b7ter", "Z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Einsam, ratlos wie ich bin,", "tokens": ["Ein\u00b7sam", ",", "rat\u00b7los", "wie", "ich", "bin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KOKOM", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, f\u00fcr eine neue L\u00fcge", "tokens": ["Ach", ",", "f\u00fcr", "ei\u00b7ne", "neu\u00b7e", "L\u00fc\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "G\u00e4b' ich alles hin!", "tokens": ["G\u00e4b'", "ich", "al\u00b7les", "hin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIS", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn der Wind die Segel bl\u00e4ht,", "tokens": ["Wenn", "der", "Wind", "die", "Se\u00b7gel", "bl\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hoff' ich wieder zu erfassen,", "tokens": ["Hoff'", "ich", "wie\u00b7der", "zu", "er\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ich deshalb nur verschm\u00e4ht", "tokens": ["Was", "ich", "des\u00b7halb", "nur", "ver\u00b7schm\u00e4ht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "PAV", "ADV", "VVPP"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und verleugnet und verlassen,", "tokens": ["Und", "ver\u00b7leug\u00b7net", "und", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eitle Sehnsucht zu vermehren,", "tokens": ["Eit\u00b7le", "Sehn\u00b7sucht", "zu", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und allein, in d\u00fcstrer Nacht,", "tokens": ["Und", "al\u00b7lein", ",", "in", "d\u00fcst\u00b7rer", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Mich in Trauer zu verzehren,", "tokens": ["Mich", "in", "Trau\u00b7er", "zu", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die ich selbst erdacht.", "tokens": ["Die", "ich", "selbst", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}