{"dta.poem.19509": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die Felswand.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Gigantisch, wildzerissen steigt die Felswand.", "tokens": ["Gi\u00b7gan\u00b7tisch", ",", "wild\u00b7ze\u00b7ris\u00b7sen", "steigt", "die", "Fels\u00b7wand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Auge schrickt zur\u00fcck. Dann irrt es unst\u00e4t", "tokens": ["Das", "Au\u00b7ge", "schrickt", "zu\u00b7r\u00fcck", ".", "Dann", "irrt", "es", "un\u00b7st\u00e4t"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Daran herum. Bang sucht es wo es hafte.", "tokens": ["Da\u00b7ran", "he\u00b7rum", ".", "Bang", "sucht", "es", "wo", "es", "haf\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$.", "NN", "VVFIN", "PPER", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dort! Ueber einem Abgrund schwebt ein Br\u00fccklein", "tokens": ["Dort", "!", "Ue\u00b7ber", "ei\u00b7nem", "Ab\u00b7grund", "schwebt", "ein", "Br\u00fcck\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie Spinnweb. H\u00f6her um die scharfe Kante", "tokens": ["Wie", "Spinn\u00b7web", ".", "H\u00f6\u00b7her", "um", "die", "schar\u00b7fe", "Kan\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "$.", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sind Stapfen eingehaun, ein Bruchst\u00fcck Weges!", "tokens": ["Sind", "Stap\u00b7fen", "ein\u00b7ge\u00b7haun", ",", "ein", "Bruchs\u00b7t\u00fcck", "We\u00b7ges", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVINF", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Fast oben ragt ein Thor mit blauer F\u00fcllung:", "tokens": ["Fast", "o\u00b7ben", "ragt", "ein", "Thor", "mit", "blau\u00b7er", "F\u00fcl\u00b7lung", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dort klimmt der Weg empor zu Licht und H\u00f6he!", "tokens": ["Dort", "klimmt", "der", "Weg", "em\u00b7por", "zu", "Licht", "und", "H\u00f6\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nicht ruht das Aug, bis ihn es aufgefunden:", "tokens": ["Nicht", "ruht", "das", "Aug", ",", "bis", "ihn", "es", "auf\u00b7ge\u00b7fun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Den ganzen Weg entlang die ganze Felswand.", "tokens": ["Den", "gan\u00b7zen", "Weg", "ent\u00b7lang", "die", "gan\u00b7ze", "Fels\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Feindselig blickte sie. Nun blickt sie gastlich,", "tokens": ["Feind\u00b7se\u00b7lig", "blick\u00b7te", "sie", ".", "Nun", "blickt", "sie", "gast\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.12": {"text": "Geeinigt im Zusammenhang des Pfades!", "tokens": ["Ge\u00b7ei\u00b7nigt", "im", "Zu\u00b7sam\u00b7men\u00b7hang", "des", "Pfa\u00b7des", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}}}}}