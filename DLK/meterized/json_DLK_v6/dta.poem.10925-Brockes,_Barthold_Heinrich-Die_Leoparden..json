{"dta.poem.10925": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Leoparden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Eine sehnliche Begier: Was er siehet, zu zerst\u00fccken,", "tokens": ["Ei\u00b7ne", "sehn\u00b7li\u00b7che", "Be\u00b7gier", ":", "Was", "er", "sie\u00b7het", ",", "zu", "zer\u00b7st\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PWS", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+---+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Schielt, nebst Mordlust, Unvergn\u00fcgen, h\u00e4mischen ge-", "tokens": ["Schielt", ",", "nebst", "Mord\u00b7lust", ",", "Un\u00b7ver\u00b7gn\u00fc\u00b7gen", ",", "h\u00e4\u00b7mi\u00b7schen", "ge"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "NN", "$,", "NN", "$,", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Grausamkeit, Wuth, Grimm und Gram, aus des Leoparden", "tokens": ["Grau\u00b7sam\u00b7keit", ",", "Wuth", ",", "Grimm", "und", "Gram", ",", "aus", "des", "Le\u00b7o\u00b7par\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NE", "KON", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Sein am Kopf gedrucktes Ohr, sein von Blut-Durst d\u00fcrrer", "tokens": ["Sein", "am", "Kopf", "ge\u00b7druck\u00b7tes", "Ohr", ",", "sein", "von", "Blut\u00b7Durst", "d\u00fcr\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "APPRART", "NN", "ADJA", "NN", "$,", "PPOSAT", "APPR", "NN", "ADJA"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Voller scharf-und starker Z\u00e4hne, gleichen einem wahren", "tokens": ["Vol\u00b7ler", "scha\u00b7rf\u00b7\u00b7und", "star\u00b7ker", "Z\u00e4h\u00b7ne", ",", "glei\u00b7chen", "ei\u00b7nem", "wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJD", "ADJA", "NN", "$,", "ADJA", "ART", "ADJA"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Ist doch, in des Todes Werkzeug, den gekr\u00fcmmten starken", "tokens": ["Ist", "doch", ",", "in", "des", "To\u00b7des", "Werk\u00b7zeug", ",", "den", "ge\u00b7kr\u00fcmm\u00b7ten", "star\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "$,", "APPR", "ART", "NN", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Und der schweren Pfoten Last, fast der sichre Tod zu schauen.", "tokens": ["Und", "der", "schwe\u00b7ren", "Pfo\u00b7ten", "Last", ",", "fast", "der", "sich\u00b7re", "Tod", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NE", "$,", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Aber welche sanfte Triebe, welche Lust, in ihrer Ruh,", "tokens": ["A\u00b7ber", "wel\u00b7che", "sanf\u00b7te", "Trie\u00b7be", ",", "wel\u00b7che", "Lust", ",", "in", "ih\u00b7rer", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "ADJA", "NN", "$,", "PWAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Sieht man die von ihren Jungen ausgesogne Mutter f\u00fchlen!", "tokens": ["Sieht", "man", "die", "von", "ih\u00b7ren", "Jun\u00b7gen", "aus\u00b7ge\u00b7sog\u00b7ne", "Mut\u00b7ter", "f\u00fch\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "APPR", "PPOSAT", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Der geschlo\u00dfnen Augen Lied deckt ihr grimmig Feuer zu,", "tokens": ["Der", "ge\u00b7schlo\u00df\u00b7nen", "Au\u00b7gen", "Lied", "deckt", "ihr", "grim\u00b7mig", "Feu\u00b7er", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "PPER", "ADJD", "NN", "PTKVZ", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Und es scheint, sie f\u00fchl, im Traum, ihrer jungen Welpen", "tokens": ["Und", "es", "scheint", ",", "sie", "f\u00fchl", ",", "im", "Traum", ",", "ih\u00b7rer", "jun\u00b7gen", "Wel\u00b7pen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "APPRART", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Wie so sanft, gelenk und weich, liegt ihr K\u00f6rper ausgestreckt!", "tokens": ["Wie", "so", "sanft", ",", "ge\u00b7lenk", "und", "weich", ",", "liegt", "ihr", "K\u00f6r\u00b7per", "aus\u00b7ge\u00b7streckt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,", "VVFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "D\u00e4chte man, da\u00df in demselben ein so harter Geist versteckt?", "tokens": ["D\u00e4ch\u00b7te", "man", ",", "da\u00df", "in", "dem\u00b7sel\u00b7ben", "ein", "so", "har\u00b7ter", "Geist", "ver\u00b7steckt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "APPR", "PDAT", "ART", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Was ist ferner nicht f\u00fcr Sch\u00f6nheit auf der Thiere Balg zu sehn!", "tokens": ["Was", "ist", "fer\u00b7ner", "nicht", "f\u00fcr", "Sch\u00f6n\u00b7heit", "auf", "der", "Thie\u00b7re", "Balg", "zu", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PTKNEG", "APPR", "NN", "APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Welche Farben! heisset man sie nicht recht erschrecklich sch\u00f6n?", "tokens": ["Wel\u00b7che", "Far\u00b7ben", "!", "heis\u00b7set", "man", "sie", "nicht", "recht", "er\u00b7schreck\u00b7lich", "sch\u00f6n", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "$.", "VVFIN", "PIS", "PPER", "PTKNEG", "ADV", "ADJD", "ADJD", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Doch ich mu\u00df, bey dieser Sch\u00f6nheit, leider eins noch zugestehn,", "tokens": ["Doch", "ich", "mu\u00df", ",", "bey", "die\u00b7ser", "Sch\u00f6n\u00b7heit", ",", "lei\u00b7der", "eins", "noch", "zu\u00b7ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "$,", "APPR", "PDAT", "NN", "$,", "ADV", "PIS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Ob mir gleich f\u00fcr das Gest\u00e4ndni\u00df dieser bittern Wahrheit", "tokens": ["Ob", "mir", "gleich", "f\u00fcr", "das", "Ge\u00b7st\u00e4nd\u00b7ni\u00df", "die\u00b7ser", "bit\u00b7tern", "Wahr\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.18": {"text": "Nemlich da\u00df, nicht nur bey Thieren, Sch\u00f6n- und Bosheit oft", "tokens": ["Nem\u00b7lich", "da\u00df", ",", "nicht", "nur", "bey", "Thie\u00b7ren", ",", "Sch\u00f6n", "und", "Bos\u00b7heit", "oft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "$,", "PTKNEG", "ADV", "APPR", "NN", "$,", "TRUNC", "KON", "NN", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.19": {"text": "Ach wie oft wird unter Menschen, unter einer sch\u00f6nen Haut,", "tokens": ["Ach", "wie", "oft", "wird", "un\u00b7ter", "Men\u00b7schen", ",", "un\u00b7ter", "ei\u00b7ner", "sch\u00f6\u00b7nen", "Haut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADV", "VAFIN", "APPR", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Auch ein Leoparden-Geist, voller Neid und Wuth, gefunden!", "tokens": ["Auch", "ein", "Le\u00b7o\u00b7par\u00b7den\u00b7Geist", ",", "vol\u00b7ler", "Neid", "und", "Wuth", ",", "ge\u00b7fun\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJA", "NN", "KON", "NN", "$,", "VVPP", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}