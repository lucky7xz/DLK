{"textgrid.poem.63843": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Das Konzert", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In meinem Zimmer mir zur Augenweide", "tokens": ["In", "mei\u00b7nem", "Zim\u00b7mer", "mir", "zur", "Au\u00b7gen\u00b7wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "H\u00e4ngt \u00fcberm Schreibtisch jenes Meisterwerk", "tokens": ["H\u00e4ngt", "\u00fc\u00b7berm", "Schreib\u00b7tisch", "je\u00b7nes", "Meis\u00b7ter\u00b7werk"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Giorgione's, das Konzert, das Kleinod aus", "tokens": ["Gior\u00b7gi\u00b7o\u00b7ne's", ",", "das", "Kon\u00b7zert", ",", "das", "Klei\u00b7nod", "aus"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "$,", "ART", "NN", "APPR"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Dem Schatz Palazzo Pitti's. In der Mitte", "tokens": ["Dem", "Schatz", "Pa\u00b7laz\u00b7zo", "Pit\u00b7ti'", "s.", "In", "der", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "word", "word"], "pos": ["ART", "NN", "NE", "NE", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der blasse junge Mann im schwarzen Kleid,", "tokens": ["Der", "blas\u00b7se", "jun\u00b7ge", "Mann", "im", "schwar\u00b7zen", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der auf den Tasten eines Orgelwerks", "tokens": ["Der", "auf", "den", "Tas\u00b7ten", "ei\u00b7nes", "Or\u00b7gel\u00b7werks"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die schlanken H\u00e4nde ruhen l\u00e4\u00dft, als schl\u00fcg' er", "tokens": ["Die", "schlan\u00b7ken", "H\u00e4n\u00b7de", "ru\u00b7hen", "l\u00e4\u00dft", ",", "als", "schl\u00fcg'", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF", "VVFIN", "$,", "KOUS", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Schlu\u00dfakkord, den feierlichen, an", "tokens": ["Den", "Schlu\u00df\u00b7ak\u00b7kord", ",", "den", "fei\u00b7er\u00b7li\u00b7chen", ",", "an"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der geistlichen Motette, die den Zwei'n", "tokens": ["Der", "geist\u00b7li\u00b7chen", "Mo\u00b7tet\u00b7te", ",", "die", "den", "Zwei'n"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Er vorgespielt, den Freunden, gleich ihm selbst", "tokens": ["Er", "vor\u00b7ge\u00b7spielt", ",", "den", "Freun\u00b7den", ",", "gleich", "ihm", "selbst"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$,", "ADV", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Verehrer Palestrina's. Oder war's", "tokens": ["Ver\u00b7eh\u00b7rer", "Pa\u00b7le\u00b7stri\u00b7na'", "s.", "O\u00b7der", "wa\u00b7r's"], "token_info": ["word", "word", "abbreviation", "word", "word"], "pos": ["ADJA", "NN", "NE", "NE", "VAFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "Ein St\u00fcck, entsprungen aus der eignen Seele", "tokens": ["Ein", "St\u00fcck", ",", "ent\u00b7sprun\u00b7gen", "aus", "der", "eig\u00b7nen", "See\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Des Spielers, oder freie Phantasie?", "tokens": ["Des", "Spie\u00b7lers", ",", "o\u00b7der", "frei\u00b7e", "Phan\u00b7ta\u00b7sie", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Nein, eines Meisters Sch\u00f6pfung mu\u00df es sein.", "tokens": ["Nein", ",", "ei\u00b7nes", "Meis\u00b7ters", "Sch\u00f6p\u00b7fung", "mu\u00df", "es", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Denn zu dem Alten hinter ihm das Haupt", "tokens": ["Denn", "zu", "dem", "Al\u00b7ten", "hin\u00b7ter", "ihm", "das", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Umwendend, scheint sein mystisch hei\u00dfer Blick", "tokens": ["Um\u00b7wen\u00b7dend", ",", "scheint", "sein", "mys\u00b7tisch", "hei\u00b7\u00dfer", "Blick"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "VVFIN", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Zu fragen: Ist's nicht wunderbar? Und hab'", "tokens": ["Zu", "fra\u00b7gen", ":", "Ist's", "nicht", "wun\u00b7der\u00b7bar", "?", "Und", "hab'"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKZU", "VVINF", "$.", "NE", "PTKNEG", "ADJD", "$.", "KON", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Ich dir's zu Dank gespielt? Allein der Freund", "tokens": ["Ich", "dir's", "zu", "Dank", "ge\u00b7spielt", "?", "Al\u00b7lein", "der", "Freund"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PIS", "APPR", "NN", "VVPP", "$.", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "(wohl ein Pr\u00e4lat, der Kleidung nach; ein Kranz", "tokens": ["(", "wohl", "ein", "Pr\u00e4\u00b7lat", ",", "der", "Klei\u00b7dung", "nach", ";", "ein", "Kranz"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADV", "ART", "NN", "$,", "ART", "NN", "PTKVZ", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Von dunklem Haar umzirkt sein kahles Haupt),", "tokens": ["Von", "dunk\u00b7lem", "Haar", "um\u00b7zirkt", "sein", "kah\u00b7les", "Haupt", ")", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$(", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "In stiller R\u00fchrung zuckt's um seinen Mund,", "tokens": ["In", "stil\u00b7ler", "R\u00fch\u00b7rung", "zuckt's", "um", "sei\u00b7nen", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und traulich auf des J\u00fcnglings Schulter legt er", "tokens": ["Und", "trau\u00b7lich", "auf", "des", "J\u00fcng\u00b7lings", "Schul\u00b7ter", "legt", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Die Rechte, gleich als spr\u00e4ch' er: Bravo, Freund!", "tokens": ["Die", "Rech\u00b7te", ",", "gleich", "als", "spr\u00e4ch'", "er", ":", "Bra\u00b7vo", ",", "Freund", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "KOKOM", "VVFIN", "PPER", "$.", "ITJ", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Du spieltest wundervoll! \u2013 Die linke Hand", "tokens": ["Du", "spiel\u00b7test", "wun\u00b7der\u00b7voll", "!", "\u2013", "Die", "lin\u00b7ke", "Hand"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "H\u00e4lt einer Laute schlanken Hals umfa\u00dft", "tokens": ["H\u00e4lt", "ei\u00b7ner", "Lau\u00b7te", "schlan\u00b7ken", "Hals", "um\u00b7fa\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "(auch er \u00fcbt wohl Musik, als Dilettant),", "tokens": ["(", "auch", "er", "\u00fcbt", "wohl", "Mu\u00b7sik", ",", "als", "Di\u00b7let\u00b7tant", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "PPER", "VVFIN", "ADV", "NN", "$,", "KOUS", "NN", "$(", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.27": {"text": "Indes der Dritt' im Bunde, jener J\u00fcngling", "tokens": ["In\u00b7des", "der", "Dritt'", "im", "Bun\u00b7de", ",", "je\u00b7ner", "J\u00fcng\u00b7ling"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$,", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "In Federhut und adligem Gewand,", "tokens": ["In", "Fe\u00b7der\u00b7hut", "und", "ad\u00b7li\u00b7gem", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Herausblickt aus dem Bilde, wie noch ganz", "tokens": ["Her\u00b7aus\u00b7blickt", "aus", "dem", "Bil\u00b7de", ",", "wie", "noch", "ganz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PWAV", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Versunken in Entz\u00fcckungen, und scheint", "tokens": ["Ver\u00b7sun\u00b7ken", "in", "Ent\u00b7z\u00fc\u00b7ckun\u00b7gen", ",", "und", "scheint"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.31": {"text": "Mich anzureden: H\u00e4ttest du's geh\u00f6rt,", "tokens": ["Mich", "an\u00b7zu\u00b7re\u00b7den", ":", "H\u00e4t\u00b7test", "du's", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVIZU", "$.", "VVFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Du st\u00fcndest unterm Zauber noch, gleich mir!", "tokens": ["Du", "st\u00fcn\u00b7dest", "un\u00b7term", "Zau\u00b7ber", "noch", ",", "gleich", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "$,", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und f\u00fchl' ich anders? In das seelenvolle", "tokens": ["Und", "f\u00fchl'", "ich", "an\u00b7ders", "?", "In", "das", "see\u00b7len\u00b7vol\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$.", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gesicht des Spielers blickend, ist es mir,", "tokens": ["Ge\u00b7sicht", "des", "Spie\u00b7lers", "bli\u00b7ckend", ",", "ist", "es", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "$,", "VAFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als h\u00f6b' ein sanftes Klingen geisterhaft", "tokens": ["Als", "h\u00f6b'", "ein", "sanf\u00b7tes", "Klin\u00b7gen", "geis\u00b7ter\u00b7haft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich an und dringe mir mit magischer", "tokens": ["Sich", "an", "und", "drin\u00b7ge", "mir", "mit", "ma\u00b7gi\u00b7scher"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "PTKVZ", "KON", "VVFIN", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gewalt an Seel' und Sinn und f\u00fclle mir", "tokens": ["Ge\u00b7walt", "an", "Seel'", "und", "Sinn", "und", "f\u00fcl\u00b7le", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das Herz mit Wonneklang, indes von drau\u00dfen", "tokens": ["Das", "Herz", "mit", "Won\u00b7ne\u00b7klang", ",", "in\u00b7des", "von", "drau\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "ADV", "APPR", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der See, anst\u00fcrmend an die Uferwehr,", "tokens": ["Der", "See", ",", "an\u00b7st\u00fcr\u00b7mend", "an", "die", "U\u00b7fer\u00b7wehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mit tiefem Orgelba\u00df die Melodie", "tokens": ["Mit", "tie\u00b7fem", "Or\u00b7gel\u00b7ba\u00df", "die", "Me\u00b7lo\u00b7die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Begleite.", "tokens": ["Be\u00b7glei\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Heil'ge Musen, schwesterlich", "tokens": ["Heil'\u00b7ge", "Mu\u00b7sen", ",", "schwes\u00b7ter\u00b7lich"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "F\u00fcrwahr reicht ihr euch oft die Hand. Denn hier", "tokens": ["F\u00fcr\u00b7wahr", "reicht", "ihr", "euch", "oft", "die", "Hand", ".", "Denn", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "$.", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Aus des Giorgione stummberedtem Bild", "tokens": ["Aus", "des", "Gi\u00b7or\u00b7gio\u00b7ne", "stumm\u00b7be\u00b7red\u00b7tem", "Bild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.13": {"text": "T\u00f6nt zauberisch mit l\u00e4ngst verklungner Macht", "tokens": ["T\u00f6nt", "zau\u00b7be\u00b7risch", "mit", "l\u00e4ngst", "ver\u00b7klung\u00b7ner", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Ein Hauch italischer Tonkunst mir entgegen.", "tokens": ["Ein", "Hauch", "i\u00b7ta\u00b7li\u00b7scher", "Ton\u00b7kunst", "mir", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}}}}