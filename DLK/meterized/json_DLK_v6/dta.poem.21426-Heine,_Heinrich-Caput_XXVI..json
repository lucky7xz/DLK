{"dta.poem.21426": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edas ist ein Zauberkessel worin", "tokens": ["\u201e", "das", "ist", "ein", "Zau\u00b7ber\u00b7kes\u00b7sel", "wo\u00b7rin"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die magischen Kr\u00e4fte brauen,", "tokens": ["Die", "ma\u00b7gi\u00b7schen", "Kr\u00e4f\u00b7te", "brau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und steckst du in die R\u00fcndung den Kopf,", "tokens": ["Und", "steckst", "du", "in", "die", "R\u00fcn\u00b7dung", "den", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "So wirst du die Zukunft schauen \u2013", "tokens": ["So", "wirst", "du", "die", "Zu\u00b7kunft", "schau\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u201edie Zukunft Deutschlands erblickst du hier,", "tokens": ["\u201e", "die", "Zu\u00b7kunft", "Deutschlands", "er\u00b7blickst", "du", "hier", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NE", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gleich wogenden Phantasmen,", "tokens": ["Gleich", "wo\u00b7gen\u00b7den", "Phan\u00b7tas\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch schaudre nicht, wenn aus dem Wust", "tokens": ["Doch", "schaud\u00b7re", "nicht", ",", "wenn", "aus", "dem", "Wust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aufsteigen die Miasmen!\u201c", "tokens": ["Auf\u00b7stei\u00b7gen", "die", "Mi\u00b7as\u00b7men", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie sprach\u2019s und lachte sonderbar,", "tokens": ["Sie", "sprach's", "und", "lach\u00b7te", "son\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich aber lie\u00df mich nicht schrecken,", "tokens": ["Ich", "a\u00b7ber", "lie\u00df", "mich", "nicht", "schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Neugierig eilte ich den Kopf", "tokens": ["Neu\u00b7gie\u00b7rig", "eil\u00b7te", "ich", "den", "Kopf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In die furchtbare R\u00fcndung zu stecken.", "tokens": ["In", "die", "furcht\u00b7ba\u00b7re", "R\u00fcn\u00b7dung", "zu", "ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.4": {"line.1": {"text": "Was ich gesehn, verrathe ich nicht,", "tokens": ["Was", "ich", "ge\u00b7sehn", ",", "ver\u00b7ra\u00b7the", "ich", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich habe zu schweigen versprochen,", "tokens": ["Ich", "ha\u00b7be", "zu", "schwei\u00b7gen", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Erlaubt ist mir zu sagen kaum,", "tokens": ["Er\u00b7laubt", "ist", "mir", "zu", "sa\u00b7gen", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PTKZU", "VVINF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Gott! was ich gerochen! \u2013 \u2013 \u2013", "tokens": ["O", "Gott", "!", "was", "ich", "ge\u00b7ro\u00b7chen", "!", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["NE", "NN", "$.", "PWS", "PPER", "VVPP", "$.", "$(", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}