{"dta.poem.9802": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Engelland.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ja freylich sorget man f\u00fcr dein und mein vergn\u00fcgen/", "tokens": ["Ja", "frey\u00b7lich", "sor\u00b7get", "man", "f\u00fcr", "dein", "und", "mein", "ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "VVFIN", "PIS", "APPR", "PPOSAT", "KON", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und di\u00df sicht unsern feind am allermeisten an/", "tokens": ["Und", "di\u00df", "sicht", "un\u00b7sern", "feind", "am", "al\u00b7ler\u00b7meis\u00b7ten", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem des verh\u00e4ngni\u00df spruch/ in fernern krieges-z\u00fcgen/", "tokens": ["Dem", "des", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "spruch", "/", "in", "fer\u00b7nern", "krie\u00b7ge\u00b7sz\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$(", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den l\u00e4ngst beschlo\u00df\u2019nen fall bereits hat kund gethan.", "tokens": ["Den", "l\u00e4ngst", "be\u00b7schlo\u00df'\u00b7nen", "fall", "be\u00b7reits", "hat", "kund", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "ADV", "VAFIN", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auf Teutschland/ la\u00df uns dann macht/ muth und treu verneue\u0303.", "tokens": ["Auf", "Teutschland", "/", "la\u00df", "uns", "dann", "macht", "/", "muth", "und", "treu", "ver\u00b7neu\u1ebd", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$(", "VVIMP", "PPER", "ADV", "VVFIN", "$(", "NN", "KON", "ADJD", "VVFIN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Ein edler friede wird doch nur durch streit gekr\u00f6nt.", "tokens": ["Ein", "ed\u00b7ler", "frie\u00b7de", "wird", "doch", "nur", "durch", "streit", "ge\u00b7kr\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein K\u00f6nig und dein held/ die uns in noth erfreuen/", "tokens": ["Mein", "K\u00f6\u00b7nig", "und", "dein", "held", "/", "die", "uns", "in", "noth", "er\u00b7freu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "VVFIN", "$(", "PRELS", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die werden nur durch di\u00df/ was uns behagt/ vers\u00f6hnt.", "tokens": ["Die", "wer\u00b7den", "nur", "durch", "di\u00df", "/", "was", "uns", "be\u00b7hagt", "/", "ver\u00b7s\u00f6hnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "PDS", "$(", "PWS", "PPER", "VVFIN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie aber wird von mir dein theurer F\u00fcrst geehret?", "tokens": ["Wie", "a\u00b7ber", "wird", "von", "mir", "dein", "theu\u00b7rer", "F\u00fcrst", "ge\u00b7eh\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den ich den meinen auch mit rechte nennen mag.", "tokens": ["Den", "ich", "den", "mei\u00b7nen", "auch", "mit", "rech\u00b7te", "nen\u00b7nen", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["NE", "PPER", "ART", "VVFIN", "ADV", "APPR", "ADJA", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Sein elter vater hat mir diesen ruhm gew\u00e4hret/", "tokens": ["Sein", "el\u00b7ter", "va\u00b7ter", "hat", "mir", "die\u00b7sen", "ruhm", "ge\u00b7w\u00e4h\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da er in meiner scho\u00df erblickt den ersten tag.", "tokens": ["Da", "er", "in", "mei\u00b7ner", "scho\u00df", "er\u00b7blickt", "den", "ers\u00b7ten", "tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "VVFIN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es liebt sein ohre zwar mehr der carthaunen knallen;", "tokens": ["Es", "liebt", "sein", "oh\u00b7re", "zwar", "mehr", "der", "car\u00b7thau\u00b7nen", "knal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Doch seine freundlichkeit verschm\u00e4h\u2019t kein schlechtes lied/", "tokens": ["Doch", "sei\u00b7ne", "freund\u00b7lich\u00b7keit", "ver\u00b7schm\u00e4h't", "kein", "schlech\u00b7tes", "lied", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Das meiner t\u00f6chter chor zu seinem lob l\u00e4st schallen/", "tokens": ["Das", "mei\u00b7ner", "t\u00f6ch\u00b7ter", "chor", "zu", "sei\u00b7nem", "lob", "l\u00e4st", "schal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wo an der Themse man die weisse schwanen sieh\u2019t.", "tokens": ["Wo", "an", "der", "Them\u00b7se", "man", "die", "weis\u00b7se", "schwa\u00b7nen", "sieh'", "t."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "APPR", "ART", "NN", "PIS", "ART", "ADJA", "ADJA", "NN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}