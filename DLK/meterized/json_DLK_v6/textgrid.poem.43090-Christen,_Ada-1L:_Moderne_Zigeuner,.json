{"textgrid.poem.43090": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Moderne Zigeuner,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Moderne Zigeuner,", "tokens": ["Mo\u00b7der\u00b7ne", "Zi\u00b7geu\u00b7ner", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "W\u00fcste Gesellen,", "tokens": ["W\u00fcs\u00b7te", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Vagabunden des Lebens.", "tokens": ["Va\u00b7ga\u00b7bun\u00b7den", "des", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Die ringen", "tokens": ["Die", "rin\u00b7gen"], "token_info": ["word", "word"], "pos": ["PDS", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Und wandern", "tokens": ["Und", "wan\u00b7dern"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Und suchen \u2013", "tokens": ["Und", "su\u00b7chen", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Doch immer vergebens!", "tokens": ["Doch", "im\u00b7mer", "ver\u00b7ge\u00b7bens", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Einsame gro\u00dfe Kinder", "tokens": ["Ein\u00b7sa\u00b7me", "gro\u00b7\u00dfe", "Kin\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "Mit halbem Wissen", "tokens": ["Mit", "hal\u00b7bem", "Wis\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Todtkrankem Herzen \u2013", "tokens": ["Todt\u00b7kran\u00b7kem", "Her\u00b7zen", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Und immer hinaus, immer weiter!", "tokens": ["Und", "im\u00b7mer", "hin\u00b7aus", ",", "im\u00b7mer", "wei\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKVZ", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Nach au\u00dfen keck,", "tokens": ["Nach", "au\u00b7\u00dfen", "keck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Nach innen verjammert,", "tokens": ["Nach", "in\u00b7nen", "ver\u00b7jam\u00b7mert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Den R\u00fccken zerschlagen von der Hand,", "tokens": ["Den", "R\u00fc\u00b7cken", "zer\u00b7schla\u00b7gen", "von", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "An die sie vertrauend sich geklammert!", "tokens": ["An", "die", "sie", "ver\u00b7trau\u00b7end", "sich", "ge\u00b7klam\u00b7mert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVPP", "PRF", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}