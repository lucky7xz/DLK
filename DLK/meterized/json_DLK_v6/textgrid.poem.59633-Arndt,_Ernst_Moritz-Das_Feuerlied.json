{"textgrid.poem.59633": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Das Feuerlied", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus Feuer ist der Geist geschaffen,", "tokens": ["Aus", "Feu\u00b7er", "ist", "der", "Geist", "ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Drum schenkt mir s\u00fc\u00dfes Feuer ein!", "tokens": ["Drum", "schenkt", "mir", "s\u00fc\u00b7\u00dfes", "Feu\u00b7er", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Lust der Lieder und der Waffen,", "tokens": ["Die", "Lust", "der", "Lie\u00b7der", "und", "der", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lust der Liebe schenkt mir ein,", "tokens": ["Die", "Lust", "der", "Lie\u00b7be", "schenkt", "mir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Traube s\u00fc\u00dfes Sonnenblut,", "tokens": ["Der", "Trau\u00b7be", "s\u00fc\u00b7\u00dfes", "Son\u00b7nen\u00b7blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Wunder glaubt und Wunder tut.", "tokens": ["Das", "Wun\u00b7der", "glaubt", "und", "Wun\u00b7der", "tut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was soll ich mit dem Zeuge machen,", "tokens": ["Was", "soll", "ich", "mit", "dem", "Zeu\u00b7ge", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Wasser ohne Saft und Kraft?", "tokens": ["Dem", "Was\u00b7ser", "oh\u00b7ne", "Saft", "und", "Kraft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gemacht f\u00fcr Fr\u00f6sche, Kr\u00f6ten, Drachen", "tokens": ["Ge\u00b7macht", "f\u00fcr", "Fr\u00f6\u00b7sche", ",", "Kr\u00f6\u00b7ten", ",", "Dra\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fcr die ganze W\u00fcrmerschaft?", "tokens": ["Und", "f\u00fcr", "die", "gan\u00b7ze", "W\u00fcr\u00b7mer\u00b7schaft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr Menschen mu\u00df es frischer sein,", "tokens": ["F\u00fcr", "Men\u00b7schen", "mu\u00df", "es", "fri\u00b7scher", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drum bringet Wein und schenket Wein!", "tokens": ["Drum", "brin\u00b7get", "Wein", "und", "schen\u00b7ket", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NN", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O Wonnesaft der edlen Reben!", "tokens": ["O", "Won\u00b7ne\u00b7saft", "der", "ed\u00b7len", "Re\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Gegengift f\u00fcr jede Pein!", "tokens": ["O", "Ge\u00b7gen\u00b7gift", "f\u00fcr", "je\u00b7de", "Pein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie matt und w\u00e4\u00dfrig flie\u00dft das Leben,", "tokens": ["Wie", "matt", "und", "w\u00e4\u00df\u00b7rig", "flie\u00dft", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ohne Stern und Sonnenschein,", "tokens": ["Wie", "oh\u00b7ne", "Stern", "und", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn du, der einzig leuchten kann,", "tokens": ["Wenn", "du", ",", "der", "ein\u00b7zig", "leuch\u00b7ten", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht z\u00fcndest deine Lichter an!", "tokens": ["Nicht", "z\u00fcn\u00b7dest", "dei\u00b7ne", "Lich\u00b7ter", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es w\u00e4re Glauben, Lieben, Hoffen", "tokens": ["Es", "w\u00e4\u00b7re", "Glau\u00b7ben", ",", "Lie\u00b7ben", ",", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "NN", "$,", "ADJA", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und alle Herzensherrlichkeit", "tokens": ["Und", "al\u00b7le", "Her\u00b7zens\u00b7herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im nassen Jammer l\u00e4ngst ersoffen,", "tokens": ["Im", "nas\u00b7sen", "Jam\u00b7mer", "l\u00e4ngst", "er\u00b7sof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und alles Leben hie\u00dfe Leid,", "tokens": ["Und", "al\u00b7les", "Le\u00b7ben", "hie\u00b7\u00dfe", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "W\u00e4rst du nicht in der Wassersnot", "tokens": ["W\u00e4rst", "du", "nicht", "in", "der", "Was\u00b7sers\u00b7not"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPR", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Des Mutes Sporn, der Sorge Tod.", "tokens": ["Des", "Mu\u00b7tes", "Sporn", ",", "der", "Sor\u00b7ge", "Tod", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Drum dreimal Ruf und Klang gegeben!", "tokens": ["Drum", "drei\u00b7mal", "Ruf", "und", "Klang", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr frohen Br\u00fcder, sto\u00dfet an!", "tokens": ["Ihr", "fro\u00b7hen", "Br\u00fc\u00b7der", ",", "sto\u00b7\u00dfet", "an", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem frischen, k\u00fchnen Wind im Leben,", "tokens": ["Dem", "fri\u00b7schen", ",", "k\u00fch\u00b7nen", "Wind", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Schiff und Segel treiben kann!", "tokens": ["Der", "Schiff", "und", "Se\u00b7gel", "trei\u00b7ben", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ruft ", "tokens": ["Ruft"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Und trinket aus und schenket ein!", "tokens": ["Und", "trin\u00b7ket", "aus", "und", "schen\u00b7ket", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Aus Feuer ist der Geist geschaffen,", "tokens": ["Aus", "Feu\u00b7er", "ist", "der", "Geist", "ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Drum schenkt mir s\u00fc\u00dfes Feuer ein!", "tokens": ["Drum", "schenkt", "mir", "s\u00fc\u00b7\u00dfes", "Feu\u00b7er", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Lust der Lieder und der Waffen,", "tokens": ["Die", "Lust", "der", "Lie\u00b7der", "und", "der", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lust der Liebe schenkt mir ein,", "tokens": ["Die", "Lust", "der", "Lie\u00b7be", "schenkt", "mir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Traube s\u00fc\u00dfes Sonnenblut,", "tokens": ["Der", "Trau\u00b7be", "s\u00fc\u00b7\u00dfes", "Son\u00b7nen\u00b7blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Wunder glaubt und Wunder tut.", "tokens": ["Das", "Wun\u00b7der", "glaubt", "und", "Wun\u00b7der", "tut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}