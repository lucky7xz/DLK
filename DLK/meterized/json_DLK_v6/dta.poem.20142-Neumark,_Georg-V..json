{"dta.poem.20142": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "V.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1666", "urn": "urn:nbn:de:kobv:b4-20424-1", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch-Historischer Lustgarten. Frankfurt (Main), 1666."}, "poem": {"stanza.1": {"line.1": {"text": "Ein aufgewekkter Geist ist nicht dem Feyren\nhold/", "tokens": ["Ein", "auf\u00b7ge\u00b7wekk\u00b7ter", "Geist", "ist", "nicht", "dem", "Fey\u00b7ren", "hold", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er ahmt der Sonnen nach/ die in dem Kleid von", "tokens": ["Er", "ahmt", "der", "Son\u00b7nen", "nach", "/", "die", "in", "dem", "Kleid", "von"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "$(", "ART", "APPR", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die\u00df gro\u00dfe Rond \u00fcmrennt/ und niemals stille", "tokens": ["Die\u00df", "gro\u00b7\u00dfe", "Rond", "\u00fcm\u00b7rennt", "/", "und", "nie\u00b7mals", "stil\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "VVFIN", "$(", "KON", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bald Ost-bald Westenwerts den Tageswagen", "tokens": ["Bald", "Ost\u00b7bald", "Wes\u00b7ten\u00b7werts", "den", "Ta\u00b7ges\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "NE", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So thut der Himmel auch Er wieget und be-", "tokens": ["So", "thut", "der", "Him\u00b7mel", "auch", "Er", "wie\u00b7get", "und", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PPER", "VVFIN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sich selber; keine Nacht ihn jemals schlaffen", "tokens": ["Sich", "sel\u00b7ber", ";", "kei\u00b7ne", "Nacht", "ihn", "je\u00b7mals", "schlaf\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "$.", "PIAT", "NN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So thut auch ihr mein Freund! legt sich die Feder", "tokens": ["So", "thut", "auch", "ihr", "mein", "Freund", "!", "legt", "sich", "die", "Fe\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "PPOSAT", "NN", "$.", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Eilt ihr Gesch\u00e4ften nach/ ihr nehmt Sie allzeit", "tokens": ["Eilt", "ihr", "Ge\u00b7sch\u00e4f\u00b7ten", "nach", "/", "ihr", "nehmt", "Sie", "all\u00b7zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "$(", "PPER", "VVFIN", "PPER", "ADV"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.9": {"text": "Die gute Tschterin. Die\u00df Thun ist eure", "tokens": ["Die", "gu\u00b7te", "Tschte\u00b7rin", ".", "Die\u00df", "Thun", "ist", "eu\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PDS", "NN", "VAFIN", "PPOSAT"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Ein Ander l\u00e4ufft dem Ball/ ihr laufft der Pallas", "tokens": ["Ein", "An\u00b7der", "l\u00e4ufft", "dem", "Ball", "/", "ihr", "laufft", "der", "Pal\u00b7las"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Wenn der die Becher schwenkt/ schenkt ihr Uns", "tokens": ["Wenn", "der", "die", "Be\u00b7cher", "schwenkt", "/", "schenkt", "ihr", "Uns"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "ART", "NN", "VVFIN", "$(", "VVFIN", "PPER", "PPER"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.12": {"text": "Durch die bleibt euer Nahm/ wol vor dem Tode", "tokens": ["Durch", "die", "bleibt", "eu\u00b7er", "Nahm", "/", "wol", "vor", "dem", "To\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "VVFIN", "PPOSAT", "NN", "$(", "ADV", "APPR", "ART", "NN"], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.13": {"text": "Ich lode dieses Thun: und lache solcher", "tokens": ["Ich", "lo\u00b7de", "die\u00b7ses", "Thun", ":", "und", "la\u00b7che", "sol\u00b7cher"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$.", "KON", "VVFIN", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die sich mit Erde labt des Himmels unbe-", "tokens": ["Die", "sich", "mit", "Er\u00b7de", "labt", "des", "Him\u00b7mels", "un\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "NN", "VVFIN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Fahrt oft/ und f\u00fchret Uns auch oft in euren", "tokens": ["Fahrt", "oft", "/", "und", "f\u00fch\u00b7ret", "Uns", "auch", "oft", "in", "eu\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$(", "KON", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Der etwas bessers zeigt/ als hundert Nelken", "tokens": ["Der", "et\u00b7was", "bes\u00b7sers", "zeigt", "/", "als", "hun\u00b7dert", "Nel\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIAT", "ADJA", "VVFIN", "$(", "KOKOM", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Wo Klio selbst begiest/ und w\u00e4rmet F", "tokens": ["Wo", "Klio", "selbst", "be\u00b7giest", "/", "und", "w\u00e4r\u00b7met", "F"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NE", "ADV", "VVFIN", "$(", "KON", "VVFIN", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Da ist ja Gartenlust/ wo Geistes Augen seyn.", "tokens": ["Da", "ist", "ja", "Gar\u00b7ten\u00b7lust", "/", "wo", "Geis\u00b7tes", "Au\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "$(", "PWAV", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}