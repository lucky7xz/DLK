{"textgrid.poem.57351": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: H\u00e4tt' ich hundert Stimmen; ich feyerte Galliens Freyheit", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00e4tt' ich hundert Stimmen; ich feyerte Galliens Freyheit", "tokens": ["H\u00e4tt'", "ich", "hun\u00b7dert", "Stim\u00b7men", ";", "ich", "fe\u00b7yer\u00b7te", "Gal\u00b7li\u00b7ens", "Frey\u00b7heit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "CARD", "NN", "$.", "PPER", "VVFIN", "NE", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Nicht mit erreichendem Ton, s\u00e4nge die g\u00f6ttliche schwach.", "tokens": ["Nicht", "mit", "er\u00b7rei\u00b7chen\u00b7dem", "Ton", ",", "s\u00e4n\u00b7ge", "die", "g\u00f6tt\u00b7li\u00b7che", "schwach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "$,", "VVFIN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Was vollbringet sie nicht! So gar das gr\u00e4sslichste aller", "tokens": ["Was", "voll\u00b7brin\u00b7get", "sie", "nicht", "!", "So", "gar", "das", "gr\u00e4ss\u00b7lichs\u00b7te", "al\u00b7ler"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PTKNEG", "$.", "ADV", "ADV", "ART", "ADJA", "PIAT"], "meter": "-+---+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ungeheuer, der Krieg, wird an die Kette gelegt!", "tokens": ["Un\u00b7ge\u00b7heu\u00b7er", ",", "der", "Krieg", ",", "wird", "an", "die", "Ket\u00b7te", "ge\u00b7legt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Cerberus hat drey Rachen; der Krieg hat tausend: und dennoch", "tokens": ["Cer\u00b7be\u00b7rus", "hat", "drey", "Ra\u00b7chen", ";", "der", "Krieg", "hat", "tau\u00b7send", ":", "und", "den\u00b7noch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VAFIN", "CARD", "NN", "$.", "ART", "NN", "VAFIN", "VVPP", "$.", "KON", "ADV"], "meter": "+--+-+--+-+-+-+", "measure": "iambic.septa.invert"}, "line.6": {"text": "Heulen sie alle durch dich, G\u00f6ttin, am Fesselgeklirr.", "tokens": ["Heu\u00b7len", "sie", "al\u00b7le", "durch", "dich", ",", "G\u00f6t\u00b7tin", ",", "am", "Fes\u00b7sel\u00b7ge\u00b7klirr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "PIS", "APPR", "PPER", "$,", "NN", "$,", "APPRART", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Ach mein Vaterland! Viel sind der Schmerzen; doch lindert", "tokens": ["Ach", "mein", "Va\u00b7ter\u00b7land", "!", "Viel", "sind", "der", "Schmer\u00b7zen", ";", "doch", "lin\u00b7dert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "PPOSAT", "NN", "$.", "ADV", "VAFIN", "ART", "NN", "$.", "ADV", "VVFIN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Sie die heilende Zeit, und sie bluten nicht mehr.", "tokens": ["Sie", "die", "hei\u00b7len\u00b7de", "Zeit", ",", "und", "sie", "blu\u00b7ten", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.9": {"text": "Aber es ist Ein Schmerz, den sie nie mir lindert! und kehrte", "tokens": ["A\u00b7ber", "es", "ist", "Ein", "Schmerz", ",", "den", "sie", "nie", "mir", "lin\u00b7dert", "!", "und", "kehr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "PPER", "VVFIN", "$.", "KON", "VVFIN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Mir das Leben zur\u00fcck; dennoch blutet' er fort!", "tokens": ["Mir", "das", "Le\u00b7ben", "zu\u00b7r\u00fcck", ";", "den\u00b7noch", "blu\u00b7tet'", "er", "fort", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+--+-+++-+", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Ach du warest es nicht, mein Vaterland, das der Freyheit", "tokens": ["Ach", "du", "wa\u00b7rest", "es", "nicht", ",", "mein", "Va\u00b7ter\u00b7land", ",", "das", "der", "Frey\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "PPER", "VAFIN", "PPER", "PTKNEG", "$,", "PPOSAT", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Gipfel erstieg, Beyspiel strahlte den V\u00f6lkern umher:", "tokens": ["Gip\u00b7fel", "er\u00b7stieg", ",", "Bey\u00b7spiel", "strahl\u00b7te", "den", "V\u00f6l\u00b7kern", "um\u00b7her", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+---+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Frankreich wars! du labtest dich nicht an der frohsten der Ehren,", "tokens": ["Fran\u00b7kreich", "wars", "!", "du", "lab\u00b7test", "dich", "nicht", "an", "der", "frohs\u00b7ten", "der", "Eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "ADJA", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Brachest den heiligen Zweig dieser Unsterblichteil nicht!", "tokens": ["Bra\u00b7chest", "den", "hei\u00b7li\u00b7gen", "Zweig", "die\u00b7ser", "U\u00b7nsterb\u00b7lich\u00b7teil", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PDAT", "NN", "PTKNEG", "$."], "meter": "+--+--+--+-++-", "measure": "dactylic.tri.plus"}, "line.15": {"text": "O ich weiss es, du f\u00fchlest, was dir nicht wurde; die Palme,", "tokens": ["O", "ich", "weiss", "es", ",", "du", "f\u00fch\u00b7lest", ",", "was", "dir", "nicht", "wur\u00b7de", ";", "die", "Pal\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "$,", "PWS", "PPER", "PTKNEG", "VAFIN", "$.", "ART", "NN", "$,"], "meter": "+-+-++--+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Aber die du nicht tr\u00e4gst, gr\u00fcnet so sch\u00f6n, wie sie ist,", "tokens": ["A\u00b7ber", "die", "du", "nicht", "tr\u00e4gst", ",", "gr\u00fc\u00b7net", "so", "sch\u00f6n", ",", "wie", "sie", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,", "VVFIN", "ADV", "ADJD", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Deinem kennenden Blick. Denn ihr gleicht, ihr gleichet die Palme,", "tokens": ["Dei\u00b7nem", "ken\u00b7nen\u00b7den", "Blick", ".", "Denn", "ihr", "gleicht", ",", "ihr", "glei\u00b7chet", "die", "Pal\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.18": {"text": "Welche du dir brachst, als du die Religion", "tokens": ["Wel\u00b7che", "du", "dir", "brachst", ",", "als", "du", "die", "Re\u00b7li\u00b7gi\u00b7on"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.19": {"text": "Reinigtest, sie, die entweiht Despoten batten, von neuem", "tokens": ["Rei\u00b7nig\u00b7test", ",", "sie", ",", "die", "ent\u00b7weiht", "Des\u00b7po\u00b7ten", "bat\u00b7ten", ",", "von", "neu\u00b7em"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PPER", "$,", "PRELS", "VVFIN", "NN", "VVFIN", "$,", "APPR", "ADJA"], "meter": "+---+-+-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.20": {"text": "Weihtest, Despoten voll Sucht Seelen zu fesseln! voll Blut,", "tokens": ["Weih\u00b7test", ",", "Des\u00b7po\u00b7ten", "voll", "Sucht", "See\u00b7len", "zu", "fes\u00b7seln", "!", "voll", "Blut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "ADJD", "NN", "NN", "PTKZU", "VVINF", "$.", "ADJD", "NN", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.21": {"text": "Welches sie str\u00f6men liessen, so bald der Beherschte nicht glaubte,", "tokens": ["Wel\u00b7ches", "sie", "str\u00f6\u00b7men", "lies\u00b7sen", ",", "so", "bald", "der", "Be\u00b7herschte", "nicht", "glaub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "VVFIN", "$,", "ADV", "ADV", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.22": {"text": "Was ihr taumelnder Wahn ihm zu glauben gebot.", "tokens": ["Was", "ihr", "tau\u00b7meln\u00b7der", "Wahn", "ihm", "zu", "glau\u00b7ben", "ge\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.23": {"text": "Wenn durch dich, mein Vaterland, der beschornen Despoten", "tokens": ["Wenn", "durch", "dich", ",", "mein", "Va\u00b7ter\u00b7land", ",", "der", "be\u00b7schor\u00b7nen", "Des\u00b7po\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Joch nicht zerbrach; so zerbrach das der gekr\u00f6nten itzt nicht.", "tokens": ["Joch", "nicht", "zer\u00b7brach", ";", "so", "zer\u00b7brach", "das", "der", "ge\u00b7kr\u00f6n\u00b7ten", "itzt", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVFIN", "$.", "ADV", "VVFIN", "ART", "ART", "ADJA", "ADV", "PTKNEG", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.25": {"text": "K\u00f6nt' ein Trost mich tr\u00f6sten; er w\u00e4re, dass du vorangingst", "tokens": ["K\u00f6nt'", "ein", "Trost", "mich", "tr\u00f6s\u00b7ten", ";", "er", "w\u00e4\u00b7re", ",", "dass", "du", "vor\u00b7an\u00b7gingst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF", "$.", "PPER", "VAFIN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.26": {"text": "Auf der erhabenen Bahn! aber er tr\u00f6stet mich nicht.", "tokens": ["Auf", "der", "er\u00b7ha\u00b7be\u00b7nen", "Bahn", "!", "a\u00b7ber", "er", "tr\u00f6s\u00b7tet", "mich", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Denn du warest es nicht, das auch von dem Staube des B\u00fcrgers", "tokens": ["Denn", "du", "wa\u00b7rest", "es", "nicht", ",", "das", "auch", "von", "dem", "Stau\u00b7be", "des", "B\u00fcr\u00b7gers"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "PTKNEG", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-----+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Freyheit erhob, Beyspiel strahlte den V\u00f6lkern umher;", "tokens": ["Frey\u00b7heit", "er\u00b7hob", ",", "Bey\u00b7spiel", "strahl\u00b7te", "den", "V\u00f6l\u00b7kern", "um\u00b7her", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--++-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.29": {"text": "Denen nicht nur, die Europa gebar. An Amerika's Str\u00f6men", "tokens": ["De\u00b7nen", "nicht", "nur", ",", "die", "Eu\u00b7ro\u00b7pa", "ge\u00b7bar", ".", "An", "A\u00b7me\u00b7ri\u00b7ka's", "Str\u00f6\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PTKNEG", "ADV", "$,", "PRELS", "NE", "ADJD", "$.", "APPR", "NE", "NN"], "meter": "+--+--+-+-+-+--+-", "measure": "dactylic.di.plus"}, "line.30": {"text": "Flamt schon eigenes Licht, leuchtet den V\u00f6lkern umher.", "tokens": ["Flamt", "schon", "ei\u00b7ge\u00b7nes", "Licht", ",", "leuch\u00b7tet", "den", "V\u00f6l\u00b7kern", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.31": {"text": "Hier auch winkte mir Trost, er war: In Amerika leuchten", "tokens": ["Hier", "auch", "wink\u00b7te", "mir", "Trost", ",", "er", "war", ":", "In", "A\u00b7me\u00b7ri\u00b7ka", "leuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "NN", "$,", "PPER", "VAFIN", "$.", "APPR", "NE", "VVINF"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.32": {"text": "Deutsche zugleich umher! aber er tr\u00f6stete nicht.", "tokens": ["Deut\u00b7sche", "zu\u00b7gleich", "um\u00b7her", "!", "a\u00b7ber", "er", "tr\u00f6s\u00b7te\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$.", "KON", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}}}}}