{"textgrid.poem.61262": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Fr\u00fchling, Fr\u00fchling, der in mildem Tauen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Fr\u00fchling, Fr\u00fchling, der in mildem Tauen", "tokens": ["O", "Fr\u00fch\u00b7ling", ",", "Fr\u00fch\u00b7ling", ",", "der", "in", "mil\u00b7dem", "Tau\u00b7en"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Voll Sch\u00f6pfungswonne du das All durchdringst,", "tokens": ["Voll", "Sch\u00f6p\u00b7fungs\u00b7won\u00b7ne", "du", "das", "All", "durch\u00b7dringst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der du das Meer, den Himmel l\u00e4ssest blauen", "tokens": ["Der", "du", "das", "Meer", ",", "den", "Him\u00b7mel", "l\u00e4s\u00b7sest", "blau\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "$,", "ART", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und rauschend mit dem Bach vom Felsen springst,", "tokens": ["Und", "rau\u00b7schend", "mit", "dem", "Bach", "vom", "Fel\u00b7sen", "springst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der du die Flur mit goldnen Schauern tr\u00e4nkst", "tokens": ["Der", "du", "die", "Flur", "mit", "gold\u00b7nen", "Schau\u00b7ern", "tr\u00e4nkst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und still in jedes Veilchens Scho\u00df dich senkst;", "tokens": ["Und", "still", "in", "je\u00b7des", "Veil\u00b7chens", "Scho\u00df", "dich", "senkst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PIAT", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der du zum Lied wirst in des Vogels Kehle,", "tokens": ["Der", "du", "zum", "Lied", "wirst", "in", "des", "Vo\u00b7gels", "Keh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VAFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die jauchzend hoch im \u00c4ther \u00fcberflie\u00dft,", "tokens": ["Die", "jauch\u00b7zend", "hoch", "im", "\u00c4\u00b7ther", "\u00fc\u00b7berf\u00b7lie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Alls Liebe schleichest in des M\u00e4dchens Seele,", "tokens": ["Alls", "Lie\u00b7be", "schlei\u00b7chest", "in", "des", "M\u00e4d\u00b7chens", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Da\u00df sch\u00f6ner, wie du sie im Tal erziehst,", "tokens": ["Da\u00df", "sch\u00f6\u00b7ner", ",", "wie", "du", "sie", "im", "Tal", "er\u00b7ziehst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "$,", "PWAV", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die rote Ros' auf ihren Wangen sprie\u00dft:", "tokens": ["Die", "ro\u00b7te", "Ros'", "auf", "ih\u00b7ren", "Wan\u00b7gen", "sprie\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "O Fr\u00fchling, tiefer, s\u00fc\u00dfer Gotteshauch,", "tokens": ["O", "Fr\u00fch\u00b7ling", ",", "tie\u00b7fer", ",", "s\u00fc\u00b7\u00dfer", "Got\u00b7tes\u00b7hauch", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Sei mir gegr\u00fc\u00dft und f\u00fclle du mich auch!", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", "und", "f\u00fcl\u00b7le", "du", "mich", "auch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "KON", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Wie eine Welle leg' dich an mein Herz", "tokens": ["Wie", "ei\u00b7ne", "Wel\u00b7le", "leg'", "dich", "an", "mein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und sp\u00fcle sanft hinweg den letzten Schmerz!", "tokens": ["Und", "sp\u00fc\u00b7le", "sanft", "hin\u00b7weg", "den", "letz\u00b7ten", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du nimmst ihn weg. Es kommt mit deinem Wehen", "tokens": ["Du", "nimmst", "ihn", "weg", ".", "Es", "kommt", "mit", "dei\u00b7nem", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein sch\u00f6nes, jugendliches Auferstehen.", "tokens": ["Ein", "sch\u00f6\u00b7nes", ",", "ju\u00b7gend\u00b7li\u00b7ches", "Auf\u00b7er\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du kleidest nicht den Forst allein in Gr\u00fcn", "tokens": ["Du", "klei\u00b7dest", "nicht", "den", "Forst", "al\u00b7lein", "in", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und lehrst die junge Brut die Fl\u00fcgel heben:", "tokens": ["Und", "lehrst", "die", "jun\u00b7ge", "Brut", "die", "Fl\u00fc\u00b7gel", "he\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit jedem Laub mu\u00df eine ", "tokens": ["Mit", "je\u00b7dem", "Laub", "mu\u00df", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Um mit den Lerchen sonnenw\u00e4rts zu schweben,", "tokens": ["Um", "mit", "den", "Ler\u00b7chen", "son\u00b7nen\u00b7w\u00e4rts", "zu", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ja, zu den Gr\u00e4bern seh' ich fromm dich schreiten,", "tokens": ["Ja", ",", "zu", "den", "Gr\u00e4\u00b7bern", "seh'", "ich", "fromm", "dich", "schrei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die tau'gen Opferspenden drauf zu breiten,", "tokens": ["Die", "tau'\u00b7gen", "Op\u00b7fer\u00b7spen\u00b7den", "drauf", "zu", "brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Als wolltest du mit Kr\u00e4nzen und mit Z\u00e4hren", "tokens": ["Als", "woll\u00b7test", "du", "mit", "Kr\u00e4n\u00b7zen", "und", "mit", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "So Gram als Tod in Herrlichkeit verkl\u00e4ren.", "tokens": ["So", "Gram", "als", "Tod", "in", "Herr\u00b7lich\u00b7keit", "ver\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "KOUS", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O Zeit, wo Rosen auf den Gr\u00fcften stehn,", "tokens": ["O", "Zeit", ",", "wo", "Ro\u00b7sen", "auf", "den", "Gr\u00fcf\u00b7ten", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wir den Tod selbst Bl\u00fcten tragen sehn!", "tokens": ["Und", "wir", "den", "Tod", "selbst", "Bl\u00fc\u00b7ten", "tra\u00b7gen", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "ADV", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da mag das Herz, nicht mehr der Sorge Raub,", "tokens": ["Da", "mag", "das", "Herz", ",", "nicht", "mehr", "der", "Sor\u00b7ge", "Raub", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "$,", "PTKNEG", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Kirchhof der ", "tokens": ["Den", "Kirch\u00b7hof", "der"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ART"], "meter": "-+--", "measure": "dactylic.init"}, "line.5": {"text": "Und, Fr\u00fchling ahnend in vermorschtem Staub,", "tokens": ["Und", ",", "Fr\u00fch\u00b7ling", "ah\u00b7nend", "in", "ver\u00b7morschtem", "Staub", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Getrost an halb versunknen M\u00e4lern beten;", "tokens": ["Ge\u00b7trost", "an", "halb", "ver\u00b7sun\u00b7knen", "M\u00e4\u00b7lern", "be\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es f\u00fchlt, kein F\u00fcnkchen Geist ist uns verloren,", "tokens": ["Es", "f\u00fchlt", ",", "kein", "F\u00fcnk\u00b7chen", "Geist", "ist", "uns", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PIAT", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Bl\u00fcte f\u00e4llt, doch auch das Samenkorn,", "tokens": ["Die", "Bl\u00fc\u00b7te", "f\u00e4llt", ",", "doch", "auch", "das", "Sa\u00b7men\u00b7korn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der Fels zerbirst, doch ihm entwallt der Born,", "tokens": ["Der", "Fels", "zer\u00b7birst", ",", "doch", "ihm", "ent\u00b7wallt", "der", "Born", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "PPER", "VVFIN", "ART", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und aus der Lava wird der Wein geboren.", "tokens": ["Und", "aus", "der", "La\u00b7va", "wird", "der", "Wein", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So denk' ich dein zuerst im Totenfeld,", "tokens": ["So", "denk'", "ich", "dein", "zu\u00b7erst", "im", "To\u00b7ten\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Hellas, bl\u00fchend Jugendland der Welt,", "tokens": ["Mein", "Hel\u00b7las", ",", "bl\u00fc\u00b7hend", "Ju\u00b7gend\u00b7land", "der", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo unter sel'gem Himmel ohne Neid", "tokens": ["Wo", "un\u00b7ter", "sel'\u00b7gem", "Him\u00b7mel", "oh\u00b7ne", "Neid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Baum emporwuchs holder Menschlichkeit;", "tokens": ["Der", "Baum", "em\u00b7por\u00b7wuchs", "hol\u00b7der", "Menschlich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo wie im Busen der gew\u00f6lbten Laute", "tokens": ["Wo", "wie", "im", "Bu\u00b7sen", "der", "ge\u00b7w\u00f6lb\u00b7ten", "Lau\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "KOKOM", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In jeder Seel' ein tiefer Wohllaut schlief,", "tokens": ["In", "je\u00b7der", "Seel'", "ein", "tie\u00b7fer", "Wohl\u00b7laut", "schlief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wo jede Trauer den Altar sich baute,", "tokens": ["Wo", "je\u00b7de", "Trau\u00b7er", "den", "Al\u00b7tar", "sich", "bau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und jede Lust nach ihrem Gotte rief,", "tokens": ["Und", "je\u00b7de", "Lust", "nach", "ih\u00b7rem", "Got\u00b7te", "rief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Du heilig Land, an dessen Sonnenk\u00fcsten", "tokens": ["Du", "hei\u00b7lig", "Land", ",", "an", "des\u00b7sen", "Son\u00b7nen\u00b7k\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJD", "NN", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die Sch\u00f6nheit stieg, da sie das Meer gezeugt,", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "stieg", ",", "da", "sie", "das", "Meer", "ge\u00b7zeugt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und dessen Kinder sie an G\u00f6tterbr\u00fcsten,", "tokens": ["Und", "des\u00b7sen", "Kin\u00b7der", "sie", "an", "G\u00f6t\u00b7ter\u00b7br\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die jungfr\u00e4uliche Amme, gro\u00df ges\u00e4ugt.", "tokens": ["Die", "jung\u00b7fr\u00e4u\u00b7li\u00b7che", "Am\u00b7me", ",", "gro\u00df", "ge\u00b7s\u00e4ugt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Ja ", "tokens": ["Ja"], "token_i