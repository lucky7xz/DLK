{"dta.poem.2616": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Abgebildtes Grab  \n  Fr. A. J. v. R. g. M. v. B. den  9.  \n Octobr. 1667.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es hat die Forder-Welt h\u00f6chst-eifrig sich beflissen", "tokens": ["Es", "hat", "die", "For\u00b7der\u00b7Welt", "h\u00f6chst\u00b7eif\u00b7rig", "sich", "be\u00b7flis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das allerletzte Hau\u00df dem Menschen wol zu bau\u2019n;", "tokens": ["Das", "al\u00b7ler\u00b7letz\u00b7te", "Hau\u00df", "dem", "Men\u00b7schen", "wol", "zu", "bau'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So bald/ was sie geliebt/ ihr ward hinweg gerissen/", "tokens": ["So", "bald", "/", "was", "sie", "ge\u00b7liebt", "/", "ihr", "ward", "hin\u00b7weg", "ge\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "PWS", "PPER", "VVPP", "$(", "PPER", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Must\u2019 eines K\u00fcnstlers Hand das Bild im Marmel", "tokens": ["Must'", "ei\u00b7nes", "K\u00fcnst\u00b7lers", "Hand", "das", "Bild", "im", "Mar\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "NN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und recht; ob an sich selbst die Tugend nicht vermodert/", "tokens": ["Und", "recht", ";", "ob", "an", "sich", "selbst", "die", "Tu\u00b7gend", "nicht", "ver\u00b7mo\u00b7dert", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "KOUS", "APPR", "PRF", "ADV", "ART", "NN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Lorbern von dem Ruhm die F\u00e4ulni\u00df nicht verzehrt/", "tokens": ["Die", "Lor\u00b7bern", "von", "dem", "Ruhm", "die", "F\u00e4ul\u00b7ni\u00df", "nicht", "ver\u00b7zehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ja der Verdienste Licht auch aus den Gr\u00fcfften lodert/", "tokens": ["Ja", "der", "Ver\u00b7diens\u00b7te", "Licht", "auch", "aus", "den", "Gr\u00fcff\u00b7ten", "lo\u00b7dert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nichts minder hat sie doch ein Denckmahl hier begehrt.", "tokens": ["Nichts", "min\u00b7der", "hat", "sie", "doch", "ein", "Denck\u00b7mahl", "hier", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Unschuld erster Zeit erkieste Berg\u2019 und H\u00f6len/", "tokens": ["Die", "Un\u00b7schuld", "ers\u00b7ter", "Zeit", "er\u00b7kies\u00b7te", "Ber\u00b7g'", "und", "H\u00f6\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "So ward der meiste Theil der V\u00e4ter beygelegt/", "tokens": ["So", "ward", "der", "meis\u00b7te", "Theil", "der", "V\u00e4\u00b7ter", "bey\u00b7ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Bi\u00df da\u00df der Ubermuth stieg in der Heyden Seelen/", "tokens": ["Bi\u00df", "da\u00df", "der", "U\u00b7ber\u00b7muth", "stieg", "in", "der", "Hey\u00b7den", "See\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die zu den Gr\u00e4bern Gold und theures Ertz gepr\u00e4gt.", "tokens": ["Die", "zu", "den", "Gr\u00e4\u00b7bern", "Gold", "und", "theu\u00b7res", "Ertz", "ge\u00b7pr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Man wei\u00df Egyptens Pracht und hohe Wunder-Spitzen/", "tokens": ["Man", "wei\u00df", "E\u00b7gyp\u00b7tens", "Pracht", "und", "ho\u00b7he", "Wun\u00b7der\u00b7Spit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NE", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die g\u00fcldenen Gef\u00e4\u00df\u2019/ aus welchen Balsam flo\u00df/", "tokens": ["Die", "g\u00fcl\u00b7de\u00b7nen", "Ge\u00b7f\u00e4\u00b7\u00df'", "/", "aus", "wel\u00b7chen", "Bal\u00b7sam", "flo\u00df", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "APPR", "PWAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Die C\u00f6rper vor dem Wurm und Schimmel zu besch\u00fctzen/", "tokens": ["Die", "C\u00f6r\u00b7per", "vor", "dem", "Wurm", "und", "Schim\u00b7mel", "zu", "be\u00b7sch\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "KON", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wie man verschwenderisch mit Narden sie bego\u00df/", "tokens": ["Wie", "man", "ver\u00b7schwen\u00b7de\u00b7risch", "mit", "Nar\u00b7den", "sie", "be\u00b7go\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "APPR", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und must\u2019 ein ewig Licht viel hundert Jahre brennen.", "tokens": ["Und", "must'", "ein", "e\u00b7wig", "Licht", "viel", "hun\u00b7dert", "Jah\u00b7re", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJD", "NN", "ADV", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "(der gleichen Ampeln hat alt Rom unzehlich viel)", "tokens": ["(", "der", "glei\u00b7chen", "Am\u00b7peln", "hat", "alt", "Rom", "un\u00b7zeh\u00b7lich", "viel", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VAFIN", "ADJD", "NE", "ADJD", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Aus den Geschichten ist das Wunder-Grab zu kennen/", "tokens": ["Aus", "den", "Ge\u00b7schich\u00b7ten", "ist", "das", "Wun\u00b7der\u00b7Grab", "zu", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So der Cleopatra gedient zum Trauer-Spiel.", "tokens": ["So", "der", "Cleo\u00b7pat\u00b7ra", "ge\u00b7dient", "zum", "Trau\u00b7e\u00b7rSpiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NE", "VVPP", "APPRART", "NN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.21": {"text": "Was that der Grieche nicht mit den verstorbnen Helden?", "tokens": ["Was", "that", "der", "Grie\u00b7che", "nicht", "mit", "den", "ver\u00b7storb\u00b7nen", "Hel\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Jhr Grab war von Crystall/ sie stets zu sehn/ gemacht/", "tokens": ["Ihr", "Grab", "war", "von", "Crys\u00b7tall", "/", "sie", "stets", "zu", "sehn", "/", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NE", "$(", "PPER", "ADV", "PTKZU", "VVINF", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Leiche stund geschm\u00fcckt/ und/ wie sie selbsten melden/", "tokens": ["Die", "Lei\u00b7che", "stund", "ge\u00b7schm\u00fcckt", "/", "und", "/", "wie", "sie", "selbs\u00b7ten", "mel\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$(", "KON", "$(", "PWAV", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Unsch\u00e4tzbar ausgeputzt in G\u00f6tter-gleicher Tracht.", "tokens": ["Un\u00b7sch\u00e4tz\u00b7bar", "aus\u00b7ge\u00b7putzt", "in", "G\u00f6t\u00b7ter\u00b7glei\u00b7cher", "Tracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.25": {"text": "Die Persen setzten gar den Grossen aus Achaten", "tokens": ["Die", "Per\u00b7sen", "setz\u00b7ten", "gar", "den", "Gros\u00b7sen", "aus", "A\u00b7cha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und andrem seltnen Stein ein herrlich Grabmahl auf.", "tokens": ["Und", "an\u00b7drem", "selt\u00b7nen", "Stein", "ein", "herr\u00b7lich", "Grab\u00b7mahl", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "ART", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Rom/ aller V\u00f6lcker Haupt/ belohnte tapffre Thaten", "tokens": ["Rom", "/", "al\u00b7ler", "V\u00f6l\u00b7cker", "Haupt", "/", "be\u00b7lohn\u00b7te", "tapf\u00b7fre", "Tha\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$(", "PIAT", "NN", "NN", "$(", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Mit Gr\u00e4bern/ derer Zier noch trotzt der Zeiten Lauff.", "tokens": ["Mit", "Gr\u00e4\u00b7bern", "/", "de\u00b7rer", "Zier", "noch", "trotzt", "der", "Zei\u00b7ten", "Lauff", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "PDS", "NN", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Hoch-Edler/ der itzt auch ein Grab mu\u00df zubereiten", "tokens": ["Hoch\u00b7Ed\u00b7ler", "/", "der", "itzt", "auch", "ein", "Grab", "mu\u00df", "zu\u00b7be\u00b7rei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "ART", "ADV", "ADV", "ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.31": {"text": "Der sich zugleich mit ", "tokens": ["Der", "sich", "zu\u00b7gleich", "mit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PRF", "ADV", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.32": {"text": "Und seinen Schatz verscharrn/ so nichts als Lust gebahr:", "tokens": ["Und", "sei\u00b7nen", "Schatz", "ver\u00b7scharrn", "/", "so", "nichts", "als", "Lust", "ge\u00b7bahr", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$(", "ADV", "PIS", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Mit was vor herbem Weh und unersch\u00f6pfftem Leiden", "tokens": ["Mit", "was", "vor", "her\u00b7bem", "Weh", "und", "un\u00b7er\u00b7sch\u00f6pff\u00b7tem", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "APPR", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Er diese letzte Pflicht der ", "tokens": ["Er", "die\u00b7se", "letz\u00b7te", "Pflicht", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PDAT", "ADJA", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Wietief der Schmertzen mag in Grund der andern schneiden/", "tokens": ["Wie\u00b7tief", "der", "Schmert\u00b7zen", "mag", "in", "Grund", "der", "an\u00b7dern", "schnei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VMFIN", "APPR", "NN", "ART", "ADJA", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Bekennt das Auge schon/ ob gleich die Lippe schweigt.", "tokens": ["Be\u00b7kennt", "das", "Au\u00b7ge", "schon", "/", "ob", "gleich", "die", "Lip\u00b7pe", "schweigt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$(", "KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ists wahr/ als wie der Schlu\u00df der meisten Weisen lehret/", "tokens": ["Ists", "wahr", "/", "als", "wie", "der", "Schlu\u00df", "der", "meis\u00b7ten", "Wei\u00b7sen", "leh\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$(", "KOUS", "KOKOM", "ART", "NN", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Da\u00df aus getreuer Lieb\u2019 entsprie\u00df\u2019 ein h\u00f6chstes Gut?", "tokens": ["Da\u00df", "aus", "ge\u00b7treu\u00b7er", "Lieb'", "ent\u00b7sprie\u00df'", "ein", "h\u00f6chs\u00b7tes", "Gut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "So ist auch di\u00df gewi\u00df/ da\u00df/ wann sie wird zerst\u00f6ret/", "tokens": ["So", "ist", "auch", "di\u00df", "ge\u00b7wi\u00df", "/", "da\u00df", "/", "wann", "sie", "wird", "zer\u00b7st\u00f6\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PDS", "ADV", "$(", "KOUS", "$(", "PWAV", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ein Centner-schweres Leid pre\u00dft den gekr\u00e4nckten Muth.", "tokens": ["Ein", "Cent\u00b7ner\u00b7schwe\u00b7res", "Leid", "pre\u00dft", "den", "ge\u00b7kr\u00e4nck\u00b7ten", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.41": {"text": "Der vorige Besitz so viel und sch\u00f6ner Gaben/", "tokens": ["Der", "vo\u00b7ri\u00b7ge", "Be\u00b7sitz", "so", "viel", "und", "sch\u00f6\u00b7ner", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Vermehret den Verlust/ und doppelt nur die Pein/", "tokens": ["Ver\u00b7meh\u00b7ret", "den", "Ver\u00b7lust", "/", "und", "dop\u00b7pelt", "nur", "die", "Pein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["