{"textgrid.poem.45208": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Pillersdorff", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich schilt der Verst\u00e4ndge, verlacht der Duns,", "tokens": ["Dich", "schilt", "der", "Ver\u00b7st\u00e4nd\u00b7ge", ",", "ver\u00b7lacht", "der", "Duns", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ich lasse so leicht dich nicht fallen,", "tokens": ["Ich", "las\u00b7se", "so", "leicht", "dich", "nicht", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Du bist der Ver\u00e4chtlichste unter uns,", "tokens": ["Du", "bist", "der", "Ver\u00b7\u00e4cht\u00b7lichs\u00b7te", "un\u00b7ter", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wenn nicht der Gescheitste von allen.", "tokens": ["Wenn", "nicht", "der", "Ge\u00b7scheits\u00b7te", "von", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "APPR", "PIAT", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}