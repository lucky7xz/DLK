{"dta.poem.8790": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Xi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Als ein Sclave deiner Gnade folgen dir!", "tokens": ["Als", "ein", "Scla\u00b7ve", "dei\u00b7ner", "Gna\u00b7de", "fol\u00b7gen", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "D\u00fcrft' ich von mir werfen jeder Fessel Druck,", "tokens": ["D\u00fcrft'", "ich", "von", "mir", "wer\u00b7fen", "je\u00b7der", "Fes\u00b7sel", "Druck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "VVFIN", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Ueber Land und Meer gerade folgen dir!", "tokens": ["Ue\u00b7ber", "Land", "und", "Meer", "ge\u00b7ra\u00b7de", "fol\u00b7gen", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "D\u00fcrft' ich, wenn dich stolz die sch\u00f6nen Rosse ziehn,", "tokens": ["D\u00fcrft'", "ich", ",", "wenn", "dich", "stolz", "die", "sch\u00f6\u00b7nen", "Ros\u00b7se", "ziehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Gleichen deinem Wagenrade, folgen dir!", "tokens": ["Glei\u00b7chen", "dei\u00b7nem", "Wa\u00b7gen\u00b7ra\u00b7de", ",", "fol\u00b7gen", "dir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "D\u00fcrft' ich, wenn dich schnell die leichte Gondel tr\u00e4gt,", "tokens": ["D\u00fcrft'", "ich", ",", "wenn", "dich", "schnell", "die", "leich\u00b7te", "Gon\u00b7del", "tr\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Gleich dem Fisch im Wogenbade folgen dir!", "tokens": ["Gleich", "dem", "Fisch", "im", "Wo\u00b7gen\u00b7ba\u00b7de", "fol\u00b7gen", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Mit den Blicken folgt die Pappel dir am Weg,", "tokens": ["Mit", "den", "Bli\u00b7cken", "folgt", "die", "Pap\u00b7pel", "dir", "am", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.9": {"text": "Und die Tulpen am Gestade folgen dir!", "tokens": ["Und", "die", "Tul\u00b7pen", "am", "Ge\u00b7sta\u00b7de", "fol\u00b7gen", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}