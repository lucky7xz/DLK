{"textgrid.poem.55999": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da trat auf seiner Stirn der Aussatz aus", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da trat auf seiner Stirn der Aussatz aus", "tokens": ["Da", "trat", "auf", "sei\u00b7ner", "Stirn", "der", "Aus\u00b7satz", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und stand auf einmal unter seiner Krone", "tokens": ["und", "stand", "auf", "ein\u00b7mal", "un\u00b7ter", "sei\u00b7ner", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "als w\u00e4r er K\u00f6nig \u00fcber allen Graus,", "tokens": ["als", "w\u00e4r", "er", "K\u00f6\u00b7nig", "\u00fc\u00b7ber", "al\u00b7len", "Graus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "VAFIN", "PPER", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "der in die Andern fuhr, die fassungsohne", "tokens": ["der", "in", "die", "An\u00b7dern", "fuhr", ",", "die", "fas\u00b7sun\u00b7gsoh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "hinstarrten nach dem furchtbaren Vollzug", "tokens": ["hins\u00b7tarr\u00b7ten", "nach", "dem", "furcht\u00b7ba\u00b7ren", "Voll\u00b7zug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "an jenem, welcher, schmal wie ein Verschn\u00fcrter,", "tokens": ["an", "je\u00b7nem", ",", "wel\u00b7cher", ",", "schmal", "wie", "ein", "Ver\u00b7schn\u00fcr\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "$,", "PRELS", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "erwartete, da\u00df einer nach ihm schlug;", "tokens": ["er\u00b7war\u00b7te\u00b7te", ",", "da\u00df", "ei\u00b7ner", "nach", "ihm", "schlug", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "doch noch war keiner Manns genug:", "tokens": ["doch", "noch", "war", "kei\u00b7ner", "Manns", "ge\u00b7nug", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "als machte ihn nur immer unber\u00fchrter", "tokens": ["als", "mach\u00b7te", "ihn", "nur", "im\u00b7mer", "un\u00b7be\u00b7r\u00fchr\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "die neue W\u00fcrde, die sich \u00fcbertrug.", "tokens": ["die", "neu\u00b7e", "W\u00fcr\u00b7de", ",", "die", "sich", "\u00fc\u00b7bert\u00b7rug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}