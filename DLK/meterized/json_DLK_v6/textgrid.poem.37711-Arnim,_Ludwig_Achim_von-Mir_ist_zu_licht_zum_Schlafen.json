{"textgrid.poem.37711": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Mir ist zu licht zum Schlafen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir ist zu licht zum Schlafen,", "tokens": ["Mir", "ist", "zu", "licht", "zum", "Schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Tag bricht in die Nacht,", "tokens": ["Der", "Tag", "bricht", "in", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Seele ruht im Hafen,", "tokens": ["Die", "See\u00b7le", "ruht", "im", "Ha\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich bin so froh verwacht!", "tokens": ["Ich", "bin", "so", "froh", "ver\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich hauchte meine Seele", "tokens": ["Ich", "hauch\u00b7te", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im ersten Kusse aus,", "tokens": ["Im", "ers\u00b7ten", "Kus\u00b7se", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was ist's, da\u00df ich mich qu\u00e4le,", "tokens": ["Was", "ist's", ",", "da\u00df", "ich", "mich", "qu\u00e4\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ob sie auch fand ein Haus!", "tokens": ["Ob", "sie", "auch", "fand", "ein", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie hat es wohl gefunden,", "tokens": ["Sie", "hat", "es", "wohl", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf ihren Lippen sch\u00f6n,", "tokens": ["Auf", "ih\u00b7ren", "Lip\u00b7pen", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O welche sel'ge Stunden,", "tokens": ["O", "wel\u00b7che", "sel'\u00b7ge", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PWAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie ist mir so geschehn!", "tokens": ["Wie", "ist", "mir", "so", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was soll ich nun noch sehen,", "tokens": ["Was", "soll", "ich", "nun", "noch", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ach alles ist in ihr,", "tokens": ["Ach", "al\u00b7les", "ist", "in", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PIS", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was f\u00fchlen, was erflehen,", "tokens": ["Was", "f\u00fch\u00b7len", ",", "was", "er\u00b7fle\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es ward ja alles mir!", "tokens": ["Es", "ward", "ja", "al\u00b7les", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich habe was zu sinnen,", "tokens": ["Ich", "ha\u00b7be", "was", "zu", "sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich hab', was mich begl\u00fcckt;", "tokens": ["Ich", "hab'", ",", "was", "mich", "be\u00b7gl\u00fcckt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In allen meinen Sinnen", "tokens": ["In", "al\u00b7len", "mei\u00b7nen", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bin ich von ihr entz\u00fcckt.", "tokens": ["Bin", "ich", "von", "ihr", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}