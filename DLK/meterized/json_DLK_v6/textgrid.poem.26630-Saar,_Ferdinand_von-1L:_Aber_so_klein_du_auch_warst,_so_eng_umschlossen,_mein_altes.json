{"textgrid.poem.26630": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aber so klein du auch warst, so eng umschlossen, mein altes", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aber so klein du auch warst, so eng umschlossen, mein altes", "tokens": ["A\u00b7ber", "so", "klein", "du", "auch", "warst", ",", "so", "eng", "um\u00b7schlos\u00b7sen", ",", "mein", "al\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ADJD", "PPER", "ADV", "VAFIN", "$,", "ADV", "ADJD", "VVINF", "$,", "PPOSAT", "ADJA"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Trauliches Wien: es ging ", "tokens": ["Trau\u00b7li\u00b7ches", "Wi\u00b7en", ":", "es", "ging"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Alles, was heute verkl\u00e4rt aufragt in Erz und in Marmor,", "tokens": ["Al\u00b7les", ",", "was", "heu\u00b7te", "ver\u00b7kl\u00e4rt", "auf\u00b7ragt", "in", "Erz", "und", "in", "Mar\u00b7mor", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ADV", "VVPP", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Redend als Denkmal zum Volk, lebte und wirkte in dir.", "tokens": ["Re\u00b7dend", "als", "Denk\u00b7mal", "zum", "Volk", ",", "leb\u00b7te", "und", "wirk\u00b7te", "in", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ADV", "APPRART", "NN", "$,", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Bargen die sch\u00fctzenden W\u00e4lle, die alten, schlichten Pal\u00e4ste", "tokens": ["Bar\u00b7gen", "die", "sch\u00fct\u00b7zen\u00b7den", "W\u00e4l\u00b7le", ",", "die", "al\u00b7ten", ",", "schlich\u00b7ten", "Pa\u00b7l\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Denn nicht Oesterreichs Ruhm? Oesterreichs Liebe und Stolz?", "tokens": ["Denn", "nicht", "O\u00b7es\u00b7ter\u00b7reichs", "Ruhm", "?", "O\u00b7es\u00b7ter\u00b7reichs", "Lie\u00b7be", "und", "Stolz", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "NN", "NN", "$.", "NE", "NN", "KON", "NN", "$."], "meter": "--+-+-+----+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Fuhr ", "tokens": ["Fuhr"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Die ihr ", "tokens": ["Die", "ihr"], "token_info": ["word", "word"], "pos": ["ART", "PPOSAT"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Waren nicht heimisch in ihnen die Sieger von ", "tokens": ["Wa\u00b7ren", "nicht", "hei\u00b7misch", "in", "ih\u00b7nen", "die", "Sie\u00b7ger", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ADJD", "APPR", "PPER", "ART", "NN", "APPR"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.10": {"text": "Denen als dritter zuletzt der von ", "tokens": ["De\u00b7nen", "als", "drit\u00b7ter", "zu\u00b7letzt", "der", "von"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "KOUS", "ADJA", "ADV", "ART", "APPR"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Wie? Und schufen in \u00e4rmlichsten H\u00e4usern nicht", "tokens": ["Wie", "?", "Und", "schu\u00b7fen", "in", "\u00e4rm\u00b7lichs\u00b7ten", "H\u00e4u\u00b7sern", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "KON", "VVFIN", "APPR", "ADJA", "NN", "PTKNEG"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Nicht ", "tokens": ["Nicht"], "token_info": ["word"], "pos": ["PTKNEG"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Klangen im engeren Weichbild zuerst nicht die Lieder von ", "tokens": ["Klan\u00b7gen", "im", "en\u00b7ge\u00b7ren", "Weich\u00b7bild", "zu\u00b7erst", "nicht", "die", "Lie\u00b7der", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN", "ADV", "PTKNEG", "ART", "NN", "APPR"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.14": {"text": "Dessen beh\u00e4biger Sinn nie sich in's Weite verlangt?", "tokens": ["Des\u00b7sen", "be\u00b7h\u00e4\u00b7bi\u00b7ger", "Sinn", "nie", "sich", "in's", "Wei\u00b7te", "ver\u00b7langt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "ADV", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.15": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.16": {"text": "Nicht im Bann der Bastei, die er stets einsam betrat?", "tokens": ["Nicht", "im", "Bann", "der", "Bas\u00b7tei", ",", "die", "er", "stets", "ein\u00b7sam", "be\u00b7trat", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+---+", "measure": "unknown.measure.hexa"}, "line.17": {"text": "Blickte mit schalkischem Aug' nicht ", "tokens": ["Blick\u00b7te", "mit", "schal\u00b7ki\u00b7schem", "Aug'", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PTKNEG"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.18": {"text": "W\u00e4hrend in ", "tokens": ["W\u00e4h\u00b7rend", "in"], "token_info": ["word", "word"], "pos": ["KOUS", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.20": {"text": "Scheltet mir nimmer ", "tokens": ["Schel\u00b7tet", "mir", "nim\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.21": {"text": "War es ein Capua auch, war es doch keines des Geists.", "tokens": ["War", "es", "ein", "Ca\u00b7pua", "auch", ",", "war", "es", "doch", "kei\u00b7nes", "des", "Geists", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NE", "ADV", "$,", "VAFIN", "PPER", "ADV", "PIS", "ART", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}