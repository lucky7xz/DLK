{"textgrid.poem.49492": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Von sich selbst", "genre": "verse", "period": "N.A.", "pub_year": 1747, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von allen Sterblichen auf Erden", "tokens": ["Von", "al\u00b7len", "Sterb\u00b7li\u00b7chen", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bin ich gewi\u00df der \u00c4rmeste;", "tokens": ["Bin", "ich", "ge\u00b7wi\u00df", "der", "\u00c4r\u00b7mes\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Ein L\u00e4mmgen dient mir statt der Heerden,", "tokens": ["Ein", "L\u00e4mm\u00b7gen", "dient", "mir", "statt", "der", "Heer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An Felder statt ein Fleckgen Klee.", "tokens": ["An", "Fel\u00b7der", "statt", "ein", "Fleck\u00b7gen", "Klee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch wenn man auf Verdienste s\u00e4h,", "tokens": ["Doch", "wenn", "man", "auf", "Ver\u00b7diens\u00b7te", "s\u00e4h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So m\u00fcst ich mindstens K\u00f6nig werden,", "tokens": ["So", "m\u00fcst", "ich", "minds\u00b7tens", "K\u00f6\u00b7nig", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Denn ich bin der Verliebteste", "tokens": ["Denn", "ich", "bin", "der", "Ver\u00b7lieb\u00b7tes\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Von allen Sterblichen auf Erden.", "tokens": ["Von", "al\u00b7len", "Sterb\u00b7li\u00b7chen", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}