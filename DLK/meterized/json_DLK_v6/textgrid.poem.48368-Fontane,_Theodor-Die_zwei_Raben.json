{"textgrid.poem.48368": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Die zwei Raben", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging \u00fcbers Heidemoor allein,", "tokens": ["Ich", "ging", "\u00fc\u00b7bers", "Hei\u00b7de\u00b7moor", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da h\u00f6rt' ich zwei Raben kreischen und schrein;", "tokens": ["Da", "h\u00f6rt'", "ich", "zwei", "Ra\u00b7ben", "krei\u00b7schen", "und", "schrein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "CARD", "NN", "VVINF", "KON", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der eine rief dem andern zu:", "tokens": ["Der", "ei\u00b7ne", "rief", "dem", "an\u00b7dern", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbwo machen wir Mittag, ich und du?\u00ab", "tokens": ["\u00bb", "wo", "ma\u00b7chen", "wir", "Mit\u00b7tag", ",", "ich", "und", "du", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "NN", "$,", "PPER", "KON", "PPER", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbim Walde dr\u00fcben liegt unbewacht", "tokens": ["\u00bb", "im", "Wal\u00b7de", "dr\u00fc\u00b7ben", "liegt", "un\u00b7be\u00b7wacht"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "APPRART", "NN", "ADV", "VVFIN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein erschlagener Ritter seit heute Nacht,", "tokens": ["Ein", "er\u00b7schla\u00b7ge\u00b7ner", "Rit\u00b7ter", "seit", "heu\u00b7te", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADV", "NN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und niemand sah ihn in Waldesgrund", "tokens": ["Und", "nie\u00b7mand", "sah", "ihn", "in", "Wal\u00b7des\u00b7grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als sein Lieb und sein Falke und sein Hund.", "tokens": ["Als", "sein", "Lieb", "und", "sein", "Fal\u00b7ke", "und", "sein", "Hund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Sein Hund auf neuer F\u00e4hrte geht,", "tokens": ["Sein", "Hund", "auf", "neu\u00b7er", "F\u00e4hr\u00b7te", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Falk' auf frische Beute sp\u00e4ht,", "tokens": ["Sein", "Fal\u00b7k'", "auf", "fri\u00b7sche", "Beu\u00b7te", "sp\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sein Lieb ist mit ihrem Buhlen fort \u2013", "tokens": ["Sein", "Lieb", "ist", "mit", "ih\u00b7rem", "Buh\u00b7len", "fort", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wir k\u00f6nnen speisen in Ruhe dort.", "tokens": ["Wir", "k\u00f6n\u00b7nen", "spei\u00b7sen", "in", "Ru\u00b7he", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "APPR", "NN", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Du setzest auf seinen Nacken dich,", "tokens": ["Du", "set\u00b7zest", "auf", "sei\u00b7nen", "Na\u00b7cken", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Seine blauen Augen, die sind f\u00fcr mich,", "tokens": ["Sei\u00b7ne", "blau\u00b7en", "Au\u00b7gen", ",", "die", "sind", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "VAFIN", "APPR", "PPER", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Eine goldene Locke aus seinem Haar", "tokens": ["Ei\u00b7ne", "gol\u00b7de\u00b7ne", "Lo\u00b7cke", "aus", "sei\u00b7nem", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Soll w\u00e4rmen das Nest uns n\u00e4chstes Jahr.", "tokens": ["Soll", "w\u00e4r\u00b7men", "das", "Nest", "uns", "n\u00e4chs\u00b7tes", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "ART", "NN", "PPER", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Manch einer wird sprechen: ich hatt' ihn lieb!", "tokens": ["Manch", "ei\u00b7ner", "wird", "spre\u00b7chen", ":", "ich", "hatt'", "ihn", "lieb", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "VAFIN", "VVINF", "$.", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Doch keiner wird wissen, wo er blieb,", "tokens": ["Doch", "kei\u00b7ner", "wird", "wis\u00b7sen", ",", "wo", "er", "blieb", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und hingehn \u00fcber sein bleich Gebein", "tokens": ["Und", "hin\u00b7gehn", "\u00fc\u00b7ber", "sein", "bleich", "Ge\u00b7bein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJD", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wird Wind und Regen und Sonnenschein.\u00ab", "tokens": ["Wird", "Wind", "und", "Re\u00b7gen", "und", "Son\u00b7nen\u00b7schein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}