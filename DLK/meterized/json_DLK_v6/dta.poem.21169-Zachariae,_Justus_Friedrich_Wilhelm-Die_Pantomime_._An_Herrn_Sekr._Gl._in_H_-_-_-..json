{"dta.poem.21169": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Pantomime .  \n An Herrn Sekr. Gl. in H - - -.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von tausend Seufzern best\u00fcrmt, bewegt sich pr\u00e4chtig\nund ernsthaft", "tokens": ["Von", "tau\u00b7send", "Seuf\u00b7zern", "be\u00b7st\u00fcrmt", ",", "be\u00b7wegt", "sich", "pr\u00e4ch\u00b7tig", "und", "ernst\u00b7haft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VVPP", "$,", "VVFIN", "PRF", "ADJD", "KON", "ADJD"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der majest\u00e4tische Vorhang vor uns.", "tokens": ["Der", "ma\u00b7jes\u00b7t\u00e4\u00b7ti\u00b7sche", "Vor\u00b7hang", "vor", "uns", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auf einmal rauscht er empor! Schon lag vor warten-", "tokens": ["Auf", "ein\u00b7mal", "rauscht", "er", "em\u00b7por", "!", "Schon", "lag", "vor", "war\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VVFIN", "APPR", "TRUNC"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die schimmernde Pantomimenwelt da.", "tokens": ["Die", "schim\u00b7mern\u00b7de", "Pan\u00b7to\u00b7mi\u00b7men\u00b7welt", "da", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Schon borsten Felsen entzwey! schon brannt\u2019 im inner-", "tokens": ["Schon", "bors\u00b7ten", "Fel\u00b7sen", "ent\u00b7zwey", "!", "schon", "brannt'", "im", "in\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "APPRART", "TRUNC"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Gluth der H\u00f6lle, gemalt auf Papier;", "tokens": ["Die", "Gluth", "der", "H\u00f6l\u00b7le", ",", "ge\u00b7malt", "auf", "Pa\u00b7pier", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "VVPP", "APPR", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da str\u00f6mten Wasser dahin; da tanzten scheckigte Teufel", "tokens": ["Da", "str\u00f6m\u00b7ten", "Was\u00b7ser", "da\u00b7hin", ";", "da", "tanz\u00b7ten", "sche\u00b7ckig\u00b7te", "Teu\u00b7fel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "PAV", "$.", "ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+--+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Vor ihrem K\u00f6nig im rothen Gewand.", "tokens": ["Vor", "ih\u00b7rem", "K\u00f6\u00b7nig", "im", "ro\u00b7then", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch alles wartete noch, es pochten die seufzenden Her-", "tokens": ["Doch", "al\u00b7les", "war\u00b7te\u00b7te", "noch", ",", "es", "poch\u00b7ten", "die", "seuf\u00b7zen\u00b7den", "Her"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ART", "ADJA", "TRUNC"], "meter": "-+-+--+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da trat sie, die Zauberin, siegend hervor,", "tokens": ["Da", "trat", "sie", ",", "die", "Zau\u00b7be\u00b7rin", ",", "sie\u00b7gend", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und schnell lief Jauchzen und Lust durch alle frohen", "tokens": ["Und", "schnell", "lief", "Jauch\u00b7zen", "und", "Lust", "durch", "al\u00b7le", "fro\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "NN", "KON", "NN", "APPR", "PIAT", "ADJA"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ah \u2014 sagte J\u00fcngling und Alter zugleich.", "tokens": ["Ah", "sag\u00b7te", "J\u00fcng\u00b7ling", "und", "Al\u00b7ter", "zu\u00b7gleich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "NN", "KON", "NN", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Sie gieng mit siegendem Stolz, so wie die G\u00f6ttin der", "tokens": ["Sie", "gieng", "mit", "sie\u00b7gen\u00b7dem", "Stolz", ",", "so", "wie", "die", "G\u00f6t\u00b7tin", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "ADV", "KOKOM", "ART", "NN", "ART"], "meter": "-+-+--+-+-+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von Amouretten begleitet, daher;", "tokens": ["Von", "A\u00b7mou\u00b7ret\u00b7ten", "be\u00b7glei\u00b7tet", ",", "da\u00b7her", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$,", "PAV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Jhr wei\u00dfes wallendes Haar flo\u00df auf dem blendenden", "tokens": ["Ihr", "wei\u00b7\u00dfes", "wal\u00b7len\u00b7des", "Haar", "flo\u00df", "auf", "dem", "blen\u00b7den\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und jedes Herz ward durch sie bestrickt.", "tokens": ["Und", "je\u00b7des", "Herz", "ward", "durch", "sie", "be\u00b7strickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Von hohem Mitleid entbrannt, sprach ihr gef\u00e4lliges", "tokens": ["Von", "ho\u00b7hem", "Mit\u00b7leid", "ent\u00b7brannt", ",", "sprach", "ihr", "ge\u00b7f\u00e4l\u00b7li\u00b7ges"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "VVFIN", "PPOSAT", "ADJA"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Trost in des armen Harlekins Herz.", "tokens": ["Trost", "in", "des", "ar\u00b7men", "Har\u00b7le\u00b7kins", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Getr\u00f6stet, kniet er vor sie; und k\u00fc\u00dft ihr die Hand mit", "tokens": ["Ge\u00b7tr\u00f6s\u00b7tet", ",", "kniet", "er", "vor", "sie", ";", "und", "k\u00fc\u00dft", "ihr", "die", "Hand", "mit"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "VVFIN", "PPER", "APPR", "PPER", "$.", "KON", "VVFIN", "PPER", "ART", "NN", "APPR"], "meter": "-+-+-+--+-++-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und in Gedanken k\u00fc\u00dft jeder mit ihm.", "tokens": ["Und", "in", "Ge\u00b7dan\u00b7ken", "k\u00fc\u00dft", "je\u00b7der", "mit", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PIS", "APPR", "PPER", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Auf einmal sah ich erstaunt, an ihre Seite gelehnet,", "tokens": ["Auf", "ein\u00b7mal", "sah", "ich", "er\u00b7staunt", ",", "an", "ih\u00b7re", "Sei\u00b7te", "ge\u00b7leh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "VVPP", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Den Gott der Liebe, mit Bogen und Pfeil.", "tokens": ["Den", "Gott", "der", "Lie\u00b7be", ",", "mit", "Bo\u00b7gen", "und", "Pfeil", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und bey ihm lag noch gespitzt ein ganzer Haufe von", "tokens": ["Und", "bey", "ihm", "lag", "noch", "ge\u00b7spitzt", "ein", "gan\u00b7zer", "Hau\u00b7fe", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ADV", "VVPP", "ART", "ADJA", "NN", "APPR"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die er mit m\u00f6rdrischen Augen besah.", "tokens": ["Die", "er", "mit", "m\u00f6rd\u00b7ri\u00b7schen", "Au\u00b7gen", "be\u00b7sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie grausam scho\u00df er umher! Es flog vom bunten", "tokens": ["Wie", "grau\u00b7sam", "scho\u00df", "er", "um\u00b7her", "!", "Es", "flog", "vom", "bun\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "APPRART", "ADJA"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gewi\u00df des Sieges, der sausende Pfeil.", "tokens": ["Ge\u00b7wi\u00df", "des", "Sie\u00b7ges", ",", "der", "sau\u00b7sen\u00b7de", "Pfeil", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein jeder grif sich ans Herz, und fand sein Herz schon", "tokens": ["Ein", "je\u00b7der", "grif", "sich", "ans", "Herz", ",", "und", "fand", "sein", "Herz", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "PRF", "APPRART", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und zog den t\u00f6dlichen Pfeil aus der Brust.", "tokens": ["Und", "zog", "den", "t\u00f6d\u00b7li\u00b7chen", "Pfeil", "aus", "der", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "So wie Ulysses ehmals den starken Bogen gespannet,", "tokens": ["So", "wie", "U\u00b7lys\u00b7ses", "eh\u00b7mals", "den", "star\u00b7ken", "Bo\u00b7gen", "ge\u00b7span\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NE", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Und siegend Freyer auf Freyer geh\u00e4uft;", "tokens": ["Und", "sie\u00b7gend", "Frey\u00b7er", "auf", "Frey\u00b7er", "ge\u00b7h\u00e4uft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So siegt des Liebesgotts Pfeil. Es fielen Freyherrn auf", "tokens": ["So", "siegt", "des", "Lie\u00b7bes\u00b7gotts", "Pfeil", ".", "Es", "fie\u00b7len", "Frey\u00b7herrn", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "NN", "APPR"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und Gnaden auf Excellenzen dahin.", "tokens": ["Und", "Gna\u00b7den", "auf", "Ex\u00b7cel\u00b7len\u00b7zen", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "O G \u2014 wie gieng es dir da! Ich sah dein Antlitz ver-", "tokens": ["O", "G", "wie", "gieng", "es", "dir", "da", "!", "Ich", "sah", "dein", "Ant\u00b7litz", "ver"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$(", "PWAV", "VVFIN", "PPER", "PPER", "ADV", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "TRUNC"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da dich der Pfeil des Kupido verletzt.", "tokens": ["Da", "dich", "der", "Pfeil", "des", "Ku\u00b7pi\u00b7do", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Freund! rief ich; \u2014 aber schon war mein warnender", "tokens": ["Freund", "!", "rief", "ich", ";", "a\u00b7ber", "schon", "war", "mein", "war\u00b7nen\u00b7der"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "PPER", "$.", "$(", "ADV", "ADV", "VAFIN", "PPOSAT", "ADJA"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Dich zog die stolze Siegerin fort.", "tokens": ["Dich", "zog", "die", "stol\u00b7ze", "Sie\u00b7ge\u00b7rin", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.10": {"line.1": {"text": "Ach! da\u00df die Liebe gesiegt! da\u00df unser G \u2014 so gefallen,", "tokens": ["Ach", "!", "da\u00df", "die", "Lie\u00b7be", "ge\u00b7siegt", "!", "da\u00df", "un\u00b7ser", "G", "so", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "ART", "NN", "VVPP", "$.", "KOUS", "PPOSAT", "NE", "$(", "ADV", "VVPP", "$,"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Der Held, der gl\u00fccklich die Liebe geflohn!", "tokens": ["Der", "Held", ",", "der", "gl\u00fcck\u00b7lich", "die", "Lie\u00b7be", "ge\u00b7flohn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nun tr\u00e4gt er Ketten, und seufzt, und schm\u00fcckt der Sie-", "tokens": ["Nun", "tr\u00e4gt", "er", "Ket\u00b7ten", ",", "und", "seufzt", ",", "und", "schm\u00fcckt", "der", "Sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "ART", "TRUNC"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und singet traurige Lieder ihr nach.", "tokens": ["Und", "sin\u00b7get", "trau\u00b7ri\u00b7ge", "Lie\u00b7der", "ihr", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}