{"textgrid.poem.38913": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie \u00fcber Matten", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie \u00fcber Matten", "tokens": ["Wie", "\u00fc\u00b7ber", "Mat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die Wolke zieht,", "tokens": ["Die", "Wol\u00b7ke", "zieht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So auch der Schatten", "tokens": ["So", "auch", "der", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Vom Leben flieht.", "tokens": ["Vom", "Le\u00b7ben", "flieht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Die Jahre eilen,", "tokens": ["Die", "Jah\u00b7re", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Kein Stillestand,", "tokens": ["Kein", "Stil\u00b7les\u00b7tand", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und kein Verweilen,", "tokens": ["Und", "kein", "Ver\u00b7wei\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sie h\u00e4lt kein Band.", "tokens": ["Sie", "h\u00e4lt", "kein", "Band", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Nur Freude kettet", "tokens": ["Nur", "Freu\u00b7de", "ket\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["ADV", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Das Leben hier,", "tokens": ["Das", "Le\u00b7ben", "hier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Frohe rettet", "tokens": ["Der", "Fro\u00b7he", "ret\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die Zeiten schier.", "tokens": ["Die", "Zei\u00b7ten", "schier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ihm sind die Stunden", "tokens": ["Ihm", "sind", "die", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Was Jahre sind,", "tokens": ["Was", "Jah\u00b7re", "sind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sind nicht verschwunden", "tokens": ["Sind", "nicht", "ver\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wer so gesinnt.", "tokens": ["Wer", "so", "ge\u00b7sinnt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Ihm sind die K\u00fcsse", "tokens": ["Ihm", "sind", "die", "K\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Der goldne Wein", "tokens": ["Der", "gold\u00b7ne", "Wein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Noch 'mal so s\u00fc\u00dfe", "tokens": ["Noch", "'mal", "so", "s\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Im Sonnenschein.", "tokens": ["Im", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Ihm naht kein Schatten", "tokens": ["Ihm", "naht", "kein", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Verg\u00e4nglichkeit,", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "F\u00fcr ihn begatten", "tokens": ["F\u00fcr", "ihn", "be\u00b7gat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sich Freud' und Zeit.", "tokens": ["Sich", "Freud'", "und", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Drum nimm die Freude", "tokens": ["Drum", "nimm", "die", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Und sperr' sie ein,", "tokens": ["Und", "sperr'", "sie", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dann m\u00fc\u00dft ihr beide", "tokens": ["Dann", "m\u00fc\u00dft", "ihr", "bei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PIS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Unsterblich sein.", "tokens": ["U\u00b7nsterb\u00b7lich", "sein", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}