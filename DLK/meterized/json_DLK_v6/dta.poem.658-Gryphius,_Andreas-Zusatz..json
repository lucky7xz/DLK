{"dta.poem.658": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Zusatz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Wir/ die alles vns zu wissen/ ", "tokens": ["Wir", "/", "die", "al\u00b7les", "vns", "zu", "wis\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "ART", "PIS", "PPER", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der ersten zeit/ beflissen: ", "tokens": ["Von", "der", "ers\u00b7ten", "zeit", "/", "be\u00b7flis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnen gleichwol nicht ergr\u00fcnden: ", "tokens": ["K\u00f6n\u00b7nen", "gleich\u00b7wol", "nicht", "er\u00b7gr\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W", "tokens": ["W"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "D", "tokens": ["D"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "K\u00f6nnen kaum/ ja nicht/ entweichen ", "tokens": ["K\u00f6n\u00b7nen", "kaum", "/", "ja", "nicht", "/", "ent\u00b7wei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VMFIN", "ADV", "$(", "ADV", "PTKNEG", "$(", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Auch viel/ in dem sie sich den tod bem\u00fcht zufliehen ", "tok