{"dta.poem.5545": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Weltweisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie! k\u00f6nnen wir ohn Unterla\u00df empfinden,", "tokens": ["Wie", "!", "k\u00f6n\u00b7nen", "wir", "ohn", "Un\u00b7ter\u00b7la\u00df", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VMFIN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df so viel Vorw\u00fcrff\u2019 sich mit unsern Sinnen binden;", "tokens": ["Da\u00df", "so", "viel", "Vor\u00b7w\u00fcrff'", "sich", "mit", "un\u00b7sern", "Sin\u00b7nen", "bin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ohn\u2019 da\u00df man sich einst zu Gem\u00fcthe f\u00fchrt,", "tokens": ["Ohn'", "da\u00df", "man", "sich", "einst", "zu", "Ge\u00b7m\u00fc\u00b7the", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PIS", "PRF", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Auf welche Weise sie formirt,", "tokens": ["Auf", "wel\u00b7che", "Wei\u00b7se", "sie", "for\u00b7mirt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was eigentlich in uns das sey, so f\u00fchlt und dencket?", "tokens": ["Was", "ei\u00b7gent\u00b7lich", "in", "uns", "das", "sey", ",", "so", "f\u00fchlt", "und", "den\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "PPER", "PDS", "VAFIN", "$,", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was die Bewegung sey, die uns beseelt und lencket?", "tokens": ["Was", "die", "Be\u00b7we\u00b7gung", "sey", ",", "die", "uns", "be\u00b7seelt", "und", "len\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "$,", "PRELS", "PPER", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}