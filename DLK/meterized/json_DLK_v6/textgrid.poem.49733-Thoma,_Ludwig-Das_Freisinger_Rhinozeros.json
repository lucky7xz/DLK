{"textgrid.poem.49733": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Das Freisinger Rhinozeros", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Freising fand man dieser Tage", "tokens": ["In", "Frei\u00b7sing", "fand", "man", "die\u00b7ser", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PIS", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Vorsintflutrhinozeros.", "tokens": ["Ein", "Vor\u00b7sint\u00b7flutr\u00b7hi\u00b7no\u00b7ze\u00b7ros", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im stillen ringt sich manche Frage", "tokens": ["Im", "stil\u00b7len", "ringt", "sich", "man\u00b7che", "Fra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VVFIN", "PRF", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus zweifelvoller Seele los.", "tokens": ["Aus", "zwei\u00b7fel\u00b7vol\u00b7ler", "See\u00b7le", "los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es ist ja sch\u00f6n, da\u00df man 's gefunden;", "tokens": ["Es", "ist", "ja", "sch\u00f6n", ",", "da\u00df", "man", "'s", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Jedoch \u2013 warum gerade hier,", "tokens": ["Je\u00b7doch", "\u2013", "wa\u00b7rum", "ge\u00b7ra\u00b7de", "hier", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PWAV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In diesem Ort der Andachtsstunden?", "tokens": ["In", "die\u00b7sem", "Ort", "der", "An\u00b7dachts\u00b7stun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was wollte nur das gute Tier?", "tokens": ["Was", "woll\u00b7te", "nur", "das", "gu\u00b7te", "Tier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was tat der alte Knochenriese", "tokens": ["Was", "tat", "der", "al\u00b7te", "Kno\u00b7chen\u00b7rie\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In unsrer fr\u00f6mmsten Zentrumsstadt?", "tokens": ["In", "uns\u00b7rer", "fr\u00f6mms\u00b7ten", "Zent\u00b7rums\u00b7stadt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wu\u00dft' er im Voraus schon, da\u00df diese", "tokens": ["Wu\u00dft'", "er", "im", "Vo\u00b7raus", "schon", ",", "da\u00df", "die\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "$,", "KOUS", "PDAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch heute seinesgleichen hat?", "tokens": ["Noch", "heu\u00b7te", "sei\u00b7nes\u00b7glei\u00b7chen", "hat", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und konserviert' er sich im Eise", "tokens": ["Und", "kon\u00b7ser\u00b7viert'", "er", "sich", "im", "Ei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im vorgef\u00fchlten Heimatsdrang", "tokens": ["Im", "vor\u00b7ge\u00b7f\u00fchl\u00b7ten", "Hei\u00b7mats\u00b7drang"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu Herrn von Dallers Wirkungskreise?", "tokens": ["Zu", "Herrn", "von", "Dal\u00b7lers", "Wir\u00b7kungs\u00b7krei\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "War's freier Wille oder Zwang?", "tokens": ["Wa\u00b7r's", "frei\u00b7er", "Wil\u00b7le", "o\u00b7der", "Zwang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Ist typisch dieser Fund zu nennen?", "tokens": ["Ist", "ty\u00b7pisch", "die\u00b7ser", "Fund", "zu", "nen\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist er Charakteristikum?", "tokens": ["Ist", "er", "Cha\u00b7rak\u00b7te\u00b7ris\u00b7ti\u00b7kum", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Man mu\u00df nur Freising n\u00e4her kennen,", "tokens": ["Man", "mu\u00df", "nur", "Frei\u00b7sing", "n\u00e4\u00b7her", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann sagt man \u00bbja\u00ab und wei\u00df, warum.", "tokens": ["Dann", "sagt", "man", "\u00bb", "ja", "\u00ab", "und", "wei\u00df", ",", "wa\u00b7rum", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$(", "PTKANT", "$(", "KON", "VVFIN", "$,", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}