{"textgrid.poem.59087": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "306. Mache Platz dem Bessern!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du ", "tokens": ["Willst", "du"], "token_info": ["word", "word"], "pos": ["VMFIN", "PPER"], "meter": "--", "measure": "unknown.measure.zero"}, "line.2": {"text": "Dann la\u00df dein Eignes Gotte ruhn", "tokens": ["Dann", "la\u00df", "dein", "Eig\u00b7nes", "Got\u00b7te", "ruhn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tief abgesp\u00e4nt und ganz ergeben,", "tokens": ["Tief", "ab\u00b7ge\u00b7sp\u00e4nt", "und", "ganz", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Jesus sei dein Tun und Leben!", "tokens": ["Da\u00df", "Je\u00b7sus", "sei", "dein", "Tun", "und", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}