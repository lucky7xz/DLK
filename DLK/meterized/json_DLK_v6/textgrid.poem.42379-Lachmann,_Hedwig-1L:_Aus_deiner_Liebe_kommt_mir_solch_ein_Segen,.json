{"textgrid.poem.42379": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus deiner Liebe kommt mir solch ein Segen,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus deiner Liebe kommt mir solch ein Segen,", "tokens": ["Aus", "dei\u00b7ner", "Lie\u00b7be", "kommt", "mir", "solch", "ein", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie macht mein Herz so sorglos und so fest,", "tokens": ["Sie", "macht", "mein", "Herz", "so", "sorg\u00b7los", "und", "so", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich kann so ruhig mich drin niederlegen,", "tokens": ["Ich", "kann", "so", "ru\u00b7hig", "mich", "drin", "nie\u00b7der\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie sich ein Kind dem Schlafe \u00fcberl\u00e4sst.", "tokens": ["Wie", "sich", "ein", "Kind", "dem", "Schla\u00b7fe", "\u00fc\u00b7berl\u00b7\u00e4sst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich geh dahin von Zuversicht getragen,", "tokens": ["Ich", "geh", "da\u00b7hin", "von", "Zu\u00b7ver\u00b7sicht", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seit neben deiner meine Seele schweift;", "tokens": ["Seit", "ne\u00b7ben", "dei\u00b7ner", "mei\u00b7ne", "See\u00b7le", "schweift", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "So, wie man wohl an sch\u00f6nen Sommertagen", "tokens": ["So", ",", "wie", "man", "wohl", "an", "sch\u00f6\u00b7nen", "Som\u00b7mer\u00b7ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PIS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch reife \u00c4hrenfelder sinnend streift.", "tokens": ["Durch", "rei\u00b7fe", "\u00c4h\u00b7ren\u00b7fel\u00b7der", "sin\u00b7nend", "streift", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da gleiten sanft die Finger \u00fcber Bl\u00fcten", "tokens": ["Da", "glei\u00b7ten", "sanft", "die", "Fin\u00b7ger", "\u00fc\u00b7ber", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Halme hin, wie eine Mutter pflegt,", "tokens": ["Und", "Hal\u00b7me", "hin", ",", "wie", "ei\u00b7ne", "Mut\u00b7ter", "pflegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKVZ", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und alles Leben m\u00f6chte man beh\u00fcten,", "tokens": ["Und", "al\u00b7les", "Le\u00b7ben", "m\u00f6ch\u00b7te", "man", "be\u00b7h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das seine heil'ge Saat zum Lichte tr\u00e4gt.", "tokens": ["Das", "sei\u00b7ne", "heil'\u00b7ge", "Saat", "zum", "Lich\u00b7te", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}