{"textgrid.poem.50791": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn einsam ", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn einsam ", "tokens": ["Wenn", "ein\u00b7sam"], "token_info": ["word", "word"], "pos": ["KOUS", "ADJD"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Nachdenklich manchmal ihre Augen senkt,", "tokens": ["Nach\u00b7denk\u00b7lich", "manch\u00b7mal", "ih\u00b7re", "Au\u00b7gen", "senkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O w\u00fc\u00dfte sie dann, da\u00df ein Herz es gibt,", "tokens": ["O", "w\u00fc\u00df\u00b7te", "sie", "dann", ",", "da\u00df", "ein", "Herz", "es", "gibt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das hier im Grab lebendig an sie denkt!", "tokens": ["Das", "hier", "im", "Grab", "le\u00b7ben\u00b7dig", "an", "sie", "denkt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPRART", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}