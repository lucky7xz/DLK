{"dta.poem.3587": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Erdtoffeln mit Rippenst\u00fcckchen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Einsmals ein M\u00e4gdlein frisch und jung,               ", "tokens": ["Eins\u00b7mals", "ein", "M\u00e4gd\u00b7lein", "frisch", "und", "jung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Gieng aufrecht wie ein Hirsch im Sprung;", "tokens": ["Gieng", "auf\u00b7recht", "wie", "ein", "Hirsch", "im", "Sprung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KOKOM", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und von einem J\u00fcngling, den sie kannt,", "tokens": ["Und", "von", "ei\u00b7nem", "J\u00fcng\u00b7ling", ",", "den", "sie", "kannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ihre Aeuglein klar durchaus nicht wandt.", "tokens": ["Ih\u00b7re", "A\u00b7e\u00b7u\u00b7glein", "klar", "durc\u00b7haus", "nicht", "wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Der J\u00fcngling schalt und sprach zu ihr,", "tokens": ["Der", "J\u00fcng\u00b7ling", "schalt", "und", "sprach", "zu", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ihr mit nichten dies geb\u00fchr,", "tokens": ["Wie", "ihr", "mit", "nich\u00b7ten", "dies", "ge\u00b7b\u00fchr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PIS", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sondern sie sollt ganz z\u00fcchtiglich", "tokens": ["Son\u00b7dern", "sie", "sollt", "ganz", "z\u00fcch\u00b7tig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die Aeuglein schlagen unter sich.", "tokens": ["Die", "A\u00b7e\u00b7u\u00b7glein", "schla\u00b7gen", "un\u00b7ter", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie sprach gar bald: Mit nichten das,", "tokens": ["Sie", "sprach", "gar", "bald", ":", "Mit", "nich\u00b7ten", "das", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$.", "APPR", "PIS", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dies Anschaun ich nit unterla\u00df;", "tokens": ["Dies", "An\u00b7schaun", "ich", "nit", "un\u00b7ter\u00b7la\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur Erd zu schauen dir geb\u00fchrt,", "tokens": ["Zur", "Erd", "zu", "schau\u00b7en", "dir", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil aus der Erd dein Ursprung r\u00fchrt.", "tokens": ["Weil", "aus", "der", "Erd", "dein", "Ur\u00b7sprung", "r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Des Mannes Ripp mein Ursprung ist,", "tokens": ["Des", "Man\u00b7nes", "Ripp", "mein", "Ur\u00b7sprung", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die such ich auch ohn Falsch und List;", "tokens": ["Die", "such", "ich", "auch", "ohn", "Falsch", "und", "List", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und da\u00df solch Ripp in Zucht und Ehr", "tokens": ["Und", "da\u00df", "solch", "Ripp", "in", "Zucht", "und", "Ehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit mir vereint werd ich begehr.", "tokens": ["Mit", "mir", "ver\u00b7eint", "werd", "ich", "be\u00b7gehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}