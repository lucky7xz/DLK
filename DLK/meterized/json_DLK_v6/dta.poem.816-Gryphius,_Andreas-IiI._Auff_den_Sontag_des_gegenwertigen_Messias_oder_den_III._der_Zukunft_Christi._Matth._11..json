{"dta.poem.816": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  Auff den Sontag des gegenwertigen  \n Messias/ oder den  III.  der Zukunft Christi.  \n Matth. 11.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Das L\u00f6segeld der welt/ der V\u00e4ter langes hoffen ", "tokens": ["Das", "L\u00f6\u00b7se\u00b7geld", "der", "welt", "/", "der", "V\u00e4\u00b7ter", "lan\u00b7ges", "hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$(", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kom\u2019t noch den augenblick/ vnd schleu\u00dft die Ohren auff", "tokens": ["Kom't", "noch", "den", "au\u00b7gen\u00b7blick", "/", "vnd", "schleu\u00dft", "die", "Oh\u00b7ren", "auff"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "$(", "KON", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Taubheit hat verstopfft/ der nicht-mehr-stummen hauff", "tokens": ["Die", "Taub\u00b7heit", "hat", "ver\u00b7stopfft", "/", "der", "nicht\u00b7mehr\u00b7stum\u00b7men", "hauff"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erzehlet seine werck\u2019/ jhm stehn die Gr\u00e4ber offen.", "tokens": ["Er\u00b7zeh\u00b7let", "sei\u00b7ne", "werck", "/", "jhm", "stehn", "die", "Gr\u00e4\u00b7ber", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer blind war siht vnd find\u2019t/ wie eben eingetroffen", "tokens": ["Wer", "blind", "war", "siht", "vnd", "find't", "/", "wie", "e\u00b7ben", "ein\u00b7ge\u00b7trof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADJD", "VAFIN", "VVFIN", "KON", "VVFIN", "$(", "KOKOM", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was jemal Gott versprach. Er schawt der Lamen lauff/", "tokens": ["Was", "je\u00b7mal", "Gott", "ver\u00b7sprach", ".", "Er", "schawt", "der", "La\u00b7men", "lauff", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Au\u00dfsatz mu\u00df vergehn/ Hier wird ohn tewren kauff", "tokens": ["Der", "Au\u00df\u00b7satz", "mu\u00df", "ver\u00b7gehn", "/", "Hier", "wird", "ohn", "tew\u00b7ren", "kauff"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$(", "ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Trost geschenckt/ die vor in thr\u00e4nen schier ersoffen.", "tokens": ["Den", "Trost", "ge\u00b7schenckt", "/", "die", "vor", "in", "thr\u00e4\u00b7nen", "schier", "er\u00b7sof\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "ART", "APPR", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O seelig den von hier kein \u00e4rgernis abdringt/", "tokens": ["O", "see\u00b7lig", "den", "von", "hier", "kein", "\u00e4r\u00b7ger\u00b7nis", "ab\u00b7dringt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ART", "APPR", "ADV", "PIAT", "ADJD", "VVFIN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Den keiner wollust Wind gleich leichtem Schilffe zwingt", "tokens": ["Den", "kei\u00b7ner", "wol\u00b7lust", "Wind", "gleich", "leich\u00b7tem", "Schilf\u00b7fe", "zwingt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VMFIN", "NN", "ADV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den kein Tyrannen trutz/ kein Schwerd in Feindes h\u00e4nden/", "tokens": ["Den", "kein", "Ty\u00b7ran\u00b7nen", "trutz", "/", "kein", "Schwerd", "in", "Fein\u00b7des", "h\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJD", "$(", "PIAT", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Kein K\u00e4rcker! keine Schmach! kein weiches Purpurkleid/", "tokens": ["Kein", "K\u00e4r\u00b7cker", "!", "kei\u00b7ne", "Schmach", "!", "kein", "wei\u00b7ches", "Pur\u00b7pur\u00b7kleid", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "PIAT", "NN", "$.", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch keiner H\u00f6fe pracht/ kein gutt/ noch grimmes leyd/", "tokens": ["Auch", "kei\u00b7ner", "H\u00f6\u00b7fe", "pracht", "/", "kein", "gutt", "/", "noch", "grim\u00b7mes", "leyd", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$(", "PIAT", "ADJD", "$(", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein Reichthumb/ kein Geschenck/ kein Armut ab-mag wen-", "tokens": ["Kein", "Reicht\u00b7humb", "/", "kein", "Ge\u00b7schenck", "/", "kein", "Ar\u00b7mut", "ab\u00b7mag", "wen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$(", "PIAT", "NN", "$(", "PIAT", "NN", "NE", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "den. ", "tokens": ["den", "."], "token_info": ["word", "punct"], "pos": ["ART", "$."], "meter": "-", "measure": "single.down"}}}}}