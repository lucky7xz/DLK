{"textgrid.poem.36119": {"metadata": {"author": {"name": "Gloger, Georg", "birth": "N.A.", "death": "N.A."}, "title": "16. Also \u00fcberwinden und \u00fcberwunden werden ist l\u00f6blich", "genre": "verse", "period": "N.A.", "pub_year": 1617, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hat der K\u00f6nig kaum ein gr\u00f6sser Lob erworben,", "tokens": ["Es", "hat", "der", "K\u00f6\u00b7nig", "kaum", "ein", "gr\u00f6s\u00b7ser", "Lob", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als wenn er ", "tokens": ["Als", "wenn", "er"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Vnd ist auch ", "tokens": ["Vnd", "ist", "auch"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Als, da er, K\u00f6nig, dir mu\u00dft r\u00e4umen Mann vnd Feld.", "tokens": ["Als", ",", "da", "er", ",", "K\u00f6\u00b7nig", ",", "dir", "mu\u00dft", "r\u00e4u\u00b7men", "Mann", "vnd", "Feld", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "$,", "NN", "$,", "PPER", "VMFIN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn K\u00f6nigen allein ist r\u00fchmlich vnterliegen,", "tokens": ["Denn", "K\u00f6\u00b7ni\u00b7gen", "al\u00b7lein", "ist", "r\u00fchm\u00b7lich", "vn\u00b7ter\u00b7lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd k\u00f6niglich, wenn man den Niegeschlagnen schl\u00e4gt.", "tokens": ["Vnd", "k\u00f6\u00b7nig\u00b7lich", ",", "wenn", "man", "den", "Nie\u00b7ge\u00b7schlag\u00b7nen", "schl\u00e4gt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So jeder seinen Preis vnd Lob darvon noch tr\u00e4gt:", "tokens": ["So", "je\u00b7der", "sei\u00b7nen", "Preis", "vnd", "Lob", "dar\u00b7von", "noch", "tr\u00e4gt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "PPOSAT", "NN", "KON", "NN", "PAV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den r\u00fchmbt die grosse Schlacht, vnd den das grosse Siegen.", "tokens": ["Den", "r\u00fchmbt", "die", "gros\u00b7se", "Schlacht", ",", "vnd", "den", "das", "gros\u00b7se", "Sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "ART", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}