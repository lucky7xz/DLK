{"textgrid.poem.63040": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein falber Zeisig, dessen Ahnen", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein falber Zeisig, dessen Ahnen", "tokens": ["Ein", "fal\u00b7ber", "Zei\u00b7sig", ",", "des\u00b7sen", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Piko d\u00fcrrer Fu\u00df gebahr", "tokens": ["Des", "Pi\u00b7ko", "d\u00fcr\u00b7rer", "Fu\u00df", "ge\u00b7bahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und der im Lande der Germanen", "tokens": ["Und", "der", "im", "Lan\u00b7de", "der", "Ger\u00b7ma\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der gr\u00f6ste Virtuose war,", "tokens": ["Der", "gr\u00f6s\u00b7te", "Vir\u00b7tu\u00b7o\u00b7se", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Fand Mittel sich von seinen Ketten", "tokens": ["Fand", "Mit\u00b7tel", "sich", "von", "sei\u00b7nen", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In einen gr\u00fcnen Hayn zu retten.", "tokens": ["In", "ei\u00b7nen", "gr\u00fc\u00b7nen", "Hayn", "zu", "ret\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Hier sang er der vereinten Schaar", "tokens": ["Hier", "sang", "er", "der", "ver\u00b7ein\u00b7ten", "Schaar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der V\u00f6gel seine Minnelieder.", "tokens": ["Der", "V\u00f6\u00b7gel", "sei\u00b7ne", "Min\u00b7ne\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ihr Zauber war ihr unbekannt;", "tokens": ["Ihr", "Zau\u00b7ber", "war", "ihr", "un\u00b7be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Allein kaum h\u00f6rten ihn die Br\u00fcder,", "tokens": ["Al\u00b7lein", "kaum", "h\u00f6r\u00b7ten", "ihn", "die", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "So wird der seltne Musikant", "tokens": ["So", "wird", "der", "selt\u00b7ne", "Mu\u00b7si\u00b7kant"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Bis an das Firmament erhoben:", "tokens": ["Bis", "an", "das", "Fir\u00b7ma\u00b7ment", "er\u00b7ho\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und kurz, der schaalste Gratulant", "tokens": ["Und", "kurz", ",", "der", "scha\u00b7als\u00b7te", "Gra\u00b7tu\u00b7lant"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Kann keinen Midas dreister loben,", "tokens": ["Kann", "kei\u00b7nen", "Mi\u00b7das", "dreis\u00b7ter", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Als ihn die bunte Horde pries.", "tokens": ["Als", "ihn", "die", "bun\u00b7te", "Hor\u00b7de", "pries", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Doch der bescheidne Kanaride", "tokens": ["Doch", "der", "be\u00b7scheid\u00b7ne", "Ka\u00b7na\u00b7ri\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Entwich, der Last des Beyfalls m\u00fcde,", "tokens": ["Ent\u00b7wich", ",", "der", "Last", "des", "Bey\u00b7falls", "m\u00fc\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Noch eh der Hahn zum Abzug blies", "tokens": ["Noch", "eh", "der", "Hahn", "zum", "Ab\u00b7zug", "blies"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und sang auf einem \u00f6den Anger", "tokens": ["Und", "sang", "auf", "ei\u00b7nem", "\u00f6\u00b7den", "An\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Itzt, unbeklatscht vom Spatz und Specht,", "tokens": ["Itzt", ",", "un\u00b7be\u00b7klatscht", "vom", "Spatz", "und", "Specht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Sein sch\u00f6nstes Lied. Er hatte recht,", "tokens": ["Sein", "sch\u00f6ns\u00b7tes", "Lied", ".", "Er", "hat\u00b7te", "recht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Das Lob ist des Verdienstes Pranger.", "tokens": ["Das", "Lob", "ist", "des", "Ver\u00b7diens\u00b7tes", "Pran\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}