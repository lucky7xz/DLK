{"textgrid.poem.25133": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn ich in meinem Zimmer einsam bin", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich in meinem Zimmer einsam bin", "tokens": ["Wenn", "ich", "in", "mei\u00b7nem", "Zim\u00b7mer", "ein\u00b7sam", "bin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und h\u00e4\u00dfliche Gedanken mich umdunkeln,", "tokens": ["Und", "h\u00e4\u00df\u00b7li\u00b7che", "Ge\u00b7dan\u00b7ken", "mich", "um\u00b7dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn keine Traumlust schmeichelt meinem Sinn,", "tokens": ["Wenn", "kei\u00b7ne", "Traum\u00b7lust", "schmei\u00b7chelt", "mei\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus kahlem Leben keine Bl\u00fcten funkeln,", "tokens": ["Aus", "kah\u00b7lem", "Le\u00b7ben", "kei\u00b7ne", "Bl\u00fc\u00b7ten", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann, s\u00fc\u00dfe Hoffnung, schenke Balsam du,", "tokens": ["Dann", ",", "s\u00fc\u00b7\u00dfe", "Hoff\u00b7nung", ",", "schen\u00b7ke", "Bal\u00b7sam", "du", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "VVFIN", "NE", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit Silberschwingen f\u00e4chle mich in Ruh.", "tokens": ["Mit", "Sil\u00b7ber\u00b7schwin\u00b7gen", "f\u00e4ch\u00b7le", "mich", "in", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wenn ich wandre zu Beginn der Nacht", "tokens": ["Und", "wenn", "ich", "wand\u00b7re", "zu", "Be\u00b7ginn", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch Dickichte, die keinen Mondglanz kennen,", "tokens": ["Durch", "Di\u00b7ckich\u00b7te", ",", "die", "kei\u00b7nen", "Mond\u00b7glanz", "ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und wenn Verzagtheit mich bek\u00fcmmert macht", "tokens": ["Und", "wenn", "Ver\u00b7zag\u00b7theit", "mich", "be\u00b7k\u00fcm\u00b7mert", "macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "NN", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und gut versteht, von Frohsinn mich zu trennen,", "tokens": ["Und", "gut", "ver\u00b7steht", ",", "von", "Froh\u00b7sinn", "mich", "zu", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann lug durchs Laubendach als lichter Stern", "tokens": ["Dann", "lug", "durchs", "Lau\u00b7ben\u00b7dach", "als", "lich\u00b7ter", "Stern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "KOKOM", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und halt den Teufel Kleinmut von mir fern.", "tokens": ["Und", "halt", "den", "Teu\u00b7fel", "Klein\u00b7mut", "von", "mir", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und sollt Verdru\u00df, der Verzweiflung liebt,", "tokens": ["Und", "sollt", "Ver\u00b7dru\u00df", ",", "der", "Ver\u00b7zwei\u00b7flung", "liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00fcr sie nach meinem jungen Herzen krallen,", "tokens": ["F\u00fcr", "sie", "nach", "mei\u00b7nem", "jun\u00b7gen", "Her\u00b7zen", "kral\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die durch die Luft gleich schwarzer Wolke schiebt", "tokens": ["Die", "durch", "die", "Luft", "gleich", "schwar\u00b7zer", "Wol\u00b7ke", "schiebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und immer lauert, auf mich herzufallen,", "tokens": ["Und", "im\u00b7mer", "lau\u00b7ert", ",", "auf", "mich", "her\u00b7zu\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "APPR", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann, s\u00fc\u00dfe Hoffnung, strahle deine Pracht", "tokens": ["Dann", ",", "s\u00fc\u00b7\u00dfe", "Hoff\u00b7nung", ",", "strah\u00b7le", "dei\u00b7ne", "Pracht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und scheuche ihn, wie Morgen scheucht die Nacht!", "tokens": ["Und", "scheu\u00b7che", "ihn", ",", "wie", "Mor\u00b7gen", "scheucht", "die", "Nacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Spricht je das Schicksal jener, die mir nah,", "tokens": ["Spricht", "je", "das", "Schick\u00b7sal", "je\u00b7ner", ",", "die", "mir", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PDAT", "$,", "PRELS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu meinem Herzen von betr\u00fcbten Sorgen,", "tokens": ["Zu", "mei\u00b7nem", "Her\u00b7zen", "von", "be\u00b7tr\u00fcb\u00b7ten", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Hoffnung, heiliges Auge, l\u00e4chle da,", "tokens": ["O", "Hoff\u00b7nung", ",", "hei\u00b7li\u00b7ges", "Au\u00b7ge", ",", "l\u00e4ch\u00b7le", "da", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "La\u00df deine s\u00fc\u00dfen Tr\u00f6stungen mich borgen,", "tokens": ["La\u00df", "dei\u00b7ne", "s\u00fc\u00b7\u00dfen", "Tr\u00f6s\u00b7tun\u00b7gen", "mich", "bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Himmlisches Leuchten, Hoffnung, schenke du,", "tokens": ["Himm\u00b7li\u00b7sches", "Leuch\u00b7ten", ",", "Hoff\u00b7nung", ",", "schen\u00b7ke", "du", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Mit Silberschwingen f\u00e4chle mich in Ruh!", "tokens": ["Mit", "Sil\u00b7ber\u00b7schwin\u00b7gen", "f\u00e4ch\u00b7le", "mich", "in", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wenn je ungl\u00fccklich Lieben an mir zehrt", "tokens": ["Wenn", "je", "un\u00b7gl\u00fcck\u00b7lich", "Lie\u00b7ben", "an", "mir", "zehrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJD", "ADJA", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu einer grausam unbarmherzigen Sch\u00f6nen,", "tokens": ["Zu", "ei\u00b7ner", "grau\u00b7sam", "un\u00b7barm\u00b7her\u00b7zi\u00b7gen", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So la\u00df mich denken, da\u00df es doch von Wert,", "tokens": ["So", "la\u00df", "mich", "den\u00b7ken", ",", "da\u00df", "es", "doch", "von", "Wert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVINF", "$,", "KOUS", "PPER", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sonette in die Mitternacht zu st\u00f6hnen!", "tokens": ["So\u00b7net\u00b7te", "in", "die", "Mit\u00b7ter\u00b7nacht", "zu", "st\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O s\u00fc\u00dfe Hoffnung, schenke Balsam du,", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Hoff\u00b7nung", ",", "schen\u00b7ke", "Bal\u00b7sam", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "VVFIN", "NE", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit Silberschwingen f\u00e4chle mich in Ruh!", "tokens": ["Mit", "Sil\u00b7ber\u00b7schwin\u00b7gen", "f\u00e4ch\u00b7le", "mich", "in", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Im langen Lauf der Jahre, die da gehn,", "tokens": ["Im", "lan\u00b7gen", "Lauf", "der", "Jah\u00b7re", ",", "die", "da", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gib mir, da\u00df unser Land der Ehre diene,", "tokens": ["Gib", "mir", ",", "da\u00df", "un\u00b7ser", "Land", "der", "Eh\u00b7re", "die\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und la\u00df mich wieder seine Seele sehn:", "tokens": ["Und", "la\u00df", "mich", "wie\u00b7der", "sei\u00b7ne", "See\u00b7le", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Freiheit \u2013 nicht nur freiheitliche Miene.", "tokens": ["Die", "Frei\u00b7heit", "\u2013", "nicht", "nur", "frei\u00b7heit\u00b7li\u00b7che", "Mie\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PTKNEG", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Besondern Glanz, o Hoffnung, schenke du \u2013", "tokens": ["Be\u00b7son\u00b7dern", "Glanz", ",", "o", "Hoff\u00b7nung", ",", "schen\u00b7ke", "du", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und gib mir unter k\u00fchlen Schwingen Ruh!", "tokens": ["Und", "gib", "mir", "un\u00b7ter", "k\u00fch\u00b7len", "Schwin\u00b7gen", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die gro\u00dfe Freiheit, wei\u00df und ungeschm\u00fcckt,", "tokens": ["Die", "gro\u00b7\u00dfe", "Frei\u00b7heit", ",", "wei\u00df", "und", "un\u00b7ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Um deren Reinheit Patrioten sterben,", "tokens": ["Um", "de\u00b7ren", "Rein\u00b7heit", "Pat\u00b7ri\u00b7o\u00b7ten", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df mich nicht sehn, wie sie der Purpur dr\u00fcckt", "tokens": ["La\u00df", "mich", "nicht", "sehn", ",", "wie", "sie", "der", "Pur\u00b7pur", "dr\u00fcckt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKNEG", "VVINF", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sie sich beugt und bietet dem Verderben;", "tokens": ["Und", "sie", "sich", "beugt", "und", "bie\u00b7tet", "dem", "Ver\u00b7der\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PRF", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch la\u00df mich deine Silberschwingen sehn,", "tokens": ["Doch", "la\u00df", "mich", "dei\u00b7ne", "Sil\u00b7ber\u00b7schwin\u00b7gen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die glitzernd breit in dunklen Himmeln stehn.", "tokens": ["Die", "glit\u00b7zernd", "breit", "in", "dunk\u00b7len", "Him\u00b7meln", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und wie wohl eines Sternes kleines Licht", "tokens": ["Und", "wie", "wohl", "ei\u00b7nes", "Ster\u00b7nes", "klei\u00b7nes", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verhei\u00dfungsvoll in schwarzen H\u00f6hen funkelt", "tokens": ["Ver\u00b7hei\u00b7\u00dfungs\u00b7voll", "in", "schwar\u00b7zen", "H\u00f6\u00b7hen", "fun\u00b7kelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und milden Strahls durch finstre Wolken bricht,", "tokens": ["Und", "mil\u00b7den", "Strahls", "durch", "finst\u00b7re", "Wol\u00b7ken", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So, s\u00fc\u00dfe Hoffnung, wenn mein Sinn umdunkelt", "tokens": ["So", ",", "s\u00fc\u00b7\u00dfe", "Hoff\u00b7nung", ",", "wenn", "mein", "Sinn", "um\u00b7dun\u00b7kelt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von tr\u00fcbem Ahnen, dann erscheine du,", "tokens": ["Von", "tr\u00fc\u00b7bem", "Ah\u00b7nen", ",", "dann", "er\u00b7schei\u00b7ne", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit Silberschwingen f\u00e4chle mich in Ruh!", "tokens": ["Mit", "Sil\u00b7ber\u00b7schwin\u00b7gen", "f\u00e4ch\u00b7le", "mich", "in", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}