{"dta.poem.1135": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "21. Die Sch\u00f6ne Lispelnd- und Stammlende.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Schatz gewohnt die Wort auff Griechisch auszuspre- ", "tokens": ["Mein", "Schatz", "ge\u00b7wohnt", "die", "Wort", "auff", "Grie\u00b7chisch", "aus\u00b7zu\u00b7spre"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "ART", "NN", "APPR", "NE", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "chen/ ", "tokens": ["chen", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Welch Stahl/ welch Tereus macht dir die Zunge schwer?", "tokens": ["Welch", "Stahl", "/", "welch", "Te\u00b7reus", "macht", "dir", "die", "Zun\u00b7ge", "schwer", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "PWAT", "NN", "VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Lernstu/ als Nachtigall zu ziern der Vogel Heer/", "tokens": ["Lerns\u00b7tu", "/", "als", "Nach\u00b7ti\u00b7gall", "zu", "zi\u00b7ern", "der", "Vo\u00b7gel", "Heer", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOUS", "NN", "PTKZU", "VVINF", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Nach Filomenens Art die Worte radebrechen?", "tokens": ["Nach", "Fi\u00b7lo\u00b7me\u00b7nens", "Art", "die", "Wor\u00b7te", "ra\u00b7de\u00b7bre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Lippen Fehler kan der Sinnen Lob nicht schw\u00e4chen/", "tokens": ["Der", "Lip\u00b7pen", "Feh\u00b7ler", "kan", "der", "Sin\u00b7nen", "Lob", "nicht", "schw\u00e4\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "ART", "NN", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dir weicht der Schwan im Flu\u00df/ und die Siren im Meer.", "tokens": ["Dir", "weicht", "der", "Schwan", "im", "Flu\u00df", "/", "und", "die", "Si\u00b7ren", "im", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$(", "KON", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Der Widerruff ist offt nicht von Verstande leer.", "tokens": ["Der", "Wi\u00b7der\u00b7ruff", "ist", "offt", "nicht", "von", "Ver\u00b7stan\u00b7de", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein halbgesagtes Wort kan auch ins Hertze stechen.", "tokens": ["Ein", "halb\u00b7ge\u00b7sag\u00b7tes", "Wort", "kan", "auch", "ins", "Hert\u00b7ze", "ste\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "APPRART", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der unterbrochne Schall/ der aus dem Munde dringt/", "tokens": ["Der", "un\u00b7ter\u00b7broch\u00b7ne", "Schall", "/", "der", "aus", "dem", "Mun\u00b7de", "dringt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Gemahnt mich als ein Pfeil/ der zwar das Ziel nicht trifft/", "tokens": ["Ge\u00b7mahnt", "mich", "als", "ein", "Pfeil", "/", "der", "zwar", "das", "Ziel", "nicht", "trifft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "KOUS", "ART", "NN", "$(", "ART", "ADV", "ART", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Doch durch die L\u00fcffte scherzt und keinen Schaden stifft.", "tokens": ["Doch", "durch", "die", "L\u00fcff\u00b7te", "scherzt", "und", "kei\u00b7nen", "Scha\u00b7den", "stifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wenn du zu sagen denckst: Izt will ich dich durchsch\u00fcssen.", "tokens": ["Wenn", "du", "zu", "sa\u00b7gen", "denckst", ":", "Izt", "will", "ich", "dich", "durch\u00b7sch\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "VVFIN", "$.", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So h\u00f6r\u2019 ich einen Thon/ der viel gelinder klingt/", "tokens": ["So", "h\u00f6r'", "ich", "ei\u00b7nen", "Thon", "/", "der", "viel", "ge\u00b7lin\u00b7der", "klingt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$(", "ART", "PIAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "(das Harte bleibt zur\u00fcck/) izt will ich dich durchs\u00fcssen.", "tokens": ["(", "das", "Har\u00b7te", "bleibt", "zu\u00b7r\u00fcck", "/", ")", "izt", "will", "ich", "dich", "durch\u00b7s\u00fcs\u00b7sen", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PTKVZ", "$(", "$(", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}