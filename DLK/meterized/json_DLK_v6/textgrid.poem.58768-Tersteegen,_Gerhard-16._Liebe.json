{"textgrid.poem.58768": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "16. Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist die Lieb' und liebst dich rein;", "tokens": ["Du", "bist", "die", "Lieb'", "und", "liebst", "dich", "rein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nichts kann so gut, nichts sch\u00f6ner sein,", "tokens": ["Nichts", "kann", "so", "gut", ",", "nichts", "sch\u00f6\u00b7ner", "sein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADJD", "$,", "PIS", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bist selbst dein hohes Lieb'sgenie\u00dfen.", "tokens": ["Bist", "selbst", "dein", "ho\u00b7hes", "Lie\u00b7b's\u00b7ge\u00b7ni\u00b7e\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Lieb'smagnet zieht uns auch an,", "tokens": ["Dein", "Lie\u00b7b's\u00b7ma\u00b7gnet", "zieht", "uns", "auch", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Du machst uns lieblich, liebst uns dann;", "tokens": ["Du", "machst", "uns", "lieb\u00b7lich", ",", "liebst", "uns", "dann", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wir soll'n mit dir in Eins zerflie\u00dfen.", "tokens": ["Wir", "soll'n", "mit", "dir", "in", "Eins", "zer\u00b7flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "O Lieb'striumph, o Wonn', o Lust!", "tokens": ["O", "Lie\u00b7b'\u00b7stri\u00b7umph", ",", "o", "Wonn'", ",", "o", "Lust", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ach Mensch, ach Mensch, w\u00e4r' dir's bewu\u00dft!", "tokens": ["Ach", "Mensch", ",", "ach", "Mensch", ",", "w\u00e4r'", "dir's", "be\u00b7wu\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "XY", "NN", "$,", "VAFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Halleluja! Halleluja!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "!", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "ITJ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}