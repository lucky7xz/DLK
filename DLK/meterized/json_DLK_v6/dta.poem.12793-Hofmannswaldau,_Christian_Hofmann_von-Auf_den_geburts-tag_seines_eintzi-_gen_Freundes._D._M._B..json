{"dta.poem.12793": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den geburts-tag seines eintzi-  \n gen Freundes.  \n D. M. B.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Fleuch, finsterni\u00df! mit deiner tr\u00fcben nacht!", "tokens": ["Fleuch", ",", "fins\u00b7ter\u00b7ni\u00df", "!", "mit", "dei\u00b7ner", "tr\u00fc\u00b7ben", "nacht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$.", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wer deine wolcken liebt, den foltern bange schmertzen,", "tokens": ["Wer", "dei\u00b7ne", "wol\u00b7cken", "liebt", ",", "den", "fol\u00b7tern", "ban\u00b7ge", "schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer aber nicht auf sorgen ist bedacht,", "tokens": ["Wer", "a\u00b7ber", "nicht", "auf", "sor\u00b7gen", "ist", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKNEG", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der w\u00fcnschet dir den tod mit einem frohen hertzen,", "tokens": ["Der", "w\u00fcn\u00b7schet", "dir", "den", "tod", "mit", "ei\u00b7nem", "fro\u00b7hen", "hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum fleuch nur fort! in meiner hellen brust", "tokens": ["Drum", "fleuch", "nur", "fort", "!", "in", "mei\u00b7ner", "hel\u00b7len", "brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "PTKVZ", "$.", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Scheint freud und lust.", "tokens": ["Scheint", "freud", "und", "lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Scheint freud und lust auf meinen muntern geist;", "tokens": ["Scheint", "freud", "und", "lust", "auf", "mei\u00b7nen", "mun\u00b7tern", "geist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So kan ich auch vergn\u00fcgt die freuden-lieder singen,", "tokens": ["So", "kan", "ich", "auch", "ver\u00b7gn\u00fcgt", "die", "freu\u00b7den\u00b7lie\u00b7der", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die mich das licht durch seine strahlen heist,", "tokens": ["Die", "mich", "das", "licht", "durch", "sei\u00b7ne", "strah\u00b7len", "heist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das licht, das sch\u00f6ne licht, das mir will rosen bringen,", "tokens": ["Das", "licht", ",", "das", "sch\u00f6\u00b7ne", "licht", ",", "das", "mir", "will", "ro\u00b7sen", "brin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VMFIN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die diesen tag in voller bl\u00fcte stehn", "tokens": ["Die", "die\u00b7sen", "tag", "in", "vol\u00b7ler", "bl\u00fc\u00b7te", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PDAT", "NN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Recht frisch und sch\u00f6n.", "tokens": ["Recht", "frisch", "und", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Recht frisch und sch\u00f6n ist heute dieser tag,", "tokens": ["Recht", "frisch", "und", "sch\u00f6n", "ist", "heu\u00b7te", "die\u00b7ser", "tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "VAFIN", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da mein geehrter Freund vor zwey und zwantzig jahren", "tokens": ["Da", "mein", "ge\u00b7ehr\u00b7ter", "Freund", "vor", "zwey", "und", "zwant\u00b7zig", "jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "CARD", "KON", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das erste mal in einer wiegen lag,", "tokens": ["Das", "ers\u00b7te", "mal", "in", "ei\u00b7ner", "wie\u00b7gen", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und durch des himmels gunst den segen kont\u2019 erfahren,", "tokens": ["Und", "durch", "des", "him\u00b7mels", "gunst", "den", "se\u00b7gen", "kont'", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ART", "VVINF", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df GOttes arm die seinen m\u00e4chtig sch\u00fctzt,", "tokens": ["Da\u00df", "Got\u00b7tes", "arm", "die", "sei\u00b7nen", "m\u00e4ch\u00b7tig", "sch\u00fctzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "ART", "PPOSAT", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wenn alles blitzt.", "tokens": ["Wenn", "al\u00b7les", "blitzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Wenn alles blitzt, so lebt mein Jonathan,", "tokens": ["Wenn", "al\u00b7les", "blitzt", ",", "so", "lebt", "mein", "Jo\u00b7na\u00b7than", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem ich mein gantzes hertz als eigen \u00fcbergeben,", "tokens": ["Dem", "ich", "mein", "gant\u00b7zes", "hertz", "als", "ei\u00b7gen", "\u00fc\u00b7ber\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "ADJA", "NN", "KOKOM", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja wo ich noch was bessers lieffern kan,", "tokens": ["Ja", "wo", "ich", "noch", "was", "bes\u00b7sers", "lief\u00b7fern", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWAV", "PPER", "ADV", "PWS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das seiner liebe werth, so ist es wol mein leben,", "tokens": ["Das", "sei\u00b7ner", "lie\u00b7be", "werth", ",", "so", "ist", "es", "wol", "mein", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "VVFIN", "ADJD", "$,", "ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das schon vorl\u00e4ngst liegt als ein freundschaffts-pfand", "tokens": ["Das", "schon", "vor\u00b7l\u00e4ngst", "liegt", "als", "ein", "freund\u00b7schaffts\u00b7pfand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADV", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In seiner hand.", "tokens": ["In", "sei\u00b7ner", "hand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "In seiner hand, die treu und glauben h\u00e4lt,", "tokens": ["In", "sei\u00b7ner", "hand", ",", "die", "treu", "und", "glau\u00b7ben", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "KON", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Liegt auch mein gantzes heyl verwahrt und eingeschlossen,", "tokens": ["Liegt", "auch", "mein", "gant\u00b7zes", "heyl", "ver\u00b7wahrt", "und", "ein\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er ist ein freund, nicht nach der falschen welt,", "tokens": ["Er", "ist", "ein", "freund", ",", "nicht", "nach", "der", "fal\u00b7schen", "welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der, wenn sein fettes maul das gute hat genossen,", "tokens": ["Der", ",", "wenn", "sein", "fet\u00b7tes", "maul", "das", "gu\u00b7te", "hat", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des andern noth verl\u00e4st zur winters-zeit", "tokens": ["Des", "an\u00b7dern", "noth", "ver\u00b7l\u00e4st", "zur", "win\u00b7ter\u00b7szeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In traurigkeit.", "tokens": ["In", "trau\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "In traurigkeit schenckt er mir tr\u00f6stung ein,", "tokens": ["In", "trau\u00b7rig\u00b7keit", "schenckt", "er", "mir", "tr\u00f6s\u00b7tung", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und wenn der rauhe sturm ein ungewitter dr\u00e4uet,", "tokens": ["Und", "wenn", "der", "rau\u00b7he", "sturm", "ein", "un\u00b7ge\u00b7wit\u00b7ter", "dr\u00e4u\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So will sein hertz in meinem hertzen seyn,", "tokens": ["So", "will", "sein", "hertz", "in", "mei\u00b7nem", "hert\u00b7zen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das sich zugleich betr\u00fcbt, und keinen hagel scheuet,", "tokens": ["Das", "sich", "zu\u00b7gleich", "be\u00b7tr\u00fcbt", ",", "und", "kei\u00b7nen", "ha\u00b7gel", "scheu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ADV", "VVPP", "$,", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sein treuer fu\u00df besucht mein armes hau\u00df,", "tokens": ["Sein", "treu\u00b7er", "fu\u00df", "be\u00b7sucht", "mein", "ar\u00b7mes", "hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "PTKVZ", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wenn alles aus.", "tokens": ["Wenn", "al\u00b7les", "aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Wenn alles aus, so ist der freund mit mir,", "tokens": ["Wenn", "al\u00b7les", "aus", ",", "so", "ist", "der", "freund", "mit", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKVZ", "$,", "ADV", "VAFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der seine redligkeit in keine seide bindet;", "tokens": ["Der", "sei\u00b7ne", "red\u00b7lig\u00b7keit", "in", "kei\u00b7ne", "sei\u00b7de", "bin\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PIAT", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein deutsches wort das bleibet seine zier,", "tokens": ["Ein", "deut\u00b7sches", "wort", "das", "blei\u00b7bet", "sei\u00b7ne", "zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Darauf die tugend sich in ihren thaten gr\u00fcndet.", "tokens": ["Da\u00b7rauf", "die", "tu\u00b7gend", "sich", "in", "ih\u00b7ren", "tha\u00b7ten", "gr\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PRF", "APPR", "PPOSAT", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum danck ich auch dem himmel vor den Freund,", "tokens": ["Drum", "danck", "ich", "auch", "dem", "him\u00b7mel", "vor", "den", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der es so meynt.", "tokens": ["Der", "es", "so", "meynt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Der es so meynt, dem geh es immer wohl!", "tokens": ["Der", "es", "so", "meynt", ",", "dem", "geh", "es", "im\u00b7mer", "wohl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es m\u00fc\u00df\u2019 in seiner schoos das gl\u00fccke selber liegen!", "tokens": ["Es", "m\u00fc\u00df'", "in", "sei\u00b7ner", "schoos", "das", "gl\u00fc\u00b7cke", "sel\u00b7ber", "lie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "ADJD", "PDS", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der weg sey gut, worauf er gehen soll,", "tokens": ["Der", "weg", "sey", "gut", ",", "wo\u00b7rauf", "er", "ge\u00b7hen", "soll", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "ADJD", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Damit der schenckel kan, was andre f\u00e4llt, besiegen!", "tokens": ["Da\u00b7mit", "der", "schen\u00b7ckel", "kan", ",", "was", "and\u00b7re", "f\u00e4llt", ",", "be\u00b7sie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "ART", "NN", "VMFIN", "$,", "PRELS", "PIS", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kurtz: Alles heyl becr\u00f6ne haupt und haar,", "tokens": ["Kurtz", ":", "Al\u00b7les", "heyl", "be\u00b7cr\u00f6\u00b7ne", "haupt", "und", "haar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "PIS", "ADJD", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Noch manches jahr!", "tokens": ["Noch", "man\u00b7ches", "jahr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Noch manches jahr erscheine dieses licht,", "tokens": ["Noch", "man\u00b7ches", "jahr", "er\u00b7schei\u00b7ne", "die\u00b7ses", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bis da\u00df des alters schnee auf seinem sch\u00e4del gl\u00e4ntzet:", "tokens": ["Bis", "da\u00df", "des", "al\u00b7ters", "schnee", "auf", "sei\u00b7nem", "sch\u00e4\u00b7del", "gl\u00e4nt\u00b7zet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bis da\u00df das volck zu ihm Herr Pastor spricht:", "tokens": ["Bis", "da\u00df", "das", "volck", "zu", "ihm", "Herr", "Pas\u00b7tor", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "APPR", "PPER", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ihn ein keusches weib mit neuen freuden cr\u00e4ntzet:", "tokens": ["Und", "ihn", "ein", "keu\u00b7sches", "weib", "mit", "neu\u00b7en", "freu\u00b7den", "cr\u00e4nt\u00b7zet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bis da\u00df ein sohn der mutter hertz erfreut,", "tokens": ["Bis", "da\u00df", "ein", "sohn", "der", "mut\u00b7ter", "hertz", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und Vater! schreyt.", "tokens": ["Und", "Va\u00b7ter", "!", "schreyt", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$.", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}