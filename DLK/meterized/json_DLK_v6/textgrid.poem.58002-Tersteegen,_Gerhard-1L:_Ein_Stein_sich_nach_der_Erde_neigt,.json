{"textgrid.poem.58002": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Stein sich nach der Erde neigt,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Stein sich nach der Erde neigt,", "tokens": ["Ein", "Stein", "sich", "nach", "der", "Er\u00b7de", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Fl\u00e4mmlein in die H\u00f6he steigt,", "tokens": ["Ein", "Fl\u00e4mm\u00b7lein", "in", "die", "H\u00f6\u00b7he", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Fisch will in dem Wasser leben,", "tokens": ["Ein", "Fisch", "will", "in", "dem", "Was\u00b7ser", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Vogel in der Luft mu\u00df schweben;", "tokens": ["Ein", "Vo\u00b7gel", "in", "der", "Luft", "mu\u00df", "schwe\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn jedes da ist, wo es soll,", "tokens": ["Wenn", "je\u00b7des", "da", "ist", ",", "wo", "es", "soll", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADV", "VAFIN", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So ist es still und ihm ist wohl.", "tokens": ["So", "ist", "es", "still", "und", "ihm", "ist", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Geist ist ruhig und vergn\u00fcget,", "tokens": ["Mein", "Geist", "ist", "ru\u00b7hig", "und", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn er in Gott, sein'm Ruhpunkt, lieget.", "tokens": ["Wenn", "er", "in", "Gott", ",", "sein'm", "Ruh\u00b7punkt", ",", "lie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}