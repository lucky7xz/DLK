{"dta.poem.868": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Lv.  Auff den Sontag de\u00df Herren vnd Sohns  \n Davids/ oder  XIIX.  Sontag nach dem Fest  \n der H. Dreyeinigkeit. Matth. 12.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "O Du grosser Himmels F\u00fcrst! IESu! K\u00f6nig aller ding/ ", "tokens": ["O", "Du", "gros\u00b7ser", "Him\u00b7mels", "F\u00fcrst", "!", "Ie\u00b7Su", "!", "K\u00f6\u00b7nig", "al\u00b7ler", "ding", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "NN", "$.", "NE", "$.", "NN", "PIAT", "NN", "$("], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "IeSu! Davids Sohn vnd HErr! dessen Macht doch Sieg ", "tokens": ["Ie\u00b7Su", "!", "Da\u00b7vids", "Sohn", "vnd", "Herr", "!", "des\u00b7sen", "Macht", "doch", "Sieg"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "NE", "NN", "KON", "NN", "$.", "PRELAT", "NN", "KON", "NN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "vnd Feld ", "tokens": ["vnd", "Feld"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "So im mittel grimmer Feind\u2019: al\u00df gespitzter list erh\u00e4lt:", "tokens": ["So", "im", "mit\u00b7tel", "grim\u00b7mer", "Feind'", ":", "al\u00df", "ge\u00b7spitz\u00b7ter", "list", "er\u00b7h\u00e4lt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADJA", "NN", "$.", "KOUS", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Vnd vnendlich herrschen wird/ nim an/ was ich vor dich", "tokens": ["Vnd", "vnend\u00b7lich", "herr\u00b7schen", "wird", "/", "nim", "an", "/", "was", "ich", "vor", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVINF", "VAFIN", "$(", "VVIMP", "PTKVZ", "$(", "PWS", "PPER", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "bring: ", "tokens": ["bring", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Nimm mein Hertz/ vmb das dein Hertz an des Creutzes", "tokens": ["Nimm", "mein", "Hertz", "/", "vmb", "das", "dein", "Hertz", "an", "des", "Creut\u00b7zes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$(", "APPR", "ART", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Galgen hing ", "tokens": ["Gal\u00b7gen", "hing"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Nimm die Seel an vmb die du deine Seel in todt gestellt/", "tokens": ["Nimm", "die", "Seel", "an", "vmb", "die", "du", "dei\u00b7ne", "Seel", "in", "todt", "ge\u00b7stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "APPR", "PRELS", "PPER", "PPOSAT", "NN", "APPR", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Nimm die krafft f\u00fcr welche dir alle krafft O krafft der Welt/", "tokens": ["Nimm", "die", "krafft", "f\u00fcr", "wel\u00b7che", "dir", "al\u00b7le", "krafft", "O", "krafft", "der", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "PRELS", "PPER", "PIAT", "NN", "NE", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Da du an dem Holtz verschmacht in verfluchter Angst ", "tokens": ["Da", "du", "an", "dem", "Holtz", "ver\u00b7schmacht", "in", "ver\u00b7fluch\u00b7ter", "Angst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "entging. ", "tokens": ["ent\u00b7ging", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Liebster nimm an mein Geschenck. ", "tokens": ["Liebs\u00b7ter", "nimm", "an", "mein", "Ge\u00b7schenck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVIMP", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die in dieser frembden Welt deine Braut auffbringen kan/ ", "tokens": ["Die", "in", "die\u00b7ser", "fremb\u00b7den", "Welt", "dei\u00b7ne", "Braut", "auff\u00b7brin\u00b7gen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PDAT", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Hilff da\u00df ich doch gleich al\u00df mich/ Gott vnd meinen N\u00e4chsten", "tokens": ["Hilff", "da\u00df", "ich", "doch", "gleich", "al\u00df", "mich", "/", "Gott", "vnd", "mei\u00b7nen", "N\u00e4chs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KOUS", "PPER", "ADV", "ADV", "KOUS", "PPER", "$(", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "liebe: ", "tokens": ["lie\u00b7be", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Da\u00df ich/ was mich jetzund kr\u00e4nckt/ der ergrimmten Feinde", "tokens": ["Da\u00df", "ich", "/", "was", "mich", "je\u00b7tzund", "kr\u00e4nckt", "/", "der", "er\u00b7grimm\u00b7ten", "Fein\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$(", "PWS", "PPER", "ADV", "VVFIN", "$(", "ART", "ADJA", "NN"], "meter": "-+-++-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Schar ", "tokens": ["Schar"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Welche dich vnd mich verletzt (wie dir alles offen bar/)", "tokens": ["Wel\u00b7che", "dich", "vnd", "mich", "ver\u00b7letzt", "(", "wie", "dir", "al\u00b7les", "of\u00b7fen", "bar", "/", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "KON", "PPER", "VVPP", "$(", "PWAV", "PPER", "PIS", "ADJD", "ADJD", "$(", "$("], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Vnter deiner ", "tokens": ["Vn\u00b7ter", "dei\u00b7ner"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}}}}}