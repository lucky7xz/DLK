{"textgrid.poem.43365": {"metadata": {"author": {"name": "Leopardi, Giacomo", "birth": "N.A.", "death": "N.A."}, "title": "XxXIII. Monduntergang.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wie in \u00f6den N\u00e4chten", "tokens": ["So", "wie", "in", "\u00f6\u00b7den", "N\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf Feld und Wellen, die von Silber gl\u00e4nzen,", "tokens": ["Auf", "Feld", "und", "Wel\u00b7len", ",", "die", "von", "Sil\u00b7ber", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo leichte Winde schweben", "tokens": ["Wo", "leich\u00b7te", "Win\u00b7de", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und tausend Zauber walten,", "tokens": ["Und", "tau\u00b7send", "Zau\u00b7ber", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und rings mit Truggestalten", "tokens": ["Und", "rings", "mit", "Trug\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Schatten in der Ferne", "tokens": ["Die", "Schat\u00b7ten", "in", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gebirg und Meer und Villen", "tokens": ["Ge\u00b7birg", "und", "Meer", "und", "Vil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und jeden Zweig und jede Hecke f\u00fcllen,", "tokens": ["Und", "je\u00b7den", "Zweig", "und", "je\u00b7de", "He\u00b7cke", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nun dicht am Himmelssaume", "tokens": ["Nun", "dicht", "am", "Him\u00b7mels\u00b7sau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Hinter Gebirgswand oder in den Schoo\u00df", "tokens": ["Hin\u00b7ter", "Ge\u00b7birgs\u00b7wand", "o\u00b7der", "in", "den", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.11": {"text": "Des Meeres still und gro\u00df", "tokens": ["Des", "Mee\u00b7res", "still", "und", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Der Mond versinkt und sich die Welt entf\u00e4rbt", "tokens": ["Der", "Mond", "ver\u00b7sinkt", "und", "sich", "die", "Welt", "ent\u00b7f\u00e4rbt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "KON", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Die Schatten fliehn und nur", "tokens": ["Die", "Schat\u00b7ten", "fliehn", "und", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KON", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Und dann im blinden Schweigen", "tokens": ["Und", "dann", "im", "blin\u00b7den", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Der Nacht mit einsam klagendem Gesang", "tokens": ["Der", "Nacht", "mit", "ein\u00b7sam", "kla\u00b7gen\u00b7dem", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Den Nachglanz des Gestirns, das seinem Wagen", "tokens": ["Den", "Nach\u00b7glanz", "des", "Ge\u00b7stirns", ",", "das", "sei\u00b7nem", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Die Leuchte vorgetragen,", "tokens": ["Die", "Leuch\u00b7te", "vor\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Der K\u00e4rrner gr\u00fc\u00dft den m\u00fcden Pfad entlang: \u2013", "tokens": ["Der", "K\u00e4rr\u00b7ner", "gr\u00fc\u00dft", "den", "m\u00fc\u00b7den", "Pfad", "ent\u00b7lang", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "So schwinden, so entschweben", "tokens": ["So", "schwin\u00b7den", ",", "so", "ent\u00b7schwe\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Sehn wir dem Erdenleben", "tokens": ["Sehn", "wir", "dem", "Er\u00b7den\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Die Jugend. So von hinnen", "tokens": ["Die", "Ju\u00b7gend", ".", "So", "von", "hin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ADV", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Flieht aller Schein und Schatten", "tokens": ["Flieht", "al\u00b7ler", "Schein", "und", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Holdsel'gen Wahns; die hoffenden Gedanken,", "tokens": ["Hold\u00b7sel'\u00b7gen", "Wahns", ";", "die", "hof\u00b7fen\u00b7den", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.24": {"text": "Die uns vertr\u00f6stet hatten", "tokens": ["Die", "uns", "ver\u00b7tr\u00f6s\u00b7tet", "hat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "VVPP", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Auf eine Zukunft, sinken und verblassen.", "tokens": ["Auf", "ei\u00b7ne", "Zu\u00b7kunft", ",", "sin\u00b7ken", "und", "ver\u00b7blas\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVINF", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Umnachtet und verlassen", "tokens": ["Um\u00b7nach\u00b7tet", "und", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Ist nun das Leben. Mit verwirrten Sinnen", "tokens": ["Ist", "nun", "das", "Le\u00b7ben", ".", "Mit", "ver\u00b7wirr\u00b7ten", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Umblickend, sucht der Wandrer ach, vergebens", "tokens": ["Um\u00b7bli\u00b7ckend", ",", "sucht", "der", "Wand\u00b7rer", "ach", ",", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVPP", "$,", "VVFIN", "ART", "NN", "XY", "$,", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Des langen Wegs, den er noch vor sich ahnt,", "tokens": ["Des", "lan\u00b7gen", "Wegs", ",", "den", "er", "noch", "vor", "sich", "ahnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Ziel oder Zweck und sieht,", "tokens": ["Ziel", "o\u00b7der", "Zweck", "und", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.31": {"text": "Wie fremd ihm das Gebiet,", "tokens": ["Wie", "fremd", "ihm", "das", "Ge\u00b7biet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.32": {"text": "Und er \u2013 wie fremd er ward im Reich des Lebens.", "tokens": ["Und", "er", "\u2013", "wie", "fremd", "er", "ward", "im", "Reich", "des", "Le\u00b7bens", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "PWAV", "ADJD", "PPER", "VAFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zu froh und gl\u00fccklich noch", "tokens": ["Zu", "froh", "und", "gl\u00fcck\u00b7lich", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "KON", "ADJD", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "W\u00fcrd' unsre Erdennoth", "tokens": ["W\u00fcrd'", "uns\u00b7re", "Er\u00b7den\u00b7noth"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort oben scheinen, wenn die Jugendzeit,", "tokens": ["Dort", "o\u00b7ben", "schei\u00b7nen", ",", "wenn", "die", "Ju\u00b7gend\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo jedes Gut die Frucht von tausend Leiden,", "tokens": ["Wo", "je\u00b7des", "Gut", "die", "Frucht", "von", "tau\u00b7send", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ART", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Fortw\u00e4hrte durch den ganzen Lebenslauf;", "tokens": ["Fort\u00b7w\u00e4hr\u00b7te", "durch", "den", "gan\u00b7zen", "Le\u00b7bens\u00b7lauf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zu gn\u00e4dig das Gesetz,", "tokens": ["Zu", "gn\u00e4\u00b7dig", "das", "Ge\u00b7setz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das jeder Creatur verh\u00e4ngt den Tod,", "tokens": ["Das", "je\u00b7der", "Crea\u00b7tur", "ver\u00b7h\u00e4ngt", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "W\u00e4r' nicht ein halbes Leben", "tokens": ["W\u00e4r'", "nicht", "ein", "hal\u00b7bes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Uns noch zuvor gegeben,", "tokens": ["Uns", "noch", "zu\u00b7vor", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Das h\u00e4rter ist als alle Todesschrecken.", "tokens": ["Das", "h\u00e4r\u00b7ter", "ist", "als", "al\u00b7le", "To\u00b7des\u00b7schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "KOKOM", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "O g\u00f6ttlicher Erfindung", "tokens": ["O", "g\u00f6tt\u00b7li\u00b7cher", "Er\u00b7fin\u00b7dung"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "H\u00f6chst w\u00fcrdig, aller Uebel", "tokens": ["H\u00f6chst", "w\u00fcr\u00b7dig", ",", "al\u00b7ler", "Ue\u00b7bel"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Unseligstes, verliehen uns die Ew'gen", "tokens": ["Un\u00b7se\u00b7ligs\u00b7tes", ",", "ver\u00b7lie\u00b7hen", "uns", "die", "Ew'\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Das Alter, wo die W\u00fcnsche", "tokens": ["Das", "Al\u00b7ter", ",", "wo", "die", "W\u00fcn\u00b7sche"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Noch gl\u00fchend sind, die Hoffnung l\u00e4ngst erloschen,", "tokens": ["Noch", "gl\u00fc\u00b7hend", "sind", ",", "die", "Hoff\u00b7nung", "l\u00e4ngst", "er\u00b7lo\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Versiegt der Freuden Quell und stets sich h\u00e4uft", "tokens": ["Ver\u00b7siegt", "der", "Freu\u00b7den", "Quell", "und", "stets", "sich", "h\u00e4uft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN", "KON", "VVFIN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Das Weh, in das kein Tropfen Wonne tr\u00e4uft.", "tokens": ["Das", "Weh", ",", "in", "das", "kein", "Trop\u00b7fen", "Won\u00b7ne", "tr\u00e4uft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "PIAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr H\u00fcgel und Gefilde,", "tokens": ["Ihr", "H\u00fc\u00b7gel", "und", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht lang nachdem der Glanz hinabgesunken,", "tokens": ["Nicht", "lang", "nach\u00b7dem", "der", "Glanz", "hin\u00b7ab\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der das Gewand der Nacht in Silber taucht,", "tokens": ["Der", "das", "Ge\u00b7wand", "der", "Nacht", "in", "Sil\u00b7ber", "taucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht lange sollt ihr harren,", "tokens": ["Nicht", "lan\u00b7ge", "sollt", "ihr", "har\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Verwais't und bang; bald naht die Morgenfr\u00fche,", "tokens": ["Ver\u00b7wais't", "und", "bang", ";", "bald", "naht", "die", "Mor\u00b7gen\u00b7fr\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$.", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die d\u00e4mmernd \u00fcberhaucht", "tokens": ["Die", "d\u00e4m\u00b7mernd", "\u00fc\u00b7ber\u00b7haucht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Euch und den Himmel und das Meer von Neuem.", "tokens": ["Euch", "und", "den", "Him\u00b7mel", "und", "das", "Meer", "von", "Neu\u00b7em", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "KON", "ART", 