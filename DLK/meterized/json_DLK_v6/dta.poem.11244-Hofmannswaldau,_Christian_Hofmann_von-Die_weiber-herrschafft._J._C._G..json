{"dta.poem.11244": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die weiber-herrschafft.  \n J. C. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die m\u00e4nner sollen stets die ober-herrschafft f\u00fchren,", "tokens": ["Die", "m\u00e4n\u00b7ner", "sol\u00b7len", "stets", "die", "o\u00b7ber\u00b7herr\u00b7schafft", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Di\u00df hat das Sachsen-recht denselben zuerkannt;", "tokens": ["Di\u00df", "hat", "das", "Sach\u00b7sen\u00b7recht", "den\u00b7sel\u00b7ben", "zu\u00b7er\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein das kammer-recht das heist die frau regieren,", "tokens": ["Al\u00b7lein", "das", "kam\u00b7mer\u00b7recht", "das", "heist", "die", "frau", "re\u00b7gie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "PDS", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da giebt ihr selbst der mann den scepter in die hand.", "tokens": ["Da", "giebt", "ihr", "selbst", "der", "mann", "den", "scep\u00b7ter", "in", "die", "hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}