{"dta.poem.19277": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zur Gotterkentnis sind die Thiere nicht erschaffen,", "tokens": ["Zur", "Got\u00b7ter\u00b7kent\u00b7nis", "sind", "die", "Thie\u00b7re", "nicht", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du unterscheidest dich durch sie, o Mensch, vom Affen.", "tokens": ["Du", "un\u00b7ter\u00b7schei\u00b7dest", "dich", "durch", "sie", ",", "o", "Mensch", ",", "vom", "Af\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "$,", "FM", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ohne sie stehst du nicht mit ihm auf gleichen Stufen,", "tokens": ["Oh\u00b7ne", "sie", "stehst", "du", "nicht", "mit", "ihm", "auf", "glei\u00b7chen", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-++--+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Sondern auf niederern, weil h\u00f6hern zuberufen.", "tokens": ["Son\u00b7dern", "auf", "nie\u00b7de\u00b7rern", ",", "weil", "h\u00f6\u00b7hern", "zu\u00b7be\u00b7ru\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "$,", "KOUS", "ADJD", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Denn Trank und Speis' und Schlaf und sinnliche Begier,", "tokens": ["Denn", "Trank", "und", "Speis'", "und", "Schlaf", "und", "sinn\u00b7li\u00b7che", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die v\u00f6llig ihm gen\u00fcgt, gen\u00fcgt nie v\u00f6llig dir.", "tokens": ["Die", "v\u00f6l\u00b7lig", "ihm", "ge\u00b7n\u00fcgt", ",", "ge\u00b7n\u00fcgt", "nie", "v\u00f6l\u00b7lig", "dir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "ADJD", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du h\u00e4ltst ein H\u00f6heres dir im Bewu\u00dftsein vor,", "tokens": ["Du", "h\u00e4ltst", "ein", "H\u00f6\u00b7he\u00b7res", "dir", "im", "Be\u00b7wu\u00df\u00b7tsein", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Und bist nicht du, wo du nicht ewig ringst empor.", "tokens": ["Und", "bist", "nicht", "du", ",", "wo", "du", "nicht", "e\u00b7wig", "ringst", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "PPER", "$,", "PWAV", "PPER", "PTKNEG", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}