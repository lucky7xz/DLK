{"textgrid.poem.26668": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Vorwurf", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4dchen, halt \u2013 wohin mit mir, du Lose?", "tokens": ["M\u00e4d\u00b7chen", ",", "halt", "\u2013", "wo\u00b7hin", "mit", "mir", ",", "du", "Lo\u00b7se", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$(", "PWAV", "APPR", "PPER", "$,", "PPER", "NE", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bin ich noch der stolze Mann? der gro\u00dfe?", "tokens": ["Bin", "ich", "noch", "der", "stol\u00b7ze", "Mann", "?", "der", "gro\u00b7\u00dfe", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$.", "ART", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "M\u00e4dchen, war das sch\u00f6n?", "tokens": ["M\u00e4d\u00b7chen", ",", "war", "das", "sch\u00f6n", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ART", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Sieh! Der Riese schrumpft durch dich zum Zwerge,", "tokens": ["Sieh", "!", "Der", "Rie\u00b7se", "schrumpft", "durch", "dich", "zum", "Zwer\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Weggehaucht die aufgew\u00e4lzten Berge", "tokens": ["Weg\u00b7ge\u00b7haucht", "die", "auf\u00b7ge\u00b7w\u00e4lz\u00b7ten", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Zu des Ruhmes Sonnenh\u00f6hn.", "tokens": ["Zu", "des", "Ruh\u00b7mes", "Son\u00b7nen\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Abgepfl\u00fccket hat du meine Blume,", "tokens": ["Ab\u00b7ge\u00b7pfl\u00fc\u00b7cket", "hat", "du", "mei\u00b7ne", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hast verblasen all die Glanzphantome,", "tokens": ["Hast", "ver\u00b7bla\u00b7sen", "all", "die", "Glanz\u00b7phan\u00b7to\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "PIAT", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Narrenteidigst in des Helden Raub.", "tokens": ["Nar\u00b7ren\u00b7tei\u00b7digst", "in", "des", "Hel\u00b7den", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Meiner Plane stolze Pyramiden", "tokens": ["Mei\u00b7ner", "Pla\u00b7ne", "stol\u00b7ze", "Py\u00b7ra\u00b7mi\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Trippelst du mit leichten Zephyrtritten", "tokens": ["Trip\u00b7pelst", "du", "mit", "leich\u00b7ten", "Ze\u00b7phyr\u00b7trit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Sch\u00e4kernd in den Staub.", "tokens": ["Sch\u00e4\u00b7kernd", "in", "den", "Staub", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Zu der Gottheit flog ich Adlerpfade,", "tokens": ["Zu", "der", "Got\u00b7theit", "flog", "ich", "Ad\u00b7ler\u00b7pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "L\u00e4chelte Fortunens Gaukelrade,", "tokens": ["L\u00e4\u00b7chel\u00b7te", "For\u00b7tu\u00b7nens", "Gau\u00b7kel\u00b7ra\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Unbesorgt, wie ihre Kugel fiel.", "tokens": ["Un\u00b7be\u00b7sorgt", ",", "wie", "ih\u00b7re", "Ku\u00b7gel", "fiel", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Jenseits dem Cocytus wollt ich schweben,", "tokens": ["Jen\u00b7seits", "dem", "Co\u00b7cy\u00b7tus", "wollt", "ich", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und empfange sklavisch Tod und Leben,", "tokens": ["Und", "emp\u00b7fan\u00b7ge", "skla\u00b7visch", "Tod", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Leben, Tod von einem Augenspiel.", "tokens": ["Le\u00b7ben", ",", "Tod", "von", "ei\u00b7nem", "Au\u00b7gen\u00b7spiel", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Siegern gleich, die wach von Donnerlanzen", "tokens": ["Sie\u00b7gern", "gleich", ",", "die", "wach", "von", "Don\u00b7ner\u00b7lan\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "PRELS", "ADJD", "APPR", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "In des Ruhmes Eisenfluren tanzen,", "tokens": ["In", "des", "Ruh\u00b7mes", "Ei\u00b7sen\u00b7flu\u00b7ren", "tan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Losgerissen von der Phrynen Brust,", "tokens": ["Los\u00b7ge\u00b7ris\u00b7sen", "von", "der", "Phry\u00b7nen", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wallet aus Aurorens Rosenbette", "tokens": ["Wal\u00b7let", "aus", "Au\u00b7ro\u00b7rens", "Ro\u00b7sen\u00b7bet\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "NE"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Gottes Sonne \u00fcber F\u00fcrstenst\u00e4dte,", "tokens": ["Got\u00b7tes", "Son\u00b7ne", "\u00fc\u00b7ber", "F\u00fcrs\u00b7ten\u00b7st\u00e4d\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Lacht die junge Welt in Lust!", "tokens": ["Lacht", "die", "jun\u00b7ge", "Welt", "in", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "H\u00fcpft der Heldin noch dies Herz entgegen?", "tokens": ["H\u00fcpft", "der", "Hel\u00b7din", "noch", "dies", "Herz", "ent\u00b7ge\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "PDS", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Trink ich, Adler, noch den Flammenregen", "tokens": ["Trink", "ich", ",", "Ad\u00b7ler", ",", "noch", "den", "Flam\u00b7men\u00b7re\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ihres Auges, das vernichtend brennt?", "tokens": ["Ih\u00b7res", "Au\u00b7ges", ",", "das", "ver\u00b7nich\u00b7tend", "brennt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "In den Blicken, die vernichtend blinken,", "tokens": ["In", "den", "Bli\u00b7cken", ",", "die", "ver\u00b7nich\u00b7tend", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Seh ich meine Laura ", "tokens": ["Seh", "ich", "mei\u00b7ne", "Lau\u00b7ra"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Sehs, und weine wie ein Kind.", "tokens": ["Sehs", ",", "und", "wei\u00b7ne", "wie", "ein", "Kind", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Meine Ruhe, gleich dem Sonnenbilde", "tokens": ["Mei\u00b7ne", "Ru\u00b7he", ",", "gleich", "dem", "Son\u00b7nen\u00b7bil\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In der Welle, wolkenlos und milde,", "tokens": ["In", "der", "Wel\u00b7le", ",", "wol\u00b7ken\u00b7los", "und", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "M\u00e4dchen, hast du hingemordt.", "tokens": ["M\u00e4d\u00b7chen", ",", "hast", "du", "hin\u00b7ge\u00b7mordt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwindelnd schwank ich auf der g\u00e4hen H\u00f6he,", "tokens": ["Schwin\u00b7delnd", "schwank", "ich", "auf", "der", "g\u00e4\u00b7hen", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Laura? \u2013 wenn mich \u2013 wenn mich Laura fl\u00f6he?", "tokens": ["Lau\u00b7ra", "?", "\u2013", "wenn", "mich", "\u2013", "wenn", "mich", "Lau\u00b7ra", "fl\u00f6\u00b7he", "?"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "$(", "KOUS", "PPER", "$(", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und hinunter strudelt mich das Wort.", "tokens": ["Und", "hin\u00b7un\u00b7ter", "stru\u00b7delt", "mich", "das", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Hell ert\u00f6nt das Evoe der Zecher,", "tokens": ["Hell", "er\u00b7t\u00f6nt", "das", "E\u00b7voe", "der", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Freuden winken vom bekr\u00e4nzten Becher,", "tokens": ["Freu\u00b7den", "win\u00b7ken", "vom", "be\u00b7kr\u00e4nz\u00b7ten", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Scherze springen aus dem goldnen Wein.", "tokens": ["Scher\u00b7ze", "sprin\u00b7gen", "aus", "dem", "gold\u00b7nen", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Seit das M\u00e4dchen meinen Sinn beschworen,", "tokens": ["Seit", "das", "M\u00e4d\u00b7chen", "mei\u00b7nen", "Sinn", "be\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Haben mich die J\u00fcnglinge verloren,", "tokens": ["Ha\u00b7ben", "mich", "die", "J\u00fcng\u00b7lin\u00b7ge", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Freundlos irr ich und allein.", "tokens": ["Freund\u00b7los", "irr", "ich", "und", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Lausch ich noch des Ruhmes Donnerglocken?", "tokens": ["Lausch", "ich", "noch", "des", "Ruh\u00b7mes", "Don\u00b7ner\u00b7glo\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Reizt mich noch der Lorbeer in den Locken?", "tokens": ["Reizt", "mich", "noch", "der", "Lor\u00b7beer", "in", "den", "Lo\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Deine Leir, Apollo Cynthius?", "tokens": ["Dei\u00b7ne", "Leir", ",", "A\u00b7pol\u00b7lo", "Cyn\u00b7thi\u00b7us", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NE", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nimmer, nimmer widerhallt mein Busen,", "tokens": ["Nim\u00b7mer", ",", "nim\u00b7mer", "wi\u00b7der\u00b7hallt", "mein", "Bu\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Traurig fliehen die besch\u00e4mten Musen,", "tokens": ["Trau\u00b7rig", "flie\u00b7hen", "die", "be\u00b7sch\u00e4m\u00b7ten", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Flieht Apollo Cynthius?", "tokens": ["Flieht", "A\u00b7pol\u00b7lo", "Cyn\u00b7thi\u00b7us", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Will ich gar zum Weibe noch erlahmen?", "tokens": ["Will", "ich", "gar", "zum", "Wei\u00b7be", "noch", "er\u00b7lah\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "H\u00fcpfen noch bei Vaterlandes Namen", "tokens": ["H\u00fcp\u00b7fen", "noch", "bei", "Va\u00b7ter\u00b7lan\u00b7des", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Meine Pulse lebend aus der Gruft?", "tokens": ["Mei\u00b7ne", "Pul\u00b7se", "le\u00b7bend", "aus", "der", "Gruft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Will ich noch nach Varus' Adler ringen?", "tokens": ["Will", "ich", "noch", "nach", "Va\u00b7rus'", "Ad\u00b7ler", "rin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "W\u00fcnsch ich noch in R\u00f6merblut zu springen,", "tokens": ["W\u00fcnsch", "ich", "noch", "in", "R\u00f6\u00b7mer\u00b7blut", "zu", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "p