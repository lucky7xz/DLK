{"textgrid.poem.66651": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Wandlung", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst sprach das Leid zu mir: du bist verloren,", "tokens": ["Einst", "sprach", "das", "Leid", "zu", "mir", ":", "du", "bist", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu fest umklammert h\u00e4lt dich meine Hand.", "tokens": ["Zu", "fest", "um\u00b7klam\u00b7mert", "h\u00e4lt", "dich", "mei\u00b7ne", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVPP", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du glaubtest dich zur Herrlichkeit erkoren,", "tokens": ["Du", "glaub\u00b7test", "dich", "zur", "Herr\u00b7lich\u00b7keit", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nun l\u00e4hm' ich dir mit Schrecken den Verstand.", "tokens": ["Nun", "l\u00e4hm'", "ich", "dir", "mit", "Schre\u00b7cken", "den", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Einst sprach das Leid zu mir: du bist verloren.", "tokens": ["Einst", "sprach", "das", "Leid", "zu", "mir", ":", "du", "bist", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O schwere Zeit der finsteren Gewalten,", "tokens": ["O", "schwe\u00b7re", "Zeit", "der", "fins\u00b7te\u00b7ren", "Ge\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da ich in Qualen lag, die niemand wei\u00df;", "tokens": ["Da", "ich", "in", "Qua\u00b7len", "lag", ",", "die", "nie\u00b7mand", "wei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da mich umarmten widrige Gestalten,", "tokens": ["Da", "mich", "um\u00b7arm\u00b7ten", "wid\u00b7ri\u00b7ge", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und fern versank der Jugend Sehnsuchtspreis ...", "tokens": ["Und", "fern", "ver\u00b7sank", "der", "Ju\u00b7gend", "Sehn\u00b7suchts\u00b7preis", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O schwere Zeit der finsteren Gewalten!", "tokens": ["O", "schwe\u00b7re", "Zeit", "der", "fins\u00b7te\u00b7ren", "Ge\u00b7wal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich taumelte, ich strauchelte, ich st\u00fcrzte \u2013", "tokens": ["Ich", "tau\u00b7mel\u00b7te", ",", "ich", "strau\u00b7chel\u00b7te", ",", "ich", "st\u00fcrz\u00b7te", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Frag keiner mich, wohin geriet mein Gram!", "tokens": ["Frag", "kei\u00b7ner", "mich", ",", "wo\u00b7hin", "ge\u00b7riet", "mein", "Gram", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "PPER", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als Ekel mir das Mahl des Lebens w\u00fcrzte", "tokens": ["Als", "E\u00b7kel", "mir", "das", "Mahl", "des", "Le\u00b7bens", "w\u00fcrz\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und die Verzweiflung mir den Schlummer nahm.", "tokens": ["Und", "die", "Ver\u00b7zwei\u00b7flung", "mir", "den", "Schlum\u00b7mer", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich taumelte, ich strauchelte, ich st\u00fcrzte.", "tokens": ["Ich", "tau\u00b7mel\u00b7te", ",", "ich", "strau\u00b7chel\u00b7te", ",", "ich", "st\u00fcrz\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wer l\u00f6st das R\u00e4tsel, wie ich auferstanden?", "tokens": ["Wer", "l\u00f6st", "das", "R\u00e4t\u00b7sel", ",", "wie", "ich", "auf\u00b7er\u00b7stan\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geheime H\u00e4nde hoben mich empor.", "tokens": ["Ge\u00b7hei\u00b7me", "H\u00e4n\u00b7de", "ho\u00b7ben", "mich", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Leben heilte mich mit neuen Banden,", "tokens": ["Das", "Le\u00b7ben", "heil\u00b7te", "mich", "mit", "neu\u00b7en", "Ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und alles ward erf\u00fcllt, was ich verlor.", "tokens": ["Und", "al\u00b7les", "ward", "er\u00b7f\u00fcllt", ",", "was", "ich", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "VVPP", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wer l\u00f6st das R\u00e4tsel, wie ich auferstanden?", "tokens": ["Wer", "l\u00f6st", "das", "R\u00e4t\u00b7sel", ",", "wie", "ich", "auf\u00b7er\u00b7stan\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich bin ein Wundergl\u00e4ubiger geworden,", "tokens": ["Ich", "bin", "ein", "Wun\u00b7der\u00b7gl\u00e4u\u00b7bi\u00b7ger", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seit mir so Wunderwirkendes geschah.", "tokens": ["Seit", "mir", "so", "Wun\u00b7der\u00b7wir\u00b7ken\u00b7des", "ge\u00b7schah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Lied der Kraft mit segnenden Akkorden", "tokens": ["Das", "Lied", "der", "Kraft", "mit", "seg\u00b7nen\u00b7den", "Ak\u00b7kor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Umrauschte mich \u2013 ein Sieger stand ich da.", "tokens": ["Um\u00b7rauschte", "mich", "\u2013", "ein", "Sie\u00b7ger", "stand", "ich", "da", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ich bin ein Wundergl\u00e4ubiger geworden.", "tokens": ["Ich", "bin", "ein", "Wun\u00b7der\u00b7gl\u00e4u\u00b7bi\u00b7ger", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Verwandelt ist mein F\u00fchlen und mein Sinnen,", "tokens": ["Ver\u00b7wan\u00b7delt", "ist", "mein", "F\u00fch\u00b7len", "und", "mein", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und keiner Wolke Dunst umarm' ich mehr.", "tokens": ["Und", "kei\u00b7ner", "Wol\u00b7ke", "Dunst", "um\u00b7arm'", "ich", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Nichts verlieren und das All gewinnen:", "tokens": ["Das", "Nichts", "ver\u00b7lie\u00b7ren", "und", "das", "All", "ge\u00b7win\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVINF", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gewaltig ist des Lebens Wiederkehr.", "tokens": ["Ge\u00b7wal\u00b7tig", "ist", "des", "Le\u00b7bens", "Wie\u00b7der\u00b7kehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verwandelt ist mein F\u00fchlen und mein Sinnen.", "tokens": ["Ver\u00b7wan\u00b7delt", "ist", "mein", "F\u00fch\u00b7len", "und", "mein", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Einst sprach das Leid zu mir: du bist verloren.", "tokens": ["Einst", "sprach", "das", "Leid", "zu", "mir", ":", "du", "bist", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Spiel war aus und meine Seele tot.", "tokens": ["Das", "Spiel", "war", "aus", "und", "mei\u00b7ne", "See\u00b7le", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "KON", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nun bin ich doch zur Herrlichkeit erkoren,", "tokens": ["Nun", "bin", "ich", "doch", "zur", "Herr\u00b7lich\u00b7keit", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zur h\u00f6chsten Wonne ward die tiefste Not. \u2013", "tokens": ["Zur", "h\u00f6chs\u00b7ten", "Won\u00b7ne", "ward", "die", "tiefs\u00b7te", "Not", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Einst sprach das Leid zu mir: du bist verloren!", "tokens": ["Einst", "sprach", "das", "Leid", "zu", "mir", ":", "du", "bist", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}