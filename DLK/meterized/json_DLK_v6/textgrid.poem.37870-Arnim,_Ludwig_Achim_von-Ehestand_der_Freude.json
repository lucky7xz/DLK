{"textgrid.poem.37870": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Ehestand der Freude", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lasset uns scherzen", "tokens": ["Las\u00b7set", "uns", "scher\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Bl\u00fchende Herzen,", "tokens": ["Bl\u00fc\u00b7hen\u00b7de", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Lasset uns lieben", "tokens": ["Las\u00b7set", "uns", "lie\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ohne Verschieben,", "tokens": ["Oh\u00b7ne", "Ver\u00b7schie\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Lauten und Geigen", "tokens": ["Lau\u00b7ten", "und", "Gei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Sollen nicht schweigen,", "tokens": ["Sol\u00b7len", "nicht", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Kommet zum Tanze,", "tokens": ["Kom\u00b7met", "zum", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Pfl\u00fccket vom Kranze.", "tokens": ["Pfl\u00fc\u00b7cket", "vom", "Kran\u00b7ze", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Dr\u00fccket die H\u00e4nde,", "tokens": ["Dr\u00fc\u00b7cket", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Legt euch zum Ende,", "tokens": ["Legt", "euch", "zum", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.3": {"text": "Gebet Euch K\u00fcsse,", "tokens": ["Ge\u00b7bet", "Euch", "K\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Tretet die F\u00fc\u00dfe,", "tokens": ["Tre\u00b7tet", "die", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Machet euch fr\u00f6hlich,", "tokens": ["Ma\u00b7chet", "euch", "fr\u00f6h\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Machet euch ehlich,", "tokens": ["Ma\u00b7chet", "euch", "eh\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Lasset die Narren", "tokens": ["Las\u00b7set", "die", "Nar\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Einsam verharren.", "tokens": ["Ein\u00b7sam", "ver\u00b7har\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Ehlich zu werden", "tokens": ["Eh\u00b7lich", "zu", "wer\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "PTKZU", "VAINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Dienet der Erden,", "tokens": ["Die\u00b7net", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ledige Leute", "tokens": ["Le\u00b7di\u00b7ge", "Leu\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mangeln der Freude;", "tokens": ["Man\u00b7geln", "der", "Freu\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Jeder mu\u00df sterben,", "tokens": ["Je\u00b7der", "mu\u00df", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Machet euch Erben", "tokens": ["Ma\u00b7chet", "euch", "Er\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Euerem Gute,", "tokens": ["Eu\u00b7e\u00b7rem", "Gu\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Namen und Blute.", "tokens": ["Na\u00b7men", "und", "Blu\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Lasset der Grauen", "tokens": ["Las\u00b7set", "der", "Grau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Murren und Schauen,", "tokens": ["Mur\u00b7ren", "und", "Schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Rathen und Wissen,", "tokens": ["Ra\u00b7then", "und", "Wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wenig erspriessen;", "tokens": ["We\u00b7nig", "er\u00b7spries\u00b7sen", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Eben sie selber", "tokens": ["E\u00b7ben", "sie", "sel\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPER", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Waren auch K\u00e4lber,", "tokens": ["Wa\u00b7ren", "auch", "K\u00e4l\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Bl\u00fchende Herzen", "tokens": ["Bl\u00fc\u00b7hen\u00b7de", "Her\u00b7zen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Lasset uns scherzen.", "tokens": ["Las\u00b7set", "uns", "scher\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}