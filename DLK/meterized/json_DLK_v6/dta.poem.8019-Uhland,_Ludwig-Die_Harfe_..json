{"dta.poem.8019": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Harfe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "In W\u00e4lder floh mit seinem Grame", "tokens": ["In", "W\u00e4l\u00b7der", "floh", "mit", "sei\u00b7nem", "Gra\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Ritter, den verschm\u00e4ht die Dame.", "tokens": ["Ein", "Rit\u00b7ter", ",", "den", "ver\u00b7schm\u00e4ht", "die", "Da\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm kommt auf ungebahnten Wegen", "tokens": ["Ihm", "kommt", "auf", "un\u00b7ge\u00b7bahn\u00b7ten", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein traut umfangen Paar entgegen.", "tokens": ["Ein", "traut", "um\u00b7fan\u00b7gen", "Paar", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er kann ihr Kosen ganz verstehen,", "tokens": ["Er", "kann", "ihr", "Ko\u00b7sen", "ganz", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da sie auf sich nur h\u00f6ren, sehen:", "tokens": ["Da", "sie", "auf", "sich", "nur", "h\u00f6\u00b7ren", ",", "se\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PRF", "ADV", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sind sich kaum zur\u00fcckgegeben", "tokens": ["Sie", "sind", "sich", "kaum", "zu\u00b7r\u00fcck\u00b7ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu neuer Liebe, neuem Leben.", "tokens": ["Zu", "neu\u00b7er", "Lie\u00b7be", ",", "neu\u00b7em", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mu\u00df Alles seinen Schmerz erfrischen!", "tokens": ["Mu\u00df", "Al\u00b7les", "sei\u00b7nen", "Schmerz", "er\u00b7fri\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er fliehet zu den dunklern B\u00fcschen.", "tokens": ["Er", "flie\u00b7het", "zu", "den", "dunk\u00b7lern", "B\u00fc\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da steht in schwarzer Tannen Mitte,", "tokens": ["Da", "steht", "in", "schwar\u00b7zer", "Tan\u00b7nen", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verlassen, eine Bruderh\u00fctte.", "tokens": ["Ver\u00b7las\u00b7sen", ",", "ei\u00b7ne", "Bru\u00b7der\u00b7h\u00fct\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hier liegt die Eremitenh\u00fclle,", "tokens": ["Hier", "liegt", "die", "E\u00b7re\u00b7mi\u00b7ten\u00b7h\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dort h\u00e4ngt die Harfe, traurig stille;", "tokens": ["Dort", "h\u00e4ngt", "die", "Har\u00b7fe", ",", "trau\u00b7rig", "stil\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gewi\u00df! den er gesehn im Gl\u00fccke,", "tokens": ["Ge\u00b7wi\u00df", "!", "den", "er", "ge\u00b7sehn", "im", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ART", "PPER", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der lie\u00df sein Trauren hier zur\u00fccke.", "tokens": ["Der", "lie\u00df", "sein", "Trau\u00b7ren", "hier", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er eilt, die Kutte anzulegen,", "tokens": ["Er", "eilt", ",", "die", "Kut\u00b7te", "an\u00b7zu\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er pr\u00fcft das Spiel mit dumpfen Schl\u00e4gen:", "tokens": ["Er", "pr\u00fcft", "das", "Spiel", "mit", "dum\u00b7pfen", "Schl\u00e4\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u201ewie lange werd\u2019 ich, fern der S\u00fc\u00dfen,", "tokens": ["\u201e", "wie", "lan\u00b7ge", "werd'", "ich", ",", "fern", "der", "S\u00fc\u00b7\u00dfen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADV", "VAFIN", "PPER", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf dieser Harfe spielen m\u00fcssen?\u201c", "tokens": ["Auf", "die\u00b7ser", "Har\u00b7fe", "spie\u00b7len", "m\u00fcs\u00b7sen", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "VMINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}