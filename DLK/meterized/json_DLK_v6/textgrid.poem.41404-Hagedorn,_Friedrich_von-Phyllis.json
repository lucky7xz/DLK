{"textgrid.poem.41404": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Phyllis", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einem Thal, wo den verj\u00fcngten Hain", "tokens": ["In", "ei\u00b7nem", "Thal", ",", "wo", "den", "ver\u00b7j\u00fcng\u00b7ten", "Hain"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Fr\u00fchling schm\u00fcckt, ein klarer Bach benetzet,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "schm\u00fcckt", ",", "ein", "kla\u00b7rer", "Bach", "be\u00b7net\u00b7zet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fand Phyllis sich zur muntern Doris ein,", "tokens": ["Fand", "Phyl\u00b7lis", "sich", "zur", "mun\u00b7tern", "Do\u00b7ris", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PRF", "APPRART", "ADJA", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die sich bereits ins Gr\u00fcne hingesetzet.", "tokens": ["Die", "sich", "be\u00b7reits", "ins", "Gr\u00fc\u00b7ne", "hin\u00b7ge\u00b7set\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ihr schwarzes Haar, und ihre wei\u00dfe Brust", "tokens": ["Ihr", "schwar\u00b7zes", "Haar", ",", "und", "ih\u00b7re", "wei\u00b7\u00dfe", "Brust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Reizt' unverh\u00fcllt, und lie\u00df den Westwind spielen;", "tokens": ["Reizt'", "un\u00b7ver\u00b7h\u00fcllt", ",", "und", "lie\u00df", "den", "West\u00b7wind", "spie\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Den leichten West besch\u00e4ftigte die Lust,", "tokens": ["Den", "leich\u00b7ten", "West", "be\u00b7sch\u00e4f\u00b7tig\u00b7te", "die", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wenn jede sprach, sie flatternd abzuk\u00fchlen.", "tokens": ["Wenn", "je\u00b7de", "sprach", ",", "sie", "flat\u00b7ternd", "ab\u00b7zu\u00b7k\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "PPER", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}