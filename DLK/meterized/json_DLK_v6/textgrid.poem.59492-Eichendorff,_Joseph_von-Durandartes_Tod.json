{"textgrid.poem.59492": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Durandartes Tod", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbo Belerma, o Belerma,", "tokens": ["\u00bb", "o", "Be\u00b7ler\u00b7ma", ",", "o", "Be\u00b7ler\u00b7ma", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "$,", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du geboren mir zum Unheil!", "tokens": ["Du", "ge\u00b7bo\u00b7ren", "mir", "zum", "Un\u00b7heil", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieben Jahr dient ich dir treulich,", "tokens": ["Sie\u00b7ben", "Jahr", "dient", "ich", "dir", "treu\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Hab mir doch kein Lieb errungen,", "tokens": ["Hab", "mir", "doch", "kein", "Lieb", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und jetzt, da du mich erh\u00f6rtest,", "tokens": ["Und", "jetzt", ",", "da", "du", "mich", "er\u00b7h\u00f6r\u00b7test", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Mu\u00df ich in der Schlacht verbluten.", "tokens": ["Mu\u00df", "ich", "in", "der", "Schlacht", "ver\u00b7blu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nicht die Todesstimmen f\u00fcrcht ich,", "tokens": ["Nicht", "die", "To\u00b7dess\u00b7tim\u00b7men", "f\u00fcrcht", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn sie auch so fr\u00fch mich rufen,", "tokens": ["Wenn", "sie", "auch", "so", "fr\u00fch", "mich", "ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Darum nur ist Tod so bitter,", "tokens": ["Da\u00b7rum", "nur", "ist", "Tod", "so", "bit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VAFIN", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Weil er mir dein Bild verdunkelt.", "tokens": ["Weil", "er", "mir", "dein", "Bild", "ver\u00b7dun\u00b7kelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "O mein Vetter Montesinos,", "tokens": ["O", "mein", "Vet\u00b7ter", "Mon\u00b7te\u00b7si\u00b7nos", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Wenn sich meine Seel entschwungen,", "tokens": ["Wenn", "sich", "mei\u00b7ne", "Seel", "ent\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Bringt mein Herze zu Belerma,", "tokens": ["Bringt", "mein", "Her\u00b7ze", "zu", "Be\u00b7ler\u00b7ma", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "VVFIN", "APPR", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Wollt ihr meinetwegen huld'gen,", "tokens": ["Wollt", "ihr", "mei\u00b7net\u00b7we\u00b7gen", "huld'\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Bitten, da\u00df sie mein gedenke,", "tokens": ["Bit\u00b7ten", ",", "da\u00df", "sie", "mein", "ge\u00b7den\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Der so treu um sie gerungen.", "tokens": ["Der", "so", "treu", "um", "sie", "ge\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Gebt ihr alle meine L\u00e4nder,", "tokens": ["Gebt", "ihr", "al\u00b7le", "mei\u00b7ne", "L\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Die ich freudig einst bezwungen;", "tokens": ["Die", "ich", "freu\u00b7dig", "einst", "be\u00b7zwun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Da mein Lieb nun untergehet,", "tokens": ["Da", "mein", "Lieb", "nun", "un\u00b7ter\u00b7ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Sei all Gut mit ihr versunken! \u2013", "tokens": ["Sei", "all", "Gut", "mit", "ihr", "ver\u00b7sun\u00b7ken", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PIAT", "NN", "APPR", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Montesinos, Montesinos,", "tokens": ["Mon\u00b7te\u00b7si\u00b7nos", ",", "Mon\u00b7te\u00b7si\u00b7nos", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.22": {"text": "Hei\u00df brennt diese Lanzenwunde,", "tokens": ["Hei\u00df", "brennt", "die\u00b7se", "Lan\u00b7zen\u00b7wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "M\u00fcde schon ist meine Rechte,", "tokens": ["M\u00fc\u00b7de", "schon", "ist", "mei\u00b7ne", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Aus viel Quellen hier verblut ich,", "tokens": ["Aus", "viel", "Quel\u00b7len", "hier", "ver\u00b7blut", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "'s wird so k\u00fchl nun \u2013 ach die Augen,", "tokens": ["'s", "wird", "so", "k\u00fchl", "nun", "\u2013", "ach", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADV", "$(", "XY", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Die uns ausziehn sahn so mutig,", "tokens": ["Die", "uns", "aus\u00b7ziehn", "sahn", "so", "mu\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVIZU", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Sehn uns nimmermehr in Frankreich. \u2013", "tokens": ["Sehn", "uns", "nim\u00b7mer\u00b7mehr", "in", "Fran\u00b7kreich", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NE", "$.", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.28": {"text": "Dr\u00fcckt noch einmal an die Brust mich,", "tokens": ["Dr\u00fcckt", "noch", "ein\u00b7mal", "an", "die", "Brust", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Vetter, denn ich sprech verworren", "tokens": ["Vet\u00b7ter", ",", "denn", "ich", "sprech", "ver\u00b7wor\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Und vor meinen Augen dunkelt's,", "tokens": ["Und", "vor", "mei\u00b7nen", "Au\u00b7gen", "dun\u00b7kelt's", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Euch befehl ich all mein Sorgen", "tokens": ["Euch", "be\u00b7fehl", "ich", "all", "mein", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Und vertraue Eurem Schwure,", "tokens": ["Und", "ver\u00b7trau\u00b7e", "Eu\u00b7rem", "Schwu\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Denn der Herr, an den Ihr glaubet,", "tokens": ["Denn", "der", "Herr", ",", "an", "den", "Ihr", "glau\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "ART", "PPER", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.34": {"text": "H\u00f6ret uns in dieser Stunde.\u00ab", "tokens": ["H\u00f6\u00b7ret", "uns", "in", "die\u00b7ser", "Stun\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Tot nun ruhet Durandarte", "tokens": ["Tot", "nun", "ru\u00b7het", "Du\u00b7rand\u00b7ar\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem stillen Felsengrunde,", "tokens": ["In", "dem", "stil\u00b7len", "Fel\u00b7sen\u00b7grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weinend l\u00f6st ihm Montesinos", "tokens": ["Wei\u00b7nend", "l\u00f6st", "ihm", "Mon\u00b7te\u00b7si\u00b7nos"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Helm und seiner R\u00fcstung Gurte,", "tokens": ["Helm", "und", "sei\u00b7ner", "R\u00fcs\u00b7tung", "Gur\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "L\u00f6st sein Herze f\u00fcr Belerma", "tokens": ["L\u00f6st", "sein", "Her\u00b7ze", "f\u00fcr", "Be\u00b7ler\u00b7ma"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAINF", "VVFIN", "APPR", "NE"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Mit dem Dolche aus der Brust ihm", "tokens": ["Mit", "dem", "Dol\u00b7che", "aus", "der", "Brust", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und begrub ihn unterm Felsen,", "tokens": ["Und", "be\u00b7grub", "ihn", "un\u00b7term", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sprach dabei aus Herzensgrunde:", "tokens": ["Sprach", "da\u00b7bei", "aus", "Her\u00b7zens\u00b7grun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "\u00bbo mein Vetter Durandarte,", "tokens": ["\u00bb", "o", "mein", "Vet\u00b7ter", "Du\u00b7rand\u00b7ar\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Tapfrer Degen, Herzensbruder,", "tokens": ["Tapf\u00b7rer", "De\u00b7gen", ",", "Her\u00b7zens\u00b7bru\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Was soll ich fortan auf Erden,", "tokens": ["Was", "soll", "ich", "for\u00b7tan", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.12": {"text": "Da die Mohren dich erschlugen!\u00ab", "tokens": ["Da", "die", "Moh\u00b7ren", "dich", "er\u00b7schlu\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}