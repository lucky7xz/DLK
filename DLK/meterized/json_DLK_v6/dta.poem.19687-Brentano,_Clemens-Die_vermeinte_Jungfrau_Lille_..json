{"dta.poem.19687": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Die vermeinte Jungfrau Lille .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Prinz Eugen. Lill, du allersch\u00f6nste Stadt,               ", "tokens": ["Prinz", "Eu\u00b7gen", ".", "Lill", ",", "du", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Stadt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "NE", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die du bist so fein und glat,", "tokens": ["Die", "du", "bist", "so", "fein", "und", "glat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Lieb, die brennt in Flammen,", "tokens": ["Mei\u00b7ne", "Lieb", ",", "die", "brennt", "in", "Flam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich lieb ich vor allen Damen,", "tokens": ["Dich", "lieb", "ich", "vor", "al\u00b7len", "Da\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lill, du allersch\u00f6nste Stadt.", "tokens": ["Lill", ",", "du", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Stadt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Stadt Lille. Lieber Herr, was saget ihr,               ", "tokens": ["Stadt", "Lil\u00b7le", ".", "Lie\u00b7ber", "Herr", ",", "was", "sa\u00b7get", "ihr", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "ADJD", "NN", "$,", "PWS", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wer seyd ihr, was macht ihr hier,", "tokens": ["Wer", "seyd", "ihr", ",", "was", "macht", "ihr", "hier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "PWS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Was die Reiter, die Soldaten,", "tokens": ["Was", "die", "Rei\u00b7ter", ",", "die", "Sol\u00b7da\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eure tapfern Kameraden,", "tokens": ["Eu\u00b7re", "tap\u00b7fern", "Ka\u00b7me\u00b7ra\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebster das erz\u00e4hlet mir?", "tokens": ["Liebs\u00b7ter", "das", "er\u00b7z\u00e4h\u00b7let", "mir", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Prinz Eugen. Ich bin der Savoyer Held               ", "tokens": ["Prinz", "Eu\u00b7gen", ".", "Ich", "bin", "der", "Sa\u00b7vo\u00b7yer", "Held"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$.", "PPER", "VAFIN", "ART", "NN", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bekannt genug in aller Welt,", "tokens": ["Be\u00b7kannt", "ge\u00b7nug", "in", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Prinz Eugen bin ich genennet,", "tokens": ["Prinz", "Eu\u00b7gen", "bin", "ich", "ge\u00b7nen\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+----", "measure": "unknown.measure.di"}, "line.4": {"text": "Der zu dir in Liebe brennet,", "tokens": ["Der", "zu", "dir", "in", "Lie\u00b7be", "bren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lill, du allersch\u00f6nste Braut.", "tokens": ["Lill", ",", "du", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Braut", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Stadt Lille. Lieber Herr, fort packet euch,               ", "tokens": ["Stadt", "Lil\u00b7le", ".", "Lie\u00b7ber", "Herr", ",", "fort", "pa\u00b7cket", "euch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "ADJD", "NN", "$,", "PTKVZ", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gehet in das deutsche Reich,", "tokens": ["Ge\u00b7het", "in", "das", "deut\u00b7sche", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Denn ich habe zum Galanten,", "tokens": ["Denn", "ich", "ha\u00b7be", "zum", "Ga\u00b7lan\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zum Gemahl und Caressanten,", "tokens": ["Zum", "Ge\u00b7mahl", "und", "Ca\u00b7res\u00b7san\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6nig Ludwig von Frankreich.", "tokens": ["K\u00f6\u00b7nig", "Lud\u00b7wig", "von", "Fran\u00b7kreich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Prinz Eugen. Liebste deine Sch\u00f6nheit gro\u00df               ", "tokens": ["Prinz", "Eu\u00b7gen", ".", "Liebs\u00b7te", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "gro\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$.", "NN", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ziehet mich in deinen Schoo\u00df,", "tokens": ["Zie\u00b7het", "mich", "in", "dei\u00b7nen", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit Gewalt will bey dir schlafen,", "tokens": ["Mit", "Ge\u00b7walt", "will", "bey", "dir", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schrecken dich nicht meine Waffen,", "tokens": ["Schre\u00b7cken", "dich", "nicht", "mei\u00b7ne", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Machen Hochzeitfeuer an.", "tokens": ["Ma\u00b7chen", "Hoch\u00b7zeit\u00b7feu\u00b7er", "an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Stadt Lille. Lieber Herr von gro\u00dfer Macht,               ", "tokens": ["Stadt", "Lil\u00b7le", ".", "Lie\u00b7ber", "Herr", "von", "gro\u00b7\u00dfer", "Macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "ADJD", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Glaubet mir, ihr seyd verlacht,", "tokens": ["Glau\u00b7bet", "mir", ",", "ihr", "seyd", "ver\u00b7lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Werk und Bastionen", "tokens": ["Mei\u00b7ne", "Werk", "und", "Bas\u00b7tion\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Citadell und halbe Monden,", "tokens": ["Ci\u00b7ta\u00b7dell", "und", "hal\u00b7be", "Mon\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bouffler sch\u00fctzet meine Ehr.", "tokens": ["Bouf\u00b7fler", "sch\u00fct\u00b7zet", "mei\u00b7ne", "Ehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Prinz Eugen. Halt das Maul und schweige still,               ", "tokens": ["Prinz", "Eu\u00b7gen", ".", "Halt", "das", "Maul", "und", "schwei\u00b7ge", "still", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "NN", "ART", "NN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "H\u00f6r was ich dir sagen will,", "tokens": ["H\u00f6r", "was", "ich", "dir", "sa\u00b7gen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWS", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hab ich nicht in Ungerlanden", "tokens": ["Hab", "ich", "nicht", "in", "Un\u00b7ger\u00b7lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PTKNEG", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00fcrken schon gemacht zu schanden,", "tokens": ["T\u00fcr\u00b7ken", "schon", "ge\u00b7macht", "zu", "schan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hundert tausend, noch viel mehr.", "tokens": ["Hun\u00b7dert", "tau\u00b7send", ",", "noch", "viel", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "CARD", "$,", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Stadt Lille. Lieber Herr, das glaub ich wohl,               ", "tokens": ["Stadt", "Lil\u00b7le", ".", "Lie\u00b7ber", "Herr", ",", "das", "glaub", "ich", "wohl", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "ADJD", "NN", "$,", "PDS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ihr damals waret toll,", "tokens": ["Da\u00df", "ihr", "da\u00b7mals", "wa\u00b7ret", "toll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber ihr habt nichts zu schaffen", "tokens": ["A\u00b7ber", "ihr", "habt", "nichts", "zu", "schaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PIS", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jezo mit den t\u00fcrkschen Affen,", "tokens": ["Je\u00b7zo", "mit", "den", "t\u00fcrk\u00b7schen", "Af\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sondern mit dem Lilien Glanz.", "tokens": ["Son\u00b7dern", "mit", "dem", "Li\u00b7li\u00b7en", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.9": {"line.1": {"text": "Prinz Eugen. Ihr Constabler frisch daran,               ", "tokens": ["Prinz", "Eu\u00b7gen", ".", "Ihr", "Cons\u00b7tab\u00b7ler", "frisch", "da\u00b7ran", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "PPOSAT", "NN", "ADJD", "PAV", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Feuert hundert tausend Mann,", "tokens": ["Feu\u00b7ert", "hun\u00b7dert", "tau\u00b7send", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "CARD", "CARD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Donnert da\u00df es kracht in Flammen,", "tokens": ["Don\u00b7nert", "da\u00df", "es", "kracht", "in", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df kein Stein h\u00e4lt mehr zusammen,", "tokens": ["Da\u00df", "kein", "Stein", "h\u00e4lt", "mehr", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lill, du ungl\u00fcckselig Weib.", "tokens": ["Lill", ",", "du", "un\u00b7gl\u00fcck\u00b7se\u00b7lig", "Weib", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJD", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.10": {"line.1": {"text": "Stadt Lille. Meint ihr denn, da\u00df mein Vandom,               ", "tokens": ["Stadt", "Lil\u00b7le", ".", "Meint", "ihr", "denn", ",", "da\u00df", "mein", "Van\u00b7dom", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mir nicht bald zu H\u00fclfe komm,", "tokens": ["Mir", "nicht", "bald", "zu", "H\u00fcl\u00b7fe", "komm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der mit hundert tausend Franzen,", "tokens": ["Der", "mit", "hun\u00b7dert", "tau\u00b7send", "Fran\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "CARD", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den Holl\u00e4ndern lehrt das Tanzen,", "tokens": ["Den", "Hol\u00b7l\u00e4n\u00b7dern", "lehrt", "das", "Tan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Eh mein Kr\u00e4nzlein mir verbrannt.", "tokens": ["Eh", "mein", "Kr\u00e4nz\u00b7lein", "mir", "ver\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Prinz Eugen. Lill, mein Engel und mein Lamm,               ", "tokens": ["Prinz", "Eu\u00b7gen", ".", "Lill", ",", "mein", "En\u00b7gel", "und", "mein", "Lamm", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "NE", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich wei\u00df dir den Br\u00e4utigam,", "tokens": ["Ich", "wei\u00df", "dir", "den", "Br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Kaiser Karl, der Weltbekannte,", "tokens": ["Kai\u00b7ser", "Karl", ",", "der", "Welt\u00b7be\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich bin nur sein Abgesandte,", "tokens": ["Ich", "bin", "nur", "sein", "Ab\u00b7ge\u00b7sand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und des Kaisers General.", "tokens": ["Und", "des", "Kai\u00b7sers", "Ge\u00b7ne\u00b7ral", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Stadt Lille. Ey wohlan, so la\u00df es seyn,               ", "tokens": ["Stadt", "Lil\u00b7le", ".", "Ey", "wo\u00b7hlan", ",", "so", "la\u00df", "es", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "NN", "ADV", "$,", "ADV", "VVIMP", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Karle sey der Liebste mein,", "tokens": ["Kar\u00b7le", "sey", "der", "Liebs\u00b7te", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn der Ludewig veraltet,", "tokens": ["Denn", "der", "Lu\u00b7de\u00b7wig", "ver\u00b7al\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Lieb ist ganz erkaltet,", "tokens": ["Und", "die", "Lieb", "ist", "ganz", "er\u00b7kal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Karl ist noch ein junger Held.", "tokens": ["Karl", "ist", "noch", "ein", "jun\u00b7ger", "Held", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}