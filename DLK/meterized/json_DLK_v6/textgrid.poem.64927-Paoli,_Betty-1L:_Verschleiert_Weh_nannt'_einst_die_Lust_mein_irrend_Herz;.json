{"textgrid.poem.64927": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verschleiert Weh nannt' einst die Lust mein irrend Herz;", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verschleiert Weh nannt' einst die Lust mein irrend Herz;", "tokens": ["Ver\u00b7schlei\u00b7ert", "Weh", "nannt'", "einst", "die", "Lust", "mein", "ir\u00b7rend", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verh\u00fcllte Lust nenn' ich nunmehr jedweden Schmerz.", "tokens": ["Ver\u00b7h\u00fcll\u00b7te", "Lust", "nenn'", "ich", "nun\u00b7mehr", "jed\u00b7we\u00b7den", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Lust, die, um unser \u00c4ug' nicht blendend zu erschrecken,", "tokens": ["Lust", ",", "die", ",", "um", "un\u00b7ser", "\u00c4ug'", "nicht", "blen\u00b7dend", "zu", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "$,", "KOUI", "PPOSAT", "NN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit schwarzem Schleier mu\u00df zu hohe Sch\u00f6nheit decken.", "tokens": ["Mit", "schwar\u00b7zem", "Schlei\u00b7er", "mu\u00df", "zu", "ho\u00b7he", "Sch\u00f6n\u00b7heit", "de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O blick dem Schmerz nur fest in's dunkle Angesicht,", "tokens": ["O", "blick", "dem", "Schmerz", "nur", "fest", "in's", "dunk\u00b7le", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADV", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann dringst du durch den Flor und schauest Gottes Licht!", "tokens": ["Dann", "dringst", "du", "durch", "den", "Flor", "und", "schau\u00b7est", "Got\u00b7tes", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "KON", "VVFIN", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}