{"textgrid.poem.26210": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Es liegt bei mir und schreit des gro\u00dfen Krieges Not", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mu\u00df h\u00f6ren durch die Nacht das St\u00f6hnen und das Stampfen,", "tokens": ["Mu\u00df", "h\u00f6\u00b7ren", "durch", "die", "Nacht", "das", "St\u00f6h\u00b7nen", "und", "das", "Stamp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Pfiff von Sch\u00fcssen, die im Flug zerst\u00f6ren,", "tokens": ["Den", "Pfiff", "von", "Sch\u00fcs\u00b7sen", ",", "die", "im", "Flug", "zer\u00b7st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Schlag der F\u00e4uste, die im Kampf sich krampfen,", "tokens": ["Den", "Schlag", "der", "F\u00e4us\u00b7te", ",", "die", "im", "Kampf", "sich", "kramp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die wie die Wurzeln werden hart und tot \u2013", "tokens": ["Die", "wie", "die", "Wur\u00b7zeln", "wer\u00b7den", "hart", "und", "tot", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es liegt bei mir und schreit des gro\u00dfen Krieges Not.", "tokens": ["Es", "liegt", "bei", "mir", "und", "schreit", "des", "gro\u00b7\u00dfen", "Krie\u00b7ges", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "KON", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}