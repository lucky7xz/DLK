{"textgrid.poem.65471": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "44. Oberfl\u00e4chlichkeit des Lebens", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Wolkenbilder \u00fcber den Flu\u00df, so schweben die Menschen durch's Leben:", "tokens": ["Wie", "Wol\u00b7ken\u00b7bil\u00b7der", "\u00fc\u00b7ber", "den", "Flu\u00df", ",", "so", "schwe\u00b7ben", "die", "Men\u00b7schen", "durch's", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Sie wollen sich \u00fcber die Fl\u00e4che nicht auf Fl\u00fcgeln gen Himmel erheben,", "tokens": ["Sie", "wol\u00b7len", "sich", "\u00fc\u00b7ber", "die", "Fl\u00e4\u00b7che", "nicht", "auf", "Fl\u00fc\u00b7geln", "gen", "Him\u00b7mel", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "ART", "NN", "PTKNEG", "APPR", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-+--+--+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sie wollen sich auch mit ihrem Gewicht hinab in die Tiefe nicht tauchen,", "tokens": ["Sie", "wol\u00b7len", "sich", "auch", "mit", "ih\u00b7rem", "Ge\u00b7wicht", "hin\u00b7ab", "in", "die", "Tie\u00b7fe", "nicht", "tau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+--+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Drum kann ein jeglicher Wind so leicht ihr Alles in Nichts verhauchen.", "tokens": ["Drum", "kann", "ein", "jeg\u00b7li\u00b7cher", "Wind", "so", "leicht", "ihr", "Al\u00b7les", "in", "Nichts", "ver\u00b7hau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ART", "PIAT", "NN", "ADV", "ADJD", "PPER", "PIS", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+--+-+-+--+-+-", "measure": "iambic.septa.relaxed"}}}}}