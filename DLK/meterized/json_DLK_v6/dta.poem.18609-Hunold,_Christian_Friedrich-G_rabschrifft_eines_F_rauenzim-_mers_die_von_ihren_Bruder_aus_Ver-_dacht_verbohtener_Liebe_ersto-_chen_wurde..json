{"dta.poem.18609": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "G rabschrifft eines  F rauenzim-  \n mers/ die von ihren Bruder aus Ver-  \n dacht verbohtener Liebe ersto-  \n chen wurde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.85", "da:0.14"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel billigte selbst meine keusche Flammen/", "tokens": ["Der", "Him\u00b7mel", "bil\u00b7lig\u00b7te", "selbst", "mei\u00b7ne", "keu\u00b7sche", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Daran der Bruder doch zum M\u00f6rder worden ist.", "tokens": ["Da\u00b7ran", "der", "Bru\u00b7der", "doch", "zum", "M\u00f6r\u00b7der", "wor\u00b7den", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "APPRART", "NN", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So kan ein falscher Wahn die Tugenden verdammen", "tokens": ["So", "kan", "ein", "fal\u00b7scher", "Wahn", "die", "Tu\u00b7gen\u00b7den", "ver\u00b7dam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer solche nur allein nach seinen Lastern mi\u00dft.", "tokens": ["Wer", "sol\u00b7che", "nur", "al\u00b7lein", "nach", "sei\u00b7nen", "Las\u00b7tern", "mi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}