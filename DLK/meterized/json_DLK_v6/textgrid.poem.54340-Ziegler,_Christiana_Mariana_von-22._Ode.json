{"textgrid.poem.54340": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "22. Ode", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe l\u00e4ngst mein Seitenspiel", "tokens": ["Ich", "ha\u00b7be", "l\u00e4ngst", "mein", "Sei\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Vorbedacht von mir geschmissen,", "tokens": ["Mit", "Vor\u00b7be\u00b7dacht", "von", "mir", "ge\u00b7schmis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und, weil es mir nicht mehr gefiel,", "tokens": ["Und", ",", "weil", "es", "mir", "nicht", "mehr", "ge\u00b7fiel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von ihm nichts weiter wollen wissen.", "tokens": ["Von", "ihm", "nichts", "wei\u00b7ter", "wol\u00b7len", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PIS", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn eine schwache Frauenhand", "tokens": ["Denn", "ei\u00b7ne", "schwa\u00b7che", "Frau\u00b7en\u00b7hand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fchlt nicht das Feur, den heissen Brand,", "tokens": ["F\u00fchlt", "nicht", "das", "Feur", ",", "den", "heis\u00b7sen", "Brand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der sich in M\u00e4nneradern reget.", "tokens": ["Der", "sich", "in", "M\u00e4n\u00b7ne\u00b7ra\u00b7dern", "re\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wir spielen, doch der Thon klingt matt,", "tokens": ["Wir", "spie\u00b7len", ",", "doch", "der", "Thon", "klingt", "matt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dieweil er nichts von Anmuth hat,", "tokens": ["Die\u00b7weil", "er", "nichts", "von", "An\u00b7muth", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und weder Geist noch Leben heget.", "tokens": ["Und", "we\u00b7der", "Geist", "noch", "Le\u00b7ben", "he\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KON", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Allein, die Muse winket mir;", "tokens": ["Al\u00b7lein", ",", "die", "Mu\u00b7se", "win\u00b7ket", "mir", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie zieht die fast zersprengten Seiten", "tokens": ["Sie", "zieht", "die", "fast", "zer\u00b7spreng\u00b7ten", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Aus ihrem Kerker heut herf\u00fcr,", "tokens": ["Aus", "ih\u00b7rem", "Ker\u00b7ker", "heut", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und will sie wieder zubereiten;", "tokens": ["Und", "will", "sie", "wie\u00b7der", "zu\u00b7be\u00b7rei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie hilft und stimmt selbst mit daran,", "tokens": ["Sie", "hilft", "und", "stimmt", "selbst", "mit", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "APPR", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und flammt mich ganz gewaltig an", "tokens": ["Und", "flammt", "mich", "ganz", "ge\u00b7wal\u00b7tig", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dies mal so Mund als Hand zu zwingen.", "tokens": ["Dies", "mal", "so", "Mund", "als", "Hand", "zu", "zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "NN", "KOUS", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie? spricht sie, solltst du bey dem Fest,", "tokens": ["Wie", "?", "spricht", "sie", ",", "solltst", "du", "bey", "dem", "Fest", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das dich das Gl\u00fcck erblicken l\u00e4\u00dft,", "tokens": ["Das", "dich", "das", "Gl\u00fcck", "er\u00b7bli\u00b7cken", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die holde Freundinn nicht besingen?", "tokens": ["Die", "hol\u00b7de", "Freun\u00b7dinn", "nicht", "be\u00b7sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf! und vers\u00e4ume nicht die Zeit,", "tokens": ["Auf", "!", "und", "ver\u00b7s\u00e4u\u00b7me", "nicht", "die", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "KON", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den so vergn\u00fcgten Tag zu feyren,", "tokens": ["Den", "so", "ver\u00b7gn\u00fcg\u00b7ten", "Tag", "zu", "fey\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "An dem die \u2013 \u2013 h\u00f6chst erfreut", "tokens": ["An", "dem", "die", "\u2013", "\u2013", "h\u00f6chst", "er\u00b7freut"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["APPR", "ART", "ART", "$(", "$(", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Will ihrer Jahre Lauf verneuren.", "tokens": ["Will", "ih\u00b7rer", "Jah\u00b7re", "Lauf", "ver\u00b7neu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie wird von dir stets hochgesch\u00e4tzt,", "tokens": ["Sie", "wird", "von", "dir", "stets", "hoch\u00b7ge\u00b7sch\u00e4tzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil dich ihr Umgang so ergetzt,", "tokens": ["Weil", "dich", "ihr", "Um\u00b7gang", "so", "er\u00b7getzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der aller Beyfall hier erreichet;", "tokens": ["Der", "al\u00b7ler", "Bey\u00b7fall", "hier", "er\u00b7rei\u00b7chet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bring ihr ein aufgewecktes Lied,", "tokens": ["Bring", "ihr", "ein", "auf\u00b7ge\u00b7weck\u00b7tes", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das unsrer \u2013 \u2013 \u00e4hnlich sieht,", "tokens": ["Das", "uns\u00b7rer", "\u2013", "\u2013", "\u00e4hn\u00b7lich", "sieht", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "$(", "$(", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Und ihrem muntern Geiste gleichet.", "tokens": ["Und", "ih\u00b7rem", "mun\u00b7tern", "Geis\u00b7te", "glei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch dieses eben ist es auch,", "tokens": ["Doch", "die\u00b7ses", "e\u00b7ben", "ist", "es", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was mich bey so gestalten Sachen", "tokens": ["Was", "mich", "bey", "so", "ge\u00b7stal\u00b7ten", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ADV", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Will wider meinen alten Brauch,", "tokens": ["Will", "wi\u00b7der", "mei\u00b7nen", "al\u00b7ten", "Brauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geliebte Freundin! furchtsam machen.", "tokens": ["Ge\u00b7lieb\u00b7te", "Freun\u00b7din", "!", "furcht\u00b7sam", "ma\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Betracht ich Dich, und deinen Geist,", "tokens": ["Be\u00b7tracht", "ich", "Dich", ",", "und", "dei\u00b7nen", "Geist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den man als unvergleichlich preist,", "tokens": ["Den", "man", "als", "un\u00b7ver\u00b7gleich\u00b7lich", "preist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "KOKOM", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So scheu ich mich Dich abzuschildern.", "tokens": ["So", "scheu", "ich", "mich", "Dich", "ab\u00b7zu\u00b7schil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dein Conterfey das aller Welt,", "tokens": ["Dein", "Con\u00b7ter\u00b7fey", "das", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Anmuthigste! so wohl gef\u00e4llt,", "tokens": ["An\u00b7mut\u00b7higs\u00b7te", "!", "so", "wohl", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "Gleicht \u00fcberhaupt den Tugendbildern.", "tokens": ["Gleicht", "\u00fc\u00b7ber\u00b7haupt", "den", "Tu\u00b7gend\u00b7bil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Woher n\u00e4hm ich so Geist als Glut,", "tokens": ["Wo\u00b7her", "n\u00e4hm", "ich", "so", "Geist", "als", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den deinigen recht auszudr\u00fccken?", "tokens": ["Den", "dei\u00b7ni\u00b7gen", "recht", "aus\u00b7zu\u00b7dr\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSS", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn was in deiner Seele ruht,", "tokens": ["Denn", "was", "in", "dei\u00b7ner", "See\u00b7le", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "L\u00e4\u00dft uns viel feuriges erblicken.", "tokens": ["L\u00e4\u00dft", "uns", "viel", "feu\u00b7ri\u00b7ges", "er\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Wesen, das sich uns entdeckt,", "tokens": ["Dein", "We\u00b7sen", ",", "das", "sich", "uns", "ent\u00b7deckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PRF", "PPER", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Ist stets belebt und aufgeweckt,", "tokens": ["Ist", "stets", "be\u00b7lebt", "und", "auf\u00b7ge\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch auch mit Sittsamkeit verbunden.", "tokens": ["Doch", "auch", "mit", "Sitt\u00b7sam\u00b7keit", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und der so aufger\u00e4umte Sinn", "tokens": ["Und", "der", "so", "auf\u00b7ge\u00b7r\u00e4um\u00b7te", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Von dem ich selbst ein Zeuge bin,", "tokens": ["Von", "dem", "ich", "selbst", "ein", "Zeu\u00b7ge", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Vers\u00fcsset uns so manche Stunden.", "tokens": ["Ver\u00b7s\u00fcs\u00b7set", "uns", "so", "man\u00b7che", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der holden Minen Freundlichkeit,", "tokens": ["Der", "hol\u00b7den", "Mi\u00b7nen", "Freund\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weis aller Huld Dir zu gewinnen.", "tokens": ["Weis", "al\u00b7ler", "Huld", "Dir", "zu", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die zeigt uns auch zu dieser Zeit", "tokens": ["Die", "zeigt", "uns", "auch", "zu", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Kleeblat dreyer Charitinnen.", "tokens": ["Das", "Klee\u00b7blat", "drey\u00b7er", "Cha\u00b7ri\u00b7tin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was nur die Damen zieren kann,", "tokens": ["Was", "nur", "die", "Da\u00b7men", "zie\u00b7ren", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": [