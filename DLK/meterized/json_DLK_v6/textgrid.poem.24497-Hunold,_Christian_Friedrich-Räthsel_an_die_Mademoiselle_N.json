{"textgrid.poem.24497": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "R\u00e4thsel an die Mademoiselle N", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Mensch von wei\u00dfen Haar und feurigem Gesichte/", "tokens": ["Ein", "Mensch", "von", "wei\u00b7\u00dfen", "Haar", "und", "feu\u00b7ri\u00b7gem", "Ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bat den ", "tokens": ["Bat", "den"], "token_info": ["word", "word"], "pos": ["VVFIN", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Auff ein annehmlich Kind die G\u00fcldne Pfeile scho\u00df.", "tokens": ["Auff", "ein", "an\u00b7nehm\u00b7lich", "Kind", "die", "G\u00fcld\u00b7ne", "Pfei\u00b7le", "scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er r\u00fchrte zwar ihr Hertz/ doch gab sie sich nicht blo\u00df.", "tokens": ["Er", "r\u00fchr\u00b7te", "zwar", "ihr", "Hertz", "/", "doch", "gab", "sie", "sich", "nicht", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$(", "ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Allein das Wunder-Thier/ ein Asse nahm sie wieder/", "tokens": ["Al\u00b7lein", "das", "Wun\u00b7der\u00b7Thier", "/", "ein", "As\u00b7se", "nahm", "sie", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$(", "ART", "NN", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und scho\u00df vor einen Kerl/ der schwartz gekleidet war", "tokens": ["Und", "scho\u00df", "vor", "ei\u00b7nen", "Kerl", "/", "der", "schwartz", "ge\u00b7klei\u00b7det", "war"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$(", "ART", "ADJD", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den Pfeil in ihre Brust/ da\u00df sie im dunckeln gar/", "tokens": ["Den", "Pfeil", "in", "ih\u00b7re", "Brust", "/", "da\u00df", "sie", "im", "dun\u00b7ckeln", "gar", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$(", "KOUS", "PPER", "APPRART", "ADJA", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sehr lange bey ihm blieb/ sich blo\u00df gab/ wolt ihrs wi\u00dfen?", "tokens": ["Sehr", "lan\u00b7ge", "bey", "ihm", "blieb", "/", "sich", "blo\u00df", "gab", "/", "wolt", "ihrs", "wi\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "VVFIN", "$(", "PRF", "ADV", "VVFIN", "$(", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein Mohr erquickte da ein sch\u00f6nes Kind mit k\u00fc\u00dfen.", "tokens": ["Ein", "Mohr", "er\u00b7quick\u00b7te", "da", "ein", "sch\u00f6\u00b7nes", "Kind", "mit", "k\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Crr\u00e4th dein Kluger Sinn/ was mein Gem\u00fcth will schreiben/", "tokens": ["Cr\u00b7r\u00e4th", "dein", "Klu\u00b7ger", "Sinn", "/", "was", "mein", "Ge\u00b7m\u00fcth", "will", "schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$(", "PWS", "PPOSAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So will ich TAG und NACHT dein Freund u. Diener bleiben.", "tokens": ["So", "will", "ich", "TaG", "und", "NaCHT", "dein", "Freund", "u.", "Die\u00b7ner", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "KON", "NE", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}