{"textgrid.poem.43029": {"metadata": {"author": {"name": "Aston, Louise", "birth": "N.A.", "death": "N.A."}, "title": "Nach der ersten Vertreibung der Berliner Volksvertreter", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Schauspiel ist zu Ende \u2013", "tokens": ["Das", "Schau\u00b7spiel", "ist", "zu", "En\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Henker hat sein Theil;", "tokens": ["Der", "Hen\u00b7ker", "hat", "sein", "Theil", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Haupt Maria Stuart's", "tokens": ["Das", "Haupt", "Ma\u00b7ria", "Stu\u00b7art's"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Fiel unter seinem Beil!", "tokens": ["Fiel", "un\u00b7ter", "sei\u00b7nem", "Beil", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbelisabeth von England,", "tokens": ["\u00bb", "e\u00b7lis\u00b7a\u00b7beth", "von", "En\u00b7gland", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nur sie ist souverain;", "tokens": ["Nur", "sie", "ist", "sou\u00b7ve\u00b7rain", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer nach der Krone tastet,", "tokens": ["Wer", "nach", "der", "Kro\u00b7ne", "tas\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mu\u00df schm\u00e4hlich untergehn!\u00ab", "tokens": ["Mu\u00df", "schm\u00e4h\u00b7lich", "un\u00b7ter\u00b7gehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbelisabeth von England", "tokens": ["\u00bb", "e\u00b7lis\u00b7a\u00b7beth", "von", "En\u00b7gland"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "APPR", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat ihre Gewalt von Gott,", "tokens": ["Hat", "ih\u00b7re", "Ge\u00b7walt", "von", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und wer sie ihr bestreitet,", "tokens": ["Und", "wer", "sie", "ihr", "be\u00b7strei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der stirbt auf dem Schaffot!\u00ab", "tokens": ["Der", "stirbt", "auf", "dem", "Schaf\u00b7fot", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "So hat man dir gerathen,", "tokens": ["So", "hat", "man", "dir", "ge\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Rathlose K\u00f6nigin!", "tokens": ["Rath\u00b7lo\u00b7se", "K\u00f6\u00b7ni\u00b7gin", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}