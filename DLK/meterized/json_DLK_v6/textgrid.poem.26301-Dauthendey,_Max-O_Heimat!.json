{"textgrid.poem.26301": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "O Heimat!", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Sehnsucht schau ich nach Westen gewandt,", "tokens": ["Mit", "Sehn\u00b7sucht", "schau", "ich", "nach", "Wes\u00b7ten", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es stirbt mein Seufzen im fremden Land,", "tokens": ["Es", "stirbt", "mein", "Seuf\u00b7zen", "im", "frem\u00b7den", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie eine Welle verl\u00e4uft im Sand.", "tokens": ["Wie", "ei\u00b7ne", "Wel\u00b7le", "ver\u00b7l\u00e4uft", "im", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Kein Weg, o Heimat, f\u00fchrt zu dir!", "tokens": ["Kein", "Weg", ",", "o", "Hei\u00b7mat", ",", "f\u00fchrt", "zu", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "FM", "NN", "$,", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur deine Sprache lebt bei mir.", "tokens": ["Nur", "dei\u00b7ne", "Spra\u00b7che", "lebt", "bei", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sonst aber bin ich toteinsam hier.", "tokens": ["Sonst", "a\u00b7ber", "bin", "ich", "to\u00b7tein\u00b7sam", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}