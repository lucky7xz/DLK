{"dta.poem.391": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Melissa  l\u00e4st sich gerne k\u00fcssen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df kein erhitzter Ku\u00df dem Frauen-Zimmer schadet/", "tokens": ["Da\u00df", "kein", "er\u00b7hitz\u00b7ter", "Ku\u00df", "dem", "Frau\u00b7en\u00b7Zim\u00b7mer", "scha\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das wei\u00df ", "tokens": ["Das", "wei\u00df"], "token_info": ["word", "word"], "pos": ["PDS", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Und lachet/ wenn der Geist im Thau der Liebe badet/", "tokens": ["Und", "la\u00b7chet", "/", "wenn", "der", "Geist", "im", "Thau", "der", "Lie\u00b7be", "ba\u00b7det", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "ART", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jhr gilt ein Ku\u00df und Gold so viel als einerley.", "tokens": ["Ihr", "gilt", "ein", "Ku\u00df", "und", "Gold", "so", "viel", "als", "ei\u00b7ner\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "NN", "ADV", "PIAT", "KOKOM", "PIAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}