{"textgrid.poem.50966": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ausgestorben scheint die Stadt,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ausgestorben scheint die Stadt,", "tokens": ["Aus\u00b7ge\u00b7stor\u00b7ben", "scheint", "die", "Stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil, was sich des Lebens freut", "tokens": ["Weil", ",", "was", "sich", "des", "Le\u00b7bens", "freut"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den Bund mit ihm erneut,", "tokens": ["Und", "den", "Bund", "mit", "ihm", "er\u00b7neut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich hinaus begeben hat", "tokens": ["Sich", "hin\u00b7aus", "be\u00b7ge\u00b7ben", "hat"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "APZR", "VVPP", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf die H\u00fcgel, auf die Berge;", "tokens": ["Auf", "die", "H\u00fc\u00b7gel", ",", "auf", "die", "Ber\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Angef\u00fcllt wird jedes Tal,", "tokens": ["An\u00b7ge\u00b7f\u00fcllt", "wird", "je\u00b7des", "Tal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "R\u00fchren mu\u00df sich Wirt und Ferge", "tokens": ["R\u00fch\u00b7ren", "mu\u00df", "sich", "Wirt", "und", "Fer\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PRF", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In dem warmen Maienstrahl.", "tokens": ["In", "dem", "war\u00b7men", "Mai\u00b7en\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Von dem h\u00f6chsten Giebel schau", "tokens": ["Von", "dem", "h\u00f6chs\u00b7ten", "Gie\u00b7bel", "schau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich hinaus, o welch Gewimmel!", "tokens": ["Ich", "hin\u00b7aus", ",", "o", "welch", "Ge\u00b7wim\u00b7mel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "FM", "PWAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, die Erde tr\u00e4gt gen Himmel", "tokens": ["Ja", ",", "die", "Er\u00b7de", "tr\u00e4gt", "gen", "Him\u00b7mel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Menschenherz und gr\u00fcne Au!", "tokens": ["Men\u00b7schen\u00b7herz", "und", "gr\u00fc\u00b7ne", "Au", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wie ferne Kirchenfahnen", "tokens": ["Und", "wie", "fer\u00b7ne", "Kir\u00b7chen\u00b7fah\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Flattert's von der Burg Gel\u00e4ndern", "tokens": ["Flat\u00b7tert's", "von", "der", "Burg", "Ge\u00b7l\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.7": {"text": "Bunt von seidnen Lenzgew\u00e4ndern", "tokens": ["Bunt", "von", "seid\u00b7nen", "Lenz\u00b7ge\u00b7w\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unter gr\u00fcnenden Platanen.", "tokens": ["Un\u00b7ter", "gr\u00fc\u00b7nen\u00b7den", "Pla\u00b7ta\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Einsam wehen hier die Linden", "tokens": ["Ein\u00b7sam", "we\u00b7hen", "hier", "die", "Lin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser Stadt um stille D\u00e4cher \u2013", "tokens": ["Die\u00b7ser", "Stadt", "um", "stil\u00b7le", "D\u00e4\u00b7cher", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, wie einen leeren Becher", "tokens": ["Ach", ",", "wie", "ei\u00b7nen", "lee\u00b7ren", "Be\u00b7cher"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df ich die verla\u00dfne finden,", "tokens": ["Mu\u00df", "ich", "die", "ver\u00b7la\u00df\u00b7ne", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einen Becher, dessen Schein", "tokens": ["Ei\u00b7nen", "Be\u00b7cher", ",", "des\u00b7sen", "Schein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird geflohn von jedem Munde", "tokens": ["Wird", "ge\u00b7flohn", "von", "je\u00b7dem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und auf dessen dunklem Grunde", "tokens": ["Und", "auf", "des\u00b7sen", "dunk\u00b7lem", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRELAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ich der letzte Tropfen Wein!", "tokens": ["Ich", "der", "letz\u00b7te", "Trop\u00b7fen", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "In die k\u00fchle D\u00e4mmernacht", "tokens": ["In", "die", "k\u00fch\u00b7le", "D\u00e4m\u00b7mer\u00b7nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meines Hauses steig ich nieder,", "tokens": ["Mei\u00b7nes", "Hau\u00b7ses", "steig", "ich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo mir meine jungen Lieder", "tokens": ["Wo", "mir", "mei\u00b7ne", "jun\u00b7gen", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlummern, bis ihr Tag erwacht;", "tokens": ["Schlum\u00b7mern", ",", "bis", "ihr", "Tag", "er\u00b7wacht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo ein Strau\u00df von Fliederzweigen", "tokens": ["Wo", "ein", "Strau\u00df", "von", "Flie\u00b7der\u00b7zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NE", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dr\u00fcber nickt mit stillem Neigen,", "tokens": ["Dr\u00fc\u00b7ber", "nickt", "mit", "stil\u00b7lem", "Nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit erwartungsvollem Schweigen", "tokens": ["Mit", "er\u00b7war\u00b7tungs\u00b7vol\u00b7lem", "Schwei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wilde R\u00f6schen halten Wacht.", "tokens": ["Wil\u00b7de", "R\u00f6\u00b7schen", "hal\u00b7ten", "Wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun in tiefer Einsamkeit", "tokens": ["Nun", "in", "tie\u00b7fer", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schreib ich, eh f\u00fcr immer schied", "tokens": ["Schreib", "ich", ",", "eh", "f\u00fcr", "im\u00b7mer", "schied"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "APPR", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir die lange Morgenzeit,", "tokens": ["Mir", "die", "lan\u00b7ge", "Mor\u00b7gen\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner Jugend letztes Lied;", "tokens": ["Mei\u00b7ner", "Ju\u00b7gend", "letz\u00b7tes", "Lied", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Hoffnung sei's geweiht!", "tokens": ["Und", "der", "Hoff\u00b7nung", "sei's", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was ich hoffe, hofft die Welt;", "tokens": ["Was", "ich", "hof\u00b7fe", ",", "hofft", "die", "Welt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ist sie nur zur Fahrt bereit,", "tokens": ["Ist", "sie", "nur", "zur", "Fahrt", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird sie selbst ihr Himmelszelt!", "tokens": ["Wird", "sie", "selbst", "ihr", "Him\u00b7mels\u00b7zelt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Tu dich auf, o sch\u00f6ner Schrein,", "tokens": ["Tu", "dich", "auf", ",", "o", "sch\u00f6\u00b7ner", "Schrein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKVZ", "$,", "FM", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lasse deine Sch\u00e4tze funkeln!", "tokens": ["Las\u00b7se", "dei\u00b7ne", "Sch\u00e4t\u00b7ze", "fun\u00b7keln", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df sie, blitzend hell, verdunkeln", "tokens": ["La\u00df", "sie", ",", "blit\u00b7zend", "hell", ",", "ver\u00b7dun\u00b7keln"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVIMP", "PPER", "$,", "ADJD", "ADJD", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Martyrer bla\u00df Gebein! \u2013", "tokens": ["Der", "Mar\u00b7ty\u00b7rer", "bla\u00df", "Ge\u00b7bein", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Weihrauch sind die Fr\u00fchlingsd\u00fcfte,", "tokens": ["Weih\u00b7rauch", "sind", "die", "Fr\u00fch\u00b7lings\u00b7d\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und auch du, mein Schwalbenzug,", "tokens": ["Und", "auch", "du", ",", "mein", "Schwal\u00b7ben\u00b7zug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Flattre, leichter Liederflug,", "tokens": ["Flatt\u00b7re", ",", "leich\u00b7ter", "Lie\u00b7der\u00b7flug", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Aufw\u00e4rts in die freien L\u00fcfte!", "tokens": ["Auf\u00b7w\u00e4rts", "in", "die", "frei\u00b7en", "L\u00fcf\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}