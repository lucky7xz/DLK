{"textgrid.poem.52046": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch sch\u00f6ner als der Mond (er gieng in vollem Schein", "tokens": ["Noch", "sch\u00f6\u00b7ner", "als", "der", "Mond", "(", "er", "gieng", "in", "vol\u00b7lem", "Schein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$(", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In vollen H\u00f6rnern auff) kam Chloris hergetreten,", "tokens": ["In", "vol\u00b7len", "H\u00f6r\u00b7nern", "auff", ")", "kam", "Chlo\u00b7ris", "her\u00b7ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "$(", "VVFIN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die auff den Feder Schmau\u00df ihr Limpidor gebeten,", "tokens": ["Die", "auff", "den", "Fe\u00b7der", "Schmau\u00df", "ihr", "Lim\u00b7pi\u00b7dor", "ge\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich sah', erschrack und schlieff mit offnen Augen ein.", "tokens": ["Ich", "sah'", ",", "er\u00b7schrack", "und", "schlieff", "mit", "off\u00b7nen", "Au\u00b7gen", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer wei\u00df ob Luna blo\u00df damals geh\u00f6rnert war,", "tokens": ["Wer", "wei\u00df", "ob", "Lu\u00b7na", "blo\u00df", "da\u00b7mals", "ge\u00b7h\u00f6r\u00b7nert", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KOUS", "NN", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil dieser Luna auch eins jeder kont entbieten:", "tokens": ["Weil", "die\u00b7ser", "Lu\u00b7na", "auch", "eins", "je\u00b7der", "kont", "ent\u00b7bie\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADV", "PIS", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie schliech bedachtsam her in halb gestohlnen Schrieten,", "tokens": ["Sie", "schliech", "be\u00b7dacht\u00b7sam", "her", "in", "halb", "ge\u00b7stohl\u00b7nen", "Schrie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und reicht ihr Honig ihm zusammt dem Stocke dar.", "tokens": ["Und", "reicht", "ihr", "Ho\u00b7nig", "ihm", "zu\u00b7sammt", "dem", "Sto\u00b7cke", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ihr Hembde war so dinn als Spinnen Weben gehn,", "tokens": ["Ihr", "Hemb\u00b7de", "war", "so", "dinn", "als", "Spin\u00b7nen", "We\u00b7ben", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "NN", "KOUS", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nu Phoebus brach drob ein: halt an, was wiltu rennen?", "tokens": ["Nu", "Phoe\u00b7bus", "brach", "drob", "ein", ":", "halt", "an", ",", "was", "wil\u00b7tu", "ren\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "ADV", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$,", "PWS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der thut dir nichts, der uns den Tag pflegt an zu brennen,", "tokens": ["Der", "thut", "dir", "nichts", ",", "der", "uns", "den", "Tag", "pflegt", "an", "zu", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIS", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo du nicht hin gedenckst, da kan dein Phoebus stehn.", "tokens": ["Wo", "du", "nicht", "hin", "ge\u00b7denckst", ",", "da", "kan", "dein", "Phoe\u00b7bus", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "ADV", "VVPP", "$,", "ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}