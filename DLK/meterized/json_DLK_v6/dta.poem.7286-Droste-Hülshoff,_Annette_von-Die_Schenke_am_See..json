{"dta.poem.7286": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die Schenke am See.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Ist's nicht ein heit'rer Ort, mein junger Freund,", "tokens": ["Ist's", "nicht", "ein", "heit'\u00b7rer", "Ort", ",", "mein", "jun\u00b7ger", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das kleine Haus, das schier vom Hange gleitet,", "tokens": ["Das", "klei\u00b7ne", "Haus", ",", "das", "schier", "vom", "Han\u00b7ge", "glei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo so possierlich uns der Wirth erscheint,", "tokens": ["Wo", "so", "pos\u00b7sier\u00b7lich", "uns", "der", "Wirth", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So \u00fcberm\u00e4chtig sich die Landschaft breitet;", "tokens": ["So", "\u00fc\u00b7ber\u00b7m\u00e4ch\u00b7tig", "sich", "die", "Land\u00b7schaft", "brei\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wo uns erg\u00f6tzt im neckischen Contrast", "tokens": ["Wo", "uns", "er\u00b7g\u00f6tzt", "im", "ne\u00b7cki\u00b7schen", "Cont\u00b7rast"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das Wurzelm\u00e4nnchen mit verschmitzter Miene,", "tokens": ["Das", "Wur\u00b7zel\u00b7m\u00e4nn\u00b7chen", "mit", "ver\u00b7schmitz\u00b7ter", "Mie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das wie ein Aal sich schlingt und kugelt fast,", "tokens": ["Das", "wie", "ein", "Aal", "sich", "schlingt", "und", "ku\u00b7gelt", "fast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KOKOM", "ART", "NN", "PRF", "VVFIN", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Im Angesicht der stolzen Alpenb\u00fchne?", "tokens": ["Im", "An\u00b7ge\u00b7sicht", "der", "stol\u00b7zen", "Al\u00b7pen\u00b7b\u00fch\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sitz nieder. \u2014 Traube! \u2014 und behend erscheint", "tokens": ["Sitz", "nie\u00b7der", ".", "Trau\u00b7be", "!", "und", "be\u00b7hend", "er\u00b7scheint"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "$(", "NN", "$.", "$(", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zopfwedelnd der gesch\u00e4ftige Pigm\u00e4e;", "tokens": ["Zopf\u00b7we\u00b7delnd", "der", "ge\u00b7sch\u00e4f\u00b7ti\u00b7ge", "Pig\u00b7m\u00e4e", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "O sieh, wie die verletzte Beere weint", "tokens": ["O", "sieh", ",", "wie", "die", "ver\u00b7letz\u00b7te", "Bee\u00b7re", "weint"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Blutige Thr\u00e4nen um des Reifes N\u00e4he;", "tokens": ["Blu\u00b7ti\u00b7ge", "Thr\u00e4\u00b7nen", "um", "des", "Rei\u00b7fes", "N\u00e4\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Frisch greif in die kristallne Schale, frisch,", "tokens": ["Frisch", "greif", "in", "die", "kris\u00b7tall\u00b7ne", "Scha\u00b7le", ",", "frisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die saftigen Rubine gl\u00fchn und locken;", "tokens": ["Die", "saf\u00b7ti\u00b7gen", "Ru\u00b7bi\u00b7ne", "gl\u00fchn", "und", "lo\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Schon f\u00fchl' ich an des Herbstes reichem Tisch", "tokens": ["Schon", "f\u00fchl'", "ich", "an", "des", "Herbs\u00b7tes", "rei\u00b7chem", "Tisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Den kargen Winter nahn auf leisen Socken.", "tokens": ["Den", "kar\u00b7gen", "Win\u00b7ter", "nahn", "auf", "lei\u00b7sen", "So\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das sind dir Hieroglyphen, junges Blut,", "tokens": ["Das", "sind", "dir", "Hie\u00b7ro\u00b7gly\u00b7phen", ",", "jun\u00b7ges", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ich, ich will an deiner lieben Seite", "tokens": ["Und", "ich", ",", "ich", "will", "an", "dei\u00b7ner", "lie\u00b7ben", "Sei\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PPER", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Froh schl\u00fcrfen meiner Neige letztes Gut.", "tokens": ["Froh", "schl\u00fcr\u00b7fen", "mei\u00b7ner", "Nei\u00b7ge", "letz\u00b7tes", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schau her, schau dr\u00fcben in die N\u00e4h' und Weite;", "tokens": ["Schau", "her", ",", "schau", "dr\u00fc\u00b7ben", "in", "die", "N\u00e4h'", "und", "Wei\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADJD", "ADV", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "+-+----+-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Wie uns zur Seite sich der Felsen b\u00e4umt,", "tokens": ["Wie", "uns", "zur", "Sei\u00b7te", "sich", "der", "Fel\u00b7sen", "b\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Als k\u00f6nnten wir mit H\u00e4nden ihn ergreifen,", "tokens": ["Als", "k\u00f6nn\u00b7ten", "wir", "mit", "H\u00e4n\u00b7den", "ihn", "er\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie uns zu F\u00fc\u00dfen das Gew\u00e4sser sch\u00e4umt,", "tokens": ["Wie", "uns", "zu", "F\u00fc\u00b7\u00dfen", "das", "Ge\u00b7w\u00e4s\u00b7ser", "sch\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Als k\u00f6nnten wir im Schwunge dr\u00fcber streifen!", "tokens": ["Als", "k\u00f6nn\u00b7ten", "wir", "im", "Schwun\u00b7ge", "dr\u00fc\u00b7ber", "strei\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "APPRART", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "H\u00f6rst du das Alphorn \u00fcber'm blauen See?", "tokens": ["H\u00f6rst", "du", "das", "Al\u00b7phorn", "\u00fc\u00b7ber'm", "blau\u00b7en", "See", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So klar die Luft, mich d\u00fcnkt ich seh' den Hirten", "tokens": ["So", "klar", "die", "Luft", ",", "mich", "d\u00fcnkt", "ich", "seh'", "den", "Hir\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Heimz\u00fcgeln von der duftbes\u00e4umten H\u00f6h' \u2014", "tokens": ["Heim\u00b7z\u00fc\u00b7geln", "von", "der", "duft\u00b7be\u00b7s\u00e4um\u00b7ten", "H\u00f6h'"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "War's nicht als ob die Rinderglocken schwirrten?", "tokens": ["Wa\u00b7r's", "nicht", "als", "ob", "die", "Rin\u00b7der\u00b7glo\u00b7cken", "schwirr\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "KOKOM", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Dort, wo die Schlucht in das Gestein sich dr\u00e4ngt \u2014", "tokens": ["Dort", ",", "wo", "die", "Schlucht", "in", "das", "Ge\u00b7stein", "sich", "dr\u00e4ngt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "APPR", "ART", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mich d\u00fcnkt ich seh den kecken J\u00e4ger schleichen;", "tokens": ["Mich", "d\u00fcnkt", "ich", "seh", "den", "ke\u00b7cken", "J\u00e4\u00b7ger", "schlei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wenn eine Gemse an der Klippe h\u00e4ngt,", "tokens": ["Wenn", "ei\u00b7ne", "Gem\u00b7se", "an", "der", "Klip\u00b7pe", "h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gewi\u00df, mein Auge m\u00fc\u00dfte sie erreichen.", "tokens": ["Ge\u00b7wi\u00df", ",", "mein", "Au\u00b7ge", "m\u00fc\u00df\u00b7te", "sie", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Trink aus! \u2014 die Alpen liegen Stundenweit,", "tokens": ["Trink", "aus", "!", "die", "Al\u00b7pen", "lie\u00b7gen", "Stun\u00b7den\u00b7weit", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "$(", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nur nah die Burg, uns heimisches Gem\u00e4uer,", "tokens": ["Nur", "nah", "die", "Burg", ",", "uns", "hei\u00b7mi\u00b7sches", "Ge\u00b7m\u00e4u\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo Tr\u00e4ume lagern langverschollner Zeit,", "tokens": ["Wo", "Tr\u00e4u\u00b7me", "la\u00b7gern", "lang\u00b7ver\u00b7scholl\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Seltsame M\u00e4hr und zorn'ge Abentheuer.", "tokens": ["Selt\u00b7sa\u00b7me", "M\u00e4hr", "und", "zorn'\u00b7ge", "A\u00b7bent\u00b7heu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Wohl ziemt es mir, in R\u00e4umen schwer und grau", "tokens": ["Wohl", "ziemt", "es", "mir", ",", "in", "R\u00e4u\u00b7men", "schwer", "und", "grau"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "APPR", "NN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zu gr\u00fcbeln \u00fcber dunkler Thaten Reste;", "tokens": ["Zu", "gr\u00fc\u00b7beln", "\u00fc\u00b7ber", "dunk\u00b7ler", "Tha\u00b7ten", "Res\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch du, Levin, schaust aus dem grimmen Bau", "tokens": ["Doch", "du", ",", "Le\u00b7vin", ",", "schaust", "aus", "dem", "grim\u00b7men", "Bau"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "NE", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wie eine Schwalbe aus dem Mauerneste.", "tokens": ["Wie", "ei\u00b7ne", "Schwal\u00b7be", "aus", "dem", "Mau\u00b7er\u00b7nes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Sieh' drunten auf dem See im Abendroth", "tokens": ["Sieh'", "drun\u00b7ten", "auf", "dem", "See", "im", "A\u00b7ben\u00b7droth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Taucherente hin und wieder schl\u00fcpfend;", "tokens": ["Die", "Tau\u00b7che\u00b7ren\u00b7te", "hin", "und", "wie\u00b7der", "schl\u00fcp\u00b7fend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun sinkt sie nieder wie des Netzes Loth,", "tokens": ["Nun", "sinkt", "sie", "nie\u00b7der", "wie", "des", "Net\u00b7zes", "Loth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nun wieder aufw\u00e4rts mit den Wellen h\u00fcpfend;", "tokens": ["Nun", "wie\u00b7der", "auf\u00b7w\u00e4rts", "mit", "den", "Wel\u00b7len", "h\u00fcp\u00b7fend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Seltsames Spiel, recht wie ein Lebenslauf!", "tokens": ["Selt\u00b7sa\u00b7mes", "Spiel", ",", "recht", "wie", "ein", "Le\u00b7bens\u00b7lauf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Wir beide schaun gespannten Blickes nieder;", "tokens": ["Wir", "bei\u00b7de", "schaun", "ge\u00b7spann\u00b7ten", "Bli\u00b7ckes", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du fl\u00fcsterst l\u00e4chelnd: immer k\u00f6mmt sie auf \u2014", "tokens": ["Du", "fl\u00fcs\u00b7ter