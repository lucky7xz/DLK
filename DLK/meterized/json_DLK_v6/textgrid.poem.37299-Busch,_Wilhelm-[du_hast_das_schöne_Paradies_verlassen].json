{"textgrid.poem.37299": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[du hast das sch\u00f6ne Paradies verlassen]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast das sch\u00f6ne Paradies verlassen,", "tokens": ["Du", "hast", "das", "sch\u00f6\u00b7ne", "Pa\u00b7ra\u00b7dies", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Tratst ein in dieses Labyrinthes Gassen,", "tokens": ["Tratst", "ein", "in", "die\u00b7ses", "La\u00b7by\u00b7rin\u00b7thes", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verlockt von lieblich winkenden Gestalten,", "tokens": ["Ver\u00b7lockt", "von", "lieb\u00b7lich", "win\u00b7ken\u00b7den", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Schale dir und Kranz entgegenhalten;", "tokens": ["Die", "Scha\u00b7le", "dir", "und", "Kranz", "ent\u00b7ge\u00b7gen\u00b7hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und unaufhaltsam zieht's dich weit und weiter.", "tokens": ["Und", "un\u00b7auf\u00b7halt\u00b7sam", "zieht's", "dich", "weit", "und", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl ist ein leises Ahnen dein Begleiter,", "tokens": ["Wohl", "ist", "ein", "lei\u00b7ses", "Ah\u00b7nen", "dein", "Be\u00b7glei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein heimlich Graun, da\u00df diese s\u00fc\u00dfen Freuden", "tokens": ["Ein", "heim\u00b7lich", "Graun", ",", "da\u00df", "die\u00b7se", "s\u00fc\u00b7\u00dfen", "Freu\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$,", "KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dich Schritt um Schritt von deiner Heimat scheiden,", "tokens": ["Dich", "Schritt", "um", "Schritt", "von", "dei\u00b7ner", "Hei\u00b7mat", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df Irren S\u00fcnde, Heimweh dein Gewissen;", "tokens": ["Da\u00df", "Ir\u00b7ren", "S\u00fcn\u00b7de", ",", "Heim\u00b7weh", "dein", "Ge\u00b7wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch ach umsonst! Der Faden ist zerrissen.", "tokens": ["Doch", "ach", "um\u00b7sonst", "!", "Der", "Fa\u00b7den", "ist", "zer\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Hohl\u00e4ugig fa\u00dft der Schmerz dich an und warnt,", "tokens": ["Hoh\u00b7l\u00e4u\u00b7gig", "fa\u00dft", "der", "Schmerz", "dich", "an", "und", "warnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du willst zur\u00fcck, die Seele ist umgarnt.", "tokens": ["Du", "willst", "zu\u00b7r\u00fcck", ",", "die", "See\u00b7le", "ist", "um\u00b7garnt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKVZ", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Vergebens steht ob deinem Haupt der Stern.", "tokens": ["Ver\u00b7ge\u00b7bens", "steht", "ob", "dei\u00b7nem", "Haupt", "der", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOUS", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Einsam, gefangen, von der Heimat fern,", "tokens": ["Ein\u00b7sam", ",", "ge\u00b7fan\u00b7gen", ",", "von", "der", "Hei\u00b7mat", "fern", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "$,", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "Ein Sklave, starrst du in des Stromes Lauf", "tokens": ["Ein", "Skla\u00b7ve", ",", "starrst", "du", "in", "des", "Stro\u00b7mes", "Lauf"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und h\u00e4ngst an Weiden deine Harfe auf.", "tokens": ["Und", "h\u00e4ngst", "an", "Wei\u00b7den", "dei\u00b7ne", "Har\u00b7fe", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun f\u00e4hrst du wohl empor, wenn so zuzeiten", "tokens": ["Nun", "f\u00e4hrst", "du", "wohl", "em\u00b7por", ",", "wenn", "so", "zu\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "KOUS", "ADV", "VVIZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im stillen Mondeslichte durch die Saiten", "tokens": ["Im", "stil\u00b7len", "Mon\u00b7des\u00b7lich\u00b7te", "durch", "die", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein leises, wehmutsvolles Klagen geht", "tokens": ["Ein", "lei\u00b7ses", ",", "weh\u00b7muts\u00b7vol\u00b7les", "Kla\u00b7gen", "geht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von einem Hauch, der aus der Heimat weht.", "tokens": ["Von", "ei\u00b7nem", "Hauch", ",", "der", "aus", "der", "Hei\u00b7mat", "weht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}