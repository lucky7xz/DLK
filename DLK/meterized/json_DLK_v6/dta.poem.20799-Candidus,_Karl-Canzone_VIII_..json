 ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KON", "ADV", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O rette deine Kirche vom Verfalle,", "tokens": ["O", "ret\u00b7te", "dei\u00b7ne", "Kir\u00b7che", "vom", "Ver\u00b7fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denn deine Diener sind es, die sie meucheln,", "tokens": ["Denn", "dei\u00b7ne", "Die\u00b7ner", "sind", "es", ",", "die", "sie", "meu\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "O Herr, durch wahngewiegtes Selbstbel\u00fcgen!", "tokens": ["O", "Herr", ",", "durch", "wahn\u00b7ge\u00b7wieg\u00b7tes", "Selbst\u00b7be\u00b7l\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auf alle Paulusr\u00fcgen,", "tokens": ["Auf", "al\u00b7le", "Pau\u00b7lus\u00b7r\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wenn je ich sie verdiene, la\u00df mich achten", "tokens": ["Wenn", "je", "ich", "sie", "ver\u00b7die\u00b7ne", ",", "la\u00df", "mich", "ach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "PPER", "VVFIN", "$,", "VVIMP", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und k\u00fchn wie Paulus jeden Flor zerrei\u00dfen", "tokens": ["Und", "k\u00fchn", "wie", "Pau\u00b7lus", "je\u00b7den", "Flor", "zer\u00b7rei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "NE", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wohinter falsches Glei\u00dfen", "tokens": ["Wo\u00b7hin\u00b7ter", "fal\u00b7sches", "Glei\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Verl\u00e4ugnet deines Geistes neue Machten.", "tokens": ["Ver\u00b7l\u00e4ug\u00b7net", "dei\u00b7nes", "Geis\u00b7tes", "neu\u00b7e", "Mach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "So la\u00df mich dienen dir in allen Treuen,", "tokens": ["So", "la\u00df", "mich", "die\u00b7nen", "dir", "in", "al\u00b7len", "Treu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Der du einherf\u00e4hrst Alles zu erneuen.", "tokens": ["Der", "du", "ein\u00b7her\u00b7f\u00e4hrst", "Al\u00b7les", "zu", "er\u00b7neu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}