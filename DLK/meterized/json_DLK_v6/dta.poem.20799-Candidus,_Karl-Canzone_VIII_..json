{"dta.poem.20799": {"metadata": {"author": {"name": "Candidus, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Canzone   VIII .", "genre": "Epos", "period": "N.A.", "pub_year": "1854", "urn": "urn:nbn:de:kobv:b4-20090519783", "language": ["de:0.99"], "booktitle": "Candidus, Karl: Der deutsche Christus. F\u00fcnfzehn Canzonen. Leipzig, 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Gef\u00fchl des Sollens und ein dunkles Sehnen,", "tokens": ["Ge\u00b7f\u00fchl", "des", "Sol\u00b7lens", "und", "ein", "dunk\u00b7les", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Obzwar ein immerw\u00e4hrend unerf\u00fclltes,", "tokens": ["Ob\u00b7zwar", "ein", "im\u00b7mer\u00b7w\u00e4h\u00b7rend", "un\u00b7er\u00b7f\u00fcll\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Empf\u00e4nglichkeit und wechselvolles Sp\u00fcren", "tokens": ["Emp\u00b7f\u00e4ng\u00b7lich\u00b7keit", "und", "wech\u00b7sel\u00b7vol\u00b7les", "Sp\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach dem Unendlichen, das ein Verh\u00fclltes,", "tokens": ["Nach", "dem", "Un\u00b7end\u00b7li\u00b7chen", ",", "das", "ein", "Ver\u00b7h\u00fcll\u00b7tes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.5": {"text": "Wer mag das Endliche damit belehnen", "tokens": ["Wer", "mag", "das", "End\u00b7li\u00b7che", "da\u00b7mit", "be\u00b7leh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "NN", "PAV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im S\u00fcndenirrsal, wenn nicht heil'ges F\u00fchren?", "tokens": ["Im", "S\u00fcn\u00b7de\u00b7nirr\u00b7sal", ",", "wenn", "nicht", "heil'\u00b7ges", "F\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So stralt, den sie mag k\u00fcren,", "tokens": ["So", "stralt", ",", "den", "sie", "mag", "k\u00fc\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der unfindbare Freund der Frauenseele", "tokens": ["Der", "un\u00b7find\u00b7ba\u00b7re", "Freund", "der", "Frau\u00b7en\u00b7see\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Als Urbild, wie in Endlichkeit herscheinet", "tokens": ["Als", "Ur\u00b7bild", ",", "wie", "in", "End\u00b7lich\u00b7keit", "her\u00b7schei\u00b7net"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "PWAV", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ein Gott, bis ihm vereinet,", "tokens": ["Ein", "Gott", ",", "bis", "ihm", "ver\u00b7ei\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Nun kennend \u00f6ftrer herber T\u00e4uschung Fehle,", "tokens": ["Nun", "ken\u00b7nend", "\u00f6ft\u00b7rer", "her\u00b7ber", "T\u00e4u\u00b7schung", "Feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Sie volle Gn\u00fcge hat und s\u00fc\u00dfes Feiern", "tokens": ["Sie", "vol\u00b7le", "Gn\u00fc\u00b7ge", "hat", "und", "s\u00fc\u00b7\u00dfes", "Fei\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "VAFIN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "In heilig stillen Lebens holden Schleiern.", "tokens": ["In", "hei\u00b7lig", "stil\u00b7len", "Le\u00b7bens", "hol\u00b7den", "Schlei\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wie ein J\u00fcngling wirbt um zartes Danken,", "tokens": ["Und", "wie", "ein", "J\u00fcng\u00b7ling", "wirbt", "um", "zar\u00b7tes", "Dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So das Unendliche. Wie auf der Stirne", "tokens": ["So", "das", "Un\u00b7end\u00b7li\u00b7che", ".", "Wie", "auf", "der", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$.", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Liebe ruht ihr Traumbild unabtrennbar,", "tokens": ["Der", "Lie\u00b7be", "ruht", "ihr", "Traum\u00b7bild", "un\u00b7ab\u00b7trenn\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ob steigen, ob sich neigen die Gestirne,", "tokens": ["Ob", "stei\u00b7gen", ",", "ob", "sich", "nei\u00b7gen", "die", "Ge\u00b7stir\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVINF", "$,", "KOUS", "PRF", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ist ewiglich der g\u00f6ttliche Gedanken", "tokens": ["Ist", "e\u00b7wig\u00b7lich", "der", "g\u00f6tt\u00b7li\u00b7che", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im Sorgen nur um's Endliche erkennbar.", "tokens": ["Im", "Sor\u00b7gen", "nur", "um's", "End\u00b7li\u00b7che", "er\u00b7kenn\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und stets durch solch unnennbar", "tokens": ["Und", "stets", "durch", "solch", "un\u00b7nenn\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Allstetes Ineinanderscheinen schwanden", "tokens": ["Alls\u00b7te\u00b7tes", "In\u00b7ein\u00b7an\u00b7der\u00b7schei\u00b7nen", "schwan\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "Die Beiden ineinander, und mit Klarheit", "tokens": ["Die", "Bei\u00b7den", "in\u00b7ein\u00b7an\u00b7der", ",", "und", "mit", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ist so, als ihre Wahrheit,", "tokens": ["Ist", "so", ",", "als", "ih\u00b7re", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "In irdischem Bewu\u00dftsein auch erstanden", "tokens": ["In", "ir\u00b7di\u00b7schem", "Be\u00b7wu\u00df\u00b7tsein", "auch", "er\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Urew'ger Liebeseinheit Bild und Wesen,", "tokens": ["U\u00b7re\u00b7w'\u00b7ger", "Lie\u00b7be\u00b7sein\u00b7heit", "Bild", "und", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.13": {"text": "Der Sohn, der \u201e", "tokens": ["Der", "Sohn", ",", "der", "\u201e"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "$("], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Jud\u00e4as Berge schweigen jede Kunde", "tokens": ["Ju\u00b7d\u00e4\u00b7as", "Ber\u00b7ge", "schwei\u00b7gen", "je\u00b7de", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VVFIN", "PIAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Vom Anfang des Bewu\u00dftseins h\u00f6chster W\u00fcrde,", "tokens": ["Vom", "An\u00b7fang", "des", "Be\u00b7wu\u00dfts\u00b7eins", "h\u00f6chs\u00b7ter", "W\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als dankend auf zum Vater mochte beten", "tokens": ["Als", "dan\u00b7kend", "auf", "zum", "Va\u00b7ter", "moch\u00b7te", "be\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "APPRART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Sohn und frei erkor die Mittlerb\u00fcrde.", "tokens": ["Der", "Sohn", "und", "frei", "er\u00b7kor", "die", "Mitt\u00b7ler\u00b7b\u00fcr\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O h\u00f6chster Himmelsoffenbarung Stunde!", "tokens": ["O", "h\u00f6chs\u00b7ter", "Him\u00b7mel\u00b7sof\u00b7fen\u00b7ba\u00b7rung", "Stun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "O feierlichste Stunde des Planeten!", "tokens": ["O", "fei\u00b7er\u00b7lichs\u00b7te", "Stun\u00b7de", "des", "Pla\u00b7ne\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ha! Lebensschauer wehten", "tokens": ["Ha", "!", "Le\u00b7bens\u00b7schau\u00b7er", "weh\u00b7ten"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$.", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aus den Unendlichkeiten her der Himmel,", "tokens": ["Aus", "den", "Un\u00b7end\u00b7lich\u00b7kei\u00b7ten", "her", "der", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APZR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Aus den Unendlichkeiten her der Seele,", "tokens": ["Aus", "den", "Un\u00b7end\u00b7lich\u00b7kei\u00b7ten", "her", "der", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APZR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und dessen was er w\u00e4hle", "tokens": ["Und", "des\u00b7sen", "was", "er", "w\u00e4h\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Bewu\u00dft, gro\u00df stand, fernab vom Weltgewimmel,", "tokens": ["Be\u00b7wu\u00dft", ",", "gro\u00df", "stand", ",", "fern\u00b7ab", "vom", "Welt\u00b7ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "VVFIN", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Der erste freie Mensch. Es singt's mit Zaudern", "tokens": ["Der", "ers\u00b7te", "frei\u00b7e", "Mensch", ".", "Es", "singt's", "mit", "Zau\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$.", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die Muse, ja! und denkt's mit s\u00fc\u00dfem Schaudern", "tokens": ["Die", "Mu\u00b7se", ",", "ja", "!", "und", "denkt's", "mit", "s\u00fc\u00b7\u00dfem", "Schau\u00b7dern"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "$.", "KON", "PDS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun lag die Wahrheit offen jenes Bundes", "tokens": ["Nun", "lag", "die", "Wahr\u00b7heit", "of\u00b7fen", "je\u00b7nes", "Bun\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Jehovah und dem erw\u00e4hlten Volke.", "tokens": ["Von", "Je\u00b7ho\u00b7vah", "und", "dem", "er\u00b7w\u00e4hl\u00b7ten", "Vol\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun war erf\u00fcllt Gesetz und Prophezeiung.", "tokens": ["Nun", "war", "er\u00b7f\u00fcllt", "Ge\u00b7setz", "und", "Pro\u00b7phe\u00b7zei\u00b7ung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Sollen war erreicht. Die Feuerwolke", "tokens": ["Das", "Sol\u00b7len", "war", "er\u00b7reicht", ".", "Die", "Feu\u00b7er\u00b7wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Schmolz und der Sohn stand da. Vor seines Mundes", "tokens": ["Schmolz", "und", "der", "Sohn", "stand", "da", ".", "Vor", "sei\u00b7nes", "Mun\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "ART", "NN", "VVFIN", "PTKVZ", "$.", "APPR", "PPOSAT", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Anl\u00e4cheln schwieg die Klage der Entzweiung.", "tokens": ["An\u00b7l\u00e4\u00b7cheln", "schwieg", "die", "Kla\u00b7ge", "der", "Ent\u00b7zwei\u00b7ung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Fortan war's um Kasteiung", "tokens": ["For\u00b7tan", "wa\u00b7r's", "um", "Kas\u00b7te\u00b7i\u00b7ung"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Opferdienst und Satzungen geschehen.", "tokens": ["Und", "Op\u00b7fer\u00b7dienst", "und", "Sat\u00b7zun\u00b7gen", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Des Sohnes sich zu freu'n schien s\u00fc\u00dfe Pflicht nur.", "tokens": ["Des", "Soh\u00b7nes", "sich", "zu", "freu'n", "schien", "s\u00fc\u00b7\u00dfe", "Pflicht", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PTKZU", "VVINF", "VVFIN", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ihn sah im Ahnungslicht nur", "tokens": ["Ihn", "sah", "im", "Ah\u00b7nungs\u00b7licht", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Die V\u00e4terwelt. Ihr Traumbild mu\u00df vergehen", "tokens": ["Die", "V\u00e4\u00b7ter\u00b7welt", ".", "Ihr", "Traum\u00b7bild", "mu\u00df", "ver\u00b7ge\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "PPOSAT", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Vor \u00fcbertreffender Erf\u00fcllungsf\u00fclle.", "tokens": ["Vor", "\u00fc\u00b7bert\u00b7ref\u00b7fen\u00b7der", "Er\u00b7f\u00fcl\u00b7lungs\u00b7f\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Den Kern entlassend f\u00e4llt die Tempelh\u00fclle.", "tokens": ["Den", "Kern", "ent\u00b7las\u00b7send", "f\u00e4llt", "die", "Tem\u00b7pel\u00b7h\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O wie mit salomonischem Ger\u00f6lle,", "tokens": ["O", "wie", "mit", "sa\u00b7lo\u00b7mo\u00b7ni\u00b7schem", "Ge\u00b7r\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O wie mit Zebaoth und Zehngeboten", "tokens": ["O", "wie", "mit", "Ze\u00b7bao\u00b7th", "und", "Zehn\u00b7ge\u00b7bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "M\u00f6gt ihr nun stets die Kirche noch verplundern?", "tokens": ["M\u00f6gt", "ihr", "nun", "stets", "die", "Kir\u00b7che", "noch", "ver\u00b7plun\u00b7dern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Alles ging ja l\u00e4ngst schon zu den Toten,", "tokens": ["Das", "Al\u00b7les", "ging", "ja", "l\u00e4ngst", "schon", "zu", "den", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "ADV", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auch l\u00e4ngst schon sind die Cherubim zur H\u00f6lle,", "tokens": ["Auch", "l\u00e4