{"textgrid.poem.44031": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Komm, Bruder, auf mein Wort und folg auch mir einmahl;", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, Bruder, auf mein Wort und folg auch mir einmahl;", "tokens": ["Komm", ",", "Bru\u00b7der", ",", "auf", "mein", "Wort", "und", "folg", "auch", "mir", "ein\u00b7mahl", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "KON", "NN", "ADV", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir wolle in der Angst das beste Theil erwehlen.", "tokens": ["Wir", "wol\u00b7le", "in", "der", "Angst", "das", "bes\u00b7te", "Theil", "er\u00b7weh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ertrag die Gegenwart der ziemlich langen Qual,", "tokens": ["Er\u00b7trag", "die", "Ge\u00b7gen\u00b7wart", "der", "ziem\u00b7lich", "lan\u00b7gen", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer nichts begehrt und hoft, dem kan es nirgends fehlen,", "tokens": ["Wer", "nichts", "be\u00b7gehrt", "und", "hoft", ",", "dem", "kan", "es", "nir\u00b7gends", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVPP", "KON", "VVFIN", "$,", "PDS", "VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das Ungl\u00fcck ist ja auch ein Spiel der Eitelkeit,", "tokens": ["Das", "Un\u00b7gl\u00fcck", "ist", "ja", "auch", "ein", "Spiel", "der", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dies giebt den Krancken Trost und l\u00e4st die Hofnung leben;", "tokens": ["Dies", "giebt", "den", "Kran\u00b7cken", "Trost", "und", "l\u00e4st", "die", "Hof\u00b7nung", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ja, h\u00fcbe Gram und Klag ein Quintchen Herzeleid,", "tokens": ["Ja", ",", "h\u00fc\u00b7be", "Gram", "und", "Klag", "ein", "Quint\u00b7chen", "Her\u00b7ze\u00b7leid", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "KON", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wollt ich heute noch mit Fluchen widerstreben.", "tokens": ["So", "wollt", "ich", "heu\u00b7te", "noch", "mit", "Flu\u00b7chen", "wi\u00b7der\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es geht nicht, wie man meint, die Vorsicht schliest und spricht,", "tokens": ["Es", "geht", "nicht", ",", "wie", "man", "meint", ",", "die", "Vor\u00b7sicht", "schliest", "und", "spricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PIS", "VVFIN", "$,", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und was sie winckt, geschieht auch wider unsern D\u00fcnckel;", "tokens": ["Und", "was", "sie", "winckt", ",", "ge\u00b7schieht", "auch", "wi\u00b7der", "un\u00b7sern", "D\u00fcn\u00b7ckel", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Indem sie dort den Stuhl des h\u00f6chsten Reiches bricht,", "tokens": ["In\u00b7dem", "sie", "dort", "den", "Stuhl", "des", "h\u00f6chs\u00b7ten", "Rei\u00b7ches", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So holt sie anderw\u00e4rts den K\u00f6nig aus dem Winckel.", "tokens": ["So", "holt", "sie", "an\u00b7der\u00b7w\u00e4rts", "den", "K\u00f6\u00b7nig", "aus", "dem", "Win\u00b7ckel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Noth, der Tugend Sporn, ist, wer es recht beherzt,", "tokens": ["Die", "Noth", ",", "der", "Tu\u00b7gend", "Sporn", ",", "ist", ",", "wer", "es", "recht", "be\u00b7herzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "$,", "VAFIN", "$,", "PWS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vor Wohlthat, nicht vor Zorn, vom Himmel anzunehmen;", "tokens": ["Vor", "Wohlt\u00b7hat", ",", "nicht", "vor", "Zorn", ",", "vom", "Him\u00b7mel", "an\u00b7zu\u00b7neh\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PTKNEG", "APPR", "NN", "$,", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sie lehrt uns, ob es gleich dem schwachen Fleische schmerzt,", "tokens": ["Sie", "lehrt", "uns", ",", "ob", "es", "gleich", "dem", "schwa\u00b7chen", "Flei\u00b7sche", "schmerzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Durch Vorsicht, Flei\u00df und Kunst den Neid zulezt besch\u00e4men.", "tokens": ["Durch", "Vor\u00b7sicht", ",", "Flei\u00df", "und", "Kunst", "den", "Neid", "zu\u00b7lezt", "be\u00b7sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Betrieg dich ja nicht selbst mit \u00c4rgern\u00fc\u00df und Groll", "tokens": ["Be\u00b7trieg", "dich", "ja", "nicht", "selbst", "mit", "\u00c4r\u00b7ger\u00b7n\u00fc\u00df", "und", "Groll"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "PTKNEG", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "An Leuten, welche stets dem Gl\u00fcck im Schoo\u00dfe sizen,", "tokens": ["An", "Leu\u00b7ten", ",", "wel\u00b7che", "stets", "dem", "Gl\u00fcck", "im", "Schoo\u00b7\u00dfe", "si\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und halt nicht irgend die vor fried- und seegensvoll,", "tokens": ["Und", "halt", "nicht", "ir\u00b7gend", "die", "vor", "frie\u00b7d", "und", "see\u00b7gens\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "ART", "APPR", "TRUNC", "KON", "ADJD", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Die unter Gold und Wein und weichen Kleidern schwizen.", "tokens": ["Die", "un\u00b7ter", "Gold", "und", "Wein", "und", "wei\u00b7chen", "Klei\u00b7dern", "schwi\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es ist geschminckte Lust und \u00fcbert\u00fcnchte Last,", "tokens": ["Es", "ist", "ge\u00b7schminck\u00b7te", "Lust", "und", "\u00fc\u00b7ber\u00b7t\u00fcnch\u00b7te", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ihr Creuz und auch ihr Schmuck sind einerley Gewichte;", "tokens": ["Ihr", "Creuz", "und", "auch", "ihr", "Schmuck", "sind", "ei\u00b7ner\u00b7ley", "Ge\u00b7wich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ADV", "PPOSAT", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Ruhe, so du jezt bey allem Mangel hast,", "tokens": ["Die", "Ru\u00b7he", ",", "so", "du", "jezt", "bey", "al\u00b7lem", "Man\u00b7gel", "hast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "ADV", "APPR", "PIS", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Kommt jenen nicht einmahl in Tr\u00e4umen zu Gesichte.", "tokens": ["Kommt", "je\u00b7nen", "nicht", "ein\u00b7mahl", "in", "Tr\u00e4u\u00b7men", "zu", "Ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "PTKNEG", "ADV", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ja, sprichstu, sieh doch recht und pr\u00fcf es ganz genau,", "tokens": ["Ja", ",", "sprichs\u00b7tu", ",", "sieh", "doch", "recht", "und", "pr\u00fcf", "es", "ganz", "ge\u00b7nau", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "$,", "VVIMP", "ADV", "ADJD", "KON", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ob sie nicht gl\u00fccklich sind; sie schwermen in der F\u00fclle,", "tokens": ["Ob", "sie", "nicht", "gl\u00fcck\u00b7lich", "sind", ";", "sie", "schwer\u00b7men", "in", "der", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VAFIN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die Wollust macht sie mehr als Sorg und Kummer grau,", "tokens": ["Die", "Wol\u00b7lust", "macht", "sie", "mehr", "als", "Sorg", "und", "Kum\u00b7mer", "grau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "KOUS", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und was das Herz begehrt, bekommt ihr starcker Wille.", "tokens": ["Und", "was", "das", "Herz", "be\u00b7gehrt", ",", "be\u00b7kommt", "ihr", "star\u00b7cker", "Wil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVPP", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "O Bruder, bleib nur stehn, was meinstu, da\u00df es sey?", "tokens": ["O", "Bru\u00b7der", ",", "bleib", "nur", "stehn", ",", "was", "meins\u00b7tu", ",", "da\u00df", "es", "sey", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "ADV", "VVINF", "$,", "PWS", "VVFIN", "$,", "KOUS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ein Blendwerck, ein Betrug der eu\u00dferlichen Sinnen.", "tokens": ["Ein", "Blend\u00b7werck", ",", "ein", "Be\u00b7trug", "der", "eu\u00b7\u00dfer\u00b7li\u00b7chen", "Sin\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "O st\u00fcnde dir einmahl ein Blick ins Herze frey,", "tokens": ["O", "st\u00fcn\u00b7de", "dir", "ein\u00b7mahl", "ein", "Blick", "ins", "Her\u00b7ze", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ART", "NN", "APPRART", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ihr Aussehn sollte bald ein ander Bild gewinnen.", "tokens": ["Ihr", "Aus\u00b7sehn", "soll\u00b7te", "bald", "ein", "an\u00b7der", "Bild", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Was w\u00fcrdestu allda vor Unruh, Angst und Pein,", "tokens": ["Was", "w\u00fcr\u00b7des\u00b7tu", "all\u00b7da", "vor", "Un\u00b7ruh", ",", "Angst", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Begierden, Schwei\u00df und Last und Sclaverey entdecken;", "tokens": ["Be\u00b7gier\u00b7den", ",", "Schwei\u00df", "und", "Last", "und", "Scla\u00b7ve\u00b7rey", "ent\u00b7de\u00b7cken", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der Anblick n\u00e4hme dich mit Reu und Eckel ein,", "tokens": ["Der", "An\u00b7blick", "n\u00e4h\u00b7me", "dich", "mit", "Reu", "und", "E\u00b7ckel", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NE", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Du w\u00fcrdest nur vom Sehn vor ihrer Furcht erschr\u00f6cken.", "tokens": ["Du", "w\u00fcr\u00b7dest", "nur", "vom", "Sehn", "vor", "ih\u00b7rer", "Furcht", "er\u00b7schr\u00f6\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Es foltert sie der Traum, es martert sie der Tag", "tokens": ["Es", "fol\u00b7tert", "sie", "der", "Traum", ",", "es", "mar\u00b7tert", "sie", "der", "Tag"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Mit scheuslicher Gestalt und zagenden Gedancken,", "tokens": ["Mit", "scheus\u00b7li\u00b7cher", "Ge\u00b7stalt", "und", "za\u00b7gen\u00b7den", "Ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "So da\u00df wohl ihre Brust die Wahlstatt hei\u00dfen mag,", "tokens": ["So", "da\u00df", "wohl", "ih\u00b7re", "Brust", "die", "Wahl\u00b7statt", "hei\u00b7\u00dfen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ADV", "PPOSAT", "NN", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Worauf sich Thorheit, Grimm und Geiz und Zweifel zancken.", "tokens": ["Wo\u00b7rauf", "sich", "Thor\u00b7heit", ",", "Grimm", "und", "Geiz", "und", "Zwei\u00b7fel", "zan\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "NN", "$,", "NE", "KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Sie hei\u00dfen Herr und gro\u00df und sind doch unterthan", "tokens": ["Sie", "hei\u00b7\u00dfen", "Herr", "und", "gro\u00df", "und", "sind", "doch", "un\u00b7ter\u00b7than"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "ADJD", "KON", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Den L\u00fcsten, der Gefahr, dem P\u00f6bel und der Mode,", "tokens": ["Den", "L\u00fcs\u00b7ten", ",", "der", "Ge\u00b7fahr", ",", "dem", "P\u00f6\u00b7bel", "und", "der", "Mo\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und giebt sich ohngefehr ein kleines Fieber an,", "tokens": ["Und", "giebt", "sich", "ohn\u00b7ge\u00b7fehr", "ein", "klei\u00b7nes", "Fie\u00b7ber", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "So sterben sie vor Furcht noch fr\u00fcher als vom Tode.", "tokens": ["So", "ster\u00b7ben", "sie", "vor", "Furcht", "noch", "fr\u00fc\u00b7her", "als", "vo