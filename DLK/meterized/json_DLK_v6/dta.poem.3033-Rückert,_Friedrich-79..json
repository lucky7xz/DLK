{"dta.poem.3033": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "79.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ungl\u00fccklich ist nicht, wer der Erde Gl\u00fcck verlor,", "tokens": ["Un\u00b7gl\u00fcck\u00b7lich", "ist", "nicht", ",", "wer", "der", "Er\u00b7de", "Gl\u00fcck", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PTKNEG", "$,", "PWS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und himmlisches daf\u00fcr im Glauben sich erkor,", "tokens": ["Und", "himm\u00b7li\u00b7sches", "da\u00b7f\u00fcr", "im", "Glau\u00b7ben", "sich", "er\u00b7kor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "PAV", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ungl\u00fccklich auch nicht, wer zufrieden sich behagt", "tokens": ["Un\u00b7gl\u00fcck\u00b7lich", "auch", "nicht", ",", "wer", "zu\u00b7frie\u00b7den", "sich", "be\u00b7hagt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PTKNEG", "$,", "PWS", "ADJD", "PRF", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An dieser Welt, und nicht nach einer andern fragt.", "tokens": ["An", "die\u00b7ser", "Welt", ",", "und", "nicht", "nach", "ei\u00b7ner", "an\u00b7dern", "fragt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "KON", "PTKNEG", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ungl\u00fccklich ist nur, wer die Lust sich sieht geraubt", "tokens": ["Un\u00b7gl\u00fcck\u00b7lich", "ist", "nur", ",", "wer", "die", "Lust", "sich", "sieht", "ge\u00b7raubt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ADV", "$,", "PWS", "ART", "NN", "PRF", "VVFIN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Irdischen, und nicht an Ueberird'sches glaubt.", "tokens": ["Am", "Ir\u00b7di\u00b7schen", ",", "und", "nicht", "an", "Ue\u00b7ber\u00b7ird'\u00b7sches", "glaubt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "PTKNEG", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}