{"textgrid.poem.24606": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Hochachtung seiner selbst ist th\u00f6richt", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr seyd sch\u00f6n/ das glaubet mir/", "tokens": ["Ihr", "seyd", "sch\u00f6n", "/", "das", "glau\u00b7bet", "mir", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$(", "PDS", "VVFIN", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer es l\u00e4ugnet/ ist ein Thier/", "tokens": ["Wer", "es", "l\u00e4ug\u00b7net", "/", "ist", "ein", "Thier", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$(", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr seyd reich/ das kan man sehn;", "tokens": ["Ihr", "seyd", "reich", "/", "das", "kan", "man", "sehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$(", "PDS", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und man mu\u00df mit Ruhm gestehn/", "tokens": ["Und", "man", "mu\u00df", "mit", "Ruhm", "ge\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "APPR", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ihr ehrlich seyd zu nennen/", "tokens": ["Da\u00df", "ihr", "ehr\u00b7lich", "seyd", "zu", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der euch nicht davor erkennt/ ist vor einen Narrn zu kennen.", "tokens": ["Der", "euch", "nicht", "da\u00b7vor", "er\u00b7kennt", "/", "ist", "vor", "ei\u00b7nen", "Narrn", "zu", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "PAV", "VVFIN", "$(", "VAFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Doch ihr seyd/ so bald ihr saget: dieser Ruhm geh\u00f6ret' euch/", "tokens": ["Doch", "ihr", "seyd", "/", "so", "bald", "ihr", "sa\u00b7get", ":", "die\u00b7ser", "Ruhm", "ge\u00b7h\u00f6\u00b7ret'", "euch", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$(", "ADV", "ADV", "PPER", "VVFIN", "$.", "PDAT", "NN", "VVFIN", "PPER", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Weder ehrlich/ sch\u00f6n noch reich.", "tokens": ["We\u00b7der", "ehr\u00b7lich", "/", "sch\u00f6n", "noch", "reich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$(", "ADJD", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}