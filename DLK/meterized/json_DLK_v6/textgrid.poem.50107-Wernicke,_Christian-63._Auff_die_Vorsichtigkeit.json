{"textgrid.poem.50107": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "63. Auff die Vorsichtigkeit", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer mit Vernunfft der Zeit und dem Verh\u00e4ngniss weicht,", "tokens": ["Wer", "mit", "Ver\u00b7nunfft", "der", "Zeit", "und", "dem", "Ver\u00b7h\u00e4ng\u00b7niss", "weicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer nicht dem Gl\u00fccke traut auch in den besten Tagen,", "tokens": ["Wer", "nicht", "dem", "Gl\u00fc\u00b7cke", "traut", "auch", "in", "den", "bes\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "NN", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den st\u00f6rt kein Zufall nicht: Das ", "tokens": ["Den", "st\u00f6rt", "kein", "Zu\u00b7fall", "nicht", ":", "Das"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "PIAT", "NN", "PTKNEG", "$.", "ART"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Wer lang' hierrauff ", "tokens": ["Wer", "lang'", "hier\u00b7rauff"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADV", "PAV"], "meter": "-+-+", "measure": "iambic.di"}}}}}