{"dta.poem.10441": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Wo ich bin, mich rings umdunkelt", "tokens": ["Wo", "ich", "bin", ",", "mich", "rings", "um\u00b7dun\u00b7kelt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VAFIN", "$,", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Finsterni\u00df, so dumpf und dicht,", "tokens": ["Fins\u00b7ter\u00b7ni\u00df", ",", "so", "dumpf", "und", "dicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seit mir nicht mehr leuchtend funkelt,", "tokens": ["Seit", "mir", "nicht", "mehr", "leuch\u00b7tend", "fun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKNEG", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebste, deiner Augen Licht.", "tokens": ["Liebs\u00b7te", ",", "dei\u00b7ner", "Au\u00b7gen", "Licht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mir erloschen ist der s\u00fc\u00dfen", "tokens": ["Mir", "er\u00b7lo\u00b7schen", "ist", "der", "s\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVPP", "VAFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebessterne goldne Pracht,", "tokens": ["Lie\u00b7bess\u00b7ter\u00b7ne", "gold\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Abgrund g\u00e4hnt zu meinen F\u00fc\u00dfen \u2014", "tokens": ["Ab\u00b7grund", "g\u00e4hnt", "zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimm mich auf, uralte Nacht!", "tokens": ["Nimm", "mich", "auf", ",", "ur\u00b7al\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}