{"textgrid.poem.47822": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Knecht Ruprecht", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von drau\u00df' vom Walde komm ich her;", "tokens": ["Von", "drau\u00df'", "vom", "Wal\u00b7de", "komm", "ich", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich mu\u00df euch sagen, es weihnachtet sehr!", "tokens": ["Ich", "mu\u00df", "euch", "sa\u00b7gen", ",", "es", "weih\u00b7nach\u00b7tet", "sehr", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "All\u00fcberall auf den Tannenspitzen", "tokens": ["Al\u00b7l\u00fc\u00b7be\u00b7rall", "auf", "den", "Tan\u00b7nen\u00b7spit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sah ich goldene Lichtlein sitzen;", "tokens": ["Sah", "ich", "gol\u00b7de\u00b7ne", "Licht\u00b7lein", "sit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Und droben aus dem Himmelstor", "tokens": ["Und", "dro\u00b7ben", "aus", "dem", "Him\u00b7mels\u00b7tor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sah mit gro\u00dfen Augen das Christkind hervor,", "tokens": ["Sah", "mit", "gro\u00b7\u00dfen", "Au\u00b7gen", "das", "Christ\u00b7kind", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+--++-+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Und wie ich so strolcht durch den finstern Tann,", "tokens": ["Und", "wie", "ich", "so", "strolcht", "durch", "den", "fins\u00b7tern", "Tann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Da rief's mich mit heller Stimme an.", "tokens": ["Da", "rie\u00b7f's", "mich", "mit", "hel\u00b7ler", "Stim\u00b7me", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "\u00bbknecht Ruprecht\u00ab, rief es, \u00bbalter Gesell,", "tokens": ["\u00bb", "knecht", "Ru\u00b7precht", "\u00ab", ",", "rief", "es", ",", "\u00bb", "al\u00b7ter", "Ge\u00b7sell", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NE", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Hebe die Beine und spute dich schnell!", "tokens": ["He\u00b7be", "die", "Bei\u00b7ne", "und", "spu\u00b7te", "dich", "schnell", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.11": {"text": "Die Kerzen fangen zu brennen an,", "tokens": ["Die", "Ker\u00b7zen", "fan\u00b7gen", "zu", "bren\u00b7nen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Das Himmelstor ist aufgetan,", "tokens": ["Das", "Him\u00b7mels\u00b7tor", "ist", "auf\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Alt' und Junge sollen nun", "tokens": ["Alt'", "und", "Jun\u00b7ge", "sol\u00b7len", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VMFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Von der Jagd des Lebens einmal ruhn;", "tokens": ["Von", "der", "Jagd", "des", "Le\u00b7bens", "ein\u00b7mal", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Und morgen flieg ich hinab zur Erden,", "tokens": ["Und", "mor\u00b7gen", "flieg", "ich", "hin\u00b7ab", "zur", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Denn es soll wieder Weihnachten werden!\u00ab", "tokens": ["Denn", "es", "soll", "wie\u00b7der", "Weih\u00b7nach\u00b7ten", "wer\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "NN", "VAINF", "$.", "$("], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.17": {"text": "Ich sprach: \u00bbO lieber Herre Christ,", "tokens": ["Ich", "sprach", ":", "\u00bb", "O", "lie\u00b7ber", "Her\u00b7re", "Christ", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ITJ", "ADV", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Meine Reise fast zu Ende ist;", "tokens": ["Mei\u00b7ne", "Rei\u00b7se", "fast", "zu", "En\u00b7de", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.19": {"text": "Ich soll nur noch in diese Stadt,", "tokens": ["Ich", "soll", "nur", "noch", "in", "die\u00b7se", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Wo's eitel gute Kinder hat.\u00ab", "tokens": ["Wo's", "ei\u00b7tel", "gu\u00b7te", "Kin\u00b7der", "hat", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJD", "ADJA", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "\u2013 \u00bbHast denn das S\u00e4cklein auch bei dir?\u00ab", "tokens": ["\u2013", "\u00bb", "Hast", "denn", "das", "S\u00e4ck\u00b7lein", "auch", "bei", "dir", "?", "\u00ab"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "$(", "VAFIN", "ADV", "ART", "NN", "ADV", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Ich sprach: \u00bbDas S\u00e4cklein, das ist hier;", "tokens": ["Ich", "sprach", ":", "\u00bb", "Das", "S\u00e4ck\u00b7lein", ",", "das", "ist", "hier", ";"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ART", "NN", "$,", "PDS", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Denn \u00c4pfel, Nu\u00df und Mandelkern", "tokens": ["Denn", "\u00c4p\u00b7fel", ",", "Nu\u00df", "und", "Man\u00b7del\u00b7kern"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Fressen fromme Kinder gern.\u00ab", "tokens": ["Fres\u00b7sen", "from\u00b7me", "Kin\u00b7der", "gern", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADJA", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "\u2013 \u00bbHast denn die Rute auch bei dir?\u00ab", "tokens": ["\u2013", "\u00bb", "Hast", "denn", "die", "Ru\u00b7te", "auch", "bei", "dir", "?", "\u00ab"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "$(", "VAFIN", "ADV", "ART", "NN", "ADV", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Ich sprach: \u00bbDie Rute, die ist hier;", "tokens": ["Ich", "sprach", ":", "\u00bb", "Die", "Ru\u00b7te", ",", "die", "ist", "hier", ";"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ART", "NN", "$,", "PRELS", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Doch f\u00fcr die Kinder nur, die schlechten,", "tokens": ["Doch", "f\u00fcr", "die", "Kin\u00b7der", "nur", ",", "die", "schlech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Die trifft sie auf den Teil, den rechten.\u00ab", "tokens": ["Die", "trifft", "sie", "auf", "den", "Teil", ",", "den", "rech\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Christkindlein sprach: \u00bbSo ist es recht;", "tokens": ["Christ\u00b7kin\u00b7dlein", "sprach", ":", "\u00bb", "So", "ist", "es", "recht", ";"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "$(", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "So geh mit Gott, mein treuer Knecht!\u00ab", "tokens": ["So", "geh", "mit", "Gott", ",", "mein", "treu\u00b7er", "Knecht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Von drau\u00df' vom Walde komm ich her;", "tokens": ["Von", "drau\u00df'", "vom", "Wal\u00b7de", "komm", "ich", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Ich mu\u00df euch sagen, es weihnachtet sehr!", "tokens": ["Ich", "mu\u00df", "euch", "sa\u00b7gen", ",", "es", "weih\u00b7nach\u00b7tet", "sehr", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.33": {"text": "Nun sprecht, wie ich's hierinnen find!", "tokens": ["Nun", "sprecht", ",", "wie", "ich's", "hie\u00b7rin\u00b7nen", "find", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "PIS", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Sind's gute Kind, sind's b\u00f6se Kind?", "tokens": ["Sin\u00b7d's", "gu\u00b7te", "Kind", ",", "sin\u00b7d's", "b\u00f6\u00b7se", "Kind", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}}}}