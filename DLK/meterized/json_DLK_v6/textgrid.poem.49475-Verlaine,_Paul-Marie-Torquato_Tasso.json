{"textgrid.poem.49475": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "Torquato Tasso", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Dichter ist ein Tor, in Wagnisse verloren,", "tokens": ["Der", "Dich\u00b7ter", "ist", "ein", "Tor", ",", "in", "Wag\u00b7nis\u00b7se", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der rastlos tr\u00e4umt von Kampf und sagenhafter Schlacht", "tokens": ["Der", "rast\u00b7los", "tr\u00e4umt", "von", "Kampf", "und", "sa\u00b7gen\u00b7haf\u00b7ter", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und tausend Taten singt, die er zu eig'nen macht,", "tokens": ["Und", "tau\u00b7send", "Ta\u00b7ten", "singt", ",", "die", "er", "zu", "eig'\u00b7nen", "macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "$,", "PRELS", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich und dem k\u00fcnftigen Geschlecht, das er erkoren.", "tokens": ["Sich", "und", "dem", "k\u00fcnf\u00b7ti\u00b7gen", "Ge\u00b7schlecht", ",", "das", "er", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "KON", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und sp\u00e4ter kalt, was ihn f\u00fcr Schmerzen auch durchbohren,", "tokens": ["Und", "sp\u00e4\u00b7ter", "kalt", ",", "was", "ihn", "f\u00fcr", "Schmer\u00b7zen", "auch", "durch\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "$,", "PRELS", "PPER", "APPR", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "(olymp'sche Trauer, s\u00e4um'ger Ruhm und Leidensnacht,)", "tokens": ["(", "o\u00b7lym\u00b7p'\u00b7sche", "Trau\u00b7er", ",", "s\u00e4um'\u00b7ger", "Ruhm", "und", "Lei\u00b7dens\u00b7nacht", ",", ")"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "ADJA", "NN", "KON", "NN", "$,", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "F\u00fchlt er von allzu k\u00fchner Glut sein Herz entfacht,", "tokens": ["F\u00fchlt", "er", "von", "all\u00b7zu", "k\u00fch\u00b7ner", "Glut", "sein", "Herz", "ent\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PTKA", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und schon sein Name zeigt ihn zur Tortur geboren.", "tokens": ["Und", "schon", "sein", "Na\u00b7me", "zeigt", "ihn", "zur", "Tor\u00b7tur", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch ist sein Name Gl\u00fcck! Ob froh, ob tr\u00fcb sein Herz", "tokens": ["Doch", "ist", "sein", "Na\u00b7me", "Gl\u00fcck", "!", "Ob", "froh", ",", "ob", "tr\u00fcb", "sein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "NN", "$.", "KOUS", "ADJD", "$,", "KOUS", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Freudenrausch des Tags, spukhafter N\u00e4chte Schmerz,", "tokens": ["Im", "Freu\u00b7den\u00b7rausch", "des", "Tags", ",", "spuk\u00b7haf\u00b7ter", "N\u00e4ch\u00b7te", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bis wechselvoll gequ\u00e4lt er stirbt an dieser Wunde.", "tokens": ["Bis", "wech\u00b7sel\u00b7voll", "ge\u00b7qu\u00e4lt", "er", "stirbt", "an", "die\u00b7ser", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Armida, Leonore, Traum und Wirklichkeit,", "tokens": ["Ar\u00b7mi\u00b7da", ",", "Le\u00b7o\u00b7no\u00b7re", ",", "Traum", "und", "Wirk\u00b7lich\u00b7keit", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und er ist toll und stirbt f\u00fcr eine fl\u00fccht'ge Stunde", "tokens": ["Und", "er", "ist", "toll", "und", "stirbt", "f\u00fcr", "ei\u00b7ne", "fl\u00fccht'\u00b7ge", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und lebt von neuem auf in der Unsterblichkeit!", "tokens": ["Und", "lebt", "von", "neu\u00b7em", "auf", "in", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "APPR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}