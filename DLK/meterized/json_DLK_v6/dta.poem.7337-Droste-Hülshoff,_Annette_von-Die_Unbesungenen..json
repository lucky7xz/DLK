{"dta.poem.7337": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die Unbesungenen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "'s giebt Gr\u00e4ber wo die Klage schweigt,", "tokens": ["'s", "giebt", "Gr\u00e4\u00b7ber", "wo", "die", "Kla\u00b7ge", "schweigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und nur das Herz von innen blutet,", "tokens": ["Und", "nur", "das", "Herz", "von", "in\u00b7nen", "blu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Tropfen in die Wimper steigt,", "tokens": ["Kein", "Trop\u00b7fen", "in", "die", "Wim\u00b7per", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und doch die Lava drinnen fluthet;", "tokens": ["Und", "doch", "die", "La\u00b7va", "drin\u00b7nen", "flut\u00b7het", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "'s giebt Gr\u00e4ber, die wie Wetternacht", "tokens": ["'s", "giebt", "Gr\u00e4\u00b7ber", ",", "die", "wie", "Wet\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "PRELS", "KOKOM", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "An unserm Horizonte stehn", "tokens": ["An", "un\u00b7serm", "Ho\u00b7ri\u00b7zon\u00b7te", "stehn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und alles Leben niederhalten,", "tokens": ["Und", "al\u00b7les", "Le\u00b7ben", "nie\u00b7der\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und doch, wenn Abendroth erwacht,", "tokens": ["Und", "doch", ",", "wenn", "A\u00b7ben\u00b7droth", "er\u00b7wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mit ihren goldnen Fl\u00fcgeln wehn", "tokens": ["Mit", "ih\u00b7ren", "gold\u00b7nen", "Fl\u00fc\u00b7geln", "wehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wie milde Seraphimgestalten.", "tokens": ["Wie", "mil\u00b7de", "Se\u00b7ra\u00b7phim\u00b7ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zu heilig sind sie f\u00fcr das Lied,", "tokens": ["Zu", "hei\u00b7lig", "sind", "sie", "f\u00fcr", "das", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und m\u00e4chtge Redner doch vor Allen,", "tokens": ["Und", "m\u00e4cht\u00b7ge", "Red\u00b7ner", "doch", "vor", "Al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie nennen dir was nimmer schied,", "tokens": ["Sie", "nen\u00b7nen", "dir", "was", "nim\u00b7mer", "schied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PWS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was nie und nimmer kann zerfallen;", "tokens": ["Was", "nie", "und", "nim\u00b7mer", "kann", "zer\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "KON", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O, wenn dich Zweifel dr\u00fcckt herab,", "tokens": ["O", ",", "wenn", "dich", "Zwei\u00b7fel", "dr\u00fcckt", "her\u00b7ab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und m\u00f6chtest athmen Aetherluft,", "tokens": ["Und", "m\u00f6ch\u00b7test", "ath\u00b7men", "A\u00b7e\u00b7ther\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVINF", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Und m\u00f6chtest schauen Seraphsfl\u00fcgel,", "tokens": ["Und", "m\u00f6ch\u00b7test", "schau\u00b7en", "Se\u00b7raphs\u00b7fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dann tritt an deines Vaters Grab!", "tokens": ["Dann", "tritt", "an", "dei\u00b7nes", "Va\u00b7ters", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dann tritt an deines Bruders Gruft!", "tokens": ["Dann", "tritt", "an", "dei\u00b7nes", "Bru\u00b7ders", "Gruft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dann tritt an deines Kindes H\u00fcgel!", "tokens": ["Dann", "tritt", "an", "dei\u00b7nes", "Kin\u00b7des", "H\u00fc\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}