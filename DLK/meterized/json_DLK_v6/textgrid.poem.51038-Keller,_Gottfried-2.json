{"textgrid.poem.51038": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So manchmal werd ich irre an der Stunde,", "tokens": ["So", "manch\u00b7mal", "werd", "ich", "ir\u00b7re", "an", "der", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An Tag und Jahr, ach, an der ganzen Zeit;", "tokens": ["An", "Tag", "und", "Jahr", ",", "ach", ",", "an", "der", "gan\u00b7zen", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ITJ", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es g\u00e4rt und tost, doch mitten auf dem Grunde", "tokens": ["Es", "g\u00e4rt", "und", "tost", ",", "doch", "mit\u00b7ten", "auf", "dem", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist es so still, so kalt, so zugeschneit!", "tokens": ["Ist", "es", "so", "still", ",", "so", "kalt", ",", "so", "zu\u00b7ge\u00b7schneit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Habt ihr euch auf ein neues Jahr gefreut,", "tokens": ["Habt", "ihr", "euch", "auf", "ein", "neu\u00b7es", "Jahr", "ge\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Zukunft preisend mit beredtem Munde?", "tokens": ["Die", "Zu\u00b7kunft", "prei\u00b7send", "mit", "be\u00b7red\u00b7tem", "Mun\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es rollt heran und schleudert, o wie weit!", "tokens": ["Es", "rollt", "he\u00b7ran", "und", "schleu\u00b7dert", ",", "o", "wie", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "$,", "FM", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Euch r\u00fcckw\u00e4rts. \u2013 Ihr versinkt im alten Schlunde.", "tokens": ["Euch", "r\u00fcck\u00b7w\u00e4rts", ".", "\u2013", "Ihr", "ver\u00b7sinkt", "im", "al\u00b7ten", "Schlun\u00b7de", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$.", "$(", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch kann ich nie die Hoffnung ganz verlieren,", "tokens": ["Doch", "kann", "ich", "nie", "die", "Hoff\u00b7nung", "ganz", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind auch noch viele N\u00e4chte zu durchtr\u00e4umen,", "tokens": ["Sind", "auch", "noch", "vie\u00b7le", "N\u00e4ch\u00b7te", "zu", "durch\u00b7tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu schlafen, zu durchwachen, zu durchfrieren!", "tokens": ["Zu", "schla\u00b7fen", ",", "zu", "durch\u00b7wa\u00b7chen", ",", "zu", "durch\u00b7frie\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So wahr erz\u00fcrnte Wasser m\u00fcssen sch\u00e4umen,", "tokens": ["So", "wahr", "er\u00b7z\u00fcrn\u00b7te", "Was\u00b7ser", "m\u00fcs\u00b7sen", "sch\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mu\u00df, ob der tiefsten Nacht, Tag triumphieren,", "tokens": ["Mu\u00df", ",", "ob", "der", "tiefs\u00b7ten", "Nacht", ",", "Tag", "tri\u00b7um\u00b7phie\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "$,", "KOUS", "ART", "ADJA", "NN", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sieh: schon bricht es rot aus Wolkens\u00e4umen!", "tokens": ["Und", "sieh", ":", "schon", "bricht", "es", "rot", "aus", "Wol\u00b7ken\u00b7s\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}