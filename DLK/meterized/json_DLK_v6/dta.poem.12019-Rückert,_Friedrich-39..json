{"dta.poem.12019": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das sagt dir dein Gef\u00fchl, da\u00df du kannst s\u00fcndigen;", "tokens": ["Das", "sagt", "dir", "dein", "Ge\u00b7f\u00fchl", ",", "da\u00df", "du", "kannst", "s\u00fcn\u00b7di\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warum du's kannst, wer kann dir das verk\u00fcndigen?", "tokens": ["Wa\u00b7rum", "du's", "kannst", ",", "wer", "kann", "dir", "das", "ver\u00b7k\u00fcn\u00b7di\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VMFIN", "$,", "PWS", "VMFIN", "PPER", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Weisen sagen dir: du kannsts, um frei zu seyn.", "tokens": ["Die", "Wei\u00b7sen", "sa\u00b7gen", "dir", ":", "du", "kannsts", ",", "um", "frei", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "$,", "KOUI", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch warum r\u00e4umte Gott dir diese Freiheit ein?", "tokens": ["Doch", "wa\u00b7rum", "r\u00e4um\u00b7te", "Gott", "dir", "die\u00b7se", "Frei\u00b7heit", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "NN", "PPER", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Weil dich, sein Bild, er nicht zum Werkzeug wollt' erniedern.", "tokens": ["Weil", "dich", ",", "sein", "Bild", ",", "er", "nicht", "zum", "Werk\u00b7zeug", "wollt'", "er\u00b7nie\u00b7dern", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "PTKNEG", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch darauf kann sogleich der schlichte Sinn erwiedern:", "tokens": ["Doch", "da\u00b7rauf", "kann", "sog\u00b7leich", "der", "schlich\u00b7te", "Sinn", "er\u00b7wie\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein K\u00f6nig g\u00f6ttlich gut, h\u00e4tt' er dazu die Macht,", "tokens": ["Ein", "K\u00f6\u00b7nig", "g\u00f6tt\u00b7lich", "gut", ",", "h\u00e4tt'", "er", "da\u00b7zu", "die", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "$,", "VAFIN", "PPER", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Seinen h\u00e4tt' er frei und gut zugleich gemacht.", "tokens": ["Die", "Sei\u00b7nen", "h\u00e4tt'", "er", "frei", "und", "gut", "zu\u00b7gleich", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSS", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Da er nun nicht zugleich uns gab die beiden Gaben,", "tokens": ["Da", "er", "nun", "nicht", "zu\u00b7gleich", "uns", "gab", "die", "bei\u00b7den", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ADV", "PPER", "VVFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird der Allm\u00e4chtige dazu die Macht nicht haben.", "tokens": ["Wird", "der", "All\u00b7m\u00e4ch\u00b7ti\u00b7ge", "da\u00b7zu", "die", "Macht", "nicht", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PAV", "ART", "NN", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Was ists nun, das die Hand der Allmacht also band?", "tokens": ["Was", "ists", "nun", ",", "das", "die", "Hand", "der", "All\u00b7macht", "al\u00b7so", "band", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "$,", "PRELS", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ist der Menschenwitz gekommen an den Rand.", "tokens": ["Da", "ist", "der", "Men\u00b7schen\u00b7witz", "ge\u00b7kom\u00b7men", "an", "den", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und \u00fcberall wird er zu solchem Rande kommen,", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "wird", "er", "zu", "sol\u00b7chem", "Ran\u00b7de", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie er das R\u00e4thsel sonst zu l\u00f6sen unternommen.", "tokens": ["Wie", "er", "das", "R\u00e4th\u00b7sel", "sonst", "zu", "l\u00f6\u00b7sen", "un\u00b7ter\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Darum zur\u00fcck in dich! du bist durch Gottes Kraft", "tokens": ["Da\u00b7rum", "zu\u00b7r\u00fcck", "in", "dich", "!", "du", "bist", "durch", "Got\u00b7tes", "Kraft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "PTKVZ", "APPR", "PPER", "$.", "PPER", "VAFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein R\u00e4thsel zwar, doch das ist dir nicht r\u00e4thselhaft:", "tokens": ["Ein", "R\u00e4th\u00b7sel", "zwar", ",", "doch", "das", "ist", "dir", "nicht", "r\u00e4th\u00b7sel\u00b7haft", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KON", "PDS", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-----+-+", "measure": "unknown.measure.tetra"}}, "stanza.9": {"line.1": {"text": "Da\u00df du nicht s\u00fcnd'gen mu\u00dft, wiewol du s\u00fcnd'gen kannst;", "tokens": ["Da\u00df", "du", "nicht", "s\u00fcn\u00b7d'\u00b7gen", "mu\u00dft", ",", "wie\u00b7wol", "du", "s\u00fcn\u00b7d'\u00b7gen", "kannst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df du's nicht sollst, und dazu Gottes Kraft gewannst.", "tokens": ["Da\u00df", "du's", "nicht", "sollst", ",", "und", "da\u00b7zu", "Got\u00b7tes", "Kraft", "ge\u00b7wannst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "VMFIN", "$,", "KON", "PAV", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}