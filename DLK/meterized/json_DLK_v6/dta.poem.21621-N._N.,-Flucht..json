{"dta.poem.21621": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Flucht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Lieber will ich ohne Zagen", "tokens": ["Lie\u00b7ber", "will", "ich", "oh\u00b7ne", "Za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schreiten durch der H\u00f6lle Thor,", "tokens": ["Schrei\u00b7ten", "durch", "der", "H\u00f6l\u00b7le", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als wie Deine N\u00e4he tragen,", "tokens": ["Als", "wie", "Dei\u00b7ne", "N\u00e4\u00b7he", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun ich ewig Dich verlor.", "tokens": ["Nun", "ich", "e\u00b7wig", "Dich", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deine sanften Augen rufen", "tokens": ["Dei\u00b7ne", "sanf\u00b7ten", "Au\u00b7gen", "ru\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die Sonnenbahn mein Herz,", "tokens": ["In", "die", "Son\u00b7nen\u00b7bahn", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich mu\u00df die schwarzen Stufen", "tokens": ["Und", "ich", "mu\u00df", "die", "schwar\u00b7zen", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Schauernd schreiten niederw\u00e4rts.", "tokens": ["Schau\u00b7ernd", "schrei\u00b7ten", "nie\u00b7der\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Leise Liebesworte werben", "tokens": ["Lei\u00b7se", "Lie\u00b7bes\u00b7wor\u00b7te", "wer\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeden Tropfen Blutes Dir.", "tokens": ["Je\u00b7den", "Trop\u00b7fen", "Blu\u00b7tes", "Dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df mich ringen, la\u00df mich sterben,", "tokens": ["La\u00df", "mich", "rin\u00b7gen", ",", "la\u00df", "mich", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber schaue nicht nach mir.", "tokens": ["A\u00b7ber", "schau\u00b7e", "nicht", "nach", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}