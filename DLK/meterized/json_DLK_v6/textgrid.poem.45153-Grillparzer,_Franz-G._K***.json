{"textgrid.poem.45153": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "G. K***", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich m\u00f6chte gern, ich w\u00fcnschte wohl,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "gern", ",", "ich", "w\u00fcnschte", "wohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und sollte wohl vor allen,", "tokens": ["Und", "soll\u00b7te", "wohl", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "APPR", "PIAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn sichs f\u00fcgte, wie es recht,", "tokens": ["Und", "wenn", "sichs", "f\u00fcg\u00b7te", ",", "wie", "es", "recht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "VVFIN", "$,", "PWAV", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich h\u00e4tte dran Gefallen.", "tokens": ["Ich", "h\u00e4t\u00b7te", "dran", "Ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So kams, da\u00df, der ein Hauptwort schien,", "tokens": ["So", "kams", ",", "da\u00df", ",", "der", "ein", "Haupt\u00b7wort", "schien", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Beiwort d\u00fcrftig ward,", "tokens": ["Zum", "Bei\u00b7wort", "d\u00fcrf\u00b7tig", "ward", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn nicht vielmehr ein Zeitwort gar", "tokens": ["Wenn", "nicht", "viel\u00b7mehr", "ein", "Zeit\u00b7wort", "gar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In der verbindenden Art.", "tokens": ["In", "der", "ver\u00b7bin\u00b7den\u00b7den", "Art", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}