{"textgrid.poem.41893": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: S\u00fc\u00dfer Traum, der mich umfangen,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00dfer Traum, der mich umfangen,", "tokens": ["S\u00fc\u00b7\u00dfer", "Traum", ",", "der", "mich", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frisches Leben, lichter May,", "tokens": ["Fri\u00b7sches", "Le\u00b7ben", ",", "lich\u00b7ter", "May", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Neues Ahnen und Verlangen,", "tokens": ["Neu\u00b7es", "Ah\u00b7nen", "und", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, wie gingt ihr schnell vorbey!", "tokens": ["Ach", ",", "wie", "gingt", "ihr", "schnell", "vor\u00b7bey", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit dem Lenz sah ich euch bl\u00fchen,", "tokens": ["Mit", "dem", "Lenz", "sah", "ich", "euch", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit dem Lenz seyd ihr entflogen;", "tokens": ["Mit", "dem", "Lenz", "seyd", "ihr", "ent\u00b7flo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur die Trauer giebt mir Kunde", "tokens": ["Nur", "die", "Trau\u00b7er", "giebt", "mir", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Von dem fr\u00fch verwelkten Gl\u00fcck.", "tokens": ["Von", "dem", "fr\u00fch", "ver\u00b7welk\u00b7ten", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Stunden habt ihr mir verliehen", "tokens": ["Stun\u00b7den", "habt", "ihr", "mir", "ver\u00b7lie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und um's Leben mich betrogen!", "tokens": ["Und", "um's", "Le\u00b7ben", "mich", "be\u00b7tro\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Ach, wer bringt nur Eine Stunde", "tokens": ["Ach", ",", "wer", "bringt", "nur", "Ei\u00b7ne", "Stun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Jener holden Zeit zur\u00fcck!", "tokens": ["Je\u00b7ner", "hol\u00b7den", "Zeit", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mag es duften jetzt und gr\u00fcnen,", "tokens": ["Mag", "es", "duf\u00b7ten", "jetzt", "und", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADV", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mag's erstarren um mich her;", "tokens": ["Mag's", "er\u00b7star\u00b7ren", "um", "mich", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was mir hold und tr\u00fcb' erschienen,", "tokens": ["Was", "mir", "hold", "und", "tr\u00fcb'", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freut und schmerzt mich jetzt nicht mehr.", "tokens": ["Freut", "und", "schmerzt", "mich", "jetzt", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur in uns ist alles Leben!", "tokens": ["Nur", "in", "uns", "ist", "al\u00b7les", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit dem Sch\u00f6nen nur im Bunde,", "tokens": ["Mit", "dem", "Sch\u00f6\u00b7nen", "nur", "im", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schwinden rasch die dunkeln Tage,", "tokens": ["Schwin\u00b7den", "rasch", "die", "dun\u00b7keln", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weilt der lichte Augenblick.", "tokens": ["Weilt", "der", "lich\u00b7te", "Au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Schmerz kann Schmerz nur sehn und geben;", "tokens": ["Schmerz", "kann", "Schmerz", "nur", "sehn", "und", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "NN", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Einsam n\u00e4hr' ich meine Wunde,", "tokens": ["Ein\u00b7sam", "n\u00e4hr'", "ich", "mei\u00b7ne", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und mit stets erneuter Klage", "tokens": ["Und", "mit", "stets", "er\u00b7neu\u00b7ter", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Traur' ich um's verlohrne Gl\u00fcck.", "tokens": ["Traur'", "ich", "um's", "ver\u00b7lohr\u00b7ne", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wechselt doch in bunter Reihe", "tokens": ["Wech\u00b7selt", "doch", "in", "bun\u00b7ter", "Rei\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stets beweglich Bild und Bild;", "tokens": ["Stets", "be\u00b7weg\u00b7lich", "Bild", "und", "Bild", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur die Liebe weilt, die Treue,", "tokens": ["Nur", "die", "Lie\u00b7be", "weilt", ",", "die", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur der Schmerz wird nie gestillt.", "tokens": ["Nur", "der", "Schmerz", "wird", "nie", "ge\u00b7stillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df doch Alles wiederkommen,", "tokens": ["Mu\u00df", "doch", "Al\u00b7les", "wie\u00b7der\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieblich, wie es einst entflogen;", "tokens": ["Lieb\u00b7lich", ",", "wie", "es", "einst", "ent\u00b7flo\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch, ein Schattenbild der Klage,", "tokens": ["Doch", ",", "ein", "Schat\u00b7ten\u00b7bild", "der", "Kla\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kehrt allein das todte Gl\u00fcck.", "tokens": ["Kehrt", "al\u00b7lein", "das", "tod\u00b7te", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.9": {"text": "Duft und Bl\u00fcthen sind verschwommen,", "tokens": ["Duft", "und", "Bl\u00fc\u00b7then", "sind", "ver\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und die V\u00f6gel fortgezogen \u2013", "tokens": ["Und", "die", "V\u00f6\u00b7gel", "fort\u00b7ge\u00b7zo\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Ach, wer bringt die sch\u00f6nen Tage,", "tokens": ["Ach", ",", "wer", "bringt", "die", "sch\u00f6\u00b7nen", "Ta\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Jene holde Zeit zur\u00fcck?", "tokens": ["Je\u00b7ne", "hol\u00b7de", "Zeit", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}