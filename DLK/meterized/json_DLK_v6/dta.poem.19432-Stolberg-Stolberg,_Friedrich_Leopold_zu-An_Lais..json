{"dta.poem.19432": {"metadata": {"author": {"name": "Stolberg-Stolberg, Friedrich Leopold zu", "birth": "N.A.", "death": "N.A."}, "title": "An Lais.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1779", "urn": "urn:nbn:de:kobv:b4-20589-5", "language": ["de:0.99"], "booktitle": "Stolberg-Stolberg, Christian zu; Stolberg-Stolberg, Friedrich Leopold zu: Gedichte. Leipzig, 1779."}, "poem": {"stanza.1": {"line.1": {"text": "Weil noch leicht, wie ein Nektartraum,", "tokens": ["Weil", "noch", "leicht", ",", "wie", "ein", "Nekt\u00b7ar\u00b7traum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Dir das Leben verfliegt; weil noch der l\u00e4chelnden", "tokens": ["Dir", "das", "Le\u00b7ben", "ver\u00b7fliegt", ";", "weil", "noch", "der", "l\u00e4\u00b7cheln\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "VVPP", "$.", "KOUS", "ADV", "ART", "ADJA"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Hebe Pinsel, in Lebenskraft", "tokens": ["He\u00b7be", "Pin\u00b7sel", ",", "in", "Le\u00b7bens\u00b7kraft"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Eingetauchet, den Mund \u00e4hnlich dem Morgenroth,", "tokens": ["Ein\u00b7ge\u00b7tau\u00b7chet", ",", "den", "Mund", "\u00e4hn\u00b7lich", "dem", "Mor\u00b7gen\u00b7roth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "ADJD", "ART", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.5": {"text": "Rosenwallend die Wange malt;", "tokens": ["Ro\u00b7sen\u00b7wal\u00b7lend", "die", "Wan\u00b7ge", "malt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Weil noch t\u00e4glich dein Blick, hell, wie der Abend-", "tokens": ["Weil", "noch", "t\u00e4g\u00b7lich", "dein", "Blick", ",", "hell", ",", "wie", "der", "A\u00b7ben\u00b7d"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJD", "PPOSAT", "NN", "$,", "ADJD", "$,", "PWAV", "ART", "TRUNC"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.7": {"text": "stern,", "tokens": ["stern", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Aber treffend, wie Sirius,", "tokens": ["A\u00b7ber", "tref\u00b7fend", ",", "wie", "Si\u00b7rius", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Die hintaumelnde Schaar deiner Gefangnen", "tokens": ["Die", "hin\u00b7tau\u00b7meln\u00b7de", "Schaar", "dei\u00b7ner", "Ge\u00b7fang\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "--+--+---+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "mehrt;", "tokens": ["mehrt", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Darum trozest du, th\u00f6rige", "tokens": ["Da\u00b7rum", "tro\u00b7zest", "du", ",", "th\u00f6\u00b7ri\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PAV", "VVFIN", "PPER", "$,", "ADJA"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Lais, k\u00fcnftiger Zeit, welche die fliegenden", "tokens": ["Lais", ",", "k\u00fcnf\u00b7ti\u00b7ger", "Zeit", ",", "wel\u00b7che", "die", "flie\u00b7gen\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "PRELS", "ART", "ADJA"], "meter": "-+--++--+--", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Stunden bringen, Unkundige!", "tokens": ["Stun\u00b7den", "brin\u00b7gen", ",", "Un\u00b7kun\u00b7di\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "$."], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.14": {"text": "Wird dir ewig die Glut schmachtender J\u00fcnglinge,", "tokens": ["Wird", "dir", "e\u00b7wig", "die", "Glut", "schmach\u00b7ten\u00b7der", "J\u00fcng\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--++-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Dir die Bl\u00e4sse der Eifersucht", "tokens": ["Dir", "die", "Bl\u00e4s\u00b7se", "der", "Ei\u00b7fer\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.16": {"text": "Ewig fr\u00f6hnen? Auch dich werden die Grazien", "tokens": ["E\u00b7wig", "fr\u00f6h\u00b7nen", "?", "Auch", "dich", "wer\u00b7den", "die", "Gra\u00b7zi\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVINF", "$.", "ADV", "PPER", "VAFIN", "ART", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Einst verlassen! der siegenden", "tokens": ["Einst", "ver\u00b7las\u00b7sen", "!", "der", "sie\u00b7gen\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVINF", "$.", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "K\u00fcnste jede! Dein Lenz schwindet auf neidender", "tokens": ["K\u00fcns\u00b7te", "je\u00b7de", "!", "Dein", "Lenz", "schwin\u00b7det", "auf", "nei\u00b7den\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "$.", "PPOSAT", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Weste Fittig! bald hauchen sie", "tokens": ["Wes\u00b7te", "Fit\u00b7tig", "!", "bald", "hau\u00b7chen", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ADV", "VVFIN", "PPER"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.20": {"text": "Deine Bl\u00fcthen herab! dann wird die bulende", "tokens": ["Dei\u00b7ne", "Bl\u00fc\u00b7then", "her\u00b7ab", "!", "dann", "wird", "die", "bu\u00b7len\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "$.", "ADV", "VAFIN", "ART", "NN"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "Lais seufzen: ihr rosigen", "tokens": ["Lais", "seuf\u00b7zen", ":", "ihr", "ro\u00b7si\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "VVINF", "$.", "PPOSAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "Tage, kommet zur\u00fcck! aber die rosigen", "tokens": ["Ta\u00b7ge", ",", "kom\u00b7met", "zu\u00b7r\u00fcck", "!", "a\u00b7ber", "die", "ro\u00b7si\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "$.", "ADV", "ART", "ADJA"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.23": {"text": "Tage flohen! Verh\u00fclle dich,", "tokens": ["Ta\u00b7ge", "flo\u00b7hen", "!", "Ver\u00b7h\u00fcl\u00b7le", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "NN", "PPER", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.24": {"text": "Lais! da\u00df der Triumph deiner Gespielen dich,", "tokens": ["Lais", "!", "da\u00df", "der", "Tri\u00b7umph", "dei\u00b7ner", "Ge\u00b7spie\u00b7len", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "KOUS", "ART", "NN", "PPOSAT", "NN", "PPER", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.25": {"text": "Die Moral der Matrone dich", "tokens": ["Die", "Mo\u00b7ral", "der", "Mat\u00b7ro\u00b7ne", "dich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "PPER"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Nicht verfolge! der Hohn deiner Entfesselten", "tokens": ["Nicht", "ver\u00b7fol\u00b7ge", "!", "der", "Hohn", "dei\u00b7ner", "Ent\u00b7fes\u00b7sel\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "$.", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.27": {"text": "Dich nicht treffe! denn eisern war", "tokens": ["Dich", "nicht", "tref\u00b7fe", "!", "denn", "ei\u00b7sern", "war"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKNEG", "VVFIN", "$.", "ADV", "ADJD", "VAFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.28": {"text": "Deine Herrschaft! dein Stolz freute der Thr\u00e4-", "tokens": ["Dei\u00b7ne", "Herr\u00b7schaft", "!", "dein", "Stolz", "freu\u00b7te", "der", "Thr\u00e4"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "VVFIN", "ART", "TRUNC"], "meter": "+-+--++--+", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "nen sich,", "tokens": ["nen", "sich", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,"], "meter": "--", "measure": "unknown.measure.zero"}, "line.30": {"text": "Und der blassen Verzweifelung!", "tokens": ["Und", "der", "blas\u00b7sen", "Ver\u00b7zwei\u00b7fe\u00b7lung", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.31": {"text": "Nun sind Thr\u00e4nen der Schmuck dieser verwel-", "tokens": ["Nun", "sind", "Thr\u00e4\u00b7nen", "der", "Schmuck", "die\u00b7ser", "ver\u00b7wel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "ART", "NN", "PDAT", "TRUNC"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.32": {"text": "kenden", "tokens": ["ken\u00b7den"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.33": {"text": "Wangen! Seufzer erheben nun", "tokens": ["Wan\u00b7gen", "!", "Seuf\u00b7zer", "er\u00b7he\u00b7ben", "nun"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "NN", "VVFIN", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.34": {"text": "Ungeheissen die Brust! jeden erl\u00f6schenden", "tokens": ["Un\u00b7ge\u00b7heis\u00b7sen", "die", "Brust", "!", "je\u00b7den", "er\u00b7l\u00f6\u00b7schen\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "PIAT", "ADJA"], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.35": {"text": "Schimmer deiner gefeierten", "tokens": ["Schim\u00b7mer", "dei\u00b7ner", "ge\u00b7fei\u00b7er\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.36": {"text": "Augen r\u00fcstet die Wuth! Lais, verh\u00fclle dich!", "tokens": ["Au\u00b7gen", "r\u00fcs\u00b7tet", "die", "Wuth", "!", "Lais", ",", "ver\u00b7h\u00fcl\u00b7le", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "NE", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.37": {"text": "Dein ist Schande! Denn eisern war", "tokens": ["Dein", "ist", "Schan\u00b7de", "!", "Denn", "ei\u00b7sern", "war"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "VAFIN", "NN", "$.", "KON", "ADJD", "VAFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.38": {"text": "Deine Herrschaft! Dein Stolz freute der Thr\u00e4-", "tokens": ["Dei\u00b7ne", "Herr\u00b7schaft", "!", "Dein", "Stolz", "freu\u00b7te", "der", "Thr\u00e4"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "VVFIN", "ART", "TRUNC"], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}, "line.39": {"text": "nen sich", "tokens": ["nen", "sich"], "token_info": ["word", "word"], "pos": ["VVFIN", "PRF"], "meter": "--", "measure": "unknown.measure.zero"}, "line.40": {"text": "Und der blassen Verzweifelung!", "tokens": ["Und", "der", "blas\u00b7sen", "Ver\u00b7zwei\u00b7fe\u00b7lung", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "--+--+--", "measure": "anapaest.di.plus"}}}}}