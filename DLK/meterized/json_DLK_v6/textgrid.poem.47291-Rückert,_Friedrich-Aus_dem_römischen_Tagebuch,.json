{"textgrid.poem.47291": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Aus dem r\u00f6mischen Tagebuch,", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr! la\u00df mich nicht im fremden Lande sterben,", "tokens": ["Herr", "!", "la\u00df", "mich", "nicht", "im", "frem\u00b7den", "Lan\u00b7de", "ster\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVIMP", "PPER", "PTKNEG", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo keine Hand die Augen zu mir dr\u00fccket", "tokens": ["Wo", "kei\u00b7ne", "Hand", "die", "Au\u00b7gen", "zu", "mir", "dr\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "ART", "NN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und keine mir den Ort mit Blumen schm\u00fccket,", "tokens": ["Und", "kei\u00b7ne", "mir", "den", "Ort", "mit", "Blu\u00b7men", "schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo man mich hinwirft bei zerbrochnen Scherben.", "tokens": ["Wo", "man", "mich", "hin\u00b7wirft", "bei", "zer\u00b7broch\u00b7nen", "Scher\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Einst w\u00fcnscht' ich eine St\u00e4tte zu erwerben", "tokens": ["Einst", "w\u00fcnscht'", "ich", "ei\u00b7ne", "St\u00e4t\u00b7te", "zu", "er\u00b7wer\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An jenem Orte, der seitdem entr\u00fccket", "tokens": ["An", "je\u00b7nem", "Or\u00b7te", ",", "der", "seit\u00b7dem", "ent\u00b7r\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PAV", "VVFIN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dem Geist ward wie den Augen, wo gepfl\u00fccket", "tokens": ["Dem", "Geist", "ward", "wie", "den", "Au\u00b7gen", ",", "wo", "ge\u00b7pfl\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,", "PWAV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom Tod ich sah die sch\u00f6nste Blum' entf\u00e4rben.", "tokens": ["Vom", "Tod", "ich", "sah", "die", "sch\u00f6ns\u00b7te", "Blum'", "ent\u00b7f\u00e4r\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das waren W\u00fcnsche, die ich that in Reimen,", "tokens": ["Das", "wa\u00b7ren", "W\u00fcn\u00b7sche", ",", "die", "ich", "that", "in", "Rei\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ich, mit Blumenspielwerk \u00fcberh\u00e4ufend", "tokens": ["Als", "ich", ",", "mit", "Blu\u00b7men\u00b7spiel\u00b7werk", "\u00fc\u00b7berh\u00b7\u00e4u\u00b7fend"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Menschengrab, Abg\u00f6tterei getrieben.", "tokens": ["Ein", "Men\u00b7schen\u00b7grab", ",", "Ab\u00b7g\u00f6t\u00b7te\u00b7rei", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Jetzt f\u00fchl' ich still den Ernst im Herzen keimen", "tokens": ["Jetzt", "f\u00fchl'", "ich", "still", "den", "Ernst", "im", "Her\u00b7zen", "kei\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In n\u00e4cht'ger Stund' und flehe, Thr\u00e4nen tr\u00e4ufend:", "tokens": ["In", "n\u00e4cht'\u00b7ger", "Stund'", "und", "fle\u00b7he", ",", "Thr\u00e4\u00b7nen", "tr\u00e4u\u00b7fend", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "VVFIN", "$,", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbherr! la\u00df mich sterben heim bei meinen Lieben!\u00ab", "tokens": ["\u00bb", "herr", "!", "la\u00df", "mich", "ster\u00b7ben", "heim", "bei", "mei\u00b7nen", "Lie\u00b7ben", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$.", "VVIMP", "PPER", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "ADJA", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}