{"textgrid.poem.58570": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Blei des falschen Scheins mu\u00df werden uns entnommen,", "tokens": ["Das", "Blei", "des", "fal\u00b7schen", "Scheins", "mu\u00df", "wer\u00b7den", "uns", "ent\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VMFIN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Schaum der Eigenheit aufs lauterste gefegt;", "tokens": ["Der", "Schaum", "der", "Ei\u00b7gen\u00b7heit", "aufs", "lau\u00b7ters\u00b7te", "ge\u00b7fegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man sieht nur Schaum und Blei, wenn's auf die Prob' gekommen,", "tokens": ["Man", "sieht", "nur", "Schaum", "und", "Blei", ",", "wenn's", "auf", "die", "Prob'", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "NN", "KON", "NN", "$,", "KOUS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df Gott sich wider uns mit Ernst als Richter legt.", "tokens": ["Da\u00df", "Gott", "sich", "wi\u00b7der", "uns", "mit", "Ernst", "als", "Rich\u00b7ter", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "APPR", "PPER", "APPR", "NE", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hier n\u00fctzet die Vernunft mit ihrem Raten wenig,", "tokens": ["Hier", "n\u00fct\u00b7zet", "die", "Ver\u00b7nunft", "mit", "ih\u00b7rem", "Ra\u00b7ten", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hier wird sie gar als blind vom Richteramt gesetzt;", "tokens": ["Hier", "wird", "sie", "gar", "als", "blind", "vom", "Rich\u00b7ter\u00b7amt", "ge\u00b7setzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "KOUS", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Weisheit kommt in dich, wird Richter, Rat und K\u00f6nig,", "tokens": ["Die", "Weis\u00b7heit", "kommt", "in", "dich", ",", "wird", "Rich\u00b7ter", ",", "Rat", "und", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,", "VAFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie es im Anfang war, so wird es dann zuletzt.", "tokens": ["Wie", "es", "im", "An\u00b7fang", "war", ",", "so", "wird", "es", "dann", "zu\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VAFIN", "$,", "ADV", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}