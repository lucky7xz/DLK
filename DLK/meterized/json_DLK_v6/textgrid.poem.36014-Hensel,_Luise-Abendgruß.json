{"textgrid.poem.36014": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "Abendgru\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6r' auch du der Kinder Singen,", "tokens": ["H\u00f6r'", "auch", "du", "der", "Kin\u00b7der", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gute Mutter, mild und wahr,", "tokens": ["Gu\u00b7te", "Mut\u00b7ter", ",", "mild", "und", "wahr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deiner T\u00f6chter Herzen bringen", "tokens": ["Dei\u00b7ner", "T\u00f6ch\u00b7ter", "Her\u00b7zen", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heut dir ihre W\u00fcnsche dar.", "tokens": ["Heut", "dir", "ih\u00b7re", "W\u00fcn\u00b7sche", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Habe Dank f\u00fcr alle Treue,", "tokens": ["Ha\u00b7be", "Dank", "f\u00fcr", "al\u00b7le", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die an uns du fromm ge\u00fcbt,", "tokens": ["Die", "an", "uns", "du", "fromm", "ge\u00b7\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Liebe Mutter, und verzeihe,", "tokens": ["Lie\u00b7be", "Mut\u00b7ter", ",", "und", "ver\u00b7zei\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn wir einmal dich betr\u00fcbt.", "tokens": ["Wenn", "wir", "ein\u00b7mal", "dich", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sieh, wir wollen uns bestreben,", "tokens": ["Sieh", ",", "wir", "wol\u00b7len", "uns", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VMFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deinen Lehren treu zu sein", "tokens": ["Dei\u00b7nen", "Leh\u00b7ren", "treu", "zu", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "PTKZU", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den Weg durchs Erdenleben", "tokens": ["Und", "den", "Weg", "durchs", "Er\u00b7den\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir mit Blumen zu bestreun.", "tokens": ["Dir", "mit", "Blu\u00b7men", "zu", "be\u00b7streun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "La\u00df uns deine Stirn' umwinden", "tokens": ["La\u00df", "uns", "dei\u00b7ne", "Stirn'", "um\u00b7win\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem Kranz von Immergr\u00fcn,", "tokens": ["Mit", "dem", "Kranz", "von", "Im\u00b7mer\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blumen konnten wir nicht finden,", "tokens": ["Blu\u00b7men", "konn\u00b7ten", "wir", "nicht", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach! sie mu\u00dften l\u00e4ngst verbl\u00fchn.", "tokens": ["Ach", "!", "sie", "mu\u00df\u00b7ten", "l\u00e4ngst", "ver\u00b7bl\u00fchn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber Gottes Frieden kr\u00f6ne", "tokens": ["A\u00b7ber", "Got\u00b7tes", "Frie\u00b7den", "kr\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Stirn mit Heiterkeit,", "tokens": ["Dei\u00b7ne", "Stirn", "mit", "Hei\u00b7ter\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsre Liebe auch versch\u00f6ne", "tokens": ["Uns\u00b7re", "Lie\u00b7be", "auch", "ver\u00b7sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir des Lebens herben Streit.", "tokens": ["Dir", "des", "Le\u00b7bens", "her\u00b7ben", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}