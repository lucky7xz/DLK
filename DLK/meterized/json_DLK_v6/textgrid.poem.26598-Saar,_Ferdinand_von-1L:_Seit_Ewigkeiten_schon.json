{"textgrid.poem.26598": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seit Ewigkeiten schon", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit Ewigkeiten schon", "tokens": ["Seit", "E\u00b7wig\u00b7kei\u00b7ten", "schon"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Werden ausgesprochen Worte der Wahrheit und Weisheit.", "tokens": ["Wer\u00b7den", "aus\u00b7ge\u00b7spro\u00b7chen", "Wor\u00b7te", "der", "Wahr\u00b7heit", "und", "Weis\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und seit Ewigkeiten auch", "tokens": ["Und", "seit", "E\u00b7wig\u00b7kei\u00b7ten", "auch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden vernommen sie,", "tokens": ["Wer\u00b7den", "ver\u00b7nom\u00b7men", "sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Werden aufgeschrieben", "tokens": ["Wer\u00b7den", "auf\u00b7ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word"], "pos": ["VAFIN", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und \u00fcberliefert der Menschheit", "tokens": ["Und", "\u00fc\u00b7berl\u00b7ie\u00b7fert", "der", "Menschheit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Als heil'ges Verm\u00e4chtni\u00df.", "tokens": ["Als", "heil'\u00b7ges", "Ver\u00b7m\u00e4cht\u00b7ni\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Aber immer noch", "tokens": ["A\u00b7ber", "im\u00b7mer", "noch"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Herrschen und walten", "tokens": ["Herr\u00b7schen", "und", "wal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wahn und Thorheit.", "tokens": ["Wahn", "und", "Thor\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Immer noch", "tokens": ["Im\u00b7mer", "noch"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Erkl\u00e4rt ein neues Geschlecht", "tokens": ["Er\u00b7kl\u00e4rt", "ein", "neu\u00b7es", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Den Irrthum der Vorgebor'nen,", "tokens": ["Den", "Irr\u00b7thum", "der", "Vor\u00b7ge\u00b7bor'\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Blind jedoch f\u00fcr den eigenen", "tokens": ["Blind", "je\u00b7doch", "f\u00fcr", "den", "ei\u00b7ge\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.8": {"text": "Und taub f\u00fcr den Rufenden in der W\u00fcste.", "tokens": ["Und", "taub", "f\u00fcr", "den", "Ru\u00b7fen\u00b7den", "in", "der", "W\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Das bedenke, einsam ringender Geist,", "tokens": ["Das", "be\u00b7den\u00b7ke", ",", "ein\u00b7sam", "rin\u00b7gen\u00b7der", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und nicht vertr\u00f6ste dich,", "tokens": ["Und", "nicht", "ver\u00b7tr\u00f6s\u00b7te", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kindisch eitel,", "tokens": ["Kin\u00b7disch", "ei\u00b7tel", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Auf das Verst\u00e4ndni\u00df der sp\u00e4ter Urtheilenden.", "tokens": ["Auf", "das", "Ver\u00b7st\u00e4nd\u00b7ni\u00df", "der", "sp\u00e4\u00b7ter", "Ur\u00b7thei\u00b7len\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--", "measure": "dactylic.tetra"}, "line.5": {"text": "Erkenne vielmehr", "tokens": ["Er\u00b7ken\u00b7ne", "viel\u00b7mehr"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Des Daseins tiefe Sinnlosigkeit,", "tokens": ["Des", "Da\u00b7seins", "tie\u00b7fe", "Sinn\u00b7lo\u00b7sig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Und erhabenen Gleichmuths schwebe,", "tokens": ["Und", "er\u00b7ha\u00b7be\u00b7nen", "Gleich\u00b7muths", "schwe\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "L\u00e4chelnd,", "tokens": ["L\u00e4\u00b7chelnd", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Ueber Vergangenheit, Mitwelt und Nachwelt.", "tokens": ["Ue\u00b7ber", "Ver\u00b7gan\u00b7gen\u00b7heit", ",", "Mit\u00b7welt", "und", "Nach\u00b7welt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}