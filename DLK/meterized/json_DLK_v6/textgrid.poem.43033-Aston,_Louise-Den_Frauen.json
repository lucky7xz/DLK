{"textgrid.poem.43033": {"metadata": {"author": {"name": "Aston, Louise", "birth": "N.A.", "death": "N.A."}, "title": "Den Frauen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr richtet streng, der Sitte heil'ge Vehm',", "tokens": ["Ihr", "rich\u00b7tet", "streng", ",", "der", "Sit\u00b7te", "heil'\u00b7ge", "Vehm'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und schleudert auf mein Haupt das Anathem!", "tokens": ["Und", "schleu\u00b7dert", "auf", "mein", "Haupt", "das", "A\u00b7nat\u00b7hem", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "M\u00f6gt ihr zu Boden st\u00fcrzen eure Kerzen", "tokens": ["M\u00f6gt", "ihr", "zu", "Bo\u00b7den", "st\u00fcr\u00b7zen", "eu\u00b7re", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und schlagen an die Brust, so tugendreich:", "tokens": ["Und", "schla\u00b7gen", "an", "die", "Brust", ",", "so", "tu\u00b7gen\u00b7dreich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich f\u00fchl' es m\u00e4chtig in dem tiefsten Herzen,", "tokens": ["Ich", "f\u00fchl'", "es", "m\u00e4ch\u00b7tig", "in", "dem", "tiefs\u00b7ten", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df meine S\u00fcnde eurer Tugend gleich.", "tokens": ["Da\u00df", "mei\u00b7ne", "S\u00fcn\u00b7de", "eu\u00b7rer", "Tu\u00b7gend", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Unschuld Lilien m\u00f6gen euch umbl\u00fchn,", "tokens": ["Der", "Un\u00b7schuld", "Li\u00b7li\u00b7en", "m\u00f6\u00b7gen", "euch", "um\u00b7bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Roth der Schaam auf euern Wangen gl\u00fchn;", "tokens": ["Das", "Roth", "der", "Schaam", "auf", "eu\u00b7ern", "Wan\u00b7gen", "gl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie Schw\u00e4ne sich auf stillen Fluthen schaukeln,", "tokens": ["Wie", "Schw\u00e4\u00b7ne", "sich", "auf", "stil\u00b7len", "Flut\u00b7hen", "schau\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gef\u00fchle still durch eure Seele ziehn;", "tokens": ["Ge\u00b7f\u00fch\u00b7le", "still", "durch", "eu\u00b7re", "See\u00b7le", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie Falter neckend durch die Blumen gaukeln,", "tokens": ["Wie", "Fal\u00b7ter", "ne\u00b7ckend", "durch", "die", "Blu\u00b7men", "gau\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Liebe W\u00fcnsche leis' vor\u00fcberfliehn!", "tokens": ["Der", "Lie\u00b7be", "W\u00fcn\u00b7sche", "leis'", "vor\u00b7\u00fc\u00b7berf\u00b7liehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Qu\u00e4lt euch ein flammend Sehnen fessellos,", "tokens": ["Qu\u00e4lt", "euch", "ein", "flam\u00b7mend", "Seh\u00b7nen", "fes\u00b7sel\u00b7los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "M\u00f6gt ihr entsagen stolz und seelengro\u00df;", "tokens": ["M\u00f6gt", "ihr", "ent\u00b7sa\u00b7gen", "stolz", "und", "see\u00b7len\u00b7gro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "M\u00f6gt still verzehren eure hei\u00dfe Jugend,", "tokens": ["M\u00f6gt", "still", "ver\u00b7zeh\u00b7ren", "eu\u00b7re", "hei\u00b7\u00dfe", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ausk\u00e4mpfen ritterlich den heil'gen Krieg,", "tokens": ["Aus\u00b7k\u00e4mp\u00b7fen", "rit\u00b7ter\u00b7lich", "den", "heil'\u00b7gen", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und mit dem Vollmachtsbriefe eurer Tugend", "tokens": ["Und", "mit", "dem", "Voll\u00b7machts\u00b7brie\u00b7fe", "eu\u00b7rer", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem Tod, der H\u00f6lle nehmen ihren Sieg!", "tokens": ["Dem", "Tod", ",", "der", "H\u00f6l\u00b7le", "neh\u00b7men", "ih\u00b7ren", "Sieg", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich achte dennoch eure Tugend nicht,", "tokens": ["Ich", "ach\u00b7te", "den\u00b7noch", "eu\u00b7re", "Tu\u00b7gend", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verwerfe k\u00fchn eu'r heiliges Gericht!", "tokens": ["Ver\u00b7wer\u00b7fe", "k\u00fchn", "eu'r", "hei\u00b7li\u00b7ges", "Ge\u00b7richt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Seid des Gesetzes Hort, der Sitte R\u00e4cher,", "tokens": ["Seid", "des", "Ge\u00b7set\u00b7zes", "Hort", ",", "der", "Sit\u00b7te", "R\u00e4\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ART", "NN", "NE", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des frommen Glaubens treuer Genius!", "tokens": ["Des", "from\u00b7men", "Glau\u00b7bens", "treu\u00b7er", "Ge\u00b7nius", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Es lebt ein heil'ger Geist auch im ", "tokens": ["Es", "lebt", "ein", "heil'\u00b7ger", "Geist", "auch", "im"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "APPRART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das Leben auch verlangt sein m\u00e4chtig Recht,", "tokens": ["Das", "Le\u00b7ben", "auch", "ver\u00b7langt", "sein", "m\u00e4ch\u00b7tig", "Recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "VAINF", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verla\u00dft des starren Wortes todten Knecht;", "tokens": ["Ver\u00b7la\u00dft", "des", "star\u00b7ren", "Wor\u00b7tes", "tod\u00b7ten", "Knecht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus edlem Feuer flossen meine S\u00fcnden,", "tokens": ["Aus", "ed\u00b7lem", "Feu\u00b7er", "flos\u00b7sen", "mei\u00b7ne", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus Drang des Herzens, gl\u00fch'nder Leidenschaft.", "tokens": ["Aus", "Drang", "des", "Her\u00b7zens", ",", "gl\u00fch'n\u00b7der", "Lei\u00b7den\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "F\u00fcr sie w\u00fcrd' ich schon hier Vergebung finden,", "tokens": ["F\u00fcr", "sie", "w\u00fcrd'", "ich", "schon", "hier", "Ver\u00b7ge\u00b7bung", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ADV", "ADV", "NN", "VVINF", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Die Zeugen meines Werthes, meiner Kraft.", "tokens": ["Die", "Zeu\u00b7gen", "mei\u00b7nes", "Wert\u00b7hes", ",", "mei\u00b7ner", "Kraft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wenn mich der Liebe Flammen hei\u00df umspr\u00fchn,", "tokens": ["Wenn", "mich", "der", "Lie\u00b7be", "Flam\u00b7men", "hei\u00df", "um\u00b7spr\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Will ich in sel'gem Feuertod vergl\u00fchn;", "tokens": ["Will", "ich", "in", "sel'\u00b7gem", "Feu\u00b7er\u00b7tod", "ver\u00b7gl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch aus den Gluthen steig' ich neugeboren,", "tokens": ["Doch", "aus", "den", "Glu\u00b7then", "steig'", "ich", "neu\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie sich der Ph\u00f6nix aus der Asche schwingt,", "tokens": ["Wie", "sich", "der", "Ph\u00f6\u00b7nix", "aus", "der", "A\u00b7sche", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gel\u00e4utert ward mein Wesen \u2013 nicht verloren,", "tokens": ["Ge\u00b7l\u00e4u\u00b7tert", "ward", "mein", "We\u00b7sen", "\u2013", "nicht", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$(", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu neuem, heil'gem Liebesgl\u00fcck verj\u00fcngt.", "tokens": ["Zu", "neu\u00b7em", ",", "heil'\u00b7gem", "Lie\u00b7bes\u00b7gl\u00fcck", "ver\u00b7j\u00fcngt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}