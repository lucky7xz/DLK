{"dta.poem.12283": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Von den klugen Jungfrauen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wachet auf, ruft uns die Stimme               ", "tokens": ["Wa\u00b7chet", "auf", ",", "ruft", "uns", "die", "Stim\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der W\u00e4chter sehr hoch auf der Zinne,", "tokens": ["Der", "W\u00e4ch\u00b7ter", "sehr", "hoch", "auf", "der", "Zin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wach auf du Stadt Jerusalem,", "tokens": ["Wach", "auf", "du", "Stadt", "Je\u00b7ru\u00b7sa\u00b7lem", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "NN", "NE", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Mitternacht hei\u00dft diese Stunde,", "tokens": ["Mit\u00b7ter\u00b7nacht", "hei\u00dft", "die\u00b7se", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie rufen uns mit hellem Munde:", "tokens": ["Sie", "ru\u00b7fen", "uns", "mit", "hel\u00b7lem", "Mun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "\u201ewohlan der Br\u00e4utigam k\u00f6mmt,", "tokens": ["\u201e", "wo\u00b7hlan", "der", "Br\u00e4u\u00b7ti\u00b7gam", "k\u00f6mmt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "NE", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "\u201esteht auf, die Lampen nehmt!", "tokens": ["\u201e", "steht", "auf", ",", "die", "Lam\u00b7pen", "nehmt", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "\u201ehalleluja!", "tokens": ["\u201e", "hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "ITJ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "\u201emacht euch bereit", "tokens": ["\u201e", "macht", "euch", "be\u00b7reit"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "ADJD"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "\u201ezu der Hochzeit,", "tokens": ["\u201e", "zu", "der", "Hoch\u00b7zeit", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "\u201eihr m\u00fcsset ihm entgegen gehn.\u201c", "tokens": ["\u201e", "ihr", "m\u00fcs\u00b7set", "ihm", "ent\u00b7ge\u00b7gen", "gehn", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "PTKVZ", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie h\u00f6rn die W\u00e4chter singen,", "tokens": ["Sie", "h\u00f6rn", "die", "W\u00e4ch\u00b7ter", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Herzen all vor Freuden springen,", "tokens": ["Die", "Her\u00b7zen", "all", "vor", "Freu\u00b7den", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wachen und stehn eilend auf;", "tokens": ["Sie", "wa\u00b7chen", "und", "stehn", "ei\u00b7lend", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Freund der kommt vom Himmel pr\u00e4chtig,", "tokens": ["Ihr", "Freund", "der", "kommt", "vom", "Him\u00b7mel", "pr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VVFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von Gnaden stark, von Wahrheit m\u00e4chtig,", "tokens": ["Von", "Gna\u00b7den", "stark", ",", "von", "Wahr\u00b7heit", "m\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Licht wird hell, ihr Stern geht auf.", "tokens": ["Ihr", "Licht", "wird", "hell", ",", "ihr", "Stern", "geht", "auf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u201enun komm du werthe Kron,", "tokens": ["\u201e", "nun", "komm", "du", "wert\u00b7he", "Kron", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "VVFIN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "\u201eherr Jesu, Gottes Sohn!", "tokens": ["\u201e", "herr", "Je\u00b7su", ",", "Got\u00b7tes", "Sohn", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NN", "NE", "$,", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201ehostana:", "tokens": ["\u201e", "hos\u00b7ta\u00b7na", ":"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "ITJ", "$."], "meter": "---", "measure": "unknown.measure.zero"}, "line.2": {"text": "\u201ewir folgen all,", "tokens": ["\u201e", "wir", "fol\u00b7gen", "all", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PIAT", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "\u201ezum Freuden-Saal,", "tokens": ["\u201e", "zum", "Freu\u00b7den\u00b7Saal", ","], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "\u201eund halten mit das Abendmahl.\u201c", "tokens": ["\u201e", "und", "hal\u00b7ten", "mit", "das", "A\u00b7bend\u00b7mahl", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}