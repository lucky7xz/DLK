{"dta.poem.21045": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "An Dieselben.   \n ( Zehn Jahre sp\u00e4ter gedichtet .)", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verkl&#228;re deinen Schleyer,               ", "tokens": ["Ver\u00b7kl", "&#228;", "re", "dei\u00b7nen", "Schle\u00b7yer", ","], "token_info": ["word", "XML_entity", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "O herbstliche Natur!", "tokens": ["O", "herbst\u00b7li\u00b7che", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erschein' in deiner Feyer,", "tokens": ["Er\u00b7schein'", "in", "dei\u00b7ner", "Fe\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O meine Lieblingsflur!", "tokens": ["O", "mei\u00b7ne", "Lieb\u00b7lings\u00b7flur", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Entw\u00f6lket euch, ihr Felder,", "tokens": ["Ent\u00b7w\u00f6l\u00b7ket", "euch", ",", "ihr", "Fel\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So freundlich, lieb und hold!", "tokens": ["So", "freund\u00b7lich", ",", "lieb", "und", "hold", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ergl\u00e4nzt, ergl\u00e4nzt, ihr W\u00e4lder,", "tokens": ["Er\u00b7gl\u00e4nzt", ",", "er\u00b7gl\u00e4nzt", ",", "ihr", "W\u00e4l\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Im Abendsonnengold.", "tokens": ["Im", "A\u00b7bend\u00b7son\u00b7nen\u00b7gold", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr ewiggr\u00fcnen Matten,", "tokens": ["Ihr", "e\u00b7wig\u00b7gr\u00fc\u00b7nen", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr sanfgew\u00f6lbten H\u00f6h'n,", "tokens": ["Ihr", "sanf\u00b7ge\u00b7w\u00f6lb\u00b7ten", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr d\u00fcstern Tannenschatten,", "tokens": ["Ihr", "d\u00fcs\u00b7tern", "Tan\u00b7nen\u00b7schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr spiegelklaren See'n,", "tokens": ["Ihr", "spie\u00b7gel\u00b7kla\u00b7ren", "See'n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr kalmusreichen Wiesen,", "tokens": ["Ihr", "kal\u00b7mus\u00b7rei\u00b7chen", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr Hayden, braun und w\u00fcst,", "tokens": ["Ihr", "Hay\u00b7den", ",", "braun", "und", "w\u00fcst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O seyd, seyd mir gepriesen,", "tokens": ["O", "seyd", ",", "seyd", "mir", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Seyd herzlich mir gegr\u00fcsst!", "tokens": ["Seyd", "herz\u00b7lich", "mir", "ge\u00b7gr\u00fcs\u00b7st", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich seh', ich seh' euch wieder;", "tokens": ["Ich", "seh'", ",", "ich", "seh'", "euch", "wie\u00b7der", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wie ich euch verliess,", "tokens": ["Und", "wie", "ich", "euch", "ver\u00b7liess", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So find' ich ganz euch wieder,", "tokens": ["So", "find'", "ich", "ganz", "euch", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So freundlich, lieb und s\u00fcss.", "tokens": ["So", "freund\u00b7lich", ",", "lieb", "und", "s\u00fcss", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr d\u00e4mmert noch so schaurig,", "tokens": ["Ihr", "d\u00e4m\u00b7mert", "noch", "so", "schau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr jubelt noch so laut,", "tokens": ["Ihr", "ju\u00b7belt", "noch", "so", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ihr lispelt noch so traurig,", "tokens": ["Ihr", "lis\u00b7pelt", "noch", "so", "trau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und schattet noch so traut!", "tokens": ["Und", "schat\u00b7tet", "noch", "so", "traut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr seht, ihr seht mich wieder;", "tokens": ["Ihr", "seht", ",", "ihr", "seht", "mich", "wie\u00b7der", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wie ihr sonst mich saht,", "tokens": ["Und", "wie", "ihr", "sonst", "mich", "saht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So seht ihr ganz mich wieder", "tokens": ["So", "seht", "ihr", "ganz", "mich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An Art und Kraft und That.", "tokens": ["An", "Art", "und", "Kraft", "und", "That", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Herz ist noch so offen,", "tokens": ["Mein", "Herz", "ist", "noch", "so", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So schw\u00e4rmend und so wild,", "tokens": ["So", "schw\u00e4r\u00b7mend", "und", "so", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein Sehnen und mein Hoffen", "tokens": ["Mein", "Seh\u00b7nen", "und", "mein", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Noch immer unerf\u00fcllt.", "tokens": ["Noch", "im\u00b7mer", "un\u00b7er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr friedenvollen Felder,", "tokens": ["Ihr", "frie\u00b7den\u00b7vol\u00b7len", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr thauberauschten Au'n,", "tokens": ["Ihr", "thau\u00b7be\u00b7rauschten", "Au'n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ihr feyerlichen W\u00e4lder,", "tokens": ["Ihr", "fey\u00b7er\u00b7li\u00b7chen", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durchweht von heilgem Graun,", "tokens": ["Durch\u00b7weht", "von", "heil\u00b7gem", "Graun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Umweht, umweht den M\u00fcden", "tokens": ["Um\u00b7weht", ",", "um\u00b7weht", "den", "M\u00fc\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit eurer tiefen Ruh,", "tokens": ["Mit", "eu\u00b7rer", "tie\u00b7fen", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und lispelt euren Frieden", "tokens": ["Und", "lis\u00b7pelt", "eu\u00b7ren", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dem heissen Schw\u00e4rmer zu.", "tokens": ["Dem", "heis\u00b7sen", "Schw\u00e4r\u00b7mer", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Auf euren braunen Hayden,", "tokens": ["Auf", "eu\u00b7ren", "brau\u00b7nen", "Hay\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Busch, im Bruch, am Bach", "tokens": ["Im", "Busch", ",", "im", "Bruch", ",", "am", "Bach"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verschw\u00e4rmen und vergeuden", "tokens": ["Ver\u00b7schw\u00e4r\u00b7men", "und", "ver\u00b7geu\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den langen Sommertag;", "tokens": ["Den", "lan\u00b7gen", "Som\u00b7mer\u00b7tag", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Beschirmt von euren B\u00e4umen,", "tokens": ["Be\u00b7schirmt", "von", "eu\u00b7ren", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gestreckt auf duftend Moos,", "tokens": ["Ge\u00b7streckt", "auf", "duf\u00b7tend", "Moos", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die laue Nacht vertr\u00e4umen \u2014", "tokens": ["Die", "lau\u00b7e", "Nacht", "ver\u00b7tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "O neideswerthes Loos!", "tokens": ["O", "nei\u00b7des\u00b7wert\u00b7hes", "Loos", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O, nehmt in eure Wonne", "tokens": ["O", ",", "nehmt", "in", "eu\u00b7re", "Won\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den m\u00fcden Waller auf!", "tokens": ["Den", "m\u00fc\u00b7den", "Wal\u00b7ler", "auf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es endet schon die Sonne", "tokens": ["Es", "en\u00b7det", "schon", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den hohen Heldenlauf.", "tokens": ["Den", "ho\u00b7hen", "Hel\u00b7den\u00b7lauf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der braune Abend schleyert", "tokens": ["Der", "brau\u00b7ne", "A\u00b7bend", "schley\u00b7ert"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Forst, die Flur, die Fluth.", "tokens": ["Den", "Forst", ",", "die", "Flur", ",", "die", "Fluth", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die matte Sch\u00f6pfung feyert,", "tokens": ["Die", "mat\u00b7te", "Sch\u00f6p\u00b7fung", "fe\u00b7yert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Und alles Leben ruht.", "tokens": ["Und", "al\u00b7les", "Le\u00b7ben", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}