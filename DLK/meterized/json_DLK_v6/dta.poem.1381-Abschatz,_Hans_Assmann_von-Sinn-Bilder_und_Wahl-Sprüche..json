{"dta.poem.1381": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sinn-Bilder und Wahl-Spr\u00fcche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Die auffgehende Sonne mit abfallendem Thau:", "tokens": ["Die", "auff\u00b7ge\u00b7hen\u00b7de", "Son\u00b7ne", "mit", "ab\u00b7fal\u00b7len\u00b7dem", "Thau", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--++--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Inter lacrumas oritur.", "tokens": ["In\u00b7ter", "la\u00b7cru\u00b7mas", "o\u00b7ri\u00b7tur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Thr\u00e4nen herbe Morgen-Frucht.", "tokens": ["Der", "Thr\u00e4\u00b7nen", "her\u00b7be", "Mor\u00b7gen\u00b7Frucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Pferd mit auffgelegtem Cap-Zaum und Sprung-\nRiemen:", "tokens": ["Ein", "Pferd", "mit", "auff\u00b7ge\u00b7leg\u00b7tem", "Cap\u00b7Zaum", "und", "Sprung", "Rie\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "KON", "TRUNC", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Lenta pati frena docetur.", "tokens": ["Len\u00b7ta", "pa\u00b7ti", "fre\u00b7na", "do\u00b7ce\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Das harte Band der strengen Zucht.", "tokens": ["Das", "har\u00b7te", "Band", "der", "stren\u00b7gen", "Zucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Habicht mit Fesseln und Schellen auff der Kr\u00fccke:", "tokens": ["Ein", "Ha\u00b7bicht", "mit", "Fes\u00b7seln", "und", "Schel\u00b7len", "auff", "der", "Kr\u00fc\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ardet &amp; haeret.", "tokens": ["Ar\u00b7det", "&amp;", "hae\u00b7ret", "."], "token_info": ["word", "XML_entity", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Der eitlen Liebe tolle Sucht.", "tokens": ["Der", "eit\u00b7len", "Lie\u00b7be", "tol\u00b7le", "Sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Eine Feuer-Lust-Kugel oder Raquete:", "tokens": ["Ei\u00b7ne", "Feu\u00b7er\u00b7Lust\u00b7Ku\u00b7gel", "o\u00b7der", "Ra\u00b7que\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "+-+-++-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Micando necatur.", "tokens": ["Mi\u00b7can\u00b7do", "ne\u00b7ca\u00b7tur", "."], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der schn\u00f6den Ehre schnelle Flucht.", "tokens": ["Der", "schn\u00f6\u00b7den", "Eh\u00b7re", "schnel\u00b7le", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein Schiff im Sturme:", "tokens": ["Ein", "Schiff", "im", "Stur\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Hostes venti, c&#339;lum, unda, procellae.", "tokens": ["Hos\u00b7tes", "ven\u00b7ti", ",", "c", "&#339;", "lum", ",", "un\u00b7da", ",", "pro\u00b7cel\u00b7lae", "."], "token_info": ["word", "word", "punct", "word", "XML_entity", "word", "punct", "word", "punct", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "FM", "FM", "FM", "$,", "FM.la", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Geh\u00e4ufften Ungl\u00fccks bittre Zucht.", "tokens": ["Ge\u00b7h\u00e4uff\u00b7ten", "Un\u00b7gl\u00fccks", "bitt\u00b7re", "Zucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Eine rothfalbe Erd-Mau\u00df/ oder Erd-Schlieffel:", "tokens": ["Ei\u00b7ne", "roth\u00b7fal\u00b7be", "Erd\u00b7Mau\u00df", "/", "o\u00b7der", "Erd\u00b7Sch\u00b7lief\u00b7fel", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Effosso concolor Auro.", "tokens": ["Ef\u00b7fos\u00b7so", "con\u00b7co\u00b7lor", "Au\u00b7ro", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die bla\u00df-besorgte Gelder-Sucht.", "tokens": ["Die", "bla\u00df\u00b7be\u00b7sorg\u00b7te", "Gel\u00b7der\u00b7Sucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ein verdorrender Pfersing-Baum/ der noch etliche Bl\u00e4tter\nund Fr\u00fcchte hat:", "tokens": ["Ein", "ver\u00b7dor\u00b7ren\u00b7der", "Pfer\u00b7sing\u00b7Baum", "/", "der", "noch", "et\u00b7li\u00b7che", "Bl\u00e4t\u00b7ter", "und", "Fr\u00fcch\u00b7te", "hat", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADV", "PIAT", "NN", "KON", "NN", "VAFIN", "$."], "meter": "--+--+-+--+--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Obrepit non intellecta senectus.", "tokens": ["O\u00b7bre\u00b7pit", "non", "in\u00b7tel\u00b7lec\u00b7ta", "sen\u00b7ec\u00b7tus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der matten Kr\u00e4ffte fr\u00fche Flucht.", "tokens": ["Der", "mat\u00b7ten", "Kr\u00e4ff\u00b7te", "fr\u00fc\u00b7he", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "J\u00fcden-Kirschen:V", "tokens": ["J\u00fc\u00b7den\u00b7Kir\u00b7schen", ":", "V"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$.", "CARD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Fallaces hominum spes.", "tokens": ["Fal\u00b7la\u00b7ces", "ho\u00b7mi\u00b7num", "spes", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Ist unsers Lebens beste Frucht.", "tokens": ["Ist", "un\u00b7sers", "Le\u00b7bens", "bes\u00b7te", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Eine Gra\u00df-meyhende Sense:", "tokens": ["Ei\u00b7ne", "Gra\u00df\u00b7mey\u00b7hen\u00b7de", "Sen\u00b7se", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rapit inclementia mortis.", "tokens": ["Ra\u00b7pit", "in\u00b7cle\u00b7men\u00b7tia", "mor\u00b7tis", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bi\u00df uns der Tod zum Grabe sucht.", "tokens": ["Bi\u00df", "uns", "der", "Tod", "zum", "Gra\u00b7be", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}