{"dta.poem.7291": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Das alte Schlo\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Auf der Burg haus' ich am Berge,", "tokens": ["Auf", "der", "Burg", "haus'", "ich", "am", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter mir der blaue See,", "tokens": ["Un\u00b7ter", "mir", "der", "blau\u00b7e", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6re n\u00e4chtlich Koboldzwerge,", "tokens": ["H\u00f6\u00b7re", "n\u00e4cht\u00b7lich", "Ko\u00b7bold\u00b7zwer\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00e4glich Adler aus der H\u00f6h',", "tokens": ["T\u00e4g\u00b7lich", "Ad\u00b7ler", "aus", "der", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die grauen Ahnenbilder", "tokens": ["Und", "die", "grau\u00b7en", "Ah\u00b7nen\u00b7bil\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind mir Stubenkameraden,", "tokens": ["Sind", "mir", "Stu\u00b7ben\u00b7ka\u00b7me\u00b7ra\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wappentruh' und Eisenschilder", "tokens": ["Wap\u00b7pen\u00b7truh'", "und", "Ei\u00b7sen\u00b7schil\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sopha mir und Kleiderladen.", "tokens": ["So\u00b7pha", "mir", "und", "Klei\u00b7der\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Schreit' ich \u00fcber die Terrasse", "tokens": ["Schreit'", "ich", "\u00fc\u00b7ber", "die", "Ter\u00b7ras\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Wie ein Geist am Runenstein,", "tokens": ["Wie", "ein", "Geist", "am", "Ru\u00b7nen\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sehe unter mir die blasse", "tokens": ["Se\u00b7he", "un\u00b7ter", "mir", "die", "blas\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alte Stadt im Mondenschein,", "tokens": ["Al\u00b7te", "Stadt", "im", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und am Walle pfeift es weidlich,", "tokens": ["Und", "am", "Wal\u00b7le", "pfeift", "es", "weid\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "\u2014 Sind es K\u00e4uze oder Knaben? \u2014", "tokens": ["Sind", "es", "K\u00e4u\u00b7ze", "o\u00b7der", "Kna\u00b7ben", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "PPER", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ist mir selber oft nicht deutlich,", "tokens": ["Ist", "mir", "sel\u00b7ber", "oft", "nicht", "deut\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ob ich lebend, ob begraben!", "tokens": ["Ob", "ich", "le\u00b7bend", ",", "ob", "be\u00b7gra\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "$,", "KOUS", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mir gen\u00fcber g\u00e4hnt die Halle,", "tokens": ["Mir", "ge\u00b7n\u00fc\u00b7ber", "g\u00e4hnt", "die", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Grauen Thores, hohl und lang,", "tokens": ["Grau\u00b7en", "Tho\u00b7res", ",", "hohl", "und", "lang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drin mit wunderlichem Schalle", "tokens": ["Drin", "mit", "wun\u00b7der\u00b7li\u00b7chem", "Schal\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Langsam dr\u00f6hnt ein schwerer Gang;", "tokens": ["Lang\u00b7sam", "dr\u00f6hnt", "ein", "schwe\u00b7rer", "Gang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mir zur Seite Riegelz\u00fcge,", "tokens": ["Mir", "zur", "Sei\u00b7te", "Rie\u00b7gel\u00b7z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ha, ich \u00f6ffne, la\u00df die Lampe", "tokens": ["Ha", ",", "ich", "\u00f6ff\u00b7ne", ",", "la\u00df", "die", "Lam\u00b7pe"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "$,", "VVIMP", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Scheinen auf der Wendelstiege", "tokens": ["Schei\u00b7nen", "auf", "der", "Wen\u00b7del\u00b7stie\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lose modergr\u00fcne Rampe,", "tokens": ["Lo\u00b7se", "mo\u00b7der\u00b7gr\u00fc\u00b7ne", "Ram\u00b7pe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Die mich lockt wie ein Verh\u00e4ngni\u00df,", "tokens": ["Die", "mich", "lockt", "wie", "ein", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu dem unbekannten Grund;", "tokens": ["Zu", "dem", "un\u00b7be\u00b7kann\u00b7ten", "Grund", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob ein Brunnen? ob Gef\u00e4ngni\u00df?", "tokens": ["Ob", "ein", "Brun\u00b7nen", "?", "ob", "Ge\u00b7f\u00e4ng\u00b7ni\u00df", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$.", "KOUS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keinem Lebenden ist's kund;", "tokens": ["Kei\u00b7nem", "Le\u00b7ben\u00b7den", "ist's", "kund", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn zerfallen sind die Stufen,", "tokens": ["Denn", "zer\u00b7fal\u00b7len", "sind", "die", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Steinwurf hat nicht Bahn,", "tokens": ["Und", "der", "Stein\u00b7wurf", "hat", "nicht", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PTKNEG", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Doch als ich hinab gerufen,", "tokens": ["Doch", "als", "ich", "hin\u00b7ab", "ge\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Donnert's fort wie ein Orkan.", "tokens": ["Don\u00b7nert's", "fort", "wie", "ein", "Or\u00b7kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KOKOM", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "Ja, wird mir nicht baldigst fade", "tokens": ["Ja", ",", "wird", "mir", "nicht", "bal\u00b7digst", "fa\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VAFIN", "PPER", "PTKNEG", "ADV", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dieses Schlosses Romantik,", "tokens": ["Die\u00b7ses", "Schlos\u00b7ses", "Ro\u00b7man\u00b7tik", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den Tr\u00fcmmern, ohne Gnade,", "tokens": ["In", "den", "Tr\u00fcm\u00b7mern", ",", "oh\u00b7ne", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Brech' ich Glieder und Genick;", "tokens": ["Brech'", "ich", "Glie\u00b7der", "und", "Ge\u00b7nick", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn, wie trotzig sich die D\u00fcne", "tokens": ["Denn", ",", "wie", "trot\u00b7zig", "sich", "die", "D\u00fc\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "ADJD", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mag am flachen Strande heben,", "tokens": ["Mag", "am", "fla\u00b7chen", "Stran\u00b7de", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "F\u00fchl' ich stark mich wie ein H\u00fcne,", "tokens": ["F\u00fchl'", "ich", "stark", "mich", "wie", "ein", "H\u00fc\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Von Zerfallendem umgeben.", "tokens": ["Von", "Zer\u00b7fal\u00b7len\u00b7dem", "um\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}