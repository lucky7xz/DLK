{"dta.poem.14149": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Begegnung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Es war mein Lieb! 's ist lange her,", "tokens": ["Es", "war", "mein", "Lieb", "!", "'s", "ist", "lan\u00b7ge", "her", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie blickt' so ohne Scheue,", "tokens": ["Sie", "blickt'", "so", "oh\u00b7ne", "Scheu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verloren ist die Treue,", "tokens": ["Ver\u00b7lo\u00b7ren", "ist", "die", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie kannte mich nicht mehr \u2014", "tokens": ["Sie", "kann\u00b7te", "mich", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da jauchzt' und geigt's auf's neue,", "tokens": ["Da", "jauchzt'", "und", "geigt's", "auf's", "neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "NE", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich aber wandt' mich fort in's Feld,", "tokens": ["Ich", "a\u00b7ber", "wandt'", "mich", "fort", "in's", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nun wandr' ich bis an's End' der Welt!", "tokens": ["Nun", "wandr'", "ich", "bis", "an's", "End'", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}