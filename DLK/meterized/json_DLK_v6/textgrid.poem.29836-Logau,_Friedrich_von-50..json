{"textgrid.poem.29836": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Iedermann hat zu Hau\u00dfinnen zwey gar ungegleichte G\u00e4ste,", "tokens": ["Ie\u00b7der\u00b7mann", "hat", "zu", "Hau\u00b7\u00dfin\u00b7nen", "zwey", "gar", "un\u00b7ge\u00b7gleich\u00b7te", "G\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "NN", "CARD", "ADV", "ADJA", "NN", "$,"], "meter": "---+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Einen Doctor, einen Narren, die mit seinem Brot er m\u00e4ste;", "tokens": ["Ei\u00b7nen", "Doc\u00b7tor", ",", "ei\u00b7nen", "Nar\u00b7ren", ",", "die", "mit", "sei\u00b7nem", "Brot", "er", "m\u00e4s\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wil er nun nicht vor sich sehn und den Narren halten ein,", "tokens": ["Wil", "er", "nun", "nicht", "vor", "sich", "sehn", "und", "den", "Nar\u00b7ren", "hal\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "APPR", "PRF", "VVINF", "KON", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Wird er, als der Doctor, mehr an der Th\u00fcr und Fenster seyn.", "tokens": ["Wird", "er", ",", "als", "der", "Doc\u00b7tor", ",", "mehr", "an", "der", "Th\u00fcr", "und", "Fens\u00b7ter", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "KON", "NN", "VAINF", "$."], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}}}}}