{"textgrid.poem.56905": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sieh mit wei\u00dfen Armen, schwellenden Br\u00fcsten,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh mit wei\u00dfen Armen, schwellenden Br\u00fcsten,", "tokens": ["Sieh", "mit", "wei\u00b7\u00dfen", "Ar\u00b7men", ",", "schwel\u00b7len\u00b7den", "Br\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "purpurnen Lippen, blitzenden Augen dort", "tokens": ["pur\u00b7pur\u00b7nen", "Lip\u00b7pen", ",", "blit\u00b7zen\u00b7den", "Au\u00b7gen", "dort"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "der jungen Weiber hold erregte Reigen", "tokens": ["der", "jun\u00b7gen", "Wei\u00b7ber", "hold", "er\u00b7reg\u00b7te", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "aus den immergr\u00fcnen Toren der Jugend,", "tokens": ["aus", "den", "im\u00b7mer\u00b7gr\u00fc\u00b7nen", "To\u00b7ren", "der", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "gleich aus brechenden K\u00f6rben rollenden Fr\u00fcchten,", "tokens": ["gleich", "aus", "bre\u00b7chen\u00b7den", "K\u00f6r\u00b7ben", "rol\u00b7len\u00b7den", "Fr\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "quellen \u2013 str\u00f6men \u2013 \u2013 sich ergie\u00dfen \u2013 \u2013 \u2013", "tokens": ["quel\u00b7len", "\u2013", "str\u00f6\u00b7men", "\u2013", "\u2013", "sich", "er\u00b7gie\u00b7\u00dfen", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "$(", "VVFIN", "$(", "$(", "PRF", "VVINF", "$(", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "des Lebens unversiegliche B\u00fcrgschaft selber.", "tokens": ["des", "Le\u00b7bens", "un\u00b7ver\u00b7sieg\u00b7li\u00b7che", "B\u00fcrg\u00b7schaft", "sel\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Und du st\u00fcrzest nieder in deiner Kraft,", "tokens": ["Und", "du", "st\u00fcr\u00b7zest", "nie\u00b7der", "in", "dei\u00b7ner", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und, besiegt vom Zauber unendlicher Anmut,", "tokens": ["und", ",", "be\u00b7siegt", "vom", "Zau\u00b7ber", "un\u00b7end\u00b7li\u00b7cher", "An\u00b7mut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "l\u00e4ssest du willenlos dich mit Rosenbanden", "tokens": ["l\u00e4s\u00b7sest", "du", "wil\u00b7len\u00b7los", "dich", "mit", "Ro\u00b7sen\u00b7ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "PRF", "APPR", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "fesseln, und durch den zierlichen Fu\u00df der Erw\u00e4hlten", "tokens": ["fes\u00b7seln", ",", "und", "durch", "den", "zier\u00b7li\u00b7chen", "Fu\u00df", "der", "Er\u00b7w\u00e4hl\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVINF", "$,", "KON", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "k\u00fcssest und wirkst du mit neuen Gel\u00f6bnissen dich", "tokens": ["k\u00fcs\u00b7sest", "und", "wirkst", "du", "mit", "neu\u00b7en", "Ge\u00b7l\u00f6b\u00b7nis\u00b7sen", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PPER"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.6": {"text": "an den g\u00fctigen Scho\u00df deiner ewigen Mutter.", "tokens": ["an", "den", "g\u00fc\u00b7ti\u00b7gen", "Scho\u00df", "dei\u00b7ner", "e\u00b7wi\u00b7gen", "Mut\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}}, "stanza.3": {"line.1": {"text": "Aus den immergr\u00fcnen Toren der Jugend", "tokens": ["Aus", "den", "im\u00b7mer\u00b7gr\u00fc\u00b7nen", "To\u00b7ren", "der", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "wiegen jungfr\u00e4uliche Reigen sich", "tokens": ["wie\u00b7gen", "jung\u00b7fr\u00e4u\u00b7li\u00b7che", "Rei\u00b7gen", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "PRF"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "in die grauen Gefilde der Welt.", "tokens": ["in", "die", "grau\u00b7en", "Ge\u00b7fil\u00b7de", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Und es zittert die keusche Myrte,", "tokens": ["Und", "es", "zit\u00b7tert", "die", "keu\u00b7sche", "Myr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "und unruhig atmet die Rose,", "tokens": ["und", "un\u00b7ru\u00b7hig", "at\u00b7met", "die", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "wenn im hohen \u00c4thergew\u00f6lbe", "tokens": ["wenn", "im", "ho\u00b7hen", "\u00c4t\u00b7her\u00b7ge\u00b7w\u00f6l\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "die Kerzen der Nacht aufflammen.", "tokens": ["die", "Ker\u00b7zen", "der", "Nacht", "auf\u00b7flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}