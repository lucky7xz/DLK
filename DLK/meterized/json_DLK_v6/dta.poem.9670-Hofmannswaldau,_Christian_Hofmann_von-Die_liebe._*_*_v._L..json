{"dta.poem.9670": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die liebe.  \n * * v. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wundre dich nicht/ da\u00df die liebe meistens unbest\u00e4ndig ist;", "tokens": ["Wund\u00b7re", "dich", "nicht", "/", "da\u00df", "die", "lie\u00b7be", "meis\u00b7tens", "un\u00b7be\u00b7st\u00e4n\u00b7dig", "ist", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$(", "KOUS", "ART", "VVFIN", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Venus die hat eine stelle beyn planeten ihr erkiest.", "tokens": ["Ve\u00b7nus", "die", "hat", "ei\u00b7ne", "stel\u00b7le", "beyn", "pla\u00b7ne\u00b7ten", "ihr", "er\u00b7kiest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "VAFIN", "ART", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}