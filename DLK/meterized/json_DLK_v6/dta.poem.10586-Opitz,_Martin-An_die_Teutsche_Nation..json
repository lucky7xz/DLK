{"dta.poem.10586": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "An die Teutsche Nation.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Der blinden Venus werck/ die s\u00fcsse gifft zu lieben", "tokens": ["Der", "blin\u00b7den", "Ve\u00b7nus", "werck", "/", "die", "s\u00fcs\u00b7se", "gifft", "zu", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd sch\u00f6ne Zauberey/ in diesem Buch beschrieben/", "tokens": ["Vnd", "sch\u00f6\u00b7ne", "Zau\u00b7be\u00b7rey", "/", "in", "die\u00b7sem", "Buch", "be\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$(", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nim erstlich an von mir/ du werthes Vatterlandt/", "tokens": ["Nim", "erst\u00b7lich", "an", "von", "mir", "/", "du", "wert\u00b7hes", "Vat\u00b7ter\u00b7landt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "APPR", "PPER", "$(", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nim an der Liebe sach/ als meiner Liebe pfandt.", "tokens": ["Nim", "an", "der", "Lie\u00b7be", "sach", "/", "als", "mei\u00b7ner", "Lie\u00b7be", "pfandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$(", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Sinn flog vber hoch: Ich wolte dir vermelden", "tokens": ["Mein", "Sinn", "flog", "vber", "hoch", ":", "Ich", "wol\u00b7te", "dir", "ver\u00b7mel\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJD", "$.", "PPER", "VMFIN", "PPER", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Durch der ", "tokens": ["Durch", "der"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Die sich vor dieser Zeit den R\u00f6mern widersetzt/", "tokens": ["Die", "sich", "vor", "die\u00b7ser", "Zeit", "den", "R\u00f6\u00b7mern", "wi\u00b7der\u00b7setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PDAT", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vnd jhrer Schwerter sch\u00e4rff in jhrem Blut genetzt.", "tokens": ["Vnd", "jhrer", "Schwer\u00b7ter", "sch\u00e4rff", "in", "jhrem", "Blut", "ge\u00b7netzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Apollo nam mich an in seine Gunst vnd holde/", "tokens": ["A\u00b7pol\u00b7lo", "nam", "mich", "an", "in", "sei\u00b7ne", "Gunst", "vnd", "hol\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vulcanus hatte schon gemacht von gutem Golde/", "tokens": ["Vul\u00b7ca\u00b7nus", "hat\u00b7te", "schon", "ge\u00b7macht", "von", "gu\u00b7tem", "Gol\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Feder meiner Faust: Ich war nun gantz bereit", "tokens": ["Die", "Fe\u00b7der", "mei\u00b7ner", "Faust", ":", "Ich", "war", "nun", "gantz", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mit meines Geistes frucht zu brechen durch die Zeit.", "tokens": ["Mit", "mei\u00b7nes", "Geis\u00b7tes", "frucht", "zu", "bre\u00b7chen", "durch", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da kam der Venus Kindt/ bracht eine Kron von Myrten", "tokens": ["Da", "kam", "der", "Ve\u00b7nus", "Kindt", "/", "bracht", "ei\u00b7ne", "Kron", "von", "Myr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$(", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vor meinen Lorbeerkrantz/ verstie\u00df mich zu den Hirten", "tokens": ["Vor", "mei\u00b7nen", "Lor\u00b7beer\u00b7krantz", "/", "ver\u00b7stie\u00df", "mich", "zu", "den", "Hir\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$(", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "In einen gr\u00fcnen Wald/ wie\u00df mir ein sch\u00f6nes Bild/", "tokens": ["In", "ei\u00b7nen", "gr\u00fc\u00b7nen", "Wald", "/", "wie\u00df", "mir", "ein", "sch\u00f6\u00b7nes", "Bild", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die edle Nymf hat mir Gem\u00fcth vnd Sinn erf\u00fcllt.", "tokens": ["Die", "ed\u00b7le", "Nymf", "hat", "mir", "Ge\u00b7m\u00fcth", "vnd", "Sinn", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "In ihren \u00e4ugelein hab ich das allesfunden", "tokens": ["In", "ih\u00b7ren", "\u00e4u\u00b7ge\u00b7lein", "hab", "ich", "das", "al\u00b7les\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Was ich mich in di\u00df Buch zuschreiben vnderwunden/", "tokens": ["Was", "ich", "mich", "in", "di\u00df", "Buch", "zu\u00b7schrei\u00b7ben", "vn\u00b7der\u00b7wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PRF", "APPR", "PDS", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Das jrrdische Gestirn hat meinen hohen Geist", "tokens": ["Das", "jrr\u00b7di\u00b7sche", "Ge\u00b7stirn", "hat", "mei\u00b7nen", "ho\u00b7hen", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "In dieses enge Meer der Eitelkeit geweist.", "tokens": ["In", "die\u00b7ses", "en\u00b7ge", "Meer", "der", "Ei\u00b7tel\u00b7keit", "ge\u00b7weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "In dieses enge Meer auff welchem meine Sinnen", "tokens": ["In", "die\u00b7ses", "en\u00b7ge", "Meer", "auff", "wel\u00b7chem", "mei\u00b7ne", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "APPR", "PWAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Nichts als von Freundligkeit vnd Liebe dencken k\u00fcnnen/", "tokens": ["Nichts", "als", "von", "Freund\u00b7lig\u00b7keit", "vnd", "Lie\u00b7be", "den\u00b7cken", "k\u00fcn\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "APPR", "NN", "KON", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Von Lieb vnd freundligkeit: Die bitter s\u00fcsse Pein", "tokens": ["Von", "Lieb", "vnd", "freund\u00b7lig\u00b7keit", ":", "Die", "bit\u00b7ter", "s\u00fcs\u00b7se", "Pein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$.", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die muste mir an statt der Heldenthaten sein.", "tokens": ["Die", "mus\u00b7te", "mir", "an", "statt", "der", "Hel\u00b7den\u00b7tha\u00b7ten", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "APPR", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich thue/ Asterie/ nach deinem wolbehagen", "tokens": ["Ich", "thue", "/", "As\u00b7te\u00b7rie", "/", "nach", "dei\u00b7nem", "wol\u00b7be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "NN", "$(", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Vnd will dein hohes Lob bi\u00df an die Sternen tragen:", "tokens": ["Vnd", "will", "dein", "ho\u00b7hes", "Lob", "bi\u00df", "an", "die", "Ster\u00b7nen", "tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So weit der Teutschen Red vnd Tugendt ist bekandt/", "tokens": ["So", "weit", "der", "Teut\u00b7schen", "Red", "vnd", "Tu\u00b7gendt", "ist", "be\u00b7kandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "KON", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Soll auch dein Ehr vnd Prei\u00df durchtringen alles Landt.", "tokens": ["Soll", "auch", "dein", "Ehr", "vnd", "Prei\u00df", "durch\u00b7trin\u00b7gen", "al\u00b7les", "Landt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "KON", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "O hohe werthe Seel in Wei\u00dfheit au\u00dferkoren", "tokens": ["O", "ho\u00b7he", "wert\u00b7he", "Seel", "in", "Wei\u00df\u00b7heit", "au\u00b7\u00dfer\u00b7ko\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "VVFIN", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Zum Spiegel weiblicher vollkommenheit geboren/", "tokens": ["Zum", "Spie\u00b7gel", "weib\u00b7li\u00b7cher", "voll\u00b7kom\u00b7men\u00b7heit", "ge\u00b7bo\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Sey mir mit deiner Gunst/ mit deiner Huld bereit/", "tokens": ["Sey", "mir", "mit", "dei\u00b7ner", "Gunst", "/", "mit", "dei\u00b7ner", "Huld", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$(", "APPR", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Komm/ komm/ vnd la\u00df vns gehn den Weg der Ewigkeit.", "tokens": ["Komm", "/", "komm", "/", "vnd", "la\u00df", "vns", "gehn", "den", "Weg", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "$(", "KON", "VVIMP", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Du Teutsche Nation voll Freyheit Ehr vnd Tugendt/", "tokens": ["Du", "Teut\u00b7sche", "Na\u00b7tion", "voll", "Frey\u00b7heit", "Ehr", "vnd", "Tu\u00b7gendt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADJD", "NN", "NN", "KON", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Nimb an di\u00df kleine Buch/ die fr\u00fcchte meiner Jugendt/", "tokens": ["Nimb", "an", "di\u00df", "klei\u00b7ne", "Buch", "/", "die", "fr\u00fcch\u00b7te", "mei\u00b7ner", "Ju\u00b7gendt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDS", "ADJA", "NN", "$(", "ART", "ADJA", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.35": {"text": "Bi\u00df da\u00df ich h\u00f6her steig vnd deiner Thaten zahl", "tokens": ["Bi\u00df", "da\u00df", "ich", "h\u00f6\u00b7her", "steig", "vnd", "dei\u00b7ner", "Tha\u00b7ten", "zahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "VVFIN", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Werd vnabl\u00e4ssiglich verk\u00fcnden vberal.", "tokens": ["Werd", "vnab\u00b7l\u00e4s\u00b7sig\u00b7lich", "ver\u00b7k\u00fcn\u00b7den", "vbe\u00b7ral", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.37": {"text": "Di\u00df Buch ist mein beginn in Lieb vnd auch das ende:", "tokens": ["Di\u00df", "Buch", "ist", "mein", "be\u00b7ginn", "in", "Lieb", "vnd", "auch", "das", "en\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ein ander besser Werck zu dem ich jetzt mich wende/", "tokens": ["Ein", "an\u00b7der", "bes\u00b7ser", "Werck", "zu", "dem", "ich", "jetzt", "mich", "wen\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "APPR", "PRELS", "PPER", "ADV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Das soll vor diesem Buch so vielmahl besser sein/", "tokens": ["Das", "soll", "vor", "die\u00b7sem", "Buch", "so", "viel\u00b7mahl", "bes\u00b7ser", "sein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "PDAT", "NN", "ADV", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Je besser Wei\u00dfheit ist als Venus s\u00fcsse Pein.", "tokens": ["Je", "bes\u00b7ser", "Wei\u00df\u00b7heit", "ist", "als", "Ve\u00b7nus", "s\u00fcs\u00b7se", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "VAFIN", "KOKOM", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}