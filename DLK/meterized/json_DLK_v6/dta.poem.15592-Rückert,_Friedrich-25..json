{"dta.poem.15592": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O V\u00e4ter, M\u00fctter, o Erzieher, habet Acht", "tokens": ["O", "V\u00e4\u00b7ter", ",", "M\u00fct\u00b7ter", ",", "o", "Er\u00b7zie\u00b7her", ",", "ha\u00b7bet", "Acht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "NN", "$,", "FM", "NN", "$,", "VAFIN", "CARD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des wichtigen Berufs, wie gro\u00df ist eure Macht.", "tokens": ["Des", "wich\u00b7ti\u00b7gen", "Be\u00b7rufs", ",", "wie", "gro\u00df", "ist", "eu\u00b7re", "Macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Menschheit Aufgab' ist die Menschheit zu erziehn;", "tokens": ["Der", "Menschheit", "Auf\u00b7gab'", "ist", "die", "Menschheit", "zu", "er\u00b7ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bedenkt, da\u00df euch daran ein Antheil ist verliehn.", "tokens": ["Be\u00b7denkt", ",", "da\u00df", "euch", "da\u00b7ran", "ein", "An\u00b7theil", "ist", "ver\u00b7liehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PAV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O wirkt gewissenhaft dazu an euerm Theil,", "tokens": ["O", "wirkt", "ge\u00b7wis\u00b7sen\u00b7haft", "da\u00b7zu", "an", "eu\u00b7erm", "Theil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "PAV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit der Menschheit komm' ihr Heiland oder Heil.", "tokens": ["Da\u00b7mit", "der", "Menschheit", "komm'", "ihr", "Hei\u00b7land", "o\u00b7der", "Heil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Betrachtet jedes Kind mit Ehrfurcht, denn geheim", "tokens": ["Be\u00b7trach\u00b7tet", "je\u00b7des", "Kind", "mit", "Ehr\u00b7furcht", ",", "denn", "ge\u00b7heim"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kann seyn in jedem ja des neuen Heiles Keim.", "tokens": ["Kann", "seyn", "in", "je\u00b7dem", "ja", "des", "neu\u00b7en", "Hei\u00b7les", "Keim", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VAINF", "APPR", "PIS", "ADV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das Heil, ob es Gestalt des Einzlen angenommen,", "tokens": ["Das", "Heil", ",", "ob", "es", "Ge\u00b7stalt", "des", "Einz\u00b7len", "an\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob es als Ganzes komm', es wird das Heil uns kommen.", "tokens": ["Ob", "es", "als", "Gan\u00b7zes", "komm'", ",", "es", "wird", "das", "Heil", "uns", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}