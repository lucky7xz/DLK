-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Ihrer stillen Hallen Nacht", "tokens": ["In", "Ih\u00b7rer", "stil\u00b7len", "Hal\u00b7len", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herab, und spricht mit Ihr.", "tokens": ["Her\u00b7ab", ",", "und", "spricht", "mit", "Ihr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.27": {"line.1": {"text": "Er spricht von ew'gem Wiederseh'n,", "tokens": ["Er", "spricht", "von", "ew'\u00b7gem", "Wie\u00b7der\u00b7seh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von himmlischen Umarmungen,", "tokens": ["Von", "himm\u00b7li\u00b7schen", "Um\u00b7ar\u00b7mun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vom Lohne jeder guten That,", "tokens": ["Vom", "Loh\u00b7ne", "je\u00b7der", "gu\u00b7ten", "That", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von frommer Herrscher Lust.", "tokens": ["Von", "from\u00b7mer", "Herr\u00b7scher", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.28": {"line.1": {"text": "Bald spricht er auch von Deutschlands Wohl,", "tokens": ["Bald", "spricht", "er", "auch", "von", "Deutschlands", "Wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "wo