{"textgrid.poem.41188": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie kehren, Freund! doch Einer bleibt;", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie kehren, Freund! doch Einer bleibt;", "tokens": ["Sie", "keh\u00b7ren", ",", "Freund", "!", "doch", "Ei\u00b7ner", "bleibt", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$.", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sieht in Huldrichs Herz, und sieht", "tokens": ["Er", "sieht", "in", "Huld\u00b7richs", "Herz", ",", "und", "sieht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein ihm gef\u00e4llig werdend Lied.", "tokens": ["Ein", "ihm", "ge\u00b7f\u00e4l\u00b7lig", "wer\u00b7dend", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Brich aus, o Lied! Er bleibt!", "tokens": ["Brich", "aus", ",", "o", "Lied", "!", "Er", "bleibt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "FM", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6n gl\u00e4nzt er her aus blauer Luft.", "tokens": ["Sch\u00f6n", "gl\u00e4nzt", "er", "her", "aus", "blau\u00b7er", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Weste str\u00f6mt sein Friedenskleid", "tokens": ["Im", "Wes\u00b7te", "str\u00f6mt", "sein", "Frie\u00b7dens\u00b7kleid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch wei\u00dfer, als der j\u00fcngste Schnee.", "tokens": ["Noch", "wei\u00b7\u00dfer", ",", "als", "der", "j\u00fcngs\u00b7te", "Schnee", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Brich aus, o Lied! Brich aus!", "tokens": ["Brich", "aus", ",", "o", "Lied", "!", "Brich", "aus", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "FM", "NN", "$.", "NE", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du t\u00f6nest seiner Gattinn Lob.", "tokens": ["Du", "t\u00f6\u00b7nest", "sei\u00b7ner", "Gat\u00b7tinn", "Lob", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer liebte seine Gattinn so?", "tokens": ["Wer", "lieb\u00b7te", "sei\u00b7ne", "Gat\u00b7tinn", "so", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wen liebte seine Gattinn so?", "tokens": ["Wen", "lieb\u00b7te", "sei\u00b7ne", "Gat\u00b7tinn", "so", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Brich aus, o Lied! Er horcht!", "tokens": ["Brich", "aus", ",", "o", "Lied", "!", "Er", "horcht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "FM", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Im Erbe Teut's, im hohen Wien,", "tokens": ["Im", "Er\u00b7be", "Teut's", ",", "im", "ho\u00b7hen", "Wi\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da wuchs des Herrschers Tochter auf;", "tokens": ["Da", "wuchs", "des", "Herr\u00b7schers", "Toch\u00b7ter", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wie des Herrschers Tochter war,", "tokens": ["Doch", "wie", "des", "Herr\u00b7schers", "Toch\u00b7ter", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das singt gewi\u00df kein Lied.", "tokens": ["Das", "singt", "ge\u00b7wi\u00df", "kein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und n\u00e4hm' ich Barde, was der Lenz", "tokens": ["Und", "n\u00e4hm'", "ich", "Bar\u00b7de", ",", "was", "der", "Lenz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Farben auf die Fluren haucht,", "tokens": ["Von", "Far\u00b7ben", "auf", "die", "Flu\u00b7ren", "haucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Morgenroth und Abendroth,", "tokens": ["Das", "Mor\u00b7gen\u00b7roth", "und", "A\u00b7ben\u00b7droth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Regenbogens Glanz!", "tokens": ["Des", "Re\u00b7gen\u00b7bo\u00b7gens", "Glanz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und jeden lauten Reiz des Tag's,", "tokens": ["Und", "je\u00b7den", "lau\u00b7ten", "Reiz", "des", "Tag's", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und jeden stillen Reiz der Nacht,", "tokens": ["Und", "je\u00b7den", "stil\u00b7len", "Reiz", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sterne Blick, der Blumen Duft,", "tokens": ["Der", "Ster\u00b7ne", "Blick", ",", "der", "Blu\u00b7men", "Duft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der schlanksten Tanne Wuchs;", "tokens": ["Der", "schlanks\u00b7ten", "Tan\u00b7ne", "Wuchs", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Quellen Lispel und den Laut", "tokens": ["Der", "Quel\u00b7len", "Lis\u00b7pel", "und", "den", "Laut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der menschenholden Nachtigall;", "tokens": ["Der", "men\u00b7schen\u00b7hol\u00b7den", "Nach\u00b7ti\u00b7gall", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sch\u00f6n w\u00e4re dieses Bild; allein", "tokens": ["Sch\u00f6n", "w\u00e4\u00b7re", "die\u00b7ses", "Bild", ";", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "VAFIN", "PDAT", "NN", "$.", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch nicht Theresia.", "tokens": ["Noch", "nicht", "The\u00b7re\u00b7sia", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Und wie der Leib, so war Ihr Geist,", "tokens": ["Und", "wie", "der", "Leib", ",", "so", "war", "Ihr", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wie der Geist, so war Ihr Gl\u00fcck.", "tokens": ["Und", "wie", "der", "Geist", ",", "so", "war", "Ihr", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sollte vieler V\u00f6lker Frau", "tokens": ["Sie", "soll\u00b7te", "vie\u00b7ler", "V\u00f6l\u00b7ker", "Frau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Ihrem Vater seyn.", "tokens": ["Nach", "Ih\u00b7rem", "Va\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Von Ihrem Werthe sprach der Ruf", "tokens": ["Von", "Ih\u00b7rem", "Wert\u00b7he", "sprach", "der", "Ruf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In S\u00fcd und Ost, und Nord und West.", "tokens": ["In", "S\u00fcd", "und", "Ost", ",", "und", "Nord", "und", "West", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "KON", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihn h\u00f6rte junges F\u00fcrstenblut,", "tokens": ["Ihn", "h\u00f6r\u00b7te", "jun\u00b7ges", "F\u00fcrs\u00b7ten\u00b7blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wallte l\u00fcstern auf.", "tokens": ["Und", "wall\u00b7te", "l\u00fcs\u00b7tern", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und jedes Auge sah nach Wien,", "tokens": ["Und", "je\u00b7des", "Au\u00b7ge", "sah", "nach", "Wi\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und jeder Seufzer flog nach Wien,", "tokens": ["Und", "je\u00b7der", "Seuf\u00b7zer", "flog", "nach", "Wi\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und jeder Wunsch im Stillen war", "tokens": ["Und", "je\u00b7der", "Wunsch", "im", "Stil\u00b7len", "war"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPRART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der F\u00fcrstentochter Hand.", "tokens": ["Der", "F\u00fcrs\u00b7ten\u00b7toch\u00b7ter", "Hand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Da kam ein J\u00fcngling, dessen Blut", "tokens": ["Da", "kam", "ein", "J\u00fcng\u00b7ling", ",", "des\u00b7sen", "Blut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus eben jener Quelle war,", "tokens": ["Aus", "e\u00b7ben", "je\u00b7ner", "Quel\u00b7le", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PDAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus der der Tochter Habsburgs Blut", "tokens": ["Aus", "der", "der", "Toch\u00b7ter", "Habs\u00b7burgs", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seit grauen Altern flo\u00df.", "tokens": ["Seit", "grau\u00b7en", "Al\u00b7tern", "flo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "So sah ich Huldrich keinen noch,", "tokens": ["So", "sah", "ich", "Huld\u00b7rich", "kei\u00b7nen", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie dieser junge F\u00fchrer war.", "tokens": ["Wie", "die\u00b7ser", "jun\u00b7ge", "F\u00fch\u00b7rer", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er zog einher dem Hirschen gleich,", "tokens": ["Er", "zog", "ein\u00b7her", "dem", "Hir\u00b7schen", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem h\u00f6chsten auf der Flur.", "tokens": ["Dem", "h\u00f6chs\u00b7ten", "auf", "der", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Auf seinen Wangen bl\u00fchte Lenz,", "tokens": ["Auf", "sei\u00b7nen", "Wan\u00b7gen", "bl\u00fch\u00b7te", "Lenz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Rabenlocken fiel sein Haar,", "tokens": ["In", "Ra\u00b7ben\u00b7lo\u00b7cken", "fiel", "sein", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und was sein schmachtend Auge sprach,", "tokens": ["Und", "was", "sein", "schmach\u00b7tend", "Au\u00b7ge", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das singt kein S\u00e4nger aus.", "tokens": ["Das", "singt", "kein", "S\u00e4n\u00b7ger", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Vor seinem Geiste stiegen ihm", "tokens": ["Vor", "sei\u00b7nem", "Geis\u00b7te", "stie\u00b7gen", "ihm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht selten Siegesfelder auf;", "tokens": ["Nicht", "sel\u00b7ten", "Sie\u00b7ges\u00b7fel\u00b7der", "auf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An seiner hellen Spitze sa\u00df", "tokens": ["An", "sei\u00b7ner", "hel\u00b7len", "Spit\u00b7ze", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mondentr\u00e4ger Tod.", "tokens": ["Der", "Mon\u00b7den\u00b7tr\u00e4\u00b7ger", "Tod", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Er kam und sah Theresien", "tokens": ["Er", "kam", "und", "sah", "The\u00b7re\u00b7si\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text