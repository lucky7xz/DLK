{"textgrid.poem.39184": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[blitzesflammen, Wolkensch\u00e4ume]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blitzesflammen, Wolkensch\u00e4ume,", "tokens": ["Blit\u00b7zes\u00b7flam\u00b7men", ",", "Wol\u00b7ken\u00b7sch\u00e4u\u00b7me", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Duftgestalt'ge Zaubertr\u00e4ume,", "tokens": ["Duft\u00b7ge\u00b7stalt'\u00b7ge", "Zau\u00b7ber\u00b7tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lichter, die am Moore gaukeln,", "tokens": ["Lich\u00b7ter", ",", "die", "am", "Moo\u00b7re", "gau\u00b7keln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rosenbl\u00e4tter, Bl\u00fctenseime:", "tokens": ["Ro\u00b7sen\u00b7bl\u00e4t\u00b7ter", ",", "Bl\u00fc\u00b7ten\u00b7sei\u00b7me", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle schlang ich sie zusammen", "tokens": ["Al\u00b7le", "schlang", "ich", "sie", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch die B\u00e4nder loser Reime;", "tokens": ["Durch", "die", "B\u00e4n\u00b7der", "lo\u00b7ser", "Rei\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df auch Gr\u00fcn dazwischen dunkle,", "tokens": ["Da\u00df", "auch", "Gr\u00fcn", "da\u00b7zwi\u00b7schen", "dunk\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Pfl\u00fcckt' ich Laub der Palmenb\u00e4ume.", "tokens": ["Pfl\u00fcckt'", "ich", "Laub", "der", "Pal\u00b7men\u00b7b\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Fang' ihn auf, den Kranz der Lieder,", "tokens": ["Fang'", "ihn", "auf", ",", "den", "Kranz", "der", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKVZ", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Den ich werfe durch die R\u00e4ume,", "tokens": ["Den", "ich", "wer\u00b7fe", "durch", "die", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Da\u00df der Herrin duft'ge Locke", "tokens": ["Da\u00df", "der", "Her\u00b7rin", "duft'\u00b7ge", "Lo\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Er mit duft'gen Bl\u00fcten s\u00e4ume.", "tokens": ["Er", "mit", "duft'\u00b7gen", "Bl\u00fc\u00b7ten", "s\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}