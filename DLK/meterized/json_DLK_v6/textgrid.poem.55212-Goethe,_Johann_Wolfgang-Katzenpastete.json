{"textgrid.poem.55212": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Katzenpastete", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bew\u00e4hrt den Forscher der Natur", "tokens": ["Be\u00b7w\u00e4hrt", "den", "For\u00b7scher", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein frei und ruhig Schauen,", "tokens": ["Ein", "frei", "und", "ru\u00b7hig", "Schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So folge Me\u00dfkunst seiner Spur", "tokens": ["So", "fol\u00b7ge", "Me\u00df\u00b7kunst", "sei\u00b7ner", "Spur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Vorsicht und Vertrauen.", "tokens": ["Mit", "Vor\u00b7sicht", "und", "Ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zwar mag in ", "tokens": ["Zwar", "mag", "in"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Sich beides auch vereinen;", "tokens": ["Sich", "bei\u00b7des", "auch", "ver\u00b7ei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch da\u00df es zwei Gewerbe sind,", "tokens": ["Doch", "da\u00df", "es", "zwei", "Ge\u00b7wer\u00b7be", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "CARD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das l\u00e4\u00dft sich nicht verneinen.", "tokens": ["Das", "l\u00e4\u00dft", "sich", "nicht", "ver\u00b7nei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es war einmal ein braver Koch,", "tokens": ["Es", "war", "ein\u00b7mal", "ein", "bra\u00b7ver", "Koch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Geschickt im Appretieren;", "tokens": ["Ge\u00b7schickt", "im", "Ap\u00b7pre\u00b7tie\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem fiel es ein, er wollte doch", "tokens": ["Dem", "fiel", "es", "ein", ",", "er", "woll\u00b7te", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als J\u00e4ger sich gerieren.", "tokens": ["Als", "J\u00e4\u00b7ger", "sich", "ge\u00b7rie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er zog bewehrt zu gr\u00fcnem Wald,", "tokens": ["Er", "zog", "be\u00b7wehrt", "zu", "gr\u00fc\u00b7nem", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo manches Wildpret hauste,", "tokens": ["Wo", "man\u00b7ches", "Wild\u00b7pret", "haus\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und einen Kater scho\u00df er bald,", "tokens": ["Und", "ei\u00b7nen", "Ka\u00b7ter", "scho\u00df", "er", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der junge V\u00f6gel schmauste.", "tokens": ["Der", "jun\u00b7ge", "V\u00f6\u00b7gel", "schmaus\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sah ihn f\u00fcr einen Hasen an", "tokens": ["Sah", "ihn", "f\u00fcr", "ei\u00b7nen", "Ha\u00b7sen", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und lie\u00df sich nicht bedeuten,", "tokens": ["Und", "lie\u00df", "sich", "nicht", "be\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Pastetete viel W\u00fcrze dran", "tokens": ["Pas\u00b7te\u00b7te\u00b7te", "viel", "W\u00fcr\u00b7ze", "dran"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "PAV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Und setzt' ihn vor den Leuten.", "tokens": ["Und", "setzt'", "ihn", "vor", "den", "Leu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch manche G\u00e4ste das verdro\u00df,", "tokens": ["Doch", "man\u00b7che", "G\u00e4s\u00b7te", "das", "ver\u00b7dro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gewisse feine Nasen:", "tokens": ["Ge\u00b7wis\u00b7se", "fei\u00b7ne", "Na\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Katze, die der J\u00e4ger scho\u00df,", "tokens": ["Die", "Kat\u00b7ze", ",", "die", "der", "J\u00e4\u00b7ger", "scho\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Macht nie der Koch zum Hasen.", "tokens": ["Macht", "nie", "der", "Koch", "zum", "Ha\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}