{"textgrid.poem.45691": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[da die Deutschen noch bescheiden, nach alter Weise]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da die Deutschen noch bescheiden, nach alter Weise,", "tokens": ["Da", "die", "Deut\u00b7schen", "noch", "be\u00b7schei\u00b7den", ",", "nach", "al\u00b7ter", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sagt ich gern ein Wort zu ihrem Preise,", "tokens": ["Sagt", "ich", "gern", "ein", "Wort", "zu", "ih\u00b7rem", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Nun aber, da sie sich selber loben,", "tokens": ["Nun", "a\u00b7ber", ",", "da", "sie", "sich", "sel\u00b7ber", "lo\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "F\u00fchl ich mich f\u00fcrder der M\u00fch enthoben.", "tokens": ["F\u00fchl", "ich", "mich", "f\u00fcr\u00b7der", "der", "M\u00fch", "ent\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}