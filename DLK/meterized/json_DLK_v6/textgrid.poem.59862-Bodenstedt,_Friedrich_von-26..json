{"textgrid.poem.59862": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Worin besteht, Mirza-Schaffy,", "tokens": ["Wo\u00b7rin", "be\u00b7steht", ",", "Mir\u00b7za\u00b7Schaffy", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Zauber deiner Poesie?", "tokens": ["Der", "Zau\u00b7ber", "dei\u00b7ner", "Poe\u00b7sie", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da\u00df du in allem wahr bist", "tokens": ["Da\u00df", "du", "in", "al\u00b7lem", "wahr", "bist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIS", "ADJD", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und die Natur zu wahren wei\u00dft;", "tokens": ["Und", "die", "Na\u00b7tur", "zu", "wah\u00b7ren", "wei\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df du in allem klar bist", "tokens": ["Da\u00df", "du", "in", "al\u00b7lem", "klar", "bist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIS", "ADJD", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Wort und Sinn zu paaren wei\u00dft.", "tokens": ["Und", "Wort", "und", "Sinn", "zu", "paa\u00b7ren", "wei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da\u00df du nur nach dem Rechten greifst,", "tokens": ["Da\u00df", "du", "nur", "nach", "dem", "Rech\u00b7ten", "greifst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und alles recht betrachtest \u2013", "tokens": ["Und", "al\u00b7les", "recht", "be\u00b7trach\u00b7test", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df du nur Diamanten schleifst", "tokens": ["Da\u00df", "du", "nur", "Di\u00b7a\u00b7man\u00b7ten", "schleifst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Kiesel nicht beachtest!", "tokens": ["Und", "Kie\u00b7sel", "nicht", "be\u00b7ach\u00b7test", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}