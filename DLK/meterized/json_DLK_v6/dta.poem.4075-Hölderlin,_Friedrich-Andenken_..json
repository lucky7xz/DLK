{"dta.poem.4075": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Andenken .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Der Nordost weht,", "tokens": ["Der", "Nord\u00b7ost", "weht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der liebste unter den Winden", "tokens": ["Der", "liebs\u00b7te", "un\u00b7ter", "den", "Win\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mir, weil er feurigen Geist", "tokens": ["Mir", ",", "weil", "er", "feu\u00b7ri\u00b7gen", "Geist"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und gute Fahrt verhei\u00dfet den Schiffern.", "tokens": ["Und", "gu\u00b7te", "Fahrt", "ver\u00b7hei\u00b7\u00dfet", "den", "Schif\u00b7fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Geh' aber nun und gr\u00fc\u00dfe", "tokens": ["Geh'", "a\u00b7ber", "nun", "und", "gr\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die sch\u00f6ne Garonne,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Ga\u00b7ron\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Und die G\u00e4rten von Bourdeaux,", "tokens": ["Und", "die", "G\u00e4r\u00b7ten", "von", "Bour\u00b7dea\u00b7ux", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NE", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Dort wo am schroffen Ufer", "tokens": ["Dort", "wo", "am", "schrof\u00b7fen", "U\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Hingehet der Steg und in den Strom", "tokens": ["Hin\u00b7ge\u00b7het", "der", "Steg", "und", "in", "den", "Strom"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Tief f\u00e4llt der Bach, dar\u00fcber aber", "tokens": ["Tief", "f\u00e4llt", "der", "Bach", ",", "da\u00b7r\u00fc\u00b7ber", "a\u00b7ber"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "PAV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Hinschauet ein edel Paar", "tokens": ["Hin\u00b7schau\u00b7et", "ein", "e\u00b7del", "Paar"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Von Eichen und Silberpappeln!", "tokens": ["Von", "Ei\u00b7chen", "und", "Sil\u00b7ber\u00b7pap\u00b7peln", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+---", "measure": "iambic.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Noch denket das mir wohl und wie", "tokens": ["Noch", "den\u00b7ket", "das", "mir", "wohl", "und", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "PPER", "ADV", "KON", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die breiten Gipfel neiget", "tokens": ["Die", "brei\u00b7ten", "Gip\u00b7fel", "nei\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Ulmwald \u00fcber die M\u00fchl',", "tokens": ["Der", "Ulm\u00b7wald", "\u00fc\u00b7ber", "die", "M\u00fchl'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Im Hofe aber w\u00e4chst ein Feigenbaum,", "tokens": ["Im", "Ho\u00b7fe", "a\u00b7ber", "w\u00e4chst", "ein", "Fei\u00b7gen\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "An Feiertagen gehn", "tokens": ["An", "Fei\u00b7er\u00b7ta\u00b7gen", "gehn"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die braunen Frauen daselbst", "tokens": ["Die", "brau\u00b7nen", "Frau\u00b7en", "da\u00b7selbst"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PAV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Auf seidnen Boden,", "tokens": ["Auf", "seid\u00b7nen", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Zur M\u00e4rzenzeit,", "tokens": ["Zur", "M\u00e4r\u00b7zen\u00b7zeit", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Wenn gleich ist Nacht und Tag,", "tokens": ["Wenn", "gleich", "ist", "Nacht", "und", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Und \u00fcber langsamen Stegen,", "tokens": ["Und", "\u00fc\u00b7ber", "lang\u00b7sa\u00b7men", "Ste\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Von goldenen Tr\u00e4umen schwer,", "tokens": ["Von", "gol\u00b7de\u00b7nen", "Tr\u00e4u\u00b7men", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Einwiegende L\u00fcfte ziehen.", "tokens": ["Ein\u00b7wie\u00b7gen\u00b7de", "L\u00fcf\u00b7te", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Es reiche aber,", "tokens": ["Es", "rei\u00b7che", "a\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Des dunkeln Lichtes voll,", "tokens": ["Des", "dun\u00b7keln", "Lich\u00b7tes", "voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir Einer den duftenden Becher,", "tokens": ["Mir", "Ei\u00b7ner", "den", "duf\u00b7ten\u00b7den", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Damit ich ruhen m\u00f6ge; denn s\u00fc\u00df", "tokens": ["Da\u00b7mit", "ich", "ru\u00b7hen", "m\u00f6\u00b7ge", ";", "denn", "s\u00fc\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$.", "KON", "NE"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "W\u00e4r' unter Schatten der Schlummer.", "tokens": ["W\u00e4r'", "un\u00b7ter", "Schat\u00b7ten", "der", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Nicht ist es gut,", "tokens": ["Nicht", "ist", "es", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Seellos vor sterblichen", "tokens": ["Seel\u00b7los", "vor", "sterb\u00b7li\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "VVINF"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Gedanken zu seyn, doch gut", "tokens": ["Ge\u00b7dan\u00b7ken", "zu", "seyn", ",", "doch", "gut"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PTKZU", "VAINF", "$,", "ADV", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Ist ein Gespr\u00e4ch und zu sagen", "tokens": ["Ist", "ein", "Ge\u00b7spr\u00e4ch", "und", "zu", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "KON", "PTKZU", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Des Herzens Meinung, zu h\u00f6ren viel", "tokens": ["Des", "Her\u00b7zens", "Mei\u00b7nung", ",", "zu", "h\u00f6\u00b7ren", "viel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PTKZU", "VVINF", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Von Tagen der Lieb',", "tokens": ["Von", "Ta\u00b7gen", "der", "Lieb'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Und Thaten, welche geschahen.", "tokens": ["Und", "Tha\u00b7ten", ",", "wel\u00b7che", "ge\u00b7scha\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Wo aber sind die Freunde? Bellarmin", "tokens": ["Wo", "a\u00b7ber", "sind", "die", "Freun\u00b7de", "?", "Be\u00b7llar\u00b7min"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ADV", "VAFIN", "ART", "NN", "$.", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit dem Gef\u00e4hrten? Mancher", "tokens": ["Mit", "dem", "Ge\u00b7f\u00e4hr\u00b7ten", "?", "Man\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$.", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Tr\u00e4gt Scheue, an die Quelle zu gehn;", "tokens": ["Tr\u00e4gt", "Scheu\u00b7e", ",", "an", "die", "Quel\u00b7le", "zu", "gehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Es beginnt nehmlich der Reichthum", "tokens": ["Es", "be\u00b7ginnt", "nehm\u00b7lich", "der", "Reicht\u00b7hum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Im Meere. Sie,", "tokens": ["Im", "Mee\u00b7re", ".", "Sie", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$.", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Wie Mahler, bringen zusammen", "tokens": ["Wie", "Mah\u00b7ler", ",", "brin\u00b7gen", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAV", "NN", "$,", "VVINF", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Das Sch\u00f6ne der Erd' und verschm\u00e4hn", "tokens": ["Das", "Sch\u00f6\u00b7ne", "der", "Erd'", "und", "ver\u00b7schm\u00e4hn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "KON", "VVINF"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Den gefl\u00fcgelten Krieg nicht, und", "tokens": ["Den", "ge\u00b7fl\u00fc\u00b7gel\u00b7ten", "Krieg", "nicht", ",", "und"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "$,", "KON"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Zu wohnen einsam, jahrlang, unter", "tokens": ["Zu", "woh\u00b7nen", "ein\u00b7sam", ",", "jahr\u00b7lang", ",", "un\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PTKZU", "VVINF", "ADJD", "$,", "ADJD", "$,", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dem entlaubten Mast, wo nicht die Nacht durch-", "tokens": ["Dem", "ent\u00b7laub\u00b7ten", "Mast", ",", "wo", "nicht", "die", "Nacht", "durch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PTKNEG", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "gl\u00e4nzen", "tokens": ["gl\u00e4n\u00b7zen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Die Feiertage der Stadt,", "tokens": ["Die", "Fei\u00b7er\u00b7ta\u00b7ge", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Und Saitenspiel und eingeborner Tanz nicht.", "tokens": ["Und", "Sai\u00b7ten\u00b7spiel", "und", "ein\u00b7ge\u00b7bor\u00b7ner", "Tanz", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Nun aber sind zu Indiern", "tokens": ["Nun", "a\u00b7ber", "sind", "zu", "In\u00b7diern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "APPR", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die M\u00e4nner gegangen,", "tokens": ["Die", "M\u00e4n\u00b7ner", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Dort an der luftigen Spitz'", "tokens": ["Dort", "an", "der", "luf\u00b7ti\u00b7gen", "Spitz'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "An Traubenbergen, wo herab", "tokens": ["An", "Trau\u00b7ben\u00b7ber\u00b7gen", ",", "wo", "her\u00b7ab"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "PWAV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Dordogne kommt", "tokens": ["Die", "Dor\u00b7dog\u00b7ne", "kommt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-++-+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Und zusammen mit der pr\u00e4cht'gen", "tokens": ["Und", "zu\u00b7sam\u00b7men", "mit", "der", "pr\u00e4cht'\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Garonne meerbreit", "tokens": ["Ga\u00b7ron\u00b7ne", "meer\u00b7breit"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Ausgehet der Strom. Es mehret aber", "tokens": ["Aus\u00b7ge\u00b7het", "der", "Strom", ".", "Es", "meh\u00b7ret", "a\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Und giebt Ged\u00e4chtni\u00df die See", "tokens": ["Und", "giebt", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "die", "See"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Und die Lieb' auch heftet flei\u00dfig die Augen,", "tokens": ["Und", "die", "Lieb'", "auch", "hef\u00b7tet", "flei\u00b7\u00dfig", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Was bleibt aber, stiften die Dichter.", "tokens": ["Was", "bleibt", "a\u00b7ber", ",", "stif\u00b7ten", "die", "Dich\u00b7ter."], "token_info": ["word", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["PWS", "VVFIN", "ADV", "$,", "VVFIN", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}