{"dta.poem.13772": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Tyroler.  \n Im Jahre 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "O Herr! la\u00df diese Lohen weh'n, sich breiten", "tokens": ["O", "Herr", "!", "la\u00df", "die\u00b7se", "Lo\u00b7hen", "weh'n", ",", "sich", "brei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$.", "VVIMP", "PDAT", "NN", "PTKVZ", "$,", "PRF", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auffordernd \u00fcber alle deutsche Lande,", "tokens": ["Auf\u00b7for\u00b7dernd", "\u00fc\u00b7ber", "al\u00b7le", "deut\u00b7sche", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wer da f\u00e4llt, dem schenk' so glorreich Ende!", "tokens": ["Und", "wer", "da", "f\u00e4llt", ",", "dem", "schenk'", "so", "glor\u00b7reich", "En\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "$,", "PRELS", "VVFIN", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}