{"textgrid.poem.54318": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die stoltze Rosilis tr\u00e4gt stets den Kopf empor,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die stoltze Rosilis tr\u00e4gt stets den Kopf empor,", "tokens": ["Die", "stolt\u00b7ze", "Ro\u00b7si\u00b7lis", "tr\u00e4gt", "stets", "den", "Kopf", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schwatzt der gantzen Welt von ihren Ahnen vor,", "tokens": ["Und", "schwatzt", "der", "gant\u00b7zen", "Welt", "von", "ih\u00b7ren", "Ah\u00b7nen", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die m\u00fcsten, dencket sie, nur blo\u00df die Damen ziehren,", "tokens": ["Die", "m\u00fcs\u00b7ten", ",", "den\u00b7cket", "sie", ",", "nur", "blo\u00df", "die", "Da\u00b7men", "zieh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "PPER", "$,", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch l\u00e4st kein Ritter sich durch den Gesang verf\u00fchren.", "tokens": ["Doch", "l\u00e4st", "kein", "Rit\u00b7ter", "sich", "durch", "den", "Ge\u00b7sang", "ver\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Warum? die Sitten seynd zu schlecht, wie man erblickt,", "tokens": ["Wa\u00b7rum", "?", "die", "Sit\u00b7ten", "seynd", "zu", "schlecht", ",", "wie", "man", "er\u00b7blickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "ART", "NN", "VAFIN", "PTKA", "ADJD", "$,", "PWAV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Hochmuth, der sie fast zur Erden niederdr\u00fcckt", "tokens": ["Der", "Hoch\u00b7muth", ",", "der", "sie", "fast", "zur", "Er\u00b7den", "nie\u00b7der\u00b7dr\u00fcckt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Verg\u00f6nnt ihr dennoch nicht ein einges Knie zu beugen,", "tokens": ["Ver\u00b7g\u00f6nnt", "ihr", "den\u00b7noch", "nicht", "ein", "ein\u00b7ges", "Knie", "zu", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn man durch Gr\u00fcssen ihr will H\u00f6flichkeit erzeigen.", "tokens": ["Wenn", "man", "durch", "Gr\u00fcs\u00b7sen", "ihr", "will", "H\u00f6f\u00b7lich\u00b7keit", "er\u00b7zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie tritt, nach Pfauen Art, r\u00fcmpfft ihren Mund darbey,", "tokens": ["Sie", "tritt", ",", "nach", "Pfau\u00b7en", "Art", ",", "r\u00fcmpfft", "ih\u00b7ren", "Mund", "dar\u00b7bey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "NN", "NN", "$,", "VVFIN", "PPOSAT", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und thut als ob sie selbst des Mogols Tochter sey.", "tokens": ["Und", "thut", "als", "ob", "sie", "selbst", "des", "Mo\u00b7gols", "Toch\u00b7ter", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "KOUS", "PPER", "ADV", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wer nicht viel Ahnen zehlt, den pflegt sie zu verlachen", "tokens": ["Wer", "nicht", "viel", "Ah\u00b7nen", "zehlt", ",", "den", "pflegt", "sie", "zu", "ver\u00b7la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PTKNEG", "PIAT", "NN", "VVFIN", "$,", "ART", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Als k\u00f6nten sie nur blo\u00df das Spiel gewinnend machen.", "tokens": ["Als", "k\u00f6n\u00b7ten", "sie", "nur", "blo\u00df", "das", "Spiel", "ge\u00b7win\u00b7nend", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ADV", "ADV", "ART", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Viel Ahnen, wenig Geld, von Witz und Klugheit blo\u00df,", "tokens": ["Viel", "Ah\u00b7nen", ",", "we\u00b7nig", "Geld", ",", "von", "Witz", "und", "Klug\u00b7heit", "blo\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "APPR", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und auch an Tugend arm, klingt, d\u00fcnckt mich, nicht recht gro\u00df.", "tokens": ["Und", "auch", "an", "Tu\u00b7gend", "arm", ",", "klingt", ",", "d\u00fcnckt", "mich", ",", "nicht", "recht", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "ADJD", "$,", "VVFIN", "$,", "VVFIN", "PPER", "$,", "PTKNEG", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Beth\u00f6rte Rosilis, es ist bereits entdecket,", "tokens": ["Be\u00b7th\u00f6r\u00b7te", "Ro\u00b7si\u00b7lis", ",", "es", "ist", "be\u00b7reits", "ent\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPER", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wie weit dein Staat und auch sein Blendwerck sich erstrecket.", "tokens": ["Wie", "weit", "dein", "Staat", "und", "auch", "sein", "Blend\u00b7werck", "sich", "er\u00b7stre\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "KON", "ADV", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ein hundert G\u00fcldgen seynd und diese kaum zur Noth,", "tokens": ["Ein", "hun\u00b7dert", "G\u00fcld\u00b7gen", "seynd", "und", "die\u00b7se", "kaum", "zur", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "VAFIN", "KON", "PDS", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dein gantzes Erb und Gut; ein kleines St\u00fcckgen Brod!", "tokens": ["Dein", "gant\u00b7zes", "Erb", "und", "Gut", ";", "ein", "klei\u00b7nes", "St\u00fcck\u00b7gen", "Brod", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "ADJD", "$.", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die Freyer pflegen sich nicht sehr darum zu reissen,", "tokens": ["Die", "Frey\u00b7er", "pfle\u00b7gen", "sich", "nicht", "sehr", "da\u00b7rum", "zu", "reis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "ADV", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Denn obgleich Crito scheint dein Spa\u00df-Galan zu heissen.", "tokens": ["Denn", "ob\u00b7gleich", "Cri\u00b7to", "scheint", "dein", "Spa\u00df\u00b7Ga\u00b7lan", "zu", "heis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Dieweil du dir um ihn recht grosse M\u00fche giebst,", "tokens": ["Die\u00b7weil", "du", "dir", "um", "ihn", "recht", "gros\u00b7se", "M\u00fc\u00b7he", "giebst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "PPER", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und ihn, wie man an dir versp\u00fchrt, recht \u00e4ngstlich liebst,", "tokens": ["Und", "ihn", ",", "wie", "man", "an", "dir", "ver\u00b7sp\u00fchrt", ",", "recht", "\u00e4ngst\u00b7lich", "liebst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PWAV", "PIS", "APPR", "PPER", "VVPP", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.23": {"text": "So d\u00fcrffte selbger doch, wohl schwerlich sich bequemen,", "tokens": ["So", "d\u00fcrff\u00b7te", "selb\u00b7ger", "doch", ",", "wohl", "schwer\u00b7lich", "sich", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADJD", "ADV", "$,", "ADV", "ADJD", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der Ahnen Meng und Wust statt Mit-Gifft anzunehmen.", "tokens": ["Der", "Ah\u00b7nen", "Meng", "und", "Wust", "statt", "Mit\u00b7Gifft", "an\u00b7zu\u00b7neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "NN", "APPR", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Gesetzt, da\u00df dein Geschlecht gar nicht zu tadeln ist /", "tokens": ["Ge\u00b7setzt", ",", "da\u00df", "dein", "Ge\u00b7schlecht", "gar", "nicht", "zu", "ta\u00b7deln", "ist", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPOSAT", "NN", "ADV", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "So fragt es sich darbey, ob du es w\u00fcrdig bist?", "tokens": ["So", "fragt", "es", "sich", "dar\u00b7bey", ",", "ob", "du", "es", "w\u00fcr\u00b7dig", "bist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PAV", "$,", "KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Verstand und Tugend mu\u00df dem Adel Glantz und Leben,", "tokens": ["Ver\u00b7stand", "und", "Tu\u00b7gend", "mu\u00df", "dem", "A\u00b7del", "Glantz", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wofern er soll der Welt ins Auge schimmern, geben.", "tokens": ["Wo\u00b7fern", "er", "soll", "der", "Welt", "ins", "Au\u00b7ge", "schim\u00b7mern", ",", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ART", "NN", "APPRART", "NN", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Da man di\u00df aber nicht an dir erblicken kan,", "tokens": ["Da", "man", "di\u00df", "a\u00b7ber", "nicht", "an", "dir", "er\u00b7bli\u00b7cken", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PDS", "ADV", "PTKNEG", "APPR", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So bist du, Rosilis, wahrhafftig \u00fcbel dran.", "tokens": ["So", "bist", "du", ",", "Ro\u00b7si\u00b7lis", ",", "wahr\u00b7haff\u00b7tig", "\u00fc\u00b7bel", "dran", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NE", "$,", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wilst du gef\u00e4llig seyn, so \u00e4ndre dein Beginnen,", "tokens": ["Wilst", "du", "ge\u00b7f\u00e4l\u00b7lig", "seyn", ",", "so", "\u00e4nd\u00b7re", "dein", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VAINF", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Durch Hochmuth l\u00e4st sich nicht der Menschen Gunst gewinnen.", "tokens": ["Durch", "Hoch\u00b7muth", "l\u00e4st", "sich", "nicht", "der", "Men\u00b7schen", "Gunst", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "PTKNEG", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}