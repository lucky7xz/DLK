{"textgrid.poem.52482": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "1. Mailied", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ringsum ist alles neu belebt!", "tokens": ["Ring\u00b7sum", "ist", "al\u00b7les", "neu", "be\u00b7lebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Es gl\u00e4nzt das bunte Thal;", "tokens": ["Es", "gl\u00e4nzt", "das", "bun\u00b7te", "Thal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am H\u00fcgel, durch die Fichten bebt", "tokens": ["Am", "H\u00fc\u00b7gel", ",", "durch", "die", "Fich\u00b7ten", "bebt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der goldnen Sonne Strahl;", "tokens": ["Der", "gold\u00b7nen", "Son\u00b7ne", "Strahl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und lauter Jubel der Natur", "tokens": ["Und", "lau\u00b7ter", "Ju\u00b7bel", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Steigt hoch empor von gr\u00fcner Flur!", "tokens": ["Steigt", "hoch", "em\u00b7por", "von", "gr\u00fc\u00b7ner", "Flur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Belaubt ist schon der Bl\u00fctenbaum,", "tokens": ["Be\u00b7laubt", "ist", "schon", "der", "Bl\u00fc\u00b7ten\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und d\u00e4mmernd die Allee;", "tokens": ["Und", "d\u00e4m\u00b7mernd", "die", "Al\u00b7lee", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und silbern blinkt der Quelle Schaum;", "tokens": ["Und", "sil\u00b7bern", "blinkt", "der", "Quel\u00b7le", "Schaum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und golden ruht der See;", "tokens": ["Und", "gol\u00b7den", "ruht", "der", "See", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Lerche Lied, der Wachtel Schlag,", "tokens": ["Der", "Ler\u00b7che", "Lied", ",", "der", "Wach\u00b7tel", "Schlag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Begr\u00fc\u00dft den sch\u00f6nsten Fr\u00fchlingstag.", "tokens": ["Be\u00b7gr\u00fc\u00dft", "den", "sch\u00f6ns\u00b7ten", "Fr\u00fch\u00b7lings\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Aus vollgeschwollnen Knospen dringt", "tokens": ["Aus", "voll\u00b7ge\u00b7schwoll\u00b7nen", "Knos\u00b7pen", "dringt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Manch Bl\u00fcmchen gelb und blau;", "tokens": ["Manch", "Bl\u00fcm\u00b7chen", "gelb", "und", "blau", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf roten Pfirsichbl\u00fcten blinkt,", "tokens": ["Auf", "ro\u00b7ten", "Pfir\u00b7sich\u00b7bl\u00fc\u00b7ten", "blinkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Perlen, heller Tau,", "tokens": ["Wie", "Per\u00b7len", ",", "hel\u00b7ler", "Tau", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und s\u00fc\u00dfer Hyazinthen Duft", "tokens": ["Und", "s\u00fc\u00b7\u00dfer", "Hy\u00b7a\u00b7zin\u00b7then", "Duft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durchw\u00fcrzt die reine Fr\u00fchlingsluft.", "tokens": ["Durch\u00b7w\u00fcrzt", "die", "rei\u00b7ne", "Fr\u00fch\u00b7lings\u00b7luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf gr\u00fcnem Weizenfelde bebt", "tokens": ["Auf", "gr\u00fc\u00b7nem", "Wei\u00b7zen\u00b7fel\u00b7de", "bebt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des \u00c4thers Silberglanz;", "tokens": ["Des", "\u00c4\u00b7thers", "Sil\u00b7ber\u00b7glanz", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um jedes zarte Gr\u00e4schen schwebt", "tokens": ["Um", "je\u00b7des", "zar\u00b7te", "Gr\u00e4sc\u00b7hen", "schwebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein M\u00fcckenheer im Tanz;", "tokens": ["Ein", "M\u00fc\u00b7cken\u00b7heer", "im", "Tanz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Laut pl\u00e4tschern, unterm Erlendach,", "tokens": ["Laut", "pl\u00e4t\u00b7schern", ",", "un\u00b7term", "Er\u00b7len\u00b7dach", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Forellen in dem Kieselbach.", "tokens": ["Fo\u00b7rel\u00b7len", "in", "dem", "Kie\u00b7sel\u00b7bach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Kein leichtes, graues W\u00f6lkchen wallt", "tokens": ["Kein", "leich\u00b7tes", ",", "grau\u00b7es", "W\u00f6lk\u00b7chen", "wallt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ADJA", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im dunklen Himmelblau;", "tokens": ["Im", "dunk\u00b7len", "Him\u00b7mel\u00b7blau", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Nachtigallenlied durchschallt,", "tokens": ["Und", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7lied", "durch\u00b7schallt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Laut wirbelnd, Busch und Au'!", "tokens": ["Laut", "wir\u00b7belnd", ",", "Busch", "und", "Au'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "VVPP", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich will hinaus; auf gr\u00fcner Flur", "tokens": ["Ich", "will", "hin\u00b7aus", ";", "auf", "gr\u00fc\u00b7ner", "Flur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKVZ", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mich freu'n der pr\u00e4chtigen Natur!", "tokens": ["Mich", "freu'n", "der", "pr\u00e4ch\u00b7ti\u00b7gen", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}