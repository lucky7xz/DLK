{"dta.poem.7006": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Herrn  B .  H .  B rockes  \n eigene,  \n so wohl  M oralische als  \n Physicalische,  \n  G edichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was an den Gesch\u00f6pfen sch\u00f6n,", "tokens": ["Was", "an", "den", "Ge\u00b7sch\u00f6p\u00b7fen", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Kann der Sch\u00f6pfer in den Seelen,", "tokens": ["Kann", "der", "Sch\u00f6p\u00b7fer", "in", "den", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welche solchen Zierrath wehlen,", "tokens": ["Wel\u00b7che", "sol\u00b7chen", "Zier\u00b7rath", "weh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doppelt und vergeistert sehn.", "tokens": ["Dop\u00b7pelt", "und", "ver\u00b7geis\u00b7tert", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja es scheint, da\u00df sie die Pracht", "tokens": ["Ja", "es", "scheint", ",", "da\u00df", "sie", "die", "Pracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich nicht nur zum Schmuck erlesen,", "tokens": ["Sich", "nicht", "nur", "zum", "Schmuck", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sondern, da\u00df sie gar ihr Wesen", "tokens": ["Son\u00b7dern", ",", "da\u00df", "sie", "gar", "ihr", "We\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Selber noch vollkommner macht.", "tokens": ["Sel\u00b7ber", "noch", "voll\u00b7komm\u00b7ner", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}