{"dta.poem.2888": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "89.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vergleiche dich nur oft nach unten und nach oben,", "tokens": ["Ver\u00b7glei\u00b7che", "dich", "nur", "oft", "nach", "un\u00b7ten", "und", "nach", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "ADV", "KON", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du dem\u00fctig hier und dort dich f\u00fchlst erhoben;", "tokens": ["Da\u00df", "du", "de\u00b7m\u00fc\u00b7tig", "hier", "und", "dort", "dich", "f\u00fchlst", "er\u00b7ho\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "KON", "ADV", "PPER", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dem\u00fctig, wenn du f\u00fchlst, den Schw\u00e4chsten gleichest du,", "tokens": ["De\u00b7m\u00fc\u00b7tig", ",", "wenn", "du", "f\u00fchlst", ",", "den", "Schw\u00e4chs\u00b7ten", "glei\u00b7chest", "du", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erhoben, weil du strebst mit H\u00f6chsten H\u00f6chstem zu.", "tokens": ["Er\u00b7ho\u00b7ben", ",", "weil", "du", "strebst", "mit", "H\u00f6chs\u00b7ten", "H\u00f6chs\u00b7tem", "zu", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}