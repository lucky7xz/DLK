{"textgrid.poem.44419": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Bei der R\u00fcckkehr nach \u00d6sterreich", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei mir gegr\u00fc\u00dft, mein \u00d6sterreich,", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "mein", "\u00d6s\u00b7ter\u00b7reich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du herrlich bl\u00fchnder Gottesgarten,", "tokens": ["Du", "herr\u00b7lich", "bl\u00fchn\u00b7der", "Got\u00b7tes\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit deinen Fluren Eden gleich,", "tokens": ["Mit", "dei\u00b7nen", "Flu\u00b7ren", "E\u00b7den", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit deiner Berge ewgen Warten,", "tokens": ["Mit", "dei\u00b7ner", "Ber\u00b7ge", "ew\u00b7gen", "War\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit deinem Volk, so brav und treu,", "tokens": ["Mit", "dei\u00b7nem", "Volk", ",", "so", "brav", "und", "treu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In deiner Sch\u00f6nheit ewig neu,", "tokens": ["In", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "e\u00b7wig", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit allem, was dir Gott gegeben,", "tokens": ["Mit", "al\u00b7lem", ",", "was", "dir", "Gott", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PWS", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vor allen andern dich zu heben.", "tokens": ["Vor", "al\u00b7len", "an\u00b7dern", "dich", "zu", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PIS", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr kalte Schw\u00e4tzer, spottet ihr,", "tokens": ["Ihr", "kal\u00b7te", "Schw\u00e4t\u00b7zer", ",", "spot\u00b7tet", "ihr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als ob die Kraft uns z\u00f6gernd schliefe,", "tokens": ["Als", "ob", "die", "Kraft", "uns", "z\u00f6\u00b7gernd", "schlie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hoch in der H\u00f6he fliegen wir", "tokens": ["Hoch", "in", "der", "H\u00f6\u00b7he", "flie\u00b7gen", "wir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und g\u00f6nnen gern dem Fisch die Tiefe.", "tokens": ["Und", "g\u00f6n\u00b7nen", "gern", "dem", "Fisch", "die", "Tie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Grabt nur in eures Wissens Schacht,", "tokens": ["Grabt", "nur", "in", "eu\u00b7res", "Wis\u00b7sens", "Schacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und teilt, was ihr zu Tag gebracht:", "tokens": ["Und", "teilt", ",", "was", "ihr", "zu", "Tag", "ge\u00b7bracht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Teile kann das Buch euch geben,", "tokens": ["Die", "Tei\u00b7le", "kann", "das", "Buch", "euch", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das Ganze finden ", "tokens": ["Das", "Gan\u00b7ze", "fin\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Um recht zu tun und gut, gebricht", "tokens": ["Um", "recht", "zu", "tun", "und", "gut", ",", "ge\u00b7bricht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUI", "ADJD", "PTKZU", "VVINF", "KON", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dem \u00d6streichs Mann die Freiheit nicht;", "tokens": ["Dem", "\u00d6s\u00b7treichs", "Mann", "die", "Frei\u00b7heit", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Den Argen aber und den B\u00f6sen,", "tokens": ["Den", "Ar\u00b7gen", "a\u00b7ber", "und", "den", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "M\u00f6g nirgends Gott die Z\u00fcgel l\u00f6sen.", "tokens": ["M\u00f6g", "nir\u00b7gends", "Gott", "die", "Z\u00fc\u00b7gel", "l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "...", "tokens": ["..."], "token_info": ["punct"], "pos": ["$("]}}}}}