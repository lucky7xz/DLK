{"textgrid.poem.54543": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf mein R\u00f6schgin! das der H\u00f6chste in dem Dornen-Pusch der Zeiten/", "tokens": ["Auf", "mein", "R\u00f6schgin", "!", "das", "der", "H\u00f6chs\u00b7te", "in", "dem", "Dor\u00b7nen\u00b7Pusch", "der", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ART", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Die uns ritzen/ die uns stechen; Da\u00df die Hertzens-Ader springt:", "tokens": ["Die", "uns", "rit\u00b7zen", "/", "die", "uns", "ste\u00b7chen", ";", "Da\u00df", "die", "Hert\u00b7zens\u00b7A\u00b7der", "springt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "$(", "PRELS", "PPER", "VVINF", "$.", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.3": {"text": "Indem selbst die Rose Sarons mit dem sch\u00e4rffsten Tode ringt/", "tokens": ["In\u00b7dem", "selbst", "die", "Ro\u00b7se", "Sa\u00b7rons", "mit", "dem", "sch\u00e4rffs\u00b7ten", "To\u00b7de", "ringt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NE", "APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Er die Rose Sarons selber/ wolt in diese Zeit einleiten.", "tokens": ["Er", "die", "Ro\u00b7se", "Sa\u00b7rons", "sel\u00b7ber", "/", "wolt", "in", "die\u00b7se", "Zeit", "ein\u00b7lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NE", "ADV", "$(", "VMFIN", "APPR", "PDAT", "NN", "VVIZU", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Auf mein R\u00f6schgin! Das beflecket Mutter Evens He\u00dfligkeiten/", "tokens": ["Auf", "mein", "R\u00f6schgin", "!", "Das", "be\u00b7fle\u00b7cket", "Mut\u00b7ter", "E\u00b7vens", "He\u00df\u00b7lig\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PDS", "VVFIN", "NN", "NE", "NE", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Das verdorret von dem Fluchen welches Dorn und Disteln bringt:", "tokens": ["Das", "ver\u00b7dor\u00b7ret", "von", "dem", "Flu\u00b7chen", "wel\u00b7ches", "Dorn", "und", "Dis\u00b7teln", "bringt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "PWAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Dich erquicke diese Quelle/ die aus Christi Seit entspringt;", "tokens": ["Dich", "er\u00b7qui\u00b7cke", "die\u00b7se", "Quel\u00b7le", "/", "die", "aus", "Chris\u00b7ti", "Seit", "ent\u00b7springt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$(", "ART", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "So wird dich kein Sonnen-stechen auch kein Nachtes Frost bestreiten.", "tokens": ["So", "wird", "dich", "kein", "Son\u00b7nen\u00b7ste\u00b7chen", "auch", "kein", "Nach\u00b7tes", "Frost", "be\u00b7strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "ADV", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.9": {"text": "Sind Rosen von der G\u00f6ttin Blute roth:", "tokens": ["Sind", "Ro\u00b7sen", "von", "der", "G\u00f6t\u00b7tin", "Blu\u00b7te", "roth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wer glaubet di\u00df? dir blutet selber Gott/", "tokens": ["Wer", "glau\u00b7bet", "di\u00df", "?", "dir", "blu\u00b7tet", "sel\u00b7ber", "Gott", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDS", "$.", "PPER", "VVFIN", "ADV", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und l\u00e4st Ihm dir zu gut H\u00e4nd/ Fu\u00df und Hertz auffreissen.", "tokens": ["Und", "l\u00e4st", "Ihm", "dir", "zu", "gut", "H\u00e4nd", "/", "Fu\u00df", "und", "Hertz", "auf\u00b7freis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKA", "ADJD", "NN", "$(", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn dich dieser Purpur schm\u00fccket;", "tokens": ["Wenn", "dich", "die\u00b7ser", "Pur\u00b7pur", "schm\u00fc\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Ist dirs hier und dort gegl\u00fccket", "tokens": ["Ist", "dirs", "hier", "und", "dort", "ge\u00b7gl\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "ADV", "KON", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und kanst bey Mensch und Gott ein sch\u00f6nes R\u00f6schgin heissen.", "tokens": ["Und", "kanst", "bey", "Mensch", "und", "Gott", "ein", "sch\u00f6\u00b7nes", "R\u00f6schgin", "heis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}