{"textgrid.poem.52448": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "30. An ein Thal", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Entlegnes Thal, von Fichtenh\u00f6hn begrenzt,", "tokens": ["Ent\u00b7leg\u00b7nes", "Thal", ",", "von", "Fich\u00b7ten\u00b7h\u00f6hn", "be\u00b7grenzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Erlenreihn umhegte, flache Matten!", "tokens": ["Mit", "Er\u00b7len\u00b7reihn", "um\u00b7heg\u00b7te", ",", "fla\u00b7che", "Mat\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Bach, auf dem ein g\u00fcldnes Schlaglicht gl\u00e4nzt!", "tokens": ["O", "Bach", ",", "auf", "dem", "ein", "g\u00fcld\u00b7nes", "Schlag\u00b7licht", "gl\u00e4nzt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O Meierhof, in dunkeln Wallnu\u00dfschatten!", "tokens": ["O", "Mei\u00b7er\u00b7hof", ",", "in", "dun\u00b7keln", "Wall\u00b7nu\u00df\u00b7schat\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Freudenruf entz\u00fcckter Wandrer gr\u00fc\u00dft", "tokens": ["Der", "Freu\u00b7den\u00b7ruf", "ent\u00b7z\u00fcck\u00b7ter", "Wand\u00b7rer", "gr\u00fc\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dich, holdes Thal, vom Gipfel ferner H\u00fcgel;", "tokens": ["Dich", ",", "hol\u00b7des", "Thal", ",", "vom", "Gip\u00b7fel", "fer\u00b7ner", "H\u00fc\u00b7gel", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Betrachtung sinnt, wo sich dein Quell ergie\u00dft;", "tokens": ["Be\u00b7trach\u00b7tung", "sinnt", ",", "wo", "sich", "dein", "Quell", "er\u00b7gie\u00dft", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In deinem Hain saust der Begeist'rung Fl\u00fcgel.", "tokens": ["In", "dei\u00b7nem", "Hain", "saust", "der", "Be\u00b7geist'\u00b7rung", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nimm, trauter Hain, nimm Schattengang, mich auf!", "tokens": ["Nimm", ",", "trau\u00b7ter", "Hain", ",", "nimm", "Schat\u00b7ten\u00b7gang", ",", "mich", "auf", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "VVIMP", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In deiner Nacht entschlummern alle Sorgen!", "tokens": ["In", "dei\u00b7ner", "Nacht", "ent\u00b7schlum\u00b7mern", "al\u00b7le", "Sor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Beschr\u00e4nkt, wie du, ist auch mein Erdenlauf;", "tokens": ["Be\u00b7schr\u00e4nkt", ",", "wie", "du", ",", "ist", "auch", "mein", "Er\u00b7den\u00b7lauf", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dein Ausgang mir, so wie sein Schlu\u00df, verborgen.", "tokens": ["Dein", "Aus\u00b7gang", "mir", ",", "so", "wie", "sein", "Schlu\u00df", ",", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$,", "ADV", "KOKOM", "PPOSAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hier ruht der Ehrsucht Schiff am treuen Strand;", "tokens": ["Hier", "ruht", "der", "Ehr\u00b7sucht", "Schiff", "am", "treu\u00b7en", "Strand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gen\u00fcgsamkeit band es an Blumenk\u00fcsten.", "tokens": ["Ge\u00b7n\u00fcg\u00b7sam\u00b7keit", "band", "es", "an", "Blu\u00b7men\u00b7k\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Der Vorwitz legt sein Fernrohr aus der Hand;", "tokens": ["Der", "Vor\u00b7witz", "legt", "sein", "Fern\u00b7rohr", "aus", "der", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Besorgnis sp\u00e4ht nicht nach der Zukunft W\u00fcsten.", "tokens": ["Be\u00b7sorg\u00b7nis", "sp\u00e4ht", "nicht", "nach", "der", "Zu\u00b7kunft", "W\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die Bosheit spr\u00fcht hier nicht ihr Nattergift", "tokens": ["Die", "Bos\u00b7heit", "spr\u00fcht", "hier", "nicht", "ihr", "Nat\u00b7ter\u00b7gift"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf unbesorgter Unschuld Rosenkronen:", "tokens": ["Auf", "un\u00b7be\u00b7sorg\u00b7ter", "Un\u00b7schuld", "Ro\u00b7sen\u00b7kro\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gerechte Gleichheit teilt des Landmanns Trift,", "tokens": ["Ge\u00b7rech\u00b7te", "Gleich\u00b7heit", "teilt", "des", "Land\u00b7manns", "Trift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Freiheit herrscht, wo gute Menschen wohnen.", "tokens": ["Und", "Frei\u00b7heit", "herrscht", ",", "wo", "gu\u00b7te", "Men\u00b7schen", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "PWAV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Das Hohngezisch des Witzlers mengt sich nicht", "tokens": ["Das", "Hohn\u00b7ge\u00b7zisch", "des", "Witz\u00b7lers", "mengt", "sich", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PRF", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In dieser Espen friedes\u00e4uselnd Wehen:", "tokens": ["In", "die\u00b7ser", "Es\u00b7pen", "frie\u00b7de\u00b7s\u00e4u\u00b7selnd", "We\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein L\u00e4sterkreis h\u00e4lt hier sein Strafgericht;", "tokens": ["Kein", "L\u00e4s\u00b7ter\u00b7kreis", "h\u00e4lt", "hier", "sein", "Straf\u00b7ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kein Neider lau'rt, Gebrechen auszusp\u00e4hen.", "tokens": ["Kein", "Nei\u00b7der", "lau'rt", ",", "Ge\u00b7bre\u00b7chen", "aus\u00b7zu\u00b7sp\u00e4\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die Muse wallt auf zartbehalmtem Plan:", "tokens": ["Die", "Mu\u00b7se", "wallt", "auf", "zart\u00b7be\u00b7halm\u00b7tem", "Plan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie folgt dem Bach, der jene Fl\u00e4chen teilet,", "tokens": ["Sie", "folgt", "dem", "Bach", ",", "der", "je\u00b7ne", "Fl\u00e4\u00b7chen", "tei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und, gern verirrt auf sanftgewund'ner Bahn,", "tokens": ["Und", ",", "gern", "ver\u00b7irrt", "auf", "sanft\u00b7ge\u00b7wun\u00b7d'\u00b7ner", "Bahn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "So lang er kann, in diesem Tempe weilet.", "tokens": ["So", "lang", "er", "kann", ",", "in", "die\u00b7sem", "Tem\u00b7pe", "wei\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VMFIN", "$,", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Aus jener Dorfkapell', in Laub verh\u00fcllt,", "tokens": ["Aus", "je\u00b7ner", "Dorf\u00b7ka\u00b7pell'", ",", "in", "Laub", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Klang nie das Sturmgel\u00e4ut' in Schreckensn\u00e4chten,", "tokens": ["Klang", "nie", "das", "Sturm\u00b7ge\u00b7l\u00e4ut'", "in", "Schre\u00b7cken\u00b7sn\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn Aufruhr tobt, der tausendstimmig br\u00fcllt", "tokens": ["Wenn", "Auf\u00b7ruhr", "tobt", ",", "der", "tau\u00b7sends\u00b7tim\u00b7mig", "br\u00fcllt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Brand und Dolch in hochgeschwung'ner Rechten.", "tokens": ["Mit", "Brand", "und", "Dolch", "in", "hoch\u00b7ge\u00b7schwung'\u00b7ner", "Rech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Den Wiederhall der Eppichkl\u00fcfte schreckt", "tokens": ["Den", "Wie\u00b7der\u00b7hall", "der", "Ep\u00b7pich\u00b7kl\u00fcf\u00b7te", "schreckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kein Schlachtgescho\u00df; statt rauher Kriegstrommeten", "tokens": ["Kein", "Schlacht\u00b7ge\u00b7scho\u00df", ";", "statt", "rau\u00b7her", "Kriegs\u00b7trom\u00b7me\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hallt hier das Horn, das fr\u00fch die Hirtin weckt;", "tokens": ["Hallt", "hier", "das", "Horn", ",", "das", "fr\u00fch", "die", "Hir\u00b7tin", "weckt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Tag erlischt beim Ton der Weidenfl\u00f6ten.", "tokens": ["Der", "Tag", "er\u00b7lischt", "beim", "Ton", "der", "Wei\u00b7den\u00b7fl\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Hier muht die Kuh auf gelbbebl\u00fcmter Au',", "tokens": ["Hier", "muht", "die", "Kuh", "auf", "gelb\u00b7be\u00b7bl\u00fcm\u00b7ter", "Au'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dort klingeln hell der Ziegenherde Schellen;", "tokens": ["Dort", "klin\u00b7geln", "hell", "der", "Zie\u00b7gen\u00b7her\u00b7de", "Schel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das K\u00e4uzlein schnaubt im alten Ritterbau,", "tokens": ["Das", "K\u00e4uz\u00b7lein", "schnaubt", "im", "al\u00b7ten", "Rit\u00b7ter\u00b7bau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Bienen sumsen an des Gie\u00dfbachs F\u00e4llen.", "tokens": ["Und", "Bie\u00b7nen", "sum\u00b7sen", "an", "des", "Gie\u00df\u00b7bachs", "F\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Dort fl\u00fcstern Silberpappeln sanft umweht,", "tokens": ["Dort", "fl\u00fcs\u00b7tern", "Sil\u00b7ber\u00b7pap\u00b7peln", "sanft", "um\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die, gr\u00fcn und wei\u00df, die Bl\u00e4tter wechselnd regen;", "tokens": ["Die", ",", "gr\u00fcn", "und", "wei\u00df", ",", "die", "Bl\u00e4t\u00b7ter", "wech\u00b7selnd", "re\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "KON", "VVFIN", "$,", "ART", "NN", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das M\u00fchlenrad, das tr\u00e4g' die Schaufeln dreht,", "tokens": ["Das", "M\u00fch\u00b7len\u00b7rad", ",", "das", "tr\u00e4g'", "die", "Schau\u00b7feln", "dreht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Klappt langsam fort mit gleich geme\u00dfnen Schl\u00e4gen.", "tokens": ["Klappt", "lang\u00b7sam", "fort", "mit", "gleich", "ge\u00b7me\u00df\u00b7nen", "Schl\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Im Dickicht schallt der Drossel Waldgesang,", "tokens": ["Im", "Di\u00b7ckicht", "schallt", "der", "Dros\u00b7sel", "Wald\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Heupferd zirpt auf frischgem\u00e4hter Weide;", "tokens": ["Das", "Heu\u00b7pferd", "zirpt", "auf", "frischge\u00b7m\u00e4h\u00b7ter", "Wei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Am H\u00fcgel klirrt gewetzter Sensen Klang,", 