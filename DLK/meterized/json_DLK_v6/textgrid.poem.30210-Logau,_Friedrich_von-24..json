{"textgrid.poem.30210": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Glottus ist ein guter Redner; was er redet, thut er nicht;", "tokens": ["Glot\u00b7tus", "ist", "ein", "gu\u00b7ter", "Red\u00b7ner", ";", "was", "er", "re\u00b7det", ",", "thut", "er", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$.", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Dann er hat gar nimmer weile, da\u00df er th\u00e4te, was er spricht,", "tokens": ["Dann", "er", "hat", "gar", "nim\u00b7mer", "wei\u00b7le", ",", "da\u00df", "er", "th\u00e4\u00b7te", ",", "was", "er", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "ADV", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Hat ihm einen auffgenummen, der das Thun f\u00fcr ihn verricht.", "tokens": ["Hat", "ihm", "ei\u00b7nen", "auff\u00b7ge\u00b7num\u00b7men", ",", "der", "das", "Thun", "f\u00fcr", "ihn", "ver\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "$,", "PRELS", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}