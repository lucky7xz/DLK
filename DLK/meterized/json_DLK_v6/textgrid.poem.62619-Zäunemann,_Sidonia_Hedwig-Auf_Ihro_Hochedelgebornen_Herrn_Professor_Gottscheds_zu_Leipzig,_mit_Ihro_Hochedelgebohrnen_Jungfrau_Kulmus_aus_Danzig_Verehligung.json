{"textgrid.poem.62619": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf Ihro Hochedelgebornen Herrn Professor Gottscheds zu Leipzig, mit Ihro Hochedelgebohrnen Jungfrau Kulmus aus Danzig Verehligung", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nett, lebhaft, sch\u00f6n und feurig mu\u00df man schreiben,", "tokens": ["Nett", ",", "leb\u00b7haft", ",", "sch\u00f6n", "und", "feu\u00b7rig", "mu\u00df", "man", "schrei\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn man die\u00df Fest besingen will:", "tokens": ["Wenn", "man", "die\u00df", "Fest", "be\u00b7sin\u00b7gen", "will", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PDS", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum solte wohl mein Kiel zur\u00fccke bleiben;", "tokens": ["Drum", "sol\u00b7te", "wohl", "mein", "Kiel", "zu\u00b7r\u00fc\u00b7cke", "blei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ADV", "PPOSAT", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ja besser w\u00e4rs ich schwiege still;", "tokens": ["Ja", "bes\u00b7ser", "w\u00e4rs", "ich", "schwie\u00b7ge", "still", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "VAFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Allein die Lust, die Euer Band mir schencket,", "tokens": ["Al\u00b7lein", "die", "Lust", ",", "die", "Eu\u00b7er", "Band", "mir", "schen\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Macht, da\u00df ich jetzt nicht schweigen kan;", "tokens": ["Macht", ",", "da\u00df", "ich", "jetzt", "nicht", "schwei\u00b7gen", "kan", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Die Pflicht woran mein Herze stets gedenket,", "tokens": ["Die", "Pflicht", "wo\u00b7ran", "mein", "Her\u00b7ze", "stets", "ge\u00b7den\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWAV", "PPOSAT", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Reitzt mich noch mehr und st\u00e4rker an:", "tokens": ["Reitzt", "mich", "noch", "mehr", "und", "st\u00e4r\u00b7ker", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und also sing ich auch ohn wiederstreben", "tokens": ["Und", "al\u00b7so", "sing", "ich", "auch", "ohn", "wie\u00b7der\u00b7stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Von Amor, dem Ihr Euch: jetzt ganz ergeben.", "tokens": ["Von", "A\u00b7mor", ",", "dem", "Ihr", "Euch", ":", "jetzt", "ganz", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "PPER", "PPER", "$.", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Majest\u00e4t, der alle H\u00e4upter dienen,", "tokens": ["Die", "Ma\u00b7jes\u00b7t\u00e4t", ",", "der", "al\u00b7le", "H\u00e4up\u00b7ter", "die\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beschlosse schon eh etwas war,", "tokens": ["Be\u00b7schlos\u00b7se", "schon", "eh", "et\u00b7was", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KOUS", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Liebe solt auf dieser Erden gr\u00fcnen,", "tokens": ["Die", "Lie\u00b7be", "solt", "auf", "die\u00b7ser", "Er\u00b7den", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dauren bis ins letzte Jahr.", "tokens": ["Und", "dau\u00b7ren", "bis", "ins", "letz\u00b7te", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So lang der Nil Egyptens Felder w\u00e4ssert;", "tokens": ["So", "lang", "der", "Nil", "E\u00b7gyp\u00b7tens", "Fel\u00b7der", "w\u00e4s\u00b7sert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So lang der Po im Cirkel geht;", "tokens": ["So", "lang", "der", "Po", "im", "Cir\u00b7kel", "geht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So lang der Thau die welken Saaten bessert,", "tokens": ["So", "lang", "der", "Thau", "die", "wel\u00b7ken", "Saa\u00b7ten", "bes\u00b7sert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Und dem Verderben wiedersteht;", "tokens": ["Und", "dem", "Ver\u00b7der\u00b7ben", "wie\u00b7ders\u00b7teht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ja bis der Bau der Welt wird untergehen,", "tokens": ["Ja", "bis", "der", "Bau", "der", "Welt", "wird", "un\u00b7ter\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "So lange wird die Liebe auch bestehen.", "tokens": ["So", "lan\u00b7ge", "wird", "die", "Lie\u00b7be", "auch", "be\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Bey Donner, Blitz und Zittern, Furcht und Grauen", "tokens": ["Bey", "Don\u00b7ner", ",", "Blitz", "und", "Zit\u00b7tern", ",", "Furcht", "und", "Grau\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ward das Gebot der Heiligkeit,", "tokens": ["Ward", "das", "Ge\u00b7bot", "der", "Hei\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf einen Fel\u00df, in Steine eingehauen.", "tokens": ["Auf", "ei\u00b7nen", "Fel\u00df", ",", "in", "Stei\u00b7ne", "ein\u00b7ge\u00b7hau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hier zeiget sich der Unterscheid!", "tokens": ["Hier", "zei\u00b7get", "sich", "der", "Un\u00b7ter\u00b7scheid", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Liebe ward mit grossen Z\u00e4rtlichkeiten,", "tokens": ["Die", "Lie\u00b7be", "ward", "mit", "gros\u00b7sen", "Z\u00e4rt\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im Paradie\u00df, ins Herz gepr\u00e4gt.", "tokens": ["Im", "Pa\u00b7ra\u00b7die\u00df", ",", "ins", "Herz", "ge\u00b7pr\u00e4gt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Sch\u00f6pfer sucht uns hierdurch anzudeuten,", "tokens": ["Der", "Sch\u00f6p\u00b7fer", "sucht", "uns", "hier\u00b7durch", "an\u00b7zu\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PAV", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Seegen, den er drauf gelegt.", "tokens": ["Den", "See\u00b7gen", ",", "den", "er", "drauf", "ge\u00b7legt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und dieser Trieb zwingt Helden, Majest\u00e4ten,", "tokens": ["Und", "die\u00b7ser", "Trieb", "zwingt", "Hel\u00b7den", ",", "Ma\u00b7jes\u00b7t\u00e4\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der Weisen Schaar, ja selber die Propheten.", "tokens": ["Der", "Wei\u00b7sen", "Schaar", ",", "ja", "sel\u00b7ber", "die", "Pro\u00b7phe\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr Tempel ist wohl niemahls leer zu schauen,", "tokens": ["Ihr", "Tem\u00b7pel", "ist", "wohl", "nie\u00b7mahls", "leer", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil stets daselbst ein Opfer brennt.", "tokens": ["Weil", "stets", "da\u00b7selbst", "ein", "Op\u00b7fer", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie weis den Thron der K\u00f6nige zu bauen,", "tokens": ["Sie", "weis", "den", "Thron", "der", "K\u00f6\u00b7ni\u00b7ge", "zu", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So, da\u00df die Welt sie m\u00e4chtig nennt.", "tokens": ["So", ",", "da\u00df", "die", "Welt", "sie", "m\u00e4ch\u00b7tig", "nennt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Liebe wohnt in Lager und Gezelten,", "tokens": ["Die", "Lie\u00b7be", "wohnt", "in", "La\u00b7ger", "und", "Ge\u00b7zel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und l\u00e4\u00dft sich auch bey Fahnen sehn;", "tokens": ["Und", "l\u00e4\u00dft", "sich", "auch", "bey", "Fah\u00b7nen", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Schwerder Blitz kan gegen ihr nichts gelten,", "tokens": ["Der", "Schwer\u00b7der", "Blitz", "kan", "ge\u00b7gen", "ihr", "nichts", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "APPR", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und kein Tyrann vor ihr bestehn.", "tokens": ["Und", "kein", "Ty\u00b7rann", "vor", "ihr", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sie kan mit Recht der Inbegrif der Erden,", "tokens": ["Sie", "kan", "mit", "Recht", "der", "In\u00b7be\u00b7grif", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und Wunderwerk der Welt genennet werden.", "tokens": ["Und", "Wun\u00b7der\u00b7werk", "der", "Welt", "ge\u00b7nen\u00b7net", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ihr Wunderwerk, der abgelebten Zeiten,", "tokens": ["Ihr", "Wun\u00b7der\u00b7werk", ",", "der", "ab\u00b7ge\u00b7leb\u00b7ten", "Zei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr seyd vor ihr nicht nennens werth!", "tokens": ["Ihr", "seyd", "vor", "ihr", "nicht", "nen\u00b7nens", "werth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wo sieht man euch und eure Seltenheiten?", "tokens": ["Wo", "sieht", "man", "euch", "und", "eu\u00b7re", "Sel\u00b7ten\u00b7hei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht wahr; ihr seyd in Staub verkehrt?", "tokens": ["Nicht", "wahr", ";", "ihr", "seyd", "in", "Staub", "ver\u00b7kehrt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$.", "PPER", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Tempel steht, wenn eure Mauren brechen,", "tokens": ["Ihr", "Tem\u00b7pel", "steht", ",", "wenn", "eu\u00b7re", "Mau\u00b7ren", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Man schauet ihn best\u00e4ndig an.", "tokens": ["Man", "schau\u00b7et", "ihn", "be\u00b7st\u00e4n\u00b7dig", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Darius komm, la\u00df deine F\u00fcrsten sprechen,", "tokens": ["Da\u00b7ri\u00b7us", "komm", ",", "la\u00df", "dei\u00b7ne", "F\u00fcrs\u00b7ten", "spre\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVIMP", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und sage selbst was Liebe kan:", "tokens": ["Und", "sa\u00b7ge", "selbst", "was", "Lie\u00b7be", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PWS", "NN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ich weis, man wird aus euren Augen lesen,", "tokens": ["Ich", "weis", ",", "man", "wird", "aus", "eu\u00b7ren", "Au\u00b7gen", "le\u00b7sen", ","], "token_info": ["