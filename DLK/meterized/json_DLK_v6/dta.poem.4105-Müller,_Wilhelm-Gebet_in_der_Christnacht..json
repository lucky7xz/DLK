{"dta.poem.4105": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Gebet in der Christnacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Liebe, die den Tod bezwang", "tokens": ["O", "Lie\u00b7be", ",", "die", "den", "Tod", "be\u00b7zwang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr alle Menschenkinder,", "tokens": ["F\u00fcr", "al\u00b7le", "Men\u00b7schen\u00b7kin\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gedenk' in dieser sel'gen Nacht,", "tokens": ["Ge\u00b7denk'", "in", "die\u00b7ser", "sel'\u00b7gen", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die dich zu uns herabgebracht,", "tokens": ["Die", "dich", "zu", "uns", "her\u00b7ab\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Seelen, die dir fehlen!", "tokens": ["Der", "See\u00b7len", ",", "die", "dir", "feh\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O Liebe, die den Stern gesandt", "tokens": ["O", "Lie\u00b7be", ",", "die", "den", "Stern", "ge\u00b7sandt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinaus in's ferne Morgenland,", "tokens": ["Hin\u00b7aus", "in's", "fer\u00b7ne", "Mor\u00b7gen\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die K\u00f6nige zu rufen;", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7ge", "zu", "ru\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die laut durch ihres Boten Mund", "tokens": ["Die", "laut", "durch", "ih\u00b7res", "Bo\u00b7ten", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sich gab den armen Hirten kund,", "tokens": ["Sich", "gab", "den", "ar\u00b7men", "Hir\u00b7ten", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie bist du still geworden?", "tokens": ["Wie", "bist", "du", "still", "ge\u00b7wor\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Noch eine fromme Hirtin liegt", "tokens": ["Noch", "ei\u00b7ne", "from\u00b7me", "Hir\u00b7tin", "liegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In blinden Schlummer eingewiegt,", "tokens": ["In", "blin\u00b7den", "Schlum\u00b7mer", "ein\u00b7ge\u00b7wiegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und tr\u00e4umt von gr\u00fcnen B\u00e4umen.", "tokens": ["Und", "tr\u00e4umt", "von", "gr\u00fc\u00b7nen", "B\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Singt nicht vor ihrem Fensterlein", "tokens": ["Singt", "nicht", "vor", "ih\u00b7rem", "Fens\u00b7ter\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Engel: Esther, la\u00df mich ein,", "tokens": ["Ein", "En\u00b7gel", ":", "E\u00b7sther", ",", "la\u00df", "mich", "ein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "$,", "VVIMP", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Heiland ist geboren?", "tokens": ["Der", "Hei\u00b7land", "ist", "ge\u00b7bo\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}