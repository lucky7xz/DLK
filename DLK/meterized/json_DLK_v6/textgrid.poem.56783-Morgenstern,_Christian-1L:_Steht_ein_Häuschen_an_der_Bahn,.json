{"textgrid.poem.56783": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Steht ein H\u00e4uschen an der Bahn,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steht ein H\u00e4uschen an der Bahn,", "tokens": ["Steht", "ein", "H\u00e4usc\u00b7hen", "an", "der", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "hoch auf gr\u00fcnem H\u00fcgelplan.", "tokens": ["hoch", "auf", "gr\u00fc\u00b7nem", "H\u00fc\u00b7gel\u00b7plan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Tag und Nacht, in schnellem Flug,", "tokens": ["Tag", "und", "Nacht", ",", "in", "schnel\u00b7lem", "Flug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "braust vor\u00fcber Zug um Zug.", "tokens": ["braust", "vor\u00b7\u00fc\u00b7ber", "Zug", "um", "Zug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jedesmal bei dem Gebraus", "tokens": ["Je\u00b7des\u00b7mal", "bei", "dem", "Ge\u00b7braus"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "zittert leis das kleine Haus \u2013:", "tokens": ["zit\u00b7tert", "leis", "das", "klei\u00b7ne", "Haus", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbwen verl\u00e4\u00dft, wen sucht auf", "tokens": ["\u00bb", "wen", "ver\u00b7l\u00e4\u00dft", ",", "wen", "sucht", "auf"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PWS", "VVFIN", "$,", "PWS", "VVFIN", "APPR"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "euer nimmerm\u00fcder Lauf?\u00ab", "tokens": ["eu\u00b7er", "nim\u00b7mer\u00b7m\u00fc\u00b7der", "Lauf", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bboh nehmt mit, oh bestellt", "tokens": ["\u00bb", "oh", "nehmt", "mit", ",", "oh", "be\u00b7stellt"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADJD", "VVFIN", "PTKVZ", "$,", "ADJD", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gr\u00fc\u00dfe an die weite Welt!\u00ab", "tokens": ["Gr\u00fc\u00b7\u00dfe", "an", "die", "wei\u00b7te", "Welt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Rauch, Gestampf, Geroll, Geschrill ...", "tokens": ["Rauch", ",", "Ge\u00b7stampf", ",", "Ge\u00b7roll", ",", "Ge\u00b7schrill", "..."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles wieder totenstill.", "tokens": ["Al\u00b7les", "wie\u00b7der", "to\u00b7tens\u00b7till", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Tag und Nacht dr\u00f6hnt das Gleis.", "tokens": ["Tag", "und", "Nacht", "dr\u00f6hnt", "das", "Gleis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Einsam H\u00e4uschen zittert leis.", "tokens": ["Ein\u00b7sam", "H\u00e4usc\u00b7hen", "zit\u00b7tert", "leis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}