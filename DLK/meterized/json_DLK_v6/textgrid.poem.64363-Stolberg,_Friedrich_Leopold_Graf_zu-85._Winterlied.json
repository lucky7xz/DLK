{"textgrid.poem.64363": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "85. Winterlied", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da laurt er hinter", "tokens": ["Da", "laurt", "er", "hin\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Dem Boreas,", "tokens": ["Dem", "Bo\u00b7re\u00b7as", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der alte Winter,", "tokens": ["Der", "al\u00b7te", "Win\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und d\u00f6rrt das Gras;", "tokens": ["Und", "d\u00f6rrt", "das", "Gras", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Hat mir verhunzet", "tokens": ["Hat", "mir", "ver\u00b7hun\u00b7zet"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Des Gartens Zier,", "tokens": ["Des", "Gar\u00b7tens", "Zier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und knurrt und grunzet", "tokens": ["Und", "knurrt", "und", "grun\u00b7zet"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Vor meiner Th\u00fcr.", "tokens": ["Vor", "mei\u00b7ner", "Th\u00fcr."], "token_info": ["word", "word", "abbreviation"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Er steht und r\u00fcttelt", "tokens": ["Er", "steht", "und", "r\u00fct\u00b7telt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Bei Nacht und Tag", "tokens": ["Bei", "Nacht", "und", "Tag"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Am Baum, und sch\u00fcttelt", "tokens": ["Am", "Baum", ",", "und", "sch\u00fct\u00b7telt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die Eulen wach.", "tokens": ["Die", "Eu\u00b7len", "wach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Da grinzt und schnaubet", "tokens": ["Da", "grinzt", "und", "schnau\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Das Unget\u00fcm", "tokens": ["Das", "Un\u00b7ge\u00b7t\u00fcm"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Im Wald, und raubet", "tokens": ["Im", "Wald", ",", "und", "rau\u00b7bet"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Mit Ungest\u00fcm.", "tokens": ["Mit", "Un\u00b7ge\u00b7st\u00fcm", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Die F\u00fcchse gellen", "tokens": ["Die", "F\u00fcch\u00b7se", "gel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Vor seinem Zorn;", "tokens": ["Vor", "sei\u00b7nem", "Zorn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bis zu den Quellen", "tokens": ["Bis", "zu", "den", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Erstarrt der Vorn.", "tokens": ["Er\u00b7starrt", "der", "Vorn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Er treibt die Rinder", "tokens": ["Er", "treibt", "die", "Rin\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Mir in den Stall,", "tokens": ["Mir", "in", "den", "Stall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und kneipt die Kinder,", "tokens": ["Und", "kneipt", "die", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Wie R\u00fcbezahl.", "tokens": ["Wie", "R\u00fc\u00b7be\u00b7zahl", "."], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Er thut nicht fremde", "tokens": ["Er", "thut", "nicht", "frem\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Vor Mann und Frau;", "tokens": ["Vor", "Mann", "und", "Frau", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Im wei\u00dfen Hemde", "tokens": ["Im", "wei\u00b7\u00dfen", "Hem\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Steht er zur Schau:", "tokens": ["Steht", "er", "zur", "Schau", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Er steht, und rupfet", "tokens": ["Er", "steht", ",", "und", "rup\u00b7fet"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Am weichen Saum", "tokens": ["Am", "wei\u00b7chen", "Saum"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Der Wolk', und zupfet", "tokens": ["Der", "Wol\u00b7k'", ",", "und", "zup\u00b7fet"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Den wei\u00dfen Flaum;", "tokens": ["Den", "wei\u00b7\u00dfen", "Flaum", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Und bettet fr\u00fche", "tokens": ["Und", "bet\u00b7tet", "fr\u00fc\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wohl ohne Zelt,", "tokens": ["Wohl", "oh\u00b7ne", "Zelt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und sonder M\u00fche,", "tokens": ["Und", "son\u00b7der", "M\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Auf hartem Feld;", "tokens": ["Auf", "har\u00b7tem", "Feld", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Auch schnarcht und st\u00f6hnet", "tokens": ["Auch", "schnarcht", "und", "st\u00f6h\u00b7net"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Er manche Nacht", "tokens": ["Er", "man\u00b7che", "Nacht"], "token_info": ["word", "word", "word"], "pos": ["PPER", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Im See, und dehnet", "tokens": ["Im", "See", ",", "und", "deh\u00b7net"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Sich, da\u00df es kracht.", "tokens": ["Sich", ",", "da\u00df", "es", "kracht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Er \u00e4fft possierlich", "tokens": ["Er", "\u00e4fft", "pos\u00b7sier\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Dem Fr\u00fchling nach,", "tokens": ["Dem", "Fr\u00fch\u00b7ling", "nach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und kritzelt zierlich,", "tokens": ["Und", "krit\u00b7zelt", "zier\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Im Schlafgemach,", "tokens": ["Im", "Schlaf\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Vor meinem Fenster,", "tokens": ["Vor", "mei\u00b7nem", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "So Blum' als Wald;", "tokens": ["So", "Blum'", "als", "Wald", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Doch wie Gespenster", "tokens": ["Doch", "wie", "Ge\u00b7spens\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Zerrinnt es bald.", "tokens": ["Zer\u00b7rinnt", "es", "bald", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Auch an den B\u00e4umen", "tokens": ["Auch", "an", "den", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wei\u00df er gar fein", "tokens": ["Wei\u00df", "er", "gar", "fein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJD"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mit Reis zu s\u00e4umen", "tokens": ["Mit", "Reis", "zu", "s\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die Zweigelein.", "tokens": ["Die", "Zwei\u00b7ge\u00b7lein", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Er \u00fcbt am Himmel", "tokens": ["Er", "\u00fcbt", "am", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Auch seine Kunst;", "tokens": ["Auch", "sei\u00b7ne", "Kunst", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Malt Schlachtgewimmel", "tokens": ["Malt", "Schlacht\u00b7ge\u00b7wim\u00b7mel"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Aus hellem Dunst.", "tokens": ["Aus", "hel\u00b7lem", "Dunst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Und Schwerter zischen", "tokens": ["Und", "Schwer\u00b7ter", "zi\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "In heller Nacht;", "tokens": ["In", "hel\u00b7ler", "Nacht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und Riesen mischen", "tokens": ["Und", "Rie\u00b7sen", "mi\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sich in der Schlacht.", "tokens": ["Sich", "in", "der", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Der Landmann zittert", "tokens": ["Der", "Land\u00b7mann", "zit\u00b7tert"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Beim Ebentheu'r;", "tokens": ["Beim", "E\u00b7bentheu'r", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Der K\u00fcster wittert", "tokens": ["Der", "K\u00fcs\u00b7ter", "wit\u00b7tert"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Des Krieges Feu'r.", "tokens": ["Des", "Krie\u00b7ges", "Feu'", "r."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "NE", "NE"], "meter": "-+-+", "measure": "iambic.di"}}}}}