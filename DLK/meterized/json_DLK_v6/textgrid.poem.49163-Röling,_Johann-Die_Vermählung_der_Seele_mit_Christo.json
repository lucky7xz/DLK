{"textgrid.poem.49163": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Die Verm\u00e4hlung der Seele mit Christo", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm', liebe Seele, lass' die Welt,", "tokens": ["Komm'", ",", "lie\u00b7be", "See\u00b7le", ",", "lass'", "die", "Welt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schau, was in unsres Gottes Zelt", "tokens": ["Schau", ",", "was", "in", "uns\u00b7res", "Got\u00b7tes", "Zelt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "F\u00fcr eine Feier vor wird gehen;", "tokens": ["F\u00fcr", "ei\u00b7ne", "Fei\u00b7er", "vor", "wird", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schau an das grosse Hochzeitsfest,", "tokens": ["Schau", "an", "das", "gros\u00b7se", "Hoch\u00b7zeits\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das sich das Lamm bereiten l\u00e4\u00dft,", "tokens": ["Das", "sich", "das", "Lamm", "be\u00b7rei\u00b7ten", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Erd' und Himmel dienstbar stehen.", "tokens": ["Dem", "Erd'", "und", "Him\u00b7mel", "dienst\u00b7bar", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wer ist die auserw\u00e4hlte Braut,", "tokens": ["Wer", "ist", "die", "au\u00b7ser\u00b7w\u00e4hl\u00b7te", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die solchem F\u00fcrsten wird vertraut?", "tokens": ["Die", "sol\u00b7chem", "F\u00fcrs\u00b7ten", "wird", "ver\u00b7traut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dies sollst du selbst, o Seele, sein;", "tokens": ["Dies", "sollst", "du", "selbst", ",", "o", "See\u00b7le", ",", "sein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "$,", "FM", "NN", "$,", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Ehre, mehr als ungemein!", "tokens": ["O", "Eh\u00b7re", ",", "mehr", "als", "un\u00b7ge\u00b7mein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PIAT", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gedenk', wie hoch du wirst begl\u00fccket,", "tokens": ["Ge\u00b7denk'", ",", "wie", "hoch", "du", "wirst", "be\u00b7gl\u00fc\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df sich des H\u00f6chsten ein'ges Kind", "tokens": ["Da\u00df", "sich", "des", "H\u00f6chs\u00b7ten", "ein'\u00b7ges", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit dir geringer Magd verbindt.", "tokens": ["Mit", "dir", "ge\u00b7rin\u00b7ger", "Magd", "ver\u00b7bindt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie aber wirst du sein geschm\u00fccket,", "tokens": ["Wie", "a\u00b7ber", "wirst", "du", "sein", "ge\u00b7schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df du f\u00fcr solchen Liebsten recht?", "tokens": ["Da\u00df", "du", "f\u00fcr", "sol\u00b7chen", "Liebs\u00b7ten", "recht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du selbst f\u00fcr dich bist arm und schlecht.", "tokens": ["Du", "selbst", "f\u00fcr", "dich", "bist", "arm", "und", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Bitt' deinen Br\u00e4utgam um das Kleid", "tokens": ["Bitt'", "dei\u00b7nen", "Br\u00e4ut\u00b7gam", "um", "das", "Kleid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der herrlichen Gerechtigkeit,", "tokens": ["Der", "herr\u00b7li\u00b7chen", "Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das er dir durch den Tod erworben;", "tokens": ["Das", "er", "dir", "durch", "den", "Tod", "er\u00b7wor\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der blut'ge Rock, in welchem er", "tokens": ["Der", "blut'\u00b7ge", "Rock", ",", "in", "wel\u00b7chem", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit grosser Marter und Beschwer", "tokens": ["Mit", "gros\u00b7ser", "Mar\u00b7ter", "und", "Be\u00b7schwer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am Kreuz gelitten und gestorben,", "tokens": ["Am", "Kreuz", "ge\u00b7lit\u00b7ten", "und", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der ist die sch\u00f6ne Seid' allein,", "tokens": ["Der", "ist", "die", "sch\u00f6\u00b7ne", "Seid'", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In der du seiner werth kannst sein.", "tokens": ["In", "der", "du", "sei\u00b7ner", "werth", "kannst", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Den lege dir im Glauben bei", "tokens": ["Den", "le\u00b7ge", "dir", "im", "Glau\u00b7ben", "bei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPRART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sei so aller Sorgen frei;", "tokens": ["Und", "sei", "so", "al\u00b7ler", "Sor\u00b7gen", "frei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hierin wird er, dein Schatz, dich k\u00fcssen", "tokens": ["Hie\u00b7rin", "wird", "er", ",", "dein", "Schatz", ",", "dich", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bei dem Mahl der Ewigkeit", "tokens": ["Und", "bei", "dem", "Mahl", "der", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dich freundlich setzen an die Seit',", "tokens": ["Dich", "freund\u00b7lich", "set\u00b7zen", "an", "die", "Seit'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Himmelsfreude zu geniessen.", "tokens": ["Der", "Him\u00b7mels\u00b7freu\u00b7de", "zu", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "O seel'ge Lust, erw\u00fcnschter Tag,", "tokens": ["O", "seel'\u00b7ge", "Lust", ",", "er\u00b7w\u00fcnschter", "Tag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Den hier kein Herz begreifen mag!", "tokens": ["Den", "hier", "kein", "Herz", "be\u00b7grei\u00b7fen", "mag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach liebster Jesu, Gottes Lamm,", "tokens": ["Ach", "liebs\u00b7ter", "Je\u00b7su", ",", "Got\u00b7tes", "Lamm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NE", "$,", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Bruder, Heil und Br\u00e4utigam,", "tokens": ["Mein", "Bru\u00b7der", ",", "Heil", "und", "Br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du giebst mir hier dein Fleisch zu essen", "tokens": ["Du", "giebst", "mir", "hier", "dein", "Fleisch", "zu", "es\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und tr\u00e4nkest mich mit deinem Blut;", "tokens": ["Und", "tr\u00e4n\u00b7kest", "mich", "mit", "dei\u00b7nem", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dies heil'ge Mahl komm' mir zu gut,", "tokens": ["Dies", "heil'\u00b7ge", "Mahl", "komm'", "mir", "zu", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "VVFIN", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df, da ich hier dich selbst gegessen,", "tokens": ["Da\u00df", ",", "da", "ich", "hier", "dich", "selbst", "ge\u00b7ges\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich dort auch, theurer Wirth, mag dein", "tokens": ["Ich", "dort", "auch", ",", "theu\u00b7rer", "Wirth", ",", "mag", "dein"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "ADV", "$,", "ADJD", "NN", "$,", "VMFIN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und deiner Tafel w\u00fcrdig sein!", "tokens": ["Und", "dei\u00b7ner", "Ta\u00b7fel", "w\u00fcr\u00b7dig", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}