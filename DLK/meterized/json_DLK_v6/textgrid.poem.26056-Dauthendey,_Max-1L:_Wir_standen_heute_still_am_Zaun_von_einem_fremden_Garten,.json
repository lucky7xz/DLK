{"textgrid.poem.26056": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir standen heute still am Zaun von einem fremden Garten,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir standen heute still am Zaun von einem fremden Garten,", "tokens": ["Wir", "stan\u00b7den", "heu\u00b7te", "still", "am", "Zaun", "von", "ei\u00b7nem", "frem\u00b7den", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Sah'n hin und sah'n das Wintergras am Teich auf Sonne warten.", "tokens": ["Sah'n", "hin", "und", "sah'n", "das", "Win\u00b7ter\u00b7gras", "am", "Teich", "auf", "Son\u00b7ne", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "APPRART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Im Wasser lag verj\u00e4hrtes Laub gleichwie auf Glas,", "tokens": ["Im", "Was\u00b7ser", "lag", "ver\u00b7j\u00e4hr\u00b7tes", "Laub", "gleich\u00b7wie", "auf", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJA", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Am Ufer sa\u00df ein B\u00fcschel Veilchen jung erbl\u00fcht im gelben Gras,", "tokens": ["Am", "U\u00b7fer", "sa\u00df", "ein", "B\u00fc\u00b7schel", "Veil\u00b7chen", "jung", "er\u00b7bl\u00fcht", "im", "gel\u00b7ben", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "ADJD", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.5": {"text": "Und frisches Lilienkraut wuchs gr\u00fcn bei Tuffsteinbl\u00f6cken,", "tokens": ["Und", "fri\u00b7sches", "Li\u00b7li\u00b7en\u00b7kraut", "wuchs", "gr\u00fcn", "bei", "Tuff\u00b7stein\u00b7bl\u00f6\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Am Himmel oben gingen Wolken jugendlich in wei\u00dfen R\u00f6cken.", "tokens": ["Am", "Him\u00b7mel", "o\u00b7ben", "gin\u00b7gen", "Wol\u00b7ken", "ju\u00b7gend\u00b7lich", "in", "wei\u00b7\u00dfen", "R\u00f6\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "NN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.7": {"text": "Wie wenig Welt tut schon den Augen gut!", "tokens": ["Wie", "we\u00b7nig", "Welt", "tut", "schon", "den", "Au\u00b7gen", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nur ein paar Atemz\u00fcge lang hat's Herz dort ausgeruht,", "tokens": ["Nur", "ein", "paar", "A\u00b7tem\u00b7z\u00fc\u00b7ge", "lang", "hat's", "Herz", "dort", "aus\u00b7ge\u00b7ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PIAT", "NN", "ADJD", "VAFIN", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "Nur ein paar Augenblicke tat es s\u00e4umen ...", "tokens": ["Nur", "ein", "paar", "Au\u00b7gen\u00b7bli\u00b7cke", "tat", "es", "s\u00e4u\u00b7men", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PIAT", "NN", "VVFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wir sind doch alle in den weiten Lebensr\u00e4umen", "tokens": ["Wir", "sind", "doch", "al\u00b7le", "in", "den", "wei\u00b7ten", "Le\u00b7bens\u00b7r\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Zaung\u00e4ste nur bei W\u00fcnschen und bei Tr\u00e4umen.", "tokens": ["Zaun\u00b7g\u00e4s\u00b7te", "nur", "bei", "W\u00fcn\u00b7schen", "und", "bei", "Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}