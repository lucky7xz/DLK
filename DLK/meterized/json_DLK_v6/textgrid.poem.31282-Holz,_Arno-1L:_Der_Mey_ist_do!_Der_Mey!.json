{"textgrid.poem.31282": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Mey ist do! Der Mey!", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mey ist do! Der Mey!", "tokens": ["Der", "Mey", "ist", "do", "!", "Der", "Mey", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADV", "$.", "ART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "O s\u00fcsser Jubel-Schrey!", "tokens": ["O", "s\u00fcs\u00b7ser", "Ju\u00b7bel\u00b7Schrey", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Himmel h\u00e4ngkt so tieff al\u00df blau/", "tokens": ["Der", "Him\u00b7mel", "h\u00e4ngkt", "so", "tieff", "al\u00df", "blau", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "KOKOM", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "die Welt ist wie au\u00df Morgen-Thau.", "tokens": ["die", "Welt", "ist", "wie", "au\u00df", "Mor\u00b7gen\u00b7\u00b7T\u00b7hau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "APPR", "NE", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "mit Rohsen gantz behangen/", "tokens": ["mit", "Roh\u00b7sen", "gantz", "be\u00b7han\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "der West l\u00e4sst ohngesehn", "tokens": ["der", "West", "l\u00e4sst", "ohn\u00b7ge\u00b7sehn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "bloh\u00df Amber-L\u00fcfftgens wehn!", "tokens": ["bloh\u00df", "Am\u00b7ber\u00b7L\u00fcfft\u00b7gens", "wehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Von den H\u00fchten", "tokens": ["Von", "den", "H\u00fch\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "nikken Bl\u00fchten/", "tokens": ["nik\u00b7ken", "Bl\u00fch\u00b7ten", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "alles jubelt/ dantzt und springt/", "tokens": ["al\u00b7les", "ju\u00b7belt", "/", "dantzt", "und", "springt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "ADV", "KON", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "seit im Walde/", "tokens": ["seit", "im", "Wal\u00b7de", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "nach der Halde/", "tokens": ["nach", "der", "Hal\u00b7de", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "wihder\u00fcmb der Gukguk singt!", "tokens": ["wih\u00b7de\u00b7r\u00fcmb", "der", "Guk\u00b7guk", "singt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Mey ist do! Der Mey!", "tokens": ["Der", "Mey", "ist", "do", "!", "Der", "Mey", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADV", "$.", "ART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nur ich b\u00fcn nicht darbey!", "tokens": ["Nur", "ich", "b\u00fcn", "nicht", "dar\u00b7bey", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PTKNEG", "PAV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Kein Qwintgen sp\u00fchr ich mehr an Krafft/", "tokens": ["Kein", "Qwint\u00b7gen", "sp\u00fchr", "ich", "mehr", "an", "Krafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ich lige welck und Lager-hafft!", "tokens": ["ich", "li\u00b7ge", "welck", "und", "La\u00b7ger\u00b7hafft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Bald werd ich hingerissen/", "tokens": ["Bald", "werd", "ich", "hin\u00b7ge\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "kein M\u00e4ntsch wird von mir wissen/", "tokens": ["kein", "M\u00e4ntsch", "wird", "von", "mir", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "ein Schatten war ich und ein Schaum/", "tokens": ["ein", "Schat\u00b7ten", "war", "ich", "und", "ein", "Schaum", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "kaum mehr al\u00df jener Apffel-Baum!", "tokens": ["kaum", "mehr", "al\u00df", "je\u00b7ner", "Apf\u00b7fel\u00b7Baum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "KOKOM", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Durch mein Fenster", "tokens": ["Durch", "mein", "Fens\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "blinckt und glentzt er/", "tokens": ["blinckt", "und", "glentzt", "er", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "eine Mutter hertzt ihr Kind \u2013", "tokens": ["ei\u00b7ne", "Mut\u00b7ter", "hertzt", "ihr", "Kind", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "ich vergehe/", "tokens": ["ich", "ver\u00b7ge\u00b7he", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "da ich sehe/", "tokens": ["da", "ich", "se\u00b7he", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "wie die Beyde fr\u00f6lig sind!", "tokens": ["wie", "die", "Bey\u00b7de", "fr\u00f6\u00b7lig", "sind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}