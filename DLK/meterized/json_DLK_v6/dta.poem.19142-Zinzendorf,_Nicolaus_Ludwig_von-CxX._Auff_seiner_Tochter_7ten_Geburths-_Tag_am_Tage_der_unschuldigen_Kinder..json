{"dta.poem.19142": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "CxX.  Auff seiner Tochter 7ten Geburths-  \n Tag am Tage der unschuldigen  \n Kinder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Du von der Gnad erregte", "tokens": ["Du", "von", "der", "Gnad", "er\u00b7reg\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Macht bewegte", "tokens": ["Mit", "Macht", "be\u00b7weg\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und in den Grund gelegte", "tokens": ["Und", "in", "den", "Grund", "ge\u00b7leg\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Je mehr und mehr;", "tokens": ["Je", "mehr", "und", "mehr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Du bey der kleinen M\u00e4gde", "tokens": ["Du", "bey", "der", "klei\u00b7nen", "M\u00e4g\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Begl\u00fccktem Heer", "tokens": ["Be\u00b7gl\u00fcck\u00b7tem", "Heer"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Aus Gnaden mit gehegte", "tokens": ["Aus", "Gna\u00b7den", "mit", "ge\u00b7heg\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bi\u00dfher gepflegte", "tokens": ["Bi\u00df\u00b7her", "ge\u00b7pfleg\u00b7te"], "token_info": ["word", "word"], "pos": ["ADV", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Und Christo eingepr\u00e4gte", "tokens": ["Und", "Chris\u00b7to", "ein\u00b7ge\u00b7pr\u00e4g\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "NE", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Benigna h\u00f6r!", "tokens": ["Be\u00b7nig\u00b7na", "h\u00f6r", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Der abgejagten H\u00fcndlein", "tokens": ["Der", "ab\u00b7ge\u00b7jag\u00b7ten", "H\u00fcnd\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum W\u00fcrge-St\u00fcndlein", "tokens": ["Zum", "W\u00fcr\u00b7ge\u00b7St\u00fcnd\u00b7lein"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Mit Blut-verstellte M\u00fcndlein", "tokens": ["Mit", "Blut\u00b7ver\u00b7stell\u00b7te", "M\u00fcnd\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schreyn Himmel an,", "tokens": ["Schreyn", "Him\u00b7mel", "an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ein jedes hat sein Pf\u00fcndlein", "tokens": ["Ein", "je\u00b7des", "hat", "sein", "Pf\u00fcnd\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "VAFIN", "PPOSAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wohl ausgethan.", "tokens": ["Wohl", "aus\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Jhr, ihr ins Lebens-B\u00fcndlein", "tokens": ["Ihr", ",", "ihr", "ins", "Le\u00b7bens\u00b7B\u00fcnd\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mehr als in Windlein", "tokens": ["Mehr", "als", "in", "Wind\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "KOKOM", "APPR", "NN"], "meter": "++-+-", "measure": "iambic.di"}, "line.9": {"text": "Hinein geh\u00fcllte Kindlein,", "tokens": ["Hin\u00b7ein", "ge\u00b7h\u00fcll\u00b7te", "Kin\u00b7dlein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Macht JEsu Bahn!", "tokens": ["Macht", "Je\u00b7su", "Bahn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Indem nun wir vom Bunde", "tokens": ["In\u00b7dem", "nun", "wir", "vom", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus Hertzens Grunde", "tokens": ["Aus", "Hert\u00b7zens", "Grun\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Mit Lob-erf\u00fclltem Munde", "tokens": ["Mit", "Lob\u00b7er\u00b7f\u00fcll\u00b7tem", "Mun\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Di\u00df Heer erh\u00f6hn,", "tokens": ["Di\u00df", "Heer", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und gleichsam ihre Wunde", "tokens": ["Und", "gleich\u00b7sam", "ih\u00b7re", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Noch vor uns sehn,", "tokens": ["Noch", "vor", "uns", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Rufft zu derselben Stunde", "tokens": ["Rufft", "zu", "der\u00b7sel\u00b7ben", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der HErr der Pfunde", "tokens": ["Der", "Herr", "der", "Pfun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Dir auf dem Erden-Runde", "tokens": ["Dir", "auf", "dem", "Er\u00b7den\u00b7Run\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Herf\u00fcr zu gehn.", "tokens": ["Her\u00b7f\u00fcr", "zu", "gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Gleich ist er mit Verlangen", "tokens": ["Gleich", "ist", "er", "mit", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dir nachgegangen", "tokens": ["Dir", "nach\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word"], "pos": ["PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und hat mit Siege Prangen", "tokens": ["Und", "hat", "mit", "Sie\u00b7ge", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu deinem Gl\u00fcck", "tokens": ["Zu", "dei\u00b7nem", "Gl\u00fcck"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dein zartes Hertz gefangen", "tokens": ["Dein", "zar\u00b7tes", "Hertz", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Augenblick,", "tokens": ["Den", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Da dich die Welt empfangen", "tokens": ["Da", "dich", "die", "Welt", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und alle Schlangen,", "tokens": ["Und", "al\u00b7le", "Schlan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Die sich an dich gehangen,", "tokens": ["Die", "sich", "an", "dich", "ge\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Trieb er zur\u00fcck.", "tokens": ["Trieb", "er", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}