{"dta.poem.10437": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Lx.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Es f\u00e4llt ein Stern herunter", "tokens": ["Es", "f\u00e4llt", "ein", "Stern", "her\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APZR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus seiner funkelnden H\u00f6h';", "tokens": ["Aus", "sei\u00b7ner", "fun\u00b7keln\u00b7den", "H\u00f6h'", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das ist der Stern der Liebe,", "tokens": ["Das", "ist", "der", "Stern", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den ich dort fallen seh'.", "tokens": ["Den", "ich", "dort", "fal\u00b7len", "seh'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es fallen vom Apfelbaume", "tokens": ["Es", "fal\u00b7len", "vom", "Ap\u00b7fel\u00b7bau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der wei\u00dfen Bl\u00e4tter viel;", "tokens": ["Der", "wei\u00b7\u00dfen", "Bl\u00e4t\u00b7ter", "viel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es kommen die neckenden L\u00fcfte,", "tokens": ["Es", "kom\u00b7men", "die", "ne\u00b7cken\u00b7den", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Und treiben damit ihr Spiel.", "tokens": ["Und", "trei\u00b7ben", "da\u00b7mit", "ihr", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Es singt der Schwan im Weiher,", "tokens": ["Es", "singt", "der", "Schwan", "im", "Wei\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und rudert auf und ab,", "tokens": ["Und", "ru\u00b7dert", "auf", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und immer leiser singend,", "tokens": ["Und", "im\u00b7mer", "lei\u00b7ser", "sin\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Taucht er in's Fluthengrab.", "tokens": ["Taucht", "er", "in's", "Flut\u00b7hen\u00b7grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Es ist so still und so dunkel!", "tokens": ["Es", "ist", "so", "still", "und", "so", "dun\u00b7kel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Verweht ist Blatt und Bl\u00fcth',", "tokens": ["Ver\u00b7weht", "ist", "Blatt", "und", "Bl\u00fcth'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Stern ist knisternd zerstoben,", "tokens": ["Der", "Stern", "ist", "knis\u00b7ternd", "zer\u00b7sto\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Verklungen das Schwanenlied.", "tokens": ["Ver\u00b7klun\u00b7gen", "das", "Schwa\u00b7nen\u00b7lied", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}