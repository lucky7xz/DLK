{"textgrid.poem.57855": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Die Entf\u00fchrung", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf der Masch bin ich geboren", "tokens": ["Auf", "der", "Masch", "bin", "ich", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu Celle, der wundersch\u00f6nen Stadt;", "tokens": ["Zu", "Cel\u00b7le", ",", "der", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Stadt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Vom Himmel bin ich gefallen,", "tokens": ["Vom", "Him\u00b7mel", "bin", "ich", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Einen Vater ich niemals hatt'.", "tokens": ["Ei\u00b7nen", "Va\u00b7ter", "ich", "nie\u00b7mals", "hatt'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Was brauche ich denn einen Vater,", "tokens": ["Was", "brau\u00b7che", "ich", "denn", "ei\u00b7nen", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hab' ich eine liebe Mutter nur;", "tokens": ["Hab'", "ich", "ei\u00b7ne", "lie\u00b7be", "Mut\u00b7ter", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo sie blieb, das wei\u00df der Himmel,", "tokens": ["Wo", "sie", "blieb", ",", "das", "wei\u00df", "der", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn sie kam auf die Bettelfuhr'.", "tokens": ["Denn", "sie", "kam", "auf", "die", "Bet\u00b7tel\u00b7fuhr'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was brauche ich denn eine Mutter,", "tokens": ["Was", "brau\u00b7che", "ich", "denn", "ei\u00b7ne", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist mein lieber Schatz mir hold und treu;", "tokens": ["Ist", "mein", "lie\u00b7ber", "Schatz", "mir", "hold", "und", "treu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo er blieb, das wei\u00df der Himmel", "tokens": ["Wo", "er", "blieb", ",", "das", "wei\u00df", "der", "Him\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die hohe Polizei.", "tokens": ["Und", "die", "ho\u00b7he", "Po\u00b7li\u00b7zei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und ich suche hin und wieder,", "tokens": ["Und", "ich", "su\u00b7che", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "KON", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Such' ihn dort und such' ihn hier;", "tokens": ["Such'", "ihn", "dort", "und", "such'", "ihn", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich werde ihn nicht finden,", "tokens": ["Und", "ich", "wer\u00b7de", "ihn", "nicht", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn er sitzt in Himmelst\u00fcr.", "tokens": ["Denn", "er", "sitzt", "in", "Him\u00b7mel\u00b7st\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Himmelst\u00fcr hat feste Mauern", "tokens": ["Him\u00b7mel\u00b7st\u00fcr", "hat", "fes\u00b7te", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es hat ein Eisentor;", "tokens": ["Und", "es", "hat", "ein", "Ei\u00b7sen\u00b7tor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich stehe da und weine,", "tokens": ["Und", "ich", "ste\u00b7he", "da", "und", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil ich meinen Schatz verlor.", "tokens": ["Weil", "ich", "mei\u00b7nen", "Schatz", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und die Nacht ist k\u00fchl und dunkel", "tokens": ["Und", "die", "Nacht", "ist", "k\u00fchl", "und", "dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Und mein Schatz wei\u00df Hausgelegenheit;", "tokens": ["Und", "mein", "Schatz", "wei\u00df", "Haus\u00b7ge\u00b7le\u00b7gen\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Morgen fr\u00fch, wenn sie uns suchen,", "tokens": ["Mor\u00b7gen", "fr\u00fch", ",", "wenn", "sie", "uns", "su\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind wir l\u00e4ngst, wer wei\u00df wie weit.", "tokens": ["Sind", "wir", "l\u00e4ngst", ",", "wer", "wei\u00df", "wie", "weit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "PWS", "VVFIN", "KOKOM", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}