{"dta.poem.5847": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mechanismus hat, (sind sie gleich nicht zu z\u00e4hlen,)", "tokens": ["Der", "Me\u00b7cha\u00b7nis\u00b7mus", "hat", ",", "(", "sind", "sie", "gleich", "nicht", "zu", "z\u00e4h\u00b7len", ",", ")"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "$(", "VAFIN", "PPER", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr alle einerley Gesetze, die nicht fehlen", "tokens": ["F\u00fcr", "al\u00b7le", "ei\u00b7ner\u00b7ley", "Ge\u00b7set\u00b7ze", ",", "die", "nicht", "feh\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "PIAT", "NN", "$,", "PRELS", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die DER, so alle Ding aufs kr\u00e4fftigste beweget,", "tokens": ["Die", "DeR", ",", "so", "al\u00b7le", "Ding", "aufs", "kr\u00e4ff\u00b7tigs\u00b7te", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PIAT", "NN", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In sch\u00f6nster Harmonie darein geleget.", "tokens": ["In", "sch\u00f6ns\u00b7ter", "Har\u00b7mo\u00b7nie", "da\u00b7rein", "ge\u00b7le\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es m\u00fcssen sich die Dinge, die so klein,", "tokens": ["Es", "m\u00fcs\u00b7sen", "sich", "die", "Din\u00b7ge", ",", "die", "so", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df sie nicht sicht-nicht f\u00fchlbar seyn,", "tokens": ["Da\u00df", "sie", "nicht", "sicht\u00b7nicht", "f\u00fchl\u00b7bar", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Durch eben solche R\u00e4der regen,", "tokens": ["Durch", "e\u00b7ben", "sol\u00b7che", "R\u00e4\u00b7der", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Durch welche sich die Gr\u00f6ssesten bewegen.", "tokens": ["Durch", "wel\u00b7che", "sich", "die", "Gr\u00f6s\u00b7ses\u00b7ten", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}