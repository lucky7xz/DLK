{"textgrid.poem.64278": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "R\u00fcckblick", "genre": "verse", "period": "N.A.", "pub_year": 1881, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit zwanzig Jahren steht nun unser Reich,", "tokens": ["Seit", "zwan\u00b7zig", "Jah\u00b7ren", "steht", "nun", "un\u00b7ser", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wohl geziemt's, den Blick darauf zu lenken,", "tokens": ["Und", "wohl", "ge\u00b7ziemt's", ",", "den", "Blick", "da\u00b7rauf", "zu", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "$,", "ART", "NN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie es geworden ist und wie es ist! \u2013", "tokens": ["Wie", "es", "ge\u00b7wor\u00b7den", "ist", "und", "wie", "es", "ist", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "VAPP", "VAFIN", "KON", "PWAV", "PPER", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was wi\u00dft ihr Knaben, selbst ihr J\u00fcnglinge,", "tokens": ["Was", "wi\u00dft", "ihr", "Kna\u00b7ben", ",", "selbst", "ihr", "J\u00fcng\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Von all' dem Gram, dem Zorn, dem wilden Weh,", "tokens": ["Von", "all'", "dem", "Gram", ",", "dem", "Zorn", ",", "dem", "wil\u00b7den", "Weh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der immer wieder aufgelebten Hoffnung", "tokens": ["Der", "im\u00b7mer", "wie\u00b7der", "auf\u00b7ge\u00b7leb\u00b7ten", "Hoff\u00b7nung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ach! dem stets erneuten Hoffnungstod,", "tokens": ["Und", "ach", "!", "dem", "stets", "er\u00b7neu\u00b7ten", "Hoff\u00b7nungs\u00b7tod", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von der Verzweiflung, welche wechselnd uns,", "tokens": ["Von", "der", "Ver\u00b7zwei\u00b7flung", ",", "wel\u00b7che", "wech\u00b7selnd", "uns", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das \u00e4ltere Geschlecht, jahrzehntelang", "tokens": ["Das", "\u00e4l\u00b7te\u00b7re", "Ge\u00b7schlecht", ",", "jahr\u00b7zehn\u00b7te\u00b7lang"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gequ\u00e4lt, genarrt, emp\u00f6rt und matt gehetzt!", "tokens": ["Ge\u00b7qu\u00e4lt", ",", "ge\u00b7narrt", ",", "em\u00b7p\u00f6rt", "und", "matt", "ge\u00b7hetzt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$,", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich denke jenes sonn'gen Februars,", "tokens": ["Ich", "den\u00b7ke", "je\u00b7nes", "sonn'\u00b7gen", "Feb\u00b7ru\u00b7ars", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da \u00fcber'n Rhein her so verhei\u00dfungsvoll", "tokens": ["Da", "\u00fc\u00b7ber'n", "Rhein", "her", "so", "ver\u00b7hei\u00b7\u00dfungs\u00b7voll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "APZR", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der V\u00f6lkerfr\u00fchling zu den Deutschen zog:", "tokens": ["Der", "V\u00f6l\u00b7ker\u00b7fr\u00fch\u00b7ling", "zu", "den", "Deut\u00b7schen", "zog", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O welche Wonne, welcher Jugendschwung!", "tokens": ["O", "wel\u00b7che", "Won\u00b7ne", ",", "wel\u00b7cher", "Ju\u00b7gend\u00b7schwung", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PWAT", "NN", "$,", "PWAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da schien kein Ziel zu hoch, zu k\u00fchn kein Wunsch:", "tokens": ["Da", "schien", "kein", "Ziel", "zu", "hoch", ",", "zu", "k\u00fchn", "kein", "Wunsch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wirklich: \u00fcber Torheit, Kampf und Wirrsal", "tokens": ["Und", "wirk\u00b7lich", ":", "\u00fc\u00b7ber", "Tor\u00b7heit", ",", "Kampf", "und", "Wirr\u00b7sal"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$.", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Stieg tr\u00f6stend auf das alte deutsche Traumbild", "tokens": ["Stieg", "tr\u00f6s\u00b7tend", "auf", "das", "al\u00b7te", "deut\u00b7sche", "Traum\u00b7bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom Kaiser und vom Reich! \u2013 \u2013 Es ", "tokens": ["Vom", "Kai\u00b7ser", "und", "vom", "Reich", "!", "\u2013", "\u2013", "Es"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "word"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$.", "$(", "$(", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Mann, der ihn erf\u00fcllen sollte, ach!", "tokens": ["Der", "Mann", ",", "der", "ihn", "er\u00b7f\u00fcl\u00b7len", "soll\u00b7te", ",", "ach", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,", "ITJ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der tr\u00e4umte ", "tokens": ["Der", "tr\u00e4um\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Der Kaiser aber fehlte! \u2013 Und der D\u00e4ne", "tokens": ["Der", "Kai\u00b7ser", "a\u00b7ber", "fehl\u00b7te", "!", "\u2013", "Und", "der", "D\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "$.", "$(", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ri\u00df Schleswig-Holstein in sein Joch zur\u00fcck", "tokens": ["Ri\u00df", "Schles\u00b7wig\u00b7Hol\u00b7stein", "in", "sein", "Joch", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und als Piratenflagge drohte England", "tokens": ["Und", "als", "Pi\u00b7ra\u00b7ten\u00b7flag\u00b7ge", "droh\u00b7te", "En\u00b7gland"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der deutschen Flotte Flagge zu verfolgen,", "tokens": ["Der", "deut\u00b7schen", "Flot\u00b7te", "Flag\u00b7ge", "zu", "ver\u00b7fol\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und hilflos schien der Zwietracht, Schmach und Ohnmacht", "tokens": ["Und", "hil\u00b7flos", "schien", "der", "Zwiet\u00b7racht", ",", "Schmach", "und", "Ohn\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "F\u00fcr immerdar verfallen unser Volk! \u2013 \u2013", "tokens": ["F\u00fcr", "im\u00b7mer\u00b7dar", "ver\u00b7fal\u00b7len", "un\u00b7ser", "Volk", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Lang war der dumpfe Schlaf, der Todesschlaf:", "tokens": ["Lang", "war", "der", "dump\u00b7fe", "Schlaf", ",", "der", "To\u00b7des\u00b7schlaf", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da horch! Was klingt so hell her von der ", "tokens": ["Da", "horch", "!", "Was", "klingt", "so", "hell", "her", "von", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$.", "PWS", "VVFIN", "ADV", "ADJD", "ADV", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So kriegerisch, ein Weckeruf von Erz?", "tokens": ["So", "krie\u00b7ge\u00b7risch", ",", "ein", "We\u00b7cke\u00b7ruf", "von", "Erz", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das ist der Klang der preu\u00dfischen Trompete!", "tokens": ["Das", "ist", "der", "Klang", "der", "preu\u00b7\u00dfi\u00b7schen", "Trom\u00b7pe\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Bei Gott! Ein Weckruf f\u00fcr das deutsche Volk!", "tokens": ["Bei", "Gott", "!", "Ein", "Weck\u00b7ruf", "f\u00fcr", "das", "deut\u00b7sche", "Volk", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und einen neuen Akt der Weltgeschichte", "tokens": ["Und", "ei\u00b7nen", "neu\u00b7en", "Akt", "der", "Welt\u00b7ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Verk\u00fcndet er dem staunenden Europa! \u2013", "tokens": ["Ver\u00b7k\u00fcn\u00b7det", "er", "dem", "stau\u00b7nen\u00b7den", "Eu\u00b7ro\u00b7pa", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NE", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Zwar durch des Bruderkrieges dunklen Engpa\u00df", "tokens": ["Zwar", "durch", "des", "Bru\u00b7der\u00b7krie\u00b7ges", "dunk\u00b7len", "Eng\u00b7pa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bricht Blut und Eisen nun sich furchtbar Bahn", "tokens": ["Bricht", "Blut", "und", "Ei\u00b7sen", "nun", "sich", "furcht\u00b7bar", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN", "ADV", "PRF", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und banger Zweifel dr\u00fcckt: \u00bbWird den Besiegten", "tokens": ["Und", "ban\u00b7ger", "Zwei\u00b7fel", "dr\u00fcckt", ":", "\u00bb", "Wird", "den", "Be\u00b7sieg\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$.", "$(", "VAFIN", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Wunde der Besiegung je verharschen?", "tokens": ["Die", "Wun\u00b7de", "der", "Be\u00b7sie\u00b7gung", "je", "ver\u00b7har\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wird uns der Fremde einig finden, wenn ...\u00ab", "tokens": ["Wird", "uns", "der", "Frem\u00b7de", "ei\u00b7nig", "fin\u00b7den", ",", "wenn", "...", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "VVINF", "$,", "KOUS", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Da horch! Was klingt so hell her ", "tokens": ["Da", "horch", "!", "Was", "klingt", "so", "hell", "her"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$.", "PWS", "VVFIN", "ADV", "ADJD", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das ist der Klang der preu\u00dfischen Trompete!", "tokens": ["Das", "ist", "der", "Klang", "der", "preu\u00b7\u00dfi\u00b7schen", "Trom\u00b7pe\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Er ruft zum Schutz des Vaterlands: \u2013 und schon", "tokens": ["Er", "ruft", "zum", "Schutz", "des", "Va\u00b7ter\u00b7lands", ":", "\u2013", "und", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$.", "$(", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Antwortet ihm des Bayern J\u00e4gerhorn,", "tokens": ["Ant\u00b7wor\u00b7tet", "ihm", "des", "Bay\u00b7ern", "J\u00e4\u00b7ger\u00b7horn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Schon eilt ", "tokens": ["Schon", "eilt"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Zur Wacht am Rhein. \u2013 Und nun wird all' die Torheit", "tokens": ["Zur", "Wacht", "am", "Rhein", ".", "\u2013", "Und", "nun", "wird", "all'", "die", "Tor\u00b7heit"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NE", "$.", "$(", "KON", "ADV", "VAFIN", "PIS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der F\u00fcrsten und der St\u00e4mme Neid und Ha\u00df,", "tokens": ["Der", "F\u00fcrs\u00b7ten", "und", "der", "St\u00e4m\u00b7me", "Neid", "und", "Ha\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wird all' die Schuld von sechs Jahrhunderten", "tokens": ["Wird", "all'", "die", "Schuld", "von", "sechs", "Jahr\u00b7hun\u00b7der\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "ART", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Im Gottesurteil nie erh\u00f6rter Siege,", "tokens": ["Im", "Got\u00b7tes\u00b7ur\u00b7teil", "nie", "er\u00b7h\u00f6r\u00b7ter", "Sie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "In Schlachtenglut gel\u00e4utert und ges\u00fchnt:", "tokens": ["In", "Schlach\u00b7ten\u00b7glut", "ge\u00b7l\u00e4u\u00b7tert", "und", "ge\u00b7s\u00fchnt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Auf Stra\u00dfburgs M\u00fcn