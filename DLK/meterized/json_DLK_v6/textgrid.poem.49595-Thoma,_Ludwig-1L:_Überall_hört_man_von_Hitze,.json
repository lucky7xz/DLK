{"textgrid.poem.49595": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00dcberall h\u00f6rt man von Hitze,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcberall h\u00f6rt man von Hitze,", "tokens": ["\u00dc\u00b7be\u00b7rall", "h\u00f6rt", "man", "von", "Hit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Manchen trifft sogar der Schlag,", "tokens": ["Man\u00b7chen", "trifft", "so\u00b7gar", "der", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Na\u00df wird man am Hosensitze", "tokens": ["Na\u00df", "wird", "man", "am", "Ho\u00b7sen\u00b7sit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PIS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon am fr\u00fchen Vormittag.", "tokens": ["Schon", "am", "fr\u00fc\u00b7hen", "Vor\u00b7mit\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Damen, denen man begegnet,", "tokens": ["Da\u00b7men", ",", "de\u00b7nen", "man", "be\u00b7geg\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leiden sehr am Ambopo\u00e4ng:", "tokens": ["Lei\u00b7den", "sehr", "am", "Am\u00b7bo\u00b7po\u00b7\u00e4ng", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "\u00bbgott! Wenn es nur endlich regnet'!\u00ab", "tokens": ["\u00bb", "gott", "!", "Wenn", "es", "nur", "end\u00b7lich", "reg\u00b7net'", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "$.", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist der ewige Refr\u00e4ng.", "tokens": ["Ist", "der", "e\u00b7wi\u00b7ge", "Re\u00b7fr\u00e4ng", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Oberlehrer und Pastoren", "tokens": ["O\u00b7ber\u00b7leh\u00b7rer", "und", "Pas\u00b7to\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Baden sich in diesem Jahr,", "tokens": ["Ba\u00b7den", "sich", "in", "die\u00b7sem", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre Scham geht auch verloren,", "tokens": ["Ih\u00b7re", "Scham", "geht", "auch", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und man nimmt sie nackicht wahr.", "tokens": ["Und", "man", "nimmt", "sie", "na\u00b7ckicht", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Busen, Hintern, Waden, B\u00e4uche", "tokens": ["Bu\u00b7sen", ",", "Hin\u00b7tern", ",", "Wa\u00b7den", ",", "B\u00e4u\u00b7che"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeigt man heuer l\u00e4chelnd her,", "tokens": ["Zeigt", "man", "heu\u00b7er", "l\u00e4\u00b7chelnd", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wir kriegen schon Gebr\u00e4uche", "tokens": ["Und", "wir", "krie\u00b7gen", "schon", "Ge\u00b7br\u00e4u\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die Neger ungef\u00e4hr.", "tokens": ["Wie", "die", "Ne\u00b7ger", "un\u00b7ge\u00b7f\u00e4hr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn das Barometer s\u00e4nke,", "tokens": ["Wenn", "das", "Ba\u00b7ro\u00b7me\u00b7ter", "s\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00e4me eine bess're Zeit", "tokens": ["K\u00e4\u00b7me", "ei\u00b7ne", "bess'\u00b7re", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In bezug auf die Gest\u00e4nke", "tokens": ["In", "be\u00b7zug", "auf", "die", "Ge\u00b7st\u00e4n\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Und in puncto Sittlichkeit.", "tokens": ["Und", "in", "punc\u00b7to", "Sitt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}