{"textgrid.poem.60642": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dem alten Wolfe, dem es schwer geworden,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem alten Wolfe, dem es schwer geworden,", "tokens": ["Dem", "al\u00b7ten", "Wol\u00b7fe", ",", "dem", "es", "schwer", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die L\u00e4mmer von dem Weideland zu stehlen,", "tokens": ["Die", "L\u00e4m\u00b7mer", "von", "dem", "Wei\u00b7de\u00b7land", "zu", "steh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gedachte jetzt auf leichtre Art zu morden;", "tokens": ["Ge\u00b7dach\u00b7te", "jetzt", "auf", "leicht\u00b7re", "Art", "zu", "mor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es sollte ihm an List und Kunst nicht fehlen.", "tokens": ["Es", "soll\u00b7te", "ihm", "an", "List", "und", "Kunst", "nicht", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "NN", "KON", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Er steckte sich in einen Hirtenrock,", "tokens": ["Er", "steck\u00b7te", "sich", "in", "ei\u00b7nen", "Hir\u00b7ten\u00b7rock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nahm R\u00e4nzel, Dudelsack und langen Stock", "tokens": ["Nahm", "R\u00e4n\u00b7zel", ",", "Du\u00b7del\u00b7sack", "und", "lan\u00b7gen", "Stock"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und h\u00e4tte zur Vollendung seiner List", "tokens": ["Und", "h\u00e4t\u00b7te", "zur", "Vol\u00b7len\u00b7dung", "sei\u00b7ner", "List"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Noch gern auf seine Stirn geschrieben: \u00bbDieser ist", "tokens": ["Noch", "gern", "auf", "sei\u00b7ne", "Stirn", "ge\u00b7schrie\u00b7ben", ":", "\u00bb", "Die\u00b7ser", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$.", "$(", "PDS", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Guillot, der Herde Hirt.\u00ab Der falsche Guillot ging,", "tokens": ["Guil\u00b7lot", ",", "der", "Her\u00b7de", "Hirt", ".", "\u00ab", "Der", "fal\u00b7sche", "Guil\u00b7lot", "ging", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "NN", "$.", "$(", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Indem er auf den Stab die Vorderpfoten legte,", "tokens": ["In\u00b7dem", "er", "auf", "den", "Stab", "die", "Vor\u00b7der\u00b7pfo\u00b7ten", "leg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Zum Ort des echten Hirts, der grad der Ruhe pflegte", "tokens": ["Zum", "Ort", "des", "ech\u00b7ten", "Hirts", ",", "der", "grad", "der", "Ru\u00b7he", "pfleg\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und dessen schlummernd Haupt das hohe Gras umfing.", "tokens": ["Und", "des\u00b7sen", "schlum\u00b7mernd", "Haupt", "das", "ho\u00b7he", "Gras", "um\u00b7fing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADJD", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es schliefen auch der Dudelsack, die Hunde", "tokens": ["Es", "schlie\u00b7fen", "auch", "der", "Du\u00b7del\u00b7sack", ",", "die", "Hun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und selbst die meisten Schafe in der Runde.", "tokens": ["Und", "selbst", "die", "meis\u00b7ten", "Scha\u00b7fe", "in", "der", "Run\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Heuchler lie\u00df die Schl\u00e4fer gerne ruhn", "tokens": ["Der", "Heuch\u00b7ler", "lie\u00df", "die", "Schl\u00e4\u00b7fer", "ger\u00b7ne", "ruhn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und er ergriff, um einige L\u00e4mmer fort", "tokens": ["Und", "er", "er\u00b7griff", ",", "um", "ei\u00b7ni\u00b7ge", "L\u00e4m\u00b7mer", "fort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUI", "PIAT", "NN", "PTKVZ"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Zum Wald zu treiben, alsobald das Wort,", "tokens": ["Zum", "Wald", "zu", "trei\u00b7ben", ",", "al\u00b7so\u00b7bald", "das", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Im Glauben, n\u00f6tig sei es, das zu tun.", "tokens": ["Im", "Glau\u00b7ben", ",", "n\u00f6\u00b7tig", "sei", "es", ",", "das", "zu", "tun", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "VAFIN", "PPER", "$,", "PRELS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch grade so verdarb er seine Sache nun.", "tokens": ["Doch", "gra\u00b7de", "so", "ver\u00b7darb", "er", "sei\u00b7ne", "Sa\u00b7che", "nun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zu schlecht gelang der Lockungsruf,", "tokens": ["Zu", "schlecht", "ge\u00b7lang", "der", "Lo\u00b7ckungs\u00b7ruf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Den hier ein R\u00e4uberrachen schuf.", "tokens": ["Den", "hier", "ein", "R\u00e4u\u00b7berr\u00b7a\u00b7chen", "schuf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Rauh scholl der Stimme Widerhall", "tokens": ["Rauh", "scholl", "der", "Stim\u00b7me", "Wi\u00b7der\u00b7hall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Vom Wald zur\u00fcck. Bei solchem Schall", "tokens": ["Vom", "Wald", "zu\u00b7r\u00fcck", ".", "Bei", "sol\u00b7chem", "Schall"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Erwachten j\u00e4hlings Hirt und Hunde", "tokens": ["Er\u00b7wach\u00b7ten", "j\u00e4h\u00b7lings", "Hirt", "und", "Hun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und alle Schafe in der Runde.", "tokens": ["Und", "al\u00b7le", "Scha\u00b7fe", "in", "der", "Run\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der Wolf kam auf der Flucht zu Fall", "tokens": ["Der", "Wolf", "kam", "auf", "der", "Flucht", "zu", "Fall"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Durch sein Gewand und seine Last", "tokens": ["Durch", "sein", "Ge\u00b7wand", "und", "sei\u00b7ne", "Last"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Und wurde, wehrlos, abgefa\u00dft.", "tokens": ["Und", "wur\u00b7de", ",", "wehr\u00b7los", ",", "ab\u00b7ge\u00b7fa\u00dft", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Mag sich ein Schurke auch verwandeln,", "tokens": ["Mag", "sich", "ein", "Schur\u00b7ke", "auch", "ver\u00b7wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Um g\u00fcnstig sich herauszustreichen,", "tokens": ["Um", "g\u00fcns\u00b7tig", "sich", "her\u00b7aus\u00b7zu\u00b7strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Stets bleiben doch Erkennungszeichen:", "tokens": ["Stets", "blei\u00b7ben", "doch", "Er\u00b7ken\u00b7nungs\u00b7zei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Wer Wolf ist, wird als Wolf auch handeln.", "tokens": ["Wer", "Wolf", "ist", ",", "wird", "als", "Wolf", "auch", "han\u00b7deln", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VAFIN", "$,", "VAFIN", "KOKOM", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}