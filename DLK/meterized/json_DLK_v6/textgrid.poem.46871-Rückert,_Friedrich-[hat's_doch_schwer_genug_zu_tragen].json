{"textgrid.poem.46871": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[hat's doch schwer genug zu tragen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hat's doch schwer genug zu tragen,", "tokens": ["Hat's", "doch", "schwer", "ge\u00b7nug", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Am Verluste schwer genug;", "tokens": ["Am", "Ver\u00b7lus\u00b7te", "schwer", "ge\u00b7nug", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll das Herz dazu sich sagen,", "tokens": ["Soll", "das", "Herz", "da\u00b7zu", "sich", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PAV", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df es ihn als Strafe trug?", "tokens": ["Da\u00df", "es", "ihn", "als", "Stra\u00b7fe", "trug", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "KOUS", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nagte, meine Schuld zu strafen,", "tokens": ["Nag\u00b7te", ",", "mei\u00b7ne", "Schuld", "zu", "stra\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unschuldsknospe, dich der Wurm?", "tokens": ["Un\u00b7schulds\u00b7knos\u00b7pe", ",", "dich", "der", "Wurm", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder da\u00df du ruhst im Hafen,", "tokens": ["O\u00b7der", "da\u00df", "du", "ruhst", "im", "Ha\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll mich werfen in den Sturm?", "tokens": ["Soll", "mich", "wer\u00b7fen", "in", "den", "Sturm", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nein, die Rose, die erschienen", "tokens": ["Nein", ",", "die", "Ro\u00b7se", ",", "die", "er\u00b7schie\u00b7nen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN", "$,", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War an meinem Lebensstrauch,", "tokens": ["War", "an", "mei\u00b7nem", "Le\u00b7bens\u00b7strauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hab' ich ohne mein Verdienen,", "tokens": ["Hab'", "ich", "oh\u00b7ne", "mein", "Ver\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie gehabt, verloren auch.", "tokens": ["Wie", "ge\u00b7habt", ",", "ver\u00b7lo\u00b7ren", "auch", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAPP", "$,", "VVPP", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber Dank dem Fr\u00fchlingshauche,", "tokens": ["A\u00b7ber", "Dank", "dem", "Fr\u00fch\u00b7lings\u00b7hau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der mir lie\u00df die Rose bl\u00fchn,", "tokens": ["Der", "mir", "lie\u00df", "die", "Ro\u00b7se", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blieb daf\u00fcr auch nun am Strauche", "tokens": ["Blieb", "da\u00b7f\u00fcr", "auch", "nun", "am", "Strau\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PAV", "ADV", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur der Dorn des Schmerzes gr\u00fcn.", "tokens": ["Nur", "der", "Dorn", "des", "Schmer\u00b7zes", "gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}