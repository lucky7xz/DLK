{"dta.poem.18973": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Corydon.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Weil Ich nicht mehr die Zeit", "tokens": ["Weil", "Ich", "nicht", "mehr", "die", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nach langem layd vnd streit", "tokens": ["Nach", "lan\u00b7gem", "layd", "vnd", "streit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verzweiflend kan vertreiben/", "tokens": ["Ver\u00b7zwei\u00b7flend", "kan", "ver\u00b7trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So mu\u00df Ich/ Cloris/ schreiben", "tokens": ["So", "mu\u00df", "Ich", "/", "Clo\u00b7ris", "/", "schrei\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VMFIN", "PPER", "$(", "NE", "$(", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die zeugnu\u00df meiner beut;", "tokens": ["Die", "zeug\u00b7nu\u00df", "mei\u00b7ner", "beut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dieweil ich nichts erworben", "tokens": ["Die\u00b7weil", "ich", "nichts", "er\u00b7wor\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Euch dan hassz zu lohn/", "tokens": ["Von", "Euch", "dan", "hassz", "zu", "lohn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$("], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Bin Ich hie Corydon", "tokens": ["Bin", "Ich", "hie", "Co\u00b7ry\u00b7don"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Au\u00df lieb vnd layd gestorben.", "tokens": ["Au\u00df", "lieb", "vnd", "layd", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Al\u00dfdan der arme H\u00fcrt/ beschliessend seinen mund/", "tokens": ["Al\u00df\u00b7dan", "der", "ar\u00b7me", "H\u00fcrt", "/", "be\u00b7schlies\u00b7send", "sei\u00b7nen", "mund", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$(", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sunck mit dem haupt gantz blaich vnd sprachlo\u00df zu", "tokens": ["Sunck", "mit", "dem", "haupt", "gantz", "blaich", "vnd", "sprac\u00b7hlo\u00df", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "ADJD", "KON", "VVFIN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "dem grund;", "tokens": ["dem", "grund", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Als Cloris solches sah/wolt sie gar nicht verziehen/", "tokens": ["Als", "Clo\u00b7ris", "sol\u00b7ches", "sah", "/", "wolt", "sie", "gar", "nicht", "ver\u00b7zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PIS", "VVFIN", "$(", "VMFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sondern fieng an davon/ schnell wie ein Reh/ zu", "tokens": ["Son\u00b7dern", "fi\u00b7eng", "an", "da\u00b7von", "/", "schnell", "wie", "ein", "Reh", "/", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "APPR", "PAV", "$(", "ADJD", "KOKOM", "ART", "NN", "$(", "APPR"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "fliehen/", "tokens": ["flie\u00b7hen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "So voll zorn/ stoltz vnd hassz/ Als Sie den Filodor", "tokens": ["So", "voll", "zorn", "/", "stoltz", "vnd", "hassz", "/", "Als", "Sie", "den", "Fi\u00b7lo\u00b7dor"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "NN", "$(", "ADJD", "KON", "NN", "$(", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vnd seine Myrt verliessz/ voll lieb vnd lust zuvor/", "tokens": ["Vnd", "sei\u00b7ne", "Myrt", "ver\u00b7liessz", "/", "voll", "lieb", "vnd", "lust", "zu\u00b7vor", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NE", "$(", "ADJD", "ADJD", "KON", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vnd voll verwunderung jetz/ vnd als voll des tods", "tokens": ["Vnd", "voll", "ver\u00b7wun\u00b7de\u00b7rung", "jetz", "/", "vnd", "als", "voll", "des", "tods"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "NN", "ADV", "$(", "KON", "KOUS", "ADJD", "ART", "NN"], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "zaichen", "tokens": ["zai\u00b7chen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Sie sahen Corydon au\u00df lieb vnd layd verblaichen/", "tokens": ["Sie", "sa\u00b7hen", "Co\u00b7ry\u00b7don", "au\u00df", "lieb", "vnd", "layd", "ver\u00b7blai\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "APPR", "ADJD", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vnd sterben/ wa sie nicht mit flei\u00df/ mit trost/ mit", "tokens": ["Vnd", "ster\u00b7ben", "/", "wa", "sie", "nicht", "mit", "flei\u00df", "/", "mit", "trost", "/", "mit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "VVINF", "$(", "PWAV", "PPER", "PTKNEG", "APPR", "NN", "$(", "APPR", "NN", "$(", "APPR"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "macht/", "tokens": ["macht", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.14": {"text": "In seinen schwachen leib den gaist zu ruck gebracht.", "tokens": ["In", "sei\u00b7nen", "schwa\u00b7chen", "leib", "den", "gaist", "zu", "ruck", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}