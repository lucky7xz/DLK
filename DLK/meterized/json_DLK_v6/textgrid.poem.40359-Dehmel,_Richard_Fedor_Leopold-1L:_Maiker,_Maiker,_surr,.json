{"textgrid.poem.40359": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Maiker, Maiker, surr,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Maiker, Maiker, surr,", "tokens": ["Mai\u00b7ker", ",", "Mai\u00b7ker", ",", "surr", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "bleib nur sitzen, burr!", "tokens": ["bleib", "nur", "sit\u00b7zen", ",", "burr", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "VVINF", "$,", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Breite deine F\u00fchler aus,", "tokens": ["Brei\u00b7te", "dei\u00b7ne", "F\u00fch\u00b7ler", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "mach zwei kleine F\u00e4cher draus,", "tokens": ["mach", "zwei", "klei\u00b7ne", "F\u00e4\u00b7cher", "draus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "ADJA", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "schwing sie kreuz und quer,", "tokens": ["schwing", "sie", "kreuz", "und", "quer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "z\u00e4hle mir was her!", "tokens": ["z\u00e4h\u00b7le", "mir", "was", "her", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Z\u00e4hle, ich will mit dir z\u00e4hlen,", "tokens": ["Z\u00e4h\u00b7le", ",", "ich", "will", "mit", "dir", "z\u00e4h\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "wieviel noch Minuten fehlen,", "tokens": ["wie\u00b7viel", "noch", "Mi\u00b7nu\u00b7ten", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "bis Herr Heuschreck wuppt", "tokens": ["bis", "Herr", "Heu\u00b7schreck", "wuppt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "NE", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "und mir auf die Nase huppt.", "tokens": ["und", "mir", "auf", "die", "Na\u00b7se", "huppt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Maik\u00e4ber, Maiker,", "tokens": ["Mai\u00b7k\u00e4\u00b7ber", ",", "Mai\u00b7ker", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "sonst holt dich der Deiker!", "tokens": ["sonst", "holt", "dich", "der", "Dei\u00b7ker", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}