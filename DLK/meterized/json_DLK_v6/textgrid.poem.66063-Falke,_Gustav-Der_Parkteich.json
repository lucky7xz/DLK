{"textgrid.poem.66063": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Parkteich", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein stiller Teich tr\u00e4umt im verlassnen Park,", "tokens": ["Ein", "stil\u00b7ler", "Teich", "tr\u00e4umt", "im", "ver\u00b7lass\u00b7nen", "Park", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von sonnendunklem Laub dicht \u00fcberschattet.", "tokens": ["Von", "son\u00b7nen\u00b7dunk\u00b7lem", "Laub", "dicht", "\u00fc\u00b7bersc\u00b7hat\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur manchmal, wenn der Wind heftiger rauscht,", "tokens": ["Nur", "manch\u00b7mal", ",", "wenn", "der", "Wind", "hef\u00b7ti\u00b7ger", "rauscht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Huscht ein verlorner Lichtstrahl \u00fcbers Wasser,", "tokens": ["Huscht", "ein", "ver\u00b7lor\u00b7ner", "Licht\u00b7strahl", "\u00fc\u00b7bers", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und zittert ein erschrockenes Wellchen auf", "tokens": ["Und", "zit\u00b7tert", "ein", "er\u00b7schro\u00b7cke\u00b7nes", "Well\u00b7chen", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und hastet \u00e4ngstlich in das Uferkraut.", "tokens": ["Und", "has\u00b7tet", "\u00e4ngst\u00b7lich", "in", "das", "Uf\u00b7er\u00b7kraut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Einsamer Weg f\u00fchrt um den stillen Teich,", "tokens": ["Ein\u00b7sa\u00b7mer", "Weg", "f\u00fchrt", "um", "den", "stil\u00b7len", "Teich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Gleich ihm von h\u00e4ngenden Zweigen \u00fcberd\u00e4mmert.", "tokens": ["Gleich", "ihm", "von", "h\u00e4n\u00b7gen\u00b7den", "Zwei\u00b7gen", "\u00fc\u00b7berd\u00b7\u00e4m\u00b7mert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Halbausgel\u00f6schte Spuren sind im Weg", "tokens": ["Hal\u00b7baus\u00b7ge\u00b7l\u00f6schte", "Spu\u00b7ren", "sind", "im", "Weg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "APPRART", "NN"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Vom Regen halb verwaschen und vom Wind", "tokens": ["Vom", "Re\u00b7gen", "halb", "ver\u00b7wa\u00b7schen", "und", "vom", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sacht \u00fcberst\u00e4ubt. Von wem erz\u00e4hlen sie?", "tokens": ["Sacht", "\u00fc\u00b7ber\u00b7st\u00e4ubt", ".", "Von", "wem", "er\u00b7z\u00e4h\u00b7len", "sie", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$.", "APPR", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mir ist, als m\u00fc\u00dfte diese gro\u00dfe Stille", "tokens": ["Mir", "ist", ",", "als", "m\u00fc\u00df\u00b7te", "die\u00b7se", "gro\u00b7\u00dfe", "Stil\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein M\u00e4dchenlachen pl\u00f6tzlich unterbrechen,", "tokens": ["Ein", "M\u00e4d\u00b7chen\u00b7la\u00b7chen", "pl\u00f6tz\u00b7lich", "un\u00b7ter\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus ihrem gr\u00fcnen Traum aufst\u00f6ren. Wenn der Wind", "tokens": ["Aus", "ih\u00b7rem", "gr\u00fc\u00b7nen", "Traum", "auf\u00b7st\u00f6\u00b7ren", ".", "Wenn", "der", "Wind"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVIZU", "$.", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Laub ein wenig hebt, und in dem Spiegel", "tokens": ["Das", "Laub", "ein", "we\u00b7nig", "hebt", ",", "und", "in", "dem", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PIS", "VVFIN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des dunklen Teichs ein Licht aufblitzt, gedenk ich", "tokens": ["Des", "dunk\u00b7len", "Teichs", "ein", "Licht", "auf\u00b7blitzt", ",", "ge\u00b7denk", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Eines tieflieben, jungen Augenpaares,", "tokens": ["Ei\u00b7nes", "tief\u00b7lie\u00b7ben", ",", "jun\u00b7gen", "Au\u00b7gen\u00b7paa\u00b7res", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Das ich aus einem stillen M\u00e4dchentraum", "tokens": ["Das", "ich", "aus", "ei\u00b7nem", "stil\u00b7len", "M\u00e4d\u00b7chen\u00b7traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Manchmal aufleuchten sehe, und ich meine,", "tokens": ["Manch\u00b7mal", "auf\u00b7leuch\u00b7ten", "se\u00b7he", ",", "und", "ich", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVIZU", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "Es h\u00e4tte hier wohl einmal vor dem Bild", "tokens": ["Es", "h\u00e4t\u00b7te", "hier", "wohl", "ein\u00b7mal", "vor", "dem", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Parkstillen Friedens lieblich sich erhellt.", "tokens": ["Park\u00b7stil\u00b7len", "Frie\u00b7dens", "lieb\u00b7lich", "sich", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJD", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein sanftes Wellchen hebt sich an das Ufer.", "tokens": ["Ein", "sanf\u00b7tes", "Well\u00b7chen", "hebt", "sich", "an", "das", "U\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Will es den Platz mir zeigen, wo sie stand?", "tokens": ["Will", "es", "den", "Platz", "mir", "zei\u00b7gen", ",", "wo", "sie", "stand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PPER", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo sie gesessen? Leise rauscht das Laub.", "tokens": ["Wo", "sie", "ge\u00b7ses\u00b7sen", "?", "Lei\u00b7se", "rauscht", "das", "Laub", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$.", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es ist ein Fl\u00fcstern. Ach, was fl\u00fcsterts doch?", "tokens": ["Es", "ist", "ein", "Fl\u00fcs\u00b7tern", ".", "Ach", ",", "was", "fl\u00fcs\u00b7terts", "doch", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "ITJ", "$,", "PRELS", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nichts. Nur ein Laub im Wind. Doch in mir wacht", "tokens": ["Nichts", ".", "Nur", "ein", "Laub", "im", "Wind", ".", "Doch", "in", "mir", "wacht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "$.", "ADV", "ART", "NN", "APPRART", "NN", "$.", "KON", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein Holdes auf und sucht nach Worten, findet", "tokens": ["Ein", "Hol\u00b7des", "auf", "und", "sucht", "nach", "Wor\u00b7ten", ",", "fin\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "PTKVZ", "KON", "VVFIN", "APPR", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nur einen lieben Namen, und der schwebt,", "tokens": ["Nur", "ei\u00b7nen", "lie\u00b7ben", "Na\u00b7men", ",", "und", "der", "schwebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "KON", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Leise dem Wind vertraut, \u00fcber den Teich.", "tokens": ["Lei\u00b7se", "dem", "Wind", "ver\u00b7traut", ",", "\u00fc\u00b7ber", "den", "Teich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "$,", "APPR", "ART", "NN", "$."], "meter": "+--+-+---+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Bewahr den Namen, m\u00e4rchentiefe Stille,", "tokens": ["Be\u00b7wahr", "den", "Na\u00b7men", ",", "m\u00e4r\u00b7chen\u00b7tie\u00b7fe", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bewahre ihn, da\u00df er, ein s\u00fc\u00dfer Laut", "tokens": ["Be\u00b7wah\u00b7re", "ihn", ",", "da\u00df", "er", ",", "ein", "s\u00fc\u00b7\u00dfer", "Laut"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der lieblichen Natur, hier Heimat hat.", "tokens": ["Der", "lieb\u00b7li\u00b7chen", "Na\u00b7tur", ",", "hier", "Hei\u00b7mat", "hat", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und kehrt sie wieder, wandelt einmal noch", "tokens": ["Und", "kehrt", "sie", "wie\u00b7der", ",", "wan\u00b7delt", "ein\u00b7mal", "noch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Durch diesen Frieden, der nun doppelt heilig,", "tokens": ["Durch", "die\u00b7sen", "Frie\u00b7den", ",", "der", "nun", "dop\u00b7pelt", "hei\u00b7lig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mag sie, wie ich heut, lauschend stehn und fragen:", "tokens": ["Mag", "sie", ",", "wie", "ich", "heut", ",", "lau\u00b7schend", "stehn", "und", "fra\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PWAV", "PPER", "ADV", "$,", "ADJD", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Was fl\u00fcstert doch das Laub? Und mag err\u00f6ten", "tokens": ["Was", "fl\u00fcs\u00b7tert", "doch", "das", "Laub", "?", "Und", "mag", "er\u00b7r\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "$.", "KON", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und l\u00e4cheln, meint sie, \u00fcbern Teich her ruft", "tokens": ["Und", "l\u00e4\u00b7cheln", ",", "meint", "sie", ",", "\u00fc\u00b7bern", "Teich", "her", "ruft"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "PPER", "$,", "ADJA", "NN", "APZR", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ein andrer sie mit Namen.", "tokens": ["Ein", "an\u00b7drer", "sie", "mit", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Leise rauscht", "tokens": ["Lei\u00b7se", "rauscht"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Das sommerdunkle Laub rings um den Teich.", "tokens": ["Das", "som\u00b7mer\u00b7dunk\u00b7le", "Laub", "rings", "um", "den", "Teich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Sonnenl\u00e4cheln zittert auf dem Spiegel.", "tokens": ["Ein", "Son\u00b7nen\u00b7l\u00e4\u00b7cheln", "zit\u00b7tert", "auf", "dem", "Spie\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und horch! Ein M\u00e4dchenlachen? Nein, Herz, nein.", "tokens": ["Und", "horch", "!", "Ein", "M\u00e4d\u00b7chen\u00b7la\u00b7chen", "?", "Nein", ",", "Herz", ",", "nein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "ART", "NN", "$.", "PTKANT", "$,", "NN", "$,", "PTKANT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Traumstille Einsamkeit nur atmete", "tokens": ["Traum\u00b7stil\u00b7le", "Ein\u00b7sam\u00b7keit", "nur", "at\u00b7me\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "VVFIN"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.6": {"text": "Einmal aus ihrem Frieden selig auf.", "tokens": ["Ein\u00b7mal", "aus", "ih\u00b7rem", "Frie\u00b7den", "se\u00b7lig", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}