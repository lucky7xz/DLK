{"textgrid.poem.55203": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Amor als Landschaftsmaler", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sa\u00df ich fr\u00fch auf einer Felsenspitze,", "tokens": ["Sa\u00df", "ich", "fr\u00fch", "auf", "ei\u00b7ner", "Fel\u00b7sen\u00b7spit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sah mit starren Augen in den Nebel;", "tokens": ["Sah", "mit", "star\u00b7ren", "Au\u00b7gen", "in", "den", "Ne\u00b7bel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wie ein grau grundiertes Tuch gespannet,", "tokens": ["Wie", "ein", "grau", "grun\u00b7dier\u00b7tes", "Tuch", "ge\u00b7span\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deckt' er alles in die Breit und H\u00f6he.", "tokens": ["Deckt'", "er", "al\u00b7les", "in", "die", "Breit", "und", "H\u00f6\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Stellt' ein Knabe sich mir an die Seite,", "tokens": ["Stellt'", "ein", "Kna\u00b7be", "sich", "mir", "an", "die", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sagte: \u00bbLieber Freund, wie magst du starrend", "tokens": ["Sag\u00b7te", ":", "\u00bb", "Lie\u00b7ber", "Freund", ",", "wie", "magst", "du", "star\u00b7rend"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "$(", "ADJD", "NN", "$,", "PWAV", "VMFIN", "PPER", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Auf das leere Tuch gelassen schauen?", "tokens": ["Auf", "das", "lee\u00b7re", "Tuch", "ge\u00b7las\u00b7sen", "schau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hast du denn zum Malen und zum Bilden", "tokens": ["Hast", "du", "denn", "zum", "Ma\u00b7len", "und", "zum", "Bil\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Alle Lust auf ewig wohl verloren?\u00ab", "tokens": ["Al\u00b7le", "Lust", "auf", "e\u00b7wig", "wohl", "ver\u00b7lo\u00b7ren", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJD", "ADV", "VVPP", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Sah ich an das Kind und dachte heimlich:", "tokens": ["Sah", "ich", "an", "das", "Kind", "und", "dach\u00b7te", "heim\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Will das B\u00fcbchen doch den Meister machen!", "tokens": ["Will", "das", "B\u00fcb\u00b7chen", "doch", "den", "Meis\u00b7ter", "ma\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbwillst du immer tr\u00fcb und m\u00fc\u00dfig bleiben\u00ab,", "tokens": ["\u00bb", "willst", "du", "im\u00b7mer", "tr\u00fcb", "und", "m\u00fc\u00b7\u00dfig", "blei\u00b7ben", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VMFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "VVINF", "$(", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sprach der Knabe, \u00bbkann nichts Kluges werden:", "tokens": ["Sprach", "der", "Kna\u00b7be", ",", "\u00bb", "kann", "nichts", "Klu\u00b7ges", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "$(", "VMFIN", "PIS", "ADJA", "VAINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sieh, ich will dir gleich ein Bildchen malen,", "tokens": ["Sieh", ",", "ich", "will", "dir", "gleich", "ein", "Bild\u00b7chen", "ma\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dich ein h\u00fcbsches Bildchen malen lehren.\u00ab", "tokens": ["Dich", "ein", "h\u00fcb\u00b7sches", "Bild\u00b7chen", "ma\u00b7len", "leh\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVINF", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Und er richtete den Zeigefinger,", "tokens": ["Und", "er", "rich\u00b7te\u00b7te", "den", "Zei\u00b7ge\u00b7fin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der so r\u00f6tlich war wie eine Rose,", "tokens": ["Der", "so", "r\u00f6t\u00b7lich", "war", "wie", "ei\u00b7ne", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Nach dem weiten, ausgespannten Teppich,", "tokens": ["Nach", "dem", "wei\u00b7ten", ",", "aus\u00b7ge\u00b7spann\u00b7ten", "Tep\u00b7pich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Fing mit seinem Finger an zu zeichnen:", "tokens": ["Fing", "mit", "sei\u00b7nem", "Fin\u00b7ger", "an", "zu", "zeich\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "APZR", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Oben malt' er eine sch\u00f6ne Sonne,", "tokens": ["O\u00b7ben", "malt'", "er", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die mir in die Augen m\u00e4chtig gl\u00e4nzte,", "tokens": ["Die", "mir", "in", "die", "Au\u00b7gen", "m\u00e4ch\u00b7tig", "gl\u00e4nz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und den Saum der Wolken macht' er golden,", "tokens": ["Und", "den", "Saum", "der", "Wol\u00b7ken", "macht'", "er", "gol\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lie\u00df die Strahlen durch die Wolken dringen;", "tokens": ["Lie\u00df", "die", "Strah\u00b7len", "durch", "die", "Wol\u00b7ken", "drin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Malte dann die zarten, leichten Wipfel", "tokens": ["Mal\u00b7te", "dann", "die", "zar\u00b7ten", ",", "leich\u00b7ten", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Frisch erquickter B\u00e4ume, zog die H\u00fcgel,", "tokens": ["Frisch", "er\u00b7quick\u00b7ter", "B\u00e4u\u00b7me", ",", "zog", "die", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Einen nach dem andern, frei dahinter;", "tokens": ["Ei\u00b7nen", "nach", "dem", "an\u00b7dern", ",", "frei", "da\u00b7hin\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "$,", "ADJD", "PAV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Unten lie\u00df er's nicht an Wasser fehlen,", "tokens": ["Un\u00b7ten", "lie\u00df", "er's", "nicht", "an", "Was\u00b7ser", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Zeichnete den Flu\u00df so ganz nat\u00fcrlich,", "tokens": ["Zeich\u00b7ne\u00b7te", "den", "Flu\u00df", "so", "ganz", "na\u00b7t\u00fcr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Da\u00df er schien im Sonnenstrahl zu glitzern,", "tokens": ["Da\u00df", "er", "schien", "im", "Son\u00b7nen\u00b7strahl", "zu", "glit\u00b7zern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Da\u00df er schien am hohen Rand zu rauschen.", "tokens": ["Da\u00df", "er", "schien", "am", "ho\u00b7hen", "Rand", "zu", "rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Ach, da standen Blumen an dem Flusse,", "tokens": ["Ach", ",", "da", "stan\u00b7den", "Blu\u00b7men", "an", "dem", "Flus\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und da waren Farben auf der Wiese,", "tokens": ["Und", "da", "wa\u00b7ren", "Far\u00b7ben", "auf", "der", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Gold und Schmelz und Purpur und ein Gr\u00fcnes,", "tokens": ["Gold", "und", "Schmelz", "und", "Pur\u00b7pur", "und", "ein", "Gr\u00fc\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "KON", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Alles wie Smaragd und wie Karfunkel!", "tokens": ["Al\u00b7les", "wie", "Sma\u00b7ragd", "und", "wie", "Kar\u00b7fun\u00b7kel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "NN", "KON", "PWAV", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Hell und rein lasiert' er drauf den Himmel", "tokens": ["Hell", "und", "rein", "la\u00b7siert'", "er", "drauf", "den", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "VVFIN", "PPER", "PAV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und die blauen Berge fern und ferner,", "tokens": ["Und", "die", "blau\u00b7en", "Ber\u00b7ge", "fern", "und", "fer\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJD", "KON", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Da\u00df ich ganz entz\u00fcckt und neu geboren", "tokens": ["Da\u00df", "ich", "ganz", "ent\u00b7z\u00fcckt", "und", "neu", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "KON", "ADJD", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Bald den Maler, bald das Bild beschaute.", "tokens": ["Bald", "den", "Ma\u00b7ler", ",", "bald", "das", "Bild", "be\u00b7schau\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "\u00bbhab ich doch\u00ab, so sagt' er, \u00bbdir bewiesen,", "tokens": ["\u00bb", "hab", "ich", "doch", "\u00ab", ",", "so", "sagt'", "er", ",", "\u00bb", "dir", "be\u00b7wie\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ADV", "$(", "$,", "ADV", "VVFIN", "PPER", "$,", "$(", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df ich dieses Handwerk gut verstehe;", "tokens": ["Da\u00df", "ich", "die\u00b7ses", "Hand\u00b7werk", "gut", "ver\u00b7ste\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Doch es ist das Schwerste noch zur\u00fccke.\u00ab", "tokens": ["Doch", "es", "ist", "das", "Schwers\u00b7te", "noch", "zu\u00b7r\u00fc\u00b7cke", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "ADV", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zeichnete darnach mit spitzem Finger", "tokens": ["Zeich\u00b7ne\u00b7te", "dar\u00b7nach", "mit", "spit\u00b7zem", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und mit gro\u00dfer Sorgfalt an dem W\u00e4ldchen,", "tokens": ["Und", "mit", "gro\u00b7\u00dfer", "Sorg\u00b7falt", "an", "dem", "W\u00e4ld\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Grad ans Ende, wo die Sonne kr\u00e4ftig", "tokens": ["Grad", "ans", "En\u00b7de", ",", "wo", "die", "Son\u00b7ne", "kr\u00e4f\u00b7tig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "PWAV", "ART", "NN", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Von dem hellen Boden widergl\u00e4nzte,", "tokens": ["Von", "dem", "hel\u00b7len", "Bo\u00b7den", "wi\u00b7der\u00b7gl\u00e4nz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Zeichnete das allerliebste M\u00e4dchen,", "tokens": ["Zeich\u00b7ne\u00b7te", "das", "al\u00b7ler\u00b7liebs\u00b7te", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Wohlgebildet, zierlich angekleidet,", "tokens": ["Wohl\u00b7ge\u00b7bil\u00b7det", ",", "zier\u00b7lich", "an\u00b7ge\u00b7klei\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Frische Wangen unter braunen Haaren,", "tokens": ["Fri\u00b7sche", "Wan\u00b7gen", "un\u00b7ter", "brau\u00b7nen", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+