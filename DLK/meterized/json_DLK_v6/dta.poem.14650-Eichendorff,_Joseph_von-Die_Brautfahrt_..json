{"dta.poem.14650": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Brautfahrt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201elieblich,\u201c sprach der wilde Ritter", "tokens": ["\u201e", "lieb\u00b7lich", ",", "\u201c", "sprach", "der", "wil\u00b7de", "Rit\u00b7ter"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "$,", "$(", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu der zarten, sch\u00f6nen Braut,", "tokens": ["Zu", "der", "zar\u00b7ten", ",", "sch\u00f6\u00b7nen", "Braut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201elieblich girrt die sanfte Zitter \u2014", "tokens": ["\u201e", "lieb\u00b7lich", "girrt", "die", "sanf\u00b7te", "Zit\u00b7ter"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sturm ist meiner Seele Laut,", "tokens": ["Sturm", "ist", "mei\u00b7ner", "See\u00b7le", "Laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "APPR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Wogen dumpfes Brausen", "tokens": ["Und", "der", "Wo\u00b7gen", "dum\u00b7pfes", "Brau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Hebt das Herz in k\u00fchnem Grausen.", "tokens": ["Hebt", "das", "Herz", "in", "k\u00fch\u00b7nem", "Grau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}