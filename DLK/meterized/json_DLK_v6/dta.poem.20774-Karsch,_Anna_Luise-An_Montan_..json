{"dta.poem.20774": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An Montan .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Tauch' ich die Feder in den Wein:               ", "tokens": ["Tauch'", "ich", "die", "Fe\u00b7der", "in", "den", "Wein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So kann dich dieser Irrthum lehren,", "tokens": ["So", "kann", "dich", "die\u00b7ser", "Irr\u00b7thum", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo mein Gedanke m\u00fcsse seyn;", "tokens": ["Wo", "mein", "Ge\u00b7dan\u00b7ke", "m\u00fcs\u00b7se", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O, k\u00f6nnte doch sein Ohr nur den Gedanken h\u00f6ren!", "tokens": ["O", ",", "k\u00f6nn\u00b7te", "doch", "sein", "Ohr", "nur", "den", "Ge\u00b7dan\u00b7ken", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "ADV", "PPOSAT", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}