{"textgrid.poem.57574": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "An ...", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur vorw\u00e4rts auf dem Siegeswege,", "tokens": ["Nur", "vor\u00b7w\u00e4rts", "auf", "dem", "Sie\u00b7ges\u00b7we\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur vorw\u00e4rts ohne Ruh und Rast,", "tokens": ["Nur", "vor\u00b7w\u00e4rts", "oh\u00b7ne", "Ruh", "und", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es bleibt dein Lohn, du Wahrheitsstreiter,", "tokens": ["Es", "bleibt", "dein", "Lohn", ",", "du", "Wahr\u00b7heits\u00b7strei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df du doch eine Stufe weiter", "tokens": ["Da\u00df", "du", "doch", "ei\u00b7ne", "Stu\u00b7fe", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vom Menschheitsziel erklettert hast.", "tokens": ["Vom", "Menschheits\u00b7ziel", "er\u00b7klet\u00b7tert", "hast", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn Dummheit und Gemeinheit hocken", "tokens": ["Wenn", "Dumm\u00b7heit", "und", "Ge\u00b7mein\u00b7heit", "ho\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch oben auf der Leiter Thron,", "tokens": ["Auch", "o\u00b7ben", "auf", "der", "Lei\u00b7ter", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts soll dich schrecken und verwirren,", "tokens": ["Nichts", "soll", "dich", "schre\u00b7cken", "und", "ver\u00b7wir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df deine Kugeln pfeifend schwirren", "tokens": ["La\u00df", "dei\u00b7ne", "Ku\u00b7geln", "pfei\u00b7fend", "schwir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "VVPP", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und achte nicht auf Spott und Hohn.", "tokens": ["Und", "ach\u00b7te", "nicht", "auf", "Spott", "und", "Hohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "PTKNEG", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du stehst allein und bist am st\u00e4rksten", "tokens": ["Du", "stehst", "al\u00b7lein", "und", "bist", "am", "st\u00e4rks\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VAFIN", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schaust nicht abw\u00e4rts ins Gew\u00fchl,", "tokens": ["Und", "schaust", "nicht", "ab\u00b7w\u00e4rts", "ins", "Ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ob sie schimpfen dich und l\u00e4stern,", "tokens": ["Und", "ob", "sie", "schimp\u00b7fen", "dich", "und", "l\u00e4s\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "PPER", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Neid und Bewunderung sind Schwestern,", "tokens": ["Neid", "und", "Be\u00b7wun\u00b7de\u00b7rung", "sind", "Schwes\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Kein Dummbescheidner kommt ans Ziel.", "tokens": ["Kein", "Dumm\u00b7be\u00b7scheid\u00b7ner", "kommt", "ans", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wo alles dumpfig und verschlossen", "tokens": ["Wo", "al\u00b7les", "dump\u00b7fig", "und", "ver\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ADJD", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und voll von Staub und Todeskeim,", "tokens": ["Und", "voll", "von", "Staub", "und", "To\u00b7des\u00b7keim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da \u00f6ffne du die blinden Fenster,", "tokens": ["Da", "\u00f6ff\u00b7ne", "du", "die", "blin\u00b7den", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vertreibe L\u00fcge und Gespenster", "tokens": ["Ver\u00b7trei\u00b7be", "L\u00fc\u00b7ge", "und", "Ge\u00b7spens\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und l\u00fcfte aus das alte Heim.", "tokens": ["Und", "l\u00fcf\u00b7te", "aus", "das", "al\u00b7te", "Heim", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Schon seh' am Himmelsrand ich zucken", "tokens": ["Schon", "seh'", "am", "Him\u00b7mels\u00b7rand", "ich", "zu\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Wetterstrahl von deinem Ruhm,", "tokens": ["Den", "Wet\u00b7ter\u00b7strahl", "von", "dei\u00b7nem", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zerfeilt sind Eisenstab und Ketten,", "tokens": ["Zer\u00b7feilt", "sind", "Ei\u00b7sen\u00b7stab", "und", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf steht die T\u00fcr, du kannst dich retten", "tokens": ["Auf", "steht", "die", "T\u00fcr", ",", "du", "kannst", "dich", "ret\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "ART", "NN", "$,", "PPER", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In freies, sch\u00f6nes Dichtertum.", "tokens": ["In", "frei\u00b7es", ",", "sch\u00f6\u00b7nes", "Dich\u00b7ter\u00b7tum", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}