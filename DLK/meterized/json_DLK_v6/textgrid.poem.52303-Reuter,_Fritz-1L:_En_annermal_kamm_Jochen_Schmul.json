{"textgrid.poem.52303": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: En annermal kamm Jochen Schmul", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "En annermal kamm Jochen Schmul", "tokens": ["En", "an\u00b7ner\u00b7mal", "kamm", "Jo\u00b7chen", "Schmul"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ut Hanschend\u00f6rp tau Stadt herin.", "tokens": ["Ut", "Han\u00b7schen\u00b7d\u00f6rp", "tau", "Stadt", "he\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Oll Jochen was en Leckermul:", "tokens": ["Oll", "Jo\u00b7chen", "was", "en", "Le\u00b7cker\u00b7mul", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PWS", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "'ne Potschon Koffe s\u00fcll dat sin!", "tokens": ["'ne", "Pot\u00b7schon", "Kof\u00b7fe", "s\u00fcll", "dat", "sin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hei hadd so vel von Koffe h\u00fcrt", "tokens": ["Hei", "hadd", "so", "vel", "von", "Kof\u00b7fe", "h\u00fcrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "ADV", "ADJD", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Un hadd sin Dag em nich prebiert \u2013", "tokens": ["Un", "hadd", "sin", "Dag", "em", "nich", "pre\u00b7biert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Hei et des Morgens Kl\u00fctersupp.", "tokens": ["Hei", "et", "des", "Mor\u00b7gens", "Kl\u00fc\u00b7ter\u00b7supp", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "ART", "ADV", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "H\u00fct steg em nu so'n Giwwel up,", "tokens": ["H\u00fct", "steg", "em", "nu", "so'n", "Giw\u00b7wel", "up", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Hei wull h\u00fct mal ens v\u00f6rnehm leben", "tokens": ["Hei", "wull", "h\u00fct", "mal", "ens", "v\u00f6r\u00b7nehm", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "ADV", "ADV", "ADJD", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Un let sick also Koffe geben.", "tokens": ["Un", "let", "sick", "al\u00b7so", "Kof\u00b7fe", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "De Koffe w\u00fcrd herinner br\u00f6cht,", "tokens": ["De", "Kof\u00b7fe", "w\u00fcrd", "he\u00b7rin\u00b7ner", "br\u00f6cht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Oll Jochen set't sick nu taurecht", "tokens": ["Oll", "Jo\u00b7chen", "set't", "sick", "nu", "tau\u00b7recht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Un s\u00fcht sick de Geschicht irst an:", "tokens": ["Un", "s\u00fcht", "sick", "de", "Ge\u00b7schicht", "irst", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.14": {"text": "De Tass', den L\u00e4pel un de Kann,", "tokens": ["De", "Tass'", ",", "den", "L\u00e4\u00b7pel", "un", "de", "Kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "FM", "FM", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Wotau de Dinger woll s\u00fcnd n\u00fctt!", "tokens": ["Wo\u00b7tau", "de", "Din\u00b7ger", "woll", "s\u00fcnd", "n\u00fctt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "De L\u00e4pel schint em gor tau l\u00fctt,", "tokens": ["De", "L\u00e4\u00b7pel", "schint", "em", "gor", "tau", "l\u00fctt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Hei is tau l\u00fctt f\u00f6r sine Finger.", "tokens": ["Hei", "is", "tau", "l\u00fctt", "f\u00f6r", "si\u00b7ne", "Fin\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "Un denn de beiden Tassendinger!", "tokens": ["Un", "denn", "de", "bei\u00b7den", "Tas\u00b7sen\u00b7din\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Na, endlich m\u00f6t hei doch heran.", "tokens": ["Na", ",", "end\u00b7lich", "m\u00f6t", "hei", "doch", "he\u00b7ran", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADJD", "VAFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Hei langt sick also her de Kann", "tokens": ["Hei", "langt", "sick", "al\u00b7so", "her", "de", "Kann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "ADV", "ADV", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Un schenkt sick ok 'ne Tass' vull in,", "tokens": ["Un", "schenkt", "sick", "ok", "'", "ne", "Tass", "'", "vull", "in", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "FM", "FM", "$(", "NE", "NE", "$(", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Un as hei dit St\u00fcck hett taurecht,", "tokens": ["Un", "as", "hei", "dit", "St\u00fcck", "hett", "tau\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "NN", "VAFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Nimmt hei den L\u00e4pel, s\u00fcfzt un seggt:", "tokens": ["Nimmt", "hei", "den", "L\u00e4\u00b7pel", ",", "s\u00fcfzt", "un", "seggt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ART", "NN", "$,", "VVFIN", "FM", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "\u00bbje, 't mag jo Mod woll jetzund sin!\u00ab", "tokens": ["\u00bb", "je", ",", "'t", "mag", "jo", "Mod", "woll", "je\u00b7tzund", "sin", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$,", "NE", "VMFIN", "NE", "NN", "ADV", "ADV", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Un f\u00e4ngt nu langsam an tau l\u00e4peln.", "tokens": ["Un", "f\u00e4ngt", "nu", "lang\u00b7sam", "an", "tau", "l\u00e4\u00b7peln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADJD", "APPR", "NE", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.26": {"text": "Hei ett un ett, dat will nich sch\u00e4peln:", "tokens": ["Hei", "ett", "un", "ett", ",", "dat", "will", "nich", "sch\u00e4\u00b7peln", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "$,", "PAV", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.27": {"text": "De Sak k\u00fcmmt em tau tahrig v\u00f6r,", "tokens": ["De", "Sak", "k\u00fcmmt", "em", "tau", "tah\u00b7rig", "v\u00f6r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.28": {"text": "Un as de Wirt geiht ut de D\u00f6r,", "tokens": ["Un", "as", "de", "Wirt", "geiht", "ut", "de", "D\u00f6r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Dunn kickt hei sick so wild her\u00fcm,", "tokens": ["Dunn", "kickt", "hei", "sick", "so", "wild", "he\u00b7r\u00fcm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "FM", "FM", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Ob em ok wer woll wohren k\u00fcnn", "tokens": ["Ob", "em", "ok", "wer", "woll", "woh\u00b7ren", "k\u00fcnn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "PWS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Un ob hei wir ok ganz allein.", "tokens": ["Un", "ob", "hei", "wir", "ok", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NE", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.32": {"text": "\u00bbje, wenn ick w\u00fc\u00dft, dat sehg mi kein\u00ab,", "tokens": ["\u00bb", "je", ",", "wenn", "ick", "w\u00fc\u00dft", ",", "dat", "sehg", "mi", "kein", "\u00ab", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "PRELS", "ADV", "NE", "PIAT", "$(", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.33": {"text": "Seggt hei, \u00bbick ded't, ick ded't, der D\u00fcwel hal!", "tokens": ["Seggt", "hei", ",", "\u00bb", "ick", "ded't", ",", "ick", "ded't", ",", "der", "D\u00fc\u00b7wel", "hal", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "$(", "PPER", "VVFIN", "$,", "FM.la", "FM.la", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Ick nem de Tass' un s\u00f6p enmal!\u00ab", "tokens": ["Ick", "nem", "de", "Tass'", "un", "s\u00f6p", "en\u00b7mal", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}