{"textgrid.poem.49257": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Diese Sitten, diese Gaben,", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diese Sitten, diese Gaben,", "tokens": ["Die\u00b7se", "Sit\u00b7ten", ",", "die\u00b7se", "Ga\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese keusche Freundlichkeit,", "tokens": ["Die\u00b7se", "keu\u00b7sche", "Freund\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welcher Glantz euch hat erhaben,", "tokens": ["Wel\u00b7cher", "Glantz", "euch", "hat", "er\u00b7ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PPER", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ihr Liebens w\u00fcrdig seyd,", "tokens": ["Da\u00df", "ihr", "Lie\u00b7bens", "w\u00fcr\u00b7dig", "seyd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Uebertrifft der Sachen Prei\u00df,", "tokens": ["Ue\u00b7bert\u00b7rifft", "der", "Sa\u00b7chen", "Prei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die man sonst zu finden wei\u00df.", "tokens": ["Die", "man", "sonst", "zu", "fin\u00b7den", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch mein williges Gem\u00fchte,", "tokens": ["Doch", "mein", "wil\u00b7li\u00b7ges", "Ge\u00b7m\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Darmit ich euch zugethan,", "tokens": ["Dar\u00b7mit", "ich", "euch", "zu\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Uebertrifft de\u00df Bandes G\u00fcte,", "tokens": ["Ue\u00b7bert\u00b7rifft", "de\u00df", "Ban\u00b7des", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welches ich jetzt kn\u00fcpffen kan;", "tokens": ["Wel\u00b7ches", "ich", "jetzt", "kn\u00fcpf\u00b7fen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil der Sinn nun nicht gebricht,", "tokens": ["Weil", "der", "Sinn", "nun", "nicht", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "So verschmeht das Band auch nicht.", "tokens": ["So", "ver\u00b7schmeht", "das", "Band", "auch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gott, der euch die Ziehr gegeben,", "tokens": ["Gott", ",", "der", "euch", "die", "Ziehr", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lasse den gew\u00fcndschten Tag", "tokens": ["Las\u00b7se", "den", "ge\u00b7w\u00fcnd\u00b7schten", "Tag"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Euch mit Freuden bald erleben,", "tokens": ["Euch", "mit", "Freu\u00b7den", "bald", "er\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ein Band euch binden mag,", "tokens": ["Da\u00df", "ein", "Band", "euch", "bin\u00b7den", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das vom Himmel selbst erkiest", "tokens": ["Das", "vom", "Him\u00b7mel", "selbst", "er\u00b7kiest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPRART", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Sch\u00f6nheit w\u00fcrdig ist.", "tokens": ["Und", "der", "Sch\u00f6n\u00b7heit", "w\u00fcr\u00b7dig", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}