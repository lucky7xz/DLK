{"textgrid.poem.37448": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "So nicht", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ums Paradies ging eine Mauer", "tokens": ["Ums", "Pa\u00b7ra\u00b7dies", "ging", "ei\u00b7ne", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00fcbsch hoch vom besten Marmelstein.", "tokens": ["H\u00fcbsch", "hoch", "vom", "bes\u00b7ten", "Mar\u00b7mel\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Kain, als ein Bub ein schlauer,", "tokens": ["Der", "Kain", ",", "als", "ein", "Bub", "ein", "schlau\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Denkt sich: \u00bbIch komme doch hinein.\u00ab", "tokens": ["Denkt", "sich", ":", "\u00bb", "Ich", "kom\u00b7me", "doch", "hin\u00b7ein", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PRF", "$.", "$(", "PPER", "VVFIN", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er stieg hinauf zu diesem Zwecke", "tokens": ["Er", "stieg", "hin\u00b7auf", "zu", "die\u00b7sem", "Zwe\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An einer Leiter m\u00e4uschenstumm.", "tokens": ["An", "ei\u00b7ner", "Lei\u00b7ter", "m\u00e4u\u00b7schen\u00b7stumm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da schlich der Teufel um die Ecke", "tokens": ["Da", "schlich", "der", "Teu\u00b7fel", "um", "die", "E\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und stie\u00df ihn samt der Leiter um.", "tokens": ["Und", "stie\u00df", "ihn", "samt", "der", "Lei\u00b7ter", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Vater Adam, der's gesehen,", "tokens": ["Der", "Va\u00b7ter", "A\u00b7dam", ",", "der's", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach, w\u00e4hrend er ihn liegen lie\u00df:", "tokens": ["Sprach", ",", "w\u00e4h\u00b7rend", "er", "ihn", "lie\u00b7gen", "lie\u00df", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbdu Schlingel! Dir ist recht geschehen.", "tokens": ["\u00bb", "du", "Schlin\u00b7gel", "!", "Dir", "ist", "recht", "ge\u00b7sche\u00b7hen", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "NN", "$.", "PPER", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So kommt man nicht ins Paradies.\u00ab", "tokens": ["So", "kommt", "man", "nicht", "ins", "Pa\u00b7ra\u00b7dies", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKNEG", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}