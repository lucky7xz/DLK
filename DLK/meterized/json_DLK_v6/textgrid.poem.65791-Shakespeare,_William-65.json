{"textgrid.poem.65791": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "65", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Erz, Stein, Erde, weite Meeresflut", "tokens": ["Wenn", "Erz", ",", "Stein", ",", "Er\u00b7de", ",", "wei\u00b7te", "Mee\u00b7res\u00b7flut"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "NN", "$,", "NN", "$,", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der tr\u00fcben Sterblichkeit Gewalten weicht;", "tokens": ["Der", "tr\u00fc\u00b7ben", "Sterb\u00b7lich\u00b7keit", "Ge\u00b7wal\u00b7ten", "weicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie m\u00e4\u00dfe Sch\u00f6nheit sich mit solcher Wut,", "tokens": ["Wie", "m\u00e4\u00b7\u00dfe", "Sch\u00f6n\u00b7heit", "sich", "mit", "sol\u00b7cher", "Wut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie, deren Kraft der Blume Kr\u00e4ften gleicht?", "tokens": ["Sie", ",", "de\u00b7ren", "Kraft", "der", "Blu\u00b7me", "Kr\u00e4f\u00b7ten", "gleicht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O, wie soll Sommers honigs\u00fc\u00dfer Flor", "tokens": ["O", ",", "wie", "soll", "Som\u00b7mers", "ho\u00b7nig\u00b7s\u00fc\u00b7\u00dfer", "Flor"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "PIAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verw\u00fcsterischer Jahre Sturm bestehn,", "tokens": ["Ver\u00b7w\u00fcs\u00b7te\u00b7ri\u00b7scher", "Jah\u00b7re", "Sturm", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wenn weder Urgebirg noch Eisentor", "tokens": ["Wenn", "we\u00b7der", "Ur\u00b7ge\u00b7birg", "noch", "Ei\u00b7sen\u00b7tor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KON", "NN", "ADV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So m\u00e4chtig sind, dem Wandel zu entgehn?", "tokens": ["So", "m\u00e4ch\u00b7tig", "sind", ",", "dem", "Wan\u00b7del", "zu", "ent\u00b7gehn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Furchtbare Vorstellung! Wo soll vorm Sarge", "tokens": ["Furcht\u00b7ba\u00b7re", "Vor\u00b7stel\u00b7lung", "!", "Wo", "soll", "vorm", "Sar\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PWAV", "VMFIN", "APPRART", "NN"], "meter": "+--++-+--+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Der Zeit ihr best Juwel gesichert sein?", "tokens": ["Der", "Zeit", "ihr", "best", "Ju\u00b7wel", "ge\u00b7si\u00b7chert", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "NN", "VVPP", "VAINF", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.11": {"text": "Wer h\u00e4lt am schnellen Fu\u00df zur\u00fcck die arge?", "tokens": ["Wer", "h\u00e4lt", "am", "schnel\u00b7len", "Fu\u00df", "zu\u00b7r\u00fcck", "die", "ar\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Wer steuert ihren Sch\u00f6nheitsr\u00e4uberein?", "tokens": ["Wer", "steu\u00b7ert", "ih\u00b7ren", "Sch\u00f6n\u00b7heits\u00b7r\u00e4u\u00b7be\u00b7re\u00b7in", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "O, niemand: wird ", "tokens": ["O", ",", "nie\u00b7mand", ":", "wird"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "PIS", "$.", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Da\u00df dunkle Tinte hell den Freund verkl\u00e4rt.", "tokens": ["Da\u00df", "dunk\u00b7le", "Tin\u00b7te", "hell", "den", "Freund", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}