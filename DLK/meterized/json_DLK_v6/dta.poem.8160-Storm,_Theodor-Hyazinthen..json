{"dta.poem.8160": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Hyazinthen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Mit Schlummerduft anhauchen mich die Pflanzen;", "tokens": ["Mit", "Schlum\u00b7mer\u00b7duft", "an\u00b7hau\u00b7chen", "mich", "die", "Pflan\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich habe immer, immer dein gedacht,", "tokens": ["Ich", "ha\u00b7be", "im\u00b7mer", ",", "im\u00b7mer", "dein", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "ADV", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich m\u00f6chte schlafen; aber du mu\u00dft tanzen.", "tokens": ["Ich", "m\u00f6ch\u00b7te", "schla\u00b7fen", ";", "a\u00b7ber", "du", "mu\u00dft", "tan\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$.", "KON", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es h\u00f6rt nicht auf, es ras't ohn' Unterla\u00df;", "tokens": ["Es", "h\u00f6rt", "nicht", "auf", ",", "es", "ras't", "ohn'", "Un\u00b7ter\u00b7la\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Kerzen brennen und die Geigen schreien,", "tokens": ["Die", "Ker\u00b7zen", "bren\u00b7nen", "und", "die", "Gei\u00b7gen", "schrei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es theilen und es schlie\u00dfen sich die Reihen,", "tokens": ["Es", "thei\u00b7len", "und", "es", "schlie\u00b7\u00dfen", "sich", "die", "Rei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Alle gl\u00fchen; aber du bist bla\u00df.", "tokens": ["Und", "Al\u00b7le", "gl\u00fc\u00b7hen", ";", "a\u00b7ber", "du", "bist", "bla\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVINF", "$.", "KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und du mu\u00dft tanzen; fremde Arme schmiegen", "tokens": ["Und", "du", "mu\u00dft", "tan\u00b7zen", ";", "frem\u00b7de", "Ar\u00b7me", "schmie\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "$.", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich an dein Herz; o leide nicht Gewalt!", "tokens": ["Sich", "an", "dein", "Herz", ";", "o", "lei\u00b7de", "nicht", "Ge\u00b7walt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "$.", "FM", "VVFIN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich seh' dein wei\u00dfes Kleid vor\u00fcberfliegen", "tokens": ["Ich", "seh'", "dein", "wei\u00b7\u00dfes", "Kleid", "vor\u00b7\u00fc\u00b7berf\u00b7lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und deine leichte, z\u00e4rtliche Gestalt. \u2014 \u2014", "tokens": ["Und", "dei\u00b7ne", "leich\u00b7te", ",", "z\u00e4rt\u00b7li\u00b7che", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und s\u00fc\u00dfer str\u00f6mend quillt der Duft der Nacht", "tokens": ["Und", "s\u00fc\u00b7\u00dfer", "str\u00f6\u00b7mend", "quillt", "der", "Duft", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und tr\u00e4umerischer aus dem Kelch der Pflanzen.", "tokens": ["Und", "tr\u00e4u\u00b7me\u00b7ri\u00b7scher", "aus", "dem", "Kelch", "der", "Pflan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich habe immer, immer dein gedacht;", "tokens": ["Ich", "ha\u00b7be", "im\u00b7mer", ",", "im\u00b7mer", "dein", "ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "ADV", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich m\u00f6chte schlafen; aber du mu\u00dft tanzen.", "tokens": ["Ich", "m\u00f6ch\u00b7te", "schla\u00b7fen", ";", "a\u00b7ber", "du", "mu\u00dft", "tan\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$.", "KON", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}