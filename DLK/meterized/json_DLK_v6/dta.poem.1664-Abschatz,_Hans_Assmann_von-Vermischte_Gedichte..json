{"dta.poem.1664": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Keine Sonn in kurtzem sieht der sonst heitre Tag erscheinen/", "tokens": ["Kei\u00b7ne", "Sonn", "in", "kurt\u00b7zem", "sieht", "der", "sonst", "heit\u00b7re", "Tag", "er\u00b7schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJA", "VVFIN", "ART", "ADV", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Wer zum fr\u00fchen Morgen lacht/ mu\u00df auf sp\u00e4ten Abend weinen/", "tokens": ["Wer", "zum", "fr\u00fc\u00b7hen", "Mor\u00b7gen", "lacht", "/", "mu\u00df", "auf", "sp\u00e4\u00b7ten", "A\u00b7bend", "wei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "ADJA", "NN", "VVFIN", "$(", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Sizt er in der glatten Scho\u00df", "tokens": ["Sizt", "er", "in", "der", "glat\u00b7ten", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "An Verm\u00f6gen reich dem Gl\u00fccke:", "tokens": ["An", "Ver\u00b7m\u00f6\u00b7gen", "reich", "dem", "Gl\u00fc\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie ist er im Augenblicke/", "tokens": ["Wie", "ist", "er", "im", "Au\u00b7gen\u00b7bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "So von Gutt und Hoffnung blo\u00df.", "tokens": ["So", "von", "Gutt", "und", "Hoff\u00b7nung", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hat jemand/ so haben wir di\u00df nur allzuviel erfahren/", "tokens": ["Hat", "je\u00b7mand", "/", "so", "ha\u00b7ben", "wir", "di\u00df", "nur", "all\u00b7zu\u00b7viel", "er\u00b7fah\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$(", "ADV", "VAFIN", "PPER", "PDS", "ADV", "PIS", "VVINF", "$("], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Unser Gl\u00fccke strich dahin/ eh wir dessen f\u00e4hig waren.", "tokens": ["Un\u00b7ser", "Gl\u00fc\u00b7cke", "strich", "da\u00b7hin", "/", "eh", "wir", "des\u00b7sen", "f\u00e4\u00b7hig", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PAV", "$(", "KOUS", "PPER", "PDS", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Nichts als ein getreuer Sinn/", "tokens": ["Nichts", "als", "ein", "ge\u00b7treu\u00b7er", "Sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nichts als redliche Gem\u00fctter", "tokens": ["Nichts", "als", "red\u00b7li\u00b7che", "Ge\u00b7m\u00fct\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist vom Schiffbruch unsrer G\u00fctter", "tokens": ["Ist", "vom", "Schiff\u00b7bruch", "uns\u00b7rer", "G\u00fct\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "(edles Kleinod!) der Gewinn.", "tokens": ["(", "ed\u00b7les", "Klei\u00b7nod", "!", ")", "der", "Ge\u00b7winn", "."], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$.", "$(", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Diese/ diese wie wir sie allbereit verwechselt haben/", "tokens": ["Die\u00b7se", "/", "die\u00b7se", "wie", "wir", "sie", "all\u00b7be\u00b7reit", "ver\u00b7wech\u00b7selt", "ha\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "$(", "PDS", "KOKOM", "PPER", "PPER", "ADV", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Wollen wir in eine Grufft/ neben unsern Leib/ vergraben.", "tokens": ["Wol\u00b7len", "wir", "in", "ei\u00b7ne", "Grufft", "/", "ne\u00b7ben", "un\u00b7sern", "Leib", "/", "ver\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$(", "APPR", "PPOSAT", "NN", "$(", "VVPP", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Treues Hertze/ treue Hand/", "tokens": ["Treu\u00b7es", "Hert\u00b7ze", "/", "treu\u00b7e", "Hand", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fcssen sie sich izt gleich schmiegen/", "tokens": ["M\u00fcs\u00b7sen", "sie", "sich", "izt", "gleich", "schmie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADV", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter fremden F\u00fcssen liegen/", "tokens": ["Un\u00b7ter", "frem\u00b7den", "F\u00fcs\u00b7sen", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gehen doch durch jedes Land.", "tokens": ["Ge\u00b7hen", "doch", "durch", "je\u00b7des", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Liebet das Verh\u00e4ngnis gleich unsre Gegenwart zu trennen/", "tokens": ["Lie\u00b7bet", "das", "Ver\u00b7h\u00e4ng\u00b7nis", "gleich", "uns\u00b7re", "Ge\u00b7gen\u00b7wart", "zu", "tren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "+---+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Soll doch dessen strenge Macht unsre Freundschafft nicht er-", "tokens": ["Soll", "doch", "des\u00b7sen", "stren\u00b7ge", "Macht", "uns\u00b7re", "Freund\u00b7schafft", "nicht", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PDS", "VVFIN", "NN", "PPOSAT", "NN", "PTKNEG", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Die/ auff ewig steiff und fest/", "tokens": ["Die", "/", "auff", "e\u00b7wig", "steiff", "und", "fest", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "APPR", "ADJD", "VVFIN", "KON", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "An welch End und Ort der Erden", "tokens": ["An", "welch", "End", "und", "Ort", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "KON", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie auch mag verschlagen werden/", "tokens": ["Sie", "auch", "mag", "ver\u00b7schla\u00b7gen", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "VVINF", "VAFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihre Flammen scheinen l\u00e4st.", "tokens": ["Ih\u00b7re", "Flam\u00b7men", "schei\u00b7nen", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wer wei\u00df/ was das Gl\u00fccke noch ein und andern kan bescheren/", "tokens": ["Wer", "wei\u00df", "/", "was", "das", "Gl\u00fc\u00b7cke", "noch", "ein", "und", "an\u00b7dern", "kan", "be\u00b7sche\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "PWS", "ART", "NN", "ADV", "PTKVZ", "KON", "PIS", "VMFIN", "VVINF", "$("], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wo dir das Verh\u00e4ngnis noch in die Armen kan gew\u00e4hren", "tokens": ["Wo", "dir", "das", "Ver\u00b7h\u00e4ng\u00b7nis", "noch", "in", "die", "Ar\u00b7men", "kan", "ge\u00b7w\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "APPR", "ART", "NN", "VMFIN", "VVINF"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}}}}}