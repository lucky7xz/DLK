{"dta.poem.8500": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df ein bessres Bild zu finden,", "tokens": ["Ich", "wei\u00df", "ein", "bess\u00b7res", "Bild", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als jenes, dem du ferner geh\u2019st,", "tokens": ["Als", "je\u00b7nes", ",", "dem", "du", "fer\u00b7ner", "geh'st", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie tiefer deine Wurzeln gr\u00fcnden", "tokens": ["Wie", "tie\u00b7fer", "dei\u00b7ne", "Wur\u00b7zeln", "gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und reifer du die Ernte m\u00e4h\u2019st;", "tokens": ["Und", "rei\u00b7fer", "du", "die", "Ern\u00b7te", "m\u00e4h'st", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein bessres, als zu dessen Rahmen,", "tokens": ["Ein", "bess\u00b7res", ",", "als", "zu", "des\u00b7sen", "Rah\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KOUS", "APPR", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn Jahre flohen, Jahre kamen,", "tokens": ["Wenn", "Jah\u00b7re", "flo\u00b7hen", ",", "Jah\u00b7re", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du wie dein eigner Schatten steh\u2019st.", "tokens": ["Du", "wie", "dein", "eig\u00b7ner", "Schat\u00b7ten", "steh'", "st."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "KOKOM", "PPOSAT", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Weil\u2019 ich am Strande ob der lauen", "tokens": ["Weil'", "ich", "am", "Stran\u00b7de", "ob", "der", "lau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "KOUS", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entschlafnen Flut mit scheuer Lust:", "tokens": ["Ent\u00b7schlaf\u00b7nen", "Flut", "mit", "scheu\u00b7er", "Lust", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird unter\u2019m Stahl, dem silbern blauen,", "tokens": ["Wird", "un\u00b7ter'm", "Stahl", ",", "dem", "sil\u00b7bern", "blau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "$,", "ART", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lebendig mir die tiefe Rust;", "tokens": ["Le\u00b7ben\u00b7dig", "mir", "die", "tie\u00b7fe", "Rust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Am Grunde gl\u00fchende Korallen", "tokens": ["Am", "Grun\u00b7de", "gl\u00fc\u00b7hen\u00b7de", "Ko\u00b7ral\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Fischlein goldig schimmernd Wallen;", "tokens": ["Der", "Fisc\u00b7hlein", "gol\u00b7dig", "schim\u00b7mernd", "Wal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dann schau ich tief in deine Brust.", "tokens": ["Dann", "schau", "ich", "tief", "in", "dei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und schwebend an der Gr\u00fcfte Bogen", "tokens": ["Und", "schwe\u00b7bend", "an", "der", "Gr\u00fcf\u00b7te", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seh\u2019 ich der Mauerflechte Stab,", "tokens": ["Seh'", "ich", "der", "Mau\u00b7er\u00b7flech\u00b7te", "Stab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit allen Fasern eingesogen", "tokens": ["Mit", "al\u00b7len", "Fa\u00b7sern", "ein\u00b7ge\u00b7so\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tief in das Felsenherz hinab;", "tokens": ["Tief", "in", "das", "Fel\u00b7sen\u00b7herz", "hin\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von Thr\u00e4nen schwer die grauen Locken,", "tokens": ["Von", "Thr\u00e4\u00b7nen", "schwer", "die", "grau\u00b7en", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die dunkeln Wimper, zarten Flocken;", "tokens": ["Die", "dun\u00b7keln", "Wim\u00b7per", ",", "zar\u00b7ten", "Flo\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das ist die Liebe \u00fcber\u2019s Grab!", "tokens": ["Das", "ist", "die", "Lie\u00b7be", "\u00fc\u00b7ber's", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und dann an der Genesung Bronnen \u2014", "tokens": ["Und", "dann", "an", "der", "Ge\u00b7ne\u00b7sung", "Bron\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Im Saale tafeln Stern und Band, \u2014", "tokens": ["Im", "Saa\u00b7le", "ta\u00b7feln", "Stern", "und", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "KON", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich arme d\u00fcrft\u2019ge Kranke sonnen", "tokens": ["Sich", "ar\u00b7me", "d\u00fcrft'\u00b7ge", "Kran\u00b7ke", "son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ADJA", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und gierig schl\u00fcrfen \u00fcber\u2019m Rand;", "tokens": ["Und", "gie\u00b7rig", "schl\u00fcr\u00b7fen", "\u00fc\u00b7ber'm", "Rand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mitleidig tr\u00e4nkt der Quell die Armen,", "tokens": ["Mit\u00b7lei\u00b7dig", "tr\u00e4nkt", "der", "Quell", "die", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dann denk\u2019 ich still an dein Erbarmen,", "tokens": ["Dann", "denk'", "ich", "still", "an", "dein", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "An deine warme, offne Hand.", "tokens": ["An", "dei\u00b7ne", "war\u00b7me", ",", "off\u00b7ne", "Hand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O jener Quell, der hei\u00df und springend,", "tokens": ["O", "je\u00b7ner", "Quell", ",", "der", "hei\u00df", "und", "sprin\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "$,", "PRELS", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Geiser, deiner Brust entquillt,", "tokens": ["Ein", "Gei\u00b7ser", ",", "dei\u00b7ner", "Brust", "ent\u00b7quillt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Schnee und Eisesscholle dringend", "tokens": ["Durch", "Schnee", "und", "Ei\u00b7ses\u00b7schol\u00b7le", "drin\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Blumen seinen Gletscher f\u00fcllt. \u2014", "tokens": ["Mit", "Blu\u00b7men", "sei\u00b7nen", "Glet\u00b7scher", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihm sieht nur gleich, was nie verloren,", "tokens": ["Ihm", "sieht", "nur", "gleich", ",", "was", "nie", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was ewig frisch und neugeboren,", "tokens": ["Was", "e\u00b7wig", "frisch", "und", "neu\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und die Natur nur ist dein Bild!", "tokens": ["Und", "die", "Na\u00b7tur", "nur", "ist", "dein", "Bild", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}