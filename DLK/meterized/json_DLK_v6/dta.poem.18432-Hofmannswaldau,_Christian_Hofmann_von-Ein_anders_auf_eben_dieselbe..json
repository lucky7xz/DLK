{"dta.poem.18432": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ein anders auf eben dieselbe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So bald dich Friderich dein K\u00f6nigsberg gebohren/", "tokens": ["So", "bald", "dich", "Fri\u00b7de\u00b7rich", "dein", "K\u00f6\u00b7nigs\u00b7berg", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NE", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So k\u00fcndigte sein Dach ", "tokens": ["So", "k\u00fcn\u00b7dig\u00b7te", "sein", "Dach"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was meinte doch der mann?", "tokens": ["Was", "mein\u00b7te", "doch", "der", "mann", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es war ja Carl Aemil\u2019 damahls noch unverlohren.", "tokens": ["Es", "war", "ja", "Carl", "A\u00b7e\u00b7mil'", "da\u00b7mahls", "noch", "un\u00b7ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NE", "NE", "ADV", "ADV", "ADJD", "$."], "meter": "-+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.5": {"text": "Ach! er sah\u2019/ wie es scheint/ viel weiter/ als dein Hau\u00df:", "tokens": ["Ach", "!", "er", "sah'", "/", "wie", "es", "scheint", "/", "viel", "wei\u00b7ter", "/", "als", "dein", "Hau\u00df", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "$(", "PWAV", "PPER", "VVFIN", "$(", "ADV", "ADV", "$(", "KOUS", "PPOSAT", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Drum rieff er dich bey zeit f\u00fcr seinen K\u00f6nig aus.", "tokens": ["Drum", "rieff", "er", "dich", "bey", "zeit", "f\u00fcr", "sei\u00b7nen", "K\u00f6\u00b7nig", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "APPR", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch scheint es/ da\u00df sein trieb sich heut in mir verneue.", "tokens": ["Doch", "scheint", "es", "/", "da\u00df", "sein", "trieb", "sich", "heut", "in", "mir", "ver\u00b7neu\u00b7e", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "KOUS", "PPOSAT", "VVFIN", "PRF", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dir/ oder deinem Sohn ein zweytes K\u00f6nigreich.", "tokens": ["Dir", "/", "o\u00b7der", "dei\u00b7nem", "Sohn", "ein", "zwey\u00b7tes", "K\u00f6\u00b7nig\u00b7reich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "KON", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie? sind wohl einige/ die es nicht k\u00f6nnen hoffen?", "tokens": ["Wie", "?", "sind", "wohl", "ei\u00b7ni\u00b7ge", "/", "die", "es", "nicht", "k\u00f6n\u00b7nen", "hof\u00b7fen", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VAFIN", "ADV", "PIAT", "$(", "PRELS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Dach war ein mensch wie ich/ er hat es doch getroffen.", "tokens": ["Dach", "war", "ein", "mensch", "wie", "ich", "/", "er", "hat", "es", "doch", "ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "ADJD", "KOKOM", "PPER", "$(", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}}}}}