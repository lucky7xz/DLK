{"textgrid.poem.36219": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "B\u00fcndnis", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo mehre bildend sich in Eins verbunden,", "tokens": ["Wo", "meh\u00b7re", "bil\u00b7dend", "sich", "in", "Eins", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PRF", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gewinnt der K\u00fcnstler seines Daseins Mitte,", "tokens": ["Ge\u00b7winnt", "der", "K\u00fcnst\u00b7ler", "sei\u00b7nes", "Da\u00b7seins", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wei\u00df nun, wohin er richten soll die Schritte,", "tokens": ["Wei\u00df", "nun", ",", "wo\u00b7hin", "er", "rich\u00b7ten", "soll", "die", "Schrit\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sieht die Teile sich zum Ganzen runden.", "tokens": ["Und", "sieht", "die", "Tei\u00b7le", "sich", "zum", "Gan\u00b7zen", "run\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In neuer Jugend wird die Kraft gesunden,", "tokens": ["In", "neu\u00b7er", "Ju\u00b7gend", "wird", "die", "Kraft", "ge\u00b7sun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die fort von Stuf' und Stufe h\u00f6her schritte,", "tokens": ["Die", "fort", "von", "Stuf'", "und", "Stu\u00b7fe", "h\u00f6\u00b7her", "schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKVZ", "APPR", "NN", "KON", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wenn man noch so starke Schmerzen litte:", "tokens": ["Und", "wenn", "man", "noch", "so", "star\u00b7ke", "Schmer\u00b7zen", "lit\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Bildung bleibt, es fliehen nur die Stunden.", "tokens": ["Die", "Bil\u00b7dung", "bleibt", ",", "es", "flie\u00b7hen", "nur", "die", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es darf der Mensch von Herzensgrund nur wollen,", "tokens": ["Es", "darf", "der", "Mensch", "von", "Her\u00b7zens\u00b7grund", "nur", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "NN", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Mut sich schlie\u00dfen an die mut'gen Br\u00fcder,", "tokens": ["Mit", "Mut", "sich", "schlie\u00b7\u00dfen", "an", "die", "mut'\u00b7gen", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den festen Sinn vom Ziele nimmer wenden;", "tokens": ["Den", "fes\u00b7ten", "Sinn", "vom", "Zie\u00b7le", "nim\u00b7mer", "wen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So mu\u00df ihm jeder Stoff Gestaltung zollen,", "tokens": ["So", "mu\u00df", "ihm", "je\u00b7der", "Stoff", "Ge\u00b7stal\u00b7tung", "zol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die h\u00f6chsten W\u00fcrden steigen zu ihm nieder,", "tokens": ["Die", "h\u00f6chs\u00b7ten", "W\u00fcr\u00b7den", "stei\u00b7gen", "zu", "ihm", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er kann des Lebens Kunstwerk gro\u00df vollenden.", "tokens": ["Er", "kann", "des", "Le\u00b7bens", "Kunst\u00b7werk", "gro\u00df", "voll\u00b7en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}