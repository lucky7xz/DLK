{"textgrid.poem.33962": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Kirme\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist ein Geigen und Fl\u00f6ten", "tokens": ["Das", "ist", "ein", "Gei\u00b7gen", "und", "Fl\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bis \u00fcber das Dorf hinaus:", "tokens": ["Bis", "\u00fc\u00b7ber", "das", "Dorf", "hin\u00b7aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie feiern die Kirme\u00df heute", "tokens": ["Sie", "fei\u00b7ern", "die", "Kir\u00b7me\u00df", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV"], "meter": "-+--++--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit Tanz und Spiel und Schmaus.", "tokens": ["Mit", "Tanz", "und", "Spiel", "und", "Schmaus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn ich ein M\u00e4dchen w\u00e4re,", "tokens": ["Wenn", "ich", "ein", "M\u00e4d\u00b7chen", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So schaut' ich die Burschen an,", "tokens": ["So", "schaut'", "ich", "die", "Bur\u00b7schen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch jetzt betracht' ich die M\u00e4dchen,", "tokens": ["Doch", "jetzt", "be\u00b7tracht'", "ich", "die", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein Mann sucht keinen Mann!", "tokens": ["Ein", "Mann", "sucht", "kei\u00b7nen", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Blonde hat mir gefallen,", "tokens": ["Die", "Blon\u00b7de", "hat", "mir", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So lang' ich die Braune nicht sah,", "tokens": ["So", "lang'", "ich", "die", "Brau\u00b7ne", "nicht", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Jetzt ist mir, als h\u00e4tt' ich ges\u00fcndigt;", "tokens": ["Jetzt", "ist", "mir", ",", "als", "h\u00e4tt'", "ich", "ge\u00b7s\u00fcn\u00b7digt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOKOM", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ei, war sie denn schon da?", "tokens": ["Ei", ",", "war", "sie", "denn", "schon", "da", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es darf sie nur Einer k\u00fcssen,", "tokens": ["Es", "darf", "sie", "nur", "Ei\u00b7ner", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Doch Jeder tanzt mit ihr,", "tokens": ["Doch", "Je\u00b7der", "tanzt", "mit", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und auch den plattsten Gesellen", "tokens": ["Und", "auch", "den", "platt\u00b7sten", "Ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Vergoldet ihr Auge mir.", "tokens": ["Ver\u00b7gol\u00b7det", "ihr", "Au\u00b7ge", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Und schl\u00e4gt sie's ergl\u00fchend nieder,", "tokens": ["Und", "schl\u00e4gt", "sie's", "er\u00b7gl\u00fc\u00b7hend", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVPP", "PTKVZ", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Weil sie des Sponsen sich sch\u00e4mt,", "tokens": ["Weil", "sie", "des", "Spon\u00b7sen", "sich", "sch\u00e4mt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Erhebt es daf\u00fcr das seine,", "tokens": ["Er\u00b7hebt", "es", "da\u00b7f\u00fcr", "das", "sei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "ART", "PPOSAT", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Man sieht, da\u00df ihn's nicht gr\u00e4mt.", "tokens": ["Man", "sieht", ",", "da\u00df", "ihn's", "nicht", "gr\u00e4mt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und dies gef\u00e4llt mir eben,", "tokens": ["Und", "dies", "ge\u00b7f\u00e4llt", "mir", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er f\u00fchlt die Ehre doch,", "tokens": ["Er", "f\u00fchlt", "die", "Eh\u00b7re", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und denkt er daran im Alter,", "tokens": ["Und", "denkt", "er", "da\u00b7ran", "im", "Al\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PAV", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So steift sich sein R\u00fccken noch.", "tokens": ["So", "steift", "sich", "sein", "R\u00fc\u00b7cken", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Im Alter, ach, im Alter!", "tokens": ["Im", "Al\u00b7ter", ",", "ach", ",", "im", "Al\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ITJ", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ja, ja, wir werden alt!", "tokens": ["Ja", ",", "ja", ",", "wir", "wer\u00b7den", "alt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er, ich, du selbst, wir Alle,", "tokens": ["Er", ",", "ich", ",", "du", "selbst", ",", "wir", "Al\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "$,", "PPER", "ADV", "$,", "PPER", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir werden alt und kalt!", "tokens": ["Wir", "wer\u00b7den", "alt", "und", "kalt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Kinder stecken des Abends", "tokens": ["Die", "Kin\u00b7der", "ste\u00b7cken", "des", "A\u00b7bends"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zuweilen Papier in Brand", "tokens": ["Zu\u00b7wei\u00b7len", "Pa\u00b7pier", "in", "Brand"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und legen's auf den Ofen", "tokens": ["Und", "le\u00b7gen's", "auf", "den", "O\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und kauern sich um den Rand.", "tokens": ["Und", "kau\u00b7ern", "sich", "um", "den", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Sie freu'n sich der h\u00fcpfenden Funken", "tokens": ["Sie", "freu'n", "sich", "der", "h\u00fcp\u00b7fen\u00b7den", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mit Grau und Schwarz vermischt,", "tokens": ["Mit", "Grau", "und", "Schwarz", "ver\u00b7mischt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wetten, wer von Allen", "tokens": ["Und", "wet\u00b7ten", ",", "wer", "von", "Al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWS", "APPR", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am letzten wohl erlischt.", "tokens": ["Am", "letz\u00b7ten", "wohl", "er\u00b7lischt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wir h\u00fcpfen, wie diese Funken,", "tokens": ["Wir", "h\u00fcp\u00b7fen", ",", "wie", "die\u00b7se", "Fun\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PDAT", "NN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ueber der Erde Rund", "tokens": ["Ue\u00b7ber", "der", "Er\u00b7de", "Rund"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und leuchten vielleicht am hellsten", "tokens": ["Und", "leuch\u00b7ten", "viel\u00b7leicht", "am", "hells\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In dieser frohen Stund'.", "tokens": ["In", "die\u00b7ser", "fro\u00b7hen", "Stund'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Wer wei\u00df, wer von uns Allen", "tokens": ["Wer", "wei\u00df", ",", "wer", "von", "uns", "Al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "PWS", "APPR", "PPER", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zuletzt erl\u00f6schen mag?", "tokens": ["Zu\u00b7letzt", "er\u00b7l\u00f6\u00b7schen", "mag", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der wei\u00df auch, wer am l\u00e4ngsten", "tokens": ["Der", "wei\u00df", "auch", ",", "wer", "am", "l\u00e4ngs\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "$,", "PWS", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erz\u00e4hlt von diesem Tag!", "tokens": ["Er\u00b7z\u00e4hlt", "von", "die\u00b7sem", "Tag", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Du sch\u00f6nstes Kind, ich ahne,", "tokens": ["Du", "sch\u00f6ns\u00b7tes", "Kind", ",", "ich", "ah\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das wirst du selber sein,", "tokens": ["Das", "wirst", "du", "sel\u00b7ber", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sehe dich, wie doppelt,", "tokens": ["Ich", "se\u00b7he", "dich", ",", "wie", "dop\u00b7pelt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Maifrisch, und alt, wie Stein.", "tokens": ["Maif\u00b7risch", ",", "und", "alt", ",", "wie", "Stein", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "ADJD", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Jetzt drehst du dich im Reigen,", "tokens": ["Jetzt", "drehst", "du", "dich", "im", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So reizend und geschwind,", "tokens": ["So", "rei\u00b7zend", "und", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie dort das Rosenbl\u00e4ttchen", "tokens": ["Wie", "dort", "das", "Ro\u00b7sen\u00b7bl\u00e4tt\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Sommerabendwind.", "tokens": ["Im", "Som\u00b7mer\u00b7a\u00b7bend\u00b7wind", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Jetzt hockst du blind im Lehnstuhl,", "tokens": ["Jetzt", "hockst", "du", "blind", "im", "Lehn\u00b7stuhl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Enkel um dich her,", "tokens": ["Die", "En\u00b7kel", "um", "dich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du sprichst von diesem Tage,", "tokens": ["Du", "sprichst", "von", "die\u00b7sem", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie glauben, von einer M\u00e4hr'.", "tokens": ["Sie", "glau\u00b7ben", ",", "von", "ei\u00b7ner", "M\u00e4hr'", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.15": {"line.1": {"text": "Du streichelst mit kn\u00f6chernem Finger", "tokens": ["Du", "strei\u00b7chelst", "mit", "kn\u00f6\u00b7cher\u00b7nem", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Enkelin, die dir gleicht,", "tokens": ["Die", "En\u00b7ke\u00b7lin", ",", "die", "dir", "gleicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Du sagst: ich war dir \u00e4hnlich,", "tokens": ["Du", "sagst", ":", "ich", "war", "dir", "\u00e4hn\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So jung, so sch\u00f6n, so leicht!", "tokens": ["So", "jung", ",", "so", "sch\u00f6n", ",", "so", "leicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Sie aber kann's nicht glauben,", "tokens": ["Sie", "a\u00b7ber", "kann's", "nicht", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und das verdenk' ich ihr nicht,", "tokens": ["Und", "das", "ver\u00b7denk'", "ich", "ihr", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "PPER", "PTKNEG", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Sie m\u00fc\u00dfte sich sagen: ich selber", "tokens": ["Sie", "m\u00fc\u00df\u00b7te", "sich", "sa\u00b7gen", ":", "ich", "sel\u00b7ber"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$.", "PPER", "ADV"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Bekomm' einst ein solches Gesicht!", "tokens": ["Be\u00b7komm'", "einst", "ein", "sol\u00b7ches", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "PIAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}