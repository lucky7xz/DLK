{"textgrid.poem.42628": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Glockenh\u00f6hle", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df mir eine Grotte,", "tokens": ["Ich", "wei\u00df", "mir", "ei\u00b7ne", "Grot\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gew\u00f6lbt mit Bergkristalle,", "tokens": ["Ge\u00b7w\u00f6lbt", "mit", "Berg\u00b7kris\u00b7tal\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die ist von einem Gotte", "tokens": ["Die", "ist", "von", "ei\u00b7nem", "Got\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Begabt mit seltnem Halle:", "tokens": ["Be\u00b7gabt", "mit", "selt\u00b7nem", "Hal\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was jemand sprach, was jemand sang,", "tokens": ["Was", "je\u00b7mand", "sprach", ",", "was", "je\u00b7mand", "sang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das wird in ihr zu Glockenklang.", "tokens": ["Das", "wird", "in", "ihr", "zu", "Glo\u00b7cken\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dort tauschen zwei Begl\u00fcckte,", "tokens": ["Dort", "tau\u00b7schen", "zwei", "Be\u00b7gl\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bewegt von gleichem Triebe,", "tokens": ["Be\u00b7wegt", "von", "glei\u00b7chem", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was l\u00e4ngst die Herzen dr\u00fcckte,", "tokens": ["Was", "l\u00e4ngst", "die", "Her\u00b7zen", "dr\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das erste Ja der Liebe;", "tokens": ["Das", "ers\u00b7te", "Ja", "der", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein leises Gl\u00f6cklein stimmt so rein", "tokens": ["Ein", "lei\u00b7ses", "Gl\u00f6c\u00b7klein", "stimmt", "so", "rein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu einem lautern, vollern ein.", "tokens": ["Zu", "ei\u00b7nem", "lau\u00b7tern", ",", "vol\u00b7lern", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dort lassen lust'ge Zecher", "tokens": ["Dort", "las\u00b7sen", "lust'\u00b7ge", "Ze\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich auf der Felsbank nieder,", "tokens": ["Sich", "auf", "der", "Fels\u00b7bank", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie schwingen volle Becher", "tokens": ["Sie", "schwin\u00b7gen", "vol\u00b7le", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und singen trunkne Lieder;", "tokens": ["Und", "sin\u00b7gen", "trunk\u00b7ne", "Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nie klang die Grotte so wie heut", "tokens": ["Nie", "klang", "die", "Grot\u00b7te", "so", "wie", "heut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "KOKOM", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Feuerl\u00e4rm und Sturmgel\u00e4ut.", "tokens": ["Von", "Feu\u00b7er\u00b7l\u00e4rm", "und", "Sturm\u00b7ge\u00b7l\u00e4ut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zween M\u00e4nner, ernst und sinnig,", "tokens": ["Zween", "M\u00e4n\u00b7ner", ",", "ernst", "und", "sin\u00b7nig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vereint durch heil'ge Bande,", "tokens": ["Ver\u00b7eint", "durch", "heil'\u00b7ge", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie reden dort so innig", "tokens": ["Sie", "re\u00b7den", "dort", "so", "in\u00b7nig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vom deutschen Vaterlande;", "tokens": ["Vom", "deut\u00b7schen", "Va\u00b7ter\u00b7lan\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da t\u00f6nt die tiefste Kluft entlang", "tokens": ["Da", "t\u00f6nt", "die", "tiefs\u00b7te", "Kluft", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPO"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein dumpfer Grabesglockenklang.", "tokens": ["Ein", "dum\u00b7pfer", "Gra\u00b7bes\u00b7glo\u00b7cken\u00b7klang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}