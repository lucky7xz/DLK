{"textgrid.poem.57988": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "26. Kannst du nichts tun, so mu\u00dft du ruhn", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kannst du nicht vieles tun, so sei nur still und leide,", "tokens": ["Kannst", "du", "nicht", "vie\u00b7les", "tun", ",", "so", "sei", "nur", "still", "und", "lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "PIS", "VVINF", "$,", "ADV", "VAFIN", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was zu lassen ist, so viel dir m\u00f6glich, meide", "tokens": ["Und", "was", "zu", "las\u00b7sen", "ist", ",", "so", "viel", "dir", "m\u00f6g\u00b7lich", ",", "mei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PWS", "PTKZU", "VVINF", "VAFIN", "$,", "ADV", "ADV", "PPER", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und harre in Geduld! Wirst du dem Herren ruhn,", "tokens": ["Und", "har\u00b7re", "in", "Ge\u00b7duld", "!", "Wirst", "du", "dem", "Her\u00b7ren", "ruhn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$.", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wird er alles selbst in dir und durch dich tun.", "tokens": ["So", "wird", "er", "al\u00b7les", "selbst", "in", "dir", "und", "durch", "dich", "tun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADV", "APPR", "PPER", "KON", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}