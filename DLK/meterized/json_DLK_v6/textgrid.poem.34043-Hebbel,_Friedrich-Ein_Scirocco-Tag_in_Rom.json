{"textgrid.poem.34043": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein Scirocco-Tag in Rom", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steht in Flammen die Welt? Sind rings die Meere verd\u00fcnstet,", "tokens": ["Steht", "in", "Flam\u00b7men", "die", "Welt", "?", "Sind", "rings", "die", "Mee\u00b7re", "ver\u00b7d\u00fcns\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "$.", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Welche mit linderndem Hauch sonst doch die Glut wohl gek\u00fchlt?", "tokens": ["Wel\u00b7che", "mit", "lin\u00b7dern\u00b7dem", "Hauch", "sonst", "doch", "die", "Glut", "wohl", "ge\u00b7k\u00fchlt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "ADJA", "NN", "ADV", "ADV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sinken sie alle in Asche zusammen, die St\u00e4dte der Menschen?", "tokens": ["Sin\u00b7ken", "sie", "al\u00b7le", "in", "A\u00b7sche", "zu\u00b7sam\u00b7men", ",", "die", "St\u00e4d\u00b7te", "der", "Men\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIS", "APPR", "NN", "PTKVZ", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "W\u00e4lzt den gl\u00fchenden Qualm langsam her\u00fcber der Wind?", "tokens": ["W\u00e4lzt", "den", "gl\u00fc\u00b7hen\u00b7den", "Qualm", "lang\u00b7sam", "her\u00b7\u00fc\u00b7ber", "der", "Wind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJD", "ADV", "ART", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Oder ist's der Scirocco, der zwar die Orange uns zeitigt", "tokens": ["O\u00b7der", "ist's", "der", "Sci\u00b7roc\u00b7co", ",", "der", "zwar", "die", "O\u00b7ran\u00b7ge", "uns", "zei\u00b7tigt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "ART", "NE", "$,", "PRELS", "ADV", "ART", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Und die Traube uns kocht, aber uns selbst auch erstickt,", "tokens": ["Und", "die", "Trau\u00b7be", "uns", "kocht", ",", "a\u00b7ber", "uns", "selbst", "auch", "er\u00b7stickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVFIN", "$,", "KON", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "So da\u00df Jeglicher zweifelt, er werde die Fr\u00fcchte noch kosten,", "tokens": ["So", "da\u00df", "Jeg\u00b7li\u00b7cher", "zwei\u00b7felt", ",", "er", "wer\u00b7de", "die", "Fr\u00fcch\u00b7te", "noch", "kos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Die er uns s\u00fc\u00dft, und des Weins, den er uns w\u00fcrzt, sich erfreu'n?", "tokens": ["Die", "er", "uns", "s\u00fc\u00dft", ",", "und", "des", "Weins", ",", "den", "er", "uns", "w\u00fcrzt", ",", "sich", "er\u00b7freu'n", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$,", "KON", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "PRF", "VVINF", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.9": {"text": "Sei es, was es auch sei, das blo\u00dfe Athmen wird Arbeit,", "tokens": ["Sei", "es", ",", "was", "es", "auch", "sei", ",", "das", "blo\u00b7\u00dfe", "Ath\u00b7men", "wird", "Ar\u00b7beit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PWS", "PPER", "ADV", "VAFIN", "$,", "ART", "ADJA", "NN", "VAFIN", "NN", "$,"], "meter": "-----+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Und das Leben begr\u00e4bt scheu sich im dumpfesten Schlaf,", "tokens": ["Und", "das", "Le\u00b7ben", "be\u00b7gr\u00e4bt", "scheu", "sich", "im", "dum\u00b7pfes\u00b7ten", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.11": {"text": "Kaum noch rettet es sich den leisen Wunsch, zu erwachen,", "tokens": ["Kaum", "noch", "ret\u00b7tet", "es", "sich", "den", "lei\u00b7sen", "Wunsch", ",", "zu", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "ART", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Denn es f\u00fchlt sich dem Tod, f\u00fchlt sich dem Nichts schon zu nah'!", "tokens": ["Denn", "es", "f\u00fchlt", "sich", "dem", "Tod", ",", "f\u00fchlt", "sich", "dem", "Nichts", "schon", "zu", "nah'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "NN", "$,", "VVFIN", "PRF", "ART", "NN", "ADV", "PTKZU", "PTKVZ", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}}}}