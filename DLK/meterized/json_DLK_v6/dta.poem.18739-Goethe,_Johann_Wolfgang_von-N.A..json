{"dta.poem.18739": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewarum ist Wahrheit fern und weit?", "tokens": ["\u201e", "wa\u00b7rum", "ist", "Wahr\u00b7heit", "fern", "und", "weit", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Birgt sich hinab in tiefste Gr\u00fcnde?\u201c", "tokens": ["Birgt", "sich", "hin\u00b7ab", "in", "tiefs\u00b7te", "Gr\u00fcn\u00b7de", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Niemand verstehet zur rechten Zeit! \u2014", "tokens": ["Nie\u00b7mand", "ver\u00b7ste\u00b7het", "zur", "rech\u00b7ten", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wenn man zur rechten Zeit verst\u00fcnde;", "tokens": ["Wenn", "man", "zur", "rech\u00b7ten", "Zeit", "ver\u00b7st\u00fcn\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So w\u00e4re Wahrheit nah und breit,", "tokens": ["So", "w\u00e4\u00b7re", "Wahr\u00b7heit", "nah", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00e4re lieblich und gelinde.", "tokens": ["Und", "w\u00e4\u00b7re", "lieb\u00b7lich", "und", "ge\u00b7lin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "KON", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}