{"textgrid.poem.26523": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du meinst, weil ich ein Dichter bin, so w\u00e4re", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du meinst, weil ich ein Dichter bin, so w\u00e4re", "tokens": ["Du", "meinst", ",", "weil", "ich", "ein", "Dich\u00b7ter", "bin", ",", "so", "w\u00e4\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$,", "ADV", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Dasein reich an hohen Weihestunden,", "tokens": ["Mein", "Da\u00b7sein", "reich", "an", "ho\u00b7hen", "Wei\u00b7hes\u00b7tun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und in mir selbst h\u00e4tt' ich den Lohn gefunden,", "tokens": ["Und", "in", "mir", "selbst", "h\u00e4tt'", "ich", "den", "Lohn", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ob ich die ird'schen G\u00fcter auch entbehre.", "tokens": ["Ob", "ich", "die", "ird'\u00b7schen", "G\u00fc\u00b7ter", "auch", "ent\u00b7beh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vielleicht, mein Freund! Doch h\u00f6r die bitt're Lehre:", "tokens": ["Viel\u00b7leicht", ",", "mein", "Freund", "!", "Doch", "h\u00f6r", "die", "bitt'\u00b7re", "Leh\u00b7re", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$.", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das \u00e4chte Schaffen wird als Qual empfunden;", "tokens": ["Das", "\u00e4ch\u00b7te", "Schaf\u00b7fen", "wird", "als", "Qual", "emp\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "KOKOM", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn ist man auch zum Fluge schon entbunden,", "tokens": ["Denn", "ist", "man", "auch", "zum", "Flu\u00b7ge", "schon", "ent\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "ADV", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fchlt man den Kampf stets mit der eig'nen Schwere.", "tokens": ["F\u00fchlt", "man", "den", "Kampf", "stets", "mit", "der", "eig'\u00b7nen", "Schwe\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wie \u2013 hab' ich auch nicht umsonst gerungen \u2013", "tokens": ["Und", "wie", "\u2013", "hab'", "ich", "auch", "nicht", "um\u00b7sonst", "ge\u00b7run\u00b7gen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$(", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kann ich mich freu'n am fast verwais'ten Streben,", "tokens": ["Kann", "ich", "mich", "freu'n", "am", "fast", "ver\u00b7wais'\u00b7ten", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "NE", "APPRART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von l\u00e4rmenden Erfolgen rings umklungen?", "tokens": ["Von", "l\u00e4r\u00b7men\u00b7den", "Er\u00b7fol\u00b7gen", "rings", "um\u00b7klun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Unsterblichkeit \u2013 wer kann sie je erleben!", "tokens": ["U\u00b7nsterb\u00b7lich\u00b7keit", "\u2013", "wer", "kann", "sie", "je", "er\u00b7le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Glaub' mir: den Lorbeer h\u00e4tt' ich, nothbezwungen,", "tokens": ["Glaub'", "mir", ":", "den", "Lor\u00b7beer", "h\u00e4tt'", "ich", ",", "noth\u00b7be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ART", "NN", "VAFIN", "PPER", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gern f\u00fcr ein Linsenmus oft hingegeben.", "tokens": ["Gern", "f\u00fcr", "ein", "Lin\u00b7sen\u00b7mus", "oft", "hin\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}