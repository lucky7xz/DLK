{"dta.poem.12775": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Das klagende Goldberg,  \n Bey  Tit.  Herrn Johann Preusses,  \n  Diaconi  der kirchen zu Goldberg,  \n todes-falle.  \n D. M. B.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So blitzt der himmel nun auf meine mauren lo\u00df?", "tokens": ["So", "blitzt", "der", "him\u00b7mel", "nun", "auf", "mei\u00b7ne", "mau\u00b7ren", "lo\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Versetzt er keil auf keil mit seinen donner-wettern?", "tokens": ["Ver\u00b7setzt", "er", "keil", "auf", "keil", "mit", "sei\u00b7nen", "don\u00b7ner\u00b7wet\u00b7tern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "APPR", "PIAT", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Will er denn meinen berg itzt gantz und gar zerschmettern?", "tokens": ["Will", "er", "denn", "mei\u00b7nen", "berg", "itzt", "gantz", "und", "gar", "zer\u00b7schmet\u00b7tern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "ADV", "ADV", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "O mehr als harter schlag! o flammen-voller sto\u00df!", "tokens": ["O", "mehr", "als", "har\u00b7ter", "schlag", "!", "o", "flam\u00b7men\u00b7vol\u00b7ler", "sto\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "KOKOM", "ADJA", "NN", "$.", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn \u00fcber meiner stadt so offte feuer knallen,", "tokens": ["Wenn", "\u00fc\u00b7ber", "mei\u00b7ner", "stadt", "so", "off\u00b7te", "feu\u00b7er", "knal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mu\u00df der erschellte bau in asch und staub verfallen.", "tokens": ["Mu\u00df", "der", "er\u00b7schell\u00b7te", "bau", "in", "asch", "und", "staub", "ver\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "APPR", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein haupt ist noch entzwey, ich wei\u00df nicht wo ich bin,", "tokens": ["Mein", "haupt", "ist", "noch", "ent\u00b7zwey", ",", "ich", "wei\u00df", "nicht", "wo", "ich", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKVZ", "$,", "PPER", "VVFIN", "PTKNEG", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die wolcken fallen mir auf den bedr\u00e4ngten r\u00fccken,", "tokens": ["Die", "wol\u00b7cken", "fal\u00b7len", "mir", "auf", "den", "be\u00b7dr\u00e4ng\u00b7ten", "r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Jhr ungest\u00fcmer bruch will meinen leib erdr\u00fccken.", "tokens": ["Ihr", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "bruch", "will", "mei\u00b7nen", "leib", "er\u00b7dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "O weh! es ist geschehn! ich sinck in ohnmacht hin!", "tokens": ["O", "weh", "!", "es", "ist", "ge\u00b7schehn", "!", "ich", "sinck", "in", "ohn\u00b7macht", "hin", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PPER", "VAFIN", "VVPP", "$.", "PPER", "PRF", "APPR", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der offt geqvetschte fu\u00df kan l\u00e4nger nicht mehr stehen,", "tokens": ["Der", "offt", "ge\u00b7qvetschte", "fu\u00df", "kan", "l\u00e4n\u00b7ger", "nicht", "mehr", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PTKVZ", "VMFIN", "ADJD", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Weil das, worauf er ruht, in dr\u00fcmmern mu\u00df zergehen.", "tokens": ["Weil", "das", ",", "wo\u00b7rauf", "er", "ruht", ",", "in", "dr\u00fcm\u00b7mern", "mu\u00df", "zer\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPR", "VVINF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mein ungl\u00fccks-brausend meer, treibt wie vorhin sein spiel,", "tokens": ["Mein", "un\u00b7gl\u00fccks\u00b7brau\u00b7send", "meer", ",", "treibt", "wie", "vor\u00b7hin", "sein", "spiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "VVFIN", "KOKOM", "ADV", "VAINF", "VVFIN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Ich wei\u00df die b\u00f6se zeit, ich rechne noch die wochen,", "tokens": ["Ich", "wei\u00df", "die", "b\u00f6\u00b7se", "zeit", ",", "ich", "rech\u00b7ne", "noch", "die", "wo\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da ein erboster wind den mast-baum hat zerbrochen,", "tokens": ["Da", "ein", "er\u00b7bos\u00b7ter", "wind", "den", "mast\u00b7baum", "hat", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df er zersplittert weg von meinem schiffe fiel.", "tokens": ["Da\u00df", "er", "zer\u00b7split\u00b7tert", "weg", "von", "mei\u00b7nem", "schif\u00b7fe", "fiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "O Hertel! Theurer Mann! was hab ich schon vernommen,", "tokens": ["O", "Her\u00b7tel", "!", "Theu\u00b7rer", "Mann", "!", "was", "hab", "ich", "schon", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADJD", "NN", "$.", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Seit da\u00df dein blasser leib ins todten-meer ist kommen;", "tokens": ["Seit", "da\u00df", "dein", "blas\u00b7ser", "leib", "ins", "tod\u00b7ten\u00b7meer", "ist", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Das war der erste sturm, drauf brach die mitter-nacht,", "tokens": ["Das", "war", "der", "ers\u00b7te", "sturm", ",", "drauf", "brach", "die", "mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "PAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "In der die wolcken sich mit finsterni\u00df umzogen,", "tokens": ["In", "der", "die", "wol\u00b7cken", "sich", "mit", "fins\u00b7ter\u00b7ni\u00df", "um\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PRF", "APPR", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Daraus ein starcker knall mit krachen kam geflogen,", "tokens": ["Da\u00b7raus", "ein", "star\u00b7cker", "knall", "mit", "kra\u00b7chen", "kam", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "APPR", "ADJA", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der meinen steuer-mann auch um das leben bracht.", "tokens": ["Der", "mei\u00b7nen", "steu\u00b7er\u00b7mann", "auch", "um", "das", "le\u00b7ben", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "APPR", "PDS", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ach Hallmann! Gro\u00dfer Mann! wie bist du fortgeschwommen,", "tokens": ["Ach", "Hall\u00b7mann", "!", "Gro\u00b7\u00dfer", "Mann", "!", "wie", "bist", "du", "fort\u00b7ge\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "ADJA", "NN", "$.", "PWAV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sind \u00fcber meinen port viel tausend strudel kommen.", "tokens": ["Sind", "\u00fc\u00b7ber", "mei\u00b7nen", "port", "viel", "tau\u00b7send", "stru\u00b7del", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du ancker meiner ruh! o meines kummers trost!", "tokens": ["Du", "an\u00b7cker", "mei\u00b7ner", "ruh", "!", "o", "mei\u00b7nes", "kum\u00b7mers", "trost", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$.", "FM", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ich mochte meine last auf deine schultern legen,", "tokens": ["Ich", "moch\u00b7te", "mei\u00b7ne", "last", "auf", "dei\u00b7ne", "schul\u00b7tern", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "VVFIN", "APPR", "PPOSAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Trotz schrecken, furcht und angst! trotz allen scharffen schl\u00e4gen!", "tokens": ["Trotz", "schre\u00b7cken", ",", "furcht", "und", "angst", "!", "trotz", "al\u00b7len", "scharf\u00b7fen", "schl\u00e4\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "$,", "NN", "KON", "VVPP", "$.", "APPR", "PIS", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und war gleich ha\u00df und neid auf meinen hals erbost,", "tokens": ["Und", "war", "gleich", "ha\u00df", "und", "neid", "auf", "mei\u00b7nen", "hals", "er\u00b7bost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVIMP", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So wuste deine hand das ruder so zu r\u00fchren,", "tokens": ["So", "wus\u00b7te", "dei\u00b7ne", "hand", "das", "ru\u00b7der", "so", "zu", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da\u00df mich kein zwirbel-rad auf klippen konte f\u00fchren;", "tokens": ["Da\u00df", "mich", "kein", "zwir\u00b7bel\u00b7rad", "auf", "klip\u00b7pen", "kon\u00b7te", "f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "APPR", "VVINF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wie dein compa\u00df verr\u00fcckt, ward der durchbohrte kahn", "tokens": ["Wie", "dein", "com\u00b7pa\u00df", "ver\u00b7r\u00fcckt", ",", "ward", "der", "durch\u00b7bohr\u00b7te", "kahn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "ADJD", "$,", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "In einem strengen Nord von wellen hingerissen,", "tokens": ["In", "ei\u00b7nem", "stren\u00b7gen", "Nord", "von", "wel\u00b7len", "hin\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PWAT", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Bald tieff, bald in die h\u00f6h, als wie ein ball, geschmissen,", "tokens": ["Bald", "tieff", ",", "bald", "in", "die", "h\u00f6h", ",", "als", "wie", "ein", "ball", ",", "ge\u00b7schmis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "APPR", "ART", "ADJD", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ja er griff voller zorn den letzten ancker an.", "tokens": ["Ja", "er", "griff", "vol\u00b7ler", "zorn", "den", "letz\u00b7ten", "an\u00b7cker", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Das war mein S\u00fc\u00dfenbach, der aus den todten-k\u00f6pffen", "tokens": ["Das", "war", "mein", "S\u00fc\u00b7\u00dfen\u00b7bach", ",", "der", "aus", "den", "tod\u00b7ten\u00b7k\u00f6pf\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Mich statt der s\u00fc\u00dfen fluth l\u00e4st bittre thr\u00e4nen sch\u00f6pffen.", "tokens": ["Mich", "statt", "der", "s\u00fc\u00b7\u00dfen", "fluth", "l\u00e4st", "bitt\u00b7re", "thr\u00e4\u00b7nen", "sch\u00f6pf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "So schlug der lichte strahl in meinen rath-stuhl ein.", "tokens": ["So", "schlug", "der", "lich\u00b7te", "strahl", "in", "mei\u00b7nen", "ra\u00b7th\u00b7stuhl", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.38": {"text": "Ach w\u00e4r er nur nicht auch in Zions tempel kommen!", "tokens": ["Ach", "w\u00e4r", "er", "nur", "nicht", "auch", "in", "Zi\u00b7ons", "tem\u00b7pel", "kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "APPR", "NE", "NE", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "Daraus er meinen glantz, fast alles gold genommen:", "tokens": ["Da\u00b7raus", "er", "mei\u00b7nen", "glantz", ",", "fast", "al\u00b7les", "gold", "ge\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPOSAT", "NN", "$,", "ADV", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Das sch\u00f6ne heiligthum verlohr der priester schein,", "tokens": ["Das", "sch\u00f6\u00b7ne", "hei\u00b7lig\u00b7thum", "ver\u00b7lohr", "der", "pries\u00b7ter", "schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Als Sper und Stiller sich zu ihren v\u00e4tern machten,", "tokens": ["Als", "Sper", "und", "Stil\u00b7ler", "sich", "zu", "ih\u00b7ren", "v\u00e4\u00b7tern", "mach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Die cantzel und altar in gro\u00dfen kummer brachten.", "tokens": ["Die", "cant\u00b7zel", "und", "al\u00b7tar", "in", "gro\u00b7\u00dfen", "kum\u00b7mer", "brach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ach! Theuren Lehrer! ach! w\u00e4r euch die angst bewust!", "tokens": ["Ach", "!", "Theu\u00b7ren", "Leh\u00b7rer", "!", "ach", "!", "w\u00e4r", "euch", "die", "angst", "be\u00b7wust", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "NN", "$.", "XY", "$.", "VAFIN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Jhr w\u00fcrdet recht best\u00fcrtzt aus euren gr\u00e4bern brechen,",