{"dta.poem.13256": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eder wiederhohlte Ruff bracht mir Verwunderung;", "tokens": ["\u201e", "der", "wie\u00b7der\u00b7hohl\u00b7te", "Ruff", "bracht", "mir", "Ver\u00b7wun\u00b7de\u00b7rung", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ebi\u00df sich um meine Stirn ein junger Adler schwung.", "tokens": ["\u201e", "bi\u00df", "sich", "um", "mei\u00b7ne", "Stirn", "ein", "jun\u00b7ger", "Ad\u00b7ler", "schwung", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRF", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201efast h\u00e4tt er mich erschreckt. Drauf blickt ich nach den H\u00f6hen", "tokens": ["\u201e", "fast", "h\u00e4tt", "er", "mich", "er\u00b7schreckt", ".", "Drauf", "blickt", "ich", "nach", "den", "H\u00f6\u00b7hen"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VAFIN", "PPER", "PRF", "VVPP", "$.", "PAV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "320\u201dUnd sahe, wie, warum das Lust-Geschrey geschehen.", "tokens": ["\"", "Und", "sa\u00b7he", ",", "wie", ",", "wa\u00b7rum", "das", "Lust\u00b7Ge\u00b7schrey", "ge\u00b7sche\u00b7hen", "."], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "$,", "PWAV", "$,", "PWAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eein unerh\u00f6rter Flug von einer Adler-Schaar", "tokens": ["\u201e", "ein", "un\u00b7er\u00b7h\u00f6r\u00b7ter", "Flug", "von", "ei\u00b7ner", "Ad\u00b7ler\u00b7Schaar"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ebracht meinem Augen-Blick ein fremdes Lust-Spiel dar;", "tokens": ["\u201e", "bracht", "mei\u00b7nem", "Au\u00b7gen\u00b7Blick", "ein", "frem\u00b7des", "Lust\u00b7Spiel", "dar", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eda sie sich hin und her noch auf die D\u00e4cher sezen,", "tokens": ["\u201e", "da", "sie", "sich", "hin", "und", "her", "noch", "auf", "die", "D\u00e4\u00b7cher", "se\u00b7zen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PRF", "PTKVZ", "KON", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eund fast die ganze Stadt durch ihren Scherz erg\u00f6zen.", "tokens": ["\u201e", "und", "fast", "die", "gan\u00b7ze", "Stadt", "durch", "ih\u00b7ren", "Scherz", "er\u00b7g\u00f6\u00b7zen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "325\u201dSie fliegen auf den Thurn, auf Fenster, auf das Dach;", "tokens": ["\"", "Sie", "flie\u00b7gen", "auf", "den", "Thurn", ",", "auf", "Fens\u00b7ter", ",", "auf", "das", "Dach", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "ART", "NN", "$,", "APPR", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eder diesem, jener dem, der einem andern nach.", "tokens": ["\u201e", "der", "die\u00b7sem", ",", "je\u00b7ner", "dem", ",", "der", "ei\u00b7nem", "an\u00b7dern", "nach", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "$,", "PDAT", "ART", "$,", "PRELS", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201esie laufen, heben sich, sie schlagen mit den Schwingen,", "tokens": ["\u201e", "sie", "lau\u00b7fen", ",", "he\u00b7ben", "sich", ",", "sie", "schla\u00b7gen", "mit", "den", "Schwin\u00b7gen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVINF", "$,", "VVFIN", "PRF", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eals w\u00e4ren sie bem\u00fcht, die Freude vorzubringen.", "tokens": ["\u201e", "als", "w\u00e4\u00b7ren", "sie", "be\u00b7m\u00fcht", ",", "die", "Freu\u00b7de", "vor\u00b7zu\u00b7brin\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "VAFIN", "PPER", "VVFIN", "$,", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}