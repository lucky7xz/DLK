{"textgrid.poem.36204": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "R\u00fcckkehr zum Licht", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unsre Erde liebt den \u00c4ther,", "tokens": ["Uns\u00b7re", "Er\u00b7de", "liebt", "den", "\u00c4\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6chte gern der Sonne nahn.", "tokens": ["M\u00f6ch\u00b7te", "gern", "der", "Son\u00b7ne", "nahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Starres Eisen ward lebendig,", "tokens": ["Star\u00b7res", "Ei\u00b7sen", "ward", "le\u00b7ben\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als das Licht hernieder kam,", "tokens": ["Als", "das", "Licht", "her\u00b7nie\u00b7der", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heil'ges Licht der heil'gen Sonne,", "tokens": ["Heil'\u00b7ges", "Licht", "der", "heil'\u00b7gen", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und uns alles Sch\u00f6ne gab.", "tokens": ["Und", "uns", "al\u00b7les", "Sch\u00f6\u00b7ne", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "K\u00fchne Felsen trieb die Tiefe,", "tokens": ["K\u00fch\u00b7ne", "Fel\u00b7sen", "trieb", "die", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hohe L\u00fcfte schwebten nah,", "tokens": ["Ho\u00b7he", "L\u00fcf\u00b7te", "schweb\u00b7ten", "nah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Von dem \u00c4ther abgesendet", "tokens": ["Von", "dem", "\u00c4\u00b7ther", "ab\u00b7ge\u00b7sen\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Um die gro\u00dfe Braut zu fahn.", "tokens": ["Um", "die", "gro\u00b7\u00dfe", "Braut", "zu", "fahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Scham macht rot den blauen Schleier,", "tokens": ["Scham", "macht", "rot", "den", "blau\u00b7en", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "In den Adern rinnt Metall,", "tokens": ["In", "den", "A\u00b7dern", "rinnt", "Me\u00b7tall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Edelsteine blitzen unten,", "tokens": ["E\u00b7del\u00b7stei\u00b7ne", "blit\u00b7zen", "un\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und in Wolken bl\u00fcht der Strahl.", "tokens": ["Und", "in", "Wol\u00b7ken", "bl\u00fcht", "der", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "S\u00fc\u00dfes Blut durchdringt die Glieder,", "tokens": ["S\u00fc\u00b7\u00dfes", "Blut", "durch\u00b7dringt", "die", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Flammen rieseln unsichtbar,", "tokens": ["Flam\u00b7men", "rie\u00b7seln", "un\u00b7sicht\u00b7bar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Sehnsucht schwellt die \u00fcpp'gen H\u00fcgel,", "tokens": ["Sehn\u00b7sucht", "schwellt", "die", "\u00fcpp'\u00b7gen", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Gr\u00fcne F\u00fclle quillt im Tal,", "tokens": ["Gr\u00fc\u00b7ne", "F\u00fcl\u00b7le", "quillt", "im", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Und es spielen bunte Tiere,", "tokens": ["Und", "es", "spie\u00b7len", "bun\u00b7te", "Tie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Wo den Scho\u00df der \u00c4ther traf.", "tokens": ["Wo", "den", "Scho\u00df", "der", "\u00c4\u00b7ther", "traf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Pflanzen, Tiere und Metall", "tokens": ["Pflan\u00b7zen", ",", "Tie\u00b7re", "und", "Me\u00b7tall"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Atmen nur des Lichtes Kraft;", "tokens": ["At\u00b7men", "nur", "des", "Lich\u00b7tes", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Andre Wesen leuchten anders,", "tokens": ["And\u00b7re", "We\u00b7sen", "leuch\u00b7ten", "an\u00b7ders", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Mancher Schein von Einem Strahl.", "tokens": ["Man\u00b7cher", "Schein", "von", "Ei\u00b7nem", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Leichtes Eisen, fester \u00c4ther,", "tokens": ["Leich\u00b7tes", "Ei\u00b7sen", ",", "fes\u00b7ter", "\u00c4\u00b7ther", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Steht der Mensch vollendet da;", "tokens": ["Steht", "der", "Mensch", "voll\u00b7en\u00b7det", "da", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "In dem Antlitz gl\u00e4nzt die Erde,", "tokens": ["In", "dem", "Ant\u00b7litz", "gl\u00e4nzt", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Und zur Sonne will die Tat.", "tokens": ["Und", "zur", "Son\u00b7ne", "will", "die", "Tat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VMFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Wo die Farben wieder Eins,", "tokens": ["Wo", "die", "Far\u00b7ben", "wie\u00b7der", "Eins", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Wird das Licht sich selber klar,", "tokens": ["Wird", "das", "Licht", "sich", "sel\u00b7ber", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PRF", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Denket mutig auf die R\u00fcckkehr,", "tokens": ["Den\u00b7ket", "mu\u00b7tig", "auf", "die", "R\u00fcck\u00b7kehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Wann der Heimat es gewahrt.", "tokens": ["Wann", "der", "Hei\u00b7mat", "es", "ge\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Frohe Zeichen schaut das Auge,", "tokens": ["Fro\u00b7he", "Zei\u00b7chen", "schaut", "das", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Wo das k\u00fchne Leben wallt,", "tokens": ["Wo", "das", "k\u00fch\u00b7ne", "Le\u00b7ben", "wallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Wo die wilde Erdenf\u00fclle", "tokens": ["Wo", "die", "wil\u00b7de", "Er\u00b7den\u00b7f\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Sch\u00f6n vereint ist zum Gesang;", "tokens": ["Sch\u00f6n", "ver\u00b7eint", "ist", "zum", "Ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Da erinnert an die Sonne", "tokens": ["Da", "e\u00b7rin\u00b7nert", "an", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.38": {"text": "Uns ihr Abglanz, die Gestalt.", "tokens": ["Uns", "ihr", "Ab\u00b7glanz", ",", "die", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Freier regt sich dann die Liebe", "tokens": ["Frei\u00b7er", "regt", "sich", "dann", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Die so tief verschlossen lag;", "tokens": ["Die", "so", "tief", "ver\u00b7schlos\u00b7sen", "lag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Wo die Sch\u00f6nheit angesprochen,", "tokens": ["Wo", "die", "Sch\u00f6n\u00b7heit", "an\u00b7ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Hatte Liebe schon gefragt.", "tokens": ["Hat\u00b7te", "Lie\u00b7be", "schon", "ge\u00b7fragt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Wenn das Herz in sch\u00f6ner Liebe", "tokens": ["Wenn", "das", "Herz", "in", "sch\u00f6\u00b7ner", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "K\u00fchnlich schwebet gleich dem Aar,", "tokens": ["K\u00fchn\u00b7lich", "schwe\u00b7bet", "gleich", "dem", "Aar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Str\u00f6met hoch die Fantasie,", "tokens": ["Str\u00f6\u00b7met", "hoch", "die", "Fan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "Wie die Flamme vom Altar.", "tokens": ["Wie", "die", "Flam\u00b7me", "vom", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Was der Geist so hell gedichtet", "tokens": ["Was", "der", "Geist", "so", "hell", "ge\u00b7dich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "ADV", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Lebet ewig fest und wahr;", "tokens": ["Le\u00b7bet", "e\u00b7wig", "fest", "und", "wahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Und zur Sonne kehrt das Licht,", "tokens": ["Und", "zur", "Son\u00b7ne", "kehrt", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.50": {"text": "Wo das heil'ge rein und klar.", "tokens": ["Wo", "das", "heil'\u00b7ge", "rein", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}