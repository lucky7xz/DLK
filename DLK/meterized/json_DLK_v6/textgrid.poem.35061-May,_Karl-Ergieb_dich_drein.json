{"textgrid.poem.35061": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Ergieb dich drein", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ergieb dich drein, du liebes Menschenkind,", "tokens": ["Er\u00b7gieb", "dich", "drein", ",", "du", "lie\u00b7bes", "Men\u00b7schen\u00b7kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df deine Wege nicht die meinen sind.", "tokens": ["Da\u00df", "dei\u00b7ne", "We\u00b7ge", "nicht", "die", "mei\u00b7nen", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "ART", "PPOSAT", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es kann nicht Alles so, wie du willst, sein;", "tokens": ["Es", "kann", "nicht", "Al\u00b7les", "so", ",", "wie", "du", "willst", ",", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PIS", "ADV", "$,", "PWAV", "PPER", "VMFIN", "$,", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du bist nicht Herr; ergieb dich ruhig drein!", "tokens": ["Du", "bist", "nicht", "Herr", ";", "er\u00b7gieb", "dich", "ru\u00b7hig", "drein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "$.", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ergieb dich drein, und forsch und hadre nicht;", "tokens": ["Er\u00b7gieb", "dich", "drein", ",", "und", "forsch", "und", "had\u00b7re", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "KON", "ADJD", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Thu, was die heilge Stimme in dir spricht.", "tokens": ["Thu", ",", "was", "die", "heil\u00b7ge", "Stim\u00b7me", "in", "dir", "spricht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Sie fl\u00fcstert dir das einzig Richtge ein;", "tokens": ["Sie", "fl\u00fcs\u00b7tert", "dir", "das", "ein\u00b7zig", "Richt\u00b7ge", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie t\u00e4uscht dich nicht; ergieb dich ruhig drein!", "tokens": ["Sie", "t\u00e4uscht", "dich", "nicht", ";", "er\u00b7gieb", "dich", "ru\u00b7hig", "drein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ergieb dich drein. Beschwerlich ist der Steg,", "tokens": ["Er\u00b7gieb", "dich", "drein", ".", "Be\u00b7schwer\u00b7lich", "ist", "der", "Steg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$.", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der deiner harrt, fernab vom breiten Weg.", "tokens": ["Der", "dei\u00b7ner", "harrt", ",", "fern\u00b7ab", "vom", "brei\u00b7ten", "Weg", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "VVFIN", "$,", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schl\u00e4gst du ihn ein, schl\u00e4gst du ihn gl\u00e4ubig ein,", "tokens": ["Schl\u00e4gst", "du", "ihn", "ein", ",", "schl\u00e4gst", "du", "ihn", "gl\u00e4u\u00b7big", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So wird er dir ein Pfad zum Himmel sein!", "tokens": ["So", "wird", "er", "dir", "ein", "Pfad", "zum", "Him\u00b7mel", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ART", "NN", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}