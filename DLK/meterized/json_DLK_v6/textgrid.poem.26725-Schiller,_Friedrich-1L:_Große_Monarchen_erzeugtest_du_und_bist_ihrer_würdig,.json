{"textgrid.poem.26725": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gro\u00dfe Monarchen erzeugtest du und bist ihrer w\u00fcrdig,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00dfe Monarchen erzeugtest du und bist ihrer w\u00fcrdig,", "tokens": ["Gro\u00b7\u00dfe", "Mon\u00b7ar\u00b7chen", "er\u00b7zeug\u00b7test", "du", "und", "bist", "ih\u00b7rer", "w\u00fcr\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "KON", "VAFIN", "PPOSAT", "ADJD", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Den Gebietenden macht nur der Gehorchende gro\u00df.", "tokens": ["Den", "Ge\u00b7bie\u00b7ten\u00b7den", "macht", "nur", "der", "Ge\u00b7hor\u00b7chen\u00b7de", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Aber versuch es, o Deutschland, und mach es deinen Beherrschern", "tokens": ["A\u00b7ber", "ver\u00b7such", "es", ",", "o", "Deutschland", ",", "und", "mach", "es", "dei\u00b7nen", "Be\u00b7herr\u00b7schern"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "$,", "FM", "NE", "$,", "KON", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Schwerer, als K\u00f6nige gro\u00df, leichter, nur Menschen zu sein!", "tokens": ["Schwe\u00b7rer", ",", "als", "K\u00f6\u00b7ni\u00b7ge", "gro\u00df", ",", "leich\u00b7ter", ",", "nur", "Men\u00b7schen", "zu", "sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "p