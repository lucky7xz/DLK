{"textgrid.poem.67101": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An die klugen Ratgeber", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sollte nicht im Lebensfelde ringen,", "tokens": ["Ich", "soll\u00b7te", "nicht", "im", "Le\u00b7bens\u00b7fel\u00b7de", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Solang mein Herz nach h\u00f6chster Sch\u00f6ne strebt,", "tokens": ["So\u00b7lang", "mein", "Herz", "nach", "h\u00f6chs\u00b7ter", "Sch\u00f6\u00b7ne", "strebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich soll mein Schwanenlied am Grabe singen,", "tokens": ["Ich", "soll", "mein", "Schwa\u00b7nen\u00b7lied", "am", "Gra\u00b7be", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo ihr so gern lebendig uns begr\u00e4bt?", "tokens": ["Wo", "ihr", "so", "gern", "le\u00b7ben\u00b7dig", "uns", "be\u00b7gr\u00e4bt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O! schonet mein und la\u00dft das rege Streben,", "tokens": ["O", "!", "scho\u00b7net", "mein", "und", "la\u00dft", "das", "re\u00b7ge", "Stre\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPOSAT", "KON", "VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bis seine Flut ins fernste Meer sich st\u00fcrzt,", "tokens": ["Bis", "sei\u00b7ne", "Flut", "ins", "ferns\u00b7te", "Meer", "sich", "st\u00fcrzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "La\u00dft immerhin, ihr \u00c4rzte, la\u00dft mich leben,", "tokens": ["La\u00dft", "im\u00b7mer\u00b7hin", ",", "ihr", "\u00c4rz\u00b7te", ",", "la\u00dft", "mich", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPOSAT", "NN", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Solang die Parze nicht die Bahn verk\u00fcrzt.", "tokens": ["So\u00b7lang", "die", "Par\u00b7ze", "nicht", "die", "Bahn", "ver\u00b7k\u00fcrzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Des Weins Gew\u00e4chs verschm\u00e4ht die k\u00fchlen Tale,", "tokens": ["Des", "Weins", "Ge\u00b7w\u00e4chs", "ver\u00b7schm\u00e4ht", "die", "k\u00fch\u00b7len", "Ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hesperiens begl\u00fcckter Garten bringt", "tokens": ["Hes\u00b7pe\u00b7ri\u00b7ens", "be\u00b7gl\u00fcck\u00b7ter", "Gar\u00b7ten", "bringt"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die goldnen Fr\u00fcchte nur im hei\u00dfen Strahle,", "tokens": ["Die", "gold\u00b7nen", "Fr\u00fcch\u00b7te", "nur", "im", "hei\u00b7\u00dfen", "Strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der, wie ein Pfeil, ins Herz der Erde dringt;", "tokens": ["Der", ",", "wie", "ein", "Pfeil", ",", "ins", "Herz", "der", "Er\u00b7de", "dringt", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ART", "NN", "$,", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was warnt ihr dann, wenn stolz und ungesch\u00e4ndet", "tokens": ["Was", "warnt", "ihr", "dann", ",", "wenn", "stolz", "und", "un\u00b7ge\u00b7sch\u00e4n\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "KOUS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Menschen Herz von k\u00fchnem Zorn entbrennt,", "tokens": ["Des", "Men\u00b7schen", "Herz", "von", "k\u00fch\u00b7nem", "Zorn", "ent\u00b7brennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Was nimmt ihr ihm, der nur im Kampf vollendet,", "tokens": ["Was", "nimmt", "ihr", "ihm", ",", "der", "nur", "im", "Kampf", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$,", "PRELS", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ihr Weichlinge, sein gl\u00fchend Element?", "tokens": ["Ihr", "Weich\u00b7lin\u00b7ge", ",", "sein", "gl\u00fc\u00b7hend", "E\u00b7le\u00b7ment", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "NN", "$."], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Er hat das Schwert zum Spiele nicht genommen,", "tokens": ["Er", "hat", "das", "Schwert", "zum", "Spie\u00b7le", "nicht", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Richter, der die alte Nacht verdammt,", "tokens": ["Der", "Rich\u00b7ter", ",", "der", "die", "al\u00b7te", "Nacht", "ver\u00b7dammt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er ist zum Schlafe nicht herabgekommen,", "tokens": ["Er", "ist", "zum", "Schla\u00b7fe", "nicht", "her\u00b7ab\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der reine Geist, der aus dem Aether stammt;", "tokens": ["Der", "rei\u00b7ne", "Geist", ",", "der", "aus", "dem", "A\u00b7e\u00b7ther", "stammt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Er strahlt heran, er schr\u00f6ckt, wie Meteore,", "tokens": ["Er", "strahlt", "he\u00b7ran", ",", "er", "schr\u00f6ckt", ",", "wie", "Me\u00b7te\u00b7o\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "$,", "PWAV", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Befreit und b\u00e4ndigt, ohne Ruh und Sold,", "tokens": ["Be\u00b7freit", "und", "b\u00e4n\u00b7digt", ",", "oh\u00b7ne", "Ruh", "und", "Sold", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bis, wiederkehrend durch des Himmels Tore,", "tokens": ["Bis", ",", "wie\u00b7der\u00b7keh\u00b7rend", "durch", "des", "Him\u00b7mels", "To\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVPP", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sein K\u00e4mpferwagen im Triumphe rollt.", "tokens": ["Sein", "K\u00e4mp\u00b7fer\u00b7wa\u00b7gen", "im", "Tri\u00b7um\u00b7phe", "rollt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und ihr, ihr wollt des R\u00e4chers Arme l\u00e4hmen,", "tokens": ["Und", "ihr", ",", "ihr", "wollt", "des", "R\u00e4\u00b7chers", "Ar\u00b7me", "l\u00e4h\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VMFIN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Geiste, der mit G\u00f6tterrecht gebeut,", "tokens": ["Dem", "Geis\u00b7te", ",", "der", "mit", "G\u00f6t\u00b7ter\u00b7recht", "ge\u00b7beut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bedeutet ihr, sich knechtisch zu bequemen,", "tokens": ["Be\u00b7deu\u00b7tet", "ihr", ",", "sich", "knech\u00b7tisch", "zu", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRF", "ADJD", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach eures P\u00f6bels Unerbittlichkeit?", "tokens": ["Nach", "eu\u00b7res", "P\u00f6\u00b7bels", "Un\u00b7er\u00b7bitt\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Irrhaus w\u00e4hlt ihr euch zum Tribunale,", "tokens": ["Das", "I\u00b7rrhaus", "w\u00e4hlt", "ihr", "euch", "zum", "Tri\u00b7bu\u00b7na\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem soll der Herrliche sich unterziehn,", "tokens": ["Dem", "soll", "der", "Herr\u00b7li\u00b7che", "sich", "un\u00b7ter\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den Gott in uns, den macht ihr zum Skandale,", "tokens": ["Den", "Gott", "in", "uns", ",", "den", "macht", "ihr", "zum", "Skan\u00b7da\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "ART", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und setzt den Wurm zum K\u00f6nig \u00fcber ihn. \u2013", "tokens": ["Und", "setzt", "den", "Wurm", "zum", "K\u00f6\u00b7nig", "\u00fc\u00b7ber", "ihn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sonst ward der Schw\u00e4rmer doch ans Kreuz geschlagen,", "tokens": ["Sonst", "ward", "der", "Schw\u00e4r\u00b7mer", "doch", "ans", "Kreuz", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und oft in edlem L\u00f6wengrimme rang", "tokens": ["Und", "oft", "in", "ed\u00b7lem", "L\u00f6\u00b7wen\u00b7grim\u00b7me", "rang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Mensch an donnernden Entscheidungstagen,", "tokens": ["Der", "Mensch", "an", "don\u00b7nern\u00b7den", "Ent\u00b7schei\u00b7dungs\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis Gl\u00fcck und Wut das k\u00fchne Recht bezwang;", "tokens": ["Bis", "Gl\u00fcck", "und", "Wut", "das", "k\u00fch\u00b7ne", "Recht", "be\u00b7zwang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ach! wie die Sonne, sank zur Ruhe nieder,", "tokens": ["Ach", "!", "wie", "die", "Son\u00b7ne", ",", "sank", "zur", "Ru\u00b7he", "nie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "ART", "NN", "$,", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wer unter Kampf ein herrlich Werk begann,", "tokens": ["Wer", "un\u00b7ter", "Kampf", "ein", "herr\u00b7lich", "Werk", "be\u00b7gann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er sank und morgenr\u00f6tlich hub er wieder", "tokens": ["Er", "sank", "und", "mor\u00b7gen\u00b7r\u00f6t\u00b7lich", "hub", "er", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "ADJD", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In seinen Lieblingen zu leuchten an.", "tokens": ["In", "sei\u00b7nen", "Lieb\u00b7lin\u00b7gen", "zu", "leuch\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVFIN", "PTKVZ", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Jetzt bl\u00fcht die neue Kunst, das Herz zu morden,", "tokens": ["Jetzt", "bl\u00fcht", "die", "neu\u00b7e", "Kunst", ",", "das", "Herz", "zu", "mor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Todesdolch in meuchlerischer Hand", "tokens": ["Zum", "To\u00b7des\u00b7dolch", "in", "meuch\u00b7le\u00b7ri\u00b7scher", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist nun der Rat des klugen Manns geworden,", "tokens": ["Ist", "nun", "der", "Rat", "des", "klu\u00b7gen", "Manns", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und furchtbar, wie ein Scherge, der Verstand;", "tokens": ["Und", "furcht\u00b7bar", ",", "wie", "ein", "Scher\u00b7ge", ",", "der", "Ver\u00b7stand", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bekehrt von euch zu feiger Ruhe, findet", "tokens": ["Be\u00b7kehrt", "von", "euch", "zu", "fei\u00b7ger", "Ru\u00b7he", ",", "fin\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVPP", "APPR", "PPER", "APPR", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Geist der J\u00fcnglinge sein schm\u00e4hlich Grab,", "tokens": ["Der", "Geist", "der", "J\u00fcng\u00b7lin\u00b7ge", "sein", "schm\u00e4h\u00b7lich", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Ach! ruhmlos in di