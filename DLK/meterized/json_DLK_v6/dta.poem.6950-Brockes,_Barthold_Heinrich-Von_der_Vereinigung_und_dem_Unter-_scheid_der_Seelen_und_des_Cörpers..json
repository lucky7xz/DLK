{"dta.poem.6950": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein, man lehre mich,", "tokens": ["Al\u00b7lein", ",", "man", "leh\u00b7re", "mich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PIS", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "An welchem Ort, auf welcher Stelle sich", "tokens": ["An", "wel\u00b7chem", "Ort", ",", "auf", "wel\u00b7cher", "Stel\u00b7le", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "$,", "APPR", "PWAT", "NN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Handlung endiget, die blos nur c\u00f6rperlich,", "tokens": ["Die", "Hand\u00b7lung", "en\u00b7di\u00b7get", ",", "die", "blos", "nur", "c\u00f6r\u00b7per\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und, wenn der Leiblichen ihr Ziel bestimmet,", "tokens": ["Und", ",", "wenn", "der", "Leib\u00b7li\u00b7chen", "ihr", "Ziel", "be\u00b7stim\u00b7met", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wo dann die Geistige den Anfang nimmt?", "tokens": ["Wo", "dann", "die", "Geis\u00b7ti\u00b7ge", "den", "An\u00b7fang", "nimmt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Auf welche Weis\u2019 ein Wesen, das vorhin", "tokens": ["Auf", "wel\u00b7che", "Weis'", "ein", "We\u00b7sen", ",", "das", "vor\u00b7hin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PWAT", "NN", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein Stoff war, ohn Gef\u00fchl und sonder Sinn;", "tokens": ["Ein", "Stoff", "war", ",", "ohn", "Ge\u00b7f\u00fchl", "und", "son\u00b7der", "Sinn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "KOUI", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Jm Augenblick kan f\u00fchlen und gedencken:", "tokens": ["Jm", "Au\u00b7gen\u00b7blick", "kan", "f\u00fch\u00b7len", "und", "ge\u00b7den\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "VVINF", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wird auch ein eitler Geist, verf\u00fchrt durch falschen Schein,", "tokens": ["Wird", "auch", "ein", "eit\u00b7ler", "Geist", ",", "ver\u00b7f\u00fchrt", "durch", "fal\u00b7schen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Aus diesem Abgrund sich zu retten m\u00e4chtig seyn?", "tokens": ["Aus", "die\u00b7sem", "Ab\u00b7grund", "sich", "zu", "ret\u00b7ten", "m\u00e4ch\u00b7tig", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PRF", "PTKZU", "VVINF", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}