{"dta.poem.3372": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Orpheus Hymne  \n  an den Schlaf .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schlaf, du aller Seligen, aller Sterblichen", "tokens": ["Schlaf", ",", "du", "al\u00b7ler", "Se\u00b7li\u00b7gen", ",", "al\u00b7ler", "Sterb\u00b7li\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PPER", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "K\u00f6nig,", "tokens": ["K\u00f6\u00b7nig", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Alles Lebendigen Herr, was die weite Erd' er-", "tokens": ["Al\u00b7les", "Le\u00b7ben\u00b7di\u00b7gen", "Herr", ",", "was", "die", "wei\u00b7te", "Erd'", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "n\u00e4hret,", "tokens": ["n\u00e4h\u00b7ret", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Der du alles besuchst, und alles besiegst und be-", "tokens": ["Der", "du", "al\u00b7les", "be\u00b7suchst", ",", "und", "al\u00b7les", "be\u00b7siegst", "und", "be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PIS", "VVFIN", "$,", "KON", "PIS", "ADJD", "KON", "TRUNC"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "zwingest,", "tokens": ["zwin\u00b7gest", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Alles Leben umspinnst mit leisefesselndem Netze,", "tokens": ["Al\u00b7les", "Le\u00b7ben", "um\u00b7spinnst", "mit", "lei\u00b7se\u00b7fes\u00b7seln\u00b7dem", "Net\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Alle Sorgen linderst und endest jede Beschwerde,", "tokens": ["Al\u00b7le", "Sor\u00b7gen", "lin\u00b7derst", "und", "en\u00b7dest", "je\u00b7de", "Be\u00b7schwer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Allen Jammer lullst in stillen heiligen Frieden,", "tokens": ["Al\u00b7len", "Jam\u00b7mer", "lullst", "in", "stil\u00b7len", "hei\u00b7li\u00b7gen", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Der du st\u00e4rkest den Geist, und beschw\u00f6rst das", "tokens": ["Der", "du", "st\u00e4r\u00b7kest", "den", "Geist", ",", "und", "be\u00b7schw\u00f6rst", "das"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "ART"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Grauen des Todes,", "tokens": ["Grau\u00b7en", "des", "To\u00b7des", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Deines leiblichen Bruders, und deiner Schwester,", "tokens": ["Dei\u00b7nes", "leib\u00b7li\u00b7chen", "Bru\u00b7ders", ",", "und", "dei\u00b7ner", "Schwes\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "der Lethe \u2014", "tokens": ["der", "Le\u00b7the"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "H\u00f6re mich, Seliger, komm' in deiner freundlichen", "tokens": ["H\u00f6\u00b7re", "mich", ",", "Se\u00b7li\u00b7ger", ",", "komm'", "in", "dei\u00b7ner", "freund\u00b7li\u00b7chen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "ADJA"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Milde,", "tokens": ["Mil\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Komm' und umfl\u00fcgle mit k\u00fchlendem Fittig den", "tokens": ["Komm'", "und", "um\u00b7fl\u00fcg\u00b7le", "mit", "k\u00fch\u00b7len\u00b7dem", "Fit\u00b7tig", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "KON", "VVFIN", "APPR", "ADJA", "NN", "ART"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.17": {"text": "frommen Fey'rer!", "tokens": ["from\u00b7men", "Fey'\u00b7rer", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}