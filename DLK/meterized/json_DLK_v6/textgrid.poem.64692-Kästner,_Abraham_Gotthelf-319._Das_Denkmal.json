{"textgrid.poem.64692": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "319. Das Denkmal", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der schlecht bezahlt so viel f\u00fcr ihn gedichtet,", "tokens": ["Der", "schlecht", "be\u00b7zahlt", "so", "viel", "f\u00fcr", "ihn", "ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ADV", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Monument hat er dem nun errichtet.", "tokens": ["Ein", "Mo\u00b7nu\u00b7ment", "hat", "er", "dem", "nun", "er\u00b7rich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "H\u00e4tt' er ihm Brot bey seinem Leben,", "tokens": ["H\u00e4tt'", "er", "ihm", "Brot", "bey", "sei\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht nach dem Tode Stein gegeben!", "tokens": ["Nicht", "nach", "dem", "To\u00b7de", "Stein", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}