{"dta.poem.19179": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ohr oder Auge, mit der T\u00f6n' und Farben Flimmer,", "tokens": ["Ohr", "o\u00b7der", "Au\u00b7ge", ",", "mit", "der", "T\u00f6n'", "und", "Far\u00b7ben", "Flim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "APPR", "ART", "NN", "KON", "NN", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Was ist wol besser? was, taub oder blind, ist schlimmer?", "tokens": ["Was", "ist", "wol", "bes\u00b7ser", "?", "was", ",", "taub", "o\u00b7der", "blind", ",", "ist", "schlim\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "$.", "PWS", "$,", "ADJD", "KON", "ADJD", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Auf gleicher Linie sosehr stehn diese beiden", "tokens": ["Auf", "glei\u00b7cher", "Li\u00b7nie", "so\u00b7sehr", "stehn", "die\u00b7se", "bei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN", "PDAT", "PIAT"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im Menschenangesicht, da\u00df schwer ist zu entscheiden.", "tokens": ["Im", "Men\u00b7schen\u00b7an\u00b7ge\u00b7sicht", ",", "da\u00df", "schwer", "ist", "zu", "ent\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ADJD", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Recht entscheidet nicht, entscheide denn nach seiner", "tokens": ["Das", "Recht", "ent\u00b7schei\u00b7det", "nicht", ",", "ent\u00b7schei\u00b7de", "denn", "nach", "sei\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "VVFIN", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vorliebe jeder, ich entscheide so nach meiner:", "tokens": ["Vor\u00b7lie\u00b7be", "je\u00b7der", ",", "ich", "ent\u00b7schei\u00b7de", "so", "nach", "mei\u00b7ner", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "$,", "PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Von blinden Dichtern hab' ich vieles schon gelesen,", "tokens": ["Von", "blin\u00b7den", "Dich\u00b7tern", "hab'", "ich", "vie\u00b7les", "schon", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von keinem gro\u00dfen doch geh\u00f6rt, der taub gewesen.", "tokens": ["Von", "kei\u00b7nem", "gro\u00b7\u00dfen", "doch", "ge\u00b7h\u00f6rt", ",", "der", "taub", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "ADV", "VVFIN", "$,", "PRELS", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}