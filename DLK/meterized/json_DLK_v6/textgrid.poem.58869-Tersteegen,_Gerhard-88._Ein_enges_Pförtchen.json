{"textgrid.poem.58869": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "88. Ein enges Pf\u00f6rtchen", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Pf\u00f6rtchen ist so eng und klein,", "tokens": ["Das", "Pf\u00f6rt\u00b7chen", "ist", "so", "eng", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wodurch man aus sich selbst mu\u00df gehen;", "tokens": ["Wo\u00b7durch", "man", "aus", "sich", "selbst", "mu\u00df", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PRF", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein nackt, gebeugtes Kind allein", "tokens": ["Ein", "nackt", ",", "ge\u00b7beug\u00b7tes", "Kind", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "$,", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kann diesen Ausgang recht verstehen.", "tokens": ["Kann", "die\u00b7sen", "Aus\u00b7gang", "recht", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}