{"textgrid.poem.35474": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Zwischen-Motiv", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In ein Wirtshaus bin ich eingekehrt,", "tokens": ["In", "ein", "Wirts\u00b7haus", "bin", "ich", "ein\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da hat mir keiner den Wein verwehrt.", "tokens": ["Da", "hat", "mir", "kei\u00b7ner", "den", "Wein", "ver\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Haben mich alle aufgenommen,", "tokens": ["Ha\u00b7ben", "mich", "al\u00b7le", "auf\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Als w\u00e4r' ich zu hohem Ro\u00df gekommen.", "tokens": ["Als", "w\u00e4r'", "ich", "zu", "ho\u00b7hem", "Ro\u00df", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Sind alle zu mir heranger\u00fcckt \u2013", "tokens": ["Sind", "al\u00b7le", "zu", "mir", "her\u00b7an\u00b7ge\u00b7r\u00fcckt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "PPER", "VVPP", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Haben uns in die Augen geblickt ...", "tokens": ["Ha\u00b7ben", "uns", "in", "die", "Au\u00b7gen", "ge\u00b7blickt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Haben eng beieinander gesessen,", "tokens": ["Ha\u00b7ben", "eng", "bei\u00b7ein\u00b7an\u00b7der", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "VVPP", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wein geschl\u00fcrft und Schwarzbrot gegessen.", "tokens": ["Wein", "ge\u00b7schl\u00fcrft", "und", "Schwarz\u00b7brot", "ge\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "NN", "VVPP", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Es war im M\u00e4rz und da fiel mir ein:", "tokens": ["Es", "war", "im", "M\u00e4rz", "und", "da", "fiel", "mir", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "KON", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es m\u00fc\u00dfte doch balde Fr\u00fchling sein!", "tokens": ["Es", "m\u00fc\u00df\u00b7te", "doch", "bal\u00b7de", "Fr\u00fch\u00b7ling", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "NN", "VAINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Habe mich auf den Fr\u00fchling gespitzt", "tokens": ["Ha\u00b7be", "mich", "auf", "den", "Fr\u00fch\u00b7ling", "ge\u00b7spitzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "VVPP"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und alles Gr\u00fcbeln hinweggeblitzt!", "tokens": ["Und", "al\u00b7les", "Gr\u00fc\u00b7beln", "hin\u00b7weg\u00b7ge\u00b7blitzt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "War ein Zecher, und nahm es nicht krumm,", "tokens": ["War", "ein", "Ze\u00b7cher", ",", "und", "nahm", "es", "nicht", "krumm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Ging der Schalksnarr im Saale herum ...", "tokens": ["Ging", "der", "Schalks\u00b7narr", "im", "Saa\u00b7le", "he\u00b7rum", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Doch schlie\u00dflich kriegt' ich den Unsinn dick,", "tokens": ["Doch", "schlie\u00df\u00b7lich", "kriegt'", "ich", "den", "Un\u00b7sinn", "dick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und wieder heller ward mein Blick ...", "tokens": ["Und", "wie\u00b7der", "hel\u00b7ler", "ward", "mein", "Blick", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Da hab' ich die Menschlein mir angeguckt", "tokens": ["Da", "hab'", "ich", "die", "Mensc\u00b7hlein", "mir", "an\u00b7ge\u00b7guckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und hinterher weidlich ausgespuckt ...", "tokens": ["Und", "hin\u00b7ter\u00b7her", "weid\u00b7lich", "aus\u00b7ge\u00b7spuckt", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}