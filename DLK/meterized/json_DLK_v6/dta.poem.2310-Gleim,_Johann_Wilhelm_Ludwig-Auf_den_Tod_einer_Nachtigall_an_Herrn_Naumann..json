{"dta.poem.2310": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Tod einer Nachtigall  \n an  \n Herrn Naumann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Singe! Meister starker Lieder,", "tokens": ["Sin\u00b7ge", "!", "Meis\u00b7ter", "star\u00b7ker", "Lie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singe! Prei\u00df der Nachtigallen,", "tokens": ["Sin\u00b7ge", "!", "Prei\u00df", "der", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Singe! Liebling meines Freundes,", "tokens": ["Sin\u00b7ge", "!", "Lieb\u00b7ling", "mei\u00b7nes", "Freun\u00b7des", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die gewohnten Abendlieder.", "tokens": ["Die", "ge\u00b7wohn\u00b7ten", "A\u00b7ben\u00b7dlie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Siehst du nicht? die Spree wird dunkel,", "tokens": ["Siehst", "du", "nicht", "?", "die", "Spree", "wird", "dun\u00b7kel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$.", "ART", "NE", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und es dient ihr helles Ufer", "tokens": ["Und", "es", "dient", "ihr", "hel\u00b7les", "U\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Keiner Sch\u00f6nen mehr zum Spiegel;", "tokens": ["Kei\u00b7ner", "Sch\u00f6\u00b7nen", "mehr", "zum", "Spie\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dennoch kommen sie gepaaret,", "tokens": ["Den\u00b7noch", "kom\u00b7men", "sie", "ge\u00b7paa\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Aus Verlangen dich zu h\u00f6ren,", "tokens": ["Aus", "Ver\u00b7lan\u00b7gen", "dich", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Oder doch aus Lust zum Schatten.", "tokens": ["O\u00b7der", "doch", "aus", "Lust", "zum", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Siehst du nicht, du Freund des Schattens,", "tokens": ["Siehst", "du", "nicht", ",", "du", "Freund", "des", "Schat\u00b7tens", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Siehst du nicht die Sonne weichen?", "tokens": ["Siehst", "du", "nicht", "die", "Son\u00b7ne", "wei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Singe doch! sie geht zur Ruhe;", "tokens": ["Sin\u00b7ge", "doch", "!", "sie", "geht", "zur", "Ru\u00b7he", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Singe doch den Stern zu Grabe.", "tokens": ["Sin\u00b7ge", "doch", "den", "Stern", "zu", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Vogel! nein, bei todten Gr\u00e4bern", "tokens": ["Vo\u00b7gel", "!", "nein", ",", "bei", "tod\u00b7ten", "Gr\u00e4\u00b7bern"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PTKANT", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Kannst du deine Lieder sparen.", "tokens": ["Kannst", "du", "dei\u00b7ne", "Lie\u00b7der", "spa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Nein, du bist kein Leichens\u00e4nger.", "tokens": ["Nein", ",", "du", "bist", "kein", "Lei\u00b7chen\u00b7s\u00e4n\u00b7ger", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Du besch\u00e4m\u2019st mit frohen T\u00f6nen", "tokens": ["Du", "be\u00b7sch\u00e4m'st", "mit", "fro\u00b7hen", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Tausend Opers\u00e4ngerinnen;", "tokens": ["Tau\u00b7send", "O\u00b7per\u00b7s\u00e4n\u00b7ge\u00b7rin\u00b7nen", ";"], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Du besingst nur Scherz und Liebe", "tokens": ["Du", "be\u00b7singst", "nur", "Scherz", "und", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Und das Volk im stillen Schatten,", "tokens": ["Und", "das", "Volk", "im", "stil\u00b7len", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Das f\u00fcr neue Leichen sorget.", "tokens": ["Das", "f\u00fcr", "neu\u00b7e", "Lei\u00b7chen", "sor\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Soll ich meine Doris holen?", "tokens": ["Soll", "ich", "mei\u00b7ne", "Do\u00b7ris", "ho\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NE", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Oder soll mein Freund im Schatten", "tokens": ["O\u00b7der", "soll", "mein", "Freund", "im", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Eine Sch\u00e4ferinn vers\u00f6nen?", "tokens": ["Ei\u00b7ne", "Sch\u00e4\u00b7fe\u00b7rinn", "ver\u00b7s\u00f6\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Nachtigall! dann wirst du singen.", "tokens": ["Nach\u00b7ti\u00b7gall", "!", "dann", "wirst", "du", "sin\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Aber wie? du bist so stille.", "tokens": ["A\u00b7ber", "wie", "?", "du", "bist", "so", "stil\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$.", "PPER", "VAFIN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Schl\u00e4fst du? oder bist du traurig?", "tokens": ["Schl\u00e4fst", "du", "?", "o\u00b7der", "bist", "du", "trau\u00b7rig", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "KON", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Denn es regt sich ia kein Fl\u00fcgel.", "tokens": ["Denn", "es", "regt", "sich", "i\u00b7a", "kein", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADV", "PIAT", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.30": {"text": "Freund! du bist noch nicht gestorben,", "tokens": ["Freund", "!", "du", "bist", "noch", "nicht", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "H\u00fcpfe doch so frei, wie gestern.", "tokens": ["H\u00fcp\u00b7fe", "doch", "so", "frei", ",", "wie", "ge\u00b7stern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADJD", "$,", "PWAV", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.32": {"text": "Sieh! dort geht dein Herr gepaaret,", "tokens": ["Sieh", "!", "dort", "geht", "dein", "Herr", "ge\u00b7paa\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Sieh doch! welchen Schatz er f\u00fchret.", "tokens": ["Sieh", "doch", "!", "wel\u00b7chen", "Schatz", "er", "f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$.", "PWAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Wilst du denn kein Brautlied singen?", "tokens": ["Wilst", "du", "denn", "kein", "Braut\u00b7lied", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Nachtigall! bald werd\u2019 ich schelten.", "tokens": ["Nach\u00b7ti\u00b7gall", "!", "bald", "werd'", "ich", "schel\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VAFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "H\u00f6rst du keine K\u00fcsse rauschen?", "tokens": ["H\u00f6rst", "du", "kei\u00b7ne", "K\u00fcs\u00b7se", "rau\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Siehst du keine Z\u00e4rtlichkeiten?", "tokens": ["Siehst", "du", "kei\u00b7ne", "Z\u00e4rt\u00b7lich\u00b7kei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Keine Boten s\u00fcsser Freuden?", "tokens": ["Kei\u00b7ne", "Bo\u00b7ten", "s\u00fcs\u00b7ser", "Freu\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Keine Zeichen der Verliebten?", "tokens": ["Kei\u00b7ne", "Zei\u00b7chen", "der", "Ver\u00b7lieb\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "St\u00f6re sie mit lauten T\u00f6nen", "tokens": ["St\u00f6\u00b7re", "sie", "mit", "lau\u00b7ten", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "In der Reihe des Vergn\u00fcgens.", "tokens": ["In", "der", "Rei\u00b7he", "des", "Ver\u00b7gn\u00fc\u00b7gens", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Sage, wilst du sie nicht st\u00f6ren?", "tokens": ["Sa\u00b7ge", ",", "wilst", "du", "sie", "nicht", "st\u00f6\u00b7ren", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Schweigst du noch? h\u00f6r auf zu schweigen.", "tokens": ["Schweigst", "du", "noch", "?", "h\u00f6r", "auf", "zu", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$.", "VVFIN", "APPR", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Schlage, da\u00df sie sich erschrekken,", "tokens": ["Schla\u00b7ge", ",", "da\u00df", "sie", "sich", "er\u00b7schrek\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "St\u00e4rker, wie die Abendglokke.", "tokens": ["St\u00e4r\u00b7ker", ",", "wie", "die", "A\u00b7bend\u00b7glo\u00b7kke", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.46": {"text": "Hilft kein Bitten? Wilst du trotzen?", "tokens": ["Hilft", "kein", "Bit\u00b7ten", "?", "Wilst", "du", "trot\u00b7zen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$.", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Vogel! soll ich zornig werden?", "tokens": ["Vo\u00b7gel", "!", "soll", "ich", "zor\u00b7nig", "wer\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Bald wird mich dein Schweigen \u00e4rgern.", "tokens": ["Bald", "wird", "mich", "dein", "Schwei\u00b7gen", "\u00e4r\u00b7gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.49": {"text": "Warte nur! man soll dich strafen;", "tokens": ["War\u00b7te", "nur", "!", "man", "soll", "dich", "stra\u00b7fen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "PIS", "VMFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Denn dein Herrr soll auf mein Bitten", "tokens": ["Denn", "dein", "Herrr", "soll", "auf", "mein", "Bit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Dich von deiner Gattin trennen.", "tokens": ["Dich", "von", "dei\u00b7ner", "Gat\u00b7tin", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "H\u00f6re doch ihr z\u00e4rtlichs Girren.", "tokens": ["H\u00f6\u00b7re", "doch", "ihr", "z\u00e4rt\u00b7lichs", "Gir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Du, der stets die Liebe h\u00f6rte,", "tokens": ["Du", ",", "der", "stets", "die", "Lie\u00b7be", "h\u00f6r\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Wilst du sie denn ietzt nicht h\u00f6ren?", "tokens": ["Wilst", "du", "sie", "denn", "ietzt", "nicht", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Doris! komm nur mit der Kerze,", "tokens": ["Do\u00b7ris", "!", "komm", "nur", "mit", "der", "Ker\u00b7ze", ","], "token_info": ["word", "punct", "w