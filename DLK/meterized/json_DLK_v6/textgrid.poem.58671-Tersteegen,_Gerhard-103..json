{"textgrid.poem.58671": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "103.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O gro\u00dfer Tag der Reinigung, da Gott durch tiefes Kreuz und Leiden", "tokens": ["O", "gro\u00b7\u00dfer", "Tag", "der", "Rei\u00b7ni\u00b7gung", ",", "da", "Gott", "durch", "tie\u00b7fes", "Kreuz", "und", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "$,", "KOUS", "NN", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Will alles Unrecht von der Seel', die Schlacken von dem Golde scheiden!", "tokens": ["Will", "al\u00b7les", "Un\u00b7recht", "von", "der", "Seel'", ",", "die", "Schla\u00b7cken", "von", "dem", "Gol\u00b7de", "schei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "NN", "APPR", "ART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.3": {"text": "Da wird der Seele tief entdeckt", "tokens": ["Da", "wird", "der", "See\u00b7le", "tief", "ent\u00b7deckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Eigne, das verborgen steckt,", "tokens": ["Das", "Eig\u00b7ne", ",", "das", "ver\u00b7bor\u00b7gen", "steckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da hei\u00dfet b\u00f6s' ihr gutes Tun,", "tokens": ["Da", "hei\u00b7\u00dfet", "b\u00f6s'", "ihr", "gu\u00b7tes", "Tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie wird ihr selbst ein Ekel nun.", "tokens": ["Sie", "wird", "ihr", "selbst", "ein", "E\u00b7kel", "nun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So wird die Seel' entbl\u00f6\u00dfet ganz, doch, eben an dem Tage,", "tokens": ["So", "wird", "die", "Seel'", "ent\u00b7bl\u00f6\u00b7\u00dfet", "ganz", ",", "doch", ",", "e\u00b7ben", "an", "dem", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "ADV", "$,", "ADV", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Wann alles scheinet aus zu sein, dann end't sich ihre Plage,", "tokens": ["Wann", "al\u00b7les", "schei\u00b7net", "aus", "zu", "sein", ",", "dann", "end't", "sich", "ih\u00b7re", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PTKVZ", "PTKZU", "VAINF", "$,", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "Gott kommt und zieht ins Herz hinein,", "tokens": ["Gott", "kommt", "und", "zieht", "ins", "Herz", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wie sch\u00f6n wird's dann bewohnet sein!", "tokens": ["Wie", "sch\u00f6n", "wird's", "dann", "be\u00b7woh\u00b7net", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Es lebet, gr\u00fcnt und bl\u00fchet gar,", "tokens": ["Es", "le\u00b7bet", ",", "gr\u00fcnt", "und", "bl\u00fc\u00b7het", "gar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Was vor so w\u00fcst und einsam war.", "tokens": ["Was", "vor", "so", "w\u00fcst", "und", "ein\u00b7sam", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADV", "VVFIN", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}