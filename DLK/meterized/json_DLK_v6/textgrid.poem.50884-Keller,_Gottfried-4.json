{"textgrid.poem.50884": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "4", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und als die Sch\u00f6pfung bleischwer das Haupt im Schlafe wog", "tokens": ["Und", "als", "die", "Sch\u00f6p\u00b7fung", "blei\u00b7schwer", "das", "Haupt", "im", "Schla\u00b7fe", "wog"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und sie ein qu\u00e4lend Traumbild, da\u00df sie ", "tokens": ["Und", "sie", "ein", "qu\u00e4\u00b7lend", "Traum\u00b7bild", ",", "da\u00df", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "ART", "ADJD", "NN", "$,", "KOUS", "PPER"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und Gott im Himmel selber schlief, vergessend Meer und Land,", "tokens": ["Und", "Gott", "im", "Him\u00b7mel", "sel\u00b7ber", "schlief", ",", "ver\u00b7ges\u00b7send", "Meer", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "ADV", "VVFIN", "$,", "VVPP", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Wor\u00fcberhin kein Lufthauch als Lebenszeichen zog:", "tokens": ["Wo\u00b7r\u00fc\u00b7berh\u00b7in", "kein", "Luft\u00b7hauch", "als", "Le\u00b7bens\u00b7zei\u00b7chen", "zog", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "KOUS", "NN", "VVFIN", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Da wachte eine Lilie auf, die einsam, einsam stand", "tokens": ["Da", "wach\u00b7te", "ei\u00b7ne", "Li\u00b7lie", "auf", ",", "die", "ein\u00b7sam", ",", "ein\u00b7sam", "stand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "$,", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Und die den fernen Sternglanz mit leisem Atem sog;", "tokens": ["Und", "die", "den", "fer\u00b7nen", "Stern\u00b7glanz", "mit", "lei\u00b7sem", "A\u00b7tem", "sog", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Da fiel ein Falter tief in sie, mit dunklem Schwingenrand,", "tokens": ["Da", "fiel", "ein", "Fal\u00b7ter", "tief", "in", "sie", ",", "mit", "dunk\u00b7lem", "Schwin\u00b7gen\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "APPR", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Der durch den kalten Nachttau mit M\u00fche zitternd flog.", "tokens": ["Der", "durch", "den", "kal\u00b7ten", "Nacht\u00b7tau", "mit", "M\u00fc\u00b7he", "zit\u00b7ternd", "flog", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "APPR", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Die Fl\u00fcgel schmiegte bebend er an ihres Kelches Wand,", "tokens": ["Die", "Fl\u00fc\u00b7gel", "schmieg\u00b7te", "be\u00b7bend", "er", "an", "ih\u00b7res", "Kel\u00b7ches", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "Die, auch erbebend, ob ihm sich eng zusammenbog.", "tokens": ["Die", ",", "auch", "er\u00b7be\u00b7bend", ",", "ob", "ihm", "sich", "eng", "zu\u00b7sam\u00b7men\u00b7bog", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "VVPP", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "------+-+-+-+", "measure": "unknown.measure.tetra"}}}}}