{"textgrid.poem.47718": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[eine holde S\u00e4ngerin]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine holde S\u00e4ngerin,", "tokens": ["Ei\u00b7ne", "hol\u00b7de", "S\u00e4n\u00b7ge\u00b7rin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fcmelein am Bache sucht sie,", "tokens": ["Bl\u00fc\u00b7me\u00b7lein", "am", "Ba\u00b7che", "sucht", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Gleitet aus und f\u00e4llt hinein,", "tokens": ["Glei\u00b7tet", "aus", "und", "f\u00e4llt", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hin zum Meere sp\u00fclt der Bach sie.", "tokens": ["Hin", "zum", "Mee\u00b7re", "sp\u00fclt", "der", "Bach", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Von dem Meere wird sie drauf", "tokens": ["Von", "dem", "Mee\u00b7re", "wird", "sie", "drauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wogend an den Strand getrieben;", "tokens": ["Wo\u00b7gend", "an", "den", "Strand", "ge\u00b7trie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Alsofort am Strande w\u00e4chst", "tokens": ["Al\u00b7so\u00b7fort", "am", "Stran\u00b7de", "w\u00e4chst"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Eine dichtbelaubte Linde.", "tokens": ["Ei\u00b7ne", "dicht\u00b7be\u00b7laub\u00b7te", "Lin\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Br\u00fcderlein aus ihrem Holz", "tokens": ["Br\u00fc\u00b7derl\u00b7ein", "aus", "ih\u00b7rem", "Holz"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Schnitzet eine feine Harfe.", "tokens": ["Schnit\u00b7zet", "ei\u00b7ne", "fei\u00b7ne", "Har\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "\u00bbach, wie lieblich, ach, wie sch\u00f6n", "tokens": ["\u00bb", "ach", ",", "wie", "lieb\u00b7lich", ",", "ach", ",", "wie", "sch\u00f6n"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["$(", "ITJ", "$,", "PWAV", "ADJD", "$,", "ITJ", "$,", "PWAV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Klingt sie, meine Lindenharfe!\u00ab \u2013", "tokens": ["Klingt", "sie", ",", "mei\u00b7ne", "Lin\u00b7den\u00b7har\u00b7fe", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "\u00bblindenharfe w\u00e4re das?\u00ab", "tokens": ["\u00bb", "lin\u00b7den\u00b7har\u00b7fe", "w\u00e4\u00b7re", "das", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "VAFIN", "PDS", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Also spricht die Mutter weinend,", "tokens": ["Al\u00b7so", "spricht", "die", "Mut\u00b7ter", "wei\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "\u00bbdein verlornes Schwesterlein,", "tokens": ["\u00bb", "dein", "ver\u00b7lor\u00b7nes", "Schwes\u00b7ter\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Meine s\u00fc\u00dfe Tochter ist es.", "tokens": ["Mei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Toch\u00b7ter", "ist", "es", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "O wie deutlich in mein Ohr", "tokens": ["O", "wie", "deut\u00b7lich", "in", "mein", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "H\u00f6r' ich ihre Stimme klingen!", "tokens": ["H\u00f6r'", "ich", "ih\u00b7re", "Stim\u00b7me", "klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "O wie z\u00e4rtlich an mein Herz", "tokens": ["O", "wie", "z\u00e4rt\u00b7lich", "an", "mein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "F\u00fchl' ich ihre Seele dringen!\u00ab ", "tokens": ["F\u00fchl'", "ich", "ih\u00b7re", "See\u00b7le", "drin\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}