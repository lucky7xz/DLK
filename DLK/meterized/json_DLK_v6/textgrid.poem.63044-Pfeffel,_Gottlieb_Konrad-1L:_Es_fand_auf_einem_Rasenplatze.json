{"textgrid.poem.63044": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es fand auf einem Rasenplatze", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es fand auf einem Rasenplatze", "tokens": ["Es", "fand", "auf", "ei\u00b7nem", "Ra\u00b7sen\u00b7plat\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Stinkthier eine Bisamratze.", "tokens": ["Ein", "Stink\u00b7thier", "ei\u00b7ne", "Bi\u00b7sam\u00b7rat\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie hatten sich noch kaum erblickt,", "tokens": ["Sie", "hat\u00b7ten", "sich", "noch", "kaum", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So hielten sie sich um die Wette", "tokens": ["So", "hiel\u00b7ten", "sie", "sich", "um", "die", "Wet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Die Nase zu. Bist du verr\u00fcckt?", "tokens": ["Die", "Na\u00b7se", "zu", ".", "Bist", "du", "ver\u00b7r\u00fcckt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Sprach endlich zu der armen Frette", "tokens": ["Sprach", "end\u00b7lich", "zu", "der", "ar\u00b7men", "Fret\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Ratze voller Bitterkeit:", "tokens": ["Die", "Rat\u00b7ze", "vol\u00b7ler", "Bit\u00b7ter\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du stinkst gleich einem alten Aase", "tokens": ["Du", "stinkst", "gleich", "ei\u00b7nem", "al\u00b7ten", "Aa\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Auf eine Viertelmeile weit;", "tokens": ["Auf", "ei\u00b7ne", "Vier\u00b7tel\u00b7mei\u00b7le", "weit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und doch verstopfst du dir die Nase", "tokens": ["Und", "doch", "ver\u00b7stopfst", "du", "dir", "die", "Na\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "ART", "NN"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.11": {"text": "Vor mir, die selbst der Zimmetstrauch", "tokens": ["Vor", "mir", ",", "die", "selbst", "der", "Zim\u00b7mets\u00b7trauch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Um ihren Duft beneiden m\u00f6chte.", "tokens": ["Um", "ih\u00b7ren", "Duft", "be\u00b7nei\u00b7den", "m\u00f6ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "\u00bbgut, sprach die Frette, doch ich d\u00e4chte,", "tokens": ["\u00bb", "gut", ",", "sprach", "die", "Fret\u00b7te", ",", "doch", "ich", "d\u00e4ch\u00b7te", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wer zu wohl riecht, der stinket auch.\u00ab", "tokens": ["Wer", "zu", "wohl", "riecht", ",", "der", "stin\u00b7ket", "auch", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "APPR", "ADV", "VVFIN", "$,", "PRELS", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}