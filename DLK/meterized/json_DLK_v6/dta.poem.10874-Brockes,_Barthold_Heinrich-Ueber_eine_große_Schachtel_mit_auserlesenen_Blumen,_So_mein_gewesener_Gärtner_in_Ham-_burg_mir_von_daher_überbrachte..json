{"dta.poem.10874": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ueber eine gro\u00dfe Schachtel mit  \n auserlesenen Blumen,  \n So mein gewesener G\u00e4rtner in Ham-  \n burg mir von daher \u00fcberbrachte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als wenn, aus dieser offnen Schachtel, ein buntes Feuer lo-\ndernd spr\u00fcnge,", "tokens": ["Als", "wenn", ",", "aus", "die\u00b7ser", "off\u00b7nen", "Schach\u00b7tel", ",", "ein", "bun\u00b7tes", "Feu\u00b7er", "lo", "dernd", "spr\u00fcn\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "$,", "APPR", "PDAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "TRUNC", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Und mir, mit Farben reichen Stralen, durchs Aug, in meine", "tokens": ["Und", "mir", ",", "mit", "Far\u00b7ben", "rei\u00b7chen", "Stra\u00b7len", ",", "durchs", "Aug", ",", "in", "mei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$,", "APPR", "NN", "ADJA", "NN", "$,", "APPRART", "NN", "$,", "APPR", "PPOSAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So nahm der tausendfach vermengte, der dicht vereinte Glanz", "tokens": ["So", "nahm", "der", "tau\u00b7send\u00b7fach", "ver\u00b7meng\u00b7te", ",", "der", "dicht", "ver\u00b7ein\u00b7te", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN", "$,", "PRELS", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Mit tausend kleinen bunten Blitzen, die angestralten Augen ein.", "tokens": ["Mit", "tau\u00b7send", "klei\u00b7nen", "bun\u00b7ten", "Blit\u00b7zen", ",", "die", "an\u00b7ge\u00b7stral\u00b7ten", "Au\u00b7gen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Ich stund, beym ersten Anblick, starr; der Farben Glanz, der", "tokens": ["Ich", "stund", ",", "beym", "ers\u00b7ten", "An\u00b7blick", ",", "starr", ";", "der", "Far\u00b7ben", "Glanz", ",", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "ADJA", "NN", "$,", "ADJD", "$.", "ART", "NN", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jhr theils entdeckt, theils halb versteckt, vermischtes schim-", "tokens": ["Ihr", "theils", "ent\u00b7deckt", ",", "theils", "halb", "ver\u00b7steckt", ",", "ver\u00b7mischtes", "schim"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "VVPP", "$,", "ADV", "ADJD", "VVPP", "$,", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Fiel, recht mit freundlicher Gewalt, mit einem lieblichen Ge-", "tokens": ["Fiel", ",", "recht", "mit", "freund\u00b7li\u00b7cher", "Ge\u00b7walt", ",", "mit", "ei\u00b7nem", "lieb\u00b7li\u00b7chen", "Ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADV", "APPR", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Mir, durch den Blick, in mein Gehirn. Ich wu\u00dft, aus pl\u00f6tz-", "tokens": ["Mir", ",", "durch", "den", "Blick", ",", "in", "mein", "Ge\u00b7hirn", ".", "Ich", "wu\u00dft", ",", "aus", "pl\u00f6tz"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "APPR", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vor \u00fcberh\u00e4ufter Lust und Anmuth, zuerst fast nicht ein Wort", "tokens": ["Vor", "\u00fc\u00b7berh\u00b7\u00e4uf\u00b7ter", "Lust", "und", "An\u00b7muth", ",", "zu\u00b7erst", "fast", "nicht", "ein", "Wort"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,", "ADV", "ADV", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Bis vom zur\u00fcckgehaltnen Othem der Ausbruch diese Stille", "tokens": ["Bis", "vom", "zu\u00b7r\u00fcck\u00b7ge\u00b7halt\u00b7nen", "O\u00b7them", "der", "Aus\u00b7bruch", "die\u00b7se", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPRART", "ADJA", "NN", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "Und ich ein fr\u00f6liches Gottlob! mit recht ger\u00fchrter Seele, sprach.", "tokens": ["Und", "ich", "ein", "fr\u00f6\u00b7li\u00b7ches", "Gott\u00b7lob", "!", "mit", "recht", "ge\u00b7r\u00fchr\u00b7ter", "See\u00b7le", ",", "sprach", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "$.", "APPR", "ADJD", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.12": {"text": "Worauf ich sie, mit spitzen Fingern, aus ihrem gr\u00fcnen Kerker", "tokens": ["Wo\u00b7rauf", "ich", "sie", ",", "mit", "spit\u00b7zen", "Fin\u00b7gern", ",", "aus", "ih\u00b7rem", "gr\u00fc\u00b7nen", "Ker\u00b7ker"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.13": {"text": "Was etwan sich gekr\u00fcmmet hatte, daran gemach zu rechte bog,", "tokens": ["Was", "et\u00b7wan", "sich", "ge\u00b7kr\u00fcm\u00b7met", "hat\u00b7te", ",", "da\u00b7ran", "ge\u00b7mach", "zu", "rech\u00b7te", "bog", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PRF", "VVPP", "VAFIN", "$,", "PAV", "ADV", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.14": {"text": "Und zween damit belegte Tische, mit Anmuth und mit Ernst,", "tokens": ["Und", "zween", "da\u00b7mit", "be\u00b7leg\u00b7te", "Ti\u00b7sche", ",", "mit", "An\u00b7muth", "und", "mit", "Ernst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ADJA", "NN", "$,", "APPR", "NN", "KON", "APPR", "NE", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.15": {"text": "Ich sah sie fast mehr gl\u00fchn, als bl\u00fchn; ich sahe durch ein-", "tokens": ["Ich", "sah", "sie", "fast", "mehr", "gl\u00fchn", ",", "als", "bl\u00fchn", ";", "ich", "sa\u00b7he", "durch", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "VVINF", "$,", "KOUS", "VVINF", "$.", "PPER", "VVFIN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.16": {"text": "Leucojen, Anemonen, Rosen von vielen Sorten, gelben", "tokens": ["Leu\u00b7co\u00b7jen", ",", "A\u00b7ne\u00b7mo\u00b7nen", ",", "Ro\u00b7sen", "von", "vie\u00b7len", "Sor\u00b7ten", ",", "gel\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "NN", "$,", "NN", "APPR", "PIAT", "NN", "$,", "ADJA"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.17": {"text": "Violen, Tuberosen, Liljen, ", "tokens": ["Vi\u00b7o\u00b7len", ",", "Tu\u00b7be\u00b7ro\u00b7sen", ",", "Lil\u00b7jen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Orangen-Bl\u00fcthe, ", "tokens": ["O\u00b7ran\u00b7gen\u00b7B\u00b7l\u00fc\u00b7the", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Die alle, durch den sch\u00f6nen Grund des mannigfaltig-sch\u00f6nen", "tokens": ["Die", "al\u00b7le", ",", "durch", "den", "sch\u00f6\u00b7nen", "Grund", "des", "man\u00b7nig\u00b7fal\u00b7tig\u00b7sch\u00f6\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "$,", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.20": {"text": "Der gleichfalls sch\u00f6n formirten Kr\u00e4uter, erh\u00f6ht, und noch ver-", "tokens": ["Der", "gleich\u00b7falls", "sch\u00f6n", "for\u00b7mir\u00b7ten", "Kr\u00e4u\u00b7ter", ",", "er\u00b7h\u00f6ht", ",", "und", "noch", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "ADJA", "NN", "$,", "VVPP", "$,", "KON", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Woher entsteht der holde Glanz? rief ich, woher die bunte", "tokens": ["Wo\u00b7her", "ent\u00b7steht", "der", "hol\u00b7de", "Glanz", "?", "rief", "ich", ",", "wo\u00b7her", "die", "bun\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$.", "VVFIN", "PPER", "$,", "PWAV", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.22": {"text": "Aus kleinen Bl\u00e4slein ist ihr Leib geweht, gef\u00fcget und gemacht:", "tokens": ["Aus", "klei\u00b7nen", "Bl\u00e4s\u00b7lein", "ist", "ihr", "Leib", "ge\u00b7weht", ",", "ge\u00b7f\u00fc\u00b7get", "und", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.23": {"text": "Doch sind die eigentlich die Quell und Ursprung ihrer Sch\u00f6n-", "tokens": ["Doch", "sind", "die", "ei\u00b7gent\u00b7lich", "die", "Quell", "und", "Ur\u00b7sprung", "ih\u00b7rer", "Sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADV", "ART", "NN", "KON", "NN", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.24": {"text": "Sie brechen und sie mildern nur der sch\u00f6nen Farben Quell,", "tokens": ["Sie", "bre\u00b7chen", "und", "sie", "mil\u00b7dern", "nur", "der", "sch\u00f6\u00b7nen", "Far\u00b7ben", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "PPER", "ADJA", "ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.25": {"text": "Es zeigt uns zwar der Sonnen Stral der wundersch\u00f6nen", "tokens": ["Es", "zeigt", "uns", "zwar", "der", "Son\u00b7nen", "Stral", "der", "wun\u00b7der\u00b7sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Doch zeigen die geschm\u00fcckten Blumen den Schmuck des, der sie", "tokens": ["Doch", "zei\u00b7gen", "die", "ge\u00b7schm\u00fcck\u00b7ten", "Blu\u00b7men", "den", "Schmuck", "des", ",", "der", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "ART", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Da ja die allersch\u00f6nsten Farben, auf dieser Erden, anders nichts,", "tokens": ["Da", "ja", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Far\u00b7ben", ",", "auf", "die\u00b7ser", "Er\u00b7den", ",", "an\u00b7ders", "nichts", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,", "APPR", "PDAT", "NN", "$,", "ADV", "PIS", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.28": {"text": "Als Kinder des noch sch\u00f6nern Vaters, des all er freunden Son-", "tokens": ["Als", "Kin\u00b7der", "des", "noch", "sch\u00f6\u00b7nern", "Va\u00b7ters", ",", "des", "all", "er", "freun\u00b7den", "Son"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "ADV", "ADJA", "NN", "$,", "ART", "PIAT", "PPER", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.29": {"text": "Und weil auch die von selbst nicht ist: So zeigt sie uns ein g\u00f6tt-", "tokens": ["Und", "weil", "auch", "die", "von", "selbst", "nicht", "ist", ":", "So", "zeigt", "sie", "uns", "ein", "g\u00f6t\u00b7t"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ART", "APPR", "ADV", "PTKNEG", "VAFIN", "$.", "ADV", "VVFIN", "PPER", "PRF", "ART", "TRUNC"], "meter": "-+-+-+-+-+--++-", "measure": "iambic.septa.relaxed"}, "line.30": {"text": "Aus welchem sie und alle Sch\u00f6nheit, als wie ein Strom, be-", "tokens": ["Aus", "wel\u00b7chem", "sie", "und", "al\u00b7le", "Sch\u00f6n\u00b7heit", ",", "als", "wie", "ein", "Strom", ",", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PRELS", "PPER", "KON", "PIAT", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "So f\u00fchren Blumen uns zum Licht; das Licht zu dem, der seine", "tokens": ["So", "f\u00fch\u00b7ren", "Blu\u00b7men", "uns", "zum", "Licht", ";", "das", "Licht", "zu", "dem", ",", "der", "sei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJA", "NN", "PPER", "APPRART", "NN", "$.", "ART", "NN", "APPR", "ART", "$,", "PRELS", "PPOSAT"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.32": {"text": "So sch\u00f6n, so wunderw\u00fcrdig sch\u00f6n, ihm anerschaffen und gemacht.", "tokens": ["So", "sch\u00f6n", ",", "so", "wun\u00b7der\u00b7w\u00fcr\u00b7dig", "sch\u00f6n", ",", "ihm", "a\u00b7ner\u00b7schaf\u00b7fen", "und", "ge\u00b7macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "ADJD", "$,", "PPER", "VVINF", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.33": {"text": "Den la\u00dft uns, wenn wir Blumen sehn, als aller Sch\u00f6nheit", "tokens": ["Den", "la\u00dft", "uns", ",", "wenn", "wir", "Blu\u00b7men", "sehn", ",", "als", "al\u00b7ler", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "VVIMP", "PPER", "$,", "KOUS", "PPER", "NN", "VVINF", "$,", "KOUS", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und seine weise Macht und Liebe recht zu verehren, uns be-", "tokens": ["Und", "sei\u00b7ne", "wei\u00b7se", "Macht", "und", "Lie\u00b7be", "recht", "zu", "ver\u00b7eh\u00b7ren", ",", "uns", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "KON", "NN", "ADJD", "PTKZU", "VVFIN", "$,", "PPER", "TRUNC"], "meter": "-+-+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.35": {"text": "Ein solch Erheben kann nicht besser, durch unsre Schwach-", "tokens": ["Ein", "solch", "Er\u00b7he\u00b7ben", "kann", "nicht", "bes\u00b7ser", ",", "durch", "uns\u00b7re", "Schwach"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VMFIN", "PTKNEG", "ADJD", "$,", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "Nicht besser ausge\u00fcbet werden, als wenn wir, da wir Blu-", "tokens": ["Nicht", "bes\u00b7ser", "aus\u00b7ge\u00b7\u00fc\u00b7bet", "wer\u00b7den", ",", "als", "wenn", "wir", ",", "da", "wir", "Blu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VVPP", "VAINF", "$,", "KOKOM", "KOUS", "PPER", "$,", "KOUS", "PPER", "TRUNC"], "mete