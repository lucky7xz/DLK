{"dta.poem.3905": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "\u00a7. 1.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Verliebte, wenn sie erst ein artigs M\u00e4gdgen ken\u0303en,", "tokens": ["Ver\u00b7lieb\u00b7te", ",", "wenn", "sie", "erst", "ein", "ar\u00b7tigs", "M\u00e4gd\u00b7gen", "ke\u00f1en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind insgemein gewohnt, den gantzen Tag zu", "tokens": ["Sind", "ins\u00b7ge\u00b7mein", "ge\u00b7wohnt", ",", "den", "gant\u00b7zen", "Tag", "zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "VVPP", "$,", "ART", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "rennen;", "tokens": ["ren\u00b7nen", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Dieselben mercken sich hier die gewisse Zahl:", "tokens": ["Die\u00b7sel\u00b7ben", "mer\u00b7cken", "sich", "hier", "die", "ge\u00b7wis\u00b7se", "Zahl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es soll nicht mehr geschehn, als t\u00e4glich nur einmahl.", "tokens": ["Es", "soll", "nicht", "mehr", "ge\u00b7schehn", ",", "als", "t\u00e4g\u00b7lich", "nur", "ein\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVPP", "$,", "KOUS", "ADJD", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}