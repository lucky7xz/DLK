{"textgrid.poem.54047": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Deutschland erwache!", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df sie ein Grab dir graben,", "tokens": ["Da\u00df", "sie", "ein", "Grab", "dir", "gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "da\u00df sie mit F\u00fcrstengeld", "tokens": ["da\u00df", "sie", "mit", "F\u00fcrs\u00b7ten\u00b7geld"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "das Land verwildert haben,", "tokens": ["das", "Land", "ver\u00b7wil\u00b7dert", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "da\u00df Stadt um Stadt verf\u00e4llt . . .", "tokens": ["da\u00df", "Stadt", "um", "Stadt", "ver\u00b7f\u00e4llt", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "VVPP", "$.", "$.", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie wollen den B\u00fcrgerkrieg entfachen \u2013", "tokens": ["Sie", "wol\u00b7len", "den", "B\u00fcr\u00b7ger\u00b7krieg", "ent\u00b7fa\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "(das sollten die Kommunisten mal machen!)", "tokens": ["(", "das", "soll\u00b7ten", "die", "Kom\u00b7mu\u00b7nis\u00b7ten", "mal", "ma\u00b7chen", "!", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VMFIN", "ART", "NN", "ADV", "VVINF", "$.", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "da\u00df der Nazi dir einen Totenkranz flicht \u2013:", "tokens": ["da\u00df", "der", "Na\u00b7zi", "dir", "ei\u00b7nen", "To\u00b7ten\u00b7kranz", "flicht", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ART", "NN", "VVFIN", "$(", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Deutschland, siehst du das nicht \u2013?", "tokens": ["Deutschland", ",", "siehst", "du", "das", "nicht", "\u2013", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PDS", "PTKNEG", "$(", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Da\u00df sie im Dunkel nagen,", "tokens": ["Da\u00df", "sie", "im", "Dun\u00b7kel", "na\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "da\u00df sie im Hellen schrein;", "tokens": ["da\u00df", "sie", "im", "Hel\u00b7len", "schrein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "da\u00df sie an allen Tagen", "tokens": ["da\u00df", "sie", "an", "al\u00b7len", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Faschismus prophezein . . .", "tokens": ["Fa\u00b7schis\u00b7mus", "pro\u00b7phe\u00b7zein", ".", ".", "."], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["FM.la", "FM.la", "$.", "$.", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "F\u00fcr die Richter haben sie nichts als Lachen \u2013", "tokens": ["F\u00fcr", "die", "Rich\u00b7ter", "ha\u00b7ben", "sie", "nichts", "als", "La\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "PIS", "KOKOM", "NN", "$("], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.6": {"text": "(das sollten die Kommunisten mal machen!)", "tokens": ["(", "das", "soll\u00b7ten", "die", "Kom\u00b7mu\u00b7nis\u00b7ten", "mal", "ma\u00b7chen", "!", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VMFIN", "ART", "NN", "ADV", "VVINF", "$.", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "da\u00df der Nazi f\u00fcr die Ausbeuter ficht \u2013:", "tokens": ["da\u00df", "der", "Na\u00b7zi", "f\u00fcr", "die", "Aus\u00b7beu\u00b7ter", "ficht", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$(", "$."], "meter": "+----+-+-+", "measure": "dactylic.init"}, "line.8": {"text": "Deutschland, h\u00f6rst du das nicht \u2013?", "tokens": ["Deutschland", ",", "h\u00f6rst", "du", "das", "nicht", "\u2013", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PDS", "PTKNEG", "$(", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Da\u00df sie in Waffen starren,", "tokens": ["Da\u00df", "sie", "in", "Waf\u00b7fen", "star\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "da\u00df sie landauf, landab", "tokens": ["da\u00df", "sie", "land\u00b7auf", ",", "land\u00b7ab"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "PTKVZ", "$,", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "ihre Agenten karren", "tokens": ["ih\u00b7re", "A\u00b7gen\u00b7ten", "kar\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "im nimmerm\u00fcden Trab . . .", "tokens": ["im", "nim\u00b7mer\u00b7m\u00fc\u00b7den", "Trab", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die \u00dcbungsgranaten krachen . . .", "tokens": ["Die", "\u00dc\u00b7bungs\u00b7gra\u00b7na\u00b7ten", "kra\u00b7chen", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "$.", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "(das sollten die Kommunisten mal machen!)", "tokens": ["(", "das", "soll\u00b7ten", "die", "Kom\u00b7mu\u00b7nis\u00b7ten", "mal", "ma\u00b7chen", "!", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VMFIN", "ART", "NN", "ADV", "VVINF", "$.", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "da\u00df der Nazi dein Todesurteil spricht \u2013:", "tokens": ["da\u00df", "der", "Na\u00b7zi", "dein", "To\u00b7des\u00b7ur\u00b7teil", "spricht", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NE", "PPOSAT", "NN", "VVFIN", "$(", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Deutschland, f\u00fchlst du das nicht \u2013?", "tokens": ["Deutschland", ",", "f\u00fchlst", "du", "das", "nicht", "\u2013", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PDS", "PTKNEG", "$(", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Und es braust aus den Betrieben ein Chor", "tokens": ["Und", "es", "braust", "aus", "den", "Be\u00b7trie\u00b7ben", "ein", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "--+---+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "von Millionen Arbeiterstimmen hervor:", "tokens": ["von", "Mil\u00b7lion\u00b7en", "Ar\u00b7bei\u00b7ter\u00b7stim\u00b7men", "her\u00b7vor", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PTKVZ", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Wir wissen alles. Uns sperren sie ein.", "tokens": ["Wir", "wis\u00b7sen", "al\u00b7les", ".", "Uns", "sper\u00b7ren", "sie", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir wissen alles. Uns l\u00e4\u00dft man bespein.", "tokens": ["Wir", "wis\u00b7sen", "al\u00b7les", ".", "Uns", "l\u00e4\u00dft", "man", "be\u00b7spein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$.", "PPER", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wir werden aufgel\u00f6st. Und verboten.", "tokens": ["Wir", "wer\u00b7den", "auf\u00b7ge\u00b7l\u00f6st", ".", "Und", "ver\u00b7bo\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "KON", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wir z\u00e4hlen die Opfer; wir z\u00e4hlen die Toten.", "tokens": ["Wir", "z\u00e4h\u00b7len", "die", "Op\u00b7fer", ";", "wir", "z\u00e4h\u00b7len", "die", "To\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Kein Minister r\u00fchrt sich, wenn Hitler spricht.", "tokens": ["Kein", "Mi\u00b7nis\u00b7ter", "r\u00fchrt", "sich", ",", "wenn", "Hit\u00b7ler", "spricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "$,", "KOUS", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "F\u00fcr jene die Stra\u00dfe. Gegen uns das Reichsgericht.", "tokens": ["F\u00fcr", "je\u00b7ne", "die", "Stra\u00b7\u00dfe", ".", "Ge\u00b7gen", "uns", "das", "Reichs\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ART", "NN", "$.", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Wir sehen. Wir h\u00f6ren. Wir f\u00fchlen den kommenden Krach.", "tokens": ["Wir", "se\u00b7hen", ".", "Wir", "h\u00f6\u00b7ren", ".", "Wir", "f\u00fch\u00b7len", "den", "kom\u00b7men\u00b7den", "Krach", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "PPER", "VVINF", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.8": {"text": "Und wenn Deutschland schl\u00e4ft \u2013:", "tokens": ["Und", "wenn", "Deutschland", "schl\u00e4ft", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "KOUS", "NE", "VVFIN", "$(", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}