{"dta.poem.13066": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edoch fuhr er endlich fort: \u201eihr wi\u00dft von was ich sprach:", "tokens": ["\u201e", "doch", "fuhr", "er", "end\u00b7lich", "fort", ":", "\u201e", "ihr", "wi\u00dft", "von", "was", "ich", "sprach", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "$(", "PPER", "VVFIN", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ewas man uns anbefahl, taugt zu derselben Sach.", "tokens": ["\u201e", "was", "man", "uns", "an\u00b7be\u00b7fahl", ",", "taugt", "zu", "der\u00b7sel\u00b7ben", "Sach", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PIS", "PPER", "ADV", "$,", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ewas uns ", "tokens": ["\u201e", "was", "uns"], "token_info": ["punct", "word", "word"], "pos": ["$(", "PWS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "600\u201dIst eins mit dem, was ich noch in dem Sinn verschliesse.", "tokens": ["\"", "Ist", "eins", "mit", "dem", ",", "was", "ich", "noch", "in", "dem", "Sinn", "ver\u00b7schlies\u00b7se", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PIS", "APPR", "ART", "$,", "PWS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201edie den Befehl gebracht, ist die ", "tokens": ["\u201e", "die", "den", "Be\u00b7fehl", "ge\u00b7bracht", ",", "ist", "die"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PRELS", "ART", "NN", "VVPP", "$,", "VAFIN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201esie lobt den Ehrgeiz nicht. Sie liebt die Sittsamkeit.", "tokens": ["\u201e", "sie", "lobt", "den", "Ehr\u00b7geiz", "nicht", ".", "Sie", "liebt", "die", "Sitt\u00b7sam\u00b7keit", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201esie wird die K\u00f6niginn gewi\u00df bewogen haben,", "tokens": ["\u201e", "sie", "wird", "die", "K\u00f6\u00b7ni\u00b7ginn", "ge\u00b7wi\u00df", "be\u00b7wo\u00b7gen", "ha\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201esie soll den Marmel-Stein, und was darein zu graben,", "tokens": ["\u201e", "sie", "soll", "den", "Mar\u00b7mel\u00b7Stein", ",", "und", "was", "da\u00b7rein", "zu", "gra\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ART", "NN", "$,", "KON", "PWS", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "605\u201dJa was des Nahmens Rost vertilgt, den Glanz erh\u00e4lt", "tokens": ["\"", "Ja", "was", "des", "Nah\u00b7mens", "Rost", "ver\u00b7tilgt", ",", "den", "Glanz", "er\u00b7h\u00e4lt"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PTKANT", "PWS", "ART", "NN", "NN", "VVPP", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eda\u00df er der sp\u00e4tsten Zeit in das Gesichte f\u00e4llt;", "tokens": ["\u201e", "da\u00df", "er", "der", "sp\u00e4ts\u00b7ten", "Zeit", "in", "das", "Ge\u00b7sich\u00b7te", "f\u00e4llt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201ewas unser Vorschlag war; den Tugenden verbieten,", "tokens": ["\u201e", "was", "un\u00b7ser", "Vor\u00b7schlag", "war", ";", "den", "Tu\u00b7gen\u00b7den", "ver\u00b7bie\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPOSAT", "NN", "VAFIN", "$.", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eman m\u00fcsse sich davor als Eitelkeiten h\u00fctten.", "tokens": ["\u201e", "man", "m\u00fcs\u00b7se", "sich", "da\u00b7vor", "als", "Ei\u00b7tel\u00b7kei\u00b7ten", "h\u00fct\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VMFIN", "PRF", "PAV", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201edas, glaub ich, war ihr Rath. Wer aber kann die Lieb,", "tokens": ["\u201e", "das", ",", "glaub", "ich", ",", "war", "ihr", "Rath", ".", "Wer", "a\u00b7ber", "kann", "die", "Lieb", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPOSAT", "NN", "$.", "PWS", "ADV", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "610\u201dDen Eifer, die Begier, der Ehrfurcht edlen Trieb", "tokens": ["\"", "Den", "Ei\u00b7fer", ",", "die", "Be\u00b7gier", ",", "der", "Ehr\u00b7furcht", "ed\u00b7len", "Trieb"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201eso leicht mi\u00dfbilligen, den man mit Recht empfindet,", "tokens": ["\u201e", "so", "leicht", "mi\u00df\u00b7bil\u00b7li\u00b7gen", ",", "den", "man", "mit", "Recht", "emp\u00b7fin\u00b7det", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "VVINF", "$,", "PRELS", "PIS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201eda er der Herzen Macht durch Anzugs-Kraft entz\u00fcndet?", "tokens": ["\u201e", "da", "er", "der", "Her\u00b7zen", "Macht", "durch", "An\u00b7zugs\u00b7Kraft", "ent\u00b7z\u00fcn\u00b7det", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "NN", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201edoch, weil es ihr Befehl, so leben wir darnach;", "tokens": ["\u201e", "doch", ",", "weil", "es", "ihr", "Be\u00b7fehl", ",", "so", "le\u00b7ben", "wir", "dar\u00b7nach", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u201eder Weeg zum Ziel der Frag ist noch viel hundertfach;", "tokens": ["\u201e", "der", "Weeg", "zum", "Ziel", "der", "Frag", "ist", "noch", "viel", "hun\u00b7dert\u00b7fach", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPRART", "NN", "ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "615\u201dNur einen w\u00e4hlen wir. Ergreiffen wir das Mittel:", "tokens": ["\"", "Nur", "ei\u00b7nen", "w\u00e4h\u00b7len", "wir", ".", "Er\u00b7greif\u00b7fen", "wir", "das", "Mit\u00b7tel", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PIS", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u201ebezieren wir den Stein mit einem andern Titel.", "tokens": ["\u201e", "be\u00b7zie\u00b7ren", "wir", "den", "Stein", "mit", "ei\u00b7nem", "an\u00b7dern", "Ti\u00b7tel", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "\u201egebt meinem Vorschlag Statt! es kommt nur auf die Wahl:", "tokens": ["\u201e", "gebt", "mei\u00b7nem", "Vor\u00b7schlag", "Statt", "!", "es", "kommt", "nur", "auf", "die", "Wahl", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPOSAT", "NN", "NN", "$.", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "\u201ejhr kennt das Helden-Herz in ihrem Ehgemahl?", "tokens": ["\u201e", "jhr", "kennt", "das", "Hel\u00b7den\u00b7Herz", "in", "ih\u00b7rem", "Eh\u00b7ge\u00b7mahl", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}