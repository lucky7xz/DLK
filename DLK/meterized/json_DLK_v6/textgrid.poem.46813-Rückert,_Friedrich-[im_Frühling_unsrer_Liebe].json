{"textgrid.poem.46813": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[im Fr\u00fchling unsrer Liebe]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Fr\u00fchling unsrer Liebe,", "tokens": ["Im", "Fr\u00fch\u00b7ling", "uns\u00b7rer", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo wir zusammen ruhten,", "tokens": ["Wo", "wir", "zu\u00b7sam\u00b7men", "ruh\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Liebchen, im stillen Hain,", "tokens": ["Lieb\u00b7chen", ",", "im", "stil\u00b7len", "Hain", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Da schl\u00fcpften Liebesg\u00f6tter", "tokens": ["Da", "schl\u00fcpf\u00b7ten", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durch die bewegten Zweige", "tokens": ["Durch", "die", "be\u00b7weg\u00b7ten", "Zwei\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Unruhig aus und ein.", "tokens": ["Un\u00b7ru\u00b7hig", "aus", "und", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Der Fr\u00fchling ist vergangen,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ist", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch gr\u00fcn sind Sommerfluren,", "tokens": ["Doch", "gr\u00fcn", "sind", "Som\u00b7mer\u00b7flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wo wir jetzo ruhn,", "tokens": ["Und", "wo", "wir", "jet\u00b7zo", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wenn Liebesg\u00f6tter fehlen,", "tokens": ["Wenn", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So treten Liebesengel", "tokens": ["So", "tre\u00b7ten", "Lie\u00b7be\u00b7sen\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "An ihre Stelle nun.", "tokens": ["An", "ih\u00b7re", "Stel\u00b7le", "nun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ach, unsre Liebesengel,", "tokens": ["Ach", ",", "uns\u00b7re", "Lie\u00b7be\u00b7sen\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich sehe, wie sie schweben", "tokens": ["Ich", "se\u00b7he", ",", "wie", "sie", "schwe\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit minder eiliger", "tokens": ["Mit", "min\u00b7der", "ei\u00b7li\u00b7ger"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADV", "ADJA"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Umflattrung in den Zweigen,", "tokens": ["Um\u00b7flatt\u00b7rung", "in", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und um uns ist das Schweigen", "tokens": ["Und", "um", "uns", "ist", "das", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUI", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Haines heiliger.", "tokens": ["Des", "Hai\u00b7nes", "hei\u00b7li\u00b7ger", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}