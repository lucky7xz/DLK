{"textgrid.poem.62554": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Am heiligen Osterabend", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O heil'ges Grab!", "tokens": ["O", "heil'\u00b7ges", "Grab", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Hier ist mein Ziel erreichet;", "tokens": ["Hier", "ist", "mein", "Ziel", "er\u00b7rei\u00b7chet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der matte Pilger, der die Welt durchstreichet,", "tokens": ["Der", "mat\u00b7te", "Pil\u00b7ger", ",", "der", "die", "Welt", "durch\u00b7strei\u00b7chet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Pflanzt hier mit Freuden seinen Stab.", "tokens": ["Pflanzt", "hier", "mit", "Freu\u00b7den", "sei\u00b7nen", "Stab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O heil'ges Grab!", "tokens": ["O", "heil'\u00b7ges", "Grab", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Hier werd' ich Ruhe finden;", "tokens": ["Hier", "werd'", "ich", "Ru\u00b7he", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hier werf' ich Welt und Gl\u00fcck, und Lust und S\u00fcnden,", "tokens": ["Hier", "wer\u00b7f'", "ich", "Welt", "und", "Gl\u00fcck", ",", "und", "Lust", "und", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN", "$,", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Und Furcht und Kleinmuth ewig ab.", "tokens": ["Und", "Furcht", "und", "Klein\u00b7muth", "e\u00b7wig", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O heil'ges Grab!", "tokens": ["O", "heil'\u00b7ges", "Grab", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wo tausend Leben keimen!", "tokens": ["Wo", "tau\u00b7send", "Le\u00b7ben", "kei\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dich gr\u00fc\u00df' ich, das der V\u00e4ter frommen Tr\u00e4umen", "tokens": ["Dich", "gr\u00fc\u00df'", "ich", ",", "das", "der", "V\u00e4\u00b7ter", "from\u00b7men", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Wahrheit ew'ges Siegel gab.", "tokens": ["Der", "Wahr\u00b7heit", "ew'\u00b7ges", "Sie\u00b7gel", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}