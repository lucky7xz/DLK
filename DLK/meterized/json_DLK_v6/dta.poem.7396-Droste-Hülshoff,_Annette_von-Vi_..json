{"dta.poem.7396": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Vi .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Weh, Glockensturm! Trompetensto\u00df! und Spritzen rasseln", "tokens": ["Weh", ",", "Glo\u00b7cken\u00b7sturm", "!", "Trom\u00b7pe\u00b7ten\u00b7sto\u00df", "!", "und", "Sprit\u00b7zen", "ras\u00b7seln"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "NN", "$.", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "durch die Gassen,", "tokens": ["durch", "die", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Der aufgeschreckte P\u00f6bel dr\u00e4ngt und kr\u00e4uselt sich in w\u00fcsten", "tokens": ["Der", "auf\u00b7ge\u00b7schreck\u00b7te", "P\u00f6\u00b7bel", "dr\u00e4ngt", "und", "kr\u00e4u\u00b7selt", "sich", "in", "w\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "PRF", "APPR", "VVFIN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Massen,", "tokens": ["Mas\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Hoch schl\u00e4gt die Brunst am Giebel auf, Gewieher kreischt", "tokens": ["Hoch", "schl\u00e4gt", "die", "Brunst", "am", "Gie\u00b7bel", "auf", ",", "Ge\u00b7wie\u00b7her", "kreischt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "aus Stall und Scheunen,", "tokens": ["aus", "Stall", "und", "Scheu\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Der Eimer fliegt hinab, hinauf, umhergesto\u00dfne Kinder", "tokens": ["Der", "Ei\u00b7mer", "fliegt", "hin\u00b7ab", ",", "hin\u00b7auf", ",", "um\u00b7her\u00b7ge\u00b7sto\u00df\u00b7ne", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PTKVZ", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "weinen,", "tokens": ["wei\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Und z\u00f6gernd steigt das Morgenroth", "tokens": ["Und", "z\u00f6\u00b7gernd", "steigt", "das", "Mor\u00b7gen\u00b7roth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dem doppelt Glut entgegen loht.", "tokens": ["Dem", "dop\u00b7pelt", "Glut", "ent\u00b7ge\u00b7gen", "loht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es war beim ersten Hahnenschrei als alle B\u00fcrger aufge-", "tokens": ["Es", "war", "beim", "ers\u00b7ten", "Hah\u00b7nen\u00b7schrei", "als", "al\u00b7le", "B\u00fcr\u00b7ger", "auf\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "KOKOM", "PIAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "sch\u00fcttert", "tokens": ["sch\u00fct\u00b7tert"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Mit Schlossenpfeifen Knall auf Knall; so gr\u00e4ulich hat es nie", "tokens": ["Mit", "Schlos\u00b7sen\u00b7pfei\u00b7fen", "Knall", "auf", "Knall", ";", "so", "gr\u00e4u\u00b7lich", "hat", "es", "nie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "APPR", "NN", "$.", "ADV", "ADJD", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "gewittert!", "tokens": ["ge\u00b7wit\u00b7tert", "!"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Grad ob des reichen B\u00f6hmen Dach, des T\u00e4uschers, ballte", "tokens": ["Grad", "ob", "des", "rei\u00b7chen", "B\u00f6h\u00b7men", "Dach", ",", "des", "T\u00e4u\u00b7schers", ",", "ball\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "KOUS", "ART", "ADJA", "NN", "NN", "$,", "ART", "NN", "$,", "VVFIN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "sich das Wetter,", "tokens": ["sich", "das", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Wo Blitz an Blitze niederzuckt, mit ohrbet\u00e4ubendem Ge-", "tokens": ["Wo", "Blitz", "an", "Blit\u00b7ze", "nie\u00b7der\u00b7zuckt", ",", "mit", "ohr\u00b7be\u00b7t\u00e4u\u00b7ben\u00b7dem", "Ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "NN", "VVPP", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "schmetter,", "tokens": ["schmet\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Nun \u00fcberall an Scheun' und Haus", "tokens": ["Nun", "\u00fc\u00b7be\u00b7rall", "an", "Scheun'", "und", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Prasselt der Flammenhaag hinaus.", "tokens": ["Pras\u00b7selt", "der", "Flam\u00b7men\u00b7haag", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Im Hof die Knechte hin und her mit Axt und Beilen fluchend", "tokens": ["Im", "Hof", "die", "Knech\u00b7te", "hin", "und", "her", "mit", "Axt", "und", "Bei\u00b7len", "flu\u00b7chend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "KON", "ADV", "APPR", "NN", "KON", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "rennen,", "tokens": ["ren\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wer schob die innern Riegel vor? die Th\u00fcren weichen nicht", "tokens": ["Wer", "schob", "die", "in\u00b7nern", "Rie\u00b7gel", "vor", "?", "die", "Th\u00fc\u00b7ren", "wei\u00b7chen", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "und brennen,", "tokens": ["und", "bren\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "\u201eder Herr! der Herr!\u201c ruft's hier und dort: \u201ewo ist der Herr!\u201c", "tokens": ["\u201e", "der", "Herr", "!", "der", "Herr", "!", "\u201c", "ruft's", "hier", "und", "dort", ":", "\u201e", "wo", "ist", "der", "Herr", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "$.", "ART", "NN", "$.", "$(", "VVFIN", "ADV", "KON", "ADV", "$.", "$(", "PWAV", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "da\u00df Gott ihm gnade,", "tokens": ["da\u00df", "Gott", "ihm", "gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "An seinem Kammerfenster leckt die Loh' aus der geschlossnen", "tokens": ["An", "sei\u00b7nem", "Kam\u00b7mer\u00b7fens\u00b7ter", "leckt", "die", "Loh'", "aus", "der", "ge\u00b7schloss\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Lade!", "tokens": ["La\u00b7de", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Und eben krachte in's Portal", "tokens": ["Und", "e\u00b7ben", "krach\u00b7te", "in's", "Por\u00b7tal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die Stiege zu dem obern Saal!", "tokens": ["Die", "Stie\u00b7ge", "zu", "dem", "o\u00b7bern", "Saal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Entsetzt Gemurmel l\u00e4uft umher und schwillt in des Gedr\u00e4nges", "tokens": ["Ent\u00b7setzt", "Ge\u00b7mur\u00b7mel", "l\u00e4uft", "um\u00b7her", "und", "schwillt", "in", "des", "Ge\u00b7dr\u00e4n\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "VVFIN", "PTKVZ", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Wogen,", "tokens": ["Wo\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Dann Alles todtenstill, sie stehn, die Brauen finster einge-", "tokens": ["Dann", "Al\u00b7les", "tod\u00b7ten\u00b7still", ",", "sie", "stehn", ",", "die", "Brau\u00b7en", "fins\u00b7ter", "ein\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "ADJD", "$,", "PPER", "VVINF", "$,", "ART", "NN", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "zogen;", "tokens": ["zo\u00b7gen", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "So um den Scheiterhaufen einst gruppirten sich des S\u00fcdens", "tokens": ["So", "um", "den", "Schei\u00b7ter\u00b7hau\u00b7fen", "einst", "grup\u00b7pir\u00b7ten", "sich", "des", "S\u00fc\u00b7dens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "S\u00f6hne:", "tokens": ["S\u00f6h\u00b7ne", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "\u201eda brennt der Sch\u00e4cher, dessen Vieh das Land verlockt mit", "tokens": ["\u201e", "da", "brennt", "der", "Sch\u00e4\u00b7cher", ",", "des\u00b7sen", "Vieh", "das", "Land", "ver\u00b7lockt", "mit"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "$,", "PRELAT", "NN", "ART", "NN", "VVFIN", "APPR"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "fremder Sch\u00f6ne", "tokens": ["frem\u00b7der", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Und kaum verkauft, am dritten Tag,", "tokens": ["Und", "kaum", "ver\u00b7kauft", ",", "am", "drit\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein todtes Aas im Stalle lag!", "tokens": ["Ein", "tod\u00b7tes", "Aas", "im", "Stal\u00b7le", "lag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Gaukler brennt, aus dessen Gurt ein wunderlich Ge-", "tokens": ["Der", "Gauk\u00b7ler", "brennt", ",", "aus", "des\u00b7sen", "Gurt", "ein", "wun\u00b7der\u00b7lich", "Ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "PRELAT", "NN", "ART", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "klingel surrte,", "tokens": ["klin\u00b7gel", "surr\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Da\u00df man in rabenschwarzer Nacht ihn kennen mocht' an", "tokens": ["Da\u00df", "man", "in", "ra\u00b7ben\u00b7schwar\u00b7zer", "Nacht", "ihn", "ken\u00b7nen", "mocht'", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ADJA", "NN", "PPER", "VVINF", "VMFIN", "APPR"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "seinem Gurte,", "tokens": ["sei\u00b7nem", "Gur\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Der keine Kirche je betrat, vor keinem Gnadenbild sich neigte,", "tokens": ["Der", "kei\u00b7ne", "Kir\u00b7che", "je", "be\u00b7trat", ",", "vor", "kei\u00b7nem", "Gna\u00b7den\u00b7bild", "sich", "neig\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVFIN", "$,", "APPR", "PIAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.6": {"text": "Wenn ihm begegnet Christi Leib von Schwindel stammelt'", "tokens": ["Wenn", "ihm", "be\u00b7geg\u00b7net", "Chris\u00b7ti", "Leib", "von", "Schwin\u00b7del", "stam\u00b7melt'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "NE", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.7": {"text": "und erbleichte,", "tokens": ["und", "er\u00b7bleich\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Im gottgesandten Element", "tokens": ["Im", "gott\u00b7ge\u00b7sand\u00b7ten", "E\u00b7le\u00b7ment"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der T\u00e4uscher, mit der Kuppel, brennt!\u201c", "tokens": ["Der", "T\u00e4u\u00b7scher", ",", "mit", "der", "Kup\u00b7pel", ",", "brennt", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "$,", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}