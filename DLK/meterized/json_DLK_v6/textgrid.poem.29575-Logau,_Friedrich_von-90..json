{"textgrid.poem.29575": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "90.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo der Zustand knechtisch ist, wil die Zunge herrisch seyn,", "tokens": ["Wo", "der", "Zu\u00b7stand", "knech\u00b7tisch", "ist", ",", "wil", "die", "Zun\u00b7ge", "her\u00b7risch", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VAFIN", "$,", "VMFIN", "ART", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Wird sie nicht au\u00df Knechtschafft au\u00df-, wird sie mehr sich wickeln ein;", "tokens": ["Wird", "sie", "nicht", "au\u00df", "Knecht\u00b7schafft", "au\u00df", ",", "wird", "sie", "mehr", "sich", "wi\u00b7ckeln", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPR", "NN", "TRUNC", "$,", "VAFIN", "PPER", "ADV", "PRF", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Wo das reden nicht verf\u00e4ngt, hat das schweigen be\u00dfre stat;", "tokens": ["Wo", "das", "re\u00b7den", "nicht", "ver\u00b7f\u00e4ngt", ",", "hat", "das", "schwei\u00b7gen", "be\u00df\u00b7re", "stat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVFIN", "PTKNEG", "VVFIN", "$,", "VAFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Besser, da\u00df man nichts gesagt, als gesagt vergebens hat.", "tokens": ["Bes\u00b7ser", ",", "da\u00df", "man", "nichts", "ge\u00b7sagt", ","