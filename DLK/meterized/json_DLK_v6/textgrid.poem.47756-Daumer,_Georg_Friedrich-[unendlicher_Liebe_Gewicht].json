{"textgrid.poem.47756": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[unendlicher Liebe Gewicht]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unendlicher Liebe Gewicht,", "tokens": ["Un\u00b7end\u00b7li\u00b7cher", "Lie\u00b7be", "Ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Die Himmel ertrugen es nicht;", "tokens": ["Die", "Him\u00b7mel", "er\u00b7tru\u00b7gen", "es", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und siehe, die s\u00e4mmtliche Last,", "tokens": ["Und", "sie\u00b7he", ",", "die", "s\u00e4mmt\u00b7li\u00b7che", "Last", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Die hehre, sie senkete sich,", "tokens": ["Die", "heh\u00b7re", ",", "sie", "sen\u00b7ke\u00b7te", "sich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PPER", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie st\u00fcrzete nieder in Hast,", "tokens": ["Sie", "st\u00fcr\u00b7ze\u00b7te", "nie\u00b7der", "in", "Hast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Sie st\u00fcrzete, weh, auf ", "tokens": ["Sie", "st\u00fcr\u00b7ze\u00b7te", ",", "weh", ",", "auf"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "PTKVZ", "$,", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}