{"dta.poem.9863": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die zeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist die edle zeit/ die allezeit vergehet?", "tokens": ["Was", "ist", "die", "ed\u00b7le", "zeit", "/", "die", "al\u00b7le\u00b7zeit", "ver\u00b7ge\u00b7het", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$(", "ART", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein kleiner punct/ worauf der grosse welt-krei\u00df stehet:", "tokens": ["Ein", "klei\u00b7ner", "punct", "/", "wo\u00b7rauf", "der", "gros\u00b7se", "welt\u00b7krei\u00df", "ste\u00b7het", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein schneller blick/ der uns weist/ da\u00df sie etwas sey:", "tokens": ["Ein", "schnel\u00b7ler", "blick", "/", "der", "uns", "weist", "/", "da\u00df", "sie", "et\u00b7was", "sey", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PRELS", "PPER", "VVFIN", "$(", "KOUS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nichts/ ehe sie kommt an; nichts/ wenn sie schon vorbey.", "tokens": ["Nichts", "/", "e\u00b7he", "sie", "kommt", "an", ";", "nichts", "/", "wenn", "sie", "schon", "vor\u00b7bey", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$(", "KOUS", "PPER", "VVFIN", "PTKVZ", "$.", "PIS", "$(", "KOUS", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhr wesen l\u00e4st die zeit nicht bl\u00f6de sinnen kennen/", "tokens": ["Ihr", "we\u00b7sen", "l\u00e4st", "die", "zeit", "nicht", "bl\u00f6\u00b7de", "sin\u00b7nen", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKNEG", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das kluge seelen kaum verstehen recht zu nennen;", "tokens": ["Das", "klu\u00b7ge", "see\u00b7len", "kaum", "ver\u00b7ste\u00b7hen", "recht", "zu", "nen\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie giebt in der natur den anfang und beschlu\u00df/", "tokens": ["Sie", "giebt", "in", "der", "na\u00b7tur", "den", "an\u00b7fang", "und", "be\u00b7schlu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Lehrt/ was verschwunden ist/ zeigt/ was geschehen mu\u00df.", "tokens": ["Lehrt", "/", "was", "ver\u00b7schwun\u00b7den", "ist", "/", "zeigt", "/", "was", "ge\u00b7sche\u00b7hen", "mu\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PWS", "VVPP", "VAFIN", "$(", "VVFIN", "$(", "PWS", "VVINF", "VMFIN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.9": {"text": "Wenn sie vergangen ist/ wer kan sie wieder bringen?", "tokens": ["Wenn", "sie", "ver\u00b7gan\u00b7gen", "ist", "/", "wer", "kan", "sie", "wie\u00b7der", "brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$(", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wer kan/ wenn sie noch fern/ sie bald zu kom\u0303en zwingen?", "tokens": ["Wer", "kan", "/", "wenn", "sie", "noch", "fern", "/", "sie", "bald", "zu", "kom\u0303en", "zwin\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$(", "KOUS", "PPER", "ADV", "ADJD", "$(", "PPER", "ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+--++-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Wie starck ist ihre krafft? sie bricht den stein und stahl/", "tokens": ["Wie", "starck", "ist", "ih\u00b7re", "krafft", "?", "sie", "bricht", "den", "stein", "und", "stahl", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ver\u00e4ndert st\u00e4dt und leut\u2019/ ist m\u00e4chtig \u00fcberall.", "tokens": ["Ver\u00b7\u00e4n\u00b7dert", "st\u00e4dt", "und", "leut'", "/", "ist", "m\u00e4ch\u00b7tig", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "KON", "VVFIN", "$(", "VAFIN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wohl dem/ wer in die zeit sich allzeit wei\u00df zu schicken/", "tokens": ["Wohl", "dem", "/", "wer", "in", "die", "zeit", "sich", "all\u00b7zeit", "wei\u00df", "zu", "schi\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$(", "PWS", "APPR", "ART", "NN", "PRF", "ADV", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und/ was zuk\u00fcnfftig ist/ bey zeiten kan erblicken.", "tokens": ["Und", "/", "was", "zu\u00b7k\u00fcnff\u00b7tig", "ist", "/", "bey", "zei\u00b7ten", "kan", "er\u00b7bli\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PWS", "ADJD", "VAFIN", "$(", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wer seine zeit auf GOtt wendt in der sterbligkeit/", "tokens": ["Wer", "sei\u00b7ne", "zeit", "auf", "Gott", "wendt", "in", "der", "ster\u00b7blig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der macht ihm hier und dort aus b\u00f6ser gute zeit.", "tokens": ["Der", "macht", "ihm", "hier", "und", "dort", "aus", "b\u00f6\u00b7ser", "gu\u00b7te", "zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "KON", "ADV", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie lang ist unser zeit? spricht der betr\u00fcbten klage/", "tokens": ["Wie", "lang", "ist", "un\u00b7ser", "zeit", "?", "spricht", "der", "be\u00b7tr\u00fcb\u00b7ten", "kla\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$.", "VVFIN", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Weil kummer/ kranckheit/ angst uns macht aus stunden tage.", "tokens": ["Weil", "kum\u00b7mer", "/", "kran\u00b7ck\u00b7heit", "/", "angst", "uns", "macht", "aus", "stun\u00b7den", "ta\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "NN", "$(", "VVFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Wie kurtz ist bey der lust die zeit der ewigkeit/", "tokens": ["Wie", "kurtz", "ist", "bey", "der", "lust", "die", "zeit", "der", "e\u00b7wig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wo tausend jahre sind nur eines tages zeit.", "tokens": ["Wo", "tau\u00b7send", "jah\u00b7re", "sind", "nur", "ei\u00b7nes", "ta\u00b7ges", "zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}