{"dta.poem.18422": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Marquis de Gastanaga.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du hast die grosse last nicht allzuwohl getragen/", "tokens": ["Du", "hast", "die", "gros\u00b7se", "last", "nicht", "all\u00b7zu\u00b7wohl", "ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "VVFIN", "PTKNEG", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum steht ein Atlas da/ dem wird sie zugetheilt;", "tokens": ["Drum", "steht", "ein", "At\u00b7las", "da", "/", "dem", "wird", "sie", "zu\u00b7ge\u00b7theilt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NE", "ADV", "$(", "PDS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Indessen wird man dir den lobspruch nicht versagen/", "tokens": ["In\u00b7des\u00b7sen", "wird", "man", "dir", "den", "lob\u00b7spruch", "nicht", "ver\u00b7sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df deme gravit\u00e4t sich niemals \u00fcbereilt.", "tokens": ["Da\u00df", "de\u00b7me", "gra\u00b7vi\u00b7t\u00e4t", "sich", "nie\u00b7mals", "\u00fc\u00b7be\u00b7reilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure":