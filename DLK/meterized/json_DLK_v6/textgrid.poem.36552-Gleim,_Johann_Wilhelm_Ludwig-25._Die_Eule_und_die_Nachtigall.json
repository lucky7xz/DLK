{"textgrid.poem.36552": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "25. Die Eule und die Nachtigall", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Eule sprach zur Nachtigall,", "tokens": ["Die", "Eu\u00b7le", "sprach", "zur", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mit dem Wiederhall,", "tokens": ["Die", "mit", "dem", "Wie\u00b7der\u00b7hall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Des Abends und des Morgens, immerfort", "tokens": ["Des", "A\u00b7bends", "und", "des", "Mor\u00b7gens", ",", "im\u00b7mer\u00b7fort"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "KON", "ART", "ADV", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In Liedern scherzte: Du! h\u00f6r' auf zu singen, dort", "tokens": ["In", "Lie\u00b7dern", "scherz\u00b7te", ":", "Du", "!", "h\u00f6r'", "auf", "zu", "sin\u00b7gen", ",", "dort"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "VVFIN", "$.", "PPER", "$.", "VVFIN", "APPR", "PTKZU", "VVINF", "$,", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auf deinem Baum, in deinem Schatten;", "tokens": ["Auf", "dei\u00b7nem", "Baum", ",", "in", "dei\u00b7nem", "Schat\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und komm herein zu mir, zu mir!", "tokens": ["Und", "komm", "her\u00b7ein", "zu", "mir", ",", "zu", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPR", "PPER", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und werde meinem Gatten", "tokens": ["Und", "wer\u00b7de", "mei\u00b7nem", "Gat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein h\u00fcbsches Abendessen hier!", "tokens": ["Ein", "h\u00fcb\u00b7sches", "A\u00b7ben\u00b7des\u00b7sen", "hier", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Nachtigall, die sonst nicht spottet,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", ",", "die", "sonst", "nicht", "spot\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die spottete der Th\u00f6rin ins Gesicht:", "tokens": ["Die", "spot\u00b7te\u00b7te", "der", "Th\u00f6\u00b7rin", "ins", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du hast das Mausgeschlecht gefr\u00e4\u00dfig ausgerottet,", "tokens": ["Du", "hast", "das", "Maus\u00b7ge\u00b7schlecht", "ge\u00b7fr\u00e4\u00b7\u00dfig", "aus\u00b7ge\u00b7rot\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und V\u00f6gel kommen wohl in deine Klause nicht;", "tokens": ["Und", "V\u00f6\u00b7gel", "kom\u00b7men", "wohl", "in", "dei\u00b7ne", "Klau\u00b7se", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Besonders ich, geboren, frei zu singen,", "tokens": ["Be\u00b7son\u00b7ders", "ich", ",", "ge\u00b7bo\u00b7ren", ",", "frei", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "VVPP", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und dich, Kallm\u00e4user, auszulachen; ich", "tokens": ["Und", "dich", ",", "Kall\u00b7m\u00e4u\u00b7ser", ",", "aus\u00b7zu\u00b7la\u00b7chen", ";", "ich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["KON", "PPER", "$,", "NN", "$,", "VVIZU", "$.", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Will mich nicht selber dir zum Abendessen bringen.", "tokens": ["Will", "mich", "nicht", "sel\u00b7ber", "dir", "zum", "A\u00b7ben\u00b7des\u00b7sen", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Willst aber du Kallm\u00e4user mich verschlingen,", "tokens": ["Willst", "a\u00b7ber", "du", "Kall\u00b7m\u00e4u\u00b7ser", "mich", "ver\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So komm heraus und hole mich!", "tokens": ["So", "komm", "he\u00b7raus", "und", "ho\u00b7le", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}