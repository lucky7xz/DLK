{"dta.poem.3758": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der gescheide Hansel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hansel am Bach,               ", "tokens": ["Han\u00b7sel", "am", "Bach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Hat lauter gut Sach,", "tokens": ["Hat", "lau\u00b7ter", "gut", "Sach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJD", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Hats H\u00e4usel verbrennt,", "tokens": ["Hats", "H\u00e4u\u00b7sel", "ver\u00b7brennt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Hat Lumpen drum gehenkt.", "tokens": ["Hat", "Lum\u00b7pen", "drum", "ge\u00b7henkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hansel am Bach,", "tokens": ["Han\u00b7sel", "am", "Bach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Hat lauter gut Sach,", "tokens": ["Hat", "lau\u00b7ter", "gut", "Sach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJD", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Hat Fischlein gefangen,", "tokens": ["Hat", "Fisc\u00b7hlein", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PTKVZ", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Hat die Schuppen heimbracht.", "tokens": ["Hat", "die", "Schup\u00b7pen", "heim\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Hansel und Gretel,", "tokens": ["Han\u00b7sel", "und", "Gre\u00b7tel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Zwei lustige Leut,", "tokens": ["Zwei", "lus\u00b7ti\u00b7ge", "Leut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der Hansel ist n\u00e4rrisch,", "tokens": ["Der", "Han\u00b7sel", "ist", "n\u00e4r\u00b7risch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Gretel nit gescheidt.", "tokens": ["Die", "Gre\u00b7tel", "nit", "ge\u00b7scheidt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}