{"dta.poem.7419": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Protest.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So lang ich noch ein Protestant,", "tokens": ["So", "lang", "ich", "noch", "ein", "Pro\u00b7tes\u00b7tant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will ich auch protestiren,", "tokens": ["Will", "ich", "auch", "pro\u00b7tes\u00b7ti\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und jeder deutsche Musikant", "tokens": ["Und", "je\u00b7der", "deut\u00b7sche", "Mu\u00b7si\u00b7kant"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll's weiter musiziren!", "tokens": ["Soll's", "wei\u00b7ter", "mu\u00b7si\u00b7zi\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Singt alle Welt: Der ", "tokens": ["Singt", "al\u00b7le", "Welt", ":", "Der"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PIAT", "NN", "$.", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "So sing' doch ich: Ihr Herren, nein!", "tokens": ["So", "sing'", "doch", "ich", ":", "Ihr", "Her\u00b7ren", ",", "nein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$.", "PPOSAT", "NN", "$,", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Rhein, der Rhein k\u00f6nnt' freier sein \u2014", "tokens": ["Der", "Rhein", ",", "der", "Rhein", "k\u00f6nnt'", "frei\u00b7er", "sein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "NE", "VMFIN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So will ich protestiren.", "tokens": ["So", "will", "ich", "pro\u00b7tes\u00b7ti\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Kaum war die Taufe abgethan,", "tokens": ["Kaum", "war", "die", "Tau\u00b7fe", "ab\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich kroch noch auf den Vieren,", "tokens": ["Ich", "kroch", "noch", "auf", "den", "Vie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da fing ich schon voll Glaubens an,", "tokens": ["Da", "fing", "ich", "schon", "voll", "Glau\u00b7bens", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Macht zu protestiren,", "tokens": ["Mit", "Macht", "zu", "pro\u00b7tes\u00b7ti\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und protestire fort und fort,", "tokens": ["Und", "pro\u00b7tes\u00b7ti\u00b7re", "fort", "und", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O Wort, o Wind, o Wind, o Wort,", "tokens": ["O", "Wort", ",", "o", "Wind", ",", "o", "Wind", ",", "o", "Wort", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,", "FM", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O selig sind, die hier und dort,", "tokens": ["O", "se\u00b7lig", "sind", ",", "die", "hier", "und", "dort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "$,", "PRELS", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die ewig protestiren.", "tokens": ["Die", "e\u00b7wig", "pro\u00b7tes\u00b7ti\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nur Eins ist Not, dran halt' ich fest", "tokens": ["Nur", "Eins", "ist", "Not", ",", "dran", "halt'", "ich", "fest"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VAFIN", "NN", "$,", "PAV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und will es nit verlieren,", "tokens": ["Und", "will", "es", "nit", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das ist mein christlicher Protest,", "tokens": ["Das", "ist", "mein", "christ\u00b7li\u00b7cher", "Pro\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mein christlich Protestiren.", "tokens": ["Mein", "christ\u00b7lich", "Pro\u00b7tes\u00b7ti\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was geht mich all das Wasser an", "tokens": ["Was", "geht", "mich", "all", "das", "Was\u00b7ser", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PIAT", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Rheine bis zum Ocean?", "tokens": ["Vom", "Rhei\u00b7ne", "bis", "zum", "O\u00b7cean", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sind keine freien M\u00e4nner dran,", "tokens": ["Sind", "kei\u00b7ne", "frei\u00b7en", "M\u00e4n\u00b7ner", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So will ich protestiren.", "tokens": ["So", "will", "ich", "pro\u00b7tes\u00b7ti\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Von nun an bis in Ewigkeit", "tokens": ["Von", "nun", "an", "bis", "in", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPR", "KON", "APPR", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Soll euch der Name zieren:", "tokens": ["Soll", "euch", "der", "Na\u00b7me", "zie\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So lang ihr Protestanten seid,", "tokens": ["So", "lang", "ihr", "Pro\u00b7tes\u00b7tan\u00b7ten", "seid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "M\u00fc\u00dft ihr auch protestiren.", "tokens": ["M\u00fc\u00dft", "ihr", "auch", "pro\u00b7tes\u00b7ti\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und singt die Welt: Der ", "tokens": ["Und", "singt", "die", "Welt", ":", "Der"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$.", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "So singet: Ach! ihr Herren, nein!", "tokens": ["So", "sin\u00b7get", ":", "Ach", "!", "ihr", "Her\u00b7ren", ",", "nein", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "ITJ", "$.", "PPOSAT", "NN", "$,", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Rhein, der Rhein k\u00f6nnt' freier sein,", "tokens": ["Der", "Rhein", ",", "der", "Rhein", "k\u00f6nnt'", "frei\u00b7er", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "NE", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wir m\u00fcssen protestiren.", "tokens": ["Wir", "m\u00fcs\u00b7sen", "pro\u00b7tes\u00b7ti\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}