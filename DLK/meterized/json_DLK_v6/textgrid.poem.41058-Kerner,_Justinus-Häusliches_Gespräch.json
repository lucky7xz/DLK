{"textgrid.poem.41058": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "H\u00e4usliches Gespr\u00e4ch", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbmir leeren die M\u00e4use,", "tokens": ["\u00bb", "mir", "lee\u00b7ren", "die", "M\u00e4u\u00b7se", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Spitzm\u00e4use und Ratten,", "tokens": ["Spitz\u00b7m\u00e4u\u00b7se", "und", "Rat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Verschlo\u00dfne Geh\u00e4use", "tokens": ["Ver\u00b7schlo\u00df\u00b7ne", "Ge\u00b7h\u00e4u\u00b7se"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und offene Platten.", "tokens": ["Und", "of\u00b7fe\u00b7ne", "Plat\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Mann! die Apotheke", "tokens": ["Mann", "!", "die", "A\u00b7pot\u00b7he\u00b7ke"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "ART", "NN"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.6": {"text": "Hilft sicherlich hier,", "tokens": ["Hilft", "si\u00b7cher\u00b7lich", "hier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Gift schaffe aus ihr,", "tokens": ["Gift", "schaf\u00b7fe", "aus", "ihr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.8": {"text": "Auf da\u00df ich es lege", "tokens": ["Auf", "da\u00df", "ich", "es", "le\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Dem w\u00fcsten Getier!\u00ab \u2013", "tokens": ["Dem", "w\u00fcs\u00b7ten", "Ge\u00b7tier", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["PDS", "VVFIN", "NN", "$.", "$(", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "\u00bbweib! lasse das Morden!", "tokens": ["\u00bb", "weib", "!", "las\u00b7se", "das", "Mor\u00b7den", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$.", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Verg\u00f6nn' unsre Speisen,", "tokens": ["Ver\u00b7g\u00f6nn'", "uns\u00b7re", "Spei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die b\u00fcrgerlich hei\u00dfen,", "tokens": ["Die", "b\u00fcr\u00b7ger\u00b7lich", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Den schw\u00e4bischen Spatzen,", "tokens": ["Den", "schw\u00e4\u00b7bi\u00b7schen", "Spat\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Den M\u00e4usen und Ratzen.", "tokens": ["Den", "M\u00e4u\u00b7sen", "und", "Rat\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wenn voll die geworden,", "tokens": ["Wenn", "voll", "die", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "VAPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "In stillem Behagen", "tokens": ["In", "stil\u00b7lem", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Die M\u00e4uler sich wischen", "tokens": ["Die", "M\u00e4u\u00b7ler", "sich", "wi\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Und weiter dann jagen,", "tokens": ["Und", "wei\u00b7ter", "dann", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Die nirgends doch klagen,", "tokens": ["Die", "nir\u00b7gends", "doch", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Wie sehr sie gelitten", "tokens": ["Wie", "sehr", "sie", "ge\u00b7lit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "An unseren Tischen!", "tokens": ["An", "un\u00b7se\u00b7ren", "Ti\u00b7schen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Durch Mangel an Fischen", "tokens": ["Durch", "Man\u00b7gel", "an", "Fi\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Und Schnepfendreckschnitten.", "tokens": ["Und", "Schnep\u00b7fen\u00b7dreck\u00b7schnit\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}