{"dta.poem.6571": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Farben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man glaubet insgemein", "tokens": ["Man", "glau\u00b7bet", "ins\u00b7ge\u00b7mein"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df helle Farben leicht und lufftig seyn,", "tokens": ["Da\u00df", "hel\u00b7le", "Far\u00b7ben", "leicht", "und", "luff\u00b7tig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADJD", "KON", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und da\u00df die dunckelen hingegen,", "tokens": ["Und", "da\u00df", "die", "dun\u00b7cke\u00b7len", "hin\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als die mehr irdisches und schweres in sich hegen,", "tokens": ["Als", "die", "mehr", "ir\u00b7di\u00b7sches", "und", "schwe\u00b7res", "in", "sich", "he\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIAT", "ADJA", "KON", "ADJA", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der leichten Lieblichkeit verdringen.", "tokens": ["Der", "leich\u00b7ten", "Lieb\u00b7lich\u00b7keit", "ver\u00b7drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So, wie verschiedne T\u00f6n\u2019 offt wol zusammen klingen,", "tokens": ["So", ",", "wie", "ver\u00b7schied\u00b7ne", "T\u00f6n'", "offt", "wol", "zu\u00b7sam\u00b7men", "klin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ADJA", "NN", "ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Auch \u00f6ffters sich verwirren und sich hindern;", "tokens": ["Auch", "\u00f6ff\u00b7ters", "sich", "ver\u00b7wir\u00b7ren", "und", "sich", "hin\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "VVINF", "KON", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}