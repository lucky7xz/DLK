{"textgrid.poem.46429": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Der Wiederspruch", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als neulich mir ein Weiser,", "tokens": ["Als", "neu\u00b7lich", "mir", "ein", "Wei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Widerspruch erkl\u00e4rte,", "tokens": ["Den", "Wi\u00b7der\u00b7spruch", "er\u00b7kl\u00e4r\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sprach ich: ist das nicht einer?", "tokens": ["Sprach", "ich", ":", "ist", "das", "nicht", "ei\u00b7ner", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "VAFIN", "PDS", "PTKNEG", "PIS", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wenn Iris mit mir z\u00fcrnet,", "tokens": ["Wenn", "I\u00b7ris", "mit", "mir", "z\u00fcr\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df ich sie j\u00fcngst gek\u00fcsset,", "tokens": ["Da\u00df", "ich", "sie", "j\u00fcngst", "ge\u00b7k\u00fcs\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und itzt noch immer z\u00fcrnet,", "tokens": ["Und", "itzt", "noch", "im\u00b7mer", "z\u00fcr\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df ich sie nicht mehr k\u00fcsse?", "tokens": ["Da\u00df", "ich", "sie", "nicht", "mehr", "k\u00fcs\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}