{"textgrid.poem.48784": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "9. Als sie ihm umfangen hielte", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo ist nun meine Not, mein t\u00f6tliches Beschweren,", "tokens": ["Wo", "ist", "nun", "mei\u00b7ne", "Not", ",", "mein", "t\u00f6t\u00b7li\u00b7ches", "Be\u00b7schwe\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das mich vor kurzer Zeit kein Wort nicht machen lie\u00df?", "tokens": ["das", "mich", "vor", "kur\u00b7zer", "Zeit", "kein", "Wort", "nicht", "ma\u00b7chen", "lie\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "ADJA", "NN", "PIAT", "NN", "PTKNEG", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo ist die Traurigkeit, die mich verzagen hie\u00df,", "tokens": ["Wo", "ist", "die", "Trau\u00b7rig\u00b7keit", ",", "die", "mich", "ver\u00b7za\u00b7gen", "hie\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die Seufzer, der Verdru\u00df, die siedenhei\u00dfen Z\u00e4hren?", "tokens": ["die", "Seuf\u00b7zer", ",", "der", "Ver\u00b7dru\u00df", ",", "die", "sie\u00b7den\u00b7hei\u00b7\u00dfen", "Z\u00e4h\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ists m\u00fcglich, da\u00df es sich so balde kan verkehren", "tokens": ["Ists", "m\u00fcg\u00b7lich", ",", "da\u00df", "es", "sich", "so", "bal\u00b7de", "kan", "ver\u00b7keh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "$,", "KOUS", "PPER", "PRF", "ADV", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und anders gehn mit mir? Was mir der Tod einblies", "tokens": ["und", "an\u00b7ders", "gehn", "mit", "mir", "?", "Was", "mir", "der", "Tod", "ein\u00b7blies"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPER", "$.", "PWS", "PPER", "ART", "NN", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und mit vergifter Angst an mein schwach Herze stie\u00df,", "tokens": ["und", "mit", "ver\u00b7gif\u00b7ter", "Angst", "an", "mein", "schwach", "Her\u00b7ze", "stie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "ADJD", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "das kan und wird mich nun und nimmermehr gef\u00e4hren.", "tokens": ["das", "kan", "und", "wird", "mich", "nun", "und", "nim\u00b7mer\u00b7mehr", "ge\u00b7f\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "KON", "VAFIN", "PPER", "ADV", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Schatz, deiner Treflichkeit ist dieses zuzumessen,", "tokens": ["Schatz", ",", "dei\u00b7ner", "Tref\u00b7lich\u00b7keit", "ist", "die\u00b7ses", "zu\u00b7zu\u00b7mes\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VAFIN", "PDAT", "VVIZU", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "die auch die Sterbenden des Todes l\u00e4\u00dft vergessen.", "tokens": ["die", "auch", "die", "Ster\u00b7ben\u00b7den", "des", "To\u00b7des", "l\u00e4\u00dft", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": ". . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}}, "stanza.4": {"line.1": {"text": ". . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}, "line.2": {"text": "Umfang mich stets also, o \u00c4rztin meiner Seelen,", "tokens": ["Um\u00b7fang", "mich", "stets", "al\u00b7so", ",", "o", "\u00c4rz\u00b7tin", "mei\u00b7ner", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "FM", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so wird mich nimmermehr kein Schmerz mehr k\u00f6nnen qu\u00e4len.", "tokens": ["so", "wird", "mich", "nim\u00b7mer\u00b7mehr", "kein", "Schmerz", "mehr", "k\u00f6n\u00b7nen", "qu\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIAT", "NN", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}