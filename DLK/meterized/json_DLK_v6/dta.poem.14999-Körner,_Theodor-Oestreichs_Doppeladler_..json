{"dta.poem.14999": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Oestreichs Doppeladler .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Das ich trotz diesem Wirbelsturm der Jahre", "tokens": ["Das", "ich", "trotz", "die\u00b7sem", "Wir\u00b7bels\u00b7turm", "der", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "APPR", "PDAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In heiterm Stolz und leuchtender gewahre! \u2014", "tokens": ["In", "hei\u00b7term", "Stolz", "und", "leuch\u00b7ten\u00b7der", "ge\u00b7wah\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ja hier beginnst du, freies Land der Eichen!", "tokens": ["Ja", "hier", "be\u00b7ginnst", "du", ",", "frei\u00b7es", "Land", "der", "Ei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Ruf, dem nur der Seel'gen Stimmen gleichen,", "tokens": ["Ein", "Ruf", ",", "dem", "nur", "der", "Seel'\u00b7gen", "Stim\u00b7men", "glei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zog mich zu deinem nachbarlichen Aare,", "tokens": ["Zog", "mich", "zu", "dei\u00b7nem", "nach\u00b7bar\u00b7li\u00b7chen", "Aa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Es flo\u00df mein Blut am Vaterlands Altare.", "tokens": ["Es", "flo\u00df", "mein", "Blut", "am", "Va\u00b7ter\u00b7lands", "Al\u00b7ta\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich sank getroffen von Verr\u00e4ther Streichen.", "tokens": ["Ich", "sank", "ge\u00b7trof\u00b7fen", "von", "Ver\u00b7r\u00e4\u00b7ther", "Strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da find' ich dich, sch\u00f6n wie im Land der Dichtung,", "tokens": ["Da", "find'", "ich", "dich", ",", "sch\u00f6n", "wie", "im", "Land", "der", "Dich\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "ADJD", "KOKOM", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Zween Blitze gl\u00fcht der Augen Doppelrichtung,", "tokens": ["Zween", "Blit\u00b7ze", "gl\u00fcht", "der", "Au\u00b7gen", "Dop\u00b7pel\u00b7rich\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der Freiheit Sieg, der Tyranney Vernichtung.", "tokens": ["Der", "Frei\u00b7heit", "Sieg", ",", "der", "Ty\u00b7ran\u00b7ney", "Ver\u00b7nich\u00b7tung", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Frisch auf, Habsburg, der Teufel mu\u00df erliegen,", "tokens": ["Frisch", "auf", ",", "Habs\u00b7burg", ",", "der", "Teu\u00b7fel", "mu\u00df", "er\u00b7lie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "NN", "$,", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Gott ist mit dir, wo deine Banner fliegen.", "tokens": ["Gott", "ist", "mit", "dir", ",", "wo", "dei\u00b7ne", "Ban\u00b7ner", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "$,", "PWAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Hoch, Oestreich, hoch! \u2014 dein Schwerdt, dein Karl", "tokens": ["Hoch", ",", "O\u00b7e\u00b7streich", ",", "hoch", "!", "dein", "Schwerdt", ",", "dein", "Karl"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "$,", "NE", "$,", "ADJD", "$.", "$(", "PPOSAT", "NN", "$,", "PPOSAT", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}