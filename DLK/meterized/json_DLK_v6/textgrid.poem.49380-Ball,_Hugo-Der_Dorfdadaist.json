{"textgrid.poem.49380": {"metadata": {"author": {"name": "Ball, Hugo", "birth": "N.A.", "death": "N.A."}, "title": "Der Dorfdadaist", "genre": "verse", "period": "N.A.", "pub_year": 1906, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Schnabelschuhen und im Schn\u00fcrkorsett", "tokens": ["In", "Schna\u00b7bel\u00b7schu\u00b7hen", "und", "im", "Schn\u00fcr\u00b7kor\u00b7sett"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hat er den Winter \u00fcberstanden,", "tokens": ["Hat", "er", "den", "Win\u00b7ter", "\u00fc\u00b7bers\u00b7tan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als Schlangenmensch im Teufelskabinett", "tokens": ["Als", "Schlan\u00b7gen\u00b7mensch", "im", "Teu\u00b7fels\u00b7ka\u00b7bi\u00b7nett"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gastierte er bei Vorstadtdilletanten.", "tokens": ["Gas\u00b7tier\u00b7te", "er", "bei", "Vor\u00b7stadt\u00b7dil\u00b7le\u00b7tan\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun sich der Fr\u00fchling wieder eingestellt", "tokens": ["Nun", "sich", "der", "Fr\u00fch\u00b7ling", "wie\u00b7der", "ein\u00b7ge\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Frau Natura kr\u00e4ftig promenierte,", "tokens": ["Und", "Frau", "Na\u00b7tu\u00b7ra", "kr\u00e4f\u00b7tig", "pro\u00b7me\u00b7nier\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NE", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hat ihn die Lappen- und Atrappenwelt", "tokens": ["Hat", "ihn", "die", "Lap\u00b7pen", "und", "A\u00b7trap\u00b7pen\u00b7welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verdrossen erst und schlie\u00dflich degoutieret.", "tokens": ["Ver\u00b7dros\u00b7sen", "erst", "und", "schlie\u00df\u00b7lich", "de\u00b7gou\u00b7tie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Er hat sich eine Laute aufgezimmert", "tokens": ["Er", "hat", "sich", "ei\u00b7ne", "Lau\u00b7te", "auf\u00b7ge\u00b7zim\u00b7mert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus Kistenholz und langen Schneckenschrauben,", "tokens": ["Aus", "Kis\u00b7ten\u00b7holz", "und", "lan\u00b7gen", "Schne\u00b7cken\u00b7schrau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Saiten rasseln und die Stimme wimmert,", "tokens": ["Die", "Sai\u00b7ten", "ras\u00b7seln", "und", "die", "Stim\u00b7me", "wim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch l\u00e4\u00dft er sich die Illusion nicht rauben.", "tokens": ["Doch", "l\u00e4\u00dft", "er", "sich", "die", "Il\u00b7lu\u00b7si\u00b7on", "nicht", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+----+-+-", "measure": "unknown.measure.tetra"}}, "stanza.4": {"line.1": {"text": "Er br\u00fcllt und johlt, als hinge er am Spie\u00dfe.", "tokens": ["Er", "br\u00fcllt", "und", "johlt", ",", "als", "hin\u00b7ge", "er", "am", "Spie\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "KOUS", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er schwenkt juchelend seinen Brautzylinder.", "tokens": ["Er", "schwenkt", "ju\u00b7che\u00b7lend", "sei\u00b7nen", "Braut\u00b7zy\u00b7lin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Als Schellenk\u00f6nig tanzt er auf der Wiese", "tokens": ["Als", "Schel\u00b7len\u00b7k\u00f6\u00b7nig", "tanzt", "er", "auf", "der", "Wie\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Purzelbaum der Narren und der Kinder.", "tokens": ["Zum", "Pur\u00b7zel\u00b7baum", "der", "Nar\u00b7ren", "und", "der", "Kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}