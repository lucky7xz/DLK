{"textgrid.poem.66799": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gl\u00e4ser klirren,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00e4ser klirren,", "tokens": ["Gl\u00e4\u00b7ser", "klir\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Plaudereien schwirren,", "tokens": ["Plau\u00b7de\u00b7rei\u00b7en", "schwir\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "\u00dcbers Billard saust der glatte Ball;", "tokens": ["\u00dc\u00b7bers", "Bil\u00b7lard", "saust", "der", "glat\u00b7te", "Ball", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Zigaretten glimmen,", "tokens": ["Zi\u00b7ga\u00b7ret\u00b7ten", "glim\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Blaue W\u00f6lkchen schwimmen,", "tokens": ["Blau\u00b7e", "W\u00f6lk\u00b7chen", "schwim\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Flinke Kellnersch\u00f6\u00dfe \u00fcberall.", "tokens": ["Flin\u00b7ke", "Kell\u00b7ner\u00b7sch\u00f6\u00b7\u00dfe", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Ist ein Summen und Zeitungsrauschen,", "tokens": ["Ist", "ein", "Sum\u00b7men", "und", "Zei\u00b7tungs\u00b7rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Kugelsto\u00dfen und Debattieren \u2013", "tokens": ["Ku\u00b7gel\u00b7sto\u00b7\u00dfen", "und", "De\u00b7bat\u00b7tie\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "In der Ecke bequem zu lauschen,", "tokens": ["In", "der", "E\u00b7cke", "be\u00b7quem", "zu", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Mag ein Weilchen mich am\u00fcsieren.", "tokens": ["Mag", "ein", "Weil\u00b7chen", "mich", "a\u00b7m\u00fc\u00b7sie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+--++--", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Aus dem Geschlacker zum Heil meiner Seele", "tokens": ["Aus", "dem", "Ge\u00b7schla\u00b7cker", "zum", "Heil", "mei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.12": {"text": "Lockte der g\u00fctige Gott ins Caf\u00e9,", "tokens": ["Lock\u00b7te", "der", "g\u00fc\u00b7ti\u00b7ge", "Gott", "ins", "Ca\u00b7f\u00e9", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.13": {"text": "W\u00e4rmender Mokka rinnt in die Kehle,", "tokens": ["W\u00e4r\u00b7men\u00b7der", "Mok\u00b7ka", "rinnt", "in", "die", "Keh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.14": {"text": "An den Scheiben vertr\u00e4ut der Schnee.", "tokens": ["An", "den", "Schei\u00b7ben", "ver\u00b7tr\u00e4ut", "der", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Jener Spieler, der elegante,", "tokens": ["Je\u00b7ner", "Spie\u00b7ler", ",", "der", "e\u00b7leg\u00b7an\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "Weit vorbeugt er die schlanke Gestalt", "tokens": ["Weit", "vor\u00b7beugt", "er", "die", "schlan\u00b7ke", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.17": {"text": "\u00dcber des gr\u00fcnen Tuches Kante \u2013", "tokens": ["\u00dc\u00b7ber", "des", "gr\u00fc\u00b7nen", "Tu\u00b7ches", "Kan\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VMFIN", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.18": {"text": "L\u00e4chelnd richtet sich auf der Gewandte,", "tokens": ["L\u00e4\u00b7chelnd", "rich\u00b7tet", "sich", "auf", "der", "Ge\u00b7wand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.19": {"text": "Gl\u00fccklich Kugel auf Kugel prallt,", "tokens": ["Gl\u00fcck\u00b7lich", "Ku\u00b7gel", "auf", "Ku\u00b7gel", "prallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.20": {"text": "Und ein lohnendes Bravo schallt.", "tokens": ["Und", "ein", "loh\u00b7nen\u00b7des", "Bra\u00b7vo", "schallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Mir zur Seite die beiden alten", "tokens": ["Mir", "zur", "Sei\u00b7te", "die", "bei\u00b7den", "al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "ART", "PIAT", "ADJA"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.22": {"text": "Herren legen die Stirn in Falten,", "tokens": ["Her\u00b7ren", "le\u00b7gen", "die", "Stirn", "in", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.23": {"text": "Schwierig scheint die Situation:", "tokens": ["Schwie\u00b7rig", "scheint", "die", "Si\u00b7tu\u00b7a\u00b7ti\u00b7on", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.24": {"text": "Mit dem n\u00e4chsten k\u00fchnen Zuge", "tokens": ["Mit", "dem", "n\u00e4chs\u00b7ten", "k\u00fch\u00b7nen", "Zu\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Naht die Entscheidung, naht im Fluge,", "tokens": ["Naht", "die", "Ent\u00b7schei\u00b7dung", ",", "naht", "im", "Flu\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Wird trotz seinen Trabanten jetzt", "tokens": ["Wird", "trotz", "sei\u00b7nen", "Tra\u00b7ban\u00b7ten", "jetzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.27": {"text": "Majest\u00e4t schachmatt gesetzt,", "tokens": ["Ma\u00b7jes\u00b7t\u00e4t", "schach\u00b7matt", "ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Und kein Turm, kein Bismarck naht ...", "tokens": ["Und", "kein", "Turm", ",", "kein", "Bis\u00b7marck", "naht", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Drei Studenten \u2013 versteht sich \u2013 Skat ...", "tokens": ["Drei", "Stu\u00b7den\u00b7ten", "\u2013", "ver\u00b7steht", "sich", "\u2013", "Skat", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["CARD", "NN", "$(", "VVFIN", "PRF", "$(", "NN", "$("], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.30": {"text": "Zwischen der G\u00e4ste gleichg\u00fcltigen Reihn", "tokens": ["Zwi\u00b7schen", "der", "G\u00e4s\u00b7te", "gleich\u00b7g\u00fcl\u00b7ti\u00b7gen", "Reihn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.31": {"text": "Dr\u00fcckt sich frostzitternd von Tisch zu Tisch", "tokens": ["Dr\u00fcckt", "sich", "frost\u00b7zit\u00b7ternd", "von", "Tisch", "zu", "Tisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "NN", "APPR", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.32": {"text": "Lilienbl\u00e4\u00dflich ein M\u00e4gdelein,", "tokens": ["Li\u00b7li\u00b7en\u00b7bl\u00e4\u00df\u00b7lich", "ein", "M\u00e4g\u00b7del\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.33": {"text": "Rosen im Korbe, junifrisch.", "tokens": ["Ro\u00b7sen", "im", "Kor\u00b7be", ",", "ju\u00b7ni\u00b7frisch", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.34": {"text": "Von dem Rotblond wirrer Locken", "tokens": ["Von", "dem", "Rot\u00b7blond", "wir\u00b7rer", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Niederschmelzen die nassen Flocken,", "tokens": ["Nie\u00b7der\u00b7schmel\u00b7zen", "die", "nas\u00b7sen", "Flo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.36": {"text": "Da\u00df das Wasser dem armen Kind", "tokens": ["Da\u00df", "das", "Was\u00b7ser", "dem", "ar\u00b7men", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.37": {"text": "In den offenen Nacken rinnt.", "tokens": ["In", "den", "of\u00b7fe\u00b7nen", "Na\u00b7cken", "rinnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.38": {"text": "\u00bbrosen, Rosen!\u00ab", "tokens": ["\u00bb", "ro\u00b7sen", ",", "Ro\u00b7sen", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "VVINF", "$,", "NN", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.39": {"text": "Freudelosen", "tokens": ["Freu\u00b7de\u00b7lo\u00b7sen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.40": {"text": "Schrittes schleicht's hinaus in die finstre Nacht.", "tokens": ["Schrit\u00b7tes", "schleicht's", "hin\u00b7aus", "in", "die", "finst\u00b7re", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APZR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.41": {"text": "Wirbelnder Schneesturm braust.", "tokens": ["Wir\u00b7beln\u00b7der", "Schnee\u00b7sturm", "braust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.42": {"text": "Gl\u00e4ser klirren,", "tokens": ["Gl\u00e4\u00b7ser", "klir\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.43": {"text": "Plaudereien schwirren,", "tokens": ["Plau\u00b7de\u00b7rei\u00b7en", "schwir\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.44": {"text": "Eine Kugel \u00fcber die Barri\u00e8re saust ...", "tokens": ["Ei\u00b7ne", "Ku\u00b7gel", "\u00fc\u00b7ber", "die", "Bar\u00b7ri\u00e8\u00b7re", "saust", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}