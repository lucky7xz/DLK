{"dta.poem.16285": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Viel schneller als der Schall ist, wie man wei\u00df, das Licht;", "tokens": ["Viel", "schnel\u00b7ler", "als", "der", "Schall", "ist", ",", "wie", "man", "wei\u00df", ",", "das", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "VAFIN", "$,", "PWAV", "PIS", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was aber schneller als das Licht sei, wei\u00df man nicht.", "tokens": ["Was", "a\u00b7ber", "schnel\u00b7ler", "als", "das", "Licht", "sei", ",", "wei\u00df", "man", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "KOKOM", "ART", "NN", "VAFIN", "$,", "VVFIN", "PIS", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Viel schneller als das Licht ist, denk ich, der Gedanke,", "tokens": ["Viel", "schnel\u00b7ler", "als", "das", "Licht", "ist", ",", "denk", "ich", ",", "der", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "VAFIN", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der jeden Augenblick ber\u00fchrt des Denkens Schranke.", "tokens": ["Der", "je\u00b7den", "Au\u00b7gen\u00b7blick", "be\u00b7r\u00fchrt", "des", "Den\u00b7kens", "Schran\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch auch die Schnelligkeit des Denkens scheint geringer", "tokens": ["Doch", "auch", "die", "Schnel\u00b7lig\u00b7keit", "des", "Den\u00b7kens", "scheint", "ge\u00b7rin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als ein gedankenlos bewegter Schreibefinger.", "tokens": ["Als", "ein", "ge\u00b7dan\u00b7ken\u00b7los", "be\u00b7weg\u00b7ter", "Schrei\u00b7be\u00b7fin\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und \u00fcbertroffen wird die Schnelligkeit im Schreiben", "tokens": ["Und", "\u00fc\u00b7ber\u00b7trof\u00b7fen", "wird", "die", "Schnel\u00b7lig\u00b7keit", "im", "Schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "VAFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von der des Lesens nur; wer kanns noch weiter treiben?", "tokens": ["Von", "der", "des", "Le\u00b7sens", "nur", ";", "wer", "kanns", "noch", "wei\u00b7ter", "trei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADV", "$.", "PWS", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}