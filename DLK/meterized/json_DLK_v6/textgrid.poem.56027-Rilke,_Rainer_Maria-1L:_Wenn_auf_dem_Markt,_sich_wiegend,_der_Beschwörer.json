{"textgrid.poem.56027": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn auf dem Markt, sich wiegend, der Beschw\u00f6rer", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn auf dem Markt, sich wiegend, der Beschw\u00f6rer", "tokens": ["Wenn", "auf", "dem", "Markt", ",", "sich", "wie\u00b7gend", ",", "der", "Be\u00b7schw\u00f6\u00b7rer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "PRF", "VVPP", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die K\u00fcrbisfl\u00f6te pfeift, die reizt und lullt,", "tokens": ["die", "K\u00fcr\u00b7bis\u00b7fl\u00f6\u00b7te", "pfeift", ",", "die", "reizt", "und", "lullt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "so kann es sein, da\u00df er sich einen H\u00f6rer", "tokens": ["so", "kann", "es", "sein", ",", "da\u00df", "er", "sich", "ei\u00b7nen", "H\u00f6\u00b7rer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VAINF", "$,", "KOUS", "PPER", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "her\u00fcberlockt, der ganz aus dem Tumult", "tokens": ["her\u00b7\u00fc\u00b7ber\u00b7lockt", ",", "der", "ganz", "aus", "dem", "Tu\u00b7mult"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "PRELS", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "der Buden eintritt in den Kreis der Pfeife,", "tokens": ["der", "Bu\u00b7den", "ein\u00b7tritt", "in", "den", "Kreis", "der", "Pfei\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die will und will und will und die erreicht,", "tokens": ["die", "will", "und", "will", "und", "will", "und", "die", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "KON", "VMFIN", "KON", "VMFIN", "KON", "ART", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "da\u00df das Reptil in seinem Korb sich steife", "tokens": ["da\u00df", "das", "Rep\u00b7til", "in", "sei\u00b7nem", "Korb", "sich", "stei\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und die das steife schmeichlerisch erweicht,", "tokens": ["und", "die", "das", "stei\u00b7fe", "schmeich\u00b7le\u00b7risch", "er\u00b7weicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PDS", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "abwechselnd immer schwindelnder und blinder", "tokens": ["ab\u00b7wech\u00b7selnd", "im\u00b7mer", "schwin\u00b7deln\u00b7der", "und", "blin\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "mit dem, was schreckt und streckt, und dem, was l\u00f6st \u2013;", "tokens": ["mit", "dem", ",", "was", "schreckt", "und", "streckt", ",", "und", "dem", ",", "was", "l\u00f6st", "\u2013", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "VVFIN", "KON", "VVFIN", "$,", "KON", "ART", "$,", "PWS", "VVFIN", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und dann gen\u00fcgt ein Blick: so hat der Inder", "tokens": ["und", "dann", "ge\u00b7n\u00fcgt", "ein", "Blick", ":", "so", "hat", "der", "In\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$.", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "dir eine Fremde eingefl\u00f6\u00dft,", "tokens": ["dir", "ei\u00b7ne", "Frem\u00b7de", "ein\u00b7ge\u00b7fl\u00f6\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "in der du stirbst. Es ist als \u00fcberst\u00fcrze", "tokens": ["in", "der", "du", "stirbst", ".", "Es", "ist", "als", "\u00fc\u00b7bers\u00b7t\u00fcr\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "KOKOM", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "gl\u00fchender Himmel dich. Es geht ein Sprung", "tokens": ["gl\u00fc\u00b7hen\u00b7der", "Him\u00b7mel", "dich", ".", "Es", "geht", "ein", "Sprung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPER", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "durch dein Gesicht. Es legen sich Gew\u00fcrze", "tokens": ["durch", "dein", "Ge\u00b7sicht", ".", "Es", "le\u00b7gen", "sich", "Ge\u00b7w\u00fcr\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PRF", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "auf deine nordische Erinnerung,", "tokens": ["auf", "dei\u00b7ne", "nor\u00b7di\u00b7sche", "E\u00b7rin\u00b7ne\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.5": {"line.1": {"text": "die dir nichts hilft. Dich feien keine Kr\u00e4fte,", "tokens": ["die", "dir", "nichts", "hilft", ".", "Dich", "fei\u00b7en", "kei\u00b7ne", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "VVFIN", "$.", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die Sonne g\u00e4rt, das Fieber f\u00e4llt und trifft;", "tokens": ["die", "Son\u00b7ne", "g\u00e4rt", ",", "das", "Fie\u00b7ber", "f\u00e4llt", "und", "trifft", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "von b\u00f6ser Freude steilen sich die Sch\u00e4fte,", "tokens": ["von", "b\u00f6\u00b7ser", "Freu\u00b7de", "stei\u00b7len", "sich", "die", "Sch\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und in den Schlangen gl\u00e4nzt das Gift.", "tokens": ["und", "in", "den", "Schlan\u00b7gen", "gl\u00e4nzt", "das", "Gift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}