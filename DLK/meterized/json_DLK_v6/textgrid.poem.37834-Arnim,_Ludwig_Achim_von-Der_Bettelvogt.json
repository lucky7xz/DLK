{"textgrid.poem.37834": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Bettelvogt", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich war noch so jung, und war doch schon arm,", "tokens": ["Ich", "war", "noch", "so", "jung", ",", "und", "war", "doch", "schon", "arm", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "$,", "KON", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kein Geld hat ich gar nicht, da\u00df Gott sich erbarm,", "tokens": ["Kein", "Geld", "hat", "ich", "gar", "nicht", ",", "da\u00df", "Gott", "sich", "er\u00b7barm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADV", "PTKNEG", "$,", "KOUS", "NN", "PRF", "ADJD", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "So nahm ich meinen Stab und meinen Bettelsack,", "tokens": ["So", "nahm", "ich", "mei\u00b7nen", "Stab", "und", "mei\u00b7nen", "Bet\u00b7tel\u00b7sack", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und pfiff das Vaterunser den lieben langen Tag.", "tokens": ["Und", "pfiff", "das", "Va\u00b7ter\u00b7un\u00b7ser", "den", "lie\u00b7ben", "lan\u00b7gen", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Und als ich kam vor Heidelberg hinan,", "tokens": ["Und", "als", "ich", "kam", "vor", "Hei\u00b7del\u00b7berg", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da packten mich die Bettelv\u00f6gte gleich hinten und vornen an;", "tokens": ["Da", "pack\u00b7ten", "mich", "die", "Bet\u00b7tel\u00b7v\u00f6g\u00b7te", "gleich", "hin\u00b7ten", "und", "vor\u00b7nen", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "ADV", "KON", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+--+--+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Der eine packt mich hinten, der andre packt mich vorn;", "tokens": ["Der", "ei\u00b7ne", "packt", "mich", "hin\u00b7ten", ",", "der", "and\u00b7re", "packt", "mich", "vorn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "ADV", "$,", "PRELS", "PIS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "\u00bbey ihr verfluchte Bettelv\u00f6gt, so la\u00dft mich ungeschorn.\u00ab", "tokens": ["\u00bb", "ey", "ihr", "ver\u00b7fluch\u00b7te", "Bet\u00b7tel\u00b7v\u00f6gt", ",", "so", "la\u00dft", "mich", "un\u00b7ge\u00b7schorn", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NE", "PPOSAT", "ADJA", "NN", "$,", "ADV", "VVIMP", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Und als ich kam vors Bettelvogt sein Haus,", "tokens": ["Und", "als", "ich", "kam", "vors", "Bet\u00b7tel\u00b7vogt", "sein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da schaut der alte Spizbub zum Fenster heraus,", "tokens": ["Da", "schaut", "der", "al\u00b7te", "Spiz\u00b7bub", "zum", "Fens\u00b7ter", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich dreh mich gleich herum und seh nach seiner Frau:", "tokens": ["Ich", "dreh", "mich", "gleich", "he\u00b7rum", "und", "seh", "nach", "sei\u00b7ner", "Frau", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u00bbey du verfluchter Bettelvogt, wie sch\u00f6n ist deine Frau.\u00ab", "tokens": ["\u00bb", "ey", "du", "ver\u00b7fluch\u00b7ter", "Bet\u00b7tel\u00b7vogt", ",", "wie", "sch\u00f6n", "ist", "dei\u00b7ne", "Frau", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PPER", "ADJA", "NN", "$,", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "Der Bettelvogt der fa\u00dft einen grimmen Zorn,", "tokens": ["Der", "Bet\u00b7tel\u00b7vogt", "der", "fa\u00dft", "ei\u00b7nen", "grim\u00b7men", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er l\u00e4\u00dft mich ja setzen im tiefen tiefen Thurm,", "tokens": ["Er", "l\u00e4\u00dft", "mich", "ja", "set\u00b7zen", "im", "tie\u00b7fen", "tie\u00b7fen", "Thurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF", "APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Im tiefen tiefen Thurm bey Wasser und bey Brodt,", "tokens": ["Im", "tie\u00b7fen", "tie\u00b7fen", "Thurm", "bey", "Was\u00b7ser", "und", "bey", "Brodt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u00bbey du verfluchter Bettelvogt, krieg du die schwerste Noth!\u00ab", "tokens": ["\u00bb", "ey", "du", "ver\u00b7fluch\u00b7ter", "Bet\u00b7tel\u00b7vogt", ",", "krieg", "du", "die", "schwers\u00b7te", "Noth", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PPER", "ADJA", "NN", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.5": {"line.1": {"text": "Und wenn der Bettelvogt gestorben erst ist,", "tokens": ["Und", "wenn", "der", "Bet\u00b7tel\u00b7vogt", "ge\u00b7stor\u00b7ben", "erst", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Man sollt ihn nicht begraben wie 'nen andern Christ,", "tokens": ["Man", "sollt", "ihn", "nicht", "be\u00b7gra\u00b7ben", "wie", "'nen", "an\u00b7dern", "Christ", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "PTKNEG", "VVPP", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Lebendig ihn begraben bey Wasser und bey Brodt,", "tokens": ["Le\u00b7ben\u00b7dig", "ihn", "be\u00b7gra\u00b7ben", "bey", "Was\u00b7ser", "und", "bey", "Brodt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVPP", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Wie mich der alte Bettelvogt begraben ohne Noth.", "tokens": ["Wie", "mich", "der", "al\u00b7te", "Bet\u00b7tel\u00b7vogt", "be\u00b7gra\u00b7ben", "oh\u00b7ne", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.6": {"line.1": {"text": "Ihr Br\u00fcder seyd nun lustig, der Bettelvogt ist todt,", "tokens": ["Ihr", "Br\u00fc\u00b7der", "seyd", "nun", "lus\u00b7tig", ",", "der", "Bet\u00b7tel\u00b7vogt", "ist", "todt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er h\u00e4ngt schon im Galgen ganz schwer und voller Noth,", "tokens": ["Er", "h\u00e4ngt", "schon", "im", "Gal\u00b7gen", "ganz", "schwer", "und", "vol\u00b7ler", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "ADV", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "In der verwichenen Woch am Dienstag um halber neun,", "tokens": ["In", "der", "ver\u00b7wi\u00b7che\u00b7nen", "Woch", "am", "Diens\u00b7tag", "um", "hal\u00b7ber", "neun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "APPR", "APPO", "CARD", "$,"], "meter": "---+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Da haben sie 'n gehangen in Galgen fest hinein.", "tokens": ["Da", "ha\u00b7ben", "sie", "'n", "ge\u00b7han\u00b7gen", "in", "Gal\u00b7gen", "fest", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "VVPP", "APPR", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Er h\u00e4tt die sch\u00f6ne Frau beynahe umgebracht,", "tokens": ["Er", "h\u00e4tt", "die", "sch\u00f6\u00b7ne", "Frau", "bey\u00b7na\u00b7he", "um\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil sie mich armen Lumpen freundlich angelacht.", "tokens": ["Weil", "sie", "mich", "ar\u00b7men", "Lum\u00b7pen", "freund\u00b7lich", "an\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In der vergangenen Woch, da sah er noch hinaus,", "tokens": ["In", "der", "ver\u00b7gan\u00b7ge\u00b7nen", "Woch", ",", "da", "sah", "er", "noch", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und heut bin ich bei ihr in seinem Haus.", "tokens": ["Und", "heut", "bin", "ich", "bei", "ihr", "in", "sei\u00b7nem", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}