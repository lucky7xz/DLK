{"dta.poem.23420": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Eine Singstimme.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eich bin ein Preu\u00dfe,\u201c singt nur einer,", "tokens": ["\u201e", "ich", "bin", "ein", "Preu\u00b7\u00dfe", ",", "\u201c", "singt", "nur", "ei\u00b7ner", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$,", "$(", "VVFIN", "ADV", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die andern aber brummen drein.", "tokens": ["Die", "an\u00b7dern", "a\u00b7ber", "brum\u00b7men", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das klingt wahrhaftig, als ob keiner", "tokens": ["Das", "klingt", "wahr\u00b7haf\u00b7tig", ",", "als", "ob", "kei\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "$,", "KOKOM", "KOUS", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So recht ein Preu\u00dfe wollte sein.", "tokens": ["So", "recht", "ein", "Preu\u00b7\u00dfe", "woll\u00b7te", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O f\u00e4nde doch das Brummen Anhang", "tokens": ["O", "f\u00e4n\u00b7de", "doch", "das", "Brum\u00b7men", "An\u00b7hang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und lie\u00df' uns solch ein Singsang k\u00fchl,", "tokens": ["Und", "lie\u00df'", "uns", "solch", "ein", "Sings\u00b7ang", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das w\u00e4re schon ein guter Anfang", "tokens": ["Das", "w\u00e4\u00b7re", "schon", "ein", "gu\u00b7ter", "An\u00b7fang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von deutscher Einheit Vorgef\u00fchl.", "tokens": ["Von", "deut\u00b7scher", "Ein\u00b7heit", "Vor\u00b7ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}