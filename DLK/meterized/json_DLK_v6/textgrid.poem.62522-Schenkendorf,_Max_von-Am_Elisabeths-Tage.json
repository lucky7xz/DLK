{"textgrid.poem.62522": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Am Elisabeths-Tage", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Zier der deutschen Frauen,", "tokens": ["O", "Zier", "der", "deut\u00b7schen", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Preis dir, Elisabeth,", "tokens": ["Preis", "dir", ",", "E\u00b7lis\u00b7a\u00b7beth", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "NE", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die sonder Scheu und Grauen", "tokens": ["Die", "son\u00b7der", "Scheu", "und", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur Gitterpforte geht,", "tokens": ["Zur", "Git\u00b7ter\u00b7pfor\u00b7te", "geht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Um dort mit zarten H\u00e4nden", "tokens": ["Um", "dort", "mit", "zar\u00b7ten", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Armen Brod zu spenden.", "tokens": ["Den", "Ar\u00b7men", "Brod", "zu", "spen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Heiland, der mit ihnen", "tokens": ["Der", "Hei\u00b7land", ",", "der", "mit", "ih\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An deiner Pforte stand,", "tokens": ["An", "dei\u00b7ner", "Pfor\u00b7te", "stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat in den frommen Mienen", "tokens": ["Hat", "in", "den", "from\u00b7men", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den frommen Sinn erkannt;", "tokens": ["Den", "from\u00b7men", "Sinn", "er\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was du gethan den Br\u00fcdern,", "tokens": ["Was", "du", "ge\u00b7than", "den", "Br\u00fc\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wollt' er in Lieb' erwiedern.", "tokens": ["Wollt'", "er", "in", "Lieb'", "er\u00b7wie\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er lie\u00df nicht unvergolten", "tokens": ["Er", "lie\u00df", "nicht", "un\u00b7ver\u00b7gol\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das ihm geweihte Herz;", "tokens": ["Das", "ihm", "ge\u00b7weih\u00b7te", "Herz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn als dein Herr gescholten", "tokens": ["Denn", "als", "dein", "Herr", "ge\u00b7schol\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und du vergingst in Schmerz,", "tokens": ["Und", "du", "ver\u00b7gingst", "in", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "War er, mit Wundern kr\u00e4ftig,", "tokens": ["War", "er", ",", "mit", "Wun\u00b7dern", "kr\u00e4f\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu deinem Trost gesch\u00e4ftig.", "tokens": ["Zu", "dei\u00b7nem", "Trost", "ge\u00b7sch\u00e4f\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie durch des Priesters Worte", "tokens": ["Wie", "durch", "des", "Pries\u00b7ters", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Wandlung wird vollbracht,", "tokens": ["Die", "Wand\u00b7lung", "wird", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erfuhr man, an der Pforte,", "tokens": ["Er\u00b7fuhr", "man", ",", "an", "der", "Pfor\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des reinen Weibes Macht,", "tokens": ["Des", "rei\u00b7nen", "Wei\u00b7bes", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In deinem keuschen Schoo\u00dfe", "tokens": ["In", "dei\u00b7nem", "keu\u00b7schen", "Schoo\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erbl\u00fcht die Wunderrose.", "tokens": ["Er\u00b7bl\u00fcht", "die", "Wun\u00b7der\u00b7ro\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und nun auf den Alt\u00e4ren", "tokens": ["Und", "nun", "auf", "den", "Al\u00b7t\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein holdes Bildni\u00df steht,", "tokens": ["Dein", "hol\u00b7des", "Bild\u00b7ni\u00df", "steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gl\u00e4ubigen verehren", "tokens": ["Gl\u00e4u\u00b7bi\u00b7gen", "ver\u00b7eh\u00b7ren"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Dich freudig im Gebet:", "tokens": ["Dich", "freu\u00b7dig", "im", "Ge\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Patrona, mit Gebeten", "tokens": ["Pat\u00b7ro\u00b7na", ",", "mit", "Ge\u00b7be\u00b7ten"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "APPR", "NN"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Wollst uns im Licht vertreten.", "tokens": ["Wollst", "uns", "im", "Licht", "ver\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}