{"dta.poem.1204": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die krancke Fillis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ach Amor/ soll ich dir nicht klagen meine Noth!", "tokens": ["Ach", "A\u00b7mor", "/", "soll", "ich", "dir", "nicht", "kla\u00b7gen", "mei\u00b7ne", "Noth", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "VMFIN", "PPER", "PPER", "PTKNEG", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich seh die Fillis hier in meinen Armen liegen;", "tokens": ["Ich", "seh", "die", "Fil\u00b7lis", "hier", "in", "mei\u00b7nen", "Ar\u00b7men", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die matte Seele will dem siechen Leib\u2019 entfliegen;", "tokens": ["Die", "mat\u00b7te", "See\u00b7le", "will", "dem", "sie\u00b7chen", "Leib'", "ent\u00b7flie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Stirbt sie/ so ist dein Ruhm und meine Freude todt.", "tokens": ["Stirbt", "sie", "/", "so", "ist", "dein", "Ruhm", "und", "mei\u00b7ne", "Freu\u00b7de", "todt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "ADV", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ach/ schick ihr k\u00fchle Lufft mit deinen Fl\u00fcgeln zu/", "tokens": ["Ach", "/", "schick", "ihr", "k\u00fch\u00b7le", "Lufft", "mit", "dei\u00b7nen", "Fl\u00fc\u00b7geln", "zu", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "VVIMP", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df deine zarte Sehn ihr kranckes Haubt umschl\u00fcssen/", "tokens": ["La\u00df", "dei\u00b7ne", "zar\u00b7te", "Sehn", "ihr", "kran\u00b7ckes", "Haubt", "um\u00b7schl\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gib deinen K\u00f6cher her zu legen unters K\u00fcssen/", "tokens": ["Gib", "dei\u00b7nen", "K\u00f6\u00b7cher", "her", "zu", "le\u00b7gen", "un\u00b7ters", "K\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "APZR", "PTKZU", "VVINF", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Damit ihr Leib erh\u00f6ht kan nehmen seine Ruh.", "tokens": ["Da\u00b7mit", "ihr", "Leib", "er\u00b7h\u00f6ht", "kan", "neh\u00b7men", "sei\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VVFIN", "VMFIN", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Verwechsle mit Betrug dem Tode seinen Pfeil/", "tokens": ["Ver\u00b7wechs\u00b7le", "mit", "Be\u00b7trug", "dem", "To\u00b7de", "sei\u00b7nen", "Pfeil", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie dein heilsam Gold empfind in ihrem Hertzen/", "tokens": ["Da\u00df", "sie", "dein", "heil\u00b7sam", "Gold", "emp\u00b7find", "in", "ih\u00b7rem", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn ihr sein rauher Stahl soll bringen Todes-Schmertzen/", "tokens": ["Wenn", "ihr", "sein", "rau\u00b7her", "Stahl", "soll", "brin\u00b7gen", "To\u00b7des\u00b7Schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VMFIN", "VVINF", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So machest du (in ihr und mir) zwey Hertzen heil.", "tokens": ["So", "ma\u00b7chest", "du", "(", "in", "ihr", "und", "mir", ")", "zwey", "Hert\u00b7zen", "heil", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "APPR", "PPER", "KON", "PPER", "$(", "CARD", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}