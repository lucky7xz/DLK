{"textgrid.poem.30017": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Apollo schrieb nechst au\u00df, da\u00df ieder solte m\u00fcssen", "tokens": ["A\u00b7pol\u00b7lo", "schrieb", "nechst", "au\u00df", ",", "da\u00df", "ie\u00b7der", "sol\u00b7te", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "PTKVZ", "$,", "KOUS", "PIS", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bey ihm sich stellen ein, zu mustern das Gewissen.", "tokens": ["Bey", "ihm", "sich", "stel\u00b7len", "ein", ",", "zu", "mus\u00b7tern", "das", "Ge\u00b7wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "VVFIN", "PTKVZ", "$,", "PTKZU", "VVINF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als di\u00df Gebot ergieng, wie rein hat manche Hand", "tokens": ["Als", "di\u00df", "Ge\u00b7bot", "er\u00b7gieng", ",", "wie", "rein", "hat", "man\u00b7che", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "NN", "VVFIN", "$,", "PWAV", "ADJD", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gewissen vor geputzt mit Lauge, Stroh und Sand!", "tokens": ["Ge\u00b7wis\u00b7sen", "vor", "ge\u00b7putzt", "mit", "Lau\u00b7ge", ",", "Stroh", "und", "Sand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "VVPP", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter":