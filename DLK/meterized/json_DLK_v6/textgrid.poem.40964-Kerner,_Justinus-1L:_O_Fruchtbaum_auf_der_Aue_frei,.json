{"textgrid.poem.40964": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Fruchtbaum auf der Aue frei,", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Fruchtbaum auf der Aue frei,", "tokens": ["O", "Frucht\u00b7baum", "auf", "der", "Au\u00b7e", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie bist du zu beneiden!", "tokens": ["Wie", "bist", "du", "zu", "be\u00b7nei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jedweder Lenz tut dich aufs neu'", "tokens": ["Jed\u00b7we\u00b7der", "Lenz", "tut", "dich", "aufs", "neu'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPRART", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Bl\u00fcten licht bekleiden!", "tokens": ["Mit", "Bl\u00fc\u00b7ten", "licht", "be\u00b7klei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dem armen Menschen unter dir", "tokens": ["Dem", "ar\u00b7men", "Men\u00b7schen", "un\u00b7ter", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist andres Los beschieden!", "tokens": ["Ist", "and\u00b7res", "Los", "be\u00b7schie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Trug er die Frucht, mu\u00df er von hier,", "tokens": ["Trug", "er", "die", "Frucht", ",", "mu\u00df", "er", "von", "hier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "VMFIN", "PPER", "APPR", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und nimmer treibt er Bl\u00fcten.", "tokens": ["Und", "nim\u00b7mer", "treibt", "er", "Bl\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}