{"textgrid.poem.42132": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Galathea", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oh, wie brenn ich vor Verlangen,", "tokens": ["Oh", ",", "wie", "brenn", "ich", "vor", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Galathea, sch\u00f6nes Kind,", "tokens": ["Ga\u00b7la\u00b7thea", ",", "sch\u00f6\u00b7nes", "Kind", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir zu k\u00fcssen deine Wangen,", "tokens": ["Dir", "zu", "k\u00fcs\u00b7sen", "dei\u00b7ne", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil sie so verlockend sind.", "tokens": ["Weil", "sie", "so", "ver\u00b7lo\u00b7ckend", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df ich auch die Gnade f\u00e4nde,", "tokens": ["Da\u00df", "ich", "auch", "die", "Gna\u00b7de", "f\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Galathea, sch\u00f6nes Kind,", "tokens": ["Ga\u00b7la\u00b7thea", ",", "sch\u00f6\u00b7nes", "Kind", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir zu k\u00fcssen deine H\u00e4nde,", "tokens": ["Dir", "zu", "k\u00fcs\u00b7sen", "dei\u00b7ne", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil sie so verlockend sind.", "tokens": ["Weil", "sie", "so", "ver\u00b7lo\u00b7ckend", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und was t\u00e4t ich nicht du s\u00fc\u00dfe", "tokens": ["Und", "was", "t\u00e4t", "ich", "nicht", "du", "s\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "PPER", "PTKNEG", "PPER", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Galathea, sch\u00f6nes Kind,", "tokens": ["Ga\u00b7la\u00b7thea", ",", "sch\u00f6\u00b7nes", "Kind", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir zu k\u00fcssen deine F\u00fc\u00dfe,", "tokens": ["Dir", "zu", "k\u00fcs\u00b7sen", "dei\u00b7ne", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil sie so verlockend sind.", "tokens": ["Weil", "sie", "so", "ver\u00b7lo\u00b7ckend", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und mich treibt der Pulse Stocken,", "tokens": ["Und", "mich", "treibt", "der", "Pul\u00b7se", "Sto\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Galathea, sch\u00f6nes Kind,", "tokens": ["Ga\u00b7la\u00b7thea", ",", "sch\u00f6\u00b7nes", "Kind", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir zu k\u00fcssen deine Locken,", "tokens": ["Dir", "zu", "k\u00fcs\u00b7sen", "dei\u00b7ne", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil sie so verlockend sind.", "tokens": ["Weil", "sie", "so", "ver\u00b7lo\u00b7ckend", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber deinen Mund enth\u00fclle,", "tokens": ["A\u00b7ber", "dei\u00b7nen", "Mund", "ent\u00b7h\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00e4dchen, meinen K\u00fcssen nie,", "tokens": ["M\u00e4d\u00b7chen", ",", "mei\u00b7nen", "K\u00fcs\u00b7sen", "nie", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn in seiner Reize F\u00fclle", "tokens": ["Denn", "in", "sei\u00b7ner", "Rei\u00b7ze", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fc\u00dft ihn nur die Phantasie.", "tokens": ["K\u00fc\u00dft", "ihn", "nur", "die", "Phan\u00b7ta\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}