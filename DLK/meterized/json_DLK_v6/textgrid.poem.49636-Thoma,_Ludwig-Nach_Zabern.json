{"textgrid.poem.49636": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Nach Zabern", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was wollt ihr noch, ihr lahmen Spie\u00dfer?", "tokens": ["Was", "wollt", "ihr", "noch", ",", "ihr", "lah\u00b7men", "Spie\u00b7\u00dfer", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die andern sind Maschinenschie\u00dfer,", "tokens": ["Die", "an\u00b7dern", "sind", "Ma\u00b7schi\u00b7nen\u00b7schie\u00b7\u00dfer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn man B\u00fcrgerstolz auch h\u00e4tte \u2013", "tokens": ["Und", "wenn", "man", "B\u00fcr\u00b7ger\u00b7stolz", "auch", "h\u00e4t\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "NN", "ADV", "VAFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die andern haben Bajonette.", "tokens": ["Die", "an\u00b7dern", "ha\u00b7ben", "Ba\u00b7jo\u00b7net\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch Stolz! Den konnten sich gestatten", "tokens": ["Doch", "Stolz", "!", "Den", "konn\u00b7ten", "sich", "ge\u00b7stat\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$.", "NE", "VMFIN", "PRF", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Gro\u00dfpapas, die ihn noch hatten,", "tokens": ["Die", "Gro\u00df\u00b7pa\u00b7pas", ",", "die", "ihn", "noch", "hat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vielleicht die V\u00e4ter, doch wir Jungen,", "tokens": ["Viel\u00b7leicht", "die", "V\u00e4\u00b7ter", ",", "doch", "wir", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "KON", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir haben nur Begeisterungen.", "tokens": ["Wir", "ha\u00b7ben", "nur", "Be\u00b7geis\u00b7te\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wir haben vaterl\u00e4nd'sche Feiern", "tokens": ["Wir", "ha\u00b7ben", "va\u00b7ter\u00b7l\u00e4n\u00b7d'\u00b7sche", "Fei\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und hochgestimmte Hymnusleiern,", "tokens": ["Und", "hoch\u00b7ges\u00b7timm\u00b7te", "Hym\u00b7nus\u00b7lei\u00b7ern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Geschwoll'ne Jubil\u00e4umsphrasen \u2013", "tokens": ["Ge\u00b7schwoll'\u00b7ne", "Ju\u00b7bi\u00b7l\u00e4\u00b7umsp\u00b7hra\u00b7sen", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Stolz, der ist wie weggeblasen.", "tokens": ["Der", "Stolz", ",", "der", "ist", "wie", "weg\u00b7ge\u00b7bla\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "KOKOM", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und was einmal die braven Alten", "tokens": ["Und", "was", "ein\u00b7mal", "die", "bra\u00b7ven", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In unserm Deutschland hochgehalten,", "tokens": ["In", "un\u00b7serm", "Deutschland", "hoch\u00b7ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Verreckte unter Steuerschrauben,", "tokens": ["Ver\u00b7reck\u00b7te", "un\u00b7ter", "Steu\u00b7er\u00b7schrau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erstickte unter Pickelhauben.", "tokens": ["Er\u00b7stick\u00b7te", "un\u00b7ter", "Pi\u00b7ckel\u00b7hau\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}