{"dta.poem.517": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Liebe um Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wann ich Lisetten soll verehren:", "tokens": ["Wann", "ich", "Li\u00b7set\u00b7ten", "soll", "ver\u00b7eh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NE", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So mu\u00df mir ihre Gunst geneigt entgegen gehn.", "tokens": ["So", "mu\u00df", "mir", "ih\u00b7re", "Gunst", "ge\u00b7neigt", "ent\u00b7ge\u00b7gen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "ADJD", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn warum solt ich mich mit Liebes-Last beschweren/", "tokens": ["Denn", "wa\u00b7rum", "solt", "ich", "mich", "mit", "Lie\u00b7bes\u00b7Last", "be\u00b7schwe\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn ich von ihrer Gunst nicht Lindrung werde sehn.", "tokens": ["Wenn", "ich", "von", "ih\u00b7rer", "Gunst", "nicht", "Lind\u00b7rung", "wer\u00b7de", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PTKNEG", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}