{"textgrid.poem.47043": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O s\u00fc\u00dfe G\u00f6ttin von der heil'gen Myrte,", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "G\u00f6t\u00b7tin", "von", "der", "heil'\u00b7gen", "Myr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo du magst weilen unter Paphos B\u00e4umen,", "tokens": ["Wo", "du", "magst", "wei\u00b7len", "un\u00b7ter", "Pa\u00b7phos", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hieher gelenket sei an goldnen Z\u00e4umen", "tokens": ["Hie\u00b7her", "ge\u00b7len\u00b7ket", "sei", "an", "gold\u00b7nen", "Z\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Wagen, der von Tauben angeschirrte.", "tokens": ["Dein", "Wa\u00b7gen", ",", "der", "von", "Tau\u00b7ben", "an\u00b7ge\u00b7schirr\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und jeder Zephyr, der durch Blumen schwirrte,", "tokens": ["Und", "je\u00b7der", "Ze\u00b7phyr", ",", "der", "durch", "Blu\u00b7men", "schwirr\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Soll deinen Spuren folgen ohne S\u00e4umen", "tokens": ["Soll", "dei\u00b7nen", "Spu\u00b7ren", "fol\u00b7gen", "oh\u00b7ne", "S\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu dieses Thales dir geweihten R\u00e4umen,", "tokens": ["Zu", "die\u00b7ses", "Tha\u00b7les", "dir", "ge\u00b7weih\u00b7ten", "R\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo seine Hirtin heut umf\u00e4ngt ihr Hirte.", "tokens": ["Wo", "sei\u00b7ne", "Hir\u00b7tin", "heut", "um\u00b7f\u00e4ngt", "ihr", "Hir\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Tempe sei der Wiesengrund, der feuchte,", "tokens": ["Ein", "Tem\u00b7pe", "sei", "der", "Wie\u00b7sen\u00b7grund", ",", "der", "feuch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Pindus und Ossa jener H\u00fcgel Kette,", "tokens": ["Pin\u00b7dus", "und", "Os\u00b7sa", "je\u00b7ner", "H\u00fc\u00b7gel", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Peneios Silber dieses Baches Welle;", "tokens": ["Pe\u00b7nei\u00b7os", "Sil\u00b7ber", "die\u00b7ses", "Ba\u00b7ches", "Wel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein jeder Gl\u00fchwurm eines Amors Leuchte,", "tokens": ["Ein", "je\u00b7der", "Gl\u00fch\u00b7wurm", "ei\u00b7nes", "A\u00b7mors", "Leuch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein jeder Schmetterling ein Amorette,", "tokens": ["Ein", "je\u00b7der", "Schmet\u00b7ter\u00b7ling", "ein", "A\u00b7mo\u00b7ret\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Nymphe jede flatternde Libelle.", "tokens": ["Und", "Nym\u00b7phe", "je\u00b7de", "flat\u00b7tern\u00b7de", "Li\u00b7bel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}