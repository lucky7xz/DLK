{"textgrid.poem.61687": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit ich trat in deine Kreise,", "tokens": ["Seit", "ich", "trat", "in", "dei\u00b7ne", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Goldgelockte Zauberin,", "tokens": ["Gold\u00b7ge\u00b7lock\u00b7te", "Zau\u00b7be\u00b7rin", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ward ich frohgemut und weise,", "tokens": ["Ward", "ich", "froh\u00b7ge\u00b7mut", "und", "wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Froh und weise wie Merlin.", "tokens": ["Froh", "und", "wei\u00b7se", "wie", "Mer\u00b7lin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "KOKOM", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie der Falter im Entpuppen", "tokens": ["Wie", "der", "Fal\u00b7ter", "im", "Ent\u00b7pup\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dringt mein Sinn befreit empor;", "tokens": ["Dringt", "mein", "Sinn", "be\u00b7freit", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir vom Auge fiel's wie Schuppen,", "tokens": ["Mir", "vom", "Au\u00b7ge", "fiel's", "wie", "Schup\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und erschossen ward mein Ohr.", "tokens": ["Und", "er\u00b7schos\u00b7sen", "ward", "mein", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jetzt versteh' ich, was im Bache", "tokens": ["Jetzt", "ver\u00b7steh'", "ich", ",", "was", "im", "Ba\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singt und klingt mit frohem Schall,", "tokens": ["Singt", "und", "klingt", "mit", "fro\u00b7hem", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Blumen stille Sprache", "tokens": ["Und", "der", "Blu\u00b7men", "stil\u00b7le", "Spra\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den Schlag der Nachtigall;", "tokens": ["Und", "den", "Schlag", "der", "Nach\u00b7ti\u00b7gall", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lerne, was der Fr\u00fchwind fl\u00fcstert,", "tokens": ["Ler\u00b7ne", ",", "was", "der", "Fr\u00fch\u00b7wind", "fl\u00fcs\u00b7tert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn's im Walde bl\u00fcht und lenzt,", "tokens": ["Wenn's", "im", "Wal\u00b7de", "bl\u00fcht", "und", "lenzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was aus Kluft und Wolke d\u00fcstert,", "tokens": ["Was", "aus", "Kluft", "und", "Wol\u00b7ke", "d\u00fcs\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was aus Sternen niedergl\u00e4nzt.", "tokens": ["Was", "aus", "Ster\u00b7nen", "nie\u00b7der\u00b7gl\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach, und frag' ich dann mit Liedern", "tokens": ["Ach", ",", "und", "frag'", "ich", "dann", "mit", "Lie\u00b7dern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KON", "VVIMP", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dies Stimmgewog' im Kreis,", "tokens": ["In", "dies", "Stimm\u00b7ge\u00b7wo\u00b7g'", "im", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "NN", "APPRART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Kommt so lieblich ein Erwidern,", "tokens": ["Kommt", "so", "lieb\u00b7lich", "ein", "Er\u00b7wi\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich's kaum zu fassen wei\u00df.", "tokens": ["Da\u00df", "ich's", "kaum", "zu", "fas\u00b7sen", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wei\u00dft du, Kind, was all das Schallen", "tokens": ["Wei\u00dft", "du", ",", "Kind", ",", "was", "all", "das", "Schal\u00b7len"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "PRELS", "PIAT", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Laut und leise mir erz\u00e4hlt?", "tokens": ["Laut", "und", "lei\u00b7se", "mir", "er\u00b7z\u00e4hlt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbda\u00df dein Herz getreu vor allen,", "tokens": ["\u00bb", "da\u00df", "dein", "Herz", "ge\u00b7treu", "vor", "al\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPOSAT", "NN", "ADJD", "APPR", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, und da\u00df es mich erw\u00e4hlt.\u00ab", "tokens": ["Ach", ",", "und", "da\u00df", "es", "mich", "er\u00b7w\u00e4hlt", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "KON", "KOUS", "PPER", "PRF", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}