{"textgrid.poem.33687": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Rotkehlchen", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwalben waren schon lang", "tokens": ["Schwal\u00b7ben", "wa\u00b7ren", "schon", "lang"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "ADJD"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Fort und auf der Reise,", "tokens": ["Fort", "und", "auf", "der", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nur ein Rotkehlchen sang", "tokens": ["Nur", "ein", "Rot\u00b7kehl\u00b7chen", "sang"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Lieblich und leise", "tokens": ["Lieb\u00b7lich", "und", "lei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Unter dem Dach", "tokens": ["Un\u00b7ter", "dem", "Dach"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Eines Hauses, das, halb zerst\u00f6rt,", "tokens": ["Ei\u00b7nes", "Hau\u00b7ses", ",", "das", ",", "halb", "zer\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Allm\u00e4hlich zusammenbrach.", "tokens": ["All\u00b7m\u00e4h\u00b7lich", "zu\u00b7sam\u00b7men\u00b7brach", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Es wurde von niemand geh\u00f6rt,", "tokens": ["Es", "wur\u00b7de", "von", "nie\u00b7mand", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Und dennoch sang es. Das Moos", "tokens": ["Und", "den\u00b7noch", "sang", "es", ".", "Das", "Moos"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$.", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Wuchs auf der Schwelle,", "tokens": ["Wuchs", "auf", "der", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Die Steine br\u00f6ckelten los,", "tokens": ["Die", "Stei\u00b7ne", "br\u00f6\u00b7ckel\u00b7ten", "los", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "Des Abendlichtes Helle", "tokens": ["Des", "A\u00b7bend\u00b7lich\u00b7tes", "Hel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Schlief in den Zimmern allein,", "tokens": ["Schlief", "in", "den", "Zim\u00b7mern", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.14": {"text": "Die St\u00fcrme gingen aus und ein", "tokens": ["Die", "St\u00fcr\u00b7me", "gin\u00b7gen", "aus", "und", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "In dem gro\u00dfen ver\u00f6deten Gang,", "tokens": ["In", "dem", "gro\u00b7\u00dfen", "ver\u00b7\u00f6\u00b7de\u00b7ten", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.16": {"text": "Aber das Rotkehlchen sang.", "tokens": ["A\u00b7ber", "das", "Rot\u00b7kehl\u00b7chen", "sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.17": {"text": "Lust und Freude war entflohn,", "tokens": ["Lust", "und", "Freu\u00b7de", "war", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Alles war aus,", "tokens": ["Al\u00b7les", "war", "aus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.19": {"text": "Es wu\u00dfte nichts davon;", "tokens": ["Es", "wu\u00df\u00b7te", "nichts", "da\u00b7von", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Es sang im \u00f6den, verfallenden Haus", "tokens": ["Es", "sang", "im", "\u00f6\u00b7den", ",", "ver\u00b7fal\u00b7len\u00b7den", "Haus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Mit einem eignen lieblichen Ton.", "tokens": ["Mit", "ei\u00b7nem", "eig\u00b7nen", "lieb\u00b7li\u00b7chen", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}