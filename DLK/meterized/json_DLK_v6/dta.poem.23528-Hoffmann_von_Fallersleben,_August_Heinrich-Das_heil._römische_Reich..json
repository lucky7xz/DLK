{"dta.poem.23528": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das heil. r\u00f6mische Reich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! h\u00e4ttest du vom r\u00f6m'schen Wesen", "tokens": ["Ach", "!", "h\u00e4t\u00b7test", "du", "vom", "r\u00f6m'\u00b7schen", "We\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VAFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und von der r\u00f6m'schen Litteratur,", "tokens": ["Und", "von", "der", "r\u00f6m'\u00b7schen", "Lit\u00b7te\u00b7ra\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "O Vaterland, doch nichts gelesen,", "tokens": ["O", "Va\u00b7ter\u00b7land", ",", "doch", "nichts", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts als die r\u00f6m'schen M\u00fcnzen nur!", "tokens": ["Nichts", "als", "die", "r\u00f6m'\u00b7schen", "M\u00fcn\u00b7zen", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch hat uns Rom mit seinen Waffen", "tokens": ["Doch", "hat", "uns", "Rom", "mit", "sei\u00b7nen", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "NE", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nimmer ein solches Leid erdacht,", "tokens": ["Nim\u00b7mer", "ein", "sol\u00b7ches", "Leid", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PIAT", "NN", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Als mit Latein die Lai'n und Pfaffen", "tokens": ["Als", "mit", "La\u00b7tein", "die", "Lai'n", "und", "Pfaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "ART", "NN", "KON", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ueber das deutsche Reich gebracht.", "tokens": ["Ue\u00b7ber", "das", "deut\u00b7sche", "Reich", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Deutsch wird der Papst noch eher lernen,", "tokens": ["Deutsch", "wird", "der", "Papst", "noch", "e\u00b7her", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Eher ein guter Deutscher sein,", "tokens": ["E\u00b7her", "ein", "gu\u00b7ter", "Deut\u00b7scher", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als man geneigt ist zu entfernen", "tokens": ["Als", "man", "ge\u00b7neigt", "ist", "zu", "ent\u00b7fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVPP", "VAFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Endlich aus Deutschland das Latein.", "tokens": ["End\u00b7lich", "aus", "Deutschland", "das", "La\u00b7tein."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "APPR", "NE", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}