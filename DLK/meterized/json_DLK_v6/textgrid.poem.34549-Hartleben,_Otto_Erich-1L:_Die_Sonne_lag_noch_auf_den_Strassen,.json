{"textgrid.poem.34549": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Sonne lag noch auf den Strassen,", "genre": "verse", "period": "N.A.", "pub_year": 1897, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne lag noch auf den Strassen,", "tokens": ["Die", "Son\u00b7ne", "lag", "noch", "auf", "den", "Stras\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "es war am hohen, reifen Tag;", "tokens": ["es", "war", "am", "ho\u00b7hen", ",", "rei\u00b7fen", "Tag", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ein stummer Jubel ohne Massen", "tokens": ["ein", "stum\u00b7mer", "Ju\u00b7bel", "oh\u00b7ne", "Mas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "erh\u00f6hte meines Herzens Schlag.", "tokens": ["er\u00b7h\u00f6h\u00b7te", "mei\u00b7nes", "Her\u00b7zens", "Schlag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es klang in mir ein Spiel der Sinne", "tokens": ["Es", "klang", "in", "mir", "ein", "Spiel", "der", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "aus Kinderlust und Manneskraft,", "tokens": ["aus", "Kin\u00b7der\u00b7lust", "und", "Man\u00b7nes\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und stolz und wonnig ward ich inne", "tokens": ["und", "stolz", "und", "won\u00b7nig", "ward", "ich", "in\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "VAFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "des Gl\u00fccks der freien Wanderschaft.", "tokens": ["des", "Gl\u00fccks", "der", "frei\u00b7en", "Wan\u00b7der\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kein banger F\u00fchrer, der mich leiten,", "tokens": ["Kein", "ban\u00b7ger", "F\u00fch\u00b7rer", ",", "der", "mich", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "kein Freund, der mich begleiten darf;", "tokens": ["kein", "Freund", ",", "der", "mich", "be\u00b7glei\u00b7ten", "darf", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "mein sind die H\u00f6hen, mein die Weiten,", "tokens": ["mein", "sind", "die", "H\u00f6\u00b7hen", ",", "mein", "die", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "$,", "PPOSAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "rauh weht die Luft, so frisch und scharf.", "tokens": ["rauh", "weht", "die", "Luft", ",", "so", "frisch", "und", "scharf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "ADV", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und dennoch s\u00fcss mit sanften M\u00e4chten", "tokens": ["Und", "den\u00b7noch", "s\u00fcss", "mit", "sanf\u00b7ten", "M\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "dringt Sonnenw\u00e4rme tief ins Herz,", "tokens": ["dringt", "Son\u00b7nen\u00b7w\u00e4r\u00b7me", "tief", "ins", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und wie ein Traum aus fernen N\u00e4chten", "tokens": ["und", "wie", "ein", "Traum", "aus", "fer\u00b7nen", "N\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "verschwindet jeder alte Schmerz.", "tokens": ["ver\u00b7schwin\u00b7det", "je\u00b7der", "al\u00b7te", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}