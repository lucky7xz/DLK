{"dta.poem.18685": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Gen\u00fcgsam .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewie irrig w\u00e4hnest du", "tokens": ["\u201e", "wie", "ir\u00b7rig", "w\u00e4h\u00b7nest", "du"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Aus Liebe geh\u00f6re das M\u00e4dchen dir zu.", "tokens": ["Aus", "Lie\u00b7be", "ge\u00b7h\u00f6\u00b7re", "das", "M\u00e4d\u00b7chen", "dir", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Das k\u00f6nnte mich nun gar nicht freuen,", "tokens": ["Das", "k\u00f6nn\u00b7te", "mich", "nun", "gar", "nicht", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "ADV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie versteht sich auf Schmeicheleyen.\u201c", "tokens": ["Sie", "ver\u00b7steht", "sich", "auf", "Schmei\u00b7che\u00b7le\u00b7yen", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$.", "$("], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}}}}