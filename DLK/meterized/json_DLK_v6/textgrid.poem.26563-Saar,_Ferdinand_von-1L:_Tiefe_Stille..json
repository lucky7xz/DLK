{"textgrid.poem.26563": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tiefe Stille.", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tiefe Stille.", "tokens": ["Tie\u00b7fe", "Stil\u00b7le", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Lautlos zieht vor\u00fcber, gespaltenen Laufs,", "tokens": ["Laut\u00b7los", "zieht", "vor\u00b7\u00fc\u00b7ber", ",", "ge\u00b7spal\u00b7te\u00b7nen", "Laufs", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Der breite Donaustrom,", "tokens": ["Der", "brei\u00b7te", "Do\u00b7naus\u00b7trom", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Leis' besp\u00fclend dicht gr\u00fcnendes Ufergezweig.", "tokens": ["Leis'", "be\u00b7sp\u00fc\u00b7lend", "dicht", "gr\u00fc\u00b7nen\u00b7des", "U\u00b7fer\u00b7ge\u00b7zweig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Kaum zum Lispeln bewegt,", "tokens": ["Kaum", "zum", "Lis\u00b7peln", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Schimmern im Sonnenglanz", "tokens": ["Schim\u00b7mern", "im", "Son\u00b7nen\u00b7glanz"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Die Erlen und Silberpappeln,", "tokens": ["Die", "Er\u00b7len", "und", "Sil\u00b7ber\u00b7pap\u00b7peln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Die, aufgewuchert zu lieblicher Wildni\u00df,", "tokens": ["Die", ",", "auf\u00b7ge\u00b7wu\u00b7chert", "zu", "lieb\u00b7li\u00b7cher", "Wild\u00b7ni\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVPP", "PTKZU", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Hochhalmige Wiesenflucht umschatten.", "tokens": ["Hoch\u00b7hal\u00b7mi\u00b7ge", "Wie\u00b7sen\u00b7flucht", "um\u00b7schat\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Manchmal nur ert\u00f6nt der kurze Schrei", "tokens": ["Manch\u00b7mal", "nur", "er\u00b7t\u00f6nt", "der", "kur\u00b7ze", "Schrei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Des Reihers, der einsam die Luft durchkreis't;", "tokens": ["Des", "Rei\u00b7hers", ",", "der", "ein\u00b7sam", "die", "Luft", "durch\u00b7kreis't", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "H\u00f6rbar fast", "tokens": ["H\u00f6r\u00b7bar", "fast"], "token_info": ["word", "word"], "pos": ["ADJD", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Wird des Falters Fl\u00fcgelschlag", "tokens": ["Wird", "des", "Fal\u00b7ters", "Fl\u00fc\u00b7gel\u00b7schlag"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Und der Odem des Reh's,", "tokens": ["Und", "der", "O\u00b7dem", "des", "Reh's", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Das friedlich gras't", "tokens": ["Das", "fried\u00b7lich", "gras't"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ADJD", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Wie in weltferner Sicherheit.", "tokens": ["Wie", "in", "welt\u00b7fer\u00b7ner", "Si\u00b7cher\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PWAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Wo ist die Zeit, da einst", "tokens": ["Wo", "ist", "die", "Zeit", ",", "da", "einst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "KOUS", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mit fremdverworrener Stimmen Laut,", "tokens": ["Mit", "fremd\u00b7ver\u00b7wor\u00b7re\u00b7ner", "Stim\u00b7men", "Laut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mit Waffengeklirr' und Hufgestampf", "tokens": ["Mit", "Waf\u00b7fen\u00b7ge\u00b7klirr'", "und", "Huf\u00b7ge\u00b7stampf"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Des gallischen C\u00e4sars Heer", "tokens": ["Des", "gal\u00b7li\u00b7schen", "C\u00e4\u00b7sars", "Heer"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Auf diesem Boden gelagert!? \u2013", "tokens": ["Auf", "die\u00b7sem", "Bo\u00b7den", "ge\u00b7la\u00b7gert", "!?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Damals, du sonnig stille Insel,", "tokens": ["Da\u00b7mals", ",", "du", "son\u00b7nig", "stil\u00b7le", "In\u00b7sel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Lag unter deinen Wipfeln zusammengedr\u00e4ngt", "tokens": ["Lag", "un\u00b7ter", "dei\u00b7nen", "Wip\u00b7feln", "zu\u00b7sam\u00b7men\u00b7ge\u00b7dr\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Ein Weltschicksal!", "tokens": ["Ein", "Welt\u00b7schick\u00b7sal", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und vernichtet war der kleine", "tokens": ["Und", "ver\u00b7nich\u00b7tet", "war", "der", "klei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "VAFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Gedunsene Mann mit dem Imperatorkopf,", "tokens": ["Ge\u00b7dun\u00b7se\u00b7ne", "Mann", "mit", "dem", "Im\u00b7pe\u00b7ra\u00b7tor\u00b7kopf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+---+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "In dem sich die Ichsucht der Menschheit", "tokens": ["In", "dem", "sich", "die", "Ich\u00b7sucht", "der", "Menschheit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PRF", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Zum tragischen Popanz verk\u00f6rperte.", "tokens": ["Zum", "tra\u00b7gi\u00b7schen", "Po\u00b7panz", "ver\u00b7k\u00f6r\u00b7per\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Kaum erst erf\u00fcllt", "tokens": ["Kaum", "erst", "er\u00b7f\u00fcllt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Hat ein halbes Jahrhundert sich,", "tokens": ["Hat", "ein", "hal\u00b7bes", "Jahr\u00b7hun\u00b7dert", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PRF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Seit er hier auf- und niederschritt,", "tokens": ["Seit", "er", "hier", "auf", "und", "nie\u00b7der\u00b7schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "TRUNC", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entschl\u00fcsse w\u00e4lzend in ruh'loser Brust \u2013:", "tokens": ["Ent\u00b7schl\u00fcs\u00b7se", "w\u00e4l\u00b7zend", "in", "ruh'\u00b7lo\u00b7ser", "Brust", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVPP", "APPR", "ADJA", "NN", "$(", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und heute schl\u00e4gt kaum mehr hin und wieder ein Herz,", "tokens": ["Und", "heu\u00b7te", "schl\u00e4gt", "kaum", "mehr", "hin", "und", "wie\u00b7der", "ein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ADV", "PTKVZ", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Das seinen Ruhm ged\u00fcngt,", "tokens": ["Das", "sei\u00b7nen", "Ruhm", "ge\u00b7d\u00fcngt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Oder vor ihm gezittert.", "tokens": ["O\u00b7der", "vor", "ihm", "ge\u00b7zit\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Wo sind die Reiche, die er gegr\u00fcndet?", "tokens": ["Wo", "sind", "die", "Rei\u00b7che", ",", "die", "er", "ge\u00b7gr\u00fcn\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NE", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Wo die K\u00f6nige, die er besiegt?", "tokens": ["Wo", "die", "K\u00f6\u00b7ni\u00b7ge", ",", "die", "er", "be\u00b7siegt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.10": {"text": "Wo die Frauen, die er geliebt?", "tokens": ["Wo", "die", "Frau\u00b7en", ",", "die", "er", "ge\u00b7liebt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "--+-++-+", "measure": "anapaest.init"}, "line.11": {"text": "Vor\u00fcber Alles. Sonnenbegl\u00e4nzt", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "Al\u00b7les", ".", "Son\u00b7nen\u00b7be\u00b7gl\u00e4nzt"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "PIS", "$.", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Liegt, stromaufw\u00e4rts, die Kaiserstadt,", "tokens": ["Liegt", ",", "strom\u00b7auf\u00b7w\u00e4rts", ",", "die", "Kai\u00b7ser\u00b7stadt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "$,", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.13": {"text": "In die er einzog, sieggewaltig,", "tokens": ["In", "die", "er", "ein\u00b7zog", ",", "sieg\u00b7ge\u00b7wal\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Um ihr blau\u00e4ugiges F\u00fcrstenkind", "tokens": ["Um", "ihr", "blau\u00b7\u00e4u\u00b7gi\u00b7ges", "F\u00fcrs\u00b7ten\u00b7kind"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Mit sich zu f\u00fchren an der Seine Strand.", "tokens": ["Mit", "sich", "zu", "f\u00fch\u00b7ren", "an", "der", "Sei\u00b7ne", "Strand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "PTKZU", "VVINF", "APPR", "ART", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Friedlich liegt sie; bricht die Schanzen ab,", "tokens": ["Fried\u00b7lich", "liegt", "sie", ";", "bricht", "die", "Schan\u00b7zen", "ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$.", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Die er einst gest\u00fcrmt,", "tokens": ["Die", "er", "einst", "ge\u00b7st\u00fcrmt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.18": {"text": "Umzieht sich mit neuen Stra\u00dfen,", "tokens": ["Um\u00b7zieht", "sich", "mit", "neu\u00b7en", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Baut Pal\u00e4ste und Dome,", "tokens": ["Baut", "Pa\u00b7l\u00e4s\u00b7te", "und", "Do\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.20": {"text": "Als g\u00e4lt' es, sich zu gr\u00fcnden f\u00fcr die Ewigkeit \u2013", "tokens": ["Als", "g\u00e4lt'", "es", ",", "sich", "zu", "gr\u00fcn\u00b7den", "f\u00fcr", "die", "E\u00b7wig\u00b7keit", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "$,", "PRF", "PTKZU", "VVINF", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und ahnt nicht,", "tokens": ["Und", "ahnt", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.22": {"text": "Da\u00df auch sie dereinst", "tokens": ["Da\u00df", "auch", "sie", "de\u00b7reinst"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.23": {"text": "Zerbr\u00f6ckeln wird in Schutt und Tr\u00fcmmer,", "tokens": ["Zer\u00b7br\u00f6\u00b7ckeln", "wird", "in", "Schutt", "und", "Tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Um endlich,", "tokens": ["Um", "end\u00b7lich", ","], "token_info": ["word", "word", "punct"], "pos": ["KOUI", "ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Gleich ", "tokens": ["Gleich"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.26": {"text": "Hinweggeschwemmt zu werden", "tokens": ["Hin\u00b7weg\u00b7ge\u00b7schwemmt", "zu", "wer\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "PTKZU", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Vom Strome der Zeiten.", "tokens": ["Vom", "Stro\u00b7me", "der", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}