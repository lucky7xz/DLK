{"dta.poem.685": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "2. Satz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Die heisse quell hatt' einen gang gewonnen/ ", "tokens": ["Die", "heis\u00b7se", "quell", "hatt'", "ei\u00b7nen", "gang", "ge\u00b7won\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der schier erstarten augen r\u00f6hr/", "tokens": ["Der", "schier", "er\u00b7star\u00b7ten", "au\u00b7gen", "r\u00f6hr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die reichen threnen b\u00e4che", "tokens": ["Die", "rei\u00b7chen", "thre\u00b7nen", "b\u00e4\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ergossen sich je mehr vnd mehr/", "tokens": ["Er\u00b7gos\u00b7sen", "sich", "je", "mehr", "vnd", "mehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "KON", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der strom von dem ich spreche/", "tokens": ["Der", "strom", "von", "dem", "ich", "spre\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist tag vnd nacht/ ja f\u00fcr vnd f\u00fcr geronnen.", "tokens": ["Ist", "tag", "vnd", "nacht", "/", "ja", "f\u00fcr", "vnd", "f\u00fcr", "ge\u00b7ron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$(", "ADV", "APPR", "KON", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "HeRR das ich jtzt vorbreche ", "tokens": ["HeRR", "das", "ich", "jtzt", "vor\u00b7bre\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PRELS", "PPER", "ADV", "ADJA"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Mit jauchtzen HErr! das machstu meine Sonnen ", "tokens": ["Mit", "jaucht\u00b7zen", "Herr", "!", "das", "machs\u00b7tu", "mei\u00b7ne", "Son\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "PDS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Mein trost/ mein schutz/ mein ruhm vnd ehr.", "tokens": ["Mein", "trost", "/", "mein", "schutz", "/", "mein", "ruhm", "vnd", "ehr", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}