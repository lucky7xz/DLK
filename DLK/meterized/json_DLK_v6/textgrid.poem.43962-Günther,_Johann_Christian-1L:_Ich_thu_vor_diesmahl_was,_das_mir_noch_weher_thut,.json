{"textgrid.poem.43962": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich thu vor diesmahl was, das mir noch weher thut,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich thu vor diesmahl was, das mir noch weher thut,", "tokens": ["Ich", "thu", "vor", "dies\u00b7mahl", "was", ",", "das", "mir", "noch", "we\u00b7her", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "PIS", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als wenn ich mich gleich selbst ins Herze sto\u00dfen m\u00fcste;", "tokens": ["Als", "wenn", "ich", "mich", "gleich", "selbst", "ins", "Her\u00b7ze", "sto\u00b7\u00dfen", "m\u00fcs\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PRF", "ADV", "ADV", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du Himmel weist es wohl, wenn die es gleich nicht w\u00fcste,", "tokens": ["Du", "Him\u00b7mel", "weist", "es", "wohl", ",", "wenn", "die", "es", "gleich", "nicht", "w\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "PPER", "ADV", "$,", "KOUS", "ART", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die jezt mein Wuntsch beschwert und die mein treues Blut", "tokens": ["Die", "jezt", "mein", "Wunt\u00b7sch", "be\u00b7schwert", "und", "die", "mein", "treu\u00b7es", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVPP", "KON", "ART", "PPOSAT", "ADJA", "NN"], "meter": "----+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Vielleicht zwar etwas kennt, doch noch nicht recht erkennet,", "tokens": ["Viel\u00b7leicht", "zwar", "et\u00b7was", "kennt", ",", "doch", "noch", "nicht", "recht", "er\u00b7ken\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "VVFIN", "$,", "ADV", "ADV", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df kein genieslich Docht bey meiner Liebe brennet.", "tokens": ["Da\u00df", "kein", "ge\u00b7nies\u00b7lich", "Docht", "bey", "mei\u00b7ner", "Lie\u00b7be", "bren\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJD", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Inde\u00dfen, da die Noth das Eu\u00dferste befiehlt,", "tokens": ["In\u00b7de\u00b7\u00dfen", ",", "da", "die", "Noth", "das", "Eu\u00b7\u00dfers\u00b7te", "be\u00b7fiehlt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Zu wem soll ich wohl sonst die lezte Zuflucht nehmen,", "tokens": ["Zu", "wem", "soll", "ich", "wohl", "sonst", "die", "lez\u00b7te", "Zu\u00b7flucht", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VMFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da Eltern, Freund und Neid mich \u00fcberall besch\u00e4men", "tokens": ["Da", "El\u00b7tern", ",", "Freund", "und", "Neid", "mich", "\u00fc\u00b7be\u00b7rall", "be\u00b7sch\u00e4\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und so viel \u00c4rgern\u00fc\u00df mit meinen Thr\u00e4nen spielt?", "tokens": ["Und", "so", "viel", "\u00c4r\u00b7ger\u00b7n\u00fc\u00df", "mit", "mei\u00b7nen", "Thr\u00e4\u00b7nen", "spielt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und w\u00fcrd auch dir, mein Kind, mein redlich Herz verd\u00e4chtig,", "tokens": ["Und", "w\u00fcrd", "auch", "dir", ",", "mein", "Kind", ",", "mein", "red\u00b7lich", "Herz", "ver\u00b7d\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPER", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So fa\u00df ich mich hiermit: Der Himmel ist noch m\u00e4chtig.", "tokens": ["So", "fa\u00df", "ich", "mich", "hier\u00b7mit", ":", "Der", "Him\u00b7mel", "ist", "noch", "m\u00e4ch\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PAV", "$.", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Schwermuth schl\u00e4gt die Feder hin", "tokens": ["Die", "Schwer\u00b7muth", "schl\u00e4gt", "die", "Fe\u00b7der", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schreibt nichts mehr als: Leonore!", "tokens": ["Und", "schreibt", "nichts", "mehr", "als", ":", "Le\u00b7o\u00b7no\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PIS", "KOKOM", "$.", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gedenck an Schweidniz, Roschkwiz, Bore;", "tokens": ["Ge\u00b7denck", "an", "Schweid\u00b7niz", ",", "Roschk\u00b7wiz", ",", "Bo\u00b7re", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NE", "$,", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dies, was ich dort gewesen bin,", "tokens": ["Dies", ",", "was", "ich", "dort", "ge\u00b7we\u00b7sen", "bin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "ADV", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das werd ich ewig seyn und bleiben.", "tokens": ["Das", "werd", "ich", "e\u00b7wig", "seyn", "und", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "VAINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und l\u00e4st mich auch zulezt dein Herz,", "tokens": ["Und", "l\u00e4st", "mich", "auch", "zu\u00b7lezt", "dein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So soll vor deinen Ku\u00df der Schmerz", "tokens": ["So", "soll", "vor", "dei\u00b7nen", "Ku\u00df", "der", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mir noch den kurzen Rest der Zeit mit Lust vertreiben.", "tokens": ["Mir", "noch", "den", "kur\u00b7zen", "Rest", "der", "Zeit", "mit", "Lust", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}