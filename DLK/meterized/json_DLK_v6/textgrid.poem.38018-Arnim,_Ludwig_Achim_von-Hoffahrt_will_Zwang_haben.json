{"textgrid.poem.38018": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Hoffahrt will Zwang haben", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du verdammtes Adelleben!", "tokens": ["O", "du", "ver\u00b7damm\u00b7tes", "A\u00b7del\u00b7le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O du verdammter Fr\u00e4uleinstand!", "tokens": ["O", "du", "ver\u00b7damm\u00b7ter", "Fr\u00e4u\u00b7lein\u00b7stand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt will ich mich der Lieb ergeben,", "tokens": ["Jetzt", "will", "ich", "mich", "der", "Lieb", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Adel bricht mein Liebesband:", "tokens": ["Der", "A\u00b7del", "bricht", "mein", "Lie\u00b7bes\u00b7band", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach dacht ich oft bey mir so sehr,", "tokens": ["Ach", "dacht", "ich", "oft", "bey", "mir", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach wenn ich nur kein Fr\u00e4ulein w\u00e4r.", "tokens": ["Ach", "wenn", "ich", "nur", "kein", "Fr\u00e4u\u00b7lein", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "ADV", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zu Morgens fr\u00fch, wenn ich aufstehe,", "tokens": ["Zu", "Mor\u00b7gens", "fr\u00fch", ",", "wenn", "ich", "auf\u00b7ste\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da putzet gleich mich die Mamsell,", "tokens": ["Da", "put\u00b7zet", "gleich", "mich", "die", "Mam\u00b7sell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach wenn ich in mein Schn\u00fcrleib sehe,", "tokens": ["Ach", "wenn", "ich", "in", "mein", "Schn\u00fcr\u00b7leib", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+---", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ich das Gef\u00e4ngni\u00df mir vorstell. Ach dacht usw.", "tokens": ["Ich", "das", "Ge\u00b7f\u00e4ng\u00b7ni\u00df", "mir", "vor\u00b7stell", ".", "Ach", "dacht", "us\u00b7w."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["PPER", "ART", "NN", "PPER", "ADJD", "$.", "ITJ", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O du Gef\u00e4ngni\u00df meines Leibes!", "tokens": ["O", "du", "Ge\u00b7f\u00e4ng\u00b7ni\u00df", "mei\u00b7nes", "Lei\u00b7bes", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Brust in goldnen Ketten liegt,", "tokens": ["Die", "Brust", "in", "gold\u00b7nen", "Ket\u00b7ten", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O h\u00e4tt ich doch des Zeitvertreibes,", "tokens": ["O", "h\u00e4tt", "ich", "doch", "des", "Zeit\u00b7ver\u00b7trei\u00b7bes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wovon die Kammerjungfer spricht. Ach dacht usw.", "tokens": ["Wo\u00b7von", "die", "Kam\u00b7mer\u00b7jung\u00b7fer", "spricht", ".", "Ach", "dacht", "us\u00b7w."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$.", "ITJ", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Denn wenn ich in die Kirch thu fahren,", "tokens": ["Denn", "wenn", "ich", "in", "die", "Kirch", "thu", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So h\u00fctet streng mich die Mamsell,", "tokens": ["So", "h\u00fc\u00b7tet", "streng", "mich", "die", "Mam\u00b7sell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--++", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da seh ich die verliebten Paare,", "tokens": ["Da", "seh", "ich", "die", "ver\u00b7lieb\u00b7ten", "Paa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und jede Dirn, wies ihr gef\u00e4llt. Ach dacht usw.", "tokens": ["Und", "je\u00b7de", "Dirn", ",", "wies", "ihr", "ge\u00b7f\u00e4llt", ".", "Ach", "dacht", "us\u00b7w."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["KON", "PIAT", "NN", "$,", "VVFIN", "PPER", "VVPP", "$.", "ITJ", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Will ich mit sch\u00f6nen Knaben reden,", "tokens": ["Will", "ich", "mit", "sch\u00f6\u00b7nen", "Kna\u00b7ben", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie neigen sich in Demuth gleich,", "tokens": ["Sie", "nei\u00b7gen", "sich", "in", "De\u00b7muth", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und merkens nicht, wie gern ich jedem", "tokens": ["Und", "mer\u00b7kens", "nicht", ",", "wie", "gern", "ich", "je\u00b7dem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKNEG", "$,", "PWAV", "ADV", "PPER", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sogleich den Mund zum K\u00fcssen reich. Ach dacht usw.", "tokens": ["Sog\u00b7leich", "den", "Mund", "zum", "K\u00fcs\u00b7sen", "reich", ".", "Ach", "dacht", "us\u00b7w."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "ADJD", "$.", "ITJ", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Was sch\u00f6ne Sp\u00e4sse mu\u00df ich sehen", "tokens": ["Was", "sch\u00f6\u00b7ne", "Sp\u00e4s\u00b7se", "mu\u00df", "ich", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJA", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Knecht und Magd auf offner Stra\u00df,", "tokens": ["Von", "Knecht", "und", "Magd", "auf", "off\u00b7ner", "Stra\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch mu\u00df ich gleich vom Fenster gehen,", "tokens": ["Doch", "mu\u00df", "ich", "gleich", "vom", "Fens\u00b7ter", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn die Mamsell erblickt den Spa\u00df. Ach dacht usw.", "tokens": ["Wenn", "die", "Mam\u00b7sell", "er\u00b7blickt", "den", "Spa\u00df", ".", "Ach", "dacht", "us\u00b7w."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ART", "NN", "$.", "ITJ", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Drum will ich meinen Stand verwandeln, Will eine Bauerdirne seyn,", "tokens": ["Drum", "will", "ich", "mei\u00b7nen", "Stand", "ver\u00b7wan\u00b7deln", ",", "Will", "ei\u00b7ne", "Bau\u00b7er\u00b7dir\u00b7ne", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Damit ich nicht modest mu\u00df wandern,", "tokens": ["Da\u00b7mit", "ich", "nicht", "mo\u00b7dest", "mu\u00df", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VMFIN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und krank ins Fr\u00e4uleinstift hinein;", "tokens": ["Und", "krank", "ins", "Fr\u00e4u\u00b7lein\u00b7stift", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bald denke ich nun gar nicht mehr,", "tokens": ["Bald", "den\u00b7ke", "ich", "nun", "gar", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df ich ein Fr\u00e4ulein war und w\u00e4r.", "tokens": ["Da\u00df", "ich", "ein", "Fr\u00e4u\u00b7lein", "war", "und", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "KON", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}