{"dta.poem.6827": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lasst unsern Geist mit Ernst von neuen \u00fcberlegen,", "tokens": ["Lasst", "un\u00b7sern", "Geist", "mit", "Ernst", "von", "neu\u00b7en", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NE", "APPR", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie wunderbar des C\u00f6rpers Bau formirt!", "tokens": ["Wie", "wun\u00b7der\u00b7bar", "des", "C\u00f6r\u00b7pers", "Bau", "for\u00b7mirt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Viel Nerven, um ihn zu bewegen,", "tokens": ["Viel", "Ner\u00b7ven", ",", "um", "ihn", "zu", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind allenthalben hingef\u00fchrt.", "tokens": ["Sind", "al\u00b7len\u00b7thal\u00b7ben", "hin\u00b7ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bis zu dem \u00e4usersten von seinen Theilen,", "tokens": ["Bis", "zu", "dem", "\u00e4u\u00b7sers\u00b7ten", "von", "sei\u00b7nen", "Thei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Sieht man dieselbigen gleich kleinen Seilen.", "tokens": ["Sieht", "man", "die\u00b7sel\u00b7bi\u00b7gen", "gleich", "klei\u00b7nen", "Sei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PDAT", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Haut, von welcher wir umgeben seyn,", "tokens": ["Die", "Haut", ",", "von", "wel\u00b7cher", "wir", "um\u00b7ge\u00b7ben", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ist gantz davon gewebt, die R\u00f6hren, die so klein,", "tokens": ["Ist", "gantz", "da\u00b7von", "ge\u00b7webt", ",", "die", "R\u00f6h\u00b7ren", ",", "die", "so", "klein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PAV", "VVPP", "$,", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zusammt den M\u00e4uselein,", "tokens": ["Zu\u00b7sammt", "den", "M\u00e4u\u00b7sel\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "In welchen sich ein fl\u00fcssend Feuer reget,", "tokens": ["In", "wel\u00b7chen", "sich", "ein", "fl\u00fcs\u00b7send", "Feu\u00b7er", "re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PRF", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Sind Federchen, wodurch der C\u00f6rper sich beweget.", "tokens": ["Sind", "Fe\u00b7der\u00b7chen", ",", "wo\u00b7durch", "der", "C\u00f6r\u00b7per", "sich", "be\u00b7we\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}