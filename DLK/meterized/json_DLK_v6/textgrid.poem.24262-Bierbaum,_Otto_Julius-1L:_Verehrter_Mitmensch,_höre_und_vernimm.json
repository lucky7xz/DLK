{"textgrid.poem.24262": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verehrter Mitmensch, h\u00f6re und vernimm", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verehrter Mitmensch, h\u00f6re und vernimm", "tokens": ["Ver\u00b7ehr\u00b7ter", "Mit\u00b7mensch", ",", "h\u00f6\u00b7re", "und", "ver\u00b7nimm"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "KON", "XY"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Freundwillig mit Hulden und ohne Grimm:", "tokens": ["Freund\u00b7wil\u00b7lig", "mit", "Hul\u00b7den", "und", "oh\u00b7ne", "Grimm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "APPR", "NE", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Dieweil es nun Ostern geworden ist,", "tokens": ["Die\u00b7weil", "es", "nun", "Os\u00b7tern", "ge\u00b7wor\u00b7den", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VAPP", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Sollst du, von welcher Art du auch bist,", "tokens": ["Sollst", "du", ",", "von", "wel\u00b7cher", "Art", "du", "auch", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "APPR", "PWAT", "NN", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Ob Heide, Jude, Moslem, Christ,", "tokens": ["Ob", "Hei\u00b7de", ",", "Ju\u00b7de", ",", "Mos\u00b7lem", ",", "Christ", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "NE", "$,", "NN", "$,", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durchaus vergn\u00fcgt im Herzen sein,", "tokens": ["Durc\u00b7haus", "ver\u00b7gn\u00fcgt", "im", "Her\u00b7zen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPRART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Osterw\u00fcrdig und osterrein.", "tokens": ["Os\u00b7ter\u00b7w\u00fcr\u00b7dig", "und", "os\u00b7ter\u00b7rein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Mit einem Birkenreise kehre", "tokens": ["Mit", "ei\u00b7nem", "Bir\u00b7ken\u00b7rei\u00b7se", "keh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus deiner Seele den Geist der Schwere!", "tokens": ["Aus", "dei\u00b7ner", "See\u00b7le", "den", "Geist", "der", "Schwe\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Wenns und Abers und Achs und Os,", "tokens": ["Der", "Wenns", "und", "A\u00b7bers", "und", "Achs", "und", "Os", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KON", "CARD", "KON", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die hart und starr dein Herz umwindet,", "tokens": ["Die", "hart", "und", "starr", "dein", "Herz", "um\u00b7win\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df der Geist der Leichte kaum Eingang findet,", "tokens": ["Da\u00df", "der", "Geist", "der", "Leich\u00b7te", "kaum", "Ein\u00b7gang", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.6": {"text": "Mache dich hurtig und heiter los!", "tokens": ["Ma\u00b7che", "dich", "hur\u00b7tig", "und", "hei\u00b7ter", "los", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Du brauchst nichts weiter dazuzutun,", "tokens": ["Du", "brauchst", "nichts", "wei\u00b7ter", "da\u00b7zu\u00b7zu\u00b7tun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "PAV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als dich im Gr\u00fcnen auszuruhn.", "tokens": ["Als", "dich", "im", "Gr\u00fc\u00b7nen", "aus\u00b7zu\u00b7ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da atmet sichs sehr wonnig ein,", "tokens": ["Da", "at\u00b7met", "sichs", "sehr", "won\u00b7nig", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was dir das Herz macht frei und rein:", "tokens": ["Was", "dir", "das", "Herz", "macht", "frei", "und", "rein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der jungen Blumen frischer Hauch;", "tokens": ["Der", "jun\u00b7gen", "Blu\u00b7men", "fri\u00b7scher", "Hauch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und die Augen haben der Wonne auch,", "tokens": ["Und", "die", "Au\u00b7gen", "ha\u00b7ben", "der", "Won\u00b7ne", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Denn nichts ist lieblicher anzusehn,", "tokens": ["Denn", "nichts", "ist", "lieb\u00b7li\u00b7cher", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADJD", "VVIZU", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Als wie sie da hold beisammenstehn,", "tokens": ["Als", "wie", "sie", "da", "hold", "bei\u00b7sam\u00b7men\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "Blau, wei\u00df und rosa, klar und licht,", "tokens": ["Blau", ",", "wei\u00df", "und", "ro\u00b7sa", ",", "klar", "und", "licht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "NE", "$,", "ADJD", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Der Erde s\u00fc\u00dfestes Ostergedicht.", "tokens": ["Der", "Er\u00b7de", "s\u00fc\u00b7\u00dfes\u00b7tes", "Os\u00b7ter\u00b7ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "An ihnen dir ein Beispiel zu nehmen,", "tokens": ["An", "ih\u00b7nen", "dir", "ein", "Bei\u00b7spiel", "zu", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sollst du, ach Mensch, dich keineswegs sch\u00e4men!", "tokens": ["Sollst", "du", ",", "ach", "Mensch", ",", "dich", "kei\u00b7nes\u00b7wegs", "sch\u00e4\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "XY", "NN", "$,", "PPER", "ADV", "VVINF", "$."], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Vergi\u00df dein Gehirn eine Weile und sei", "tokens": ["Ver\u00b7gi\u00df", "dein", "Ge\u00b7hirn", "ei\u00b7ne", "Wei\u00b7le", "und", "sei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "ART", "NN", "KON", "VAFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Gedankenlos dem lieben Leben", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7los", "dem", "lie\u00b7ben", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Blumeninnig hingegeben;", "tokens": ["Blu\u00b7me\u00b7nin\u00b7nig", "hin\u00b7ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vergi\u00df dein Begehren, vergi\u00df dein Streben", "tokens": ["Ver\u00b7gi\u00df", "dein", "Be\u00b7geh\u00b7ren", ",", "ver\u00b7gi\u00df", "dein", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "VVIMP", "PPOSAT", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und sei in seliger Einfalt frei", "tokens": ["Und", "sei", "in", "se\u00b7li\u00b7ger", "Ein\u00b7falt", "frei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Des Zwangs, der dich durchs Hirn regiert!", "tokens": ["Des", "Zwangs", ",", "der", "dich", "durchs", "Hirn", "re\u00b7giert", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er hat dich freilich hoch gef\u00fchrt", "tokens": ["Er", "hat", "dich", "frei\u00b7lich", "hoch", "ge\u00b7f\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und vieles dir zu wissen gegeben,", "tokens": ["Und", "vie\u00b7les", "dir", "zu", "wis\u00b7sen", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPER", "PTKZU", "VVINF", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Aber das allertiefste Leben", "tokens": ["A\u00b7ber", "das", "al\u00b7ler\u00b7tiefs\u00b7te", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wird nicht gewu\u00dft, wird nur gesp\u00fcrt.", "tokens": ["Wird", "nicht", "ge\u00b7wu\u00dft", ",", "wird", "nur", "ge\u00b7sp\u00fcrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVPP", "$,", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Blumen zarte Wurzeln f\u00fchlen", "tokens": ["Der", "Blu\u00b7men", "zar\u00b7te", "Wur\u00b7zeln", "f\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im keimlebendigen, fr\u00fchlingsk\u00fchlen", "tokens": ["Im", "keim\u00b7le\u00b7ben\u00b7di\u00b7gen", ",", "fr\u00fch\u00b7lings\u00b7k\u00fch\u00b7len"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPRART", "VVINF", "$,", "ADJA"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Erdboden mehr von ihm als du.", "tokens": ["Erd\u00b7bo\u00b7den", "mehr", "von", "ihm", "als", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPER", "KOUS", "PPER", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und bist doch auch ein Kind der Erde.", "tokens": ["Und", "bist", "doch", "auch", "ein", "Kind", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df sie nicht sinnenfremd dir werde,", "tokens": ["Da\u00df", "sie", "nicht", "sin\u00b7nen\u00b7fremd", "dir", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VAFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wende ihr heut die Sinne zu!", "tokens": ["Wen\u00b7de", "ihr", "heut", "die", "Sin\u00b7ne", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Das ist der festlich tiefe Sinn", "tokens": ["Das", "ist", "der", "fest\u00b7lich", "tie\u00b7fe", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Ostertage: Mit Entz\u00fccken", "tokens": ["Der", "Os\u00b7ter\u00b7ta\u00b7ge", ":", "Mit", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sollst du zum Mutterscho\u00df dich b\u00fccken.", "tokens": ["Sollst", "du", "zum", "Mut\u00b7ter\u00b7scho\u00df", "dich", "b\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gib heut, o Mensch, dich innerst zu begl\u00fccken,", "tokens": ["Gib", "heut", ",", "o", "Mensch", ",", "dich", "in\u00b7nerst", "zu", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "FM", "NN", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Der Mutter Erde fr\u00fchlingsfromm dich hin!", "tokens": ["Der", "Mut\u00b7ter", "Er\u00b7de", "fr\u00fch\u00b7lings\u00b7fromm", "dich", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}