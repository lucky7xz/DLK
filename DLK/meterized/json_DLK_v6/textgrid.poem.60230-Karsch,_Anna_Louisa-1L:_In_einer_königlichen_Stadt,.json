{"textgrid.poem.60230": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: In einer k\u00f6niglichen Stadt,", "genre": "verse", "period": "N.A.", "pub_year": 1756, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einer k\u00f6niglichen Stadt,", "tokens": ["In", "ei\u00b7ner", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die gro\u00dfen Prunk und gro\u00dfe S\u00fcnder", "tokens": ["Die", "gro\u00b7\u00dfen", "Prunk", "und", "gro\u00b7\u00dfe", "S\u00fcn\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und H\u00e4user hatte, wie Berlinathen sie hat,", "tokens": ["Und", "H\u00e4u\u00b7ser", "hat\u00b7te", ",", "wie", "Ber\u00b7li\u00b7na\u00b7then", "sie", "hat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "$,", "PWAV", "NN", "PPER", "VAFIN", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Zog Aphrosine zwey zugleich gebohrne Kinder", "tokens": ["Zog", "A\u00b7phro\u00b7si\u00b7ne", "zwey", "zu\u00b7gleich", "ge\u00b7bohr\u00b7ne", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "CARD", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nunmehr ins zehnte Jahr, und sparte keinen Flei\u00df", "tokens": ["Nun\u00b7mehr", "ins", "zehn\u00b7te", "Jahr", ",", "und", "spar\u00b7te", "kei\u00b7nen", "Flei\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,", "KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und keine Sorgfalt, sie zur Tugend aufzuziehen.", "tokens": ["Und", "kei\u00b7ne", "Sorg\u00b7falt", ",", "sie", "zur", "Tu\u00b7gend", "auf\u00b7zu\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PPER", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn Madchen, die so roth und wei\u00df", "tokens": ["Denn", "Mad\u00b7chen", ",", "die", "so", "roth", "und", "wei\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PRELS", "ADV", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Lilien und Rosen bl\u00fchen,", "tokens": ["Wie", "Li\u00b7li\u00b7en", "und", "Ro\u00b7sen", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sind ohne Furcht und Sitten, nichts.", "tokens": ["Sind", "oh\u00b7ne", "Furcht", "und", "Sit\u00b7ten", ",", "nichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "KON", "NN", "$,", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sch\u00f6n waren diese T\u00f6chter beyde:", "tokens": ["Sch\u00f6n", "wa\u00b7ren", "die\u00b7se", "T\u00f6ch\u00b7ter", "bey\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PDAT", "NN", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Feinheit ihres Angesichts", "tokens": ["Die", "Fein\u00b7heit", "ih\u00b7res", "An\u00b7ge\u00b7sichts"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gab ihrer Mutter Trost im schweren Wittwenkleide;", "tokens": ["Gab", "ih\u00b7rer", "Mut\u00b7ter", "Trost", "im", "schwe\u00b7ren", "Witt\u00b7wen\u00b7klei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch ihres Busens Liebling war", "tokens": ["Doch", "ih\u00b7res", "Bu\u00b7sens", "Lieb\u00b7ling", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die lieblichl\u00e4chelnde Dorine;", "tokens": ["Die", "lieb\u00b7lich\u00b7l\u00e4\u00b7cheln\u00b7de", "Do\u00b7ri\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ihr zarter Wuchs, ihr dunkles Haar", "tokens": ["Ihr", "zar\u00b7ter", "Wuchs", ",", "ihr", "dunk\u00b7les", "Haar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und ihre geistbelebte Miene", "tokens": ["Und", "ih\u00b7re", "geist\u00b7be\u00b7leb\u00b7te", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und feuerreiches Augenpaar,", "tokens": ["Und", "feu\u00b7er\u00b7rei\u00b7ches", "Au\u00b7gen\u00b7paar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Zwang jeden Menschen, der sie sahe,", "tokens": ["Zwang", "je\u00b7den", "Men\u00b7schen", ",", "der", "sie", "sa\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Zur staunenden Bewunderung;", "tokens": ["Zur", "stau\u00b7nen\u00b7den", "Be\u00b7wun\u00b7de\u00b7rung", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und da dies schon anitzt geschahe,", "tokens": ["Und", "da", "dies", "schon", "a\u00b7nitzt", "ge\u00b7scha\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PDS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Da noch ihr Auge viel zu jung", "tokens": ["Da", "noch", "ihr", "Au\u00b7ge", "viel", "zu", "jung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "ADV", "PTKA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Zum Herzbestricken war; was w\u00e4re nicht geschehen,", "tokens": ["Zum", "Herz\u00b7be\u00b7stri\u00b7cken", "war", ";", "was", "w\u00e4\u00b7re", "nicht", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "$.", "PWS", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wenn sie mit reifgewordner Macht", "tokens": ["Wenn", "sie", "mit", "reif\u00b7ge\u00b7word\u00b7ner", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Den J\u00fcngling siegend angesehen,", "tokens": ["Den", "J\u00fcng\u00b7ling", "sie\u00b7gend", "an\u00b7ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und ihrer \u00e4u\u00dfern Reize Pracht", "tokens": ["Und", "ih\u00b7rer", "\u00e4u\u00b7\u00dfern", "Rei\u00b7ze", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Mit innren Reizungen vermehret?", "tokens": ["Mit", "inn\u00b7ren", "Rei\u00b7zun\u00b7gen", "ver\u00b7meh\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.21": {"text": "Die Mutter sahe schon voraus", "tokens": ["Die", "Mut\u00b7ter", "sa\u00b7he", "schon", "vo\u00b7raus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Von einem Eydam sich verehret,", "tokens": ["Von", "ei\u00b7nem", "Ey\u00b7dam", "sich", "ver\u00b7eh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Der ihre Tochter in ein goldgeschm\u00fccktes Haus,", "tokens": ["Der", "ih\u00b7re", "Toch\u00b7ter", "in", "ein", "gold\u00b7ge\u00b7schm\u00fcck\u00b7tes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Pal\u00e4sten \u00e4hnlich, f\u00fchren w\u00fcrde.", "tokens": ["Pa\u00b7l\u00e4s\u00b7ten", "\u00e4hn\u00b7lich", ",", "f\u00fch\u00b7ren", "w\u00fcr\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Die zwote Tochter, Aphrosis,", "tokens": ["Die", "zwo\u00b7te", "Toch\u00b7ter", ",", "A\u00b7phro\u00b7sis", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "War etwas minder sch\u00f6n, doch fromm war sie gewi\u00df.", "tokens": ["War", "et\u00b7was", "min\u00b7der", "sch\u00f6n", ",", "doch", "fromm", "war", "sie", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJD", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Sie machte stets die gro\u00dfe B\u00fcrde", "tokens": ["Sie", "mach\u00b7te", "stets", "die", "gro\u00b7\u00dfe", "B\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Der schmerzlichsten Bek\u00fcmmerni\u00df", "tokens": ["Der", "schmerz\u00b7lichs\u00b7ten", "Be\u00b7k\u00fcm\u00b7mer\u00b7ni\u00df"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.29": {"text": "In ihrer Mutter Herzen leichter;", "tokens": ["In", "ih\u00b7rer", "Mut\u00b7ter", "Her\u00b7zen", "leich\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Wenn ihre beyde Wangen feuchter", "tokens": ["Wenn", "ih\u00b7re", "bey\u00b7de", "Wan\u00b7gen", "feuch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "PIAT", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Von Thr\u00e4nen waren, als vom Fall", "tokens": ["Von", "Thr\u00e4\u00b7nen", "wa\u00b7ren", ",", "als", "vom", "Fall"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Des Abendthau's die Sonnenblume,", "tokens": ["Des", "A\u00b7bendt\u00b7hau's", "die", "Son\u00b7nen\u00b7blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Dann sang die Tochter s\u00fc\u00df, gleich einer Nachtigall,", "tokens": ["Dann", "sang", "die", "Toch\u00b7ter", "s\u00fc\u00df", ",", "gleich", "ei\u00b7ner", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ein heilig Lied zum G\u00f6tterlob und Ruhme,", "tokens": ["Ein", "hei\u00b7lig", "Lied", "zum", "G\u00f6t\u00b7ter\u00b7lob", "und", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Und k\u00fc\u00dfete der Mutter Thr\u00e4nen vom Gesicht,", "tokens": ["Und", "k\u00fc\u00b7\u00dfe\u00b7te", "der", "Mut\u00b7ter", "Thr\u00e4\u00b7nen", "vom", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Das gute Kind! Sie wu\u00dft es nicht,", "tokens": ["Das", "gu\u00b7te", "Kind", "!", "Sie", "wu\u00dft", "es", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Ihr Vater gieng vor sieben Jahren", "tokens": ["Ihr", "Va\u00b7ter", "gieng", "vor", "sie\u00b7ben", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "An einen schilfbewachsnen Flu\u00df,", "tokens": ["An", "ei\u00b7nen", "schilf\u00b7be\u00b7wachs\u00b7nen", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Und t\u00f6dtete mit einem Bogenschu\u00df", "tokens": ["Und", "t\u00f6d\u00b7te\u00b7te", "mit", "ei\u00b7nem", "Bo\u00b7gen\u00b7schu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Zween Wasserv\u00f6gel, die dem Flu\u00dfgott heilig waren;", "tokens": ["Zween", "Was\u00b7ser\u00b7v\u00f6\u00b7gel", ",", "die", "dem", "Flu\u00df\u00b7gott", "hei\u00b7lig", "wa\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Und stieg itzt in ein kleines Boot,", "tokens": ["Und", "stieg", "itzt", "in", "ein", "klei\u00b7nes", "Boot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Um seine Beute sich zu hohlen,", "tokens": ["Um", "sei\u00b7ne", "Beu\u00b7te", "sich", "zu", "hoh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PRF", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Und fand im Wasser seinen Todt,", "tokens": ["Und", "fand", "im", "Was\u00b7ser", "sei\u00b7nen", "Todt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Nachdem er Weib und Kind den G\u00f6ttern anbefohlen,", "tokens": ["Nach\u00b7dem", "er", "Weib", "und", "Kind", "den", "G\u00f6t\u00b7tern", "an\u00b7be\u00b7foh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Als ihm die schnelle Fluth empfieng;", "tokens": ["Als", "ihm", "die", "schnel\u00b7le", "Fluth", "emp\u00b7fi\u00b7eng", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.46": {"text": "Die Fischer kamen ihm, ach! allzusp\u00e4t zu H\u00fclfe,", "tokens": ["Die", "Fi\u00b7scher", "ka\u00b7men", "ihm", ",", "ach", "!", "all\u00b7zu\u00b7sp\u00e4t", "zu", "H\u00fcl\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ITJ", "$.", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Starr brachten sie ihn aus den Schilfe,", "tokens": ["Starr", "brach\u00b7ten", "sie", "ihn", "aus", "den", "Schil\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.48": {"text": "Der armen Gattinn, und seit diesem Tage gieng", "tokens": ["Der", "ar\u00b7men", "Gat\u00b7tinn", ",", "und", "seit", "die\u00b7sem", "Ta\u00b7ge", "gieng"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Dies bange Weib mit jeder Abendsonne", "tokens": ["Dies", "ban\u00b7ge", "Weib", "mit", "je\u00b7der", "A\u00b7bend\u00b7son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.50": {"text": "Zum Ufer, wo sein Grabmal war.", "tokens": ["Zum", "U\u00b7fer", ",", "wo", "sein", "Grab\u00b7mal", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Hier, sprach sie: hier liegt meine Wonne,", "tokens": ["Hier", ",", "sprach", "sie", ":", "hier", "liegt", "mei\u00b7ne", "Won\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Und gab ihr aufgel\u00f6stes Haar", "tokens": ["Und", "gab", "ihr", "auf\u00b7ge\u00b7l\u00f6s\u