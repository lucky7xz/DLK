{"dta.poem.12151": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "73.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im Sonnenschein mu\u00dft du mit dir den Mantel tragen,", "tokens": ["Im", "Son\u00b7nen\u00b7schein", "mu\u00dft", "du", "mit", "dir", "den", "Man\u00b7tel", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du ihn haben willst im Regen umzuschlagen.", "tokens": ["Wenn", "du", "ihn", "ha\u00b7ben", "willst", "im", "Re\u00b7gen", "um\u00b7zu\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAINF", "VMFIN", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Villeicht tr\u00e4gst du ihn mit als unn\u00fctz Hindernis;", "tokens": ["Vil\u00b7leicht", "tr\u00e4gst", "du", "ihn", "mit", "als", "un\u00b7n\u00fctz", "Hin\u00b7der\u00b7nis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "APPR", "KOUS", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch la\u00df ihn nur zu Haus, so fehlt er dir gewis.", "tokens": ["Doch", "la\u00df", "ihn", "nur", "zu", "Haus", ",", "so", "fehlt", "er", "dir", "ge\u00b7wis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "APPR", "NN", "$,", "ADV", "V