{"dta.poem.19357": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "90.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Streit von G\u00f6ttlichkeit und Menschheit ist geschlichtet,", "tokens": ["Der", "Streit", "von", "G\u00f6tt\u00b7lich\u00b7keit", "und", "Menschheit", "ist", "ge\u00b7schlich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Denn nur vom Gleichen kann das Gleiche seyn gerichtet.", "tokens": ["Denn", "nur", "vom", "Glei\u00b7chen", "kann", "das", "Glei\u00b7che", "seyn", "ge\u00b7rich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VMFIN", "ART", "NN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn nur vom Gleichen kann das Gleiche seyn erkannt;", "tokens": ["Denn", "nur", "vom", "Glei\u00b7chen", "kann", "das", "Glei\u00b7che", "seyn", "er\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VMFIN", "ART", "NN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch die Ausgleichung ist verschieden zubenannt.", "tokens": ["Doch", "die", "Aus\u00b7glei\u00b7chung", "ist", "ver\u00b7schie\u00b7den", "zu\u00b7be\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Hochmuth sagt: Zu Gott hat sich der Mensch erhoben;", "tokens": ["Der", "Hoch\u00b7muth", "sagt", ":", "Zu", "Gott", "hat", "sich", "der", "Mensch", "er\u00b7ho\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "APPR", "NN", "VAFIN", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Demuth: Niederstieg zum Menschen Gott von oben.", "tokens": ["Die", "De\u00b7muth", ":", "Nie\u00b7ders\u00b7tieg", "zum", "Men\u00b7schen", "Gott", "von", "o\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "APPRART", "NN", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}