{"dta.poem.624": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Peter Serins.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich die reine Treu/ so wie den Muth geerbet/", "tokens": ["Wenn", "ich", "die", "rei\u00b7ne", "Treu", "/", "so", "wie", "den", "Muth", "ge\u00b7er\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$(", "ADV", "KOKOM", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nicht der Kronen Gold gel\u00fcstend angesehn/", "tokens": ["Und", "nicht", "der", "Kro\u00b7nen", "Gold", "ge\u00b7l\u00fcs\u00b7tend", "an\u00b7ge\u00b7sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "NN", "VVPP", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So h\u00e4tte nicht mein Blut des Henckers Schwerdt gef\u00e4rbet/", "tokens": ["So", "h\u00e4t\u00b7te", "nicht", "mein", "Blut", "des", "Hen\u00b7ckers", "Schwerdt", "ge\u00b7f\u00e4r\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "PPOSAT", "NN", "ART", "NE", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein nahme w\u00fcrde noch im Buch des Lebens stehn.", "tokens": ["Mein", "nah\u00b7me", "w\u00fcr\u00b7de", "noch", "im", "Buch", "des", "Le\u00b7bens", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word"