{"dta.poem.6828": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie nun in dem Gehirn der Geister Quell allein", "tokens": ["Wie", "nun", "in", "dem", "Ge\u00b7hirn", "der", "Geis\u00b7ter", "Quell", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ART", "NN", "ART", "NN", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus welcher sie von da durch alle Nerven rinnen;", "tokens": ["Aus", "wel\u00b7cher", "sie", "von", "da", "durch", "al\u00b7le", "Ner\u00b7ven", "rin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPR", "ADV", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So mu\u00df ja dies die st\u00e4rckste Probe seyn,", "tokens": ["So", "mu\u00df", "ja", "dies", "die", "st\u00e4rcks\u00b7te", "Pro\u00b7be", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PDS", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df eintzig das Gehirn der Sitz der Sinnen.", "tokens": ["Da\u00df", "eint\u00b7zig", "das", "Ge\u00b7hirn", "der", "Sitz", "der", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hieraus entstehn bey uns Empfindlichkeiten.", "tokens": ["Hier\u00b7aus", "ent\u00b7stehn", "bey", "uns", "Emp\u00b7find\u00b7lich\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Nerven, die sich \u00fcberall verbreiten,", "tokens": ["Die", "Ner\u00b7ven", ",", "die", "sich", "\u00fc\u00b7be\u00b7rall", "ver\u00b7brei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Verf\u00fcgen sich zur Zunge, zum Geh\u00f6re,", "tokens": ["Ver\u00b7f\u00fc\u00b7gen", "sich", "zur", "Zun\u00b7ge", ",", "zum", "Ge\u00b7h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zur Nase, zum Gesicht, und eine kleine R\u00f6hre,", "tokens": ["Zur", "Na\u00b7se", ",", "zum", "Ge\u00b7sicht", ",", "und", "ei\u00b7ne", "klei\u00b7ne", "R\u00f6h\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein zarter Zweig durchdringet alle Seiten:", "tokens": ["Ein", "zar\u00b7ter", "Zweig", "durch\u00b7drin\u00b7get", "al\u00b7le", "Sei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Durch diese wird, wenn uns ein Vorwurff r\u00fchrt,", "tokens": ["Durch", "die\u00b7se", "wird", ",", "wenn", "uns", "ein", "Vor\u00b7wurff", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VAFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der Druck bis ins Gehirn gef\u00fchrt.", "tokens": ["Der", "Druck", "bis", "ins", "Ge\u00b7hirn", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}