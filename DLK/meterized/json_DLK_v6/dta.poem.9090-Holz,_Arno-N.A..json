{"dta.poem.9090": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwischen Bergen im Sonnenschein", "tokens": ["Zwi\u00b7schen", "Ber\u00b7gen", "im", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "liegt am Fluss das St\u00e4dtchen.", "tokens": ["liegt", "am", "Fluss", "das", "St\u00e4dt\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Hier oben von meinem Meilenstein seh ich \u00fcber alle D\u00e4cher.", "tokens": ["Hier", "o\u00b7ben", "von", "mei\u00b7nem", "Mei\u00b7len\u00b7stein", "seh", "ich", "\u00fc\u00b7ber", "al\u00b7le", "D\u00e4\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Kerzengrade steigt der Rauch.", "tokens": ["Ker\u00b7zen\u00b7gra\u00b7de", "steigt", "der", "Rauch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Durch einen bl\u00fchenden Hollunderbusch", "tokens": ["Durch", "ei\u00b7nen", "bl\u00fc\u00b7hen\u00b7den", "Hol\u00b7lun\u00b7der\u00b7busch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "unterscheide ich deutlich,", "tokens": ["un\u00b7ter\u00b7schei\u00b7de", "ich", "deut\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "unter der alten Gr\u00fcnspankuppel,", "tokens": ["un\u00b7ter", "der", "al\u00b7ten", "Gr\u00fcn\u00b7span\u00b7kup\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "die Thurmuhr.", "tokens": ["die", "Thur\u00b7muhr", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Ein himmelblaues Zifferblatt mit weissen Zahlen.", "tokens": ["Ein", "him\u00b7mel\u00b7blau\u00b7es", "Zif\u00b7fer\u00b7blatt", "mit", "weis\u00b7sen", "Zah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Noch drei kleine Striche,", "tokens": ["Noch", "drei", "klei\u00b7ne", "Stri\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "und die gesammte B\u00fcrgerschaft", "tokens": ["und", "die", "ge\u00b7samm\u00b7te", "B\u00fcr\u00b7ger\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "setzt sich p\u00fcnktlich zu Mittag.", "tokens": ["setzt", "sich", "p\u00fcnkt\u00b7lich", "zu", "Mit\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.7": {"line.1": {"text": "Zw\u00f6lf!", "tokens": ["Zw\u00f6lf", "!"], "token_info": ["word", "punct"], "pos": ["CARD", "$."], "meter": "+", "measure": "single.up"}}, "stanza.8": {"line.1": {"text": "Es ist heute Sonnabend, es giebt also \u00fcberall Eierkuchen.", "tokens": ["Es", "ist", "heu\u00b7te", "Sonn\u00b7a\u00b7bend", ",", "es", "giebt", "al\u00b7so", "\u00fc\u00b7be\u00b7rall", "Ei\u00b7er\u00b7ku\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,", "PPER", "VVFIN", "ADV", "ADV", "NN", "$."], "meter": "-+--+---+--+--+-+-", "measure": "iambic.hexa.relaxed"}}}}}