{"dta.poem.6423": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geschmack.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dasjenige, was uns in frischer Jugend schmecket,", "tokens": ["Das\u00b7je\u00b7ni\u00b7ge", ",", "was", "uns", "in", "fri\u00b7scher", "Ju\u00b7gend", "schme\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das schmecket uns nicht mehr, so bald", "tokens": ["Das", "schme\u00b7cket", "uns", "nicht", "mehr", ",", "so", "bald"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "ADV", "$,", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man alt geworden ist und kalt.", "tokens": ["Man", "alt", "ge\u00b7wor\u00b7den", "ist", "und", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "VAPP", "VAFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Allein, ohn da\u00df zu weit das Alter sich erstrecket;", "tokens": ["Al\u00b7lein", ",", "ohn", "da\u00df", "zu", "weit", "das", "Al\u00b7ter", "sich", "er\u00b7stre\u00b7cket", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUI", "KOUS", "PTKA", "ADJD", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So zeigt ja die Erfahrung an,", "tokens": ["So", "zeigt", "ja", "die", "Er\u00b7fah\u00b7rung", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df offtermals bey uns, und zwar in wenig Tagen,", "tokens": ["Da\u00df", "off\u00b7ter\u00b7mals", "bey", "uns", ",", "und", "zwar", "in", "we\u00b7nig", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPER", "$,", "KON", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Solch eine Aenderung sich zugetragen;", "tokens": ["Solch", "ei\u00b7ne", "A\u00b7en\u00b7de\u00b7rung", "sich", "zu\u00b7ge\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Wenn unser C\u00f6rper sich nicht wol befindet,", "tokens": ["Wenn", "un\u00b7ser", "C\u00f6r\u00b7per", "sich", "nicht", "wol", "be\u00b7fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Durch Galle aufgebracht, durchs Fiebers Hitz\u2019 entz\u00fcndet,", "tokens": ["Durch", "Gal\u00b7le", "auf\u00b7ge\u00b7bracht", ",", "durchs", "Fie\u00b7bers", "Hitz'", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPRART", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und D\u00fcnste sich sodann", "tokens": ["Und", "D\u00fcns\u00b7te", "sich", "so\u00b7dann"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "PRF", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Aus unserm krancken Magen heben,", "tokens": ["Aus", "un\u00b7serm", "kran\u00b7cken", "Ma\u00b7gen", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die an der Zunge Z\u00e4sern kleben.", "tokens": ["Die", "an", "der", "Zun\u00b7ge", "Z\u00e4\u00b7sern", "kle\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Daher denn, was man isst, uns nichts als Eckel schafft,", "tokens": ["Da\u00b7her", "denn", ",", "was", "man", "isst", ",", "uns", "nichts", "als", "E\u00b7ckel", "schafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "$,", "PRELS", "PIS", "VVFIN", "$,", "PPER", "PIS", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Indem es bitter, saur, verdr\u00fc\u00dflich, unschmackhafft.", "tokens": ["In\u00b7dem", "es", "bit\u00b7ter", ",", "saur", ",", "ver\u00b7dr\u00fc\u00df\u00b7lich", ",", "un\u00b7schmack\u00b7hafft", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und solche durch die Plag\u2019 erzeugte Widrigkeit", "tokens": ["Und", "sol\u00b7che", "durch", "die", "Plag'", "er\u00b7zeug\u00b7te", "Wid\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "W\u00e4hrt, wenn man gleich gesund, noch \u00f6ffters lange Zeit.", "tokens": ["W\u00e4hrt", ",", "wenn", "man", "gleich", "ge\u00b7sund", ",", "noch", "\u00f6ff\u00b7ters", "lan\u00b7ge", "Zeit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PIS", "ADV", "ADJD", "$,", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}