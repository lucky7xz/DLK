{"textgrid.poem.35592": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Anif", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erinnerung: M\u00f6ven, gleitend \u00fcber den dunklen Himmel", "tokens": ["E\u00b7rin\u00b7ne\u00b7rung", ":", "M\u00f6\u00b7ven", ",", "glei\u00b7tend", "\u00fc\u00b7ber", "den", "dunk\u00b7len", "Him\u00b7mel"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "$,", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "M\u00e4nnlicher Schwermut.", "tokens": ["M\u00e4nn\u00b7li\u00b7cher", "Schwer\u00b7mut", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Stille wohnst du im Schatten der herbstlichen Esche,", "tokens": ["Stil\u00b7le", "wohnst", "du", "im", "Schat\u00b7ten", "der", "herbst\u00b7li\u00b7chen", "E\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Versunken in des H\u00fcgels gerechtes Ma\u00df;", "tokens": ["Ver\u00b7sun\u00b7ken", "in", "des", "H\u00fc\u00b7gels", "ge\u00b7rech\u00b7tes", "Ma\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Immer gehst du den gr\u00fcnen Flu\u00df hinab,", "tokens": ["Im\u00b7mer", "gehst", "du", "den", "gr\u00fc\u00b7nen", "Flu\u00df", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wenn es Abend geworden,", "tokens": ["Wenn", "es", "A\u00b7bend", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "T\u00f6nende Liebe; friedlich begegnet das dunkle Wild,", "tokens": ["T\u00f6\u00b7nen\u00b7de", "Lie\u00b7be", ";", "fried\u00b7lich", "be\u00b7geg\u00b7net", "das", "dunk\u00b7le", "Wild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+-+", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Ein rosiger Mensch. Trunken von bl\u00e4ulicher Witterung", "tokens": ["Ein", "ro\u00b7si\u00b7ger", "Mensch", ".", "Trun\u00b7ken", "von", "bl\u00e4u\u00b7li\u00b7cher", "Wit\u00b7te\u00b7rung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "R\u00fchrt die Stirne das sterbende Laub", "tokens": ["R\u00fchrt", "die", "Stir\u00b7ne", "das", "ster\u00b7ben\u00b7de", "Laub"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und denkt das ernste Antlitz der Mutter;", "tokens": ["Und", "denkt", "das", "erns\u00b7te", "Ant\u00b7litz", "der", "Mut\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "O, wie alles ins Dunkel hinsinkt;", "tokens": ["O", ",", "wie", "al\u00b7les", "ins", "Dun\u00b7kel", "hin\u00b7sinkt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Die gestrengen Zimmer und das alte Ger\u00e4t", "tokens": ["Die", "ge\u00b7stren\u00b7gen", "Zim\u00b7mer", "und", "das", "al\u00b7te", "Ge\u00b7r\u00e4t"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Der V\u00e4ter.", "tokens": ["Der", "V\u00e4\u00b7ter", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Dieses ersch\u00fcttert die Brust des Fremdlings.", "tokens": ["Die\u00b7ses", "er\u00b7sch\u00fct\u00b7tert", "die", "Brust", "des", "Fremd\u00b7lings", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "O, ihr Zeichen und Sterne.", "tokens": ["O", ",", "ihr", "Zei\u00b7chen", "und", "Ster\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "Gro\u00df ist die Schuld des Geborenen. Weh, ihr goldenen Schauer", "tokens": ["Gro\u00df", "ist", "die", "Schuld", "des", "Ge\u00b7bo\u00b7re\u00b7nen", ".", "Weh", ",", "ihr", "gol\u00b7de\u00b7nen", "Schau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ART", "NN", "$.", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Des Todes,", "tokens": ["Des", "To\u00b7des", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Da die Seele k\u00fchlere Bl\u00fcten tr\u00e4umt.", "tokens": ["Da", "die", "See\u00b7le", "k\u00fch\u00b7le\u00b7re", "Bl\u00fc\u00b7ten", "tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Immer schreit im kahlen Gezweig der n\u00e4chtliche Vogel", "tokens": ["Im\u00b7mer", "schreit", "im", "kah\u00b7len", "Ge\u00b7zweig", "der", "n\u00e4cht\u00b7li\u00b7che", "Vo\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "\u00dcber des Mondenen Schritt,", "tokens": ["\u00dc\u00b7ber", "des", "Mon\u00b7de\u00b7nen", "Schritt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "T\u00f6nt ein eisiger Wind an den Mauern des Dorfs.", "tokens": ["T\u00f6nt", "ein", "ei\u00b7si\u00b7ger", "Wind", "an", "den", "Mau\u00b7ern", "des", "Dorfs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}}}}