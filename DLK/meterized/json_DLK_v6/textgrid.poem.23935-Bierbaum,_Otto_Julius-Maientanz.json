{"textgrid.poem.23935": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Maientanz", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bl\u00fctenbl\u00e4tter jagt der Wind", "tokens": ["Bl\u00fc\u00b7ten\u00b7bl\u00e4t\u00b7ter", "jagt", "der", "Wind"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Von den jungen Zweigen,", "tokens": ["Von", "den", "jun\u00b7gen", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die sich nun im ersten Sturm,", "tokens": ["Die", "sich", "nun", "im", "ers\u00b7ten", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fr\u00fchlingssturme neigen.", "tokens": ["Fr\u00fch\u00b7lings\u00b7stur\u00b7me", "nei\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Rosarote Apfelbl\u00fch", "tokens": ["Ro\u00b7sa\u00b7ro\u00b7te", "Ap\u00b7fel\u00b7bl\u00fch"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tanzt mit schneeig wei\u00dfen", "tokens": ["Tanzt", "mit", "schne\u00b7e\u00b7ig", "wei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJD", "VVINF"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Kirschenbl\u00fcten Ringelreih", "tokens": ["Kir\u00b7schen\u00b7bl\u00fc\u00b7ten", "Rin\u00b7gel\u00b7reih"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hell in Wirbelkreisen.", "tokens": ["Hell", "in", "Wir\u00b7bel\u00b7krei\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Junge Birken beugen sich", "tokens": ["Jun\u00b7ge", "Bir\u00b7ken", "beu\u00b7gen", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jungferngr\u00fcn im Winde,", "tokens": ["Jung\u00b7fern\u00b7gr\u00fcn", "im", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Leise wisperts, froh erstaunt,", "tokens": ["Lei\u00b7se", "wis\u00b7perts", ",", "froh", "er\u00b7staunt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der alten Linde.", "tokens": ["In", "der", "al\u00b7ten", "Lin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Heia, erster Fr\u00fchlingssturm,", "tokens": ["Hei\u00b7a", ",", "ers\u00b7ter", "Fr\u00fch\u00b7lings\u00b7sturm", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fctenbl\u00e4tterfeger,", "tokens": ["Bl\u00fc\u00b7ten\u00b7bl\u00e4t\u00b7ter\u00b7fe\u00b7ger", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sei gegr\u00fc\u00dft, Lenzjunker Wind,", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00dft", ",", "Lenz\u00b7jun\u00b7ker", "Wind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Allerliebster J\u00e4ger!", "tokens": ["Al\u00b7ler\u00b7liebs\u00b7ter", "J\u00e4\u00b7ger", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Nicht zum Morde ruft dein Horn,", "tokens": ["Nicht", "zum", "Mor\u00b7de", "ruft", "dein", "Horn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruft zu Tanz und Leben,", "tokens": ["Ruft", "zu", "Tanz", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ueber deinem Hussah-Zug", "tokens": ["Ue\u00b7ber", "dei\u00b7nem", "Hus\u00b7sah\u00b7Zug"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schmetterlinge schweben.", "tokens": ["Schmet\u00b7ter\u00b7lin\u00b7ge", "schwe\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Letztes Winterwehtum treibt", "tokens": ["Letz\u00b7tes", "Win\u00b7ter\u00b7weh\u00b7tum", "treibt"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein Hallih von hinnen,", "tokens": ["Dein", "Hal\u00b7lih", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "H\u00fcte hoch und juhuhu!", "tokens": ["H\u00fc\u00b7te", "hoch", "und", "ju\u00b7hu\u00b7hu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Maitanz soll beginnen!", "tokens": ["Mai\u00b7tanz", "soll", "be\u00b7gin\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Wie der Bl\u00fctenbl\u00e4tterschnee", "tokens": ["Wie", "der", "Bl\u00fc\u00b7ten\u00b7bl\u00e4t\u00b7ter\u00b7schnee"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wolln wir Wirbel drehen,", "tokens": ["Wolln", "wir", "Wir\u00b7bel", "dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie's der alte Maienbaum", "tokens": ["Wie's", "der", "al\u00b7te", "Mai\u00b7en\u00b7baum"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimmer noch gesehen.", "tokens": ["Nim\u00b7mer", "noch", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Fl\u00f6te kichert, Geige singt,", "tokens": ["Fl\u00f6\u00b7te", "ki\u00b7chert", ",", "Gei\u00b7ge", "singt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Ba\u00df brummt bieder,", "tokens": ["Und", "der", "Ba\u00df", "brummt", "bie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch der Lenzwind \u00fcber uns", "tokens": ["Doch", "der", "Lenz\u00b7wind", "\u00fc\u00b7ber", "uns"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat die sch\u00f6nsten Lieder.", "tokens": ["Hat", "die", "sch\u00f6ns\u00b7ten", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Hat die gro\u00dfe Melodei,", "tokens": ["Hat", "die", "gro\u00b7\u00dfe", "Me\u00b7lo\u00b7dei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Helle Sturmlustweise;", "tokens": ["Hel\u00b7le", "Sturm\u00b7lust\u00b7wei\u00b7se", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nach des Lenzen Pfeife tanzt,", "tokens": ["Nach", "des", "Len\u00b7zen", "Pfei\u00b7fe", "tanzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tanzt die frohen Kreise!", "tokens": ["Tanzt", "die", "fro\u00b7hen", "Krei\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}