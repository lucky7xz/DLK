{"textgrid.poem.42557": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Rechtfertigung", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl geht der Jugend Sehnen", "tokens": ["Wohl", "geht", "der", "Ju\u00b7gend", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach manchem sch\u00f6nen Traum;", "tokens": ["Nach", "man\u00b7chem", "sch\u00f6\u00b7nen", "Traum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Ungest\u00fcm und Tr\u00e4nen", "tokens": ["Mit", "Un\u00b7ge\u00b7st\u00fcm", "und", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "St\u00fcrmt sie den Sternenraum.", "tokens": ["St\u00fcrmt", "sie", "den", "Ster\u00b7nen\u00b7raum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Der Himmel h\u00f6rt ihr Flehen", "tokens": ["Der", "Him\u00b7mel", "h\u00f6rt", "ihr", "Fle\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und l\u00e4chelt gn\u00e4dig: nein!", "tokens": ["Und", "l\u00e4\u00b7chelt", "gn\u00e4\u00b7dig", ":", "nein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "PTKANT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und l\u00e4\u00dft vor\u00fcbergehen", "tokens": ["Und", "l\u00e4\u00dft", "vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den Wunsch zusamt der Pein.", "tokens": ["Den", "Wunsch", "zu\u00b7samt", "der", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn aber nun vom Scheine", "tokens": ["Wenn", "a\u00b7ber", "nun", "vom", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Herz sich abgekehrt", "tokens": ["Das", "Herz", "sich", "ab\u00b7ge\u00b7kehrt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nur das Echte, Reine,", "tokens": ["Und", "nur", "das", "Ech\u00b7te", ",", "Rei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Menschliche begehrt", "tokens": ["Das", "Menschli\u00b7che", "be\u00b7gehrt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und doch mit allem Streben", "tokens": ["Und", "doch", "mit", "al\u00b7lem", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PIS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kein Ziel erreichen kann:", "tokens": ["Kein", "Ziel", "er\u00b7rei\u00b7chen", "kann", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da mu\u00df man wohl vergeben", "tokens": ["Da", "mu\u00df", "man", "wohl", "ver\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Trauer auch dem Mann.", "tokens": ["Die", "Trau\u00b7er", "auch", "dem", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}