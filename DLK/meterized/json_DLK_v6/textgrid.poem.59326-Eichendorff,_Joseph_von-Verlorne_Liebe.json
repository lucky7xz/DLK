{"textgrid.poem.59326": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Verlorne Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieder schweigen jetzt und Klagen,", "tokens": ["Lie\u00b7der", "schwei\u00b7gen", "jetzt", "und", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun will ich erst fr\u00f6hlich sein,", "tokens": ["Nun", "will", "ich", "erst", "fr\u00f6h\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "All mein Leid will ich zerschlagen", "tokens": ["All", "mein", "Leid", "will", "ich", "zer\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und Erinnern \u2013 gebt mir Wein!", "tokens": ["Und", "E\u00b7rin\u00b7nern", "\u2013", "gebt", "mir", "Wein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "VVFIN", "PPER", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Wie er mir verlockend spiegelt", "tokens": ["Wie", "er", "mir", "ver\u00b7lo\u00b7ckend", "spie\u00b7gelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "VVPP", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sterne und der Erde Lust,", "tokens": ["Ster\u00b7ne", "und", "der", "Er\u00b7de", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Stillgesch\u00e4ftig dann entriegelt", "tokens": ["Still\u00b7ge\u00b7sch\u00e4f\u00b7tig", "dann", "ent\u00b7rie\u00b7gelt"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "All die Teufel in der Brust,", "tokens": ["All", "die", "Teu\u00b7fel", "in", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Erst der Knecht und dann der Meister,", "tokens": ["Erst", "der", "Knecht", "und", "dann", "der", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Bricht er durch die Nacht herein,", "tokens": ["Bricht", "er", "durch", "die", "Nacht", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Wildester der L\u00fcgengeister,", "tokens": ["Wil\u00b7des\u00b7ter", "der", "L\u00fc\u00b7gen\u00b7geis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.12": {"text": "Ring mit mir, ich lache dein!", "tokens": ["Ring", "mit", "mir", ",", "ich", "la\u00b7che", "dein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "PPER", "VVFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Und den Becher voll Entsetzen", "tokens": ["Und", "den", "Be\u00b7cher", "voll", "Ent\u00b7set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Werf ich in des Stromes Grund,", "tokens": ["Werf", "ich", "in", "des", "Stro\u00b7mes", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Da\u00df sich nimmer dran soll letzen", "tokens": ["Da\u00df", "sich", "nim\u00b7mer", "dran", "soll", "let\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ADV", "PAV", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Wer noch fr\u00f6hlich und gesund!", "tokens": ["Wer", "noch", "fr\u00f6h\u00b7lich", "und", "ge\u00b7sund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lauten h\u00f6r ich ferne klingen,", "tokens": ["Lau\u00b7ten", "h\u00f6r", "ich", "fer\u00b7ne", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lust'ge Bursche ziehn vom Schmaus,", "tokens": ["Lust'\u00b7ge", "Bur\u00b7sche", "ziehn", "vom", "Schmaus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00e4ndchen sie den Liebsten bringen,", "tokens": ["St\u00e4nd\u00b7chen", "sie", "den", "Liebs\u00b7ten", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das lockt mich mit hinaus.", "tokens": ["Und", "das", "lockt", "mich", "mit", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PRF", "APPR", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00e4dchen hinterm bl\u00fchnden Baume", "tokens": ["M\u00e4d\u00b7chen", "hin\u00b7term", "bl\u00fchn\u00b7den", "Bau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Winkt und macht das Fenster auf", "tokens": ["Winkt", "und", "macht", "das", "Fens\u00b7ter", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "ART", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ich steige wie im Traume", "tokens": ["Und", "ich", "stei\u00b7ge", "wie", "im", "Trau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Durch das kleine Haus hinauf.", "tokens": ["Durch", "das", "klei\u00b7ne", "Haus", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Sch\u00fcttle nur die dunklen Locken", "tokens": ["Sch\u00fctt\u00b7le", "nur", "die", "dunk\u00b7len", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Aus dem sch\u00f6nen Angesicht!", "tokens": ["Aus", "dem", "sch\u00f6\u00b7nen", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sieh, ich stehe ganz erschrocken:", "tokens": ["Sieh", ",", "ich", "ste\u00b7he", "ganz", "er\u00b7schro\u00b7cken", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das sind ihre Augen licht,", "tokens": ["Das", "sind", "ih\u00b7re", "Au\u00b7gen", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Locken hatte sie wie deine,", "tokens": ["Lo\u00b7cken", "hat\u00b7te", "sie", "wie", "dei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "KOKOM", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleiche Wangen, Lippen rot \u2013", "tokens": ["Blei\u00b7che", "Wan\u00b7gen", ",", "Lip\u00b7pen", "rot", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, du bist ja doch nicht meine,", "tokens": ["Ach", ",", "du", "bist", "ja", "doch", "nicht", "mei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADV", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mein Lieb ist lange tot!", "tokens": ["Und", "mein", "Lieb", "ist", "lan\u00b7ge", "tot", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "H\u00e4ttest du nur nicht gesprochen", "tokens": ["H\u00e4t\u00b7test", "du", "nur", "nicht", "ge\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und so frech geblickt nach mir,", "tokens": ["Und", "so", "frech", "ge\u00b7blickt", "nach", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVPP", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Das hat ganz den Traum zerbrochen", "tokens": ["Das", "hat", "ganz", "den", "Traum", "zer\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "VVPP"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.10": {"text": "Und nun grauet mir vor dir.", "tokens": ["Und", "nun", "grau\u00b7et", "mir", "vor", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Da nimm Geld, kauf Putz und Flimmern,", "tokens": ["Da", "nimm", "Geld", ",", "kauf", "Putz", "und", "Flim\u00b7mern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "NN", "$,", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Fort und lache nicht so wild!", "tokens": ["Fort", "und", "la\u00b7che", "nicht", "so", "wild", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "O ich m\u00f6chte dich zertr\u00fcmmern,", "tokens": ["O", "ich", "m\u00f6ch\u00b7te", "dich", "zer\u00b7tr\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Sch\u00f6nes, l\u00fcgenhaftes Bild!", "tokens": ["Sch\u00f6\u00b7nes", ",", "l\u00fc\u00b7gen\u00b7haf\u00b7tes", "Bild", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sp\u00e4t von dem verlornen Kinde", "tokens": ["Sp\u00e4t", "von", "dem", "ver\u00b7lor\u00b7nen", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kam ich durch die Nacht daher,", "tokens": ["Kam", "ich", "durch", "die", "Nacht", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fahnen drehten sich im Winde,", "tokens": ["Fah\u00b7nen", "dreh\u00b7ten", "sich", "im", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle Gassen waren leer.", "tokens": ["Al\u00b7le", "Gas\u00b7sen", "wa\u00b7ren", "leer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Oben lag noch meine Laute", "tokens": ["O\u00b7ben", "lag", "noch", "mei\u00b7ne", "Lau\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mein Fenster stand noch auf,", "tokens": ["Und", "mein", "Fens\u00b7ter", "stand", "noch", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aus dem stillen Grunde graute", "tokens": ["Aus", "dem", "stil\u00b7len", "Grun\u00b7de", "grau\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wunderbar die Stadt herauf.", "tokens": ["Wun\u00b7der\u00b7bar", "die", "Stadt", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Drau\u00dfen aber blitzt's vom Weiten,", "tokens": ["Drau\u00b7\u00dfen", "a\u00b7ber", "blitzt's", "vom", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Alter Zeiten ich gedacht,", "tokens": ["Al\u00b7ter", "Zei\u00b7ten", "ich", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Schauernd rei\u00df ich in den Saiten", "tokens": ["Schau\u00b7ernd", "rei\u00df", "ich", "in", "den", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und ich sing die halbe Nacht.", "tokens": ["Und", "ich", "sing", "die", "hal\u00b7be", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Die verschlafnen Nachbarn sprechen,", "tokens": ["Die", "ver\u00b7schlaf\u00b7nen", "Nach\u00b7barn", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Da\u00df ich n\u00e4chtlich trunken sei \u2013", "tokens": ["Da\u00df", "ich", "n\u00e4cht\u00b7lich", "trun\u00b7ken", "sei", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "O du mein Gott! und mir brechen", "tokens": ["O", "du", "mein", "Gott", "!", "und", "mir", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "PPOSAT", "NN", "$.", "KON", "PPER", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.16": {"text": "Herz und Saitenspiel entzwei!", "tokens": ["Herz", "und", "Sai\u00b7ten\u00b7spiel", "ent\u00b7zwei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}