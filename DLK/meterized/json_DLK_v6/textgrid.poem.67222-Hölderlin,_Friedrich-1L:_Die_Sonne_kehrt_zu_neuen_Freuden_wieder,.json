{"textgrid.poem.67222": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Sonne kehrt zu neuen Freuden wieder,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne kehrt zu neuen Freuden wieder,", "tokens": ["Die", "Son\u00b7ne", "kehrt", "zu", "neu\u00b7en", "Freu\u00b7den", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Tag erscheint mit Strahlen, wie die Bl\u00fcte,", "tokens": ["Der", "Tag", "er\u00b7scheint", "mit", "Strah\u00b7len", ",", "wie", "die", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Zierde der Natur erscheint sich dem Gem\u00fcte,", "tokens": ["Die", "Zier\u00b7de", "der", "Na\u00b7tur", "er\u00b7scheint", "sich", "dem", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als wie entstanden sind Gesang und Lieder.", "tokens": ["Als", "wie", "ent\u00b7stan\u00b7den", "sind", "Ge\u00b7sang", "und", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "VVPP", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die neue Welt ist aus der Tale Grunde,", "tokens": ["Die", "neu\u00b7e", "Welt", "ist", "aus", "der", "Ta\u00b7le", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und heiter ist des Fr\u00fchlings Morgenstunde,", "tokens": ["Und", "hei\u00b7ter", "ist", "des", "Fr\u00fch\u00b7lings", "Mor\u00b7gen\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus H\u00f6hen gl\u00e4nzt der Tag, des Abends Leben", "tokens": ["Aus", "H\u00f6\u00b7hen", "gl\u00e4nzt", "der", "Tag", ",", "des", "A\u00b7bends", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist der Betrachtung auch des innern Sinns gegeben.", "tokens": ["Ist", "der", "Be\u00b7trach\u00b7tung", "auch", "des", "in\u00b7nern", "Sinns", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}