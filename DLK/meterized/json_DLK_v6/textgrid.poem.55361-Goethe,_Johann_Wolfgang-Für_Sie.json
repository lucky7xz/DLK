{"textgrid.poem.55361": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcr Sie", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbin deinem Liede walten", "tokens": ["\u00bb", "in", "dei\u00b7nem", "Lie\u00b7de", "wal\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gar manche sch\u00f6ne Namen!\u00ab", "tokens": ["Gar", "man\u00b7che", "sch\u00f6\u00b7ne", "Na\u00b7men", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sind mancherlei Gestalten,", "tokens": ["Sind", "man\u00b7cher\u00b7lei", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch nur ", "tokens": ["Doch", "nur"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "\u00bbnun aber die Sch\u00f6ne,", "tokens": ["\u00bb", "nun", "a\u00b7ber", "die", "Sch\u00f6\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die dich am Herzen hegte?\u00ab", "tokens": ["Die", "dich", "am", "Her\u00b7zen", "heg\u00b7te", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jede kennt die T\u00f6ne,", "tokens": ["Je\u00b7de", "kennt", "die", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Die sie erregte.", "tokens": ["Die", "sie", "er\u00b7reg\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}