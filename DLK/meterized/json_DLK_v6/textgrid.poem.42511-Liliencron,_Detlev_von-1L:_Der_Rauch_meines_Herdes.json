{"textgrid.poem.42511": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Rauch meines Herdes", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Rauch meines Herdes", "tokens": ["Der", "Rauch", "mei\u00b7nes", "Her\u00b7des"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Umzieht meine Linden,", "tokens": ["Um\u00b7zieht", "mei\u00b7ne", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die von Schwalben umzwitschert sind.", "tokens": ["Die", "von", "Schwal\u00b7ben", "um\u00b7zwit\u00b7schert", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVPP", "VAFIN", "$."], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.4": {"text": "Das ist das Gl\u00fcck.", "tokens": ["Das", "ist", "das", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "W\u00fcnschst du noch mehr?", "tokens": ["W\u00fcnschst", "du", "noch", "mehr", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "En gode Sigarr.", "tokens": ["En", "go\u00b7de", "Si\u00b7garr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}