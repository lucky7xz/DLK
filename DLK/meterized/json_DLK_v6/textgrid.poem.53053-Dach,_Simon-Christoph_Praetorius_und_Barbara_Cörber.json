{"textgrid.poem.53053": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Christoph Praetorius und Barbara C\u00f6rber", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df Ihr Euch vorgesetzt, von denen euch zu scheiden,", "tokens": ["Da\u00df", "Ihr", "Euch", "vor\u00b7ge\u00b7setzt", ",", "von", "de\u00b7nen", "euch", "zu", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "$,", "APPR", "PRELS", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr vnbekandter zwar, vnnd doch sehr hoher Freund,", "tokens": ["Ihr", "vn\u00b7be\u00b7kand\u00b7ter", "zwar", ",", "vnnd", "doch", "sehr", "ho\u00b7her", "Freund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,", "KON", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Marti Tag vnd Nacht zu Dienst ergeben seind,", "tokens": ["Die", "Mar\u00b7ti", "Tag", "vnd", "Nacht", "zu", "Dienst", "er\u00b7ge\u00b7ben", "seind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "KON", "NN", "APPR", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd jhren Leib vnd Sinn in Adamant verkleiden,", "tokens": ["Vnd", "jhren", "Leib", "vnd", "Sinn", "in", "A\u00b7da\u00b7mant", "ver\u00b7klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Hat euch (wie mich bed\u00fcnckt) der Liebe keusches leiden", "tokens": ["Hat", "euch", "(", "wie", "mich", "be\u00b7d\u00fcnckt", ")", "der", "Lie\u00b7be", "keu\u00b7sches", "lei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$(", "PWAV", "PPER", "VVPP", "$(", "ART", "NN", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gegeben an die Hand, so da\u00df Ihr Marti Feind", "tokens": ["Ge\u00b7ge\u00b7ben", "an", "die", "Hand", ",", "so", "da\u00df", "Ihr", "Mar\u00b7ti", "Feind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ADV", "KOUS", "PPER", "NE", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Euch seinem Sohn ergebt, der (wie Ihr nie vermeint)", "tokens": ["Euch", "sei\u00b7nem", "Sohn", "er\u00b7gebt", ",", "der", "(", "wie", "Ihr", "nie", "ver\u00b7meint", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "$,", "PRELS", "$(", "PWAV", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In meinem Vaterland' Euch mit den s\u00fcssen Frewden", "tokens": ["In", "mei\u00b7nem", "Va\u00b7ter\u00b7land'", "Euch", "mit", "den", "s\u00fcs\u00b7sen", "Frew\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Des Ehestands begabt, vnd (was die Liebe macht!)", "tokens": ["Des", "E\u00b7hes\u00b7tands", "be\u00b7gabt", ",", "vnd", "(", "was", "die", "Lie\u00b7be", "macht", "!", ")"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "KON", "$(", "PWS", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ist Vrsach, da\u00df Ihr da f\u00fcr Gottes Heerde wacht.", "tokens": ["Ist", "Vr\u00b7sach", ",", "da\u00df", "Ihr", "da", "f\u00fcr", "Got\u00b7tes", "Heer\u00b7de", "wacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "KOUS", "PPER", "ADV", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich solt' auff Ewre Bitt' jetzt mich bey Euch einstellen;", "tokens": ["Ich", "solt'", "auff", "Ew\u00b7re", "Bitt'", "jetzt", "mich", "bey", "Euch", "ein\u00b7stel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "ADV", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wann aber Ich nicht kan, al\u00df sol sich Phoebus Chor", "tokens": ["Wann", "a\u00b7ber", "Ich", "nicht", "kan", ",", "al\u00df", "sol", "sich", "Phoe\u00b7bus", "Chor"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "PTKNEG", "VMFIN", "$,", "KOUS", "VMFIN", "PRF", "NE", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sampt allen Gratien, der Amaltheen Flor", "tokens": ["Sampt", "al\u00b7len", "Gra\u00b7ti\u00b7en", ",", "der", "A\u00b7mal\u00b7theen", "Flor"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.14": {"text": "Zu Euch auff meine Bitt' an meine stat gesellen.", "tokens": ["Zu", "Euch", "auff", "mei\u00b7ne", "Bitt'", "an", "mei\u00b7ne", "stat", "ge\u00b7sel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}