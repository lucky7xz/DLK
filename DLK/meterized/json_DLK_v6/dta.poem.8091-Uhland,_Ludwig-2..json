{"dta.poem.8091": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Der J\u00fcngling steht auf dem Verdeck,", "tokens": ["Der", "J\u00fcng\u00b7ling", "steht", "auf", "dem", "Ver\u00b7deck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sieht seine Schiffe fahren,", "tokens": ["Sieht", "sei\u00b7ne", "Schif\u00b7fe", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Sonne stralt, es spielt die Luft", "tokens": ["Die", "Son\u00b7ne", "stralt", ",", "es", "spielt", "die", "Luft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit seinen goldnen Haaren.", "tokens": ["Mit", "sei\u00b7nen", "gold\u00b7nen", "Haa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das Ruder schallt, das Segel schwillt,", "tokens": ["Das", "Ru\u00b7der", "schallt", ",", "das", "Se\u00b7gel", "schwillt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die bunten Wimpel fliegen,", "tokens": ["Die", "bun\u00b7ten", "Wim\u00b7pel", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Meerfrauen mit Gesang und Spiel", "tokens": ["Meer\u00b7frau\u00b7en", "mit", "Ge\u00b7sang", "und", "Spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Sich um die Kiele wiegen.", "tokens": ["Sich", "um", "die", "Kie\u00b7le", "wie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er spricht: \u201eDas ist mein K\u00f6nigreich,", "tokens": ["Er", "spricht", ":", "\u201e", "Das", "ist", "mein", "K\u00f6\u00b7nig\u00b7reich", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das frei und lustig streifet,", "tokens": ["Das", "frei", "und", "lus\u00b7tig", "strei\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das um die tr\u00e4ge Erde her", "tokens": ["Das", "um", "die", "tr\u00e4\u00b7ge", "Er\u00b7de", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf blauen Fluten schweifet.\u201c", "tokens": ["Auf", "blau\u00b7en", "Flu\u00b7ten", "schwei\u00b7fet", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da ziehen finstre Wolken auf", "tokens": ["Da", "zie\u00b7hen", "finst\u00b7re", "Wol\u00b7ken", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "VVFIN", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Sturm und mit Gewitter.", "tokens": ["Mit", "Sturm", "und", "mit", "Ge\u00b7wit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Blitze zucken aus der Nacht,", "tokens": ["Die", "Blit\u00b7ze", "zu\u00b7cken", "aus", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Maste springen in Splitter.", "tokens": ["Die", "Mas\u00b7te", "sprin\u00b7gen", "in", "Split\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Und Wogen st\u00fcrzen auf das Schiff,", "tokens": ["Und", "Wo\u00b7gen", "st\u00fcr\u00b7zen", "auf", "das", "Schiff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wilde, Bergen gleiche;", "tokens": ["So", "wil\u00b7de", ",", "Ber\u00b7gen", "glei\u00b7che", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$,", "NN", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verschlungen ist der K\u00f6nigssohn", "tokens": ["Ver\u00b7schlun\u00b7gen", "ist", "der", "K\u00f6\u00b7nigs\u00b7sohn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sammt seinem lust\u2019gen Reiche.", "tokens": ["Sammt", "sei\u00b7nem", "lust'\u00b7gen", "Rei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}