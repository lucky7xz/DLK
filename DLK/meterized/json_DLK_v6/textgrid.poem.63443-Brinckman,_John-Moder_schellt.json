{"textgrid.poem.63443": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "Moder schellt", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dat wohrt so lang nu, as dat wohrt,", "tokens": ["Dat", "wohrt", "so", "lang", "nu", ",", "as", "dat", "wohrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADJD", "ADV", "$,", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ick, Vader, wasch min H\u00e4nn';", "tokens": ["ick", ",", "Va\u00b7der", ",", "wasch", "min", "H\u00e4nn'", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "dat hett nu so un so keen Ort", "tokens": ["dat", "hett", "nu", "so", "un", "so", "ke\u00b7en", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ADV", "ADV", "FM", "ADV", "VVFIN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "un nimmt noch'n kn\u00e4glich Enn.", "tokens": ["un", "nimmt", "noch'n", "kn\u00e4g\u00b7lich", "Enn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "VVFIN", "ADV", "ADJD", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Wi m\u00f6t dor abslut'n Insehn don \u2013", "tokens": ["Wi", "m\u00f6t", "dor", "ab\u00b7slut'n", "In\u00b7sehn", "don", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "do doch dat Mul up, segg!", "tokens": ["do", "doch", "dat", "Mul", "up", ",", "segg", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NE", "$,", "VVFIN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Dat geiht de Dirn an Kranz un Kron,", "tokens": ["Dat", "geiht", "de", "Dirn", "an", "Kranz", "un", "Kron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "NN", "APPR", "NN", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wenn wi't ehr nu nich legg'.", "tokens": ["wenn", "wi't", "ehr", "nu", "nich", "leg\u00b7g'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NN", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nich locken un nich hissen lett", "tokens": ["Nich", "lo\u00b7cken", "un", "nich", "his\u00b7sen", "lett"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVINF", "FM", "PTKNEG", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "sick mihr de olle dumm Str\u00fcn;", "tokens": ["sick", "mihr", "de", "ol\u00b7le", "dumm", "Str\u00fcn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "se hett ehrn Kopp up den Bengel sett't", "tokens": ["se", "hett", "ehrn", "Kopp", "up", "den", "Ben\u00b7gel", "sett't"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "un ward nahst nog noch schrin.", "tokens": ["un", "ward", "nahst", "nog", "noch", "schrin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "VAFIN", "ADV", "VVFIN", "ADV", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Sonn will Dirn is so swer to h\u00f6d'n", "tokens": ["Sonn", "will", "Dirn", "is", "so", "swer", "to", "h\u00f6d'n"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "NN", "FM", "FM", "FM", "FM", "FM"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "grad as sonn Sack vull Fl\u00e4uh,", "tokens": ["grad", "as", "sonn", "Sack", "vull", "Fl\u00e4uh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "KON", "NN", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "s\u00fch, Vader, denn so is't vun N\u00f6d'n,", "tokens": ["s\u00fch", ",", "Va\u00b7der", ",", "denn", "so", "is't", "vun", "N\u00f6d'n", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NE", "$,", "KON", "ADV", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wi pa\u00df' ehr up zaft twee.", "tokens": ["wi", "pa\u00df'", "ehr", "up", "zaft", "twee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-----", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Wi m\u00f6t ehr eens de Bicht verh\u00fcr'n,", "tokens": ["Wi", "m\u00f6t", "ehr", "e\u00b7ens", "de", "Bicht", "ver\u00b7h\u00fcr'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "den Kopp ehr wedder stell'n;", "tokens": ["den", "Kopp", "ehr", "wed\u00b7der", "stell'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "wat schall sonn oll unnoslig Dirn", "tokens": ["wat", "schall", "sonn", "oll", "un\u00b7nos\u00b7lig", "Dirn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "ADJD", "ADJD", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "mit sonn oll rugen Gesell'n!", "tokens": ["mit", "sonn", "oll", "ru\u00b7gen", "Ge\u00b7sell'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Dat geiht so lang nu, as dat geiht \u2013", "tokens": ["Dat", "geiht", "so", "lang", "nu", ",", "as", "dat", "geiht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADJD", "ADV", "$,", "NE", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "nee! segg'ck, un teihnmal nee!", "tokens": ["nee", "!", "seg\u00b7g'ck", ",", "un", "teihn\u00b7mal", "nee", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "NE", "$,", "FM", "FM", "FM", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wenn sonn Unrack den D\u00f6st frigen deit,", "tokens": ["Wenn", "sonn", "Un\u00b7rack", "den", "D\u00f6st", "fri\u00b7gen", "deit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "denn trugt de D\u00fcwel se.", "tokens": ["denn", "trugt", "de", "D\u00fc\u00b7wel", "se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NE", "VVFIN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}}}}