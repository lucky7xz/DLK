{"textgrid.poem.41829": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl denk ich selig meiner Kindheit Tagen,", "tokens": ["Wohl", "denk", "ich", "se\u00b7lig", "mei\u00b7ner", "Kind\u00b7heit", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da ich gespielt mit V\u00f6geln und mit Bl\u00fcten,", "tokens": ["Da", "ich", "ge\u00b7spielt", "mit", "V\u00f6\u00b7geln", "und", "mit", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo in der Mutterarmen treuem H\u00fcten", "tokens": ["Wo", "in", "der", "Mut\u00b7ter\u00b7ar\u00b7men", "treu\u00b7em", "H\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir alle Paradiese offen lagen.", "tokens": ["Mir", "al\u00b7le", "Pa\u00b7ra\u00b7die\u00b7se", "of\u00b7fen", "la\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wo ich die ersten B\u00fccher aufgeschlagen,", "tokens": ["Wo", "ich", "die", "ers\u00b7ten", "B\u00fc\u00b7cher", "auf\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich drein versenkt mit ernsthaft stillem Br\u00fcten,", "tokens": ["Mich", "drein", "ver\u00b7senkt", "mit", "ernst\u00b7haft", "stil\u00b7lem", "Br\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis meine bleichen Wangen hei\u00df ergl\u00fchten", "tokens": ["Bis", "mei\u00b7ne", "blei\u00b7chen", "Wan\u00b7gen", "hei\u00df", "er\u00b7gl\u00fch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von meines Volkes sch\u00f6nen Heldensagen.", "tokens": ["Von", "mei\u00b7nes", "Vol\u00b7kes", "sch\u00f6\u00b7nen", "Hel\u00b7den\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dann sprach die Mutter: \u00bbKind, es sind Gedichte.", "tokens": ["Dann", "sprach", "die", "Mut\u00b7ter", ":", "\u00bb", "Kind", ",", "es", "sind", "Ge\u00b7dich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "NN", "$,", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vergi\u00df es nicht: all' was du hier gelesen,", "tokens": ["Ver\u00b7gi\u00df", "es", "nicht", ":", "all'", "was", "du", "hier", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$.", "PIS", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zeigt dir das Leben oft in anderm Lichte.\u00ab", "tokens": ["Zeigt", "dir", "das", "Le\u00b7ben", "oft", "in", "an\u00b7derm", "Lich\u00b7te", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Warum denn unterscheiden Sein und Wesen?", "tokens": ["Wa\u00b7rum", "denn", "un\u00b7ter\u00b7schei\u00b7den", "Sein", "und", "We\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nein, Mutter \u2013 meinen Glauben nicht vernichte!", "tokens": ["Nein", ",", "Mut\u00b7ter", "\u2013", "mei\u00b7nen", "Glau\u00b7ben", "nicht", "ver\u00b7nich\u00b7te", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$(", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann krankte ich und k\u00f6nnte nie genesen!", "tokens": ["Dann", "krank\u00b7te", "ich", "und", "k\u00f6nn\u00b7te", "nie", "ge\u00b7ne\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VMFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}