{"textgrid.poem.24891": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon zur Heimfahrt ruft das Meer;", "tokens": ["Schon", "zur", "Heim\u00b7fahrt", "ruft", "das", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch wie wird das Herz so schwer", "tokens": ["Doch", "wie", "wird", "das", "Herz", "so", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VAFIN", "ART", "NN", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir beim Abschiednehmen!", "tokens": ["Mir", "beim", "Ab\u00b7schied\u00b7neh\u00b7men", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und auch du, mein Weggeno\u00df,", "tokens": ["Und", "auch", "du", ",", "mein", "Weg\u00b7ge\u00b7no\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Blickst so traurig, treues Ro\u00df,", "tokens": ["Blickst", "so", "trau\u00b7rig", ",", "treu\u00b7es", "Ro\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Edelstes von Jemen.", "tokens": ["E\u00b7dels\u00b7tes", "von", "Je\u00b7men."], "token_info": ["word", "word", "abbreviation"], "pos": ["NN", "APPR", "NE"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Fern von jeder Menschenspur \u2013", "tokens": ["Fern", "von", "je\u00b7der", "Men\u00b7schen\u00b7spur", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchrer uns die Sterne nur", "tokens": ["F\u00fch\u00b7rer", "uns", "die", "Ster\u00b7ne", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An des Himmels Bogen \u2013", "tokens": ["An", "des", "Him\u00b7mels", "Bo\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wie zwei Br\u00fcder, nie getrennt,", "tokens": ["Wie", "zwei", "Br\u00fc\u00b7der", ",", "nie", "ge\u00b7trennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "CARD", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch den weiten Orient", "tokens": ["Durch", "den", "wei\u00b7ten", "O\u00b7rient"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Sind wir hingezogen.", "tokens": ["Sind", "wir", "hin\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ueber Berge, steil und schroff,", "tokens": ["Ue\u00b7ber", "Ber\u00b7ge", ",", "steil", "und", "schroff", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob auch Schaum vom Bug dir troff,", "tokens": ["Ob", "auch", "Schaum", "vom", "Bug", "dir", "troff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Flogst du, nie ermattet;", "tokens": ["Flogst", "du", ",", "nie", "er\u00b7mat\u00b7tet", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Trugst mich durch der W\u00fcste Sand,", "tokens": ["Trugst", "mich", "durch", "der", "W\u00fcs\u00b7te", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo vor lohem Sonnenbrand", "tokens": ["Wo", "vor", "lo\u00b7hem", "Son\u00b7nen\u00b7brand"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Keine Palme schattet.", "tokens": ["Kei\u00b7ne", "Pal\u00b7me", "schat\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Unser Mais- und Dattelmahl", "tokens": ["Un\u00b7ser", "Mais", "und", "Dat\u00b7tel\u00b7mahl"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "TRUNC", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Teilten wir im Felsenthal", "tokens": ["Teil\u00b7ten", "wir", "im", "Fel\u00b7sen\u00b7thal"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An des Brunnens K\u00fchle;", "tokens": ["An", "des", "Brun\u00b7nens", "K\u00fch\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Nachts, an dich dahingelehnt,", "tokens": ["Nachts", ",", "an", "dich", "da\u00b7hin\u00b7ge\u00b7lehnt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deinen Nacken, weichgem\u00e4hnt,", "tokens": ["Dei\u00b7nen", "Na\u00b7cken", ",", "weich\u00b7ge\u00b7m\u00e4hnt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00e4hlt' ich mir zum Pf\u00fchle.", "tokens": ["W\u00e4hlt'", "ich", "mir", "zum", "Pf\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Achtsam sp\u00e4hend immerdar", "tokens": ["Acht\u00b7sam", "sp\u00e4\u00b7hend", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich zu sch\u00fctzen vor Gefahr,", "tokens": ["Mich", "zu", "sch\u00fct\u00b7zen", "vor", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kaum Minuten schliefst du;", "tokens": ["Kaum", "Mi\u00b7nu\u00b7ten", "schliefst", "du", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wenn, von M\u00fcdigkeit wie starr,", "tokens": ["Wenn", ",", "von", "M\u00fc\u00b7dig\u00b7keit", "wie", "starr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "NN", "KOKOM", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Noch ich lag, mit Hufgescharr", "tokens": ["Noch", "ich", "lag", ",", "mit", "Huf\u00b7ge\u00b7scharr"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "$,", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schon zum Aufbruch riefst du.", "tokens": ["Schon", "zum", "Auf\u00b7bruch", "riefst", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Als ich matt und fieberkrank", "tokens": ["Als", "ich", "matt", "und", "fie\u00b7ber\u00b7krank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Chane niedersank", "tokens": ["In", "dem", "Cha\u00b7ne", "nie\u00b7der\u00b7sank"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es in mir Nacht ward,", "tokens": ["Und", "es", "in", "mir", "Nacht", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "NN", "VAFIN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "An der harten Bl\u00e4tterstreu,", "tokens": ["An", "der", "har\u00b7ten", "Bl\u00e4t\u00b7ter\u00b7streu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Drauf ich ruhte, o wie treu", "tokens": ["Drauf", "ich", "ruh\u00b7te", ",", "o", "wie", "treu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "PPER", "VVFIN", "$,", "FM", "KOKOM", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich von dir bewacht ward!", "tokens": ["Ich", "von", "dir", "be\u00b7wacht", "ward", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Lebe wohl! Von Ort zu Ort", "tokens": ["Le\u00b7be", "wohl", "!", "Von", "Ort", "zu", "Ort"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$.", "APPR", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch im Abendland hinfort", "tokens": ["Auch", "im", "A\u00b7bend\u00b7land", "hin\u00b7fort"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Feir' ich dich im Liede;", "tokens": ["Feir'", "ich", "dich", "im", "Lie\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und, noch wenn ich heimgekehrt,", "tokens": ["Und", ",", "noch", "wenn", "ich", "heim\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Preis' im Osten, teures Pferd,", "tokens": ["Preis'", "im", "Os\u00b7ten", ",", "teu\u00b7res", "Pferd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lang dich die Kasside;", "tokens": ["Lang", "dich", "die", "Kas\u00b7si\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Preise deiner Glieder Pracht,", "tokens": ["Prei\u00b7se", "dei\u00b7ner", "Glie\u00b7der", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwarz wie Wetterwolkennacht,", "tokens": ["Schwarz", "wie", "Wet\u00b7ter\u00b7wol\u00b7ken\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlank wie die Cypresse,", "tokens": ["Schlank", "wie", "die", "Cyp\u00b7res\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und, die durch das Dunkel fern", "tokens": ["Und", ",", "die", "durch", "das", "Dun\u00b7kel", "fern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PRELS", "APPR", "ART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Leuchtet wie der Morgenstern,", "tokens": ["Leuch\u00b7tet", "wie", "der", "Mor\u00b7gens\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Deiner Stirne Bl\u00e4sse!", "tokens": ["Dei\u00b7ner", "Stir\u00b7ne", "Bl\u00e4s\u00b7se", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}