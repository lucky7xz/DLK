{"textgrid.poem.56466": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die bleiche Leinwand in den vielen Betten", "tokens": ["Die", "blei\u00b7che", "Lein\u00b7wand", "in", "den", "vie\u00b7len", "Bet\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verschwimmt in kahler Wand im Krankensaal.", "tokens": ["Ver\u00b7schwimmt", "in", "kah\u00b7ler", "Wand", "im", "Kran\u00b7ken\u00b7saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Krankheiten alle, d\u00fcnne Marionetten,", "tokens": ["Die", "Krank\u00b7hei\u00b7ten", "al\u00b7le", ",", "d\u00fcn\u00b7ne", "Ma\u00b7ri\u00b7o\u00b7net\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "$,", "ADJA", "NN", "$,"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Spazieren in den G\u00e4ngen. Eine Zahl", "tokens": ["Spa\u00b7zie\u00b7ren", "in", "den", "G\u00e4n\u00b7gen", ".", "Ei\u00b7ne", "Zahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hat jeder Kranke. Und mit wei\u00dfer Kreide", "tokens": ["Hat", "je\u00b7der", "Kran\u00b7ke", ".", "Und", "mit", "wei\u00b7\u00dfer", "Krei\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "$.", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind seine Qualen sauber aufnotiert.", "tokens": ["Sind", "sei\u00b7ne", "Qua\u00b7len", "sau\u00b7ber", "auf\u00b7no\u00b7tiert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Fieber donnert. Ihre Eingeweide", "tokens": ["Das", "Fie\u00b7ber", "don\u00b7nert", ".", "Ih\u00b7re", "Ein\u00b7ge\u00b7wei\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Brennen wie Berge. Und ihr Auge stiert", "tokens": ["Bren\u00b7nen", "wie", "Ber\u00b7ge", ".", "Und", "ihr", "Au\u00b7ge", "stiert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "NN", "$.", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Zur Decke auf, wo ein paar gro\u00dfe Spinnen", "tokens": ["Zur", "De\u00b7cke", "auf", ",", "wo", "ein", "paar", "gro\u00b7\u00dfe", "Spin\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "PWAV", "ART", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus ihrem Bauche lange F\u00e4den ziehn.", "tokens": ["Aus", "ih\u00b7rem", "Bau\u00b7che", "lan\u00b7ge", "F\u00e4\u00b7den", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie sitzen auf in ihrem kalten Linnen", "tokens": ["Sie", "sit\u00b7zen", "auf", "in", "ih\u00b7rem", "kal\u00b7ten", "Lin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ihrem Schwei\u00df mit hochgezognen Knien.", "tokens": ["Und", "ih\u00b7rem", "Schwei\u00df", "mit", "hoch\u00b7ge\u00b7zog\u00b7nen", "Kni\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie bei\u00dfen auf die N\u00e4gel ihrer Hand.", "tokens": ["Sie", "bei\u00b7\u00dfen", "auf", "die", "N\u00e4\u00b7gel", "ih\u00b7rer", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Falten ihrer Stirn, die r\u00f6tlich gl\u00fcht,", "tokens": ["Die", "Fal\u00b7ten", "ih\u00b7rer", "Stirn", ",", "die", "r\u00f6t\u00b7lich", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sind wie ein graugefurchtes Ackerland,", "tokens": ["Sind", "wie", "ein", "grau\u00b7ge\u00b7furch\u00b7tes", "A\u00b7cker\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf dem des Todes gro\u00dfes Fr\u00fchrot bl\u00fcht.", "tokens": ["Auf", "dem", "des", "To\u00b7des", "gro\u00b7\u00dfes", "Fr\u00fch\u00b7rot", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sie strecken ihre wei\u00dfen Arme vor,", "tokens": ["Sie", "stre\u00b7cken", "ih\u00b7re", "wei\u00b7\u00dfen", "Ar\u00b7me", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vor K\u00e4lte zitternd und vor Grauen stumm.", "tokens": ["Vor", "K\u00e4l\u00b7te", "zit\u00b7ternd", "und", "vor", "Grau\u00b7en", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "KON", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schon w\u00e4lzt ihr Hirn sich schwarz von Ohr zu Ohr", "tokens": ["Schon", "w\u00e4lzt", "ihr", "Hirn", "sich", "schwarz", "von", "Ohr", "zu", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PRF", "ADJD", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In ungeheurem Wirbel schnell herum.", "tokens": ["In", "un\u00b7ge\u00b7heu\u00b7rem", "Wir\u00b7bel", "schnell", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Dann g\u00e4hnt in ihrem R\u00fccken schwarz ein Spalt,", "tokens": ["Dann", "g\u00e4hnt", "in", "ih\u00b7rem", "R\u00fc\u00b7cken", "schwarz", "ein", "Spalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und aus der wei\u00dfget\u00fcnchten Mauerwand", "tokens": ["Und", "aus", "der", "wei\u00df\u00b7ge\u00b7t\u00fcnch\u00b7ten", "Mau\u00b7er\u00b7wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Streckt sich ein Arm. Um ihre Kehle ballt", "tokens": ["Streckt", "sich", "ein", "Arm", ".", "Um", "ih\u00b7re", "Keh\u00b7le", "ballt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "$.", "KOUI", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich langsam eine harte Knochenhand.", "tokens": ["Sich", "lang\u00b7sam", "ei\u00b7ne", "har\u00b7te", "Kno\u00b7chen\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}