{"textgrid.poem.51383": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Aufwecker an die Reime", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Reime fort: Ihr S\u00f6hne reiner Sinnen,", "tokens": ["Ihr", "Rei\u00b7me", "fort", ":", "Ihr", "S\u00f6h\u00b7ne", "rei\u00b7ner", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$.", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du seelge Krafft der anderen Geburt,", "tokens": ["Du", "seel\u00b7ge", "Krafft", "der", "an\u00b7de\u00b7ren", "Ge\u00b7burt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Seyd wolgemuth, steigt freudig an den Port,", "tokens": ["Seyd", "wol\u00b7ge\u00b7muth", ",", "steigt", "freu\u00b7dig", "an", "den", "Port", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "VVFIN", "ADJD", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Sch\u00fcler ihr, so Lehrer selbst beginnen.", "tokens": ["Ihr", "Sch\u00fc\u00b7ler", "ihr", ",", "so", "Leh\u00b7rer", "selbst", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$,", "ADV", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn mancher Geist der Krafft wird werden innen,", "tokens": ["Wenn", "man\u00b7cher", "Geist", "der", "Krafft", "wird", "wer\u00b7den", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "NN", "VAFIN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die aus euch strahlt und weist ihm euren Hort,", "tokens": ["Die", "aus", "euch", "strahlt", "und", "weist", "ihm", "eu\u00b7ren", "Hort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Denn wird er schreyn: Auf! Auf! itzt last uns fort,", "tokens": ["Denn", "wird", "er", "schreyn", ":", "Auf", "!", "Auf", "!", "itzt", "last", "uns", "fort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VAINF", "$.", "APPR", "$.", "APPR", "$.", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Itzt wollen wir im Wesen sammt zurinnen.", "tokens": ["Itzt", "wol\u00b7len", "wir", "im", "We\u00b7sen", "sammt", "zu\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wer inner sich das Scheide Wasser hat,", "tokens": ["Wer", "in\u00b7ner", "sich", "das", "Schei\u00b7de", "Was\u00b7ser", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PRF", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Dadurch die Seel aufl\u00f6st die Ding im Wesen,", "tokens": ["Da\u00b7durch", "die", "Seel", "auf\u00b7l\u00f6st", "die", "Ding", "im", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wird euern Kern stracks brechen in der That,", "tokens": ["Wird", "eu\u00b7ern", "Kern", "stracks", "bre\u00b7chen", "in", "der", "That", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und Gott in Ihm, Natur in ihn sich lesen.", "tokens": ["Und", "Gott", "in", "Ihm", ",", "Na\u00b7tur", "in", "ihn", "sich", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPER", "$,", "NN", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Was h\u00f6r ich: ab: gebt den Phaleucus stat,", "tokens": ["Was", "h\u00f6r", "ich", ":", "ab", ":", "gebt", "den", "Pha\u00b7leu\u00b7cus", "stat", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PTKVZ", "$.", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.14": {"text": "Wer ihn nicht h\u00f6rt: bleibt mit Euch ungenesen!", "tokens": ["Wer", "ihn", "nicht", "h\u00f6rt", ":", "bleibt", "mit", "Euch", "un\u00b7ge\u00b7ne\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVFIN", "$.", "VVFIN", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}