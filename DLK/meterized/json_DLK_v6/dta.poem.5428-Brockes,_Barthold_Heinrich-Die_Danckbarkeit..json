{"dta.poem.5428": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Danckbarkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Oft hab ich bey mir \u00fcberlegt, nachdem ich \u00fcberzeuglich", "tokens": ["Oft", "hab", "ich", "bey", "mir", "\u00fc\u00b7ber\u00b7legt", ",", "nach\u00b7dem", "ich", "\u00fc\u00b7berz\u00b7eug\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "sehe,", "tokens": ["se\u00b7he", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Da\u00df mehrentheils in frohem Dancken der wahre GOttes-", "tokens": ["Da\u00df", "meh\u00b7ren\u00b7theils", "in", "fro\u00b7hem", "Dan\u00b7cken", "der", "wah\u00b7re", "Got\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dienst bestehe,", "tokens": ["Dienst", "be\u00b7ste\u00b7he", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Was Dancken eigentlich denn sey? Wenn man der Men-", "tokens": ["Was", "Dan\u00b7cken", "ei\u00b7gent\u00b7lich", "denn", "sey", "?", "Wenn", "man", "der", "Men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "NN", "ADV", "ADV", "VAFIN", "$.", "KOUS", "PIS", "ART", "TRUNC"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "schen Danck erweget,", "tokens": ["schen", "Danck", "er\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Und die gemeine kalte Art zu dancken ernstlich \u00fcberleget;", "tokens": ["Und", "die", "ge\u00b7mei\u00b7ne", "kal\u00b7te", "Art", "zu", "dan\u00b7cken", "ernst\u00b7lich", "\u00fc\u00b7ber\u00b7le\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "PTKZU", "VVINF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.8": {"text": "So kommt es mir nicht anders f\u00fcr:", "tokens": ["So", "kommt", "es", "mir", "nicht", "an\u00b7ders", "f\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Als da\u00df man mit dem blossen Schall der Worte: ", "tokens": ["Als", "da\u00df", "man", "mit", "dem", "blos\u00b7sen", "Schall", "der", "Wor\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "ich dancke dir!", "tokens": ["ich", "dan\u00b7cke", "dir", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Die GOttheit gnug bezahlet glaube: ohn da\u00df, vom An-", "tokens": ["Die", "Got\u00b7theit", "gnug", "be\u00b7zah\u00b7let", "glau\u00b7be", ":", "ohn", "da\u00df", ",", "vom", "An"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "VVFIN", "$.", "APPR", "KOUS", "$,", "APPRART", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.12": {"text": "dachts-Feur ger\u00fchret,", "tokens": ["dachts\u00b7Feur", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Die Seele, durch Erk\u00e4nntlichkeit empfangner Gaben, et-", "tokens": ["Die", "See\u00b7le", ",", "durch", "Er\u00b7k\u00e4nnt\u00b7lich\u00b7keit", "emp\u00b7fang\u00b7ner", "Ga\u00b7ben", ",", "et"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "APPR", "NN", "ADJA", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "was sp\u00fchret,", "tokens": ["was", "sp\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "So sie zur Gegen-Liebe reitzt, und das sie von der Macht", "tokens": ["So", "sie", "zur", "Ge\u00b7gen\u00b7Lie\u00b7be", "reitzt", ",", "und", "das", "sie", "von", "der", "Macht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPRART", "NN", "VVFIN", "$,", "KON", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.16": {"text": "und G\u00fcte", "tokens": ["und", "G\u00fc\u00b7te"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Des grossen Sch\u00f6pfers aller Dinge unwiedersprechlich", "tokens": ["Des", "gros\u00b7sen", "Sch\u00f6p\u00b7fers", "al\u00b7ler", "Din\u00b7ge", "un\u00b7wie\u00b7der\u00b7sprech\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "\u00fcberf\u00fchret.", "tokens": ["\u00fc\u00b7berf\u00b7\u00fch\u00b7ret", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Wenn noch, von so viel tausenden, die gar nicht dancken,", "tokens": ["Wenn", "noch", ",", "von", "so", "viel", "tau\u00b7sen\u00b7den", ",", "die", "gar", "nicht", "dan\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "APPR", "ADV", "PIAT", "ADJA", "$,", "PRELS", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "ein Gem\u00fcthe", "tokens": ["ein", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "Dem grossen GOtt einst dancken will, so werden ihm Jdeen", "tokens": ["Dem", "gros\u00b7sen", "Gott", "einst", "dan\u00b7cken", "will", ",", "so", "wer\u00b7den", "ihm", "Jdeen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "VMFIN", "$,", "ADV", "VAFIN", "PPER", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.22": {"text": "fehlen;", "tokens": ["feh\u00b7len", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "An Worten fehlt es ebenfal\u00df. Man wei\u00df nicht was man", "tokens": ["An", "Wor\u00b7ten", "fehlt", "es", "e\u00b7ben\u00b7fal\u00df", ".", "Man", "wei\u00df", "nicht", "was", "man"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "$.", "PIS", "VVFIN", "PTKNEG", "PWS", "PIS"], "meter": "-+-+-+-+-+-++", "measure": "unknown.measure.septa"}, "line.24": {"text": "sagen soll;", "tokens": ["sa\u00b7gen", "soll", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.25": {"text": "Ein kalt ", "tokens": ["Ein", "kalt"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.26": {"text": "K\u00f6mmts hoch, so wird man, als was neues, die Wohl-", "tokens": ["K\u00f6mmts", "hoch", ",", "so", "wird", "man", ",", "als", "was", "neu\u00b7es", ",", "die", "Wohl"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJD", "$,", "ADV", "VAFIN", "PIS", "$,", "KOUS", "PIS", "ADJA", "$,", "ART", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.27": {"text": "that einem Freund' erzehlen.", "tokens": ["that", "ei\u00b7nem", "Freund'", "er\u00b7zeh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Auf, la\u00dft uns denn den gantzen Geist, auf, la\u00dft uns", "tokens": ["Auf", ",", "la\u00dft", "uns", "denn", "den", "gant\u00b7zen", "Geist", ",", "auf", ",", "la\u00dft", "uns"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "$,", "VVIMP", "PPER", "ADV", "ART", "ADJA", "NN", "$,", "PTKVZ", "$,", "VVIMP", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "unsrer Seelen Kr\u00e4fte,", "tokens": ["uns\u00b7rer", "See\u00b7len", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.30": {"text": "Mit Ernst bem\u00fcht seyn anzuwenden zu diesem heilsamen", "tokens": ["Mit", "Ernst", "be\u00b7m\u00fcht", "seyn", "an\u00b7zu\u00b7wen\u00b7den", "zu", "die\u00b7sem", "heil\u00b7sa\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVPP", "VAINF", "VVIZU", "APPR", "PDAT", "ADJA"], "meter": "-+-+-+-+--++-+-", "measure": "iambic.septa.relaxed"}, "line.31": {"text": "Gesch\u00e4fte!", "tokens": ["Ge\u00b7sch\u00e4f\u00b7te", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}