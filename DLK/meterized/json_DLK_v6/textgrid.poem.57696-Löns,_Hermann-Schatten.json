{"textgrid.poem.57696": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Schatten", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Krauser Birkenzweige Schatten", "tokens": ["Krau\u00b7ser", "Bir\u00b7ken\u00b7zwei\u00b7ge", "Schat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tanzen auf dem wei\u00dfen Sand.", "tokens": ["Tan\u00b7zen", "auf", "dem", "wei\u00b7\u00dfen", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Augen sind voll Sehnsucht", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", "sind", "voll", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Auf die Schatten hingebannt.", "tokens": ["Auf", "die", "Schat\u00b7ten", "hin\u00b7ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deiner Locken Schatten denk ich,", "tokens": ["Dei\u00b7ner", "Lo\u00b7cken", "Schat\u00b7ten", "denk", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die auf deiner Stirne sind,", "tokens": ["Die", "auf", "dei\u00b7ner", "Stir\u00b7ne", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die bei meinen Seufzern tanzen,", "tokens": ["Die", "bei", "mei\u00b7nen", "Seuf\u00b7zern", "tan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die Zweige in dem Wind.", "tokens": ["Wie", "die", "Zwei\u00b7ge", "in", "dem", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}