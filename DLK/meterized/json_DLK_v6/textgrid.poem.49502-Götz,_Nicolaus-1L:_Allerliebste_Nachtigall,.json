{"textgrid.poem.49502": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Allerliebste Nachtigall,", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Allerliebste Nachtigall,", "tokens": ["Al\u00b7ler\u00b7liebs\u00b7te", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schweige, denn wir sind alleine.", "tokens": ["Schwei\u00b7ge", ",", "denn", "wir", "sind", "al\u00b7lei\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PPER", "VAFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kl\u00e4ng dein angenehmer Schall,", "tokens": ["Kl\u00e4ng", "dein", "an\u00b7ge\u00b7neh\u00b7mer", "Schall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mi\u00dfgunst nahte diesem Haine,", "tokens": ["Mi\u00df\u00b7gunst", "nah\u00b7te", "die\u00b7sem", "Hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sezte sich zum Wa\u00dferfall,", "tokens": ["Sez\u00b7te", "sich", "zum", "Wa\u00b7\u00dfer\u00b7fall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo ich s\u00fc\u00df f\u00fcr Wohllust weine,", "tokens": ["Wo", "ich", "s\u00fc\u00df", "f\u00fcr", "Wohl\u00b7lust", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und verrieth uns \u00fcberall. \u2013", "tokens": ["Und", "ver\u00b7rieth", "uns", "\u00fc\u00b7be\u00b7rall", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df ich s\u00fc\u00df f\u00fcr Wohllust weine,", "tokens": ["Da\u00df", "ich", "s\u00fc\u00df", "f\u00fcr", "Wohl\u00b7lust", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "W\u00fcrkte mir dann lauter Quaal.", "tokens": ["W\u00fcrk\u00b7te", "mir", "dann", "lau\u00b7ter", "Qua\u00b7al", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Gl\u00fccklicher wein' ich alleine,", "tokens": ["Gl\u00fcck\u00b7li\u00b7cher", "wein'", "ich", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.11": {"text": "Allerliebste Nachtigall.", "tokens": ["Al\u00b7ler\u00b7liebs\u00b7te", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}