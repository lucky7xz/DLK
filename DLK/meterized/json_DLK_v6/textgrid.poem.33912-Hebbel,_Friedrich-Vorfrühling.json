{"textgrid.poem.33912": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Vorfr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie die Knospe h\u00fctend,", "tokens": ["Wie", "die", "Knos\u00b7pe", "h\u00fc\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Da\u00df sie nicht Blume werde,", "tokens": ["Da\u00df", "sie", "nicht", "Blu\u00b7me", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Liegt's so dumpf und br\u00fctend", "tokens": ["Liegt's", "so", "dumpf", "und", "br\u00fc\u00b7tend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ueber der dr\u00e4ngenden Erde.", "tokens": ["Ue\u00b7ber", "der", "dr\u00e4n\u00b7gen\u00b7den", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Wolkenmassen ballten", "tokens": ["Wol\u00b7ken\u00b7mas\u00b7sen", "ball\u00b7ten"], "token_info": ["word", "word"], "pos": ["NE", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sich der Sonne entgegen,", "tokens": ["Sich", "der", "Son\u00b7ne", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Doch durch tausend Spalten", "tokens": ["Doch", "durch", "tau\u00b7send", "Spal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "CARD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dringt der befruchtende Segen.", "tokens": ["Dringt", "der", "be\u00b7fruch\u00b7ten\u00b7de", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Gl\u00fch'nde D\u00fcfte ringeln", "tokens": ["Gl\u00fch'n\u00b7de", "D\u00fcf\u00b7te", "rin\u00b7geln"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "In die H\u00f6he sich munter.", "tokens": ["In", "die", "H\u00f6\u00b7he", "sich", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "ADJD", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Fl\u00fcchtig gr\u00fc\u00dfend, z\u00fcngeln", "tokens": ["Fl\u00fcch\u00b7tig", "gr\u00fc\u00b7\u00dfend", ",", "z\u00fcn\u00b7geln"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJD", "VVPP", "$,", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Streifende Lichter herunter.", "tokens": ["Strei\u00b7fen\u00b7de", "Lich\u00b7ter", "her\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Da\u00df nun, still erfrischend,", "tokens": ["Da\u00df", "nun", ",", "still", "er\u00b7fri\u00b7schend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Eins zum Andern sich finde,", "tokens": ["Eins", "zum", "An\u00b7dern", "sich", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "PRF", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "R\u00fchren, Alles mischend,", "tokens": ["R\u00fch\u00b7ren", ",", "Al\u00b7les", "mi\u00b7schend", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PIS", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sich lebendige Winde.", "tokens": ["Sich", "le\u00b7ben\u00b7di\u00b7ge", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}}}}