{"textgrid.poem.34504": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vierzig Schafe und zw\u00f6lf Malter Weizen", "tokens": ["Vier\u00b7zig", "Scha\u00b7fe", "und", "zw\u00f6lf", "Mal\u00b7ter", "Wei\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "KON", "CARD", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "nebst drei Eimern Weines wurden t\u00e4glich", "tokens": ["nebst", "drei", "Ei\u00b7mern", "Wei\u00b7nes", "wur\u00b7den", "t\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "NE", "VAFIN", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "am Altar des grossen Baal geopfert:", "tokens": ["am", "Al\u00b7tar", "des", "gros\u00b7sen", "Baal", "ge\u00b7o\u00b7pfert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "und am n\u00e4chsten Morgen war es alles", "tokens": ["und", "am", "n\u00e4chs\u00b7ten", "Mor\u00b7gen", "war", "es", "al\u00b7les"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "VAFIN", "PPER", "PIS"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "aufgezehrt, und gn\u00e4dig und ges\u00e4ttigt", "tokens": ["auf\u00b7ge\u00b7zehrt", ",", "und", "gn\u00e4\u00b7dig", "und", "ge\u00b7s\u00e4t\u00b7tigt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KON", "ADJD", "KON", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "grinste Baal herab auf seine Knechte.", "tokens": ["grins\u00b7te", "Baal", "her\u00b7ab", "auf", "sei\u00b7ne", "Knech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Auch der K\u00f6nig Cyrus diente t\u00e4glich", "tokens": ["Auch", "der", "K\u00f6\u00b7nig", "Cy\u00b7rus", "dien\u00b7te", "t\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NE", "VVFIN", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "seinem Gott und ging hinab zum Tempel,", "tokens": ["sei\u00b7nem", "Gott", "und", "ging", "hin\u00b7ab", "zum", "Tem\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "am Altar des grossen Baal zu beten.", "tokens": ["am", "Al\u00b7tar", "des", "gros\u00b7sen", "Baal", "zu", "be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Und er sprach zu Daniel, seinem Freunde,", "tokens": ["Und", "er", "sprach", "zu", "Da\u00b7ni\u00b7el", ",", "sei\u00b7nem", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "den er ehrlich hielt, obwohl er Jude:", "tokens": ["den", "er", "ehr\u00b7lich", "hielt", ",", "ob\u00b7wohl", "er", "Ju\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$,", "KOUS", "PPER", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sage mir, was betest du nicht auch an", "tokens": ["Sa\u00b7ge", "mir", ",", "was", "be\u00b7test", "du", "nicht", "auch", "an"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PWS", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "meinen Gott, den grossen Baal zu Babel?", "tokens": ["mei\u00b7nen", "Gott", ",", "den", "gros\u00b7sen", "Baal", "zu", "Ba\u00b7bel", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Daniel versetzte: Keine G\u00f6tzen,", "tokens": ["Da\u00b7ni\u00b7el", "ver\u00b7setz\u00b7te", ":", "Kei\u00b7ne", "G\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "die von Menschenhand gemacht, verehr ich,", "tokens": ["die", "von", "Men\u00b7schen\u00b7hand", "ge\u00b7macht", ",", "ver\u00b7ehr", "ich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVPP", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "einzig den lebendigen Gott des Himmels,", "tokens": ["ein\u00b7zig", "den", "le\u00b7ben\u00b7di\u00b7gen", "Gott", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Zebaoth, den Herren \u00fcber Alles!", "tokens": ["Ze\u00b7bao\u00b7th", ",", "den", "Her\u00b7ren", "\u00fc\u00b7ber", "Al\u00b7les", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "APPR", "PIS", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Sprach der K\u00f6nig: H\u00e4ltst du denn den Baal nicht", "tokens": ["Sprach", "der", "K\u00f6\u00b7nig", ":", "H\u00e4ltst", "du", "denn", "den", "Baal", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKNEG"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "f\u00fcr lebendig? Siehst du nicht, wie viel er", "tokens": ["f\u00fcr", "le\u00b7ben\u00b7dig", "?", "Siehst", "du", "nicht", ",", "wie", "viel", "er"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJD", "$.", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PIS", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "t\u00e4glich isst und trinkt?", "tokens": ["t\u00e4g\u00b7lich", "isst", "und", "trinkt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Doch Daniel lachte:", "tokens": ["Doch", "Da\u00b7ni\u00b7el", "lach\u00b7te", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Herr, mein K\u00f6nig, lass dich nicht beth\u00f6ren!", "tokens": ["Herr", ",", "mein", "K\u00f6\u00b7nig", ",", "lass", "dich", "nicht", "be\u00b7th\u00f6\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dieser Baal ist eine todte Puppe,", "tokens": ["Die\u00b7ser", "Baal", "ist", "ei\u00b7ne", "tod\u00b7te", "Pup\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "draussen Erz und drinnen eine H\u00f6hle:", "tokens": ["draus\u00b7sen", "Erz", "und", "drin\u00b7nen", "ei\u00b7ne", "H\u00f6h\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "was der G\u00f6tze frisst, verdaut der Priester!", "tokens": ["was", "der", "G\u00f6t\u00b7ze", "frisst", ",", "ver\u00b7daut", "der", "Pries\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Zornig ward der K\u00f6nig. Rufen liess er", "tokens": ["Zor\u00b7nig", "ward", "der", "K\u00f6\u00b7nig", ".", "Ru\u00b7fen", "liess", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$.", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "seine Priester, und er sprach zu ihnen:", "tokens": ["sei\u00b7ne", "Pries\u00b7ter", ",", "und", "er", "sprach", "zu", "ih\u00b7nen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Wenn ihr mir nicht sagt, wer all die Opfer", "tokens": ["Wenn", "ihr", "mir", "nicht", "sagt", ",", "wer", "all", "die", "Op\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVFIN", "$,", "PWS", "PIAT", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "t\u00e4glich frisst, die wir dem Baal bereiten,", "tokens": ["t\u00e4g\u00b7lich", "frisst", ",", "die", "wir", "dem", "Baal", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "PRELS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "m\u00fcsst ihr alle sterben. K\u00f6nnt ihr aber", "tokens": ["m\u00fcsst", "ihr", "al\u00b7le", "ster\u00b7ben", ".", "K\u00f6nnt", "ihr", "a\u00b7ber"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PIS", "VVINF", "$.", "VMFIN", "PPER", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "mir beweisen, dass sie Baal verzehre,", "tokens": ["mir", "be\u00b7wei\u00b7sen", ",", "dass", "sie", "Baal", "ver\u00b7zeh\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "so muss Daniel sterben, denn er l\u00e4stert", "tokens": ["so", "muss", "Da\u00b7ni\u00b7el", "ster\u00b7ben", ",", "denn", "er", "l\u00e4s\u00b7tert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "NE", "VVINF", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "unsern Gott!", "tokens": ["un\u00b7sern", "Gott", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Und Daniel rief: Herr! K\u00f6nig!", "tokens": ["Und", "Da\u00b7ni\u00b7el", "rief", ":", "Herr", "!", "K\u00f6\u00b7nig", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$.", "NN", "$.", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Es geschehe so, wie du geredet!", "tokens": ["Es", "ge\u00b7sche\u00b7he", "so", ",", "wie", "du", "ge\u00b7re\u00b7det", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}}}}