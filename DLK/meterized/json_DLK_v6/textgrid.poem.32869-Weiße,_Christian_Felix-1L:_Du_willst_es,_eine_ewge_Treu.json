{"textgrid.poem.32869": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du willst es, eine ewge Treu", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du willst es, eine ewge Treu", "tokens": ["Du", "willst", "es", ",", "ei\u00b7ne", "ew\u00b7ge", "Treu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Soll ich dir, sch\u00f6ne Doris, schw\u00f6ren:", "tokens": ["Soll", "ich", "dir", ",", "sch\u00f6\u00b7ne", "Do\u00b7ris", ",", "schw\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "$,", "ADJA", "NE", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du dringst darauf? wohlan es sey!", "tokens": ["Du", "dringst", "da\u00b7rauf", "?", "wo\u00b7hlan", "es", "sey", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PAV", "$.", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch mu\u00dft du meinen Wunsch erh\u00f6ren.", "tokens": ["Doch", "mu\u00dft", "du", "mei\u00b7nen", "Wunsch", "er\u00b7h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Eh hasse Freud und Jugend mich,", "tokens": ["Eh", "has\u00b7se", "Freud", "und", "Ju\u00b7gend", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eh sey mein Wein von Wasser tr\u00fcbe;", "tokens": ["Eh", "sey", "mein", "Wein", "von", "Was\u00b7ser", "tr\u00fc\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Eh ich nicht, sch\u00f6ne Doris, dich \u2013 \u2013", "tokens": ["Eh", "ich", "nicht", ",", "sch\u00f6\u00b7ne", "Do\u00b7ris", ",", "dich", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "$,", "ADJA", "NE", "$,", "PPER", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie alle M\u00e4dchen ewig liebe.", "tokens": ["Wie", "al\u00b7le", "M\u00e4d\u00b7chen", "e\u00b7wig", "lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}