{"textgrid.poem.48687": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "8. K\u00e4ufet ohne Geld!", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist das nicht wolfeil satt? Um nichts nicht biet' ich mich,", "tokens": ["Ist", "das", "nicht", "wol\u00b7feil", "satt", "?", "Um", "nichts", "nicht", "biet'", "ich", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "VMFIN", "ADJD", "$.", "APPR", "PIS", "PTKNEG", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der ich doch Alles bin und Niemand will mich k\u00e4ufen?", "tokens": ["der", "ich", "doch", "Al\u00b7les", "bin", "und", "Nie\u00b7mand", "will", "mich", "k\u00e4u\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PIS", "VAFIN", "KON", "PIS", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich bin ein starker Stab, wer will sich an mich steifen?", "tokens": ["Ich", "bin", "ein", "star\u00b7ker", "Stab", ",", "wer", "will", "sich", "an", "mich", "stei\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PWS", "VMFIN", "PRF", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Licht; sie aber tun, als sehn sie keinen Stich.", "tokens": ["Ein", "Licht", ";", "sie", "a\u00b7ber", "tun", ",", "als", "sehn", "sie", "kei\u00b7nen", "Stich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "ADV", "VVINF", "$,", "KOUS", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich bin die Liebe selbst; wer liebet mich f\u00fcr sich?", "tokens": ["Ich", "bin", "die", "Lie\u00b7be", "selbst", ";", "wer", "lie\u00b7bet", "mich", "f\u00fcr", "sich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$.", "PWS", "VVFIN", "PRF", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Brunnen Israel, wer will sich mit mir t\u00e4ufen?", "tokens": ["Der", "Brun\u00b7nen", "Is\u00b7rael", ",", "wer", "will", "sich", "mit", "mir", "t\u00e4u\u00b7fen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PWS", "VMFIN", "PRF", "APPR", "PPER", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die T\u00fcr' ins Himmelreich, wer will mich doch ergreifen?", "tokens": ["Die", "T\u00fcr'", "ins", "Him\u00b7mel\u00b7reich", ",", "wer", "will", "mich", "doch", "er\u00b7grei\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich rufe Nacht und Tag, sie schweigen trutziglich.", "tokens": ["Ich", "ru\u00b7fe", "Nacht", "und", "Tag", ",", "sie", "schwei\u00b7gen", "trut\u00b7zig\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ach, da\u00df der Sterbliche doch gar so ist verbolgen,", "tokens": ["Ach", ",", "da\u00df", "der", "Sterb\u00b7li\u00b7che", "doch", "gar", "so", "ist", "ver\u00b7bol\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "ART", "NN", "ADV", "ADV", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df er der Warheit auch verschworen hat zu folgen!", "tokens": ["da\u00df", "er", "der", "War\u00b7heit", "auch", "ver\u00b7schwo\u00b7ren", "hat", "zu", "fol\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVPP", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr Menschen sagt doch selbst, wie ihr mich haben wollt!", "tokens": ["Ihr", "Men\u00b7schen", "sagt", "doch", "selbst", ",", "wie", "ihr", "mich", "ha\u00b7ben", "wollt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADV", "$,", "PWAV", "PPER", "PRF", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Itzt wei\u00df ich wie ich euch recht werde wolgefallen", "tokens": ["Itzt", "wei\u00df", "ich", "wie", "ich", "euch", "recht", "wer\u00b7de", "wol\u00b7ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PWAV", "PPER", "PPER", "ADJD", "VAFIN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und wie ich angenehm und wert kan sein bei Allen.", "tokens": ["und", "wie", "ich", "an\u00b7ge\u00b7nehm", "und", "wert", "kan", "sein", "bei", "Al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "KON", "ADJD", "VMFIN", "PPOSAT", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil Gold ein ieder liebt, so will ich werden Gold.", "tokens": ["Weil", "Gold", "ein", "ie\u00b7der", "liebt", ",", "so", "will", "ich", "wer\u00b7den", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "PIS", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}