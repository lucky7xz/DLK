{"textgrid.poem.64683": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "310. Ein alter Dichter", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ich gesungen haben soll,", "tokens": ["Was", "ich", "ge\u00b7sun\u00b7gen", "ha\u00b7ben", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Las seit Jahrtausenden die Welt empfindungsvoll.", "tokens": ["Las", "seit", "Jahr\u00b7tau\u00b7sen\u00b7den", "die", "Welt", "emp\u00b7fin\u00b7dungs\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kalt, kritisch wird nunmehr gelesen,", "tokens": ["Kalt", ",", "kri\u00b7tisch", "wird", "nun\u00b7mehr", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was darthut, ich sey nie gewesen.", "tokens": ["Was", "dar\u00b7thut", ",", "ich", "sey", "nie", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}