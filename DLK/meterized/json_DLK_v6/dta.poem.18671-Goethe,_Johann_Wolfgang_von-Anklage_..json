{"dta.poem.18671": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Anklage .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Wisst ihr denn auf wen die Teufel lauern,", "tokens": ["Wisst", "ihr", "denn", "auf", "wen", "die", "Teu\u00b7fel", "lau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PWS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In der W\u00fcste, zwischen Fels und Mauern?", "tokens": ["In", "der", "W\u00fcs\u00b7te", ",", "zwi\u00b7schen", "Fels", "und", "Mau\u00b7ern", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "NE", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und, wie sie den Augenblick erpassen,", "tokens": ["Und", ",", "wie", "sie", "den", "Au\u00b7gen\u00b7blick", "er\u00b7pas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nach der H\u00f6lle sie entf\u00fchrend fassen?", "tokens": ["Nach", "der", "H\u00f6l\u00b7le", "sie", "ent\u00b7f\u00fch\u00b7rend", "fas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVPP", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "L\u00fcgner sind es und der B\u00f6sewicht.", "tokens": ["L\u00fcg\u00b7ner", "sind", "es", "und", "der", "B\u00f6\u00b7se\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Der Poete warum scheut er nicht", "tokens": ["Der", "Poe\u00b7te", "wa\u00b7rum", "scheut", "er", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PWAV", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich mit solchen Leuten einzulassen!", "tokens": ["Sich", "mit", "sol\u00b7chen", "Leu\u00b7ten", "ein\u00b7zu\u00b7las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Weiss denn der mit wem er geht und wandelt?", "tokens": ["Weiss", "denn", "der", "mit", "wem", "er", "geht", "und", "wan\u00b7delt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "APPR", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Er der immer nur im Wahnsinn handelt.", "tokens": ["Er", "der", "im\u00b7mer", "nur", "im", "Wahn\u00b7sinn", "han\u00b7delt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Gr\u00e4nzenlos, von eigensinn\u2019gem Lieben,", "tokens": ["Gr\u00e4n\u00b7zen\u00b7los", ",", "von", "ei\u00b7gen\u00b7sinn'\u00b7gem", "Lie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wird er in die Oede fortgetrieben,", "tokens": ["Wird", "er", "in", "die", "O\u00b7e\u00b7de", "fort\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Seiner Klagen Reim, in Sand geschrieben,", "tokens": ["Sei\u00b7ner", "Kla\u00b7gen", "Reim", ",", "in", "Sand", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sind vom Winde gleich verjagt;", "tokens": ["Sind", "vom", "Win\u00b7de", "gleich", "ver\u00b7jagt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Er versteht nicht was er sagt,", "tokens": ["Er", "ver\u00b7steht", "nicht", "was", "er", "sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Was er sagt wird er nicht halten.", "tokens": ["Was", "er", "sagt", "wird", "er", "nicht", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch sein Lied man l\u00e4sst es immer walten,", "tokens": ["Doch", "sein", "Lied", "man", "l\u00e4sst", "es", "im\u00b7mer", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PIS", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da es doch dem Coran widerspricht.", "tokens": ["Da", "es", "doch", "dem", "Co\u00b7ran", "wi\u00b7der\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Lehret nun, ihr des Gesetzes Kenner,", "tokens": ["Leh\u00b7ret", "nun", ",", "ihr", "des", "Ge\u00b7set\u00b7zes", "Ken\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "ART", "NN", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Weisheit-fromme, hochgelahrte M\u00e4nner,", "tokens": ["Weis\u00b7heit\u00b7from\u00b7me", ",", "hoch\u00b7ge\u00b7lahr\u00b7te", "M\u00e4n\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Treuer Mosleminen feste Pflicht.", "tokens": ["Treu\u00b7er", "Mos\u00b7le\u00b7mi\u00b7nen", "fes\u00b7te", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Hafis, in\u2019s besondre, schaffet Aergernisse,", "tokens": ["Ha\u00b7fis", ",", "in's", "be\u00b7sond\u00b7re", ",", "schaf\u00b7fet", "A\u00b7er\u00b7ger\u00b7nis\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "ADJA", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Mirza sprengt den Geist ins Ungewisse,", "tokens": ["Mir\u00b7za", "sprengt", "den", "Geist", "ins", "Un\u00b7ge\u00b7wis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Saget, was man thun und lassen m\u00fcsse?", "tokens": ["Sa\u00b7get", ",", "was", "man", "thun", "und", "las\u00b7sen", "m\u00fcs\u00b7se", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PIS", "VVINF", "KON", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}