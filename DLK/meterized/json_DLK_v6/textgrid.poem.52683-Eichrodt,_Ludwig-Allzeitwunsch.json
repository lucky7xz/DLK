{"textgrid.poem.52683": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Allzeitwunsch", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dieser wunderbaren Zeit", "tokens": ["In", "die\u00b7ser", "wun\u00b7der\u00b7ba\u00b7ren", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo man zerst\u00f6rt und baut,", "tokens": ["Wo", "man", "zer\u00b7st\u00f6rt", "und", "baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da hab ich viel gezweifelt", "tokens": ["Da", "hab", "ich", "viel", "ge\u00b7zwei\u00b7felt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und habe viel vertraut.", "tokens": ["Und", "ha\u00b7be", "viel", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Von Mitteln h\u00f6rt ich und vom Zweck,", "tokens": ["Von", "Mit\u00b7teln", "h\u00f6rt", "ich", "und", "vom", "Zweck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Recht und Unrecht stets,", "tokens": ["Von", "Recht", "und", "Un\u00b7recht", "stets", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Recht und Rechtsgef\u00fchlen,", "tokens": ["Von", "Recht", "und", "Rechts\u00b7ge\u00b7f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Moral und Rechts-Gesetz.", "tokens": ["Mo\u00b7ral", "und", "Rechts\u00b7Ge\u00b7setz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Man schied Geschichte und Vernunft,", "tokens": ["Man", "schied", "Ge\u00b7schich\u00b7te", "und", "Ver\u00b7nunft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach von Altar und Thron,", "tokens": ["Sprach", "von", "Al\u00b7tar", "und", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Wahrheit und von Freiheit,", "tokens": ["Von", "Wahr\u00b7heit", "und", "von", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Man sprach von Religion.", "tokens": ["Man", "sprach", "von", "Re\u00b7li\u00b7gi\u00b7on", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich sah, was gut und b\u00f6se sei,", "tokens": ["Ich", "sah", ",", "was", "gut", "und", "b\u00f6\u00b7se", "sei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und w\u00fcnschte Jenem Heil,", "tokens": ["Und", "w\u00fcnschte", "Je\u00b7nem", "Heil", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Doch ward uns ohne Dieses", "tokens": ["Doch", "ward", "uns", "oh\u00b7ne", "Die\u00b7ses"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch Jenes nicht zu Theil.", "tokens": ["Auch", "Je\u00b7nes", "nicht", "zu", "Theil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich sah die Ohnmacht, sah die Kraft,", "tokens": ["Ich", "sah", "die", "Ohn\u00b7macht", ",", "sah", "die", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tr\u00e4gheit und Eifer viel,", "tokens": ["Tr\u00e4g\u00b7heit", "und", "Ei\u00b7fer", "viel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und Ziele sonder Wege", "tokens": ["Und", "Zie\u00b7le", "son\u00b7der", "We\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Wege sonder Ziel.", "tokens": ["Und", "We\u00b7ge", "son\u00b7der", "Ziel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gott helf uns Allen, sagte man", "tokens": ["Gott", "helf", "uns", "Al\u00b7len", ",", "sag\u00b7te", "man"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "NE", "$,", "VVFIN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur guten alten Zeit,", "tokens": ["Zur", "gu\u00b7ten", "al\u00b7ten", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es sagen: hilf dir selber,", "tokens": ["Es", "sa\u00b7gen", ":", "hilf", "dir", "sel\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "VVIMP", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl heutzutag die Leut.", "tokens": ["Wohl", "heut\u00b7zu\u00b7tag", "die", "Leut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ach, wenn ich einmal w\u00fcnschen darf,", "tokens": ["Ach", ",", "wenn", "ich", "ein\u00b7mal", "w\u00fcn\u00b7schen", "darf", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So w\u00fcnsch ich Eines nur:", "tokens": ["So", "w\u00fcnsch", "ich", "Ei\u00b7nes", "nur", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie niemals zu verl\u00e4ugnen", "tokens": ["Sie", "nie\u00b7mals", "zu", "ver\u00b7l\u00e4ug\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die eigenste Natur!", "tokens": ["Die", "ei\u00b7gens\u00b7te", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}