{"dta.poem.4243": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Lieblichkeit des Grases.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In einem angenehmen W\u00e4ldchen, im Schatten einer", "tokens": ["In", "ei\u00b7nem", "an\u00b7ge\u00b7neh\u00b7men", "W\u00e4ld\u00b7chen", ",", "im", "Schat\u00b7ten", "ei\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "dichten Linden,", "tokens": ["dich\u00b7ten", "Lin\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Worunter ich zum \u00f6ftern Ruh, in stiller Anmuht, pflag zu", "tokens": ["Wo\u00b7run\u00b7ter", "ich", "zum", "\u00f6f\u00b7tern", "Ruh", ",", "in", "stil\u00b7ler", "An\u00b7muht", ",", "pflag", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "APPR"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "finden,", "tokens": ["fin\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Setzt\u2019 ich mich auf ein gr\u00fcnes Pl\u00e4tzchen in junges, dicht- und", "tokens": ["Setzt'", "ich", "mich", "auf", "ein", "gr\u00fc\u00b7nes", "Pl\u00e4tz\u00b7chen", "in", "jun\u00b7ges", ",", "dicht", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "APPR", "ADJA", "$,", "TRUNC", "KON"], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "frisches Gras,", "tokens": ["fri\u00b7sches", "Gras", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Woselbst ich anfangs zwar ein Buch, mit innigem Vergn\u00fc-", "tokens": ["Wo\u00b7selbst", "ich", "an\u00b7fangs", "zwar", "ein", "Buch", ",", "mit", "in\u00b7ni\u00b7gem", "Ver\u00b7gn\u00fc"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ART", "NN", "$,", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "gen, las;", "tokens": ["gen", ",", "las", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Allein, es rief das gr\u00fcne Prangen des Grases, das den", "tokens": ["Al\u00b7lein", ",", "es", "rief", "das", "gr\u00fc\u00b7ne", "Pran\u00b7gen", "des", "Gra\u00b7ses", ",", "das", "den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "sch\u00f6nsten Sammt,", "tokens": ["sch\u00f6ns\u00b7ten", "Sammt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Wie Diamant das Glas besch\u00e4met, zumahl, wenn oft bald", "tokens": ["Wie", "Di\u00b7a\u00b7mant", "das", "Glas", "be\u00b7sch\u00e4\u00b7met", ",", "zu\u00b7mahl", ",", "wenn", "oft", "bald"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "NN", "VVFIN", "$,", "KOUS", "$,", "KOUS", "ADV", "ADV"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "hier bald dort", "tokens": ["hier", "bald", "dort"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Der Sonnen Strahl, durch dunkle Schatten der Zweige,", "tokens": ["Der", "Son\u00b7nen", "Strahl", ",", "durch", "dunk\u00b7le", "Schat\u00b7ten", "der", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "brach, an manchem Ort,", "tokens": ["brach", ",", "an", "man\u00b7chem", "Ort", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "Auf tausendfach gef\u00e4rbte Stellen, in unterschiednen Lichtern", "tokens": ["Auf", "tau\u00b7send\u00b7fach", "ge\u00b7f\u00e4rb\u00b7te", "Stel\u00b7len", ",", "in", "un\u00b7ter\u00b7schied\u00b7nen", "Lich\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "flammt,", "tokens": ["flammt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Mich aus dem Buch auf seinen Schmuck. Es schien mich", "tokens": ["Mich", "aus", "dem", "Buch", "auf", "sei\u00b7nen", "Schmuck", ".", "Es", "schien", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "zu sich hin zu winken.", "tokens": ["zu", "sich", "hin", "zu", "win\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APZR", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Dem Wink konnt\u2019 ich nicht widersteh\u2019n,", "tokens": ["Dem", "Wink", "konnt'", "ich", "nicht", "wi\u00b7der\u00b7steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Ich lie\u00df das Buch gem\u00e4hlig sinken,", "tokens": ["Ich", "lie\u00df", "das", "Buch", "ge\u00b7m\u00e4h\u00b7lig", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Um seine Sch\u00f6nheit zu beseh\u2019n.", "tokens": ["Um", "sei\u00b7ne", "Sch\u00f6n\u00b7heit", "zu", "be\u00b7seh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein GOtt! wie vielerley Figuren! welch\u2019 eine Form-", "tokens": ["Mein", "Gott", "!", "wie", "vie\u00b7ler\u00b7ley", "Fi\u00b7gu\u00b7ren", "!", "welch'", "ei\u00b7ne", "For\u00b7m"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PWAV", "PIAT", "NN", "$.", "PWAT", "ART", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "und Farben-Menge", "tokens": ["und", "Fa\u00b7rben\u00b7Men\u00b7ge"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Brach aus den Kr\u00e4utern, Gras und Klee, allein im gr\u00fcnen", "tokens": ["Brach", "aus", "den", "Kr\u00e4u\u00b7tern", ",", "Gras", "und", "Klee", ",", "al\u00b7lein", "im", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "NN", "KON", "NN", "$,", "ADV", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schmuck, hervor!", "tokens": ["Schmuck", ",", "her\u00b7vor", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Es huben die gef\u00e4rbten H\u00e4upter, in einem wirklichen Ge-", "tokens": ["Es", "hu\u00b7ben", "die", "ge\u00b7f\u00e4rb\u00b7ten", "H\u00e4up\u00b7ter", ",", "in", "ei\u00b7nem", "wirk\u00b7li\u00b7chen", "Ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "dr\u00e4nge,", "tokens": ["dr\u00e4n\u00b7ge", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Nebst ihnen auch viel tausend Bluhmen, recht gleichsam in", "tokens": ["Nebst", "ih\u00b7nen", "auch", "viel", "tau\u00b7send", "Bluh\u00b7men", ",", "recht", "gleich\u00b7sam", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "ADV", "CARD", "NN", "$,", "ADV", "ADJD", "APPR"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "die Wett' empor.", "tokens": ["die", "Wett'", "em\u00b7por", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Es schien, ob sucht\u2019 in Kraut und Bluhmen, im gr\u00fcnen und", "tokens": ["Es", "schien", ",", "ob", "sucht'", "in", "Kraut", "und", "Bluh\u00b7men", ",", "im", "gr\u00fc\u00b7nen", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VVFIN", "APPR", "NN", "KON", "NN", "$,", "APPRART", "ADJA", "KON"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "im bunten Schein,", "tokens": ["im", "bun\u00b7ten", "Schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bald das vor diesen, dieses bald vor das, zuerst gesehn zu", "tokens": ["Bald", "das", "vor", "die\u00b7sen", ",", "die\u00b7ses", "bald", "vor", "das", ",", "zu\u00b7erst", "ge\u00b7sehn", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "APPR", "PDAT", "$,", "PRELS", "ADV", "APPR", "PDS", "$,", "ADV", "VVPP", "PTKZU"], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "seyn.", "tokens": ["seyn", "."], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Ich sah\u2019 zuerst den holden Klee in seiner dichten Lieblichkeit,", "tokens": ["Ich", "sah'", "zu\u00b7erst", "den", "hol\u00b7den", "Klee", "in", "sei\u00b7ner", "dich\u00b7ten", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.6": {"text": "Da, vor der Menge, kaum ein Blatt sich aus einander breiten", "tokens": ["Da", ",", "vor", "der", "Men\u00b7ge", ",", "kaum", "ein", "Blatt", "sich", "aus", "ein\u00b7an\u00b7der", "brei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "$,", "ADV", "ART", "NN", "PRF", "APPR", "PRF", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "kann.", "tokens": ["kann", "."], "token_info": ["word", "punct"], "pos": ["VMFIN", "$."], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Die\u00df zeigte mir, von ihrer Zeichnung, unz\u00e4hlige Verschie-", "tokens": ["Die\u00df", "zeig\u00b7te", "mir", ",", "von", "ih\u00b7rer", "Zeich\u00b7nung", ",", "un\u00b7z\u00e4h\u00b7li\u00b7ge", "Ver\u00b7schie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-++-+-+", "measure": "unknown.measure.octa.plus"}, "line.9": {"text": "denheit,", "tokens": ["den\u00b7heit", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Da sie bald Seiten-werts gedr\u00e4ngt, bald offen, bald gefaltet", "tokens": ["Da", "sie", "bald", "Sei\u00b7ten\u00b7werts", "ge\u00b7dr\u00e4ngt", ",", "bald", "of\u00b7fen", ",", "bald", "ge\u00b7fal\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "NE", "VVPP", "$,", "ADV", "ADJD", "$,", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.11": {"text": "sitzen.", "tokens": ["sit\u00b7zen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Durch ihre lieblich- runden Bl\u00e4tter sieht man sich \u00f6fters", "tokens": ["Durch", "ih\u00b7re", "lieb\u00b7lich", "run\u00b7den", "Bl\u00e4t\u00b7ter", "sieht", "man", "sich", "\u00f6f\u00b7ters"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "TRUNC", "ADJA", "NN", "VVFIN", "PIS", "PRF", "ADV"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "kleine Spitzen,", "tokens": ["klei\u00b7ne", "Spit\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "Von zart- und feinem Grase, stechen, das denn im Gegensatz", "tokens": ["Von", "zar\u00b7t", "und", "fei\u00b7nem", "Gra\u00b7se", ",", "ste\u00b7chen", ",", "das", "denn", "im", "Ge\u00b7gen\u00b7satz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN", "$,", "VVFIN", "$,", "PRELS", "ADV", "APPRART", "NN"], "meter": "-+--+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.15": {"text": "die Pracht,", "tokens": ["die", "Pracht", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "Die allen Schmelz weit \u00fcbergeht, um desto angenehmer", "tokens": ["Die", "al\u00b7len", "Schmelz", "weit", "\u00fc\u00b7ber\u00b7geht", ",", "um", "des\u00b7to", "an\u00b7ge\u00b7neh\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "ADJD", "VVFIN", "$,", "KOUI", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.17": {"text": "macht.", "tokens": ["macht", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.18": {"text": "Zumahl, wenn zwischen ihrem Schmuck die liebliche Vergi\u00df", "tokens": ["Zu\u00b7mahl", ",", "wenn", "zwi\u00b7schen", "ih\u00b7rem", "Schmuck", "die", "lieb\u00b7li\u00b7che", "Ver\u00b7gi\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.19": {"text": "mein nicht,", "tokens": ["mein", "nicht", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "PTKNEG", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.20": {"text": "In ihrem Himmel-blauen Glanz, mit Gold geziert, als wie", "tokens": ["In", "ih\u00b7rem", "Him\u00b7mel\u00b7blau\u00b7en", "Glanz", ",", "mit", "Gold", "ge\u00b7ziert", ",", "als", "wie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,", "KOUS", "KOKOM"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.21": {"text": "ein Licht,", "tokens": ["ein", "Licht", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.22": {"text": "Von einem spielenden Sapphir, durch schimmernde Sma-", "tokens": ["Von", "ei\u00b7nem", "spie\u00b7len\u00b7den", "Sap\u00b7phir", ",", "durch", "schim\u00b7mern\u00b7de", "Sma"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "TRUNC"], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "ragden bricht.", "tokens": ["rag\u00b7den", "bricht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Es scheint, man wird von unsrer Mutter, der Erd\u2019, in dieser", "tokens": ["Es", "scheint", ",", "man", "wird", "von", "uns\u00b7rer", "Mut\u00b7ter", ",", "der", "Erd'", ",", "in", "die\u00b7ser"], "token_info": ["word", "word", "punct", "word",