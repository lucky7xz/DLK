{"dta.poem.13399": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Der leidende Erl\u00f6ser.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Gekreuzigter! mein ganzes Wissen", "tokens": ["Ge\u00b7kreu\u00b7zig\u00b7ter", "!", "mein", "gan\u00b7zes", "Wis\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Besteht in deiner Leiden Kraft,", "tokens": ["Be\u00b7steht", "in", "dei\u00b7ner", "Lei\u00b7den", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach! la\u00df doch diese Wissenschaft", "tokens": ["Ach", "!", "la\u00df", "doch", "die\u00b7se", "Wis\u00b7sen\u00b7schaft"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVIMP", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir Leiden, Noth und Tod vers\u00fcssen,", "tokens": ["Mir", "Lei\u00b7den", ",", "Noth", "und", "Tod", "ver\u00b7s\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df, wenn mein Herze sich vergi\u00dft,", "tokens": ["Da\u00df", ",", "wenn", "mein", "Her\u00b7ze", "sich", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPOSAT", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Kreuz denn meine Weisheit ist.", "tokens": ["Dein", "Kreuz", "denn", "mei\u00b7ne", "Weis\u00b7heit", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}