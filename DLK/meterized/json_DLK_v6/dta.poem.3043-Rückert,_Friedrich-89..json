{"dta.poem.3043": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "89.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist in Gottes Rathsversammlung nicht gesessen,", "tokens": ["Du", "bist", "in", "Got\u00b7tes", "Raths\u00b7ver\u00b7samm\u00b7lung", "nicht", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als er den Plan der Welt nach seinem Ma\u00df gemessen;", "tokens": ["Als", "er", "den", "Plan", "der", "Welt", "nach", "sei\u00b7nem", "Ma\u00df", "ge\u00b7mes\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nun thust du doch als sei dir vorgelegt der Plan,", "tokens": ["Nun", "thust", "du", "doch", "als", "sei", "dir", "vor\u00b7ge\u00b7legt", "der", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KOKOM", "VAFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und deinen Ma\u00dfstab legst du unbek\u00fcmmert an.", "tokens": ["Und", "dei\u00b7nen", "Ma\u00df\u00b7stab", "legst", "du", "un\u00b7be\u00b7k\u00fcm\u00b7mert", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nur zu! Es ist darauf der Gro\u00dfplan angelegt,", "tokens": ["Nur", "zu", "!", "Es", "ist", "da\u00b7rauf", "der", "Gro\u00df\u00b7plan", "an\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "PPER", "VAFIN", "PAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df jedes kleinste Ma\u00df pa\u00dft das man angelegt,", "tokens": ["Da\u00df", "je\u00b7des", "kleins\u00b7te", "Ma\u00df", "pa\u00dft", "das", "man", "an\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "VVFIN", "ART", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da\u00df jeder deutet sich die Welt in seinem Sinn,", "tokens": ["Da\u00df", "je\u00b7der", "deu\u00b7tet", "sich", "die", "Welt", "in", "sei\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jeder deutet recht; soviel ist Sinn darinn.", "tokens": ["Und", "je\u00b7der", "deu\u00b7tet", "recht", ";", "so\u00b7viel", "ist", "Sinn", "da\u00b7rinn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADJD", "$.", "PIS", "VAFIN", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}