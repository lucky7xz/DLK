{"textgrid.poem.34759": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Fr\u00e4ulein stand am Meere", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "stand", "am", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und seufzte lang und bang,", "tokens": ["Und", "seufz\u00b7te", "lang", "und", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es r\u00fchrte sie so sehre", "tokens": ["Es", "r\u00fchr\u00b7te", "sie", "so", "seh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Sonnenuntergang.", "tokens": ["Der", "Son\u00b7nen\u00b7un\u00b7ter\u00b7gang", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbmein Fr\u00e4ulein! sein Sie munter,", "tokens": ["\u00bb", "mein", "Fr\u00e4u\u00b7lein", "!", "sein", "Sie", "mun\u00b7ter", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$.", "PPOSAT", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das ist ein altes St\u00fcck;", "tokens": ["Das", "ist", "ein", "al\u00b7tes", "St\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hier vorne geht sie unter", "tokens": ["Hier", "vor\u00b7ne", "geht", "sie", "un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und kehrt von hinten zur\u00fcck.\u00ab", "tokens": ["Und", "kehrt", "von", "hin\u00b7ten", "zu\u00b7r\u00fcck", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}