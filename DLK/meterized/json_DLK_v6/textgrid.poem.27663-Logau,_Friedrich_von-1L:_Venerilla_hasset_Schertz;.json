{"textgrid.poem.27663": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Venerilla hasset Schertz;", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Venerilla hasset Schertz;", "tokens": ["Ve\u00b7ne\u00b7ril\u00b7la", "has\u00b7set", "Schertz", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was sie meint, das ist ihr Hertz.", "tokens": ["Was", "sie", "meint", ",", "das", "ist", "ihr", "Hertz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer an ihr was suchen wil,", "tokens": ["Wer", "an", "ihr", "was", "su\u00b7chen", "wil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Such und s\u00e4ume nicht zu viel.", "tokens": ["Such", "und", "s\u00e4u\u00b7me", "nicht", "zu", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PTKNEG", "PTKA", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der nichts sagt und viel doch thut,", "tokens": ["Der", "nichts", "sagt", "und", "viel", "doch", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "KON", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist f\u00fcr Venerilla gut.", "tokens": ["Ist", "f\u00fcr", "Ve\u00b7ne\u00b7ril\u00b7la", "gut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NE", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}