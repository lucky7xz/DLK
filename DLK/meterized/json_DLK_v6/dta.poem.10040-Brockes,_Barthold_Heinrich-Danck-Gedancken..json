{"dta.poem.10040": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Danck-Gedancken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein GOTT! die neu-bebl\u00fchmte Welt,\n(In deren wunderbaren Pracht", "tokens": ["Mein", "GoTT", "!", "die", "neu\u00b7be\u00b7bl\u00fchm\u00b7te", "Welt", ",", "(", "In", "de\u00b7ren", "wun\u00b7der\u00b7ba\u00b7ren", "Pracht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "ART", "ADJA", "NN", "$,", "$(", "APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Du uns die Gr\u00f6sse Deiner Macht,", "tokens": ["Du", "uns", "die", "Gr\u00f6s\u00b7se", "Dei\u00b7ner", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Deiner Liebe, vorgestellt,)", "tokens": ["Und", "Dei\u00b7ner", "Lie\u00b7be", ",", "vor\u00b7ge\u00b7stellt", ",", ")"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "VVPP", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erregt mein wallendes Gebl\u00fcte.", "tokens": ["Er\u00b7regt", "mein", "wal\u00b7len\u00b7des", "Ge\u00b7bl\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des Himmels rein- und heitres Licht", "tokens": ["Des", "Him\u00b7mels", "rein", "und", "heit\u00b7res", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erheitert mein vergn\u00fcgt Gesicht,", "tokens": ["Er\u00b7hei\u00b7tert", "mein", "ver\u00b7gn\u00fcgt", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Belebt mein schl\u00e4ffriges Gem\u00fcthe.", "tokens": ["Be\u00b7lebt", "mein", "schl\u00e4f\u00b7fri\u00b7ges", "Ge\u00b7m\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da diese holde Fr\u00fchlings-Zeit", "tokens": ["Da", "die\u00b7se", "hol\u00b7de", "Fr\u00fch\u00b7lings\u00b7Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Gras und Kr\u00e4utern unsre Felder,", "tokens": ["Mit", "Gras", "und", "Kr\u00e4u\u00b7tern", "uns\u00b7re", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Fr\u00fccht\u2019 und Bl\u00e4ttern B\u00e4um\u2019 und W\u00e4lder,", "tokens": ["Mit", "Fr\u00fccht'", "und", "Bl\u00e4t\u00b7tern", "B\u00e4um'", "und", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Thier\u2019 und Menschen Blut mit reger Fruchtbarkeit", "tokens": ["Der", "Thier'", "und", "Men\u00b7schen", "Blut", "mit", "re\u00b7ger", "Frucht\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So schm\u00fcckt, als f\u00fcllt; sollt\u2019 unsern Seelen", "tokens": ["So", "schm\u00fcckt", ",", "als", "f\u00fcllt", ";", "sollt'", "un\u00b7sern", "See\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "VVFIN", "$.", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es denn allein am Trieb\u2019, auch sich zu schm\u00fccken, fehlen?", "tokens": ["Es", "denn", "al\u00b7lein", "am", "Trieb'", ",", "auch", "sich", "zu", "schm\u00fc\u00b7cken", ",", "feh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPRART", "NN", "$,", "ADV", "PRF", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und sollten sie allein auf Erden", "tokens": ["Und", "soll\u00b7ten", "sie", "al\u00b7lein", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Unfruchtbar seyn? Geliebte Menschen, nein!", "tokens": ["Un\u00b7frucht\u00b7bar", "seyn", "?", "Ge\u00b7lieb\u00b7te", "Men\u00b7schen", ",", "nein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VAINF", "$.", "NN", "NN", "$,", "PTKANT", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Lasst uns zu dieser Zeit nicht minder", "tokens": ["Lasst", "uns", "zu", "die\u00b7ser", "Zeit", "nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "PTKNEG", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie, sch\u00f6n zu schm\u00fccken, nicht allein,", "tokens": ["Sie", ",", "sch\u00f6n", "zu", "schm\u00fc\u00b7cken", ",", "nicht", "al\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "PTKZU", "VVINF", "$,", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch zu vermehren, sorgsam werden!", "tokens": ["Auch", "zu", "ver\u00b7meh\u00b7ren", ",", "sorg\u00b7sam", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gedancken sind der Seelen Kinder:", "tokens": ["Ge\u00b7dan\u00b7cken", "sind", "der", "See\u00b7len", "Kin\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So lasst die Seelen sich bem\u00fchn", "tokens": ["So", "lasst", "die", "See\u00b7len", "sich", "be\u00b7m\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch sch\u00f6ne Kinderchen zu ziehn.", "tokens": ["Auch", "sch\u00f6\u00b7ne", "Kin\u00b7der\u00b7chen", "zu", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nun r\u00e4umet mir vermuthlich ieder ein:", "tokens": ["Nun", "r\u00e4u\u00b7met", "mir", "ver\u00b7muth\u00b7lich", "ie\u00b7der", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df Danck-Gedancken GOTT die liebsten Kinder seyn.", "tokens": ["Da\u00df", "Dan\u00b7ck\u00b7Ge\u00b7dan\u00b7cken", "GoTT", "die", "liebs\u00b7ten", "Kin\u00b7der", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NE", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}