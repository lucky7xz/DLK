{"textgrid.poem.66182": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1. An Hahn bei seiner Pfarrver\u00e4nderung 1781", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mann, vor dem sich Gott enth\u00fcllte,", "tokens": ["Mann", ",", "vor", "dem", "sich", "Gott", "ent\u00b7h\u00fcll\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PRELS", "PRF", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als er dich mit Licht erf\u00fcllte,", "tokens": ["Als", "er", "dich", "mit", "Licht", "er\u00b7f\u00fcll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und an Christus statt geschickt;", "tokens": ["Und", "an", "Chris\u00b7tus", "statt", "ge\u00b7schickt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "ADJD", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hahn, der mit der Lichtgeberde", "tokens": ["Hahn", ",", "der", "mit", "der", "Licht\u00b7ge\u00b7ber\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In die Todesnacht der Erde", "tokens": ["In", "die", "To\u00b7des\u00b7nacht", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie ein Stern vom Himmel blickt.", "tokens": ["Wie", "ein", "Stern", "vom", "Him\u00b7mel", "blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00f6r' aus seines Tempels Hallen", "tokens": ["H\u00f6r'", "aus", "sei\u00b7nes", "Tem\u00b7pels", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jesu Christi Stimme schallen:", "tokens": ["Je\u00b7su", "Chris\u00b7ti", "Stim\u00b7me", "schal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeuge meiner Herrlichkeit,", "tokens": ["Zeu\u00b7ge", "mei\u00b7ner", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Komm und lasse dein Kornwesten;", "tokens": ["Komm", "und", "las\u00b7se", "dein", "Korn\u00b7wes\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sag es auch zu andern G\u00e4sten:", "tokens": ["Sag", "es", "auch", "zu", "an\u00b7dern", "G\u00e4s\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kommt, denn alles ist bereit!", "tokens": ["Kommt", ",", "denn", "al\u00b7les", "ist", "be\u00b7reit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "PIS", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, nun dr\u00e4ngt sich die Gemeinde", "tokens": ["Ach", ",", "nun", "dr\u00e4ngt", "sich", "die", "Ge\u00b7mein\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hin zum Lehrer, hin zum Freunde,", "tokens": ["Hin", "zum", "Leh\u00b7rer", ",", "hin", "zum", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Stirnen w\u00f6lken sich.", "tokens": ["Al\u00b7le", "Stir\u00b7nen", "w\u00f6l\u00b7ken", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00e4nner, Weiber, Knaben, Kinder,", "tokens": ["M\u00e4n\u00b7ner", ",", "Wei\u00b7ber", ",", "Kna\u00b7ben", ",", "Kin\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Christo zugef\u00fchrte S\u00fcnder", "tokens": ["Chris\u00b7to", "zu\u00b7ge\u00b7f\u00fchr\u00b7te", "S\u00fcn\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weinen laut und segnen dich.", "tokens": ["Wei\u00b7nen", "laut", "und", "seg\u00b7nen", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie die Aeltesten der Christen", "tokens": ["Wie", "die", "A\u00b7el\u00b7tes\u00b7ten", "der", "Chris\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihren Paulus weinend k\u00fc\u00dften", "tokens": ["Ih\u00b7ren", "Pau\u00b7lus", "wei\u00b7nend", "k\u00fc\u00df\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von ihm gen Himmel sah'n,", "tokens": ["Und", "von", "ihm", "gen", "Him\u00b7mel", "sah'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach mit so benetzten Wangen", "tokens": ["Ach", "mit", "so", "be\u00b7netz\u00b7ten", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Seh' ich dir am Halse hangen", "tokens": ["Seh'", "ich", "dir", "am", "Hal\u00b7se", "han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deine Sch\u00e4fchen, lieber Hahn.", "tokens": ["Dei\u00b7ne", "Sch\u00e4f\u00b7chen", ",", "lie\u00b7ber", "Hahn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hahn, du hast uns nichts verhalten,", "tokens": ["Hahn", ",", "du", "hast", "uns", "nichts", "ver\u00b7hal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprechen Junge mit den Alten,", "tokens": ["Spre\u00b7chen", "Jun\u00b7ge", "mit", "den", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine Wahrheit, keine Pflicht;", "tokens": ["Kei\u00b7ne", "Wahr\u00b7heit", ",", "kei\u00b7ne", "Pflicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Warst mit Demuth und mit Z\u00e4hren", "tokens": ["Warst", "mit", "De\u00b7muth", "und", "mit", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Uns ein Vorbild deiner Lehren,", "tokens": ["Uns", "ein", "Vor\u00b7bild", "dei\u00b7ner", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hattest Licht und strahltest Licht.", "tokens": ["Hat\u00b7test", "Licht", "und", "strahl\u00b7test", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch wir danken Gott und schweigen,", "tokens": ["Doch", "wir", "dan\u00b7ken", "Gott", "und", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einsame Gebete steigen", "tokens": ["Ein\u00b7sa\u00b7me", "Ge\u00b7be\u00b7te", "stei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf zum Himmel, der sie h\u00f6rt;", "tokens": ["Auf", "zum", "Him\u00b7mel", ",", "der", "sie", "h\u00f6rt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht die Menschen zu erheben,", "tokens": ["Nicht", "die", "Men\u00b7schen", "zu", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gott die Ehr' allein zu geben,", "tokens": ["Gott", "die", "Ehr'", "al\u00b7lein", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat dein Beispiel uns gelehrt.", "tokens": ["Hat", "dein", "Bei\u00b7spiel", "uns", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Gott, der Herrscher \u00fcber Welten,", "tokens": ["Gott", ",", "der", "Herr\u00b7scher", "\u00fc\u00b7ber", "Wel\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Woll' es ewig dir vergelten,", "tokens": ["Woll'", "es", "e\u00b7wig", "dir", "ver\u00b7gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was du Gutes an uns thatst.", "tokens": ["Was", "du", "Gu\u00b7tes", "an", "uns", "thatst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jede F\u00fclle, jeden Segen", "tokens": ["Je\u00b7de", "F\u00fcl\u00b7le", ",", "je\u00b7den", "Se\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Woll' er zwiefach auf dich legen,", "tokens": ["Woll'", "er", "zwie\u00b7fach", "auf", "dich", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Den du uns von Gott erbatst.", "tokens": ["Den", "du", "uns", "von", "Gott", "er\u00b7batst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Droben in des Himmels H\u00f6he,", "tokens": ["Dro\u00b7ben", "in", "des", "Him\u00b7mels", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor dem Throne Christus stehe", "tokens": ["Vor", "dem", "Thro\u00b7ne", "Chris\u00b7tus", "ste\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dein Kornwesten um dich her.", "tokens": ["Dein", "Korn\u00b7wes\u00b7ten", "um", "dich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jeder f\u00fchle neues Leben,", "tokens": ["Je\u00b7der", "f\u00fch\u00b7le", "neu\u00b7es", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jeder, den dir Gott gegeben,", "tokens": ["Je\u00b7der", ",", "den", "dir", "Gott", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stehe am krystallnen Meer.", "tokens": ["Ste\u00b7he", "am", "krys\u00b7tall\u00b7nen", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Alle rufen: Preis dem Lamme,", "tokens": ["Al\u00b7le", "ru\u00b7fen", ":", "Preis", "dem", "Lam\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$.", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein erw\u00fcrgten Gotteslamme!", "tokens": ["Dein", "er\u00b7w\u00fcrg\u00b7ten", "Got\u00b7tes\u00b7lam\u00b7me", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Br\u00fcder, Schwestern betet an!", "tokens": ["Br\u00fc\u00b7der", ",", "Schwes\u00b7tern", "be\u00b7tet", "an", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jesus Christus der Gerechte", "tokens": ["Je\u00b7sus", "Chris\u00b7tus", "der", "Ge\u00b7rech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Lohn' es seinem treuen Knechte,", "tokens": ["Lohn'", "es", "sei\u00b7nem", "treu\u00b7en", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unserm frommen Lehrer Hahn!", "tokens": ["Un\u00b7serm", "from\u00b7men", "Leh\u00b7rer", "Hahn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Welch ein Bild vom k\u00fcnft'gen Lohne!", "tokens": ["Welch", "ein", "Bild", "vom", "k\u00fcnft'\u00b7gen", "Loh\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh, ein Blitz von deiner Krone", "tokens": ["Sieh", ",", "ein", "Blitz", "von", "dei\u00b7ner", "Kro\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeigt sich schon in dieser Welt.", "tokens": ["Zeigt", "sich", "schon", "in", "die\u00b7ser", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ha, zu Myriaden malen", "tokens": ["Ha", ",", "zu", "My\u00b7ria\u00b7den", "ma\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"tex