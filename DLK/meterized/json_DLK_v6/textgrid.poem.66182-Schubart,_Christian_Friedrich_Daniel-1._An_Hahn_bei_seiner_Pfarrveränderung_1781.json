d", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jeder Treu und Hirtenm\u00fch'.", "tokens": ["Je\u00b7der", "Treu", "und", "Hir\u00b7ten\u00b7m\u00fch'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Engel, die Befehle bringen,", "tokens": ["En\u00b7gel", ",", "die", "Be\u00b7feh\u00b7le", "brin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rufen dich nach Echterdingen.", "tokens": ["Ru\u00b7fen", "dich", "nach", "Ech\u00b7ter\u00b7din\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf, gehorche dem Befehl!", "tokens": ["Auf", ",", "ge\u00b7hor\u00b7che", "dem", "Be\u00b7fehl", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn auch Welten, wie Satane", "tokens": ["Wenn", "auch", "Wel\u00b7ten", ",", "wie", "Sa\u00b7ta\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADV", "NN", "$,", "PWAV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Widerst\u00e4nden Gottes Plane \u2013", "tokens": ["Wi\u00b7der\u00b7st\u00e4n\u00b7den", "Got\u00b7tes", "Pla\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Geh', hier ist Immanuel.", "tokens": ["Geh'", ",", "hier", "ist", "Im\u00b7ma\u00b7nu\u00b7el", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VAFIN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Steure in des Geistes R\u00fcstung", "tokens": ["Steu\u00b7re", "in", "des", "Geis\u00b7tes", "R\u00fcs\u00b7tung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der satanischen Verw\u00fcstung,", "tokens": ["Der", "sa\u00b7ta\u00b7ni\u00b7schen", "Ver\u00b7w\u00fcs\u00b7tung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die der Kirche Christi droht!", "tokens": ["Die", "der", "Kir\u00b7che", "Chris\u00b7ti", "droht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df der Satan nicht verderbe,", "tokens": ["Da\u00df", "der", "Sa\u00b7tan", "nicht", "ver\u00b7der\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zeig' ihm bald sein gro\u00dfes Erbe,", "tokens": ["Zeig'", "ihm", "bald", "sein", "gro\u00b7\u00dfes", "Er\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bald den zweiten Feuertod.", "tokens": ["Bald", "den", "zwei\u00b7ten", "Feu\u00b7er\u00b7tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Schreibe fort mit Christenmuthe,", "tokens": ["Schrei\u00b7be", "fort", "mit", "Chris\u00b7ten\u00b7mu\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mach' die Feder mit dem Blute", "tokens": ["Mach'", "die", "Fe\u00b7der", "mit", "dem", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Des erw\u00fcrgten Lammes roth.", "tokens": ["Des", "er\u00b7w\u00fcrg\u00b7ten", "Lam\u00b7mes", "roth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Christi F\u00fc\u00dfe, stark wie Messing,", "tokens": ["Chris\u00b7ti", "F\u00fc\u00b7\u00dfe", ",", "stark", "wie", "Mes\u00b7sing", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "KOKOM", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Treten mehr als einen Lessing,", "tokens": ["Tre\u00b7ten", "mehr", "als", "ei\u00b7nen", "Les\u00b7sing", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Treten Teufel selbst in Koth.", "tokens": ["Tre\u00b7ten", "Teu\u00b7fel", "selbst", "in", "Koth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Ring' und k\u00e4mpf' f\u00fcr deine Br\u00fcder \u2013", "tokens": ["Ring'", "und", "k\u00e4mpf'", "f\u00fcr", "dei\u00b7ne", "Br\u00fc\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bete, ach, f\u00fcr Christus Glieder \u2013", "tokens": ["Be\u00b7te", ",", "ach", ",", "f\u00fcr", "Chris\u00b7tus", "Glie\u00b7der", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ITJ", "$,", "APPR", "NE", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ring' und bete auch f\u00fcr mich!", "tokens": ["Ring'", "und", "be\u00b7te", "auch", "f\u00fcr", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dorten blicke in das Ganze,", "tokens": ["Dor\u00b7ten", "bli\u00b7cke", "in", "das", "Gan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dorten im enth\u00fcllten Glanze,", "tokens": ["Dor\u00b7ten", "im", "ent\u00b7h\u00fcll\u00b7ten", "Glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Treuer Lehrer, seh' ich dich.", "tokens": ["Treu\u00b7er", "Leh\u00b7rer", ",", "seh'", "ich", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Bruder, g\u00f6nne mir den Namen,", "tokens": ["Bru\u00b7der", ",", "g\u00f6n\u00b7ne", "mir", "den", "Na\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, den s\u00fc\u00dfen Brudernamen!", "tokens": ["Ach", ",", "den", "s\u00fc\u00b7\u00dfen", "Bru\u00b7der\u00b7na\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einst im Reiche Jesus soll", "tokens": ["Einst", "im", "Rei\u00b7che", "Je\u00b7sus", "soll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NE", "NE", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich mein Geistleib mit Entz\u00fccken", "tokens": ["Dich", "mein", "Geist\u00b7leib", "mit", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "An das Herz voll Liebe dr\u00fccken,", "tokens": ["An", "das", "Herz", "voll", "Lie\u00b7be", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ganz von Christus Liebe voll.", "tokens": ["Ganz", "von", "Chris\u00b7tus", "Lie\u00b7be", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}