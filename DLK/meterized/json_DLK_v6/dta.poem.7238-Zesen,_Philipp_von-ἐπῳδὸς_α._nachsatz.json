{"dta.poem.7238": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "\u1f10\u03c0\u1ff3\u03b4\u1f78\u03c2 \u03b1.  nachsatz", "genre": "Lyrik", "period": "N.A.", "pub_year": "1640", "urn": "urn:nbn:de:kobv:b4-20685-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Todt/ wie hastu mir ein solches Leid gegeben/", "tokens": ["O", "Todt", "/", "wie", "has\u00b7tu", "mir", "ein", "sol\u00b7ches", "Leid", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "KOKOM", "VAFIN", "PPER", "ART", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das mir schaden thut", "tokens": ["Das", "mir", "scha\u00b7den", "thut"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PPER", "VVINF", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Vndbenimbt den Muth?", "tokens": ["Vnd\u00b7be\u00b7nimbt", "den", "Muth", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ja das mir naget ab allhier mein gantzes leben", "tokens": ["Ja", "das", "mir", "na\u00b7get", "ab", "all\u00b7hier", "mein", "gant\u00b7zes", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PRELS", "PPER", "VVFIN", "PTKVZ", "ADV", "PPOSAT", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was hilffts! der Todt nimbt hin/ durch seinen", "tokens": ["Was", "hilffts", "!", "der", "Todt", "nimbt", "hin", "/", "durch", "sei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$(", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "scharfen Zahn", "tokens": ["schar\u00b7fen", "Zahn"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Arm vnd Reiche/", "tokens": ["Arm", "vnd", "Rei\u00b7che", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "So Jhm Gleiche/", "tokens": ["So", "Jhm", "Glei\u00b7che", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Er nimmet sie zu sich auf seine bahn.", "tokens": ["Er", "nim\u00b7met", "sie", "zu", "sich", "auf", "sei\u00b7ne", "bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}