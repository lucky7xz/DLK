{"textgrid.poem.39127": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wem je im Grimm, wem je im Groll", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wem je im Grimm, wem je im Groll", "tokens": ["Wem", "je", "im", "Grimm", ",", "wem", "je", "im", "Groll"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPRART", "NE", "$,", "PWS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die blaue Stirnenader schwoll,", "tokens": ["Die", "blau\u00b7e", "Stir\u00b7nen\u00b7a\u00b7der", "schwoll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wem je das Aug' in Wut geflammt,", "tokens": ["Wem", "je", "das", "Aug'", "in", "Wut", "ge\u00b7flammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wem je den Arm der Mut gestrammt;", "tokens": ["Wem", "je", "den", "Arm", "der", "Mut", "ge\u00b7strammt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer je ein Schwert mit H\u00e4nden griff,", "tokens": ["Wer", "je", "ein", "Schwert", "mit", "H\u00e4n\u00b7den", "griff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wem je ein Schwert im Hiebe pfiff,", "tokens": ["Wem", "je", "ein", "Schwert", "im", "Hie\u00b7be", "pfiff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer je der Klinge fest und traut", "tokens": ["Wer", "je", "der", "Klin\u00b7ge", "fest", "und", "traut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "NN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ins zornige blaue Aug' geschaut:", "tokens": ["Ins", "zor\u00b7ni\u00b7ge", "blau\u00b7e", "Aug'", "ge\u00b7schaut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Der nimmt den Streich und r\u00e4cht ihn gleich,", "tokens": ["Der", "nimmt", "den", "Streich", "und", "r\u00e4cht", "ihn", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und g\u00e4lt' es Erd' und Himmelreich;", "tokens": ["Und", "g\u00e4lt'", "es", "Erd'", "und", "Him\u00b7mel\u00b7reich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr scharfes Wort den scharfen Stahl,", "tokens": ["F\u00fcr", "schar\u00b7fes", "Wort", "den", "schar\u00b7fen", "Stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und g\u00e4lt' es Fluch und H\u00f6llenqual.", "tokens": ["Und", "g\u00e4lt'", "es", "Fluch", "und", "H\u00f6l\u00b7len\u00b7qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}