{"textgrid.poem.41395": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der gro\u00dfm\u00fcthige Herr und seine Sklaven", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf dem Aeg\u00e4ermeer wird einst ein Handelsmann", "tokens": ["Auf", "dem", "A\u00b7e\u00b7g\u00e4er\u00b7meer", "wird", "einst", "ein", "Han\u00b7dels\u00b7mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "ADV", "ART", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Von einem schnellen Sturm ergriffen.", "tokens": ["Von", "ei\u00b7nem", "schnel\u00b7len", "Sturm", "er\u00b7grif\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er wendet sich, so gut er kann,", "tokens": ["Er", "wen\u00b7det", "sich", ",", "so", "gut", "er", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "ADV", "ADJD", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und darf nur langsam seitw\u00e4rts schiffen.", "tokens": ["Und", "darf", "nur", "lang\u00b7sam", "seit\u00b7w\u00e4rts", "schif\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Allein es mehret sich die Noth,", "tokens": ["Al\u00b7lein", "es", "meh\u00b7ret", "sich", "die", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er und die meisten Sklaven klagen;", "tokens": ["Er", "und", "die", "meis\u00b7ten", "Skla\u00b7ven", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die alten hoffen auf den Tod,", "tokens": ["Die", "al\u00b7ten", "hof\u00b7fen", "auf", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die jungen melden sich, die Rettung noch zu wagen;", "tokens": ["Die", "jun\u00b7gen", "mel\u00b7den", "sich", ",", "die", "Ret\u00b7tung", "noch", "zu", "wa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nur halten sie daf\u00fcr um ihre Freiheit an,", "tokens": ["Nur", "hal\u00b7ten", "sie", "da\u00b7f\u00fcr", "um", "ih\u00b7re", "Frei\u00b7heit", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Doch die wird allen abgeschlagen.", "tokens": ["Doch", "die", "wird", "al\u00b7len", "ab\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bald aber rei\u00dft der Sturm Mast, Stamm und Segel nieder.", "tokens": ["Bald", "a\u00b7ber", "rei\u00dft", "der", "Sturm", "Mast", ",", "Stamm", "und", "Se\u00b7gel", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "NN", "$,", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ruft er: Freunde, fasset Muth!", "tokens": ["Da", "ruft", "er", ":", "Freun\u00b7de", ",", "fas\u00b7set", "Muth", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NN", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir sinken; doch ich bin euch gut;", "tokens": ["Wir", "sin\u00b7ken", ";", "doch", "ich", "bin", "euch", "gut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich geb' euch jetzt die Freiheit wieder.", "tokens": ["Ich", "geb'", "euch", "jetzt", "die", "Frei\u00b7heit", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie kriechend \u00e4u\u00dfert sich gemeiner Seelen G\u00fcte!", "tokens": ["Wie", "krie\u00b7chend", "\u00e4u\u00b7\u00dfert", "sich", "ge\u00b7mei\u00b7ner", "See\u00b7len", "G\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PRF", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer karg ist, bleibt's bis in den Tod,", "tokens": ["Wer", "karg", "ist", ",", "bleibt's", "bis", "in", "den", "Tod", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In jedem Stand, im Gl\u00fcck, in Noth,", "tokens": ["In", "je\u00b7dem", "Stand", ",", "im", "Gl\u00fcck", ",", "in", "Noth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPRART", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nichts erh\u00f6het sein Gem\u00fcthe.", "tokens": ["Und", "nichts", "er\u00b7h\u00f6\u00b7het", "sein", "Ge\u00b7m\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}