{"dta.poem.18888": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Uebergang von Tropen  \n  zu Gleichnissen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Morgend\u00e4mmerung wandte sich in\u2019s Helle,", "tokens": ["Mor\u00b7gen\u00b7d\u00e4m\u00b7me\u00b7rung", "wand\u00b7te", "sich", "in's", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Herz und Geist auf einmal wurden froh,", "tokens": ["Herz", "und", "Geist", "auf", "ein\u00b7mal", "wur\u00b7den", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ADV", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Als die Nacht, die sch\u00fcchterne Gazelle,", "tokens": ["Als", "die", "Nacht", ",", "die", "sch\u00fcch\u00b7ter\u00b7ne", "Ga\u00b7zel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Vor dem Dr\u00e4un des Morgenl\u00f6wens floh.", "tokens": ["Vor", "dem", "Dr\u00e4un", "des", "Mor\u00b7gen\u00b7l\u00f6\u00b7wens", "floh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}