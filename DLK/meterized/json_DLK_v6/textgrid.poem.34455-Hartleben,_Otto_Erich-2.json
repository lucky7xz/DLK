{"textgrid.poem.34455": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leise, ganz leise vor Scham erbebte die wonnige Kleine,", "tokens": ["Lei\u00b7se", ",", "ganz", "lei\u00b7se", "vor", "Scham", "er\u00b7beb\u00b7te", "die", "won\u00b7ni\u00b7ge", "Klei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "APPR", "NN", "VVFIN", "ART", "ADJA", "ADJA", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "als ich sie traf mit dem Kuss, den sie mir lange verwehrt,", "tokens": ["als", "ich", "sie", "traf", "mit", "dem", "Kuss", ",", "den", "sie", "mir", "lan\u00b7ge", "ver\u00b7wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "doch dem beredten Gelispel des lautlos fl\u00fcsternden Mundes", "tokens": ["doch", "dem", "be\u00b7red\u00b7ten", "Ge\u00b7lis\u00b7pel", "des", "laut\u00b7los", "fl\u00fcs\u00b7tern\u00b7den", "Mun\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "gab sie allm\u00e4hlich sich hin, zuckend, im Tiefsten besiegt.", "tokens": ["gab", "sie", "all\u00b7m\u00e4h\u00b7lich", "sich", "hin", ",", "zu\u00b7ckend", ",", "im", "Tiefs\u00b7ten", "be\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PRF", "PTKVZ", "$,", "VVPP", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}}}}