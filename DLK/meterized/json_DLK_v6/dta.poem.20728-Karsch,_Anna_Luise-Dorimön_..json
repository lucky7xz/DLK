{"dta.poem.20728": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Dorim\u00f6n .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du Wonne meiner jungen Tage,                 ", "tokens": ["Du", "Won\u00b7ne", "mei\u00b7ner", "jun\u00b7gen", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du Leben meines Lebens, sage,", "tokens": ["Du", "Le\u00b7ben", "mei\u00b7nes", "Le\u00b7bens", ",", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie diese H\u00fctte dir gef\u00e4llt?", "tokens": ["Wie", "die\u00b7se", "H\u00fct\u00b7te", "dir", "ge\u00b7f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie einer von den Erdg\u00f6ttinnen", "tokens": ["Wie", "ei\u00b7ner", "von", "den", "Erd\u00b7g\u00f6t\u00b7tin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der allerh\u00f6chste Thron der Welt!", "tokens": ["Der", "al\u00b7ler\u00b7h\u00f6chs\u00b7te", "Thron", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein Vater wohnete darinnen", "tokens": ["Mein", "Va\u00b7ter", "woh\u00b7ne\u00b7te", "da\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Viel sch\u00f6ne Sommer lang, und fand", "tokens": ["Viel", "sch\u00f6\u00b7ne", "Som\u00b7mer", "lang", ",", "und", "fand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "ADJD", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vergn\u00fcgen dran mit eigner Hand", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", "dran", "mit", "eig\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die zarten B\u00e4ume zu begie\u00dfen,", "tokens": ["Die", "zar\u00b7ten", "B\u00e4u\u00b7me", "zu", "be\u00b7gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die dazumal von mir sich noch umspannen lie\u00dfen,", "tokens": ["Die", "da\u00b7zu\u00b7mal", "von", "mir", "sich", "noch", "um\u00b7span\u00b7nen", "lie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "PRF", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Und nun so hoch empor gestrebt;", "tokens": ["Und", "nun", "so", "hoch", "em\u00b7por", "ge\u00b7strebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hier hat mein Vater froh gelebt,", "tokens": ["Hier", "hat", "mein", "Va\u00b7ter", "froh", "ge\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie in dem seligen Gefilde", "tokens": ["Wie", "in", "dem", "se\u00b7li\u00b7gen", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der erste Mensch mit seiner Braut.", "tokens": ["Der", "ers\u00b7te", "Mensch", "mit", "sei\u00b7ner", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "O du nach eines Engels Bilde", "tokens": ["O", "du", "nach", "ei\u00b7nes", "En\u00b7gels", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "F\u00fcr mich so liebenswerth gebaut,", "tokens": ["F\u00fcr", "mich", "so", "lie\u00b7bens\u00b7werth", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Hier will ich leben dir zur Seite", "tokens": ["Hier", "will", "ich", "le\u00b7ben", "dir", "zur", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "So gl\u00fccklich wie der erste Mann.", "tokens": ["So", "gl\u00fcck\u00b7lich", "wie", "der", "ers\u00b7te", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hier geb ich dir durch Blumen das Geleite", "tokens": ["Hier", "geb", "ich", "dir", "durch", "Blu\u00b7men", "das", "Ge\u00b7lei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom kunstgepflanzten Garten an", "tokens": ["Vom", "kunst\u00b7ge\u00b7pflanz\u00b7ten", "Gar\u00b7ten", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis in die wilden Rosen-Hecken.", "tokens": ["Bis", "in", "die", "wil\u00b7den", "Ro\u00b7sen\u00b7He\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Laube gr\u00fcnes Dach soll dich und mich", "tokens": ["Der", "Lau\u00b7be", "gr\u00fc\u00b7nes", "Dach", "soll", "dich", "und", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VMFIN", "PPER", "KON", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "verstecken", "tokens": ["ver\u00b7ste\u00b7cken"], "token_info": ["word"], "pos": ["VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "So oft der Mittag gl\u00fcht; hier will ich Rosenduft", "tokens": ["So", "oft", "der", "Mit\u00b7tag", "gl\u00fcht", ";", "hier", "will", "ich", "Ro\u00b7sen\u00b7duft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$.", "ADV", "VMFIN", "PPER", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In langen Z\u00fcgen geitzig trinken,", "tokens": ["In", "lan\u00b7gen", "Z\u00fc\u00b7gen", "geit\u00b7zig", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und wann aus ungepaarten Finken", "tokens": ["Und", "wann", "aus", "un\u00b7ge\u00b7paar\u00b7ten", "Fin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die bange Liebe lockend ruft,", "tokens": ["Die", "ban\u00b7ge", "Lie\u00b7be", "lo\u00b7ckend", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wann die Nachtigallen klagen,", "tokens": ["Und", "wann", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df Fels und H\u00fcgel Antwort giebt,", "tokens": ["Da\u00df", "Fels", "und", "H\u00fc\u00b7gel", "Ant\u00b7wort", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dann will ich im Entz\u00fccken sagen:", "tokens": ["Dann", "will", "ich", "im", "Ent\u00b7z\u00fc\u00b7cken", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ich bin geliebt!", "tokens": ["Ich", "bin", "ge\u00b7liebt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Und ich will mich von deinem Busen stehlen", "tokens": ["Und", "ich", "will", "mich", "von", "dei\u00b7nem", "Bu\u00b7sen", "steh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PRF", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Morgens, wenn aus Lerchen-Kehlen", "tokens": ["Des", "Mor\u00b7gens", ",", "wenn", "aus", "Ler\u00b7chen\u00b7Keh\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das erste Lied gen Himmel t\u00f6nt;", "tokens": ["Das", "ers\u00b7te", "Lied", "gen", "Him\u00b7mel", "t\u00f6nt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich will die sch\u00f6nsten Blumen pfl\u00fccken", "tokens": ["Ich", "will", "die", "sch\u00f6ns\u00b7ten", "Blu\u00b7men", "pfl\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den kleinen Altar auszuschm\u00fccken,", "tokens": ["Den", "klei\u00b7nen", "Al\u00b7tar", "aus\u00b7zu\u00b7schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den deine Mutter oft gekr\u00f6nt", "tokens": ["Den", "dei\u00b7ne", "Mut\u00b7ter", "oft", "ge\u00b7kr\u00f6nt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit Rosen und mit Reben-Ranken;", "tokens": ["Mit", "Ro\u00b7sen", "und", "mit", "Re\u00b7ben\u00b7Ran\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dann wecket dich mein sanfter Ku\u00df,", "tokens": ["Dann", "we\u00b7cket", "dich", "mein", "sanf\u00b7ter", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dann folgst du meinem Wink und kniest mit mir", "tokens": ["Dann", "folgst", "du", "mei\u00b7nem", "Wink", "und", "kniest", "mit", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "am Fu\u00df", "tokens": ["am", "Fu\u00df"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Des Opfer-Heerdes, dem zu danken,", "tokens": ["Des", "Op\u00b7fer\u00b7He\u00b7er\u00b7des", ",", "dem", "zu", "dan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Der alle Wesen kommen hie\u00df,", "tokens": ["Der", "al\u00b7le", "We\u00b7sen", "kom\u00b7men", "hie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und \u00fcber unsern H\u00e4uptern Sonnen", "tokens": ["Und", "\u00fc\u00b7ber", "un\u00b7sern", "H\u00e4up\u00b7tern", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und um uns her die Flur entstehen lie\u00df,", "tokens": ["Und", "um", "uns", "her", "die", "Flur", "ent\u00b7ste\u00b7hen", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PRF", "APZR", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und dich erschuf, den ich so z\u00e4rtlich lieb gewonnen,", "tokens": ["Und", "dich", "er\u00b7schuf", ",", "den", "ich", "so", "z\u00e4rt\u00b7lich", "lieb", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PRELS", "PPER", "ADV", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dich meines Herzens s\u00fc\u00dfen Freund!", "tokens": ["Dich", "mei\u00b7nes", "Her\u00b7zens", "s\u00fc\u00b7\u00dfen", "Freund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Dann beten wir und loben mit einander", "tokens": ["Dann", "be\u00b7ten", "wir", "und", "lo\u00b7ben", "mit", "ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "PRF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Den guten Gott, der uns vereint,", "tokens": ["Den", "gu\u00b7ten", "Gott", ",", "der", "uns", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und unser Lob steigt mit einander", "tokens": ["Und", "un\u00b7ser", "Lob", "steigt", "mit", "ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "PRF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Wie zween Flammen hoch empor", "tokens": ["Wie", "zween", "Flam\u00b7men", "hoch", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "NN", "ADJD", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Und unser Lob erreicht sein Ohr!", "tokens": ["Und", "un\u00b7ser", "Lob", "er\u00b7reicht", "sein", "Ohr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}