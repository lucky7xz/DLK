{"textgrid.poem.43108": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zahmer Narrheit w\u00e4ssrig Seufzen,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zahmer Narrheit w\u00e4ssrig Seufzen,", "tokens": ["Zah\u00b7mer", "Nar\u00b7rheit", "w\u00e4ss\u00b7rig", "Seuf\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Feiges, kindisch-weiches Beten;", "tokens": ["Fei\u00b7ges", ",", "kin\u00b7dischwei\u00b7ches", "Be\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Was man th\u00f6richt selbst verschuldet,", "tokens": ["Was", "man", "th\u00f6\u00b7richt", "selbst", "ver\u00b7schul\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Daraus soll uns Gott erretten!", "tokens": ["Da\u00b7raus", "soll", "uns", "Gott", "er\u00b7ret\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Unser Gott ist vielbesch\u00e4ftigt,", "tokens": ["Un\u00b7ser", "Gott", "ist", "viel\u00b7be\u00b7sch\u00e4f\u00b7tigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4\u00dft uns jammern hier auf Erden,", "tokens": ["L\u00e4\u00dft", "uns", "jam\u00b7mern", "hier", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sagt: \u00bbwer viel geliebt (gelitten),", "tokens": ["Sagt", ":", "\u00bb", "wer", "viel", "ge\u00b7liebt", "(", "ge\u00b7lit\u00b7ten", ")", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "$.", "$(", "PWS", "ADV", "VVPP", "$(", "VVPP", "$(", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem wird viel vergeben werden.\u00ab", "tokens": ["Dem", "wird", "viel", "ver\u00b7ge\u00b7ben", "wer\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVPP", "VAINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}