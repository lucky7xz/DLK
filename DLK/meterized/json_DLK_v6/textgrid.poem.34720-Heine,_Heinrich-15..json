{"textgrid.poem.34720": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die schlanke Wasserlilie", "tokens": ["Die", "schlan\u00b7ke", "Was\u00b7ser\u00b7li\u00b7lie"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Schaut tr\u00e4umend empor aus dem See;", "tokens": ["Schaut", "tr\u00e4u\u00b7mend", "em\u00b7por", "aus", "dem", "See", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da gr\u00fc\u00dft der Mond herunter", "tokens": ["Da", "gr\u00fc\u00dft", "der", "Mond", "her\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APZR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit lichtem Liebesweh.", "tokens": ["Mit", "lich\u00b7tem", "Lie\u00b7bes\u00b7weh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Versch\u00e4mt senkt sie das K\u00f6pfchen", "tokens": ["Ver\u00b7sch\u00e4mt", "senkt", "sie", "das", "K\u00f6pf\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wieder hinab zu den Well'n \u2013", "tokens": ["Wie\u00b7der", "hin\u00b7ab", "zu", "den", "Well'n", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Da sieht sie zu ihren F\u00fc\u00dfen", "tokens": ["Da", "sieht", "sie", "zu", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den armen blassen Gesell'n.", "tokens": ["Den", "ar\u00b7men", "blas\u00b7sen", "Ge\u00b7sell'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ART", "ADJA", "ADJA", "NN", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}