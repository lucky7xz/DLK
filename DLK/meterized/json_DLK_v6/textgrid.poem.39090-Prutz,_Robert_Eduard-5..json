{"textgrid.poem.39090": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist kein echter Saft der Reben,", "tokens": ["Das", "ist", "kein", "ech\u00b7ter", "Saft", "der", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "der nicht gegoren und gesch\u00e4umt,", "tokens": ["der", "nicht", "ge\u00b7go\u00b7ren", "und", "ge\u00b7sch\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "das ist ein jammervolles Leben,", "tokens": ["das", "ist", "ein", "jam\u00b7mer\u00b7vol\u00b7les", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "wo man nicht schw\u00e4rmt mehr und mehr tr\u00e4umt!", "tokens": ["wo", "man", "nicht", "schw\u00e4rmt", "mehr", "und", "mehr", "tr\u00e4umt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PTKNEG", "VVFIN", "ADV", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo in den gleichen Gang der Tage", "tokens": ["Wo", "in", "den", "glei\u00b7chen", "Gang", "der", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "kein Fest mehr holden Wechsel bringt,", "tokens": ["kein", "Fest", "mehr", "hol\u00b7den", "Wech\u00b7sel", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "wo nie bei jauchzendem Gelage", "tokens": ["wo", "nie", "bei", "jauch\u00b7zen\u00b7dem", "Ge\u00b7la\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "die Freude mehr die M\u00fctze schwingt!", "tokens": ["die", "Freu\u00b7de", "mehr", "die", "M\u00fct\u00b7ze", "schwingt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Geschrieben steht's und steht zu lesen,", "tokens": ["Ge\u00b7schrie\u00b7ben", "steht's", "und", "steht", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die Wahrheit wohnet in dem Wein;", "tokens": ["die", "Wahr\u00b7heit", "woh\u00b7net", "in", "dem", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wir sind so lange klug gewesen,", "tokens": ["wir", "sind", "so", "lan\u00b7ge", "klug", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "nun la\u00dft uns endlich n\u00e4rrisch sein!", "tokens": ["nun", "la\u00dft", "uns", "end\u00b7lich", "n\u00e4r\u00b7risch", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Trinkern glaubt und glaubt den Dichtern,", "tokens": ["Den", "Trin\u00b7kern", "glaubt", "und", "glaubt", "den", "Dich\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "sie haben beide es erprobt:", "tokens": ["sie", "ha\u00b7ben", "bei\u00b7de", "es", "er\u00b7probt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wer euch berauscht hat, macht euch n\u00fcchtern \u2013", "tokens": ["Wer", "euch", "be\u00b7rauscht", "hat", ",", "macht", "euch", "n\u00fcch\u00b7tern", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$,", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sei, heil'ge Trunkenheit, belobt!", "tokens": ["Sei", ",", "heil'\u00b7ge", "Trun\u00b7ken\u00b7heit", ",", "be\u00b7lobt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "$,", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}