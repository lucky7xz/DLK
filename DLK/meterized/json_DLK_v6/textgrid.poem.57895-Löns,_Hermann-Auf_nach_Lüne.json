{"textgrid.poem.57895": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Auf nach L\u00fcne", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sch\u00e4fer Ast ist aus der Mode;", "tokens": ["Der", "Sch\u00e4\u00b7fer", "Ast", "ist", "aus", "der", "Mo\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu einem andern Heidjer eilt", "tokens": ["Zu", "ei\u00b7nem", "an\u00b7dern", "Heid\u00b7jer", "eilt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Volk, weil dieser, wie er selbst sagt,", "tokens": ["Das", "Volk", ",", "weil", "die\u00b7ser", ",", "wie", "er", "selbst", "sagt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PDS", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mit Hafergr\u00fctze alles heilt.", "tokens": ["Mit", "Ha\u00b7fer\u00b7gr\u00fct\u00b7ze", "al\u00b7les", "heilt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Beim altber\u00fchmten Kloster L\u00fcne", "tokens": ["Beim", "alt\u00b7be\u00b7r\u00fchm\u00b7ten", "Klos\u00b7ter", "L\u00fc\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bei L\u00fcneburg, da wohnt der Mann,", "tokens": ["Bei", "L\u00fc\u00b7ne\u00b7burg", ",", "da", "wohnt", "der", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der selbst die allerschlimmste Krankheit", "tokens": ["Der", "selbst", "die", "al\u00b7ler\u00b7schlimms\u00b7te", "Krank\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Hafergr\u00fctze heilen kann.", "tokens": ["Mit", "Ha\u00b7fer\u00b7gr\u00fct\u00b7ze", "hei\u00b7len", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er stippt den Finger in die Gr\u00fctze,", "tokens": ["Er", "stippt", "den", "Fin\u00b7ger", "in", "die", "Gr\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dann ist das Zeug magnetisiert,", "tokens": ["Dann", "ist", "das", "Zeug", "mag\u00b7ne\u00b7ti\u00b7siert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wer davon f\u00fcr zehn Mark futtert,", "tokens": ["Und", "wer", "da\u00b7von", "f\u00fcr", "zehn", "Mark", "fut\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PAV", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Derselbige ist gleich kuriert.", "tokens": ["Der\u00b7sel\u00b7bi\u00b7ge", "ist", "gleich", "ku\u00b7riert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbnach L\u00fcne!\u00ab hei\u00dft darum die Losung,", "tokens": ["\u00bb", "nach", "L\u00fc\u00b7ne", "!", "\u00ab", "hei\u00dft", "da\u00b7rum", "die", "Lo\u00b7sung", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NE", "$.", "$(", "VVFIN", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die jetzt erschallet durch die Welt;", "tokens": ["Die", "jetzt", "er\u00b7schal\u00b7let", "durch", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer keine Gr\u00fctze hat im Kopfe,", "tokens": ["Wer", "kei\u00b7ne", "Gr\u00fct\u00b7ze", "hat", "im", "Kop\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bekommt in L\u00fcne sie f\u00fcr Geld!", "tokens": ["Be\u00b7kommt", "in", "L\u00fc\u00b7ne", "sie", "f\u00fcr", "Geld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}