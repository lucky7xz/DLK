{"textgrid.poem.36338": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf Berges H\u00f6hen,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Berges H\u00f6hen,", "tokens": ["Auf", "Ber\u00b7ges", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Da wohnten die Alten,", "tokens": ["Da", "wohn\u00b7ten", "die", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Alten, die Ritter des herrlichen Landes!", "tokens": ["Die", "Al\u00b7ten", ",", "die", "Rit\u00b7ter", "des", "herr\u00b7li\u00b7chen", "Lan\u00b7des", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "In Eisen gewaffnet,", "tokens": ["In", "Ei\u00b7sen", "ge\u00b7waff\u00b7net", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Aus steinernen Burgen,", "tokens": ["Aus", "stei\u00b7ner\u00b7nen", "Bur\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "So schauten sie mutig zu Tale hernieder,", "tokens": ["So", "schau\u00b7ten", "sie", "mu\u00b7tig", "zu", "Ta\u00b7le", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Wo rund die W\u00e4lder allgr\u00fcne,", "tokens": ["Wo", "rund", "die", "W\u00e4l\u00b7der", "all\u00b7gr\u00fc\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "In Sonne und Nebel gekleidet,", "tokens": ["In", "Son\u00b7ne", "und", "Ne\u00b7bel", "ge\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Aus tausend R\u00f6hren Erfrischung duften,", "tokens": ["Aus", "tau\u00b7send", "R\u00f6h\u00b7ren", "Er\u00b7fri\u00b7schung", "duf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "In ew'gem Sturme dumpfe Lieder rauschen,", "tokens": ["In", "ew'\u00b7gem", "Stur\u00b7me", "dump\u00b7fe", "Lie\u00b7der", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Fernher,", "tokens": ["Fern\u00b7her", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Wie aus hohen Nordens dunkelm Geheimnis.", "tokens": ["Wie", "aus", "ho\u00b7hen", "Nor\u00b7dens", "dun\u00b7kelm", "Ge\u00b7heim\u00b7nis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Voll von Gedanken und selig", "tokens": ["Voll", "von", "Ge\u00b7dan\u00b7ken", "und", "se\u00b7lig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "KON", "ADJD"], "meter": "+--+----", "measure": "dactylic.di.plus"}, "line.2": {"text": "Stehet der Mann", "tokens": ["Ste\u00b7het", "der", "Mann"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Im gl\u00fchenden Sommer am Gitter,", "tokens": ["Im", "gl\u00fc\u00b7hen\u00b7den", "Som\u00b7mer", "am", "Git\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Den Helm von den Augen sich dr\u00fcckend,", "tokens": ["Den", "Helm", "von", "den", "Au\u00b7gen", "sich", "dr\u00fc\u00b7ckend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Schauet verfolgend", "tokens": ["Schau\u00b7et", "ver\u00b7fol\u00b7gend"], "token_info": ["word", "word"], "pos": ["VVFIN", "VVPP"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die schwindenden Z\u00fcge,", "tokens": ["Die", "schwin\u00b7den\u00b7den", "Z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Nichtiger Wolken", "tokens": ["Nich\u00b7ti\u00b7ger", "Wol\u00b7ken"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Riesengebilde und R\u00e4tsel,", "tokens": ["Rie\u00b7sen\u00b7ge\u00b7bil\u00b7de", "und", "R\u00e4t\u00b7sel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "Dazwischen den fr\u00f6hlichen Schwarm des Gefl\u00fcgels;", "tokens": ["Da\u00b7zwi\u00b7schen", "den", "fr\u00f6h\u00b7li\u00b7chen", "Schwarm", "des", "Ge\u00b7fl\u00fc\u00b7gels", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Und l\u00e4chelt in Freuden,", "tokens": ["Und", "l\u00e4\u00b7chelt", "in", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Wie breit und langsam", "tokens": ["Wie", "breit", "und", "lang\u00b7sam"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Der Strom sich windet,", "tokens": ["Der", "Strom", "sich", "win\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Bald schwarz, bald silbern,", "tokens": ["Bald", "schwarz", ",", "bald", "sil\u00b7bern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Durch gr\u00fcnende Anger.", "tokens": ["Durch", "gr\u00fc\u00b7nen\u00b7de", "An\u00b7ger", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Die lustigen D\u00f6rfer zur Seite,", "tokens": ["Die", "lus\u00b7ti\u00b7gen", "D\u00f6r\u00b7fer", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.16": {"text": "Und zierliche St\u00e4dte,", "tokens": ["Und", "zier\u00b7li\u00b7che", "St\u00e4d\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Mit schlanken T\u00fcrmen und Glockenspiele;", "tokens": ["Mit", "schlan\u00b7ken", "T\u00fcr\u00b7men", "und", "Glo\u00b7cken\u00b7spie\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Langsam dann im Tal gezogen,", "tokens": ["Lang\u00b7sam", "dann", "im", "Tal", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Auf allen Stra\u00dfen und Wegen", "tokens": ["Auf", "al\u00b7len", "Stra\u00b7\u00dfen", "und", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Orientes Reichtum in vollem Triumphe,", "tokens": ["O\u00b7ri\u00b7en\u00b7tes", "Reich\u00b7tum", "in", "vol\u00b7lem", "Tri\u00b7um\u00b7phe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.21": {"text": "Wagen und M\u00e4nner,", "tokens": ["Wa\u00b7gen", "und", "M\u00e4n\u00b7ner", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Elefanten und Mohren,", "tokens": ["E\u00b7lef\u00b7an\u00b7ten", "und", "Moh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.23": {"text": "Bl\u00fchende Stein' und farbige Fr\u00fcchte,", "tokens": ["Bl\u00fc\u00b7hen\u00b7de", "Stein'", "und", "far\u00b7bi\u00b7ge", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.24": {"text": "Indiens goldenster Segen.", "tokens": ["In\u00b7di\u00b7ens", "gol\u00b7dens\u00b7ter", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Wenn der Fr\u00fchling gr\u00fcnet,", "tokens": ["Wenn", "der", "Fr\u00fch\u00b7ling", "gr\u00fc\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.26": {"text": "So schweift er im Walde;", "tokens": ["So", "schweift", "er", "im", "Wal\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.27": {"text": "Bald im Schwarm der Gef\u00e4hrten,", "tokens": ["Bald", "im", "Schwarm", "der", "Ge\u00b7f\u00e4hr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.28": {"text": "Bald vertieft er sich einsam,", "tokens": ["Bald", "ver\u00b7tieft", "er", "sich", "ein\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.29": {"text": "Wo kein Tritt mehr ert\u00f6nt,", "tokens": ["Wo", "kein", "Tritt", "mehr", "er\u00b7t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.30": {"text": "Wo das Reh nicht mehr flieht,", "tokens": ["Wo", "das", "Reh", "nicht", "mehr", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.31": {"text": "Das bedeutend ihn anschaut", "tokens": ["Das", "be\u00b7deu\u00b7tend", "ihn", "an\u00b7schaut"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADJD", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Aus sittsam verst\u00e4ndigen Augen.", "tokens": ["Aus", "sitt\u00b7sam", "ver\u00b7st\u00e4n\u00b7di\u00b7gen", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.33": {"text": "Wohl bemerkt er das Zeichen,", "tokens": ["Wohl", "be\u00b7merkt", "er", "das", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.34": {"text": "Denn himmlisch naht ihm", "tokens": ["Denn", "himm\u00b7lisch", "naht", "ihm"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.35": {"text": "Aus Waldesgr\u00fcne", "tokens": ["Aus", "Wal\u00b7des\u00b7gr\u00fc\u00b7ne"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.36": {"text": "Die hohe Frau seines Herzens,", "tokens": ["Die", "ho\u00b7he", "Frau", "sei\u00b7nes", "Her\u00b7zens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.37": {"text": "Die schweigend redet;", "tokens": ["Die", "schwei\u00b7gend", "re\u00b7det", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.38": {"text": "Statt nichtiger Worte", "tokens": ["Statt", "nich\u00b7ti\u00b7ger", "Wor\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.39": {"text": "Volle Blumen ihm reichend,", "tokens": ["Vol\u00b7le", "Blu\u00b7men", "ihm", "rei\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.40": {"text": "Zum Bunde der Treue.", "tokens": ["Zum", "Bun\u00b7de", "der", "Treu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.41": {"text": "Und beide vom Dufte bezaubert,", "tokens": ["Und", "bei\u00b7de", "vom", "Duf\u00b7te", "be\u00b7zau\u00b7bert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.42": {"text": "Im Schatten der Linde versunken;", "tokens": ["Im", "Schat\u00b7ten", "der", "Lin\u00b7de", "ver\u00b7sun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.43": {"text": "Schauen in selige Augen,", "tokens": ["Schau\u00b7en", "in", "se\u00b7li\u00b7ge", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.44": {"text": "Ruhen dem Fr\u00fchling im Scho\u00dfe.", "tokens": ["Ru\u00b7hen", "dem", "Fr\u00fch\u00b7ling", "im", "Scho\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.45": {"text": "Freudig umarmt den Helden die Tugend,", "tokens": ["Freu\u00b7dig", "um\u00b7armt", "den", "Hel\u00b7den", "die", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.46": {"text": "Und inmitten der Freuden", "tokens": ["Und", "in\u00b7mit\u00b7ten", "der", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.47": {"text": "G\u00fcrtet sie ihn mit gewaltigem Schwerte,", "tokens": ["G\u00fcr\u00b7tet", "sie", "ihn", "mit", "ge\u00b7wal\u00b7ti\u00b7gem", "Schwer\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+---+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.48": {"text": "Alle Laster zu tilgen.", "tokens": ["Al\u00b7le", "Las\u00b7ter", "zu", "til\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.49": {"text": "Mutig nimmt er die Waffen,", "tokens": ["Mu\u00b7tig", "nimmt", "er", "die", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.50": {"text": "Froh der Freuden kehrt er am Abend", "tokens": ["Froh", "der", "Freu\u00b7den", "kehrt", "er", "am", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.51": {"text": "Zu seinem Felsen wieder,", "tokens": ["Zu", "sei\u00b7nem", "Fel\u00b7sen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "Wo die Freunde zusammen", "tokens": ["Wo", "die", "Freun\u00b7de", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PTKVZ"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.53": {"text": "Deutscher Freuden sich freuen.", "tokens": ["Deut\u00b7scher", "Freu\u00b7den", "sich", "freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.54": {"text": "Wenn aber die braune Erde erstarrt ist,", "tokens": ["Wenn", "a\u00b7ber", "die", "brau\u00b7ne", "Er\u00b7de", "er\u00b7starrt", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.55": {"text": "Die Fl\u00fcsse leuchten wie Eisen,", "tokens": ["Die", "Fl\u00fcs\u00b7se", "leuch\u00b7ten", "wie", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.56": {"text": "In wei\u00dfem Laube die W\u00e4lder schimmern;", "tokens": ["In", "wei\u00b7\u00dfem", "Lau\u00b7be", "die", "W\u00e4l\u00b7der", "schim\u00b7mern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.57": {"text": "Dann horchen bei fr\u00f6hlichem Feuer", "tokens": ["Dann", "hor\u00b7chen", "bei", "fr\u00f6h\u00b7li\u00b7chem", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.58": {"text": "Sie alten Geschichten,", "tokens": ["Sie", "al\u00b7ten", "Ge\u00b7schich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.59": {"text": "Wie Zwerge k\u00fcnstlich in H\u00f6hlen leben;", "tokens": ["Wie", "Zwer\u00b7ge", "k\u00fcnst\u00b7lich", "in", "H\u00f6h\u00b7len", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.60": {"text": "Sehen im Geiste", "tokens": ["Se\u00b7hen", "im", "Geis\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.61": {"text": "Dort unten die dunkelste Tiefe,", "tokens": ["Dort", "un\u00b7ten", "die", "dun\u00b7kels\u00b7te", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.62": {"text": "Von Lichtern durchschienen,", "tokens": ["Von", "Lich\u00b7tern", "durch\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.63": {"text": "Voll Sch\u00e4tze und M\u00e4rchen.", "tokens": ["Voll", "Sch\u00e4t\u00b7ze", "und", "M\u00e4r\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$."], "meter": "-+--