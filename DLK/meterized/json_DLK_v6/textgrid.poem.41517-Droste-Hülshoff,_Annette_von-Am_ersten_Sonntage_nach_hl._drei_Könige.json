{"textgrid.poem.41517": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am ersten Sonntage nach hl. drei K\u00f6nige", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und sieh, ich habe dich gesucht mit Schmerzen,", "tokens": ["Und", "sieh", ",", "ich", "ha\u00b7be", "dich", "ge\u00b7sucht", "mit", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Herr und Gott, wo werde ich dich finden?", "tokens": ["Mein", "Herr", "und", "Gott", ",", "wo", "wer\u00b7de", "ich", "dich", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,", "PWAV", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach nicht im eignen ausgestorbnen Herzen,", "tokens": ["Ach", "nicht", "im", "eig\u00b7nen", "aus\u00b7ge\u00b7storb\u00b7nen", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PTKNEG", "APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo l\u00e4ngst dein Ebenbild erlosch in S\u00fcnden:", "tokens": ["Wo", "l\u00e4ngst", "dein", "E\u00b7ben\u00b7bild", "er\u00b7losch", "in", "S\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da t\u00f6nt aus allen Winkeln, ruf' ich dich,", "tokens": ["Da", "t\u00f6nt", "aus", "al\u00b7len", "Win\u00b7keln", ",", "ruf'", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN", "$,", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mein eignes Echo wie ein Spott um mich.", "tokens": ["Mein", "eig\u00b7nes", "E\u00b7cho", "wie", "ein", "Spott", "um", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KOKOM", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wer einmal hat dein g\u00f6ttlich Bild verloren,", "tokens": ["Wer", "ein\u00b7mal", "hat", "dein", "g\u00f6tt\u00b7lich", "Bild", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "PPOSAT", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was ihm doch eigen war wie seine Seele,", "tokens": ["Was", "ihm", "doch", "ei\u00b7gen", "war", "wie", "sei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VAFIN", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit dem hat sich die ganze Welt verschworen,", "tokens": ["Mit", "dem", "hat", "sich", "die", "gan\u00b7ze", "Welt", "ver\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VAFIN", "PRF", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Da\u00df sie dein heilig Antlitz ihm verhehle;", "tokens": ["Da\u00df", "sie", "dein", "hei\u00b7lig", "Ant\u00b7litz", "ihm", "ver\u00b7heh\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wo der Fromme dich auf Tabor schaut,", "tokens": ["Und", "wo", "der", "From\u00b7me", "dich", "auf", "Ta\u00b7bor", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PRF", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da hat er sich im Tal sein Haus gebaut.", "tokens": ["Da", "hat", "er", "sich", "im", "Tal", "sein", "Haus", "ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPRART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So mu\u00df ich denn zu meinem Graun erfahren", "tokens": ["So", "mu\u00df", "ich", "denn", "zu", "mei\u00b7nem", "Graun", "er\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das R\u00e4tsel, das ich nimmer konnte l\u00f6sen,", "tokens": ["Das", "R\u00e4t\u00b7sel", ",", "das", "ich", "nim\u00b7mer", "konn\u00b7te", "l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als mir in meinen hellen Unschuldsjahren", "tokens": ["Als", "mir", "in", "mei\u00b7nen", "hel\u00b7len", "Un\u00b7schulds\u00b7jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ganz unbegreiflich schien was da vom B\u00f6sen,", "tokens": ["Ganz", "un\u00b7be\u00b7greif\u00b7lich", "schien", "was", "da", "vom", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PWS", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df eine Seele, wo dein Bild gegl\u00fcht,", "tokens": ["Da\u00df", "ei\u00b7ne", "See\u00b7le", ",", "wo", "dein", "Bild", "ge\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PWAV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dich gar nicht mehr erkennt, wenn sie dich sieht.", "tokens": ["Dich", "gar", "nicht", "mehr", "er\u00b7kennt", ",", "wenn", "sie", "dich", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "ADV", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Rings um mich t\u00f6nt der klare Vogelreigen:", "tokens": ["Rings", "um", "mich", "t\u00f6nt", "der", "kla\u00b7re", "Vo\u00b7gel\u00b7rei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbhorch auf, die V\u00f6glein singen seinem Ruhme!\u00ab", "tokens": ["\u00bb", "horch", "auf", ",", "die", "V\u00f6\u00b7glein", "sin\u00b7gen", "sei\u00b7nem", "Ruh\u00b7me", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PTKVZ", "$,", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und will ich mich zu einer Blume neigen:", "tokens": ["Und", "will", "ich", "mich", "zu", "ei\u00b7ner", "Blu\u00b7me", "nei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbsein mildes Auge schaut aus jeder Blume.\u00ab", "tokens": ["\u00bb", "sein", "mil\u00b7des", "Au\u00b7ge", "schaut", "aus", "je\u00b7der", "Blu\u00b7me", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich habe dich in der Natur gesucht,", "tokens": ["Ich", "ha\u00b7be", "dich", "in", "der", "Na\u00b7tur", "ge\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und weltlich Wissen war die eitle Frucht!", "tokens": ["Und", "welt\u00b7lich", "Wis\u00b7sen", "war", "die", "eit\u00b7le", "Frucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und mu\u00df ich schauen in des Schicksals Gange,", "tokens": ["Und", "mu\u00df", "ich", "schau\u00b7en", "in", "des", "Schick\u00b7sals", "Gan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie oft ein gutes Herz in diesem Leben", "tokens": ["Wie", "oft", "ein", "gu\u00b7tes", "Herz", "in", "die\u00b7sem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vergebens zu dir schreit aus seinem Drange,", "tokens": ["Ver\u00b7ge\u00b7bens", "zu", "dir", "schreit", "aus", "sei\u00b7nem", "Dran\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis es verzweifelnd sich der S\u00fcnd' ergeben,", "tokens": ["Bis", "es", "ver\u00b7zwei\u00b7felnd", "sich", "der", "S\u00fcnd'", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann scheint mir alle Liebe wie ein Spott,", "tokens": ["Dann", "scheint", "mir", "al\u00b7le", "Lie\u00b7be", "wie", "ein", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und keine Gnade f\u00fchl' ich, keinen Gott!", "tokens": ["Und", "kei\u00b7ne", "Gna\u00b7de", "f\u00fchl'", "ich", ",", "kei\u00b7nen", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und schlingen sich so wunderbar die Knoten,", "tokens": ["Und", "schlin\u00b7gen", "sich", "so", "wun\u00b7der\u00b7bar", "die", "Kno\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df du in Licht erscheinst dem treuen Blicke,", "tokens": ["Da\u00df", "du", "in", "Licht", "er\u00b7scheinst", "dem", "treu\u00b7en", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da hat der B\u00f6se seine Hand geboten", "tokens": ["Da", "hat", "der", "B\u00f6\u00b7se", "sei\u00b7ne", "Hand", "ge\u00b7bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und baut dem Zweifel eine Nebelbr\u00fccke,", "tokens": ["Und", "baut", "dem", "Zwei\u00b7fel", "ei\u00b7ne", "Ne\u00b7bel\u00b7br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und mein Verstand, der nur sich selber traut,", "tokens": ["Und", "mein", "Ver\u00b7stand", ",", "der", "nur", "sich", "sel\u00b7ber", "traut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "ADV", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der meint gewi\u00df sie sei von Gold gebaut!", "tokens": ["Der", "meint", "ge\u00b7wi\u00df", "sie", "sei", "von", "Gold", "ge\u00b7baut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPER", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ich wei\u00df es, da\u00df du bist, ich mu\u00df es f\u00fchlen,", "tokens": ["Ich", "wei\u00df", "es", ",", "da\u00df", "du", "bist", ",", "ich", "mu\u00df", "es", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VAFIN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie eine schwere kalte Hand mich dr\u00fccken,", "tokens": ["Wie", "ei\u00b7ne", "schwe\u00b7re", "kal\u00b7te", "Hand", "mich", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df einst ein dunkles Ende diesen Spielen,", "tokens": ["Da\u00df", "einst", "ein", "dunk\u00b7les", "En\u00b7de", "die\u00b7sen", "Spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df jede Tat sich ihre Frucht mu\u00df pfl\u00fccken;", "tokens": ["Da\u00df", "je\u00b7de", "Tat", "sich", "ih\u00b7re", "Frucht", "mu\u00df", "pfl\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PRF", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich f\u00fchle der Vergeltung mich geweiht,", "tokens": ["Ich", "f\u00fch\u00b7le", "der", "Ver\u00b7gel\u00b7tung", "mich", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich f\u00fchle dich, doch nicht mit Freudigkeit.", "tokens": ["Ich", "f\u00fch\u00b7le", "dich", ",", "doch", "nicht", "mit", "Freu\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Wo find' ich dich in Hoffnung und in Lieben!", "tokens": ["Wo", "find'", "ich", "dich", "in", "Hoff\u00b7nung", "und", "in", "Lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "APPR", "NN", "KON", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn jene ernste Macht, die ich erkoren,", "tokens": ["Denn", "je\u00b7ne", "erns\u00b7te", "Macht", ",", "die", "ich", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das ist der Schatten nur, der mir geblieben", "tokens": ["Das", "ist", "der", "Schat\u00b7ten", "nur", ",", "der", "mir", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "VVPP"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Von deinem Bilde, da ich es verloren.", "tokens": ["Von", "dei\u00b7nem", "Bil\u00b7de", ",", "da", "ich", "es", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O Gott, du bist so mild und bist so licht!", "tokens": ["O", "Gott", ",", "du", "bist", "so", "mild", "und", "bist", "so", "licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "KON", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich suche dich in Schmerzen, birg dich nicht!", "tokens": ["Ich", "su\u00b7che", "dich", "in", "Schmer\u00b7zen", ",", "birg", "dich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,", "VVIMP", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}