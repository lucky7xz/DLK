{"textgrid.poem.36759": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie sie singen, wie sie die Muse befeuert, wie w\u00fcthend", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sie singen, wie sie die Muse befeuert, wie w\u00fcthend", "tokens": ["Wie", "sie", "sin\u00b7gen", ",", "wie", "sie", "die", "Mu\u00b7se", "be\u00b7feu\u00b7ert", ",", "wie", "w\u00fct\u00b7hend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "VVPP", "$,", "PWAV", "ADJD"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sich im entz\u00fcndeten Kampf wechselnd beginnen ein Lied:", "tokens": ["Sich", "im", "ent\u00b7z\u00fcn\u00b7de\u00b7ten", "Kampf", "wech\u00b7selnd", "be\u00b7gin\u00b7nen", "ein", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "ADJA", "NN", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Bauern sind es zwar nur, Sacktr\u00e4ger und Pizzicarole,", "tokens": ["Bau\u00b7ern", "sind", "es", "zwar", "nur", ",", "Sack\u00b7tr\u00e4\u00b7ger", "und", "Piz\u00b7zi\u00b7ca\u00b7ro\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ADV", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-++--+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Stiefelputzer und solch Lumpengesindel der Stadt.", "tokens": ["Stie\u00b7fel\u00b7put\u00b7zer", "und", "solch", "Lum\u00b7pen\u00b7ge\u00b7sin\u00b7del", "der", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Aber sie sind mir lieber, denn ihresgleichen in Deutschland,", "tokens": ["A\u00b7ber", "sie", "sind", "mir", "lie\u00b7ber", ",", "denn", "ih\u00b7res\u00b7glei\u00b7chen", "in", "Deutschland", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "$,", "KON", "PPOSAT", "APPR", "NE", "$,"], "meter": "+--+-+--+-+--+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Die man zwar nirgends liest, aber zu Tausenden druckt.", "tokens": ["Die", "man", "zwar", "nir\u00b7gends", "liest", ",", "a\u00b7ber", "zu", "Tau\u00b7sen\u00b7den", "druckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "ADV", "VVFIN", "$,", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}}}}}