{"textgrid.poem.46152": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Lust und vergn\u00fcgen in der pein", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie s\u00fc\u00df seind meine schmerzen,", "tokens": ["O", "wie", "s\u00fc\u00df", "seind", "mei\u00b7ne", "schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "PPOSAT", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die ich ihrenthalb ertrag,", "tokens": ["die", "ich", "ih\u00b7ren\u00b7thalb", "er\u00b7trag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "weil sie mild in ihrem herzen", "tokens": ["weil", "sie", "mild", "in", "ih\u00b7rem", "her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "nu empfindet gleiche plag,", "tokens": ["nu", "emp\u00b7fin\u00b7det", "glei\u00b7che", "plag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "und dieweil sie mit freindlichen anblicken", "tokens": ["und", "die\u00b7weil", "sie", "mit", "freind\u00b7li\u00b7chen", "an\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "versehret vil und will nur mich erquicken.", "tokens": ["ver\u00b7seh\u00b7ret", "vil", "und", "will", "nur", "mich", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VMFIN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Meiner augen leid vermehret", "tokens": ["Mei\u00b7ner", "au\u00b7gen", "leid", "ver\u00b7meh\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "meines herzens gro\u00dfe brunst:", "tokens": ["mei\u00b7nes", "her\u00b7zens", "gro\u00b7\u00dfe", "brunst", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ich sih, da\u00df ihr herz versehret,", "tokens": ["ich", "sih", ",", "da\u00df", "ihr", "herz", "ver\u00b7seh\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "wol bei ihrer augen gunst;", "tokens": ["wol", "bei", "ih\u00b7rer", "au\u00b7gen", "gunst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "dieweil ihr glanz mit lieblichen anblicken", "tokens": ["die\u00b7weil", "ihr", "glanz", "mit", "lieb\u00b7li\u00b7chen", "an\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "will strafen vil und nur mein herz erquicken.", "tokens": ["will", "stra\u00b7fen", "vil", "und", "nur", "mein", "herz", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "ADV", "KON", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Diese stern zu allen stunden", "tokens": ["Die\u00b7se", "stern", "zu", "al\u00b7len", "stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "zeigen sich so freudenreich,", "tokens": ["zei\u00b7gen", "sich", "so", "freu\u00b7den\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df sie nah und fern verwunden", "tokens": ["da\u00df", "sie", "nah", "und", "fern", "ver\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "dem cephalischen pfeil gleich;", "tokens": ["dem", "ce\u00b7pha\u00b7li\u00b7schen", "pfeil", "gleich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "sie k\u00f6nden auch mit kr\u00e4ftigen anblicken", "tokens": ["sie", "k\u00f6n\u00b7den", "auch", "mit", "kr\u00e4f\u00b7ti\u00b7gen", "an\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "auf einmal gleich versehren und erquicken.", "tokens": ["auf", "ein\u00b7mal", "gleich", "ver\u00b7seh\u00b7ren", "und", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Darum mu\u00df die welt bekennen,", "tokens": ["Da\u00b7rum", "mu\u00df", "die", "welt", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "o ihr augen, der welt pracht,", "tokens": ["o", "ihr", "au\u00b7gen", ",", "der", "welt", "pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df, wa eure facklen brennen,", "tokens": ["da\u00df", ",", "wa", "eu\u00b7re", "fack\u00b7len", "bren\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "XY", "PPOSAT", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "da wird es niemalen nacht.", "tokens": ["da", "wird", "es", "nie\u00b7ma\u00b7len", "nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "und da\u00df ihr k\u00f6nt mit seltsamen anblicken", "tokens": ["und", "da\u00df", "ihr", "k\u00f6nt", "mit", "selt\u00b7sa\u00b7men", "an\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "des menschen herz betr\u00f6wen und erquicken.", "tokens": ["des", "men\u00b7schen", "herz", "be\u00b7tr\u00f6\u00b7wen", "und", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Mein herz mag sich wol ergetzen,", "tokens": ["Mein", "herz", "mag", "sich", "wol", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "wan es findet euch so klar,", "tokens": ["wan", "es", "fin\u00b7det", "euch", "so", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ehrend euch als seine g\u00f6tzen", "tokens": ["eh\u00b7rend", "euch", "als", "sei\u00b7ne", "g\u00f6t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "auf dem sch\u00f6nesten altar;", "tokens": ["auf", "dem", "sch\u00f6\u00b7nes\u00b7ten", "al\u00b7tar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "vor dem bit ich, da\u00df mit liebreichen blicken", "tokens": ["vor", "dem", "bit", "ich", ",", "da\u00df", "mit", "lieb\u00b7rei\u00b7chen", "bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "PPER", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "ihr wollet mich und niemand sunst erquicken.", "tokens": ["ihr", "wol\u00b7let", "mich", "und", "nie\u00b7mand", "sunst", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "KON", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ich bit euch, nicht zu verschm\u00e4hen", "tokens": ["Ich", "bit", "euch", ",", "nicht", "zu", "ver\u00b7schm\u00e4\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "mein herz f\u00fcr ein opfer schlecht,", "tokens": ["mein", "herz", "f\u00fcr", "ein", "op\u00b7fer", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "euch verbrennend anzusehen,", "tokens": ["euch", "ver\u00b7bren\u00b7nend", "an\u00b7zu\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ph\u00f6nix gleich, in lieb gerecht;", "tokens": ["Ph\u00f6\u00b7nix", "gleich", ",", "in", "lieb", "ge\u00b7recht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "APPR", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "als den der schein so lieblicher anblicken", "tokens": ["als", "den", "der", "schein", "so", "lieb\u00b7li\u00b7cher", "an\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ART", "NN", "ADV", "ADJD", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "kan t\u00f6dtend bald mit freud widrum erquicken.", "tokens": ["kan", "t\u00f6d\u00b7tend", "bald", "mit", "freud", "wid\u00b7rum", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADV", "APPR", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}