{"textgrid.poem.55901": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "M\u00e4dchen-Klage", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diese Neigung, in den Jahren,", "tokens": ["Die\u00b7se", "Nei\u00b7gung", ",", "in", "den", "Jah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da wir alle Kinder waren,", "tokens": ["da", "wir", "al\u00b7le", "Kin\u00b7der", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "viel allein zu sein, war mild;", "tokens": ["viel", "al\u00b7lein", "zu", "sein", ",", "war", "mild", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "VAINF", "$,", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "andern ging die Zeit im Streite,", "tokens": ["an\u00b7dern", "ging", "die", "Zeit", "im", "Strei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "und man hatte seine Seite,", "tokens": ["und", "man", "hat\u00b7te", "sei\u00b7ne", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "seine N\u00e4he, seine Weite,", "tokens": ["sei\u00b7ne", "N\u00e4\u00b7he", ",", "sei\u00b7ne", "Wei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "einen Weg, ein Tier, ein Bild.", "tokens": ["ei\u00b7nen", "Weg", ",", "ein", "Tier", ",", "ein", "Bild", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ich dachte noch, das Leben", "tokens": ["Und", "ich", "dach\u00b7te", "noch", ",", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "h\u00f6rte niemals auf zu geben,", "tokens": ["h\u00f6r\u00b7te", "nie\u00b7mals", "auf", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df man sich in sich besinnt.", "tokens": ["da\u00df", "man", "sich", "in", "sich", "be\u00b7sinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPR", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bin ich in mir nicht im Gr\u00f6\u00dften?", "tokens": ["Bin", "ich", "in", "mir", "nicht", "im", "Gr\u00f6\u00df\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Will mich Meines nicht mehr tr\u00f6sten", "tokens": ["Will", "mich", "Mei\u00b7nes", "nicht", "mehr", "tr\u00f6s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PIS", "PTKNEG", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und verstehen wie als Kind?", "tokens": ["und", "ver\u00b7ste\u00b7hen", "wie", "als", "Kind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KOKOM", "KOUS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Pl\u00f6tzlich bin ich wie versto\u00dfen,", "tokens": ["Pl\u00f6tz\u00b7lich", "bin", "ich", "wie", "ver\u00b7sto\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "KOKOM", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und zu einem \u00dcbergro\u00dfen", "tokens": ["und", "zu", "ei\u00b7nem", "\u00dc\u00b7ber\u00b7gro\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wird mir diese Einsamkeit,", "tokens": ["wird", "mir", "die\u00b7se", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "wenn, auf meiner Br\u00fcste H\u00fcgeln", "tokens": ["wenn", ",", "auf", "mei\u00b7ner", "Br\u00fcs\u00b7te", "H\u00fc\u00b7geln"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "stehend, mein Gef\u00fchl nach Fl\u00fcgeln", "tokens": ["ste\u00b7hend", ",", "mein", "Ge\u00b7f\u00fchl", "nach", "Fl\u00fc\u00b7geln"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PPOSAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "oder einem Ende schreit.", "tokens": ["o\u00b7der", "ei\u00b7nem", "En\u00b7de", "schreit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}