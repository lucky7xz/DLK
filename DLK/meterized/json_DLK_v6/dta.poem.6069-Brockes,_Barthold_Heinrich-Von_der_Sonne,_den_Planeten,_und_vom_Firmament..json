{"dta.poem.6069": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Offt zeigt bey dunckler Nacht den Stern-Gelehrten sich", "tokens": ["Offt", "zeigt", "bey", "dunck\u00b7ler", "Nacht", "den", "Stern\u00b7Ge\u00b7lehr\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Vorwurf, der verwunderlich.", "tokens": ["Ein", "Vor\u00b7wurf", ",", "der", "ver\u00b7wun\u00b7der\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Saturnus, Jupiter, Mars, Venus und Mereur,", "tokens": ["Sa\u00b7tur\u00b7nus", ",", "Ju\u00b7pi\u00b7ter", ",", "Mars", ",", "Ve\u00b7nus", "und", "Me\u00b7reur", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die scheinen hin und her zu irren,", "tokens": ["Die", "schei\u00b7nen", "hin", "und", "her", "zu", "ir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "KON", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und unsre Blicke zu verwirren.", "tokens": ["Und", "uns\u00b7re", "Bli\u00b7cke", "zu", "ver\u00b7wir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie geht denn dieses zu, da\u00df wir am Himmel sehn", "tokens": ["Wie", "geht", "denn", "die\u00b7ses", "zu", ",", "da\u00df", "wir", "am", "Him\u00b7mel", "sehn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "KON", "PDS", "PTKVZ", "$,", "KOUS", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dasselbige Gestirn itzt stille stehn,", "tokens": ["Das\u00b7sel\u00b7bi\u00b7ge", "Ge\u00b7stirn", "itzt", "stil\u00b7le", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Jtzt vor-itzt r\u00fcckwerts gehn?", "tokens": ["Jtzt", "vor\u00b7itzt", "r\u00fcck\u00b7werts", "gehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Daher: da unsre Erd\u2019 ohn Stillstand vorwerts r\u00fcckt;", "tokens": ["Da\u00b7her", ":", "da", "uns\u00b7re", "Erd'", "ohn", "Still\u00b7stand", "vor\u00b7werts", "r\u00fcckt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$.", "KOUS", "PPOSAT", "NN", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Entsteht, da\u00df man sie so verschiedentlich erblickt.", "tokens": ["Ent\u00b7steht", ",", "da\u00df", "man", "sie", "so", "ver\u00b7schie\u00b7dent\u00b7lich", "er\u00b7blickt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PIS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}