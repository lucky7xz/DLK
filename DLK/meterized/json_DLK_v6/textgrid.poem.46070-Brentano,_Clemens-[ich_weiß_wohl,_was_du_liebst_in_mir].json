{"textgrid.poem.46070": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[ich wei\u00df wohl, was du liebst in mir]", "genre": "verse", "period": "N.A.", "pub_year": 1825, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df wohl, was du liebst in mir \u2013", "tokens": ["Ich", "wei\u00df", "wohl", ",", "was", "du", "liebst", "in", "mir", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "VVFIN", "APPR", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es ist die Glut in meiner Brust \u2013", "tokens": ["Es", "ist", "die", "Glut", "in", "mei\u00b7ner", "Brust", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist die zauberhafte Zier,", "tokens": ["Es", "ist", "die", "zau\u00b7ber\u00b7haf\u00b7te", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der tief geheimen innern Lust,", "tokens": ["Der", "tief", "ge\u00b7hei\u00b7men", "in\u00b7nern", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die strahlt aus mir, und ruft zu dir:", "tokens": ["Die", "strahlt", "aus", "mir", ",", "und", "ruft", "zu", "dir", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "$,", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schlie\u00df mich in einen Felsenstein,", "tokens": ["Schlie\u00df", "mich", "in", "ei\u00b7nen", "Fel\u00b7sen\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So ruf ich doch durch Mark und Bein!", "tokens": ["So", "ruf", "ich", "doch", "durch", "Mark", "und", "Bein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Komm, lebe, liebe, stirb bei mir", "tokens": ["Komm", ",", "le\u00b7be", ",", "lie\u00b7be", ",", "stirb", "bei", "mir"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Du mu\u00dft, du mu\u00dft. \u2013", "tokens": ["Du", "mu\u00dft", ",", "du", "mu\u00dft", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}