{"textgrid.poem.46954": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Kriegsruf", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Ackermanns-Geschlechte,", "tokens": ["Du", "A\u00b7cker\u00b7manns\u00b7Ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Willst du nicht lassen den Pflug?", "tokens": ["Willst", "du", "nicht", "las\u00b7sen", "den", "Pflug", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "ART", "NN", "$."], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Du hast dich zum Knechte", "tokens": ["Du", "hast", "dich", "zum", "Knech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Geackert lang genug.", "tokens": ["Ge\u00b7a\u00b7ckert", "lang", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nimm deinen Pflug,", "tokens": ["Nimm", "dei\u00b7nen", "Pflug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Schmiede Schwerter klug,", "tokens": ["Schmie\u00b7de", "Schwer\u00b7ter", "klug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Pfl\u00fcg' deinem Feinde, dem Wicht,", "tokens": ["Pfl\u00fcg'", "dei\u00b7nem", "Fein\u00b7de", ",", "dem", "Wicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Furchen ins Angesicht.", "tokens": ["Fur\u00b7chen", "ins", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "La\u00dft euren Stab, ihr Hirten;", "tokens": ["La\u00dft", "eu\u00b7ren", "Stab", ",", "ihr", "Hir\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Eure L\u00e4mmer, die verirrten,", "tokens": ["Eu\u00b7re", "L\u00e4m\u00b7mer", ",", "die", "ver\u00b7irr\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Suchet sie nicht, dazu ist Zeit,", "tokens": ["Su\u00b7chet", "sie", "nicht", ",", "da\u00b7zu", "ist", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PAV", "VAFIN", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Wann ihr selbst erst geborgen seid.", "tokens": ["Wann", "ihr", "selbst", "erst", "ge\u00b7bor\u00b7gen", "seid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Teurer als das Eigentum", "tokens": ["Teu\u00b7rer", "als", "das", "Ei\u00b7gen\u00b7tum"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist Seel' und Leib;", "tokens": ["Ist", "Seel'", "und", "Leib", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Teurer ist Kind und Weib,", "tokens": ["Teu\u00b7rer", "ist", "Kind", "und", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Und teurer noch ist der Ruhm.", "tokens": ["Und", "teu\u00b7rer", "noch", "ist", "der", "Ruhm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Seht ihr eure alten Tannen,", "tokens": ["Seht", "ihr", "eu\u00b7re", "al\u00b7ten", "Tan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sie noch stehn so hoch?", "tokens": ["Wie", "sie", "noch", "stehn", "so", "hoch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo seid ihr doch,", "tokens": ["Wo", "seid", "ihr", "doch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ihr alten Mannen?", "tokens": ["Ihr", "al\u00b7ten", "Man\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Der Feind ist im Lande,", "tokens": ["Der", "Feind", "ist", "im", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Euer Weib ist die Schande,", "tokens": ["Eu\u00b7er", "Weib", "ist", "die", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Der Bastard euer Sohn;", "tokens": ["Der", "Bas\u00b7tard", "eu\u00b7er", "Sohn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Blut tilgt Hohn.", "tokens": ["Blut", "tilgt", "Hohn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Das Land zu verj\u00fcngen,", "tokens": ["Das", "Land", "zu", "ver\u00b7j\u00fcn\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Das ausgesaugte, ist gut;", "tokens": ["Das", "aus\u00b7ge\u00b7saug\u00b7te", ",", "ist", "gut", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Auf, es zu d\u00fcngen,", "tokens": ["Auf", ",", "es", "zu", "d\u00fcn\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mit Feindesblut!", "tokens": ["Mit", "Fein\u00b7des\u00b7blut", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}