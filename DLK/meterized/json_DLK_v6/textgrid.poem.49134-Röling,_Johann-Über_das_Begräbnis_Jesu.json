{"textgrid.poem.49134": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "\u00dcber das Begr\u00e4bnis Jesu", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So ist es, Jesu, o mein Leben,", "tokens": ["So", "ist", "es", ",", "Je\u00b7su", ",", "o", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NE", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So ist zum Tod' es doch gemeint?", "tokens": ["So", "ist", "zum", "Tod'", "es", "doch", "ge\u00b7meint", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es wundert selber deinen Feind,", "tokens": ["Es", "wun\u00b7dert", "sel\u00b7ber", "dei\u00b7nen", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df du den Geist hast auffgegeben.", "tokens": ["Da\u00df", "du", "den", "Geist", "hast", "auff\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gedenke denn, wie mir hiebey,", "tokens": ["Ge\u00b7den\u00b7ke", "denn", ",", "wie", "mir", "hie\u00b7bey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PWAV", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Br\u00e4utigam, zu Muthe sey.", "tokens": ["Mein", "Br\u00e4u\u00b7ti\u00b7gam", ",", "zu", "Mu\u00b7the", "sey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "$,", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie, frag' ich, kanstu doch verscheiden?", "tokens": ["Wie", ",", "frag'", "ich", ",", "kans\u00b7tu", "doch", "ver\u00b7schei\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VVIMP", "PPER", "$,", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Stirbt der, durch den der Bau der Welt", "tokens": ["Stirbt", "der", ",", "durch", "den", "der", "Bau", "der", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "$,", "APPR", "ART", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und selbst die Ewigkeit bestellt,", "tokens": ["Und", "selbst", "die", "E\u00b7wig\u00b7keit", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kan Gott, kan Gott den Tod auch leiden,", "tokens": ["Kan", "Gott", ",", "kan", "Gott", "den", "Tod", "auch", "lei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$,", "VMFIN", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und hat der schn\u00f6den Menschen Macht", "tokens": ["Und", "hat", "der", "schn\u00f6\u00b7den", "Men\u00b7schen", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Selbst ihren Sch\u00f6pffer umgebracht?", "tokens": ["Selbst", "ih\u00b7ren", "Sch\u00f6pf\u00b7fer", "um\u00b7ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie woltest du doch seyn gestorben?", "tokens": ["Wie", "wol\u00b7test", "du", "doch", "seyn", "ge\u00b7stor\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VAINF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ach leider mehr, ach mehr, als wahr!", "tokens": ["Ach", "lei\u00b7der", "mehr", ",", "ach", "mehr", ",", "als", "wahr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADV", "$,", "ITJ", "ADV", "$,", "KOUS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein treuer Joseph ist schon dar,", "tokens": ["Dein", "treu\u00b7er", "Jo\u00b7se\u00b7ph", "ist", "schon", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NE", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der deinen Leib durch Bitt' erworben,", "tokens": ["Der", "dei\u00b7nen", "Leib", "durch", "Bitt'", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der kleidt dich an und nimmt dich ab", "tokens": ["Der", "kleidt", "dich", "an", "und", "nimmt", "dich", "ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und legt dich in sein Garten-Grab.", "tokens": ["Und", "legt", "dich", "in", "sein", "Gar\u00b7ten\u00b7Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein gantzer Fel\u00df war au\u00dfgehauen,", "tokens": ["Ein", "gant\u00b7zer", "Fel\u00df", "war", "au\u00df\u00b7ge\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Drinn, Jesu, soll dein Ruhstet seyn;", "tokens": ["Drinn", ",", "Je\u00b7su", ",", "soll", "dein", "Ruhs\u00b7tet", "seyn", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NE", "$,", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie mu\u00df ich dich nach deiner Pein,", "tokens": ["Wie", "mu\u00df", "ich", "dich", "nach", "dei\u00b7ner", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Heyl, so hart gelegt anschauen?", "tokens": ["Mein", "Heyl", ",", "so", "hart", "ge\u00b7legt", "an\u00b7schau\u00b7en", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADJD", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "War denn kein Hertz vor dich nicht mehr,", "tokens": ["War", "denn", "kein", "Hertz", "vor", "dich", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "APPR", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das sich angab zu solcher Ehr?", "tokens": ["Das", "sich", "an\u00b7gab", "zu", "sol\u00b7cher", "Ehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach nein, es waren unser' Hertzen", "tokens": ["Ach", "nein", ",", "es", "wa\u00b7ren", "un\u00b7ser'", "Hert\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKANT", "$,", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weit h\u00e4rter, als kein Fel\u00df kan seyn,", "tokens": ["Weit", "h\u00e4r\u00b7ter", ",", "als", "kein", "Fel\u00df", "kan", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "KOUS", "PIAT", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ri\u00dfen sich vor Leid die Stein',", "tokens": ["Es", "ri\u00b7\u00dfen", "sich", "vor", "Leid", "die", "Stein'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wir empfinden keine Schmertzen.", "tokens": ["Und", "wir", "emp\u00b7fin\u00b7den", "kei\u00b7ne", "Schmert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Erd' erbebt, der Himmel zagt,", "tokens": ["Die", "Erd'", "er\u00b7bebt", ",", "der", "Him\u00b7mel", "zagt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Mensch ist, der dich nicht beklagt.", "tokens": ["Der", "Mensch", "ist", ",", "der", "dich", "nicht", "be\u00b7klagt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Im Garten fingstu an dein Leiden,", "tokens": ["Im", "Gar\u00b7ten", "fings\u00b7tu", "an", "dein", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Garten schlo\u00dftu deine Noth,", "tokens": ["Im", "Gar\u00b7ten", "schlo\u00df\u00b7tu", "dei\u00b7ne", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Garten fing erst an der Tod", "tokens": ["Im", "Gar\u00b7ten", "fing", "erst", "an", "der", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und endigten sich unsre Freuden:", "tokens": ["Und", "en\u00b7dig\u00b7ten", "sich", "uns\u00b7re", "Freu\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.5": {"text": "So mu\u00df darin des Todes Pein", "tokens": ["So", "mu\u00df", "da\u00b7rin", "des", "To\u00b7des", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PAV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bestritten und begraben seyn.", "tokens": ["Be\u00b7strit\u00b7ten", "und", "be\u00b7gra\u00b7ben", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "In bitter' Aloe und Myrrhen", "tokens": ["In", "bit\u00b7ter'", "A\u00b7loe", "und", "Myr\u00b7rhen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wirst, Jesu, du gewickelt ein,", "tokens": ["Wirst", ",", "Je\u00b7su", ",", "du", "ge\u00b7wi\u00b7ckelt", "ein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NE", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "So solstu unverwe\u00dflich seyn;", "tokens": ["So", "sols\u00b7tu", "un\u00b7ver\u00b7we\u00df\u00b7lich", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie schwer da\u00df deine Lieben irren:", "tokens": ["Wie", "schwer", "da\u00df", "dei\u00b7ne", "Lie\u00b7ben", "ir\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KOUS", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ist selbst die Unverwe\u00dflichkeit", "tokens": ["Ist", "selbst", "die", "Un\u00b7ver\u00b7we\u00df\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht von der F\u00e4ul' an sich befreyt?", "tokens": ["Nicht", "von", "der", "F\u00e4ul'", "an", "sich", "be\u00b7freyt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ja, Herr; doch ist uns dies erfreulich.", "tokens": ["Ja", ",", "Herr", ";", "doch", "ist", "uns", "dies", "er\u00b7freu\u00b7lich", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$.", "ADV", "VAFIN", "PPER", "PDS", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie schmertzlich schieden wir erst ab,", "tokens": ["Wie", "schmertz\u00b7lich", "schie\u00b7den", "wir", "erst", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie bitter war uns unser Grab!", "tokens": ["Wie", "bit\u00b7ter", "war", "uns", "un\u00b7ser", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun ist am Tode Nichts abscheulich.", "tokens": ["Nun", "ist", "am", "To\u00b7de", "Nichts", "ab\u00b7scheu\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du ziehest ihm den Stachel ab", "tokens": ["Du", "zie\u00b7hest", "ihm", "den", "Sta\u00b7chel", "ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und hast vers\u00fc\u00dfet unser Grab.", "tokens": ["Und", "hast", "ver\u00b7s\u00fc\u00b7\u00dfet", "un\u00b7ser", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Du nemlich bist der starke Leue,", "tokens": ["Du", "nem\u00b7lich", "bist", "der", "star\u00b7ke", "Leu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der unsre Feinde hat erlegt", "tokens": ["Der", "uns\u00b7re", "Fein\u00b7de", "hat", "er\u00b7legt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Honig in dem Tode hegt,", "tokens": ["Und", "Ho\u00b7nig", "in", "dem", "To\u00b7de", "hegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist der Pelikan, der treue,", "tokens": ["Du", "bist", "der", "Pe\u00b7li\u00b7kan", ",", "der", "treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der selber ihm die Brust auffritzt", "tokens": ["Der", "sel\u00b7ber", "ihm", "die", "Brust", "auf\u00b7fritzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Blut f\u00fcr seine Jungen schwitzt.", "tokens": ["Und", "Blut", "f\u00fcr", "sei\u00b7ne", "Jun\u00b7ge