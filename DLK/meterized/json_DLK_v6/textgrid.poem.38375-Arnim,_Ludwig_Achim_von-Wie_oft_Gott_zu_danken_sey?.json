{"textgrid.poem.38375": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Wie oft Gott zu danken sey?", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie viel Sand in dem Meer,", "tokens": ["Wie", "viel", "Sand", "in", "dem", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wie viel Sterne oben her,", "tokens": ["Wie", "viel", "Ster\u00b7ne", "o\u00b7ben", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie viel Thiere in der Welt,", "tokens": ["Wie", "viel", "Thie\u00b7re", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie viel Heller unterm Geld,", "tokens": ["Wie", "viel", "Hel\u00b7ler", "un\u00b7term", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In den Adern wie viel Blut,", "tokens": ["In", "den", "A\u00b7dern", "wie", "viel", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KOKOM", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In dem Feuer wie viel Glut,", "tokens": ["In", "dem", "Feu\u00b7er", "wie", "viel", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KOKOM", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie viel Bl\u00e4tter in den W\u00e4ldern,", "tokens": ["Wie", "viel", "Bl\u00e4t\u00b7ter", "in", "den", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie viel Gr\u00e4\u00dflein in den Feldern,", "tokens": ["Wie", "viel", "Gr\u00e4\u00df\u00b7lein", "in", "den", "Fel\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "In den Hecken wie viel D\u00f6rner,", "tokens": ["In", "den", "He\u00b7cken", "wie", "viel", "D\u00f6r\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KOKOM", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Auf dem Acker wie viel K\u00f6rner,", "tokens": ["Auf", "dem", "A\u00b7cker", "wie", "viel", "K\u00f6r\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KOKOM", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Auf den Wiesen wie viel Klee,", "tokens": ["Auf", "den", "Wie\u00b7sen", "wie", "viel", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KOKOM", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Wie viel St\u00e4ublein in der H\u00f6h,", "tokens": ["Wie", "viel", "St\u00e4ub\u00b7lein", "in", "der", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "In den Fl\u00fcssen wie viel Fischlein,", "tokens": ["In", "den", "Fl\u00fcs\u00b7sen", "wie", "viel", "Fisc\u00b7hlein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KOKOM", "PIAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.14": {"text": "In dem Meere wie viel M\u00fcschlein,", "tokens": ["In", "dem", "Mee\u00b7re", "wie", "viel", "M\u00fcsc\u00b7hlein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KOKOM", "PIAT", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Wie viel Tropfen in der See,", "tokens": ["Wie", "viel", "Trop\u00b7fen", "in", "der", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Wie viel Flocken in dem Schnee,", "tokens": ["Wie", "viel", "Flo\u00b7cken", "in", "dem", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "So viel Lebendig weit und breit,", "tokens": ["So", "viel", "Le\u00b7ben\u00b7dig", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.18": {"text": "So oft und viel sey Gott Dank in Ewigkeit.", "tokens": ["So", "oft", "und", "viel", "sey", "Gott", "Dank", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "VAFIN", "NN", "APPR", "APPR", "NN", "$."], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.19": {"text": "Amen", "tokens": ["A\u00b7men"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}}}}}