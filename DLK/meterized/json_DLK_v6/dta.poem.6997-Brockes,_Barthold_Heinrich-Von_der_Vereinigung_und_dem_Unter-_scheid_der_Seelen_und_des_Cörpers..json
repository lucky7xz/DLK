{"dta.poem.6997": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kan es der C\u00f6rper seyn, der offt sein eignes Leben,", "tokens": ["Kan", "es", "der", "C\u00f6r\u00b7per", "seyn", ",", "der", "offt", "sein", "eig\u00b7nes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VAINF", "$,", "PRELS", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus einem edlen Trieb, nicht scheut dahin zu geben?", "tokens": ["Aus", "ei\u00b7nem", "ed\u00b7len", "Trieb", ",", "nicht", "scheut", "da\u00b7hin", "zu", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PTKNEG", "VVFIN", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist er es, welcher trotzt, auch mitten in der Gluht,", "tokens": ["Ist", "er", "es", ",", "wel\u00b7cher", "trotzt", ",", "auch", "mit\u00b7ten", "in", "der", "Gluht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "PRELS", "VVFIN", "$,", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jm eisern M\u00f6rser selbst, der W\u00fcteriche Wuth?", "tokens": ["Jm", "ei\u00b7sern", "M\u00f6r\u00b7ser", "selbst", ",", "der", "W\u00fc\u00b7te\u00b7ri\u00b7che", "Wuth", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}