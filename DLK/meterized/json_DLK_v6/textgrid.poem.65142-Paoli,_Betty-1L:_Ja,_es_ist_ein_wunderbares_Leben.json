{"textgrid.poem.65142": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja, es ist ein wunderbares Leben", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, es ist ein wunderbares Leben", "tokens": ["Ja", ",", "es", "ist", "ein", "wun\u00b7der\u00b7ba\u00b7res", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das in dir, durch dich mir aufgegangen!", "tokens": ["Das", "in", "dir", ",", "durch", "dich", "mir", "auf\u00b7ge\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "$,", "APPR", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Unbezwinglich nach dem Gl\u00fcck Verlangen,", "tokens": ["Un\u00b7be\u00b7zwing\u00b7lich", "nach", "dem", "Gl\u00fcck", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Tiefster Demut freudig Sichbegeben!", "tokens": ["Tiefs\u00b7ter", "De\u00b7mut", "freu\u00b7dig", "Sich\u00b7be\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Aus dem Strahle deiner Augensterne", "tokens": ["Aus", "dem", "Strah\u00b7le", "dei\u00b7ner", "Au\u00b7gens\u00b7ter\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "M\u00f6cht' ich trinken ew'gen Lebens Fluten.", "tokens": ["M\u00f6cht'", "ich", "trin\u00b7ken", "ew'\u00b7gen", "Le\u00b7bens", "Flu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Ach und m\u00f6chte doch nicht minder gerne,", "tokens": ["Ach", "und", "m\u00f6ch\u00b7te", "doch", "nicht", "min\u00b7der", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KON", "VMFIN", "ADV", "PTKNEG", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Du Geliebter! dir im Arm verbluten!", "tokens": ["Du", "Ge\u00b7lieb\u00b7ter", "!", "dir", "im", "Arm", "ver\u00b7blu\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Mich mit s\u00fc\u00dfem Liebesband umstrickend", "tokens": ["Mich", "mit", "s\u00fc\u00b7\u00dfem", "Lie\u00b7bes\u00b7band", "um\u00b7stri\u00b7ckend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hast du an die Erde mich gekettet,", "tokens": ["Hast", "du", "an", "die", "Er\u00b7de", "mich", "ge\u00b7ket\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und dem Himmel hast du mich gerettet", "tokens": ["Und", "dem", "Him\u00b7mel", "hast", "du", "mich", "ge\u00b7ret\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "PRF", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Seine Ahnung tief in's Herz mir blickend.", "tokens": ["Sei\u00b7ne", "Ah\u00b7nung", "tief", "in's", "Herz", "mir", "bli\u00b7ckend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPRART", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Gleich gilt's, wie die Zukunft sich gestaltet,", "tokens": ["Gleich", "gilt's", ",", "wie", "die", "Zu\u00b7kunft", "sich", "ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ob mein Auge l\u00e4chelt oder weinet:", "tokens": ["Ob", "mein", "Au\u00b7ge", "l\u00e4\u00b7chelt", "o\u00b7der", "wei\u00b7net", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Eine Welt hast du in mir entfaltet", "tokens": ["Ei\u00b7ne", "Welt", "hast", "du", "in", "mir", "ent\u00b7fal\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Die den Himmel mit der Erde einet!", "tokens": ["Die", "den", "Him\u00b7mel", "mit", "der", "Er\u00b7de", "ei\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}