{"textgrid.poem.48195": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "8. Die Menschen lassen vieles gelten", "genre": "verse", "period": "N.A.", "pub_year": 1851, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Menschen lassen vieles gelten:", "tokens": ["Die", "Men\u00b7schen", "las\u00b7sen", "vie\u00b7les", "gel\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor allem lieben sie dich ", "tokens": ["Vor", "al\u00b7lem", "lie\u00b7ben", "sie", "dich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "PRF"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Doch willst du klagen, willst du schelten, \u2013", "tokens": ["Doch", "willst", "du", "kla\u00b7gen", ",", "willst", "du", "schel\u00b7ten", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "$,", "VMFIN", "PPER", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch das, man k\u00fcmmert sich nicht drum.", "tokens": ["Auch", "das", ",", "man", "k\u00fcm\u00b7mert", "sich", "nicht", "drum", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "$,", "PIS", "VVFIN", "PRF", "PTKNEG", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur, willst du rasch die Gunst verscherzen,", "tokens": ["Nur", ",", "willst", "du", "rasch", "die", "Gunst", "ver\u00b7scher\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VMFIN", "PPER", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So zeig ein F\u00fcnkchen Seligkeit, \u2013", "tokens": ["So", "zeig", "ein", "F\u00fcnk\u00b7chen", "Se\u00b7lig\u00b7keit", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man w\u00fcnscht dir Gl\u00fcck \u00bbvon ganzem Herzen\u00ab", "tokens": ["Man", "w\u00fcnscht", "dir", "Gl\u00fcck", "\u00bb", "von", "gan\u00b7zem", "Her\u00b7zen", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "NN", "$(", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und birst vor r\u00fcckgestautem Neid.", "tokens": ["Und", "birst", "vor", "r\u00fcck\u00b7ge\u00b7stau\u00b7tem", "Neid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}