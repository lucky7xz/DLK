{"dta.poem.9277": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das dritte Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Dj\u00df ist mein wunsch/ weil ich auff erden lebe:", "tokens": ["Dj\u00df", "ist", "mein", "wunsch", "/", "weil", "ich", "auff", "er\u00b7den", "le\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJD", "$(", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Gott gebe mir nicht allzu grosses gut:", "tokens": ["Gott", "ge\u00b7be", "mir", "nicht", "all\u00b7zu", "gros\u00b7ses", "gut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "PTKA", "ADJA", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Er schaffe nur was mir vonn\u00f6then thut/", "tokens": ["Er", "schaf\u00b7fe", "nur", "was", "mir", "von\u00b7n\u00f6\u00b7then", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PWS", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Damit ich nicht nach hohen dingen strebe", "tokens": ["Da\u00b7mit", "ich", "nicht", "nach", "ho\u00b7hen", "din\u00b7gen", "stre\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und gar zu sehr an diesem eitlen klebe.", "tokens": ["Und", "gar", "zu", "sehr", "an", "die\u00b7sem", "eit\u00b7len", "kle\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKA", "ADV", "APPR", "PDAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Versucht mich ja mein schn\u00f6des fleisch und blut/", "tokens": ["Ver\u00b7sucht", "mich", "ja", "mein", "schn\u00f6\u00b7des", "fleisch", "und", "blut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So komm er bald und breche meinen muth/", "tokens": ["So", "komm", "er", "bald", "und", "bre\u00b7che", "mei\u00b7nen", "muth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Damit das hertz im himmel offen schwebe.", "tokens": ["Da\u00b7mit", "das", "hertz", "im", "him\u00b7mel", "of\u00b7fen", "schwe\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPRART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der falsche glantz der stoltzen eitelkeit", "tokens": ["Der", "fal\u00b7sche", "glantz", "der", "stolt\u00b7zen", "ei\u00b7tel\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ertheilet nicht dergleichen sicherheit/", "tokens": ["Er\u00b7thei\u00b7let", "nicht", "derg\u00b7lei\u00b7chen", "si\u00b7cher\u00b7heit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PIS", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Als k\u00f6nte man des todes gifft vermeiden.", "tokens": ["Als", "k\u00f6n\u00b7te", "man", "des", "to\u00b7des", "gifft", "ver\u00b7mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PIS", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Was hilfft es nun da\u00df man die gantze welt", "tokens": ["Was", "hilfft", "es", "nun", "da\u00df", "man", "die", "gant\u00b7ze", "welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "KOUS", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Mit aller lust in seinen diensten h\u00e4lt/", "tokens": ["Mit", "al\u00b7ler", "lust", "in", "sei\u00b7nen", "diens\u00b7ten", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und mu\u00df die noth an seiner seele leiden.", "tokens": ["Und", "mu\u00df", "die", "noth", "an", "sei\u00b7ner", "see\u00b7le", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}