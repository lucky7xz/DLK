{"textgrid.poem.60606": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Lob des H\u00f6chsten", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Singt, singt mit heiligem Entz\u00fccken,", "tokens": ["Singt", ",", "singt", "mit", "hei\u00b7li\u00b7gem", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Singt unserm Gott ein neues Lied!", "tokens": ["Singt", "un\u00b7serm", "Gott", "ein", "neu\u00b7es", "Lied", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Herr ist gro\u00df! Ihn will ich preisen,", "tokens": ["Der", "Herr", "ist", "gro\u00df", "!", "Ihn", "will", "ich", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "PPER", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn, den G\u00fctigen, den Weisen,", "tokens": ["Ihn", ",", "den", "G\u00fc\u00b7ti\u00b7gen", ",", "den", "Wei\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dessen Auge nichts entflieht!", "tokens": ["Des\u00b7sen", "Au\u00b7ge", "nichts", "ent\u00b7flieht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der du den sternenvollen Himmel,", "tokens": ["Der", "du", "den", "ster\u00b7nen\u00b7vol\u00b7len", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ein Gezelt, weit ausgespannt,", "tokens": ["Wie", "ein", "Ge\u00b7zelt", ",", "weit", "aus\u00b7ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hier, umstrahlt von Sonnen, thronest,", "tokens": ["Und", "hier", ",", "um\u00b7strahlt", "von", "Son\u00b7nen", ",", "thro\u00b7nest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "$,", "VVFIN", "APPR", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hier in einem Lichte wohnest,", "tokens": ["Hier", "in", "ei\u00b7nem", "Lich\u00b7te", "woh\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo kein Sterblicher dich fand!", "tokens": ["Wo", "kein", "Sterb\u00b7li\u00b7cher", "dich", "fand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gott! ich verliere mich im Glanze;", "tokens": ["Gott", "!", "ich", "ver\u00b7lie\u00b7re", "mich", "im", "Glan\u00b7ze", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich, G\u00fctigster, verlier ich nie!", "tokens": ["Dich", ",", "G\u00fc\u00b7tigs\u00b7ter", ",", "ver\u00b7lier", "ich", "nie", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist auch unter uns zugegen;", "tokens": ["Du", "bist", "auch", "un\u00b7ter", "uns", "zu\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und entz\u00fcckt von deinen Wegen,", "tokens": ["Und", "ent\u00b7z\u00fcckt", "von", "dei\u00b7nen", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Voll Verwundrung preis' ich sie.", "tokens": ["Voll", "Ver\u00b7wund\u00b7rung", "preis'", "ich", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dich preis' ich, der du an die Erde", "tokens": ["Dich", "preis'", "ich", ",", "der", "du", "an", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit v\u00e4terlicher G\u00fcte denkst,", "tokens": ["Mit", "v\u00e4\u00b7ter\u00b7li\u00b7cher", "G\u00fc\u00b7te", "denkst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der du ihr in der Sonne leuchtest,", "tokens": ["Der", "du", "ihr", "in", "der", "Son\u00b7ne", "leuch\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und im Regen sie befeuchtest,", "tokens": ["Und", "im", "Re\u00b7gen", "sie", "be\u00b7feuch\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie mit k\u00fchlem Thaue tr\u00e4nkst:", "tokens": ["Sie", "mit", "k\u00fch\u00b7lem", "Thau\u00b7e", "tr\u00e4nkst", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Da\u00df frisches Gr\u00fcn um ihre Glieder,", "tokens": ["Da\u00df", "fri\u00b7sches", "Gr\u00fcn", "um", "ih\u00b7re", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Haupt mit jungen Bluhmen lacht,", "tokens": ["Ihr", "Haupt", "mit", "jun\u00b7gen", "Bluh\u00b7men", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihren m\u00fctterlichen R\u00fccken", "tokens": ["Und", "ih\u00b7ren", "m\u00fct\u00b7ter\u00b7li\u00b7chen", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Saat und milder Segen dr\u00fccken,", "tokens": ["Saat", "und", "mil\u00b7der", "Se\u00b7gen", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "J\u00e4hrlich mit verneuter Pracht.", "tokens": ["J\u00e4hr\u00b7lich", "mit", "ver\u00b7neu\u00b7ter", "Pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Denn du versorgest, was du schufest:", "tokens": ["Denn", "du", "ver\u00b7sor\u00b7gest", ",", "was", "du", "schu\u00b7fest", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dein kleinst Gesch\u00f6pf ist dir bekannt.", "tokens": ["Dein", "kleinst", "Ge\u00b7sch\u00f6pf", "ist", "dir", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der junge Rabe, der beschneyet", "tokens": ["Der", "jun\u00b7ge", "Ra\u00b7be", ",", "der", "be\u00b7schne\u00b7yet"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Hoch auf nackten Wipfeln schreyet,", "tokens": ["Hoch", "auf", "nack\u00b7ten", "Wip\u00b7feln", "schre\u00b7yet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "S\u00e4ttigt sich aus deiner Hand.", "tokens": ["S\u00e4t\u00b7tigt", "sich", "aus", "dei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Du bists, der zwischen rauhen Bergen", "tokens": ["Du", "bists", ",", "der", "zwi\u00b7schen", "rau\u00b7hen", "Ber\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erfrischend Wasser quellen l\u00e4\u00dft,", "tokens": ["Er\u00b7fri\u00b7schend", "Was\u00b7ser", "quel\u00b7len", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sonnenreichen H\u00f6hen Reben,", "tokens": ["Und", "son\u00b7nen\u00b7rei\u00b7chen", "H\u00f6\u00b7hen", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "B\u00e4umen ihre Frucht gegeben,", "tokens": ["B\u00e4u\u00b7men", "ih\u00b7re", "Frucht", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gr\u00fcnen W\u00e4ldern ihren West.", "tokens": ["Gr\u00fc\u00b7nen", "W\u00e4l\u00b7dern", "ih\u00b7ren", "West", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Zur Arbeit winket den Gesch\u00f6pfen", "tokens": ["Zur", "Ar\u00b7beit", "win\u00b7ket", "den", "Ge\u00b7sch\u00f6p\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tag aus strahlenvoller Luft:", "tokens": ["Der", "Tag", "aus", "strah\u00b7len\u00b7vol\u00b7ler", "Luft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis, unter dunkler Schatten H\u00fclle,", "tokens": ["Bis", ",", "un\u00b7ter", "dunk\u00b7ler", "Schat\u00b7ten", "H\u00fcl\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "K\u00fchler N\u00e4chte sanfte Stille", "tokens": ["K\u00fch\u00b7ler", "N\u00e4ch\u00b7te", "sanf\u00b7te", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zur gew\u00fcnschten Ruhe ruft.", "tokens": ["Zur", "ge\u00b7w\u00fcnschten", "Ru\u00b7he", "ruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Doch fr\u00fch erwacht zu Dank und Liedern", "tokens": ["Doch", "fr\u00fch", "er\u00b7wacht", "zu", "Dank", "und", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der V\u00f6gel buntgefiedert Chor.", "tokens": ["Der", "V\u00f6\u00b7gel", "bunt\u00b7ge\u00b7fie\u00b7dert", "Chor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann steigt von allen Nationen,", "tokens": ["Dann", "steigt", "von", "al\u00b7len", "Na\u00b7ti\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Steigt aus aller Himmel Zonen", "tokens": ["Steigt", "aus", "al\u00b7ler", "Him\u00b7mel", "Zo\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dir ein Lobgesang empor:", "tokens": ["Dir", "ein", "Lob\u00b7ge\u00b7sang", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Dir, gro\u00dfer Vater aller Wesen,", "tokens": ["Dir", ",", "gro\u00b7\u00dfer", "Va\u00b7ter", "al\u00b7ler", "We\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der allen wohlthut, alle liebt,", "tokens": ["Der", "al\u00b7len", "wohlt\u00b7hut", ",", "al\u00b7le", "liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und will, da\u00df alle, wenn sie wollen,", "tokens": ["Und", "will", ",", "da\u00df", "al\u00b7le", ",", "wenn", "sie", "wol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$,", "KOUS", "PIS", "$,", "KOUS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Alle gl\u00fccklich werden sollen,", "tokens": ["Al\u00b7le", "gl\u00fcck\u00b7lich", "wer\u00b7den", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "VAINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denen er das Leben giebt:", "tokens": ["De\u00b7nen", "er", "das", "Le\u00b7ben", "giebt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Damit sein Name herrlich werde", "tokens": ["Da\u00b7mit", "sein", "Na\u00b7me", "herr\u00b7lich", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PPOSAT", "NN", "ADJD", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch alle Welten, sein Gebieth,", "tokens": ["Durch", "al\u00b7le", "Wel\u00b7ten", ",", "sein", "Ge\u00b7bieth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihn, den G\u00fctigen, den Weisen,", "tokens": ["Und", "ihn", ",", "den", "G\u00fc\u00b7ti\u00b7gen", ",", "den", "Wei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Alle Zungen dankbar preisen,", "tokens": ["Al\u00b7le", "Zun\u00b7gen", "dank\u00b7bar", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch ein allgemeines Lied!", "tokens": ["Durch", "ein", "all\u00b7ge\u00b7mei\u00b7nes", "Lied", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}