{"textgrid.poem.59878": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies soll euch jetzt als neuestes Gebot", "tokens": ["Dies", "soll", "euch", "jetzt", "als", "neu\u00b7es\u00b7tes", "Ge\u00b7bot"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verk\u00fcndigt werden:", "tokens": ["Ver\u00b7k\u00fcn\u00b7digt", "wer\u00b7den", ":"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Es soll auf Erden nicht mehr ohne Not", "tokens": ["Es", "soll", "auf", "Er\u00b7den", "nicht", "mehr", "oh\u00b7ne", "Not"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "PTKNEG", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ges\u00fcndigt werden!", "tokens": ["Ge\u00b7s\u00fcn\u00b7digt", "wer\u00b7den", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wo nicht ein s\u00fc\u00dfer Mund, ein sch\u00f6nes Auge", "tokens": ["Wo", "nicht", "ein", "s\u00fc\u00b7\u00dfer", "Mund", ",", "ein", "sch\u00f6\u00b7nes", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verlangen weckt, \u2013", "tokens": ["Ver\u00b7lan\u00b7gen", "weckt", ",", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "$,", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Da soll den S\u00fcndern alle Gnade nun", "tokens": ["Da", "soll", "den", "S\u00fcn\u00b7dern", "al\u00b7le", "Gna\u00b7de", "nun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gek\u00fcndigt werden!", "tokens": ["Ge\u00b7k\u00fcn\u00b7digt", "wer\u00b7den", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Jedweder Mund, der sich in schlechten K\u00fcssen", "tokens": ["Jed\u00b7we\u00b7der", "Mund", ",", "der", "sich", "in", "schlech\u00b7ten", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PRELS", "PRF", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Vers\u00fcndigt hat,", "tokens": ["Ver\u00b7s\u00fcn\u00b7digt", "hat", ","], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Kann nur durch eine Flut von echten K\u00fcssen", "tokens": ["Kann", "nur", "durch", "ei\u00b7ne", "Flut", "von", "ech\u00b7ten", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ents\u00fcndigt werden!", "tokens": ["Ent\u00b7s\u00fcn\u00b7digt", "wer\u00b7den", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}