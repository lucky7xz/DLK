{"textgrid.poem.62806": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Frau L\u00f6win kam im Cedernwald", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frau L\u00f6win kam im Cedernwald", "tokens": ["Frau", "L\u00f6\u00b7win", "kam", "im", "Ce\u00b7dern\u00b7wald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "APPRART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mit einem Kn\u00e4blein wohlgestalt", "tokens": ["Mit", "ei\u00b7nem", "Kn\u00e4\u00b7blein", "wohl\u00b7ge\u00b7stalt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ins erste Wochenbette.", "tokens": ["Ins", "ers\u00b7te", "Wo\u00b7chen\u00b7bet\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da war im ganzen Reich kein Thier,", "tokens": ["Da", "war", "im", "gan\u00b7zen", "Reich", "kein", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das nicht dem Prinzen oder ihr", "tokens": ["Das", "nicht", "dem", "Prin\u00b7zen", "o\u00b7der", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PTKNEG", "ART", "NN", "KON", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was eingebunden h\u00e4tte.", "tokens": ["Was", "ein\u00b7ge\u00b7bun\u00b7den", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Esel trat zuerst herbey", "tokens": ["Der", "E\u00b7sel", "trat", "zu\u00b7erst", "her\u00b7bey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sang mit bardischem Geschrey", "tokens": ["Und", "sang", "mit", "bar\u00b7di\u00b7schem", "Ge\u00b7schrey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Lied zu beyder Lobe.", "tokens": ["Ein", "Lied", "zu", "bey\u00b7der", "Lo\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sogar gedruckt verehrt ers ihr.", "tokens": ["So\u00b7gar", "ge\u00b7druckt", "ver\u00b7ehrt", "ers", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VVFIN", "PIS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gut, sprach sie, das ist zart Papier,", "tokens": ["Gut", ",", "sprach", "sie", ",", "das", "ist", "zart", "Pa\u00b7pier", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "PPER", "$,", "PDS", "VAFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Tragts in die Garderobe.", "tokens": ["Tragts", "in", "die", "Gar\u00b7de\u00b7ro\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Drauf go\u00df der Tieger wohlgemuth", "tokens": ["Drauf", "go\u00df", "der", "Tie\u00b7ger", "wohl\u00b7ge\u00b7muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drey L\u00f6ffel voll von seinem Blut", "tokens": ["Drey", "L\u00f6f\u00b7fel", "voll", "von", "sei\u00b7nem", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem L\u00f6wchen in den Rachen:", "tokens": ["Dem", "L\u00f6w\u00b7chen", "in", "den", "Ra\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nun kannst du kalt auf Leichen stehn,", "tokens": ["Nun", "kannst", "du", "kalt", "auf", "Lei\u00b7chen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Rief er, und ohne wegzusehn", "tokens": ["Rief", "er", ",", "und", "oh\u00b7ne", "weg\u00b7zu\u00b7sehn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "APPR", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Unschuld Thr\u00e4nen lachen.", "tokens": ["Der", "Un\u00b7schuld", "Thr\u00e4\u00b7nen", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Herr Fuchs strich seinen Schwanz mit Lust", "tokens": ["Herr", "Fuchs", "strich", "sei\u00b7nen", "Schwanz", "mit", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Kind auf Stirne, Mund und Brust", "tokens": ["Dem", "Kind", "auf", "Stir\u00b7ne", ",", "Mund", "und", "Brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sprach: Erlauchter Knabe,", "tokens": ["Und", "sprach", ":", "Er\u00b7lauch\u00b7ter", "Kna\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dir bring ich den Machiavell,", "tokens": ["Dir", "bring", "ich", "den", "Ma\u00b7chi\u00b7a\u00b7vell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gebunden in ein L\u00e4mmerfell,", "tokens": ["Ge\u00b7bun\u00b7den", "in", "ein", "L\u00e4m\u00b7mer\u00b7fell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zur treuen Opfergabe,", "tokens": ["Zur", "treu\u00b7en", "Op\u00b7fer\u00b7ga\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Gleich einem Stutzer balsamiert", "tokens": ["Gleich", "ei\u00b7nem", "Stut\u00b7zer", "bal\u00b7sa\u00b7miert"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lie\u00df nun der Gei\u00dfbock hoch frisiert", "tokens": ["Lie\u00df", "nun", "der", "Gei\u00df\u00b7bock", "hoch", "fri\u00b7siert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich m\u00e4ckernd also h\u00f6ren:", "tokens": ["Sich", "m\u00e4\u00b7ckernd", "al\u00b7so", "h\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nimm hin die Kunst zum Zeitvertreib", "tokens": ["Nimm", "hin", "die", "Kunst", "zum", "Zeit\u00b7ver\u00b7treib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Wittwe Kind, des Armen Weib,", "tokens": ["Der", "Witt\u00b7we", "Kind", ",", "des", "Ar\u00b7men", "Weib", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hochf\u00fcrstlich zu entehren.", "tokens": ["Hoch\u00b7f\u00fcrst\u00b7lich", "zu", "en\u00b7teh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Das n\u00f6thigste Geschenk, versetzt", "tokens": ["Das", "n\u00f6\u00b7thigs\u00b7te", "Ge\u00b7schenk", ",", "ver\u00b7setzt"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Salamander, k\u00f6mmt zuletzt;", "tokens": ["Der", "Sa\u00b7la\u00b7man\u00b7der", ",", "k\u00f6mmt", "zu\u00b7letzt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier bring ich Molchpomade:", "tokens": ["Hier", "bring", "ich", "Molch\u00b7po\u00b7ma\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur brav das Herrchen mit geschmiert,", "tokens": ["Nur", "brav", "das", "Herr\u00b7chen", "mit", "ge\u00b7schmiert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf da\u00df ihm, wenn es einst krepirt,", "tokens": ["Auf", "da\u00df", "ihm", ",", "wenn", "es", "einst", "kre\u00b7pirt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der H\u00f6lle Glut nicht schade.", "tokens": ["Der", "H\u00f6l\u00b7le", "Glut", "nicht", "scha\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}