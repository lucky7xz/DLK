{"dta.poem.924": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Im DORF", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Aus braunen Mauern tritt ein Dorf, ein Feld.", "tokens": ["Aus", "brau\u00b7nen", "Mau\u00b7ern", "tritt", "ein", "Dorf", ",", "ein", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Hirt verwest auf einem alten Stein.", "tokens": ["Ein", "Hirt", "ver\u00b7west", "auf", "ei\u00b7nem", "al\u00b7ten", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Saum des Walds schlie\u00dft blaue Tiere ein,", "tokens": ["Der", "Saum", "des", "Walds", "schlie\u00dft", "blau\u00b7e", "Tie\u00b7re", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das sanfte Laub, das in die Stille f\u00e4llt.", "tokens": ["Das", "sanf\u00b7te", "Laub", ",", "das", "in", "die", "Stil\u00b7le", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Bauern braune Stirnen. Lange t\u00f6nt", "tokens": ["Der", "Bau\u00b7ern", "brau\u00b7ne", "Stir\u00b7nen", ".", "Lan\u00b7ge", "t\u00f6nt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Abendglocke; sch\u00f6n ist frommer Brauch,", "tokens": ["Die", "A\u00b7bend\u00b7glo\u00b7cke", ";", "sch\u00f6n", "ist", "from\u00b7mer", "Brauch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADJD", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Heilands schwarzes Haupt im Dornenstrauch", "tokens": ["Des", "Hei\u00b7lands", "schwar\u00b7zes", "Haupt", "im", "Dor\u00b7nen\u00b7strauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die k\u00fchle Stube, die der Tod vers\u00f6hnt.", "tokens": ["Die", "k\u00fch\u00b7le", "Stu\u00b7be", ",", "die", "der", "Tod", "ver\u00b7s\u00f6hnt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie bleich die M\u00fctter sind. Die Bl\u00e4ue sinkt", "tokens": ["Wie", "bleich", "die", "M\u00fct\u00b7ter", "sind", ".", "Die", "Bl\u00e4u\u00b7e", "sinkt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "VAFIN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf Glas und Truh, die stolz ihr Sinn bewahrt;", "tokens": ["Auf", "Glas", "und", "Truh", ",", "die", "stolz", "ihr", "Sinn", "be\u00b7wahrt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PRELS", "ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auch neigt ein wei\u00dfes Haupt sich hochbejahrt", "tokens": ["Auch", "neigt", "ein", "wei\u00b7\u00dfes", "Haupt", "sich", "hoch\u00b7be\u00b7jahrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PRF", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aufs Enkelkind, das Milch und Sterne trinkt.", "tokens": ["Aufs", "En\u00b7kel\u00b7kind", ",", "das", "Milch", "und", "Ster\u00b7ne", "trinkt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Arme, der im Geiste einsam starb,", "tokens": ["Der", "Ar\u00b7me", ",", "der", "im", "Geis\u00b7te", "ein\u00b7sam", "starb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Steigt w\u00e4chsern \u00fcber einen alten Pfad.", "tokens": ["Steigt", "w\u00e4ch\u00b7sern", "\u00fc\u00b7ber", "ei\u00b7nen", "al\u00b7ten", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Apfelb\u00e4ume sinken kahl und stad", "tokens": ["Die", "Ap\u00b7fel\u00b7b\u00e4u\u00b7me", "sin\u00b7ken", "kahl", "und", "stad"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ins Farbige ihrer Frucht, die schwarz verdarb.", "tokens": ["Ins", "Far\u00b7bi\u00b7ge", "ih\u00b7rer", "Frucht", ",", "die", "schwarz", "ver\u00b7darb", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Noch immer w\u00f6lbt das Dach aus d\u00fcrrem Stroh", "tokens": ["Noch", "im\u00b7mer", "w\u00f6lbt", "das", "Dach", "aus", "d\u00fcr\u00b7rem", "Stroh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sich \u00fcbern Schlaf der K\u00fche. Die blinde Magd", "tokens": ["Sich", "\u00fc\u00b7bern", "Schlaf", "der", "K\u00fc\u00b7he", ".", "Die", "blin\u00b7de", "Magd"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "APPRART", "NN", "ART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Erscheint im Hof; ein blaues Wasser klagt;", "tokens": ["Er\u00b7scheint", "im", "Hof", ";", "ein", "blau\u00b7es", "Was\u00b7ser", "klagt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$.", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Pferdesch\u00e4del starrt vom morschen Tor.", "tokens": ["Ein", "Pfer\u00b7de\u00b7sch\u00e4\u00b7del", "starrt", "vom", "mor\u00b7schen", "Tor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Der Idiot spricht dunklen Sinns ein Wort", "tokens": ["Der", "I\u00b7diot", "spricht", "dunk\u00b7len", "Sinns", "ein", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Liebe, das im schwarzen Busch verhallt,", "tokens": ["Der", "Lie\u00b7be", ",", "das", "im", "schwar\u00b7zen", "Busch", "ver\u00b7hallt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo jene steht in schmaler Traumgestalt.", "tokens": ["Wo", "je\u00b7ne", "steht", "in", "schma\u00b7ler", "Traum\u00b7ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Abend t\u00f6nt in feuchter Bl\u00e4ue fort.", "tokens": ["Der", "A\u00b7bend", "t\u00f6nt", "in", "feuch\u00b7ter", "Bl\u00e4u\u00b7e", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ans Fenster schlagen \u00c4ste f\u00f6hnentlaubt.", "tokens": ["Ans", "Fens\u00b7ter", "schla\u00b7gen", "\u00c4s\u00b7te", "f\u00f6h\u00b7nent\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Scho\u00df der B\u00e4urin w\u00e4chst ein wildes Weh.", "tokens": ["Im", "Scho\u00df", "der", "B\u00e4u\u00b7rin", "w\u00e4chst", "ein", "wil\u00b7des", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch ihre Arme rieselt schwarzer Schnee;", "tokens": ["Durch", "ih\u00b7re", "Ar\u00b7me", "rie\u00b7selt", "schwar\u00b7zer", "Schnee", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gold\u00e4ugige Eulen flattern um ihr Haupt.", "tokens": ["Gold\u00b7\u00e4u\u00b7gi\u00b7ge", "Eu\u00b7len", "flat\u00b7tern", "um", "ihr", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "+---+-+-+-+", "measure": "dactylic.init"}}, "stanza.8": {"line.1": {"text": "Die Mauern starren kahl und grauverdreckt", "tokens": ["Die", "Mau\u00b7ern", "star\u00b7ren", "kahl", "und", "grau\u00b7ver\u00b7dreckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ins k\u00fchle Dunkel. Im Fieberbette friert", "tokens": ["Ins", "k\u00fch\u00b7le", "Dun\u00b7kel", ".", "Im", "Fie\u00b7ber\u00b7bet\u00b7te", "friert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "APPRART", "NN", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der schwangere Leib, den frech der Mond bestiert.", "tokens": ["Der", "schwan\u00b7ge\u00b7re", "Leib", ",", "den", "frech", "der", "Mond", "be\u00b7stiert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Vor ihrer Kammer ist ein Hund verreckt.", "tokens": ["Vor", "ih\u00b7rer", "Kam\u00b7mer", "ist", "ein", "Hund", "ver\u00b7reckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Drei M\u00e4nner treten finster durch das Tor", "tokens": ["Drei", "M\u00e4n\u00b7ner", "tre\u00b7ten", "fins\u00b7ter", "durch", "das", "Tor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Sensen, die im Feld zerbrochen sind.", "tokens": ["Mit", "Sen\u00b7sen", ",", "die", "im", "Feld", "zer\u00b7bro\u00b7chen", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durchs Fenster klirrt der rote Abendwind;", "tokens": ["Durchs", "Fens\u00b7ter", "klirrt", "der", "ro\u00b7te", "Ab\u00b7end\u00b7wind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein schwarzer Engel tritt daraus hervor.", "tokens": ["Ein", "schwar\u00b7zer", "En\u00b7gel", "tritt", "da\u00b7raus", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}