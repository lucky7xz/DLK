{"textgrid.poem.33682": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Treuegel\u00f6bnis", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So soll es sein:", "tokens": ["So", "soll", "es", "sein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ich lebe dein,", "tokens": ["Ich", "le\u00b7be", "dein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dein Stab und deine St\u00fctze,", "tokens": ["Dein", "Stab", "und", "dei\u00b7ne", "St\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df jederzeit", "tokens": ["Da\u00df", "je\u00b7der\u00b7zeit"], "token_info": ["word", "word"], "pos": ["KOUS", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "In Sturm und Streit", "tokens": ["In", "Sturm", "und", "Streit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Dich meine Treu' besch\u00fctze.", "tokens": ["Dich", "mei\u00b7ne", "Treu'", "be\u00b7sch\u00fct\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In Streit und Schmerz", "tokens": ["In", "Streit", "und", "Schmerz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ein treues Herz \u2013", "tokens": ["Ein", "treu\u00b7es", "Herz", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So sollst du stets mich kennen.", "tokens": ["So", "sollst", "du", "stets", "mich", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein fremdes Gl\u00fcck", "tokens": ["Kein", "frem\u00b7des", "Gl\u00fcck"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Soll mein Geschick", "tokens": ["Soll", "mein", "Ge\u00b7schick"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Von deinem Lose trennen.", "tokens": ["Von", "dei\u00b7nem", "Lo\u00b7se", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gern steig' ich an", "tokens": ["Gern", "steig'", "ich", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Auf rauher Bahn,", "tokens": ["Auf", "rau\u00b7her", "Bahn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wei\u00df ich nur dich im Frieden.", "tokens": ["Wei\u00df", "ich", "nur", "dich", "im", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei dir allein", "tokens": ["Bei", "dir", "al\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ist Sonnenschein", "tokens": ["Ist", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word"], "pos": ["VAFIN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und all mein Gl\u00fcck hienieden.", "tokens": ["Und", "all", "mein", "Gl\u00fcck", "hien\u00b7ie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}