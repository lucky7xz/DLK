{"textgrid.poem.59401": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Kranke", "genre": "verse", "period": "N.A.", "pub_year": 1808, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soll ich dich denn nun verlassen,", "tokens": ["Soll", "ich", "dich", "denn", "nun", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Erde, heitres Vaterhaus?", "tokens": ["Er\u00b7de", ",", "heit\u00b7res", "Va\u00b7ter\u00b7haus", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herzlich Lieben, mutig Hassen,", "tokens": ["Herz\u00b7lich", "Lie\u00b7ben", ",", "mu\u00b7tig", "Has\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist denn alles, alles aus?", "tokens": ["Ist", "denn", "al\u00b7les", ",", "al\u00b7les", "aus", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "$,", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Vor dem Fenster durch die Linden", "tokens": ["Vor", "dem", "Fens\u00b7ter", "durch", "die", "Lin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spielt es wie ein linder Gru\u00df,", "tokens": ["Spielt", "es", "wie", "ein", "lin\u00b7der", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00fcfte, wollt ihr mir verk\u00fcnden,", "tokens": ["L\u00fcf\u00b7te", ",", "wollt", "ihr", "mir", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich bald hinunter mu\u00df? \u2013", "tokens": ["Da\u00df", "ich", "bald", "hin\u00b7un\u00b7ter", "mu\u00df", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VMFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Liebe, ferne, blaue H\u00fcgel,", "tokens": ["Lie\u00b7be", ",", "fer\u00b7ne", ",", "blau\u00b7e", "H\u00fc\u00b7gel", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stiller Flu\u00df im Talesgr\u00fcn,", "tokens": ["Stil\u00b7ler", "Flu\u00df", "im", "Ta\u00b7les\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, wie oft w\u00fcnscht ich mir Fl\u00fcgel,", "tokens": ["Ach", ",", "wie", "oft", "w\u00fcnscht", "ich", "mir", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADV", "VVFIN", "PPER", "PPER", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "\u00dcber euch hinwegzuziehn!", "tokens": ["\u00dc\u00b7ber", "euch", "hin\u00b7weg\u00b7zu\u00b7ziehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Da sich jetzt die Fl\u00fcgel dehnen", "tokens": ["Da", "sich", "jetzt", "die", "Fl\u00fc\u00b7gel", "deh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ADV", "ART", "NN", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Schaur ich in mich selbst zur\u00fcck,", "tokens": ["Schaur", "ich", "in", "mich", "selbst", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein unbeschreiblich Sehnen", "tokens": ["Und", "ein", "un\u00b7be\u00b7schreib\u00b7lich", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zieht mich zu der Welt zur\u00fcck.", "tokens": ["Zieht", "mich", "zu", "der", "Welt", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}