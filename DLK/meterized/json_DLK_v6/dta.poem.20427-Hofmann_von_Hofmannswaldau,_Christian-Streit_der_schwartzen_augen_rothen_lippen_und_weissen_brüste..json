{"dta.poem.20427": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Streit der schwartzen augen/ rothen  \n lippen/ und weissen br\u00fcste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Di\u00df/ was ihr itzt ger\u00fchmt/ das findt ihr hier begraben;", "tokens": ["Di\u00df", "/", "was", "ihr", "itzt", "ge\u00b7r\u00fchmt", "/", "das", "findt", "ihr", "hier", "be\u00b7gra\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$(", "PWS", "PPER", "ADV", "VVPP", "$(", "PDS", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des himmels rundes bild/ der rosen lieblichkeit/", "tokens": ["Des", "him\u00b7mels", "run\u00b7des", "bild", "/", "der", "ro\u00b7sen", "lieb\u00b7lich\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des fr\u00fchlings bunte lust/ des sommers s\u00fcsse gaben/", "tokens": ["Des", "fr\u00fch\u00b7lings", "bun\u00b7te", "lust", "/", "des", "som\u00b7mers", "s\u00fcs\u00b7se", "ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$(", "ART", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die sind mit reicher hand hier kr\u00e4fftig eingestreut.", "tokens": ["Die", "sind", "mit", "rei\u00b7cher", "hand", "hier", "kr\u00e4ff\u00b7tig", "ein\u00b7ge\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ADJD", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der brand-befreyte schnee kan felsen selbst entz\u00fcnden/", "tokens": ["Der", "bran\u00b7dbe\u00b7frey\u00b7te", "schnee", "kan", "fel\u00b7sen", "selbst", "ent\u00b7z\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und unsre blumen tilgt kein heisser sonnenschein;", "tokens": ["Und", "uns\u00b7re", "blu\u00b7men", "tilgt", "kein", "heis\u00b7ser", "son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Cupido wird sich uns zu loben unterwinden/", "tokens": ["Cu\u00b7pi\u00b7do", "wird", "sich", "uns", "zu", "lo\u00b7ben", "un\u00b7ter\u00b7win\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PRF", "PPER", "PTKZU", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die feder wird sein pfe", "tokens": ["Die", "fe\u00b7der", "wird", "sein", "pfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}