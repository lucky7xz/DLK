{"dta.poem.12122": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein eigner Anblick ists, im sommerlichen Thal", "tokens": ["Ein", "eig\u00b7ner", "An\u00b7blick", "ists", ",", "im", "som\u00b7mer\u00b7li\u00b7chen", "Thal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die nackten Schnitter sehn, gebr\u00e4unt vom hei\u00dfen Stral,", "tokens": ["Die", "nack\u00b7ten", "Schnit\u00b7ter", "sehn", ",", "ge\u00b7br\u00e4unt", "vom", "hei\u00b7\u00dfen", "Stral", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und dr\u00fcber hoch herein der Alpe Schneefeld hangen,", "tokens": ["Und", "dr\u00fc\u00b7ber", "hoch", "her\u00b7ein", "der", "Al\u00b7pe", "Schnee\u00b7feld", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADJD", "PTKVZ", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So nah, da\u00df man es meint mit H\u00e4nden zu erlangen.", "tokens": ["So", "nah", ",", "da\u00df", "man", "es", "meint", "mit", "H\u00e4n\u00b7den", "zu", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PIS", "PPER", "VVFIN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es schmilzt nicht von der Glut, und bleibt dort ewig k\u00fchl,", "tokens": ["Es", "schmilzt", "nicht", "von", "der", "Glut", ",", "und", "bleibt", "dort", "e\u00b7wig", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch k\u00fchlt sein Anblick nicht, und macht hier doppelt schw\u00fcl.", "tokens": ["Doch", "k\u00fchlt", "sein", "An\u00b7blick", "nicht", ",", "und", "macht", "hier", "dop\u00b7pelt", "schw\u00fcl", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$,", "KON", "VVFIN", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}