{"textgrid.poem.63369": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Weihnacht", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin der Tischler Josef,", "tokens": ["Ich", "bin", "der", "Tischler", "Jo\u00b7sef", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Meine Frau, die hei\u00dfet Marie.", "tokens": ["Mei\u00b7ne", "Frau", ",", "die", "hei\u00b7\u00dfet", "Ma\u00b7rie", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVFIN", "NE", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wir finden kein' Arbeit und Herberg'", "tokens": ["Wir", "fin\u00b7den", "kein'", "Ar\u00b7beit", "und", "Her\u00b7ber\u00b7g'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Im kalten Winter allhie.", "tokens": ["Im", "kal\u00b7ten", "Win\u00b7ter", "all\u00b7hie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Habens der Herr Wirt vom goldnen Stern", "tokens": ["Ha\u00b7bens", "der", "Herr", "Wirt", "vom", "gold\u00b7nen", "Stern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN", "APPRART", "ADJA", "NN"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Nicht ein Unterkunft f\u00fcr mein Weib?", "tokens": ["Nicht", "ein", "Un\u00b7ter\u00b7kunft", "f\u00fcr", "mein", "Weib", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Einen halbeten Kreuzer zahlert ich gern,", "tokens": ["Ei\u00b7nen", "hal\u00b7be\u00b7ten", "Kreu\u00b7zer", "zah\u00b7lert", "ich", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Zu betten den schwangren Leib. \u2013", "tokens": ["Zu", "bet\u00b7ten", "den", "schwang\u00b7ren", "Leib", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich hab kein Bett f\u00fcr Bettelleut;", "tokens": ["Ich", "hab", "kein", "Bett", "f\u00fcr", "Bet\u00b7tel\u00b7leut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch scherts euch nur in den Stall.", "tokens": ["Doch", "scherts", "euch", "nur", "in", "den", "Stall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Gevatter Ochs und Base Kuh", "tokens": ["Ge\u00b7vat\u00b7ter", "Ochs", "und", "Ba\u00b7se", "Kuh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Werden empfangen euch wohl. \u2013", "tokens": ["Wer\u00b7den", "emp\u00b7fan\u00b7gen", "euch", "wohl", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "VVPP", "PPER", "ADV", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Wir danken dem Herrn Wirt f\u00fcr seine Gnad", "tokens": ["Wir", "dan\u00b7ken", "dem", "Herrn", "Wirt", "f\u00fcr", "sei\u00b7ne", "Gnad"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und f\u00fcr die warme Stub.", "tokens": ["Und", "f\u00fcr", "die", "war\u00b7me", "Stub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Himmel lohns euch und unser Kind,", "tokens": ["Der", "Him\u00b7mel", "lohns", "euch", "und", "un\u00b7ser", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Seis Madel oder Bub.", "tokens": ["Seis", "Ma\u00b7del", "o\u00b7der", "Bub", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Marie, Marie, was schreist du so sehr? \u2013", "tokens": ["Ma\u00b7rie", ",", "Ma\u00b7rie", ",", "was", "schreist", "du", "so", "sehr", "?", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "NE", "$,", "PWS", "VVFIN", "PPER", "ADV", "ADV", "$.", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ach Josef, es sein die Wehn.", "tokens": ["Ach", "Jo\u00b7sef", ",", "es", "sein", "die", "Wehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPER", "PPOSAT", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bald wirst du den elfenbeinernen Turm,", "tokens": ["Bald", "wirst", "du", "den", "el\u00b7fen\u00b7bei\u00b7ner\u00b7nen", "Turm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das s\u00fc\u00dfeste Wunder sehn. \u2013", "tokens": ["Das", "s\u00fc\u00b7\u00dfes\u00b7te", "Wun\u00b7der", "sehn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Der Josef Hebamme und Bader war", "tokens": ["Der", "Jo\u00b7sef", "He\u00b7bam\u00b7me", "und", "Ba\u00b7der", "war"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "NN", "KON", "NN", "VAFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und hob den lieben Sohn", "tokens": ["Und", "hob", "den", "lie\u00b7ben", "Sohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus seiner Mutter dunklem Reich", "tokens": ["Aus", "sei\u00b7ner", "Mut\u00b7ter", "dunk\u00b7lem", "Reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf seinen strohernen Thron.", "tokens": ["Auf", "sei\u00b7nen", "stro\u00b7her\u00b7nen", "Thron", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Da lag er im Stroh. Die Mutter so froh", "tokens": ["Da", "lag", "er", "im", "Stroh", ".", "Die", "Mut\u00b7ter", "so", "froh"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$.", "ART", "NN", "ADV", "ADJD"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sagt Vater Unserm den Dank.", "tokens": ["Sagt", "Va\u00b7ter", "Un\u00b7serm", "den", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und Ochs und Esel und Pferd und Hund", "tokens": ["Und", "Ochs", "und", "E\u00b7sel", "und", "Pferd", "und", "Hund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Standen fromm dabei.", "tokens": ["Stan\u00b7den", "fromm", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PAV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Aber die Katze sprang auf die Streu", "tokens": ["A\u00b7ber", "die", "Kat\u00b7ze", "sprang", "auf", "die", "Streu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und w\u00e4rmte zur Nacht das Kind. \u2013", "tokens": ["Und", "w\u00e4rm\u00b7te", "zur", "Nacht", "das", "Kind", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Davon die Katzen noch heutigen Tags", "tokens": ["Da\u00b7von", "die", "Kat\u00b7zen", "noch", "heu\u00b7ti\u00b7gen", "Tags"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Maria die liebsten Tiere sind.", "tokens": ["Ma\u00b7ria", "die", "liebs\u00b7ten", "Tie\u00b7re", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}