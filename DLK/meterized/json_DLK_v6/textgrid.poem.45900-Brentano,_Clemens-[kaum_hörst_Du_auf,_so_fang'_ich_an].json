{"textgrid.poem.45900": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[kaum h\u00f6rst Du auf, so fang' ich an]", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kaum h\u00f6rst Du auf, so fang' ich an,", "tokens": ["Kaum", "h\u00f6rst", "Du", "auf", ",", "so", "fang'", "ich", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich erst recht zu vermissen,", "tokens": ["Dich", "erst", "recht", "zu", "ver\u00b7mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ich habe ein Gel\u00fcbd' getan,", "tokens": ["Ich", "ha\u00b7be", "ein", "Ge\u00b7l\u00fcbd'", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein andres Weib zu k\u00fcssen.", "tokens": ["Kein", "and\u00b7res", "Weib", "zu", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gewaltig, regt es sich in mir,", "tokens": ["Ge\u00b7wal\u00b7tig", ",", "regt", "es", "sich", "in", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "PPER", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu leben und zu lieben,", "tokens": ["Zu", "le\u00b7ben", "und", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O s\u00fc\u00dfe Frau w\u00e4r' ich bei Dir,", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Frau", "w\u00e4r'", "ich", "bei", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VAFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich wollt' Dich nicht betr\u00fcben.", "tokens": ["Ich", "wollt'", "Dich", "nicht", "be\u00b7tr\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du letzter Preis von Lieb' und Lust,", "tokens": ["Du", "letz\u00b7ter", "Preis", "von", "Lieb'", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie konnte ich Dich qu\u00e4len,", "tokens": ["Wie", "konn\u00b7te", "ich", "Dich", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach h\u00e4tt' ich jemals was gewu\u00dft,", "tokens": ["Ach", "h\u00e4tt'", "ich", "je\u00b7mals", "was", "ge\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie k\u00f6nnt' ich dann erz\u00e4hlen.", "tokens": ["Wie", "k\u00f6nnt'", "ich", "dann", "er\u00b7z\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Lippe schlie\u00dft der Liebe Ku\u00df,", "tokens": ["Die", "Lip\u00b7pe", "schlie\u00dft", "der", "Lie\u00b7be", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich hab' ihn nie empfangen,", "tokens": ["Ich", "hab'", "ihn", "nie", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es r\u00fchmt sich nur der \u00dcberdru\u00df,", "tokens": ["Es", "r\u00fchmt", "sich", "nur", "der", "\u00dc\u00b7berd\u00b7ru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es seufzt nur das Verlangen.", "tokens": ["Es", "seufzt", "nur", "das", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Kaum h\u00f6rst Du auf, so fang' ich an", "tokens": ["Kaum", "h\u00f6rst", "Du", "auf", ",", "so", "fang'", "ich", "an"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vers\u00e4umnis mu\u00df ich b\u00fc\u00dfen,", "tokens": ["Ver\u00b7s\u00e4um\u00b7nis", "mu\u00df", "ich", "b\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O wandelte die Lust mich an", "tokens": ["O", "wan\u00b7del\u00b7te", "die", "Lust", "mich", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein andres Weib zu k\u00fcssen.", "tokens": ["Ein", "and\u00b7res", "Weib", "zu", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mein Ku\u00df ist jung, mein Ku\u00df ist alt,", "tokens": ["Mein", "Ku\u00df", "ist", "jung", ",", "mein", "Ku\u00df", "ist", "alt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich k\u00fcss' mit weisen Listen,", "tokens": ["Ich", "k\u00fcss'", "mit", "wei\u00b7sen", "Lis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es w\u00fcrde Liebe und Gewalt,", "tokens": ["Es", "w\u00fcr\u00b7de", "Lie\u00b7be", "und", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Untreu' Dir nicht fristen.", "tokens": ["Die", "Un\u00b7treu'", "Dir", "nicht", "fris\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So lebe wohl, verzeihe Dir!", "tokens": ["So", "le\u00b7be", "wohl", ",", "ver\u00b7zei\u00b7he", "Dir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die keusche Bahn zu wandlen,", "tokens": ["Die", "keu\u00b7sche", "Bahn", "zu", "wand\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich lebe wohl, verzeihe mir,", "tokens": ["Ich", "le\u00b7be", "wohl", ",", "ver\u00b7zei\u00b7he", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Traum Dich zu \u2013 mi\u00dfhandlen.", "tokens": ["Im", "Traum", "Dich", "zu", "\u2013", "mi\u00df\u00b7hand\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKZU", "$(", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}