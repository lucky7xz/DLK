{"textgrid.poem.38410": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Eia popeia etzetera", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eia im Sause,", "tokens": ["Ei\u00b7a", "im", "Sau\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Zwei Wiegen in einem Hause,", "tokens": ["Zwei", "Wie\u00b7gen", "in", "ei\u00b7nem", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Soll der Vater nicht werden bang,", "tokens": ["Soll", "der", "Va\u00b7ter", "nicht", "wer\u00b7den", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "VAFIN", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Um zwei Wiegen in einem Gang,", "tokens": ["Um", "zwei", "Wie\u00b7gen", "in", "ei\u00b7nem", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Eia im Sause.", "tokens": ["Ei\u00b7a", "im", "Sau\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Eia wiwi!", "tokens": ["Ei\u00b7a", "wi\u00b7wi", "!"], "token_info": ["word", "word", "punct"], "pos": ["FM", "FM", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Wer schl\u00e4ft heut Nacht bei mir,", "tokens": ["Wer", "schl\u00e4ft", "heut", "Nacht", "bei", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Solls mein liebes H\u00e4nschen seyn,", "tokens": ["Solls", "mein", "lie\u00b7bes", "H\u00e4n\u00b7schen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird es auch h\u00fcbsch freundlich seyn,", "tokens": ["Wird", "es", "auch", "h\u00fcbsch", "freund\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eia wiwi.", "tokens": ["Ei\u00b7a", "wi\u00b7wi", "."], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Eia pum pum,", "tokens": ["Ei\u00b7a", "pum", "pum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,"], "meter": "+---", "measure": "dactylic.init"}, "line.2": {"text": "Unser kleiner Jung,", "tokens": ["Un\u00b7ser", "klei\u00b7ner", "Jung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Will noch nicht alleine schlafen,", "tokens": ["Will", "noch", "nicht", "al\u00b7lei\u00b7ne", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will sich noch rumpumpeln lassen,", "tokens": ["Will", "sich", "noch", "rum\u00b7pum\u00b7peln", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eia pum pum.", "tokens": ["Ei\u00b7a", "pum", "pum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+---", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Eia polei,", "tokens": ["Ei\u00b7a", "po\u00b7lei", ","], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Kocht dem Schelm ein Brei,", "tokens": ["Kocht", "dem", "Schelm", "ein", "Brei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Thut brav Zucker und Butter hinein,", "tokens": ["Thut", "brav", "Zu\u00b7cker", "und", "But\u00b7ter", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "So kriegt der Schelm ein geschmeidigen Sinn,", "tokens": ["So", "kriegt", "der", "Schelm", "ein", "ge\u00b7schmei\u00b7di\u00b7gen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Eia polei.", "tokens": ["Ei\u00b7a", "po\u00b7lei", "."], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Eia schlaf s\u00fc\u00dfe,", "tokens": ["Ei\u00b7a", "schlaf", "s\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ich wieg dich mit den F\u00fc\u00dfen,", "tokens": ["Ich", "wieg", "dich", "mit", "den", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich wieg dich mit dem schwarzen Schuh,", "tokens": ["Ich", "wieg", "dich", "mit", "dem", "schwar\u00b7zen", "Schuh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schlaf mein Kind schlaf immer zu.", "tokens": ["Schlaf", "mein", "Kind", "schlaf", "im\u00b7mer", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eia schlaf s\u00fc\u00dfe.", "tokens": ["Ei\u00b7a", "schlaf", "s\u00fc\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Eia popeien,", "tokens": ["Ei\u00b7a", "po\u00b7pei\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Willst du immer schreien,", "tokens": ["Willst", "du", "im\u00b7mer", "schrei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Flenn Els auf der Geigen,", "tokens": ["Flenn", "Els", "auf", "der", "Gei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Kannst du nit geschweigen,", "tokens": ["Kannst", "du", "nit", "ge\u00b7schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Eia popeien.", "tokens": ["Ei\u00b7a", "po\u00b7pei\u00b7en", "."], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Eia popille,", "tokens": ["Ei\u00b7a", "po\u00b7pil\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Schweigst du mir nicht stille,", "tokens": ["Schweigst", "du", "mir", "nicht", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Geb ich dir du S\u00fcnderlein,", "tokens": ["Geb", "ich", "dir", "du", "S\u00fcn\u00b7der\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Ruthe vor dein Hinterlein,", "tokens": ["Die", "Ru\u00b7the", "vor", "dein", "Hin\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Eia popille!", "tokens": ["Ei\u00b7a", "po\u00b7pil\u00b7le", "!"], "token_info": ["word", "word", "punct"], "pos": ["FM", "FM", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}