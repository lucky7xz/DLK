{"textgrid.poem.50126": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "14. Ein guter Nahme in Verfolgung", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um seine ", "tokens": ["Um", "sei\u00b7ne"], "token_info": ["word", "word"], "pos": ["KOUI", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Dem sprach man tr\u00f6stlich also zu:", "tokens": ["Dem", "sprach", "man", "tr\u00f6st\u00b7lich", "al\u00b7so", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil deiner ", "tokens": ["Weil", "dei\u00b7ner"], "token_info": ["word", "word"], "pos": ["KOUS", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Und jeder von dir r\u00fchmlich spricht,", "tokens": ["Und", "je\u00b7der", "von", "dir", "r\u00fchm\u00b7lich", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der sich mit jenen nicht verschworen,", "tokens": ["Der", "sich", "mit", "je\u00b7nen", "nicht", "ver\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PDS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}