{"dta.poem.2416": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "An den Neid.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Wje k\u00f6m\u0303ts vergifft er Neid/ du dein stets eigner Henker/", "tokens": ["Wie", "k\u00f6m\u0303ts", "ver\u00b7gifft", "er", "Neid", "/", "du", "dein", "stets", "eig\u00b7ner", "Hen\u00b7ker", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "NN", "$(", "PPER", "PPOSAT", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du b\u00f6ses Schlangenkind/ du hellisches Geschlecht/", "tokens": ["Du", "b\u00f6\u00b7ses", "Schlan\u00b7gen\u00b7kind", "/", "du", "hel\u00b7li\u00b7sches", "Ge\u00b7schlecht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du Friedenst\u00f6rer du/ du Teussels Stekkenknecht/", "tokens": ["Du", "Frie\u00b7den\u00b7st\u00f6\u00b7rer", "du", "/", "du", "Teus\u00b7sels", "Stek\u00b7ken\u00b7knecht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPER", "$(", "PPER", "NE", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du blauvergalter Mund/ du dein selbst eigner Kr\u00e4nker/", "tokens": ["Du", "blau\u00b7ver\u00b7gal\u00b7ter", "Mund", "/", "du", "dein", "selbst", "eig\u00b7ner", "Kr\u00e4n\u00b7ker", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "PPER", "PPOSAT", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie k\u00f6mmt es/ sag\u2019 ich noch du Ungl\u00fcks angedenker/", "tokens": ["Wie", "k\u00f6mmt", "es", "/", "sag'", "ich", "noch", "du", "Un\u00b7gl\u00fcks", "an\u00b7ge\u00b7den\u00b7ker", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$(", "VVFIN", "PPER", "ADV", "PPER", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df du must allezeit ver\u00fcben deine T\u00fckk\u2019", "tokens": ["Da\u00df", "du", "must", "al\u00b7le\u00b7zeit", "ver\u00b7\u00fc\u00b7ben", "dei\u00b7ne", "T\u00fc\u00b7kk'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.7": {"text": "Und wetzen deinen Zahn/ wenn wor ein gutes Gl\u00fctk", "tokens": ["Und", "wet\u00b7zen", "dei\u00b7nen", "Zahn", "/", "wenn", "wor", "ein", "gu\u00b7tes", "Gl\u00fctk"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$(", "KOUS", "NE", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Tugend ist beschehrt? sag an du Ehrversenker.", "tokens": ["Der", "Tu\u00b7gend", "ist", "be\u00b7schehrt", "?", "sag", "an", "du", "Ehr\u00b7ver\u00b7sen\u00b7ker", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "VVFIN", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch schweig ich weis es schon. Gleich wie dem klahren", "tokens": ["Doch", "schweig", "ich", "weis", "es", "schon", ".", "Gleich", "wie", "dem", "klah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "PPER", "ADV", "$.", "ADV", "KOKOM", "ART", "ADJA"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Liecht/", "tokens": ["Liecht", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.11": {"text": "(jm fall der g\u00f6ldne Glantz Apollens zu uns bricht.", "tokens": ["(", "jm", "fall", "der", "g\u00f6ld\u00b7ne", "Glantz", "A\u00b7pol\u00b7lens", "zu", "uns", "bricht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "NN", "ART", "ADJA", "NN", "NE", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und denn der Silberschein von seinen Ehegatten/)", "tokens": ["Und", "denn", "der", "Sil\u00b7ber\u00b7schein", "von", "sei\u00b7nen", "E\u00b7he\u00b7gat\u00b7ten", "/", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$(", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der dunkle Schatten folgt; so folgstu \u00fcberal", "tokens": ["Der", "dunk\u00b7le", "Schat\u00b7ten", "folgt", ";", "so", "folgs\u00b7tu", "\u00fc\u00b7be\u00b7ral"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der \u00e4dlen Tugend nach. Kurtz/ wisse dieses mal", "tokens": ["Der", "\u00e4d\u00b7len", "Tu\u00b7gend", "nach", ".", "Kurtz", "/", "wis\u00b7se", "die\u00b7ses", "mal"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$.", "NN", "$(", "VVFIN", "PDS", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Tugend ist das Licht/ du aber schwartzer Schatten.", "tokens": ["Die", "Tu\u00b7gend", "ist", "das", "Licht", "/", "du", "a\u00b7ber", "schwart\u00b7zer", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$(", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}