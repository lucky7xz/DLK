{"textgrid.poem.67565": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Das Eine in der Natur", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr kleinen Sterne dort bei Nacht,", "tokens": ["Ihr", "klei\u00b7nen", "Ster\u00b7ne", "dort", "bei", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die, funkelnd, unserm Angesicht", "tokens": ["Die", ",", "fun\u00b7kelnd", ",", "un\u00b7serm", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "$,", "VVPP", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mehr Zahl als Glanz erschaulich macht,", "tokens": ["Mehr", "Zahl", "als", "Glanz", "er\u00b7schau\u00b7lich", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KOUS", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Heere, denen Raum gebricht:", "tokens": ["Ihr", "Hee\u00b7re", ",", "de\u00b7nen", "Raum", "ge\u00b7bricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was seid Ihr all am Sonnenlicht?", "tokens": ["Was", "seid", "Ihr", "all", "am", "Son\u00b7nen\u00b7licht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PIAT", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr fr\u00fchen Veilchen auf der Flur,", "tokens": ["Ihr", "fr\u00fc\u00b7hen", "Veil\u00b7chen", "auf", "der", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ihr in Purpurkleiderpracht", "tokens": ["Die", "Ihr", "in", "Pur\u00b7pur\u00b7klei\u00b7der\u00b7pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als Erstgeborne der Natur", "tokens": ["Als", "Erst\u00b7ge\u00b7bor\u00b7ne", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um Euch so stolz, so spr\u00f6de lacht:", "tokens": ["Um", "Euch", "so", "stolz", ",", "so", "spr\u00f6\u00b7de", "lacht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADV", "ADJD", "$,", "ADV", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was seid Ihr, wenn die Ros' erwacht?", "tokens": ["Was", "seid", "Ihr", ",", "wenn", "die", "Ros'", "er\u00b7wacht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr regen S\u00e4nger dort im Hain,", "tokens": ["Ihr", "re\u00b7gen", "S\u00e4n\u00b7ger", "dort", "im", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit tausendfachem, muntern Schall,", "tokens": ["Mit", "tau\u00b7send\u00b7fa\u00b7chem", ",", "mun\u00b7tern", "Schall", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als w\u00e4ret Ihr, so schwach, so klein,", "tokens": ["Als", "w\u00e4\u00b7ret", "Ihr", ",", "so", "schwach", ",", "so", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Tonkunst alle: allzumal,", "tokens": ["Die", "Ton\u00b7kunst", "al\u00b7le", ":", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PIS", "$.", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was seid Ihr zu der Nachtigall?", "tokens": ["Was", "seid", "Ihr", "zu", "der", "Nach\u00b7ti\u00b7gall", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So, wenn mit ihrem G\u00f6tterblick", "tokens": ["So", ",", "wenn", "mit", "ih\u00b7rem", "G\u00f6t\u00b7ter\u00b7blick"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein M\u00e4dchen eintritt in den Kreis", "tokens": ["Mein", "M\u00e4d\u00b7chen", "ein\u00b7tritt", "in", "den", "Kreis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sch\u00f6nen, und ihr G\u00f6tterblick", "tokens": ["Der", "Sch\u00f6\u00b7nen", ",", "und", "ihr", "G\u00f6t\u00b7ter\u00b7blick"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von aller Herrlichkeit nichts wei\u00df:", "tokens": ["Von", "al\u00b7ler", "Herr\u00b7lich\u00b7keit", "nichts", "wei\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer l\u00e4\u00dft, wer giebt ihr nicht den Preis?", "tokens": ["Wer", "l\u00e4\u00dft", ",", "wer", "giebt", "ihr", "nicht", "den", "Preis", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWS", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}