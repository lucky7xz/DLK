{"dta.poem.18494": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines feuer-fressers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus kurtzweil fra\u00df ich vor das feuer in mich ein/", "tokens": ["Aus", "kurt\u00b7zweil", "fra\u00df", "ich", "vor", "das", "feu\u00b7er", "in", "mich", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "PRF", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ohn da\u00df im minsten mir es k\u00f6nte sch\u00e4dlich seyn.", "tokens": ["Ohn", "da\u00df", "im", "mins\u00b7ten", "mir", "es", "k\u00f6n\u00b7te", "sch\u00e4d\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "APPRART", "ADJA", "PPER", "PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jtzt da es n\u00f6thig w\u00e4r/ da kan ich keines fressen/", "tokens": ["Jtzt", "da", "es", "n\u00f6\u00b7thig", "w\u00e4r", "/", "da", "kan", "ich", "kei\u00b7nes", "fres\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "VAFIN", "$(", "ADV", "VMFIN", "PPER", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es scheinet/ da\u00df man hier di\u00df handwerck mu\u00df vergessen.", "tokens": ["Es", "schei\u00b7net", "/", "da\u00df", "man", "hier", "di\u00df", "hand\u00b7werck", "mu\u00df", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PIS", "ADV", "PDS", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}