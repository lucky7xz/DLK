{"dta.poem.4197": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bluhmen-Lehre.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "OgOtt, der Du in Deinen Werken", "tokens": ["O\u00b7gOtt", ",", "der", "Du", "in", "Dei\u00b7nen", "Wer\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich jedem giebest zu bemerken,", "tokens": ["Dich", "je\u00b7dem", "gie\u00b7best", "zu", "be\u00b7mer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hier sitz ich, und bemerke Dich.", "tokens": ["Hier", "sitz", "ich", ",", "und", "be\u00b7mer\u00b7ke", "Dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es giebet jedes Blatt Dein Wesen", "tokens": ["Es", "gie\u00b7bet", "je\u00b7des", "Blatt", "Dein", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit reinen Ziefern uns zu lesen,", "tokens": ["Mit", "rei\u00b7nen", "Zie\u00b7fern", "uns", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Daseyn zeigt sich sichtbarlich.", "tokens": ["Dein", "Da\u00b7seyn", "zeigt", "sich", "sicht\u00b7bar\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mich deucht, da\u00df ich die wahre Lehre", "tokens": ["Mich", "deucht", ",", "da\u00df", "ich", "die", "wah\u00b7re", "Leh\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von einer jeden Bluhme h\u00f6re,", "tokens": ["Von", "ei\u00b7ner", "je\u00b7den", "Bluh\u00b7me", "h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die sie im sanften Hauch uns giebt:", "tokens": ["Die", "sie", "im", "sanf\u00b7ten", "Hauch", "uns", "giebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Empfind' ein Wesen, das dich liebt!", "tokens": ["Emp\u00b7find'", "ein", "We\u00b7sen", ",", "das", "dich", "liebt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ich zeig' Jhn deinem Geist; bedenke,", "tokens": ["Ich", "zeig'", "Jhn", "dei\u00b7nem", "Geist", ";", "be\u00b7den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Da\u00df meines Balsams S\u00fc\u00dfigkeit,", "tokens": ["Da\u00df", "mei\u00b7nes", "Bal\u00b7sams", "S\u00fc\u00b7\u00dfig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Der oft dein Innerstes erfreut,", "tokens": ["Der", "oft", "dein", "In\u00b7ners\u00b7tes", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Sich ja von selbst nicht in mich senke.", "tokens": ["Sich", "ja", "von", "selbst", "nicht", "in", "mich", "sen\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ADV", "PTKNEG", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}