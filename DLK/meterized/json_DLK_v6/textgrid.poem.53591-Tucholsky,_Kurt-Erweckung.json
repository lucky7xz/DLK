{"textgrid.poem.53591": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Erweckung", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut, nach Jahren, sah ich Josephine.", "tokens": ["Heut", ",", "nach", "Jah\u00b7ren", ",", "sah", "ich", "Jo\u00b7se\u00b7phi\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NN", "$,", "VVFIN", "PPER", "NE", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Welch ein Schreck!", "tokens": ["Welch", "ein", "Schreck", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Ach, ich kannt sie mit der Unschuldsmiene \u2013.", "tokens": ["Ach", ",", "ich", "kannt", "sie", "mit", "der", "Un\u00b7schulds\u00b7mie\u00b7ne", "\u2013", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die ist weg.", "tokens": ["Die", "ist", "weg", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Kannte sie noch, als sie leise senkte", "tokens": ["Kann\u00b7te", "sie", "noch", ",", "als", "sie", "lei\u00b7se", "senk\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADJD", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Lid und Wimper, wenn ein Mann sie kr\u00e4nkte.", "tokens": ["Lid", "und", "Wim\u00b7per", ",", "wenn", "ein", "Mann", "sie", "kr\u00e4nk\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Durft ihr niemals nach halb neune nahn . . .", "tokens": ["Durft", "ihr", "nie\u00b7mals", "nach", "halb", "neu\u00b7ne", "nahn", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJD", "ADJA", "NN", "$.", "$.", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Wer hat diese Augen aufgetan?", "tokens": ["Wer", "hat", "die\u00b7se", "Au\u00b7gen", "auf\u00b7ge\u00b7tan", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ihre Blicke waren einstmals z\u00fcchtig.", "tokens": ["Ih\u00b7re", "Bli\u00b7cke", "wa\u00b7ren", "einst\u00b7mals", "z\u00fcch\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Keusch und blind", "tokens": ["Keusch", "und", "blind"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "k\u00fc\u00dfte sie ihr gutes Muttchen fl\u00fcchtig,", "tokens": ["k\u00fc\u00df\u00b7te", "sie", "ihr", "gu\u00b7tes", "Mutt\u00b7chen", "fl\u00fcch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "wie ein Kind.", "tokens": ["wie", "ein", "Kind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Heute rufen ihre blauen Sterne:", "tokens": ["Heu\u00b7te", "ru\u00b7fen", "ih\u00b7re", "blau\u00b7en", "Ster\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Bleib! Ich sterbe k\u00fcssend gar zu gerne \u2013!", "tokens": ["Bleib", "!", "Ich", "ster\u00b7be", "k\u00fcs\u00b7send", "gar", "zu", "ger\u00b7ne", "\u2013", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "ADJD", "ADV", "PTKA", "ADV", "$(", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wer geleitet sie auf s\u00fc\u00dfer Bahn?", "tokens": ["Wer", "ge\u00b7lei\u00b7tet", "sie", "auf", "s\u00fc\u00b7\u00dfer", "Bahn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Wer hat diese Augen aufgetan?", "tokens": ["Wer", "hat", "die\u00b7se", "Au\u00b7gen", "auf\u00b7ge\u00b7tan", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Von der Liebe immer fortzugleiten,", "tokens": ["Von", "der", "Lie\u00b7be", "im\u00b7mer", "fort\u00b7zu\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "ist mein Fluch.", "tokens": ["ist", "mein", "Fluch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Finster schreib ich Tagesneuigkeiten", "tokens": ["Fins\u00b7ter", "schreib", "ich", "Ta\u00b7ges\u00b7neu\u00b7ig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "in dies Buch.", "tokens": ["in", "dies", "Buch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDS", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ach, Germania, sieh auf Josephinen!", "tokens": ["Ach", ",", "Ger\u00b7ma\u00b7nia", ",", "sieh", "auf", "Jo\u00b7se\u00b7phi\u00b7nen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "$,", "VVIMP", "APPR", "NE", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dir ist noch kein starker Mann erschienen.", "tokens": ["Dir", "ist", "noch", "kein", "star\u00b7ker", "Mann", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Glaubst noch immer deinem Kinderwahn . . .", "tokens": ["Glaubst", "noch", "im\u00b7mer", "dei\u00b7nem", "Kin\u00b7der\u00b7wahn", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ADV", "ADV", "PPOSAT", "NN", "$.", "$.", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Wann wird dir die Seele aufgetan?", "tokens": ["Wann", "wird", "dir", "die", "See\u00b7le", "auf\u00b7ge\u00b7tan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}