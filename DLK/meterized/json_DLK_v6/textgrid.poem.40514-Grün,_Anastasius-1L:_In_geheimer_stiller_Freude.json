{"textgrid.poem.40514": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: In geheimer stiller Freude", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In geheimer stiller Freude", "tokens": ["In", "ge\u00b7hei\u00b7mer", "stil\u00b7ler", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blickt' ich eine Rose an,", "tokens": ["Blickt'", "ich", "ei\u00b7ne", "Ro\u00b7se", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die im Perl- und Purpurkleide", "tokens": ["Die", "im", "Per\u00b7l", "und", "Pur\u00b7pur\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "TRUNC", "KON", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Schwellend aufzubl\u00fchn begann.", "tokens": ["Schwel\u00b7lend", "auf\u00b7zu\u00b7bl\u00fchn", "be\u00b7gann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVIZU", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bange doch vielleicht zu Muthe", "tokens": ["Ban\u00b7ge", "doch", "viel\u00b7leicht", "zu", "Mu\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War's dem Elfen, klein und traut,", "tokens": ["Wa\u00b7r's", "dem", "El\u00b7fen", ",", "klein", "und", "traut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der in ihrem Kelche ruhte,", "tokens": ["Der", "in", "ih\u00b7rem", "Kel\u00b7che", "ruh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drin sein H\u00e4uschen er gebaut.", "tokens": ["Drin", "sein", "H\u00e4usc\u00b7hen", "er", "ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn ein Kn\u00f6spchen platzend springet,", "tokens": ["Wenn", "ein", "Kn\u00f6spc\u00b7hen", "plat\u00b7zend", "sprin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kracht's ihm wohl wie Donnerklang,", "tokens": ["Kracht's", "ihm", "wohl", "wie", "Don\u00b7ner\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "KOKOM", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ein West die Rose schwinget,", "tokens": ["Wenn", "ein", "West", "die", "Ro\u00b7se", "schwin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Macht ihm Erdebeben bang!", "tokens": ["Macht", "ihm", "Er\u00b7de\u00b7be\u00b7ben", "bang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie ihr Kelch sich aufthut Allen,", "tokens": ["Wie", "ihr", "Kelch", "sich", "auf\u00b7thut", "Al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PRF", "VVFIN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schreckt ein Abgrund schwindelnd ihn,", "tokens": ["Schreckt", "ein", "Ab\u00b7grund", "schwin\u00b7delnd", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des Bl\u00fcthenstaubes Fallen", "tokens": ["Und", "des", "Bl\u00fc\u00b7then\u00b7stau\u00b7bes", "Fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00fcrzt auf ihn als Schneelavin'.", "tokens": ["St\u00fcrzt", "auf", "ihn", "als", "Schnee\u00b7la\u00b7vin'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "KOUS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Eine Ueberschwemmung drohte", "tokens": ["Ei\u00b7ne", "Ue\u00b7ber\u00b7schwem\u00b7mung", "droh\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seiner Wohnung, Hab' und Haut,", "tokens": ["Sei\u00b7ner", "Woh\u00b7nung", ",", "Hab'", "und", "Haut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als es k\u00fchl aus Morgenrothe", "tokens": ["Als", "es", "k\u00fchl", "aus", "Mor\u00b7gen\u00b7ro\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Perlen in den Kelch gethaut.", "tokens": ["Per\u00b7len", "in", "den", "Kelch", "ge\u00b7thaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Als mein Athem freier wehte,", "tokens": ["Als", "mein", "A\u00b7them", "frei\u00b7er", "weh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schien's ihm Sturmwinds Ungest\u00fcm,", "tokens": ["Schien's", "ihm", "Sturm\u00b7winds", "Un\u00b7ge\u00b7st\u00fcm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und vielleicht gar als Komete", "tokens": ["Und", "viel\u00b7leicht", "gar", "als", "Ko\u00b7me\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "KOUS", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Droht' mein heitrer Blick ob ihm.", "tokens": ["Droht'", "mein", "hei\u00b7trer", "Blick", "ob", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "KOUS", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und mit Bangen sonder Gleichen", "tokens": ["Und", "mit", "Ban\u00b7gen", "son\u00b7der", "Glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Harrt der Kleine \u00e4ngstlichscheu,", "tokens": ["Harrt", "der", "Klei\u00b7ne", "\u00e4ngst\u00b7lich\u00b7scheu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was wohl all der Schreckenszeichen", "tokens": ["Was", "wohl", "all", "der", "Schre\u00b7cken\u00b7szei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PIAT", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Grausenhaftes Ende sei?", "tokens": ["Grau\u00b7sen\u00b7haf\u00b7tes", "En\u00b7de", "sei", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch mit tiefer stiller Freude", "tokens": ["Doch", "mit", "tie\u00b7fer", "stil\u00b7ler", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blickte ich die Rose an,", "tokens": ["Blick\u00b7te", "ich", "die", "Ro\u00b7se", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die im Perl- und Purpurkleide", "tokens": ["Die", "im", "Per\u00b7l", "und", "Pur\u00b7pur\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "TRUNC", "KON", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Bl\u00fcthenvoll sich aufgethan.", "tokens": ["Bl\u00fc\u00b7then\u00b7voll", "sich", "auf\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}