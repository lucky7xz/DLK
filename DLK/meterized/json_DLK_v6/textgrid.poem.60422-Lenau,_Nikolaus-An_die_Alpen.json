{"textgrid.poem.60422": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An die Alpen", "genre": "verse", "period": "N.A.", "pub_year": 1840, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alpen! Alpen! unverge\u00dflich seid", "tokens": ["Al\u00b7pen", "!", "Al\u00b7pen", "!", "un\u00b7ver\u00b7ge\u00df\u00b7lich", "seid"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$.", "NN", "$.", "ADJD", "VAFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Meinem Herzen ihr in allen Tagen;", "tokens": ["Mei\u00b7nem", "Her\u00b7zen", "ihr", "in", "al\u00b7len", "Ta\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Bergend vor der Welt ein herbes Leid,", "tokens": ["Ber\u00b7gend", "vor", "der", "Welt", "ein", "her\u00b7bes", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Hab ich es zu euch hinaufgetragen.", "tokens": ["Hab", "ich", "es", "zu", "euch", "hin\u00b7auf\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "F\u00fcr das Ungl\u00fcck steht ein Gnadenbild", "tokens": ["F\u00fcr", "das", "Un\u00b7gl\u00fcck", "steht", "ein", "Gna\u00b7den\u00b7bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Zwischen Felsen heimlich eingeschlossen,", "tokens": ["Zwi\u00b7schen", "Fel\u00b7sen", "heim\u00b7lich", "ein\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Eine Kluft ists, einsam, tief und wild,", "tokens": ["Ei\u00b7ne", "Kluft", "ists", ",", "ein\u00b7sam", ",", "tief", "und", "wild", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Durch den Abgrund ist ein Quell gesto\u00dfen.", "tokens": ["Durch", "den", "Ab\u00b7grund", "ist", "ein", "Quell", "ge\u00b7sto\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wie die Brust Marias schwertdurchbohrt", "tokens": ["Wie", "die", "Brust", "Ma\u00b7rias", "schwert\u00b7durch\u00b7bohrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NE", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ist zu schaun in christlicher Kapelle,", "tokens": ["Ist", "zu", "schaun", "in", "christ\u00b7li\u00b7cher", "Ka\u00b7pel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKA", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "So Natur, der heilgen Mutter dort", "tokens": ["So", "Na\u00b7tur", ",", "der", "heil\u00b7gen", "Mut\u00b7ter", "dort"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "$,", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Schien das Herz durchschnitten von dem Quelle", "tokens": ["Schien", "das", "Herz", "durch\u00b7schnit\u00b7ten", "von", "dem", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Grauer Felsen ewig starrer Blick", "tokens": ["Grau\u00b7er", "Fel\u00b7sen", "e\u00b7wig", "star\u00b7rer", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Hangt hinab zur tiefgeri\u00dfnen Wunde,", "tokens": ["Hangt", "hin\u00b7ab", "zur", "tief\u00b7ge\u00b7ri\u00df\u00b7nen", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und der Mensch mit seinem Mi\u00dfgeschick", "tokens": ["Und", "der", "Mensch", "mit", "sei\u00b7nem", "Mi\u00df\u00b7ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Lauscht dem Strom, der immer klagt im Grunde.", "tokens": ["Lauscht", "dem", "Strom", ",", "der", "im\u00b7mer", "klagt", "im", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Tausendstimmig braust ein dunkler Schmerz", "tokens": ["Tau\u00b7sends\u00b7tim\u00b7mig", "braust", "ein", "dunk\u00b7ler", "Schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "In des Stroms zerbrochenen Akkorden,", "tokens": ["In", "des", "Stroms", "zer\u00b7bro\u00b7che\u00b7nen", "Ak\u00b7kor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und aufhorchend ist des Menschen Herz", "tokens": ["Und", "auf\u00b7hor\u00b7chend", "ist", "des", "Men\u00b7schen", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "VAFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Seiner eignen Klage still geworden.", "tokens": ["Sei\u00b7ner", "eig\u00b7nen", "Kla\u00b7ge", "still", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "VAPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Wird des Ungl\u00fccks heilger Sinn geahnt,", "tokens": ["Wird", "des", "Un\u00b7gl\u00fccks", "heil\u00b7ger", "Sinn", "ge\u00b7ahnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Hat der Kummer seinen Groll verloren;", "tokens": ["Hat", "der", "Kum\u00b7mer", "sei\u00b7nen", "Groll", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Rauschend hat michs an der Kluft gemahnt:", "tokens": ["Rau\u00b7schend", "hat", "michs", "an", "der", "Kluft", "ge\u00b7mahnt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIS", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Schmerz und Liebe hat die Welt geboren.", "tokens": ["Schmerz", "und", "Lie\u00b7be", "hat", "die", "Welt", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Schmerz und Liebe ist des Menschen Teil,", "tokens": ["Schmerz", "und", "Lie\u00b7be", "ist", "des", "Men\u00b7schen", "Teil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Der dem Weltgeschick nicht feig entwichen;", "tokens": ["Der", "dem", "Welt\u00b7ge\u00b7schick", "nicht", "feig", "ent\u00b7wi\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Zieht er aus dem Busen sich den Pfeil,", "tokens": ["Zieht", "er", "aus", "dem", "Bu\u00b7sen", "sich", "den", "Pfeil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ist er f\u00fcr die Welt und Gott verblichen.", "tokens": ["Ist", "er", "f\u00fcr", "die", "Welt", "und", "Gott", "ver\u00b7bli\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Heimweh jagt des Abgrunds wilden Schaum;", "tokens": ["Heim\u00b7weh", "jagt", "des", "Ab\u00b7grunds", "wil\u00b7den", "Schaum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "L\u00e4\u00dft Natur die Erd in Freuden prangen,", "tokens": ["L\u00e4\u00dft", "Na\u00b7tur", "die", "Erd", "in", "Freu\u00b7den", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schildert sie der Zukunft sch\u00f6nen Traum;", "tokens": ["Schil\u00b7dert", "sie", "der", "Zu\u00b7kunft", "sch\u00f6\u00b7nen", "Traum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "All ihr Herz ist Sehnen und Verlangen.", "tokens": ["All", "ihr", "Herz", "ist", "Seh\u00b7nen", "und", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Heimweh ist es, wenn die Liebe naht,", "tokens": ["Heim\u00b7weh", "ist", "es", ",", "wenn", "die", "Lie\u00b7be", "naht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ist der Grund des nie gestillten Fragens,", "tokens": ["Ist", "der", "Grund", "des", "nie", "ge\u00b7still\u00b7ten", "Fra\u00b7gens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Heimweh jede gro\u00dfe Menschentat,", "tokens": ["Heim\u00b7weh", "je\u00b7de", "gro\u00b7\u00dfe", "Men\u00b7schen\u00b7tat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Wunder himmlischen Entsagens. \u2013", "tokens": ["Und", "die", "Wun\u00b7der", "himm\u00b7li\u00b7schen", "Ent\u00b7sa\u00b7gens", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Alpen, o wie st\u00e4rkte mich die Rast,", "tokens": ["Al\u00b7pen", ",", "o", "wie", "st\u00e4rk\u00b7te", "mich", "die", "Rast", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "KOKOM", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Lagernd auf dem weichen Gr\u00fcn der Wiesen,", "tokens": ["La\u00b7gernd", "auf", "dem", "wei\u00b7chen", "Gr\u00fcn", "der", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Kr\u00e4uterd\u00fcfte f\u00e4chelten den Gast,", "tokens": ["Kr\u00e4u\u00b7ter\u00b7d\u00fcf\u00b7te", "f\u00e4\u00b7chel\u00b7ten", "den", "Gast", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+---+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Eisgeharnischt ragten eure Riesen.", "tokens": ["Eis\u00b7ge\u00b7har\u00b7nischt", "rag\u00b7ten", "eu\u00b7re", "Rie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Lerche sang ihr lustverwirrtes Lied,", "tokens": ["Ler\u00b7che", "sang", "ihr", "lust\u00b7ver\u00b7wirr\u00b7tes", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Schweigend strich der Adler durchs Gesteine,", "tokens": ["Schwei\u00b7gend", "strich", "der", "Ad\u00b7ler", "durchs", "Ge\u00b7stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und die Gipfel, als die Sonne schied,", "tokens": ["Und", "die", "Gip\u00b7fel", ",", "als", "die", "Son\u00b7ne", "schied", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Schwelgten stumm im letzten Purpurscheine.", "tokens": ["Schwelg\u00b7ten", "stumm", "im", "letz\u00b7ten", "Pur\u00b7pur\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Eine Herde irrt' am Wiesenhang,", "tokens": ["Ei\u00b7ne", "Her\u00b7de", "irrt'", "am", "Wie\u00b7sen\u00b7hang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "K\u00fche weidend pfl\u00fcckten ihre Beute,", "tokens": ["K\u00fc\u00b7he", "wei\u00b7dend", "pfl\u00fcck\u00b7ten", "ih\u00b7re", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und die Glock an ihrem Halse klang", "tokens": ["Und", "die", "Glock", "an", "ih\u00b7rem", "Hal\u00b7se", "klang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "F\u00fcr die Kr\u00e4uter sanftes Sterbgel\u00e4ute.", "tokens": ["F\u00fcr", "die", "Kr\u00e4u\u00b7ter", "sanf\u00b7tes", "Sterb\u00b7ge\u00b7l\u00e4u\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Kaum vernehmbar kam der m\u00fcde Schall", "tokens": ["Kaum", "ver\u00b7nehm\u00b7bar", "kam", "der", "m\u00fc\u00b7de", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Jener Kluft her\u00fcber mit den Winden;", "tokens": ["Je\u00b7ner", "Kluft", "her\u00b7\u00fc\u00b7ber", "mit", "den", "Win\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wo so hoher Frieden \u00fcberall,", "tokens": ["Wo", "so", "ho\u00b7her", "Frie\u00b7den", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Lie\u00df die Ruh in Gott sich vorempfinden. \u2013", "tokens": [