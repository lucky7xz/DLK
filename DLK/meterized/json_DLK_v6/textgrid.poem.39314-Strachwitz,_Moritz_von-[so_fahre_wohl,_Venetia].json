{"textgrid.poem.39314": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[so fahre wohl, Venetia]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So fahre wohl, Venetia,", "tokens": ["So", "fah\u00b7re", "wohl", ",", "Ve\u00b7ne\u00b7tia", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es donnert das Signal!", "tokens": ["Es", "don\u00b7nert", "das", "Sig\u00b7nal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Woge spricht vom M\u00e4rchenglanz", "tokens": ["Die", "Wo\u00b7ge", "spricht", "vom", "M\u00e4r\u00b7chen\u00b7glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir ach! zum letztenmal!", "tokens": ["Mir", "ach", "!", "zum", "letz\u00b7ten\u00b7mal", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ITJ", "$.", "APPRART", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hab' treuen Dank, Du Meeresfee,", "tokens": ["Hab'", "treu\u00b7en", "Dank", ",", "Du", "Mee\u00b7res\u00b7fee", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr Deine reiche Huld,", "tokens": ["F\u00fcr", "Dei\u00b7ne", "rei\u00b7che", "Huld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und, da\u00df ich nicht vergessen kann \u2013", "tokens": ["Und", ",", "da\u00df", "ich", "nicht", "ver\u00b7ges\u00b7sen", "kann", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es war nicht ", "tokens": ["Es", "war", "nicht"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG"], "meter": "+--", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Schwarz\u00e4ugige Venetianerin,", "tokens": ["Schwarz\u00b7\u00e4u\u00b7gi\u00b7ge", "Ve\u00b7ne\u00b7ti\u00b7a\u00b7ne\u00b7rin", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die in der Gondel liegt,", "tokens": ["Die", "in", "der", "Gon\u00b7del", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Indes das pr\u00e4chtige Gelock", "tokens": ["In\u00b7des", "das", "pr\u00e4ch\u00b7ti\u00b7ge", "Ge\u00b7lock"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Wellenwinde fliegt,", "tokens": ["Im", "Wel\u00b7len\u00b7win\u00b7de", "fliegt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es spielt Dein Finger in der Flut,", "tokens": ["Es", "spielt", "Dein", "Fin\u00b7ger", "in", "der", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Auge flammt mich an \u2013", "tokens": ["Dein", "Au\u00b7ge", "flammt", "mich", "an", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wenn ich hier nicht gl\u00fccklich war,", "tokens": ["Und", "wenn", "ich", "hier", "nicht", "gl\u00fcck\u00b7lich", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PTKNEG", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du bist nicht schuld daran.", "tokens": ["Du", "bist", "nicht", "schuld", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Glorreiche Kunst, die, perlengleich,", "tokens": ["Glor\u00b7rei\u00b7che", "Kunst", ",", "die", ",", "per\u00b7len\u00b7gleich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Meeresscho\u00df gereift,", "tokens": ["Im", "Mee\u00b7res\u00b7scho\u00df", "ge\u00b7reift", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Prachtvolle Schwermut, fahre wohl,", "tokens": ["Pracht\u00b7vol\u00b7le", "Schwer\u00b7mut", ",", "fah\u00b7re", "wohl", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die hier so \u00fcppig tr\u00e4uft!", "tokens": ["Die", "hier", "so", "\u00fcp\u00b7pig", "tr\u00e4uft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie reizend bist Du, Stern der See!", "tokens": ["Wie", "rei\u00b7zend", "bist", "Du", ",", "Stern", "der", "See", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VAFIN", "PPER", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus Norden weht es kalt!", "tokens": ["Aus", "Nor\u00b7den", "weht", "es", "kalt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer ", "tokens": ["Wer"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Der lernt es nicht so bald!", "tokens": ["Der", "lernt", "es", "nicht", "so", "bald", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}