{"textgrid.poem.43792": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Weg zur Schule", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Winter, wenn es frieret,", "tokens": ["Im", "Win\u00b7ter", ",", "wenn", "es", "frie\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Winter, wenn es schneit,", "tokens": ["Im", "Win\u00b7ter", ",", "wenn", "es", "schneit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann ist de Weg zur Schule", "tokens": ["Dann", "ist", "de", "Weg", "zur", "Schu\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NE", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcrwahr noch mal so weit.", "tokens": ["F\u00fcr\u00b7wahr", "noch", "mal", "so", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wenn der Kuckuck rufet,", "tokens": ["Und", "wenn", "der", "Ku\u00b7ckuck", "ru\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dann ist der Fr\u00fchling da,", "tokens": ["Dann", "ist", "der", "Fr\u00fch\u00b7ling", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann ist der Weg zur Schule", "tokens": ["Dann", "ist", "der", "Weg", "zur", "Schu\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcrwahr noch mal so nah.", "tokens": ["F\u00fcr\u00b7wahr", "noch", "mal", "so", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wer aber gerne lernet,", "tokens": ["Wer", "a\u00b7ber", "ger\u00b7ne", "ler\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem ist kein Weg zu fern:", "tokens": ["Dem", "ist", "kein", "Weg", "zu", "fern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Fr\u00fchling wie im Winter", "tokens": ["Im", "Fr\u00fch\u00b7ling", "wie", "im", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geh' ich zur Schule gern.", "tokens": ["Geh'", "ich", "zur", "Schu\u00b7le", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}