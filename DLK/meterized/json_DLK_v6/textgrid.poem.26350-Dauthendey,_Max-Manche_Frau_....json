{"textgrid.poem.26350": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Manche Frau ...", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Manche Frau hat dein Lachen.", "tokens": ["Man\u00b7che", "Frau", "hat", "dein", "La\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Manche deiner Haare Glanz.", "tokens": ["Man\u00b7che", "dei\u00b7ner", "Haa\u00b7re", "Glanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Manche kann mich fast fr\u00f6hlich machen,", "tokens": ["Man\u00b7che", "kann", "mich", "fast", "fr\u00f6h\u00b7lich", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Aber keiner gelingt es ganz.", "tokens": ["A\u00b7ber", "kei\u00b7ner", "ge\u00b7lingt", "es", "ganz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Keine verdr\u00e4ngt deinen warmen Blick,", "tokens": ["Kei\u00b7ne", "ver\u00b7dr\u00e4ngt", "dei\u00b7nen", "war\u00b7men", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Keine verdr\u00e4ngt deine wortlose N\u00e4he.", "tokens": ["Kei\u00b7ne", "ver\u00b7dr\u00e4ngt", "dei\u00b7ne", "wort\u00b7lo\u00b7se", "N\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Keine ist meines Lebens Geschick. \u2013", "tokens": ["Kei\u00b7ne", "ist", "mei\u00b7nes", "Le\u00b7bens", "Ge\u00b7schick", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VAFIN", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Meine Liebe zu dir bleibt treu und z\u00e4he.", "tokens": ["Mei\u00b7ne", "Lie\u00b7be", "zu", "dir", "bleibt", "treu", "und", "z\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}