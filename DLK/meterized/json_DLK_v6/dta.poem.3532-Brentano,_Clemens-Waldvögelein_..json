{"dta.poem.3532": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Waldv\u00f6gelein .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich ging mit Lust durch einen gr\u00fcnen Wald,               ", "tokens": ["Ich", "ging", "mit", "Lust", "durch", "ei\u00b7nen", "gr\u00fc\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich h\u00f6rt die V\u00f6glein singen,", "tokens": ["Ich", "h\u00f6rt", "die", "V\u00f6\u00b7glein", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie sangen so jung, sie sangen so alt,", "tokens": ["Sie", "san\u00b7gen", "so", "jung", ",", "sie", "san\u00b7gen", "so", "alt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die kleinen Waldv\u00f6glein in dem Wald,", "tokens": ["Die", "klei\u00b7nen", "Wald\u00b7v\u00f6\u00b7glein", "in", "dem", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Wie gern h\u00f6rt ich sie singen.", "tokens": ["Wie", "gern", "h\u00f6rt", "ich", "sie", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PPER", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Nun sing, nun sing Frau Nachtigall,", "tokens": ["Nun", "sing", ",", "nun", "sing", "Frau", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sing du's bei meinem Feinsliebchen:", "tokens": ["Sing", "du's", "bei", "mei\u00b7nem", "Feins\u00b7lieb\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u201ekomm schier, komm schier wenns finster ist,", "tokens": ["\u201e", "komm", "schier", ",", "komm", "schier", "wenns", "fins\u00b7ter", "ist", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADJD", "$,", "VVFIN", "ADV", "KOUS", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201ewenn niemand auf der Gassen ist,", "tokens": ["\u201e", "wenn", "nie\u00b7mand", "auf", "der", "Gas\u00b7sen", "ist", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PIS", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201eherein will ich dich lassen.\u201c", "tokens": ["\u201e", "her\u00b7ein", "will", "ich", "dich", "las\u00b7sen", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "VMFIN", "PPER", "PRF", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Tag vergieng, die Nacht brach an,", "tokens": ["Der", "Tag", "ver\u00b7gieng", ",", "die", "Nacht", "brach", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er kam zu Feinslieb gegangen;", "tokens": ["Er", "kam", "zu", "Feins\u00b7lieb", "ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Er klopft so leis' wohl an den Ring,", "tokens": ["Er", "klopft", "so", "leis'", "wohl", "an", "den", "Ring", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ei schl\u00e4fst du, oder wachst du Kind,", "tokens": ["Ei", "schl\u00e4fst", "du", ",", "o\u00b7der", "wachst", "du", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich hab so lang gestanden.", "tokens": ["Ich", "hab", "so", "lang", "ge\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da\u00df du so lang gestanden hast,", "tokens": ["Da\u00df", "du", "so", "lang", "ge\u00b7stan\u00b7den", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich hab noch nicht geschlafen;", "tokens": ["Ich", "hab", "noch", "nicht", "ge\u00b7schla\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich dacht als frey in meinem Sinn,", "tokens": ["Ich", "dacht", "als", "frey", "in", "mei\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo ist mein Herzallerliebster hin,", "tokens": ["Wo", "ist", "mein", "Her\u00b7zal\u00b7ler\u00b7liebs\u00b7ter", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wo mag er so lang bleiben?", "tokens": ["Wo", "mag", "er", "so", "lang", "blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wo ich so lang geblieben bin,", "tokens": ["Wo", "ich", "so", "lang", "ge\u00b7blie\u00b7ben", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das darf ich dir wohl sagen;", "tokens": ["Das", "darf", "ich", "dir", "wohl", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Beim Bier und auch beim rothen Wein,", "tokens": ["Beim", "Bier", "und", "auch", "beim", "ro\u00b7then", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei einem schwarzbraunen M\u00e4delein,", "tokens": ["Bei", "ei\u00b7nem", "schwarz\u00b7brau\u00b7nen", "M\u00e4\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "H\u00e4tt deiner bald vergessen.", "tokens": ["H\u00e4tt", "dei\u00b7ner", "bald", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}