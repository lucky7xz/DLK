{"dta.poem.8252": {"metadata": {"author": {"name": "Beer, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Der  S chau- P latz ist eine  L andschafft bey auf-  \n gehender  S onnen.  \n  V orrede.", "genre": "Lyrik, Libretto", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-25222-5", "language": ["de:0.99"], "booktitle": "Beer, Johann: Antonius R\u00f6mischer Triumvir. Wei\u00dfenfels, 1697."}, "poem": {"stanza.1": {"line.1": {"text": "Was Nord/ S\u00fcd/ Ost/ West/ bewohnt/", "tokens": ["Was", "Nord", "/", "S\u00fcd", "/", "Ost", "/", "West", "/", "be\u00b7wohnt", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "NE", "$(", "NN", "$(", "NE", "$(", "NE", "$(", "VVPP", "$("], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Das besiegt die Liebe/", "tokens": ["Das", "be\u00b7siegt", "die", "Lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nichts wird in der Welt verschont/", "tokens": ["Nichts", "wird", "in", "der", "Welt", "ver\u00b7schont", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von derselben Triebe;", "tokens": ["Von", "der\u00b7sel\u00b7ben", "Trie\u00b7be", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Alles mu\u00df ihr zinsbar seyn/", "tokens": ["Al\u00b7les", "mu\u00df", "ihr", "zins\u00b7bar", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADJD", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was bestrahlet Ph&#339;bus Schein.", "tokens": ["Was", "be\u00b7strah\u00b7let", "Ph", "&#339;", "bus", "Schein", "."], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NE", "$(", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}