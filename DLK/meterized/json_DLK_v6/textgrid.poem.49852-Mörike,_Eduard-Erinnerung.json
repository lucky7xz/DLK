{"textgrid.poem.49852": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Erinnerung", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jenes war zum letzten Male,", "tokens": ["Je\u00b7nes", "war", "zum", "letz\u00b7ten", "Ma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich mit dir ging, o Cl\u00e4rchen!", "tokens": ["Da\u00df", "ich", "mit", "dir", "ging", ",", "o", "Cl\u00e4r\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,", "FM", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, das war das letztemal,", "tokens": ["Ja", ",", "das", "war", "das", "letz\u00b7te\u00b7mal", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "PDS", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df wir uns wie Kinder freuten.", "tokens": ["Da\u00df", "wir", "uns", "wie", "Kin\u00b7der", "freu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "KOKOM", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als wir eines Tages eilig", "tokens": ["Als", "wir", "ei\u00b7nes", "Ta\u00b7ges", "ei\u00b7lig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die breiten, sonnenhellen,", "tokens": ["Durch", "die", "brei\u00b7ten", ",", "son\u00b7nen\u00b7hel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Regnerischen Stra\u00dfen, unter", "tokens": ["Reg\u00b7ne\u00b7ri\u00b7schen", "Stra\u00b7\u00dfen", ",", "un\u00b7ter"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "APPR"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Beide heimlich eingeschlossen", "tokens": ["Bei\u00b7de", "heim\u00b7lich", "ein\u00b7ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PIS", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie in einem Feenst\u00fcbchen,", "tokens": ["Wie", "in", "ei\u00b7nem", "Feen\u00b7st\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,"], "meter": "--+-++-", "measure": "anapaest.init"}, "line.6": {"text": "Endlich einmal Arm in Arme!", "tokens": ["End\u00b7lich", "ein\u00b7mal", "Arm", "in", "Ar\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenig wagten wir zu reden,", "tokens": ["We\u00b7nig", "wag\u00b7ten", "wir", "zu", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn das Herz schlug zu gewaltig,", "tokens": ["Denn", "das", "Herz", "schlug", "zu", "ge\u00b7wal\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Beide merkten wir es schweigend,", "tokens": ["Bei\u00b7de", "merk\u00b7ten", "wir", "es", "schwei\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein jedes schob im stillen", "tokens": ["Und", "ein", "je\u00b7des", "schob", "im", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PIAT", "VVFIN", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Des Gesichtes gl\u00fchnde R\u00f6te", "tokens": ["Des", "Ge\u00b7sich\u00b7tes", "gl\u00fchn\u00b7de", "R\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf den Widerschein des Schirmes.", "tokens": ["Auf", "den", "Wi\u00b7der\u00b7schein", "des", "Schir\u00b7mes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, ein Engel warst du da!", "tokens": ["Ach", ",", "ein", "En\u00b7gel", "warst", "du", "da", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie du auf den Boden immer", "tokens": ["Wie", "du", "auf", "den", "Bo\u00b7den", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blicktest, und die blonden Locken", "tokens": ["Blick\u00b7test", ",", "und", "die", "blon\u00b7den", "Lo\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um den hellen Nacken fielen.", "tokens": ["Um", "den", "hel\u00b7len", "Na\u00b7cken", "fie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbjetzt ist wohl ein Regenbogen", "tokens": ["\u00bb", "jetzt", "ist", "wohl", "ein", "Re\u00b7gen\u00b7bo\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hinter uns am Himmel\u00ab, sagt ich,", "tokens": ["Hin\u00b7ter", "uns", "am", "Him\u00b7mel", "\u00ab", ",", "sagt", "ich", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "$(", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbund die Wachtel dort im Fenster,", "tokens": ["\u00bb", "und", "die", "Wach\u00b7tel", "dort", "im", "Fens\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deucht mir, schl\u00e4gt noch eins so froh!\u00ab", "tokens": ["Deucht", "mir", ",", "schl\u00e4gt", "noch", "eins", "so", "froh", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "$,", "VVFIN", "ADV", "PIS", "ADV", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und im Weitergehen dacht ich", "tokens": ["Und", "im", "Wei\u00b7ter\u00b7ge\u00b7hen", "dacht", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Unsrer ersten Jugendspiele,", "tokens": ["Uns\u00b7rer", "ers\u00b7ten", "Ju\u00b7gend\u00b7spie\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dachte an dein heimatliches", "tokens": ["Dach\u00b7te", "an", "dein", "hei\u00b7mat\u00b7li\u00b7ches"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dorf und seine tausend Freuden.", "tokens": ["Dorf", "und", "sei\u00b7ne", "tau\u00b7send", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PPOSAT", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u2013 \u00bbWei\u00dft du auch noch\u00ab, frug ich dich,", "tokens": ["\u2013", "\u00bb", "Wei\u00dft", "du", "auch", "noch", "\u00ab", ",", "frug", "ich", "dich", ","], "token_info": ["punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "$(", "VVFIN", "PPER", "ADV", "ADV", "$(", "$,", "VVFIN", "PPER", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00bbnachbar B\u00fcttnermeisters H\u00f6fchen,", "tokens": ["\u00bb", "nach\u00b7bar", "B\u00fctt\u00b7ner\u00b7meis\u00b7ters", "H\u00f6f\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo die gro\u00dfen Kufen lagen,", "tokens": ["Wo", "die", "gro\u00b7\u00dfen", "Ku\u00b7fen", "la\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Drin wir sonntags nach Mittag uns", "tokens": ["Drin", "wir", "sonn\u00b7tags", "nach", "Mit\u00b7tag", "uns"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADV", "APPR", "NN", "PPER"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Immer h\u00e4uslich niederlie\u00dfen,", "tokens": ["Im\u00b7mer", "h\u00e4us\u00b7lich", "nie\u00b7der\u00b7lie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Plauderten, Geschichten lasen,", "tokens": ["Plau\u00b7der\u00b7ten", ",", "Ge\u00b7schich\u00b7ten", "la\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVINF", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.11": {"text": "W\u00e4hrend dr\u00fcben in der Kirche", "tokens": ["W\u00e4h\u00b7rend", "dr\u00fc\u00b7ben", "in", "der", "Kir\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Kinderlehre war \u2013 (ich h\u00f6re", "tokens": ["Kin\u00b7der\u00b7leh\u00b7re", "war", "\u2013", "(", "ich", "h\u00f6\u00b7re"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["NN", "VAFIN", "$(", "$(", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Heute noch den Ton der Orgel", "tokens": ["Heu\u00b7te", "noch", "den", "Ton", "der", "Or\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Durch die Stille ringsumher):", "tokens": ["Durch", "die", "Stil\u00b7le", "rings\u00b7um\u00b7her", ")", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Sage, lesen wir nicht einmal", "tokens": ["Sa\u00b7ge", ",", "le\u00b7sen", "wir", "nicht", "ein\u00b7mal"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKNEG", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.16": {"text": "Wieder wie zu jenen Zeiten", "tokens": ["Wie\u00b7der", "wie", "zu", "je\u00b7nen", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "\u2013 Just nicht in der Kufe, mein ich \u2013", "tokens": ["\u2013", "Just", "nicht", "in", "der", "Ku\u00b7fe", ",", "mein", "ich", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NN", "PTKNEG", "APPR", "ART", "NN", "$,", "PPOSAT", "PPER", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.18": {"text": "Den beliebten \u203aRobinson\u2039?\u00ab", "tokens": ["Den", "be\u00b7lieb\u00b7ten", "\u203a", "Ro\u00b7bin\u00b7son", "\u2039", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "$(", "NE", "$(", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und du l\u00e4cheltest und bogest", "tokens": ["Und", "du", "l\u00e4\u00b7chel\u00b7test", "und", "bo\u00b7gest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit mir um die letzte Ecke.", "tokens": ["Mit", "mir", "um", "die", "letz\u00b7te", "E\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich bat dich um ein R\u00f6schen,", "tokens": ["Und", "ich", "bat", "dich", "um", "ein", "R\u00f6\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Das du an der Brust getragen,", "tokens": ["Das", "du", "an", "der", "Brust", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mit scheuen Augen schnelle", "tokens": ["Und", "mit", "scheu\u00b7en", "Au\u00b7gen", "schnel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Reichtest du mir's hin im Gehen:", "tokens": ["Reich\u00b7test", "du", "mir's", "hin", "im", "Ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Zitternd hob ich's an die Lippen,", "tokens": ["Zit\u00b7ternd", "hob", "ich's", "an", "die", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "K\u00fc\u00dft es br\u00fcnstig zwei- und dreimal;", "tokens": ["K\u00fc\u00dft", "es", "br\u00fcns\u00b7tig", "zwei", "und", "drei\u00b7mal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "TRUNC", "KON", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Niemand konnte dessen spotten,", "tokens": ["Nie\u00b7mand", "konn\u00b7te", "des\u00b7sen", "spot\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Keine Seele hat's gesehen,", "tokens": ["Kei\u00b7ne", "See\u00b7le", "hat's", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und du selber sahst es nicht.", "tokens": ["Und", "du", "sel\u00b7ber", "sahst", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "An dem fremden Haus, wohin", "tokens": ["An", "dem", "frem\u00b7den", "Haus", ",", "wo\u00b7hin"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PWAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich dich zu begleiten hatte,", "tokens": ["Ich", "dich", "zu", "be\u00b7glei\u00b7ten", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Standen wir nun, wei\u00dft, ich dr\u00fcckte", "tokens": ["Stan\u00b7den", "wir", "nun", ",", "wei\u00dft", ",", "ich", "dr\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "ADV", "$,", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir die Hand und \u2013", "tokens": ["Dir", "die", "Hand", "und", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "KON", "$("], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.9": {"line.1": {"text": "Dieses war zum letzten Male,", "tokens": ["Die\u00b7ses", "war", "zum", "letz\u00b7ten", "Ma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich mit dir ging, o Cl\u00e4rchen!", "tokens": ["Da\u00df", "ich", "mit", "dir", "ging", ",", "o", "Cl\u00e4r\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,", "FM", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, das war das letztemal,", "tokens": ["Ja", ",", "das", "war", "das", "letz\u00b7te\u00b7mal", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "PDS", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df wir uns wie Kinder freuten.", "tokens": ["Da\u00df", "wir", "uns", "wie", "Kin\u00b7der", "freu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "KOKOM", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}