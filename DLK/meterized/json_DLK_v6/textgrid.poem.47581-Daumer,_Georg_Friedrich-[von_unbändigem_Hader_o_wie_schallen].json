{"textgrid.poem.47581": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[von unb\u00e4ndigem Hader o wie schallen]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von unb\u00e4ndigem Hader o wie schallen", "tokens": ["Von", "un\u00b7b\u00e4n\u00b7di\u00b7gem", "Ha\u00b7der", "o", "wie", "schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "FM", "KOKOM", "VVINF"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Widerlichst akademische Meisterhallen!", "tokens": ["Wi\u00b7der\u00b7lichst", "a\u00b7kad\u00b7e\u00b7mi\u00b7sche", "Meis\u00b7ter\u00b7hal\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Blinde schrei'n sich allhier mit Blinden heiser,", "tokens": ["Blin\u00b7de", "schrei'n", "sich", "all\u00b7hier", "mit", "Blin\u00b7den", "hei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADV", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Es erschreckt Anathem und F\u00e4usteballen.", "tokens": ["Es", "er\u00b7schreckt", "A\u00b7nat\u00b7hem", "und", "F\u00e4us\u00b7te\u00b7bal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "La\u00df uns fl\u00fcchten in Allah's Dom, den stillen,", "tokens": ["La\u00df", "uns", "fl\u00fcch\u00b7ten", "in", "Al\u00b7lah's", "Dom", ",", "den", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Aus unsinniger Rede Tollhauskrallen!", "tokens": ["Aus", "un\u00b7sin\u00b7ni\u00b7ger", "Re\u00b7de", "Toll\u00b7haus\u00b7kral\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dem Gekose der Oste la\u00df uns horchen", "tokens": ["Dem", "Ge\u00b7ko\u00b7se", "der", "Os\u00b7te", "la\u00df", "uns", "hor\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVIMP", "PPER", "VVINF"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und krystallener Woge leisem Wallen!", "tokens": ["Und", "krys\u00b7tal\u00b7le\u00b7ner", "Wo\u00b7ge", "lei\u00b7sem", "Wal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Horchen uns der Er\u00f6rterungen sch\u00f6nster,", "tokens": ["Hor\u00b7chen", "uns", "der", "Er\u00b7\u00f6r\u00b7te\u00b7run\u00b7gen", "sch\u00f6ns\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dem Gesange verliebter Nachtigallen!", "tokens": ["Dem", "Ge\u00b7san\u00b7ge", "ver\u00b7lieb\u00b7ter", "Nach\u00b7ti\u00b7gal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}