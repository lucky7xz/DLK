{"textgrid.poem.61063": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "O sieh mich nicht so l\u00e4chelnd an", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O sieh mich nicht so l\u00e4chelnd an,", "tokens": ["O", "sieh", "mich", "nicht", "so", "l\u00e4\u00b7chelnd", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du R\u00f6slein jung, du schlankes Reh!", "tokens": ["Du", "R\u00f6s\u00b7lein", "jung", ",", "du", "schlan\u00b7kes", "Reh", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJD", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Blick, der jedem wohlgetan,", "tokens": ["Dein", "Blick", ",", "der", "je\u00b7dem", "wohl\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir tut er in der Seele weh;", "tokens": ["Mir", "tut", "er", "in", "der", "See\u00b7le", "weh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Herz wird tr\u00fcb und tr\u00fcber", "tokens": ["Mein", "Herz", "wird", "tr\u00fcb", "und", "tr\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bei deiner Freundlichkeit;", "tokens": ["Bei", "dei\u00b7ner", "Freund\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vor\u00fcber ist, vor\u00fcber", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "ist", ",", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "$,", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Liebe Zeit.", "tokens": ["Der", "Lie\u00b7be", "Zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ja w\u00e4r' ich jung und froh wie du,", "tokens": ["Ja", "w\u00e4r'", "ich", "jung", "und", "froh", "wie", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und w\u00e4r' ich so frisch, und w\u00e4r' ich so rein:", "tokens": ["Und", "w\u00e4r'", "ich", "so", "frisch", ",", "und", "w\u00e4r'", "ich", "so", "rein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADJD", "$,", "KON", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie schl\u00fcge mein Herz dem deinen zu,", "tokens": ["Wie", "schl\u00fc\u00b7ge", "mein", "Herz", "dem", "dei\u00b7nen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "ART", "PPOSAT", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie k\u00f6nnten wir selig zusammen sein!", "tokens": ["Wie", "k\u00f6nn\u00b7ten", "wir", "se\u00b7lig", "zu\u00b7sam\u00b7men", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Wie sollte durchs Gem\u00fcte", "tokens": ["Wie", "soll\u00b7te", "durchs", "Ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir ziehn ein s\u00fc\u00dfer Traum!", "tokens": ["Mir", "ziehn", "ein", "s\u00fc\u00b7\u00dfer", "Traum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch so - was soll die Bl\u00fcte", "tokens": ["Doch", "so", "was", "soll", "die", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$(", "PWS", "VMFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Am welken Baum?", "tokens": ["Am", "wel\u00b7ken", "Baum", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Mein Leben liegt im Abendrot,", "tokens": ["Mein", "Le\u00b7ben", "liegt", "im", "A\u00b7ben\u00b7drot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Deins tritt erst ein in den sonnigen Tag;", "tokens": ["Deins", "tritt", "erst", "ein", "in", "den", "son\u00b7ni\u00b7gen", "Tag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Mein Herz ist starr, mein Herz ist tot,", "tokens": ["Mein", "Herz", "ist", "starr", ",", "mein", "Herz", "ist", "tot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Deins hebt erst an den lustigsten Schlag;", "tokens": ["Deins", "hebt", "erst", "an", "den", "lus\u00b7tigs\u00b7ten", "Schlag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Du schaust nach deinem Gl\u00fccke", "tokens": ["Du", "schaust", "nach", "dei\u00b7nem", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In goldne Fernen weit,", "tokens": ["In", "gold\u00b7ne", "Fer\u00b7nen", "weit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich blicke schon zur\u00fccke", "tokens": ["Ich", "bli\u00b7cke", "schon", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In alte Zeit.", "tokens": ["In", "al\u00b7te", "Zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Drum sieh mich nicht so freundlich an,", "tokens": ["Drum", "sieh", "mich", "nicht", "so", "freund\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du R\u00f6slein jung, du schlankes Reh!", "tokens": ["Du", "R\u00f6s\u00b7lein", "jung", ",", "du", "schlan\u00b7kes", "Reh", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJD", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Blick, der jedem wohlgetan,", "tokens": ["Dein", "Blick", ",", "der", "je\u00b7dem", "wohl\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir tut er in der Seele weh.", "tokens": ["Mir", "tut", "er", "in", "der", "See\u00b7le", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df scheiden mich und wandern", "tokens": ["La\u00df", "schei\u00b7den", "mich", "und", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Welt hinauf, hinab;", "tokens": ["Die", "Welt", "hin\u00b7auf", ",", "hin\u00b7ab", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du findest einen andern,", "tokens": ["Du", "fin\u00b7dest", "ei\u00b7nen", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und ich - ein Grab.", "tokens": ["Und", "ich", "ein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}