{"textgrid.poem.26089": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Gr\u00fcnes Gras ist so wenig und ist so viel", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gr\u00fcnes Gras ist so wenig und ist so viel,", "tokens": ["Gr\u00fc\u00b7nes", "Gras", "ist", "so", "we\u00b7nig", "und", "ist", "so", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "PIS", "KON", "VAFIN", "ADV", "ADV", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wenn die Erde in Sack und Asche sa\u00df", "tokens": ["Wenn", "die", "Er\u00b7de", "in", "Sack", "und", "A\u00b7sche", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und beginnt von neuem ihr gr\u00fcnes Spiel.", "tokens": ["Und", "be\u00b7ginnt", "von", "neu\u00b7em", "ihr", "gr\u00fc\u00b7nes", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Gr\u00fcn Gras bringt Lust f\u00fcr Arm und Reich,", "tokens": ["Gr\u00fcn", "Gras", "bringt", "Lust", "f\u00fcr", "Arm", "und", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gr\u00fcn Gras nimmt die Menschen frisch an die Brust;", "tokens": ["Gr\u00fcn", "Gras", "nimmt", "die", "Men\u00b7schen", "frisch", "an", "die", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Im Gras liegt der \u00c4rmste auf Erden mal weich.", "tokens": ["Im", "Gras", "liegt", "der", "\u00c4rms\u00b7te", "auf", "Er\u00b7den", "mal", "weich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Gras w\u00e4chst \u00fcber Meilen mit Seelenruh,", "tokens": ["Gras", "w\u00e4chst", "\u00fc\u00b7ber", "Mei\u00b7len", "mit", "See\u00b7len\u00b7ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Gras auf seinem Gang deckt Taten zu und Gedanken, \u2013", "tokens": ["Gras", "auf", "sei\u00b7nem", "Gang", "deckt", "Ta\u00b7ten", "zu", "und", "Ge\u00b7dan\u00b7ken", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "NN", "PTKVZ", "KON", "NN", "$,", "$("], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Nur sieht man es lang noch im Gras, wo Zwei drin versanken.", "tokens": ["Nur", "sieht", "man", "es", "lang", "noch", "im", "Gras", ",", "wo", "Zwei", "drin", "ver\u00b7san\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ADJD", "ADV", "APPRART", "NN", "$,", "PWAV", "CARD", "ADV", "VVINF", "$."], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}}}}}