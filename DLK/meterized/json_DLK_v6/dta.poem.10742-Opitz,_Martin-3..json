{"dta.poem.10742": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Sie hats wol selbs erfahren/", "tokens": ["Sie", "hats", "wol", "selbs", "er\u00b7fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die sch\u00f6ne Venus zart/", "tokens": ["Die", "sch\u00f6\u00b7ne", "Ve\u00b7nus", "zart", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als sie stund in gefahren/", "tokens": ["Als", "sie", "stund", "in", "ge\u00b7fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "VVPP", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Vnd so zerritzet ward.", "tokens": ["Vnd", "so", "zer\u00b7rit\u00b7zet", "ward", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Daher die R\u00f6slein weis.", "tokens": ["Da\u00b7her", "die", "R\u00f6s\u00b7lein", "weis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Von Bluttrieffenden nerben", "tokens": ["Von", "Blut\u00b7trief\u00b7fen\u00b7den", "ner\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVINF"], "meter": "-++-+--", "measure": "unknown.measure.tri"}, "line.7": {"text": "Begunten sich zu ferben:", "tokens": ["Be\u00b7gun\u00b7ten", "sich", "zu", "fer\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den man verieht den preis.", "tokens": ["Den", "man", "ve\u00b7rieht", "den", "preis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}