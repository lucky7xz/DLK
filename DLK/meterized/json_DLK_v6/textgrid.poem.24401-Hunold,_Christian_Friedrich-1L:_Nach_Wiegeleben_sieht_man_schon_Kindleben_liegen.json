{"textgrid.poem.24401": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nach Wiegeleben sieht man schon Kindleben liegen/", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach Wiegeleben sieht man schon Kindleben liegen/", "tokens": ["Nach", "Wie\u00b7ge\u00b7le\u00b7ben", "sieht", "man", "schon", "Kind\u00b7le\u00b7ben", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "ADV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Da ist man Zehrung frey/ da lebt man mit Vergn\u00fcgen.", "tokens": ["Da", "ist", "man", "Zeh\u00b7rung", "frey", "/", "da", "lebt", "man", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "NN", "ADJD", "$(", "ADV", "VVFIN", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Buffleben komt darauf/ da gehn die Schulden an/", "tokens": ["Buf\u00b7fle\u00b7ben", "komt", "da\u00b7rauf", "/", "da", "gehn", "die", "Schul\u00b7den", "an", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PAV", "$(", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Wegeweisern ist man da offt \u00fcbel dran.", "tokens": ["Mit", "We\u00b7ge\u00b7wei\u00b7sern", "ist", "man", "da", "offt", "\u00fc\u00b7bel", "dran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PIS", "ADV", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch endlich l\u00e4sset sich noch Wandersleben finden.", "tokens": ["Doch", "end\u00b7lich", "l\u00e4s\u00b7set", "sich", "noch", "Wan\u00b7ders\u00b7le\u00b7ben", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da lauft man in die Welt/ vorn/ Seitswerts/ oben/ hinten.", "tokens": ["Da", "lauft", "man", "in", "die", "Welt", "/", "vorn", "/", "Seits\u00b7werts", "/", "o\u00b7ben", "/", "hin\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "$(", "ADV", "$(", "NE", "$(", "ADV", "$(", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch ein gerader Weg geht auf Nothleben zu/", "tokens": ["Doch", "ein", "ge\u00b7ra\u00b7der", "Weg", "geht", "auf", "Not\u00b7hle\u00b7ben", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da herberget man lang. Und endlich zu der Ruh", "tokens": ["Da", "her\u00b7ber\u00b7get", "man", "lang", ".", "Und", "end\u00b7lich", "zu", "der", "Ruh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "$.", "KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "(die Karte dieser Welt zeigt keinen Weg darneben)", "tokens": ["(", "die", "Kar\u00b7te", "die\u00b7ser", "Welt", "zeigt", "kei\u00b7nen", "Weg", "dar\u00b7ne\u00b7ben", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PDAT", "NN", "VVFIN", "PIAT", "NN", "PAV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Komt man in Grabesleben.", "tokens": ["Komt", "man", "in", "Gra\u00b7bes\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}