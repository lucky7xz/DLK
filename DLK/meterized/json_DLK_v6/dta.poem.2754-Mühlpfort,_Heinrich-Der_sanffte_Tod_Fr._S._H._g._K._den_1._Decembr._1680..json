{"dta.poem.2754": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der sanffte Tod/  \n  Fr. S. H. g. K. den 1. Decembr.  1680.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch Freundin/ du gehst hin in den betr\u00fcbten Zeiten/", "tokens": ["Auch", "Freun\u00b7din", "/", "du", "gehst", "hin", "in", "den", "be\u00b7tr\u00fcb\u00b7ten", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$(", "PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da \u00fcber unserm Kopf nichts als Cometen stehn/", "tokens": ["Da", "\u00fc\u00b7ber", "un\u00b7serm", "Kopf", "nichts", "als", "Co\u00b7me\u00b7ten", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PIS", "KOKOM", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da uns die Pest-Gefahr tritt n\u00e4her an der Seiten/", "tokens": ["Da", "uns", "die", "Pest\u00b7Ge\u00b7fahr", "tritt", "n\u00e4\u00b7her", "an", "der", "Sei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und niemand von uns wei\u00df/ wie es wird k\u00fcnftig gehn?", "tokens": ["Und", "nie\u00b7mand", "von", "uns", "wei\u00df", "/", "wie", "es", "wird", "k\u00fcnf\u00b7tig", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PPER", "VVFIN", "$(", "PWAV", "PPER", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da sag\u2019 ich gehst du hin. Wohin? In deine Kammer.", "tokens": ["Da", "sag'", "ich", "gehst", "du", "hin", ".", "Wo\u00b7hin", "?", "In", "dei\u00b7ne", "Kam\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "PWAV", "$.", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df dich nichts irren kan/ schleust du die Th\u00fcre zu.", "tokens": ["Da\u00df", "dich", "nichts", "ir\u00b7ren", "kan", "/", "schleust", "du", "die", "Th\u00fc\u00b7re", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVINF", "VMFIN", "$(", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach wie viel Hertzeleyd/ ach wie viel Noth und Jammer!", "tokens": ["Ach", "wie", "viel", "Hert\u00b7ze\u00b7leyd", "/", "ach", "wie", "viel", "Noth", "und", "Jam\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PIAT", "NN", "$(", "XY", "KOKOM", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wirstu hinfort nicht sehn in deiner sichren Ruh.", "tokens": ["Wirs\u00b7tu", "hin\u00b7fort", "nicht", "sehn", "in", "dei\u00b7ner", "sich\u00b7ren", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "VVINF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie nenn\u2019 ich deinen Tod? Ist es ein s\u00fcsses Schlaffen?", "tokens": ["Wie", "nenn'", "ich", "dei\u00b7nen", "Tod", "?", "Ist", "es", "ein", "s\u00fcs\u00b7ses", "Schlaf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ja/ weil dich gar gewi\u00df der Friede GOttes deckt.", "tokens": ["Ja", "/", "weil", "dich", "gar", "ge\u00b7wi\u00df", "der", "Frie\u00b7de", "Got\u00b7tes", "deckt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "KOUS", "PPER", "ADV", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es ist ein Hingang nur aus Marter und aus Straffen/", "tokens": ["Es", "ist", "ein", "Hin\u00b7gang", "nur", "aus", "Mar\u00b7ter", "und", "aus", "Straf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Womit des HErren Zorn die rohe Welt erschreckt.", "tokens": ["Wo\u00b7mit", "des", "Her\u00b7ren", "Zorn", "die", "ro\u00b7he", "Welt", "er\u00b7schreckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du bist nun auff dem Pa\u00df der wahren Ewigkeiten/", "tokens": ["Du", "bist", "nun", "auff", "dem", "Pa\u00df", "der", "wah\u00b7ren", "E\u00b7wig\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "K\u00f6mmst von der Pilgramschafft ins rechte Vaterland.", "tokens": ["K\u00f6mmst", "von", "der", "Pil\u00b7gramsc\u00b7hafft", "ins", "rech\u00b7te", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du tr\u00e4gst die Krone weg von deinem Kampff und Streiten/", "tokens": ["Du", "tr\u00e4gst", "die", "Kro\u00b7ne", "weg", "von", "dei\u00b7nem", "Kampff", "und", "Strei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und was du hier geglaubt/ hastu nun recht erkant.", "tokens": ["Und", "was", "du", "hier", "ge\u00b7glaubt", "/", "has\u00b7tu", "nun", "recht", "er\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVPP", "$(", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Jtzt bistu auffgel\u00f6st/ da du vor angebunden/", "tokens": ["Jtzt", "bis\u00b7tu", "auff\u00b7ge\u00b7l\u00f6st", "/", "da", "du", "vor", "an\u00b7ge\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$(", "KOUS", "PPER", "APPR", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wie schwere Ketten sind doch unser Fleisch und Blut!", "tokens": ["Wie", "schwe\u00b7re", "Ket\u00b7ten", "sind", "doch", "un\u00b7ser", "Fleisch", "und", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Nach Salsen dieser Welt hastu das Manna funden/", "tokens": ["Nach", "Sal\u00b7sen", "die\u00b7ser", "Welt", "has\u00b7tu", "das", "Man\u00b7na", "fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PDAT", "NN", "VAFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ein reiches Freuden-Meer fast deine Thr\u00e4nen Fluth.", "tokens": ["Ein", "rei\u00b7ches", "Freu\u00b7den\u00b7Meer", "fast", "dei\u00b7ne", "Thr\u00e4\u00b7nen", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Du schwebst in Glantz und Licht/ wir aber nur im Schatten;", "tokens": ["Du", "schwebst", "in", "Glantz", "und", "Licht", "/", "wir", "a\u00b7ber", "nur", "im", "Schat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$(", "PPER", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Bist frey/ da wir noch hier gleich Sclaven eingespannt.", "tokens": ["Bist", "frey", "/", "da", "wir", "noch", "hier", "gleich", "Scla\u00b7ven", "ein\u00b7ge\u00b7spannt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$(", "KOUS", "PPER", "ADV", "ADV", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Jtzt kan dich ferner nicht der Kranckheit Weh abmatten/", "tokens": ["Jtzt", "kan", "dich", "fer\u00b7ner", "nicht", "der", "Kran\u00b7ck\u00b7heit", "Weh", "ab\u00b7mat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Nun auch der letzte Feind/ der Tod/ ist \u00fcbermannt.", "tokens": ["Nun", "auch", "der", "letz\u00b7te", "Feind", "/", "der", "Tod", "/", "ist", "\u00fc\u00b7berm\u00b7annt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$(", "ART", "NN", "$(", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Dein Sieg ist sch\u00f6n und gro\u00df. Hier hie\u00df; noch Angst noch Leiden", "tokens": ["Dein", "Sieg", "ist", "sch\u00f6n", "und", "gro\u00df", ".", "Hier", "hie\u00df", ";", "noch", "Angst", "noch", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$.", "ADV", "VVFIN", "$.", "ADV", "NN", "ADV", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Noch Tr\u00fcbsal noch Gewalt/ noch Leben oder Tod/", "tokens": ["Noch", "Tr\u00fcb\u00b7sal", "noch", "Ge\u00b7walt", "/", "noch", "Le\u00b7ben", "o\u00b7der", "Tod", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "NN", "$(", "ADV", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Soll mich von GOttes Lieb\u2019 und meinem Heiland scheiden/", "tokens": ["Soll", "mich", "von", "Got\u00b7tes", "Lieb'", "und", "mei\u00b7nem", "Hei\u00b7land", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "NN", "NN", "KON", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Es geh\u2019 auch wie es will in meiner letzten Noth.", "tokens": ["Es", "geh'", "auch", "wie", "es", "will", "in", "mei\u00b7ner", "letz\u00b7ten", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOKOM", "PPER", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Dein Ruffen ward erh\u00f6rt/ verblichene Matrone/", "tokens": ["Dein", "Ruf\u00b7fen", "ward", "er\u00b7h\u00f6rt", "/", "ver\u00b7bli\u00b7che\u00b7ne", "Mat\u00b7ro\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der HErr verbarg darauff nicht l\u00e4nger sein Gesicht.", "tokens": ["Der", "Herr", "ver\u00b7barg", "dar\u00b7auff", "nicht", "l\u00e4n\u00b7ger", "sein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "PTKNEG", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Nahm dich als wie im Schlaf zu seinem Gnaden-Throne/", "tokens": ["Nahm", "dich", "als", "wie", "im", "Schlaf", "zu", "sei\u00b7nem", "Gna\u00b7den\u00b7Thro\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "KOKOM", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So da\u00df man kaum gesehn vergehn dein Lebens-Licht.", "tokens": ["So", "da\u00df", "man", "kaum", "ge\u00b7sehn", "ver\u00b7gehn", "dein", "Le\u00b7bens\u00b7Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "ADV", "VVPP", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Du sanckst/ als wie die Sonn in ihren Abend-Strahlen/", "tokens": ["Du", "sanckst", "/", "als", "wie", "die", "Sonn", "in", "ih\u00b7ren", "A\u00b7ben\u00b7dStrah\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "KOKOM", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Den h\u00f6chsten Purpur weist und segnet so die Welt:", "tokens": ["Den", "h\u00f6chs\u00b7ten", "Pur\u00b7pur", "weist", "und", "seg\u00b7net", "so", "die", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So muste dich zu letzt das Gold des Glaubens mahlen/", "tokens": ["So", "mus\u00b7te", "dich", "zu", "letzt", "das", "Gold", "des", "Glau\u00b7bens", "mah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "APPR", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und deine Hoffnung hat mehr die Gedult erh\u00e4lt.", "tokens": ["Und", "dei\u00b7ne", "Hoff\u00b7nung", "hat", "mehr", "die", "Ge\u00b7dult", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Lobt man denjenigen der auff dem Schauplatz stehet/", "tokens": ["Lobt", "man", "den\u00b7je\u00b7ni\u00b7gen", "der", "auff", "dem", "Schau\u00b7platz", "ste\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PDS", "ART", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.38": {"text": "Und mit Vergn\u00fcgen hat der Menschen Aug\u2019 ergetzt/", "tokens": ["Und", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "hat", "der", "Men\u00b7schen", "Aug'", "er\u00b7getzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wie vielmehr hat der Ruhm/ der so von hinnen gehet/", "tokens": ["Wie", "viel\u00b7mehr", "hat", "der", "Ruhm", "/", "der", "so", "von", "hin\u00b7nen", "ge\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "ART", "NN", "$(", "ART", "ADV", "APPR", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und aus der Eitelkeit den Fu\u00df gro\u00dfm\u00fcthig setzt?", "tokens": ["Und", "aus", "der", "Ei\u0