{"textgrid.poem.56881": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "P\u00f6blesse obligee", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbgutes la\u00dft uns stets beschweigen;", "tokens": ["\u00bb", "gu\u00b7tes", "la\u00dft", "uns", "stets", "be\u00b7schwei\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "denn es d\u00fcnkt uns selbstverst\u00e4ndlich;", "tokens": ["denn", "es", "d\u00fcnkt", "uns", "selbst\u00b7ver\u00b7st\u00e4nd\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlechtem aber stets bezeigen,", "tokens": ["Schlech\u00b7tem", "a\u00b7ber", "stets", "be\u00b7zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wie wir ihm so tief erkenntlich.\u00ab", "tokens": ["wie", "wir", "ihm", "so", "tief", "er\u00b7kennt\u00b7lich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "ADJD", "ADJD", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}