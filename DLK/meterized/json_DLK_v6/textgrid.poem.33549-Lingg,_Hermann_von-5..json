{"textgrid.poem.33549": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Voll von Gold und edlen Stoffen,", "tokens": ["Voll", "von", "Gold", "und", "ed\u00b7len", "Stof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von Juwelen wunderbar", "tokens": ["Von", "Ju\u00b7we\u00b7len", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liegt die weite Welt mir offen,", "tokens": ["Liegt", "die", "wei\u00b7te", "Welt", "mir", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ein pr\u00e4chtiger Bazar.", "tokens": ["Als", "ein", "pr\u00e4ch\u00b7ti\u00b7ger", "Ba\u00b7zar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "K\u00f6nnt' ich, was ich wollte, w\u00e4hlen,", "tokens": ["K\u00f6nnt'", "ich", ",", "was", "ich", "woll\u00b7te", ",", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PWS", "PPER", "VMFIN", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4hlt' ich wohl f\u00fcr mich und dich:", "tokens": ["W\u00e4hlt'", "ich", "wohl", "f\u00fcr", "mich", "und", "dich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "KON", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dir die Perlen und Juwelen", "tokens": ["Dir", "die", "Per\u00b7len", "und", "Ju\u00b7we\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dein goldnes Herz f\u00fcr mich.", "tokens": ["Und", "dein", "gold\u00b7nes", "Herz", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}