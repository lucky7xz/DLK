{"textgrid.poem.38734": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ueber Reisen kein Vergn\u00fcgen,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ueber Reisen kein Vergn\u00fcgen,", "tokens": ["Ue\u00b7ber", "Rei\u00b7sen", "kein", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn Gesundheit mit uns geht:", "tokens": ["Wenn", "Ge\u00b7sund\u00b7heit", "mit", "uns", "geht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hinter uns die St\u00e4dte liegen,", "tokens": ["Hin\u00b7ter", "uns", "die", "St\u00e4d\u00b7te", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Berg und Waldung vor mir steht.", "tokens": ["Berg", "und", "Wal\u00b7dung", "vor", "mir", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jenseit, jenseit, ist der Himmel heiter,", "tokens": ["Jen\u00b7seit", ",", "jen\u00b7seit", ",", "ist", "der", "Him\u00b7mel", "hei\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "$,", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Treibt mich rege Sehnsucht weiter.", "tokens": ["Treibt", "mich", "re\u00b7ge", "Sehn\u00b7sucht", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schau dich um, und la\u00df die tr\u00fcben Blicke,", "tokens": ["Schau", "dich", "um", ",", "und", "la\u00df", "die", "tr\u00fc\u00b7ben", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$,", "KON", "VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sieh, da liegt die gro\u00dfe weite Welt,", "tokens": ["Sieh", ",", "da", "liegt", "die", "gro\u00b7\u00dfe", "wei\u00b7te", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "In der Stadt blieb alles Graun zur\u00fccke,", "tokens": ["In", "der", "Stadt", "blieb", "al\u00b7les", "Graun", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Das den Sinn gefangen h\u00e4lt.", "tokens": ["Das", "den", "Sinn", "ge\u00b7fan\u00b7gen", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Endlich wieder Himmel, gr\u00fcne Flur,", "tokens": ["End\u00b7lich", "wie\u00b7der", "Him\u00b7mel", ",", "gr\u00fc\u00b7ne", "Flur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Gro\u00df und lieblich die Natur.", "tokens": ["Gro\u00df", "und", "lieb\u00b7lich", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auch ein M\u00e4dchen mu\u00df dich nimmer qu\u00e4len,", "tokens": ["Auch", "ein", "M\u00e4d\u00b7chen", "mu\u00df", "dich", "nim\u00b7mer", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kommst ja doch zu Menschen wieder hin,", "tokens": ["Kommst", "ja", "doch", "zu", "Men\u00b7schen", "wie\u00b7der", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nirgend wird es dir an Liebe fehlen,", "tokens": ["Nir\u00b7gend", "wird", "es", "dir", "an", "Lie\u00b7be", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ist dir Lieben ein Gewinn:", "tokens": ["Ist", "dir", "Lie\u00b7ben", "ein", "Ge\u00b7winn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Darum la\u00df die tr\u00fcben Blicke,", "tokens": ["Da\u00b7rum", "la\u00df", "die", "tr\u00fc\u00b7ben", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Allenthalben bl\u00fcht dein Gl\u00fccke.", "tokens": ["Al\u00b7len\u00b7thal\u00b7ben", "bl\u00fcht", "dein", "Gl\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Immer munter, Freunde, munter,", "tokens": ["Im\u00b7mer", "mun\u00b7ter", ",", "Freun\u00b7de", ",", "mun\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn mein M\u00e4dchen wartet schon,", "tokens": ["Denn", "mein", "M\u00e4d\u00b7chen", "war\u00b7tet", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Treibt den Flu\u00df nur rasch hinunter,", "tokens": ["Treibt", "den", "Flu\u00df", "nur", "rasch", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn mich d\u00fcnkt, mich lockt ihr Ton.", "tokens": ["Denn", "mich", "d\u00fcnkt", ",", "mich", "lockt", "ihr", "Ton", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "G\u00fcnstig sind uns alle Winde,", "tokens": ["G\u00fcns\u00b7tig", "sind", "uns", "al\u00b7le", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "St\u00fcrme schweigen, L\u00fcfte s\u00e4useln linde.", "tokens": ["St\u00fcr\u00b7me", "schwei\u00b7gen", ",", "L\u00fcf\u00b7te", "s\u00e4u\u00b7seln", "lin\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVFIN", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Siehst du die Sonne nicht,", "tokens": ["Siehst", "du", "die", "Son\u00b7ne", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gl\u00e4nzen im Bach?", "tokens": ["Gl\u00e4n\u00b7zen", "im", "Bach", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wo du bist, spielt das Licht", "tokens": ["Wo", "du", "bist", ",", "spielt", "das", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VAFIN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Freundlich dir nach.", "tokens": ["Freund\u00b7lich", "dir", "nach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Durch den Wald Funkelschein,", "tokens": ["Durch", "den", "Wald", "Fun\u00b7kel\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Sieht in den Quell;", "tokens": ["Sieht", "in", "den", "Quell", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Kuckt in die Fluth hinein,", "tokens": ["Kuckt", "in", "die", "Fluth", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Macht tausend Str\u00f6me hell.", "tokens": ["Macht", "tau\u00b7send", "Str\u00f6\u00b7me", "hell", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So auch der Liebe Licht,", "tokens": ["So", "auch", "der", "Lie\u00b7be", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wandelt mit dir;", "tokens": ["Wan\u00b7delt", "mit", "dir", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "L\u00f6schet wohl nimmer nicht,", "tokens": ["L\u00f6\u00b7schet", "wohl", "nim\u00b7mer", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PTKNEG", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ist dorten bald, bald hier.", "tokens": ["Ist", "dor\u00b7ten", "bald", ",", "bald", "hier", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Liebst du die Morgenpracht,", "tokens": ["Liebst", "du", "die", "Mor\u00b7gen\u00b7pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenn nach der schwarzen Nacht", "tokens": ["Wenn", "nach", "der", "schwar\u00b7zen", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf diamantner Bahn", "tokens": ["Auf", "di\u00b7a\u00b7mant\u00b7ner", "Bahn"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die Sonne ihren Weg begann?", "tokens": ["Die", "Son\u00b7ne", "ih\u00b7ren", "Weg", "be\u00b7gann", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wenn alle V\u00f6gel jubeln laut,", "tokens": ["Wenn", "al\u00b7le", "V\u00f6\u00b7gel", "ju\u00b7beln", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begr\u00fc\u00dfen fr\u00f6hlich des Tages Braut,", "tokens": ["Be\u00b7gr\u00fc\u00b7\u00dfen", "fr\u00f6h\u00b7lich", "des", "Ta\u00b7ges", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wenn Wolken sich zu F\u00fc\u00dfen schmiegen,", "tokens": ["Wenn", "Wol\u00b7ken", "sich", "zu", "F\u00fc\u00b7\u00dfen", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Brand und goldnem Feuer fliegen?", "tokens": ["In", "Brand", "und", "gold\u00b7nem", "Feu\u00b7er", "flie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Auch wenn die Sonne nun den Wagen lenkt,", "tokens": ["Auch", "wenn", "die", "Son\u00b7ne", "nun", "den", "Wa\u00b7gen", "lenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und hinter ihr das Morgenroth erbleicht,", "tokens": ["Und", "hin\u00b7ter", "ihr", "das", "Mor\u00b7gen\u00b7roth", "er\u00b7bleicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Lust, Heiterkeit durch alle Welt hinfleugt,", "tokens": ["Lust", ",", "Hei\u00b7ter\u00b7keit", "durch", "al\u00b7le", "Welt", "hin\u00b7fleugt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bis sich zum Meer die G\u00f6ttinn senkt.", "tokens": ["Bis", "sich", "zum", "Meer", "die", "G\u00f6t\u00b7tinn", "senkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Und dann funkeln neue Schimmer", "tokens": ["Und", "dann", "fun\u00b7keln", "neu\u00b7e", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber See und \u00fcber Land,", "tokens": ["Ue\u00b7ber", "See", "und", "\u00fc\u00b7ber", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Erd' und Himmel im Geflimmer", "tokens": ["Erd'", "und", "Him\u00b7mel", "im", "Ge\u00b7flim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich zu Einem Glanz verband.", "tokens": ["Sich", "zu", "Ei\u00b7nem", "Glanz", "ver\u00b7band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Pr\u00e4chtig mit Rubinen und Sapphiren,", "tokens": ["Pr\u00e4ch\u00b7tig", "mit", "Ru\u00b7bi\u00b7nen", "und", "Sap\u00b7phi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Siehst du dann den Abendhimmel prangen,", "tokens": ["Siehst", "du", "dann", "den", "A\u00b7bend\u00b7him\u00b7mel", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Goldenes Geschmeide um ihn hangen,", "tokens": ["Gol\u00b7de\u00b7nes", "Ge\u00b7schmei\u00b7de", "um", "ihn", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Edelsteine Hals und Nacken zieren,", "tokens": ["E\u00b7del\u00b7stei\u00b7ne", "Hals", "und", "Na\u00b7cken", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und in holder Gluth die sch\u00f6nen Wangen.", "tokens": ["Und", "in", "hol\u00b7der", "Gluth", "die", "sch\u00f6\u00b7nen", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dr\u00e4ngt sich nicht mit stillem Licht der Chor", "tokens": ["Dr\u00e4ngt", "sich", "nicht", "mit", "stil\u00b7lem", "Licht", "der", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PTKNEG", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Aller Sterne, ihn zu sehen, vor?", "tokens": ["Al\u00b7ler", "Ster\u00b7ne", ",", "ihn", "zu", "se\u00b7hen", ",", "vor", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Jubeln nicht die Lerchen ihre Lieder,", "tokens": ["Ju\u00b7beln", "nicht", "die", "Ler\u00b7chen", "ih\u00b7re", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "T\u00f6nt nicht Fels und Meer Ges\u00e4nge wieder? \u2013", "tokens": ["T\u00f6nt", "nicht", "Fels", "und", "Meer", "Ge\u00b7s\u00e4n\u00b7ge", "wie\u00b7der", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKNEG", "NN", "KON", "NN", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Also wenn die erste Liebe dir entschwunden,", "tokens": ["Al\u00b7so", "wenn", "die", "ers\u00b7te", "Lie\u00b7be", "dir", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Mu\u00dft du weibisch nicht verzagen,", "tokens": ["Mu\u00dft", "du", "wei\u00b7bisch", "nicht", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sondern dreist dein Gl\u00fccke wagen,", "tokens": ["Son\u00b7dern", "dreist", "dein", "Gl\u00fc\u00b7cke", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bald hast du die zweite aufgefunden;", "tokens": ["Bald", "hast", "du", "die", "zwei\u00b7te", "auf\u00b7ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und kannst du im Rausche dann noch klagen:", "tokens": ["Und", "kannst", "du", "im", "Rau\u00b7sche", "dann", "noch", "kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPRART", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Nie empfand ich, was ich vor empfunden?", "tokens": ["Nie", "emp\u00b7fand", "ich", ",", "was", "ich", "vor", "emp\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "APPR", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.14": {"line.1": {"text": "Nie vergi\u00dft der Fr\u00fchling wiederzukommen,", "tokens": ["Nie", "ver\u00b7gi\u00dft", "der", "Fr\u00fch\u00b7ling", "wie\u00b7der\u00b7zu\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wenn St\u00f6rche ziehn, wenn Schwalben auf der Wiese sind,", "tokens": ["Wenn", "St\u00f6r\u00b7che", "ziehn", ",", "wenn", "Schwal\u00b7ben", "auf", "der", "Wie\u00b7se", "sind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$,", "KOUS", "NN", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kaum ist dem Winter die Herrschaft genommen,", "tokens": ["Kaum", "ist", "dem", "Win\u00b7ter", "die", "Herr\u00b7schaft", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So erwacht und l\u00e4chelt das goldne Kind.", "tokens": ["So", "er\u00b7wacht", "und", "l\u00e4\u00b7chelt", "das", "gold\u00b7ne", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.15": {"line.1": {"text": "Dann sucht er sein Spielzeug wieder zusammen,", "tokens": ["Dann", "sucht", "er", "sein", "Spiel\u00b7zeug", "wie\u00b7der", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das der alte Winter verlegt und verst\u00f6rt,", "tokens": ["Das", "der", "al\u00b7te", "Win\u00b7ter", "ver\u00b7legt", "und", "ver\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er putzt den Wald mit gr\u00fcnen Flammen,", "tokens": ["Er", "putzt", "den", "Wald", "mit", "gr\u00fc\u00b7nen", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Nachtigall er die Lieder lehrt.", "tokens": ["Der", "Nach\u00b7ti\u00b7gall", "er", "die", "Lie\u00b7der", "lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.16": {"line.1": {"text": "Er r\u00fchrt den Obstbaum mit r\u00f6thlicher Hand,", "tokens": ["Er", "r\u00fchrt", "den", "Obst\u00b7baum", "mit", "r\u00f6th\u00b7li\u00b7cher", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er klettert hinauf die Aprikosenwand,", "tokens": ["Er", "klet\u00b7tert", "hin\u00b7auf", "die", "Ap\u00b7ri\u00b7ko\u00b7sen\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie Schnee die Bl\u00fcthe noch vor dem Blatt ausdringt,", "tokens": ["Wie", "Schnee", "die", "Bl\u00fc\u00b7the", "noch", "vor", "dem", "Blatt", "aus\u00b7dringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Er sch\u00fcttelt froh das K\u00f6pfchen, da\u00df ihm die Arbeit gelingt.", "tokens": ["Er", "sch\u00fct\u00b7telt", "froh", "das", "K\u00f6pf\u00b7chen", ",", "da\u00df", "ihm", "die", "Ar\u00b7beit", "ge\u00b7lingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.17": {"line.1": {"text": "Dann geht er, und schl\u00e4ft im waldigen Grund,", "tokens": ["Dann", "geht", "er", ",", "und", "schl\u00e4ft", "im", "wal\u00b7di\u00b7gen", "Grund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und haucht den Athem aus, den s\u00fc\u00dfen,", "tokens": ["Und", "haucht", "den", "A\u00b7them", "aus", ",", "den", "s\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Um seinen zarten rothen Mund", "tokens": ["Um", "sei\u00b7nen", "zar\u00b7ten", "ro\u00b7then", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Grase Viol' und Erdbeer sprie\u00dfen:", "tokens": ["Im", "Gra\u00b7se", "Vi\u00b7ol'", "und", "Erd\u00b7beer", "sprie\u00b7\u00dfen", ":"], "token_info": ["word", 