"trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Er r\u00e4umt den Schnee aus dem Wege,", "tokens": ["Er", "r\u00e4umt", "den", "Schnee", "aus", "dem", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Er schneidet das Buxbaum-Gehege,", "tokens": ["Er", "schnei\u00b7det", "das", "Bux\u00b7baum\u00b7Ge\u00b7he\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und feiert auch am Abend nicht,", "tokens": ["Und", "fei\u00b7ert", "auch", "am", "A\u00b7bend", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er schaufelt und arbeitet im Mondenlicht.", "tokens": ["Er", "schau\u00b7felt", "und", "ar\u00b7bei\u00b7tet", "im", "Mon\u00b7den\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.20": {"line.1": {"text": "Dann ruft er: wo s\u00e4umen die Spielkameraden,", "tokens": ["Dann", "ruft", "er", ":", "wo", "s\u00e4u\u00b7men", "die", "Spiel\u00b7ka\u00b7me\u00b7ra\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df sie so lange in der Erde bleiben?", "tokens": ["Da\u00df", "sie", "so", "lan\u00b7ge", "in", "der", "Er\u00b7de", "blei\u00b7ben", "?"], "token_info": ["word", "word", "w