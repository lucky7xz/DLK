{"textgrid.poem.58905": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seel', willst du gerne heilig sein,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seel', willst du gerne heilig sein,", "tokens": ["Seel'", ",", "willst", "du", "ger\u00b7ne", "hei\u00b7lig", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So f\u00fcrchte keine M\u00fch und Pein!", "tokens": ["So", "f\u00fcrch\u00b7te", "kei\u00b7ne", "M\u00fch", "und", "Pein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man kann ohn' Leid und Streit und Sterben", "tokens": ["Man", "kann", "ohn'", "Leid", "und", "Streit", "und", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dies sch\u00f6ne Kleinod nicht ererben.", "tokens": ["Dies", "sch\u00f6\u00b7ne", "Klei\u00b7nod", "nicht", "er\u00b7er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}