{"textgrid.poem.55548": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Kinderverstand", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In gro\u00dfen St\u00e4dten lernen fr\u00fch", "tokens": ["In", "gro\u00b7\u00dfen", "St\u00e4d\u00b7ten", "ler\u00b7nen", "fr\u00fch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die j\u00fcngsten Knaben was;", "tokens": ["Die", "j\u00fcng\u00b7sten", "Kna\u00b7ben", "was", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PWS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn manche B\u00fccher lesen sie", "tokens": ["Denn", "man\u00b7che", "B\u00fc\u00b7cher", "le\u00b7sen", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und h\u00f6ren dies und das", "tokens": ["Und", "h\u00f6\u00b7ren", "dies", "und", "das"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PDS", "KON", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vom Lieben und vom K\u00fcssen,", "tokens": ["Vom", "Lie\u00b7ben", "und", "vom", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie brauchten's nicht zu wissen.", "tokens": ["Sie", "brauch\u00b7ten's", "nicht", "zu", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und mancher ist im zw\u00f6lften Jahr", "tokens": ["Und", "man\u00b7cher", "ist", "im", "zw\u00f6lf\u00b7ten", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Fast kl\u00fcger, als sein Vater war,", "tokens": ["Fast", "kl\u00fc\u00b7ger", ",", "als", "sein", "Va\u00b7ter", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da er die Mutter nahm.", "tokens": ["Da", "er", "die", "Mut\u00b7ter", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das M\u00e4dchen w\u00fcnscht von Jugend auf", "tokens": ["Das", "M\u00e4d\u00b7chen", "w\u00fcnscht", "von", "Ju\u00b7gend", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich hochgeehrt zu sehn,", "tokens": ["Sich", "hoch\u00b7geehrt", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sie ziert sich klein und w\u00e4chst herauf", "tokens": ["Sie", "ziert", "sich", "klein", "und", "w\u00e4chst", "her\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Pracht und Assembleen.", "tokens": ["In", "Pracht", "und", "As\u00b7sem\u00b7bleen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Stolz verjagt die Triebe", "tokens": ["Der", "Stolz", "ver\u00b7jagt", "die", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Wollust und der Liebe,", "tokens": ["Der", "Wol\u00b7lust", "und", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sie sinnt nur drauf, wie sie sich ziert,", "tokens": ["Sie", "sinnt", "nur", "drauf", ",", "wie", "sie", "sich", "ziert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Aug entz\u00fcckt, ein Herze r\u00fchrt,", "tokens": ["Ein", "Aug", "ent\u00b7z\u00fcckt", ",", "ein", "Her\u00b7ze", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und denkt ans andre nicht.", "tokens": ["Und", "denkt", "ans", "and\u00b7re", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "PIS", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auf D\u00f6rfern sieht's ganz anders aus,", "tokens": ["Auf", "D\u00f6r\u00b7fern", "sieht's", "ganz", "an\u00b7ders", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da treibt die liebe Not", "tokens": ["Da", "treibt", "die", "lie\u00b7be", "Not"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Jungen auf das Feld hinaus", "tokens": ["Die", "Jun\u00b7gen", "auf", "das", "Feld", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Arbeit und nach Brot.", "tokens": ["Nach", "Ar\u00b7beit", "und", "nach", "Brot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer von der Arbeit m\u00fcde,", "tokens": ["Wer", "von", "der", "Ar\u00b7beit", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "L\u00e4\u00dft gern den M\u00e4dchen Friede.", "tokens": ["L\u00e4\u00dft", "gern", "den", "M\u00e4d\u00b7chen", "Frie\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wer noch obendrein nichts wei\u00df,", "tokens": ["Und", "wer", "noch", "o\u00b7ben\u00b7drein", "nichts", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der denkt an nichts, den macht nichts hei\u00df;", "tokens": ["Der", "denkt", "an", "nichts", ",", "den", "macht", "nichts", "hei\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PIS", "$,", "ART", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So geht's den Bauern meist.", "tokens": ["So", "geht's", "den", "Bau\u00b7ern", "meist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Bauernm\u00e4dchen aber sind", "tokens": ["Die", "Bau\u00b7ern\u00b7m\u00e4d\u00b7chen", "a\u00b7ber", "sind"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Ruhe mehr gen\u00e4hrt,", "tokens": ["In", "Ru\u00b7he", "mehr", "ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und darum w\u00fcnschen sie geschwind,", "tokens": ["Und", "da\u00b7rum", "w\u00fcn\u00b7schen", "sie", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was jede Mutter wehrt.", "tokens": ["Was", "je\u00b7de", "Mut\u00b7ter", "wehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Oft sto\u00dfen sch\u00f6ckernd Br\u00e4ute", "tokens": ["Oft", "sto\u00b7\u00dfen", "sch\u00f6\u00b7ckernd", "Br\u00e4u\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Br\u00e4ut'gam in die Seite,", "tokens": ["Den", "Br\u00e4ut'\u00b7gam", "in", "die", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Denn von der Arbeit, die sie tun,", "tokens": ["Denn", "von", "der", "Ar\u00b7beit", ",", "die", "sie", "tun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich zu erholen, auszuruhn,", "tokens": ["Sich", "zu", "er\u00b7ho\u00b7len", ",", "aus\u00b7zu\u00b7ruhn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das k\u00f6nnen sie dabei.", "tokens": ["Das", "k\u00f6n\u00b7nen", "sie", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}