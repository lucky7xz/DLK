{"textgrid.poem.56360": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Die Retraite", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Sonntag Abend auf dem Werder waren", "tokens": ["Am", "Sonn\u00b7tag", "A\u00b7bend", "auf", "dem", "Wer\u00b7der", "wa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "APPR", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum lust'gen Walzer in dem Fischerkrug", "tokens": ["Zum", "lust'\u00b7gen", "Wal\u00b7zer", "in", "dem", "Fi\u00b7scher\u00b7krug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die sechs Trompeter da von den Husaren.", "tokens": ["Die", "sechs", "Trom\u00b7pe\u00b7ter", "da", "von", "den", "Hu\u00b7sa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Her\u00fcber von dem andern Ufer trug", "tokens": ["Her\u00b7\u00fc\u00b7ber", "von", "dem", "an\u00b7dern", "U\u00b7fer", "trug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie noch das Eis, nun gab es Spiel und Tanz;", "tokens": ["Sie", "noch", "das", "Eis", ",", "nun", "gab", "es", "Spiel", "und", "Tanz", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es waren zum Orchester f\u00fcnf genug.", "tokens": ["Es", "wa\u00b7ren", "zum", "Or\u00b7ches\u00b7ter", "f\u00fcnf", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "CARD", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Der sechste hielt sich abgesondert, Franz,", "tokens": ["Der", "sechs\u00b7te", "hielt", "sich", "ab\u00b7ge\u00b7son\u00b7dert", ",", "Franz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "VVPP", "$,", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Er kos'te wohl mit seiner Braut verstohlen,", "tokens": ["Er", "kos'\u00b7te", "wohl", "mit", "sei\u00b7ner", "Braut", "ver\u00b7stoh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der Margarethe, der geh\u00f6rt er ganz.", "tokens": ["Der", "Mar\u00b7ga\u00b7re\u00b7the", ",", "der", "ge\u00b7h\u00f6rt", "er", "ganz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "\u00bbwir haben unsre Sache Gott befohlen,", "tokens": ["\u00bb", "wir", "ha\u00b7ben", "uns\u00b7re", "Sa\u00b7che", "Gott", "be\u00b7foh\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und hat der Fr\u00fchling erst den Flu\u00df befreit,", "tokens": ["Und", "hat", "der", "Fr\u00fch\u00b7ling", "erst", "den", "Flu\u00df", "be\u00b7freit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "So komm ich nur, hin\u00fcber dich zu holen.\u00ab", "tokens": ["So", "komm", "ich", "nur", ",", "hin\u00b7\u00fc\u00b7ber", "dich", "zu", "ho\u00b7len", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ADV", "PPER", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "\u00bbo Franz! und diese lange, bange Zeit!", "tokens": ["\u00bb", "o", "Franz", "!", "und", "die\u00b7se", "lan\u00b7ge", ",", "ban\u00b7ge", "Zeit", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "NE", "$.", "KON", "PDS", "ADV", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Wie soll ich, dich zu sehen, mich entw\u00f6hnen,", "tokens": ["Wie", "soll", "ich", ",", "dich", "zu", "se\u00b7hen", ",", "mich", "ent\u00b7w\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Du bist mein Leben, meine Seligkeit!\u00ab \u2013", "tokens": ["Du", "bist", "mein", "Le\u00b7ben", ",", "mei\u00b7ne", "Se\u00b7lig\u00b7keit", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "\u00bbdu h\u00f6rst mich, h\u00f6rest die Trompete dr\u00f6hnen,", "tokens": ["\u00bb", "du", "h\u00f6rst", "mich", ",", "h\u00f6\u00b7rest", "die", "Trom\u00b7pe\u00b7te", "dr\u00f6h\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Sie wird dir meiner Liebe Botschaft bringen", "tokens": ["Sie", "wird", "dir", "mei\u00b7ner", "Lie\u00b7be", "Bot\u00b7schaft", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PPOSAT", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Bei der Retrait' in Nachhalls-Zittert\u00f6nen.", "tokens": ["Bei", "der", "Re\u00b7trait'", "in", "Nach\u00b7halls\u00b7Zit\u00b7ter\u00b7t\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Wenn diese letzten T\u00f6ne zu dir dringen,", "tokens": ["Wenn", "die\u00b7se", "letz\u00b7ten", "T\u00f6\u00b7ne", "zu", "dir", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Ich bin's, gedenke mein, dann weht von dr\u00fcben", "tokens": ["Ich", "bin's", ",", "ge\u00b7den\u00b7ke", "mein", ",", "dann", "weht", "von", "dr\u00fc\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "PPOSAT", "$,", "ADV", "VVFIN", "APPR", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Dir meine Seele zu auf ihren Schwingen.", "tokens": ["Dir", "mei\u00b7ne", "See\u00b7le", "zu", "auf", "ih\u00b7ren", "Schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKZU", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Mag doch der Eisgang kurze Feindschaft \u00fcben,", "tokens": ["Mag", "doch", "der", "Eis\u00b7gang", "kur\u00b7ze", "Feind\u00b7schaft", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Der Fr\u00fchling unsrer Liebe wird erwachen,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "uns\u00b7rer", "Lie\u00b7be", "wird", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Und keine Trennung f\u00fcrder uns betr\u00fcben.\u00ab \u2013", "tokens": ["Und", "kei\u00b7ne", "Tren\u00b7nung", "f\u00fcr\u00b7der", "uns", "be\u00b7tr\u00fc\u00b7ben", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPER", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "\u00bbh\u00f6rt auf! wer mag noch l\u00e4rmen hier und lachen!\u00ab", "tokens": ["\u00bb", "h\u00f6rt", "auf", "!", "wer", "mag", "noch", "l\u00e4r\u00b7men", "hier", "und", "la\u00b7chen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$.", "PWS", "VMFIN", "ADV", "VVFIN", "ADV", "KON", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Ein Fischer sprang herein und schrie das Wort;", "tokens": ["Ein", "Fi\u00b7scher", "sprang", "her\u00b7ein", "und", "schrie", "das", "Wort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "\u00bbh\u00f6rt ihr denn drau\u00dfen nicht des Eises Krachen!?", "tokens": ["\u00bb", "h\u00f6rt", "ihr", "denn", "drau\u00b7\u00dfen", "nicht", "des", "Ei\u00b7ses", "Kra\u00b7chen", "!?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.28": {"text": "Ihr Herrn, die ihr hin\u00fcber m\u00fc\u00dft, macht fort;", "tokens": ["Ihr", "Herrn", ",", "die", "ihr", "hin\u00b7\u00fc\u00b7ber", "m\u00fc\u00dft", ",", "macht", "fort", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "VMFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Stromauf! da h\u00e4lt sich's l\u00e4nger, bis es bricht,", "tokens": ["Stro\u00b7mauf", "!", "da", "h\u00e4lt", "sich's", "l\u00e4n\u00b7ger", ",", "bis", "es", "bricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PIS", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Dem Lichte zu am andern Ufer dort!\u00ab \u2013", "tokens": ["Dem", "Lich\u00b7te", "zu", "am", "an\u00b7dern", "U\u00b7fer", "dort", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "APPR", "APPRART", "ADJA", "NN", "ADV", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "\u00bbo Franz, bleib hier!\u00ab \u2013 \u00bbMein Lieb, ich darf es nicht,", "tokens": ["\u00bb", "o", "Franz", ",", "bleib", "hier", "!", "\u00ab", "\u2013", "\u00bb", "Mein", "Lieb", ",", "ich", "darf", "es", "nicht", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NE", "$,", "VVFIN", "ADV", "$.", "$(", "$(", "$(", "PPOSAT", "NN", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Nicht Urlaub hab ich.\u00ab \u2013 \u00bbMeines Vaters Haus...\u00ab \u2013", "tokens": ["Nicht", "Ur\u00b7laub", "hab", "ich", ".", "\u00ab", "\u2013", "\u00bb", "Mei\u00b7nes", "Va\u00b7ters", "Haus", "...", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PTKNEG", "NN", "VAFIN", "PPER", "$.", "$(", "$(", "$(", "PPOSAT", "NN", "NN", "$(", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "\u00bbich bin Soldat und kenne meine Pflicht.\u00ab \u2013", "tokens": ["\u00bb", "ich", "bin", "Sol\u00b7dat", "und", "ken\u00b7ne", "mei\u00b7ne", "Pflicht", ".", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "\u00bbo lieber Franz, in solchem n\u00e4cht'gen Graus...!\u00ab \u2013", "tokens": ["\u00bb", "o", "lie\u00b7ber", "Franz", ",", "in", "sol\u00b7chem", "n\u00e4cht'\u00b7gen", "Graus", "...", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["$(", "FM", "ADV", "NE", "$,", "APPR", "PIAT", "ADJA", "NN", "$(", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "\u00bbwir scheiden ja, mein Lieb, zum letzten Male;", "tokens": ["\u00bb", "wir", "schei\u00b7den", "ja", ",", "mein", "Lieb", ",", "zum", "letz\u00b7ten", "Ma\u00b7le", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "La\u00df ab! sei stark! die andern sind voraus.\u00ab", "tokens": ["La\u00df", "ab", "!", "sei", "stark", "!", "die", "an\u00b7dern", "sind", "vo\u00b7raus", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PTKVZ", "$.", "VAFIN", "ADJD", "$.", "ART", "ADJA", "VAFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Stromauf, schr\u00e4g \u00fcber, nach dem Lichtsignale,", "tokens": ["Stro\u00b7mauf", ",", "schr\u00e4g", "\u00fc\u00b7ber", ",", "nach", "dem", "Licht\u00b7sig\u00b7na\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Sie schritten schnell und schweigsam durch die Nacht,", "tokens": ["Sie", "schrit\u00b7ten", "schnell", "und", "schweig\u00b7sam", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Erhellt von keines Sternes bleichem Strahle;", "tokens": ["Er\u00b7hellt", "von", "kei\u00b7nes", "Ster\u00b7nes", "blei\u00b7chem", "Strah\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "In Nebeln, von dem Winde hergefacht,", "tokens": ["In", "Ne\u00b7beln", ",", "von", "dem", "Win\u00b7de", "her\u00b7ge\u00b7facht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Schien ihnen oft das Lichtlein zu verschweben;", "tokens": ["Schien", "ih\u00b7nen", "oft", "das", "Licht\u00b7lein", "zu", "ver\u00b7schwe\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Sie schritten zu, als ging es in die Schlacht.", "tokens": ["Sie", "schrit\u00b7ten", "zu", ",", "als", "ging", "es", "in", "die", "Schlacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KOUS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Sie f\u00fchlten unter sich das Eis erbeben,", "tokens": ["Sie", "f\u00fchl\u00b7ten", "un\u00b7ter", "sich", "das", "Eis", "er\u00b7be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Und h\u00f6rten'