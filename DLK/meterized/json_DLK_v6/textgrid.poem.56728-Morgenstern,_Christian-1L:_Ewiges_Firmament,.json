{"textgrid.poem.56728": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ewiges Firmament,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ewiges Firmament,", "tokens": ["E\u00b7wi\u00b7ges", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "mit den feurigen Spielen", "tokens": ["mit", "den", "feu\u00b7ri\u00b7gen", "Spie\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "deiner Gestirne,", "tokens": ["dei\u00b7ner", "Ge\u00b7stir\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "wie bist du entstanden?", "tokens": ["wie", "bist", "du", "ent\u00b7stan\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Du blauer Sammet!", "tokens": ["Du", "blau\u00b7er", "Sam\u00b7met", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Welch flei\u00dfige G\u00f6ttin", "tokens": ["Welch", "flei\u00b7\u00dfi\u00b7ge", "G\u00f6t\u00b7tin"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "hat sich auf dir", "tokens": ["hat", "sich", "auf", "dir"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "APPR", "PPER"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "mit goldnen und silbernen", "tokens": ["mit", "gold\u00b7nen", "und", "sil\u00b7ber\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "KON", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.5": {"text": "Kreuzstichmustern verewigt?", "tokens": ["Kreuz\u00b7stich\u00b7mus\u00b7tern", "ve\u00b7re\u00b7wigt", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie! oder w\u00e4ren", "tokens": ["Wie", "!", "o\u00b7der", "w\u00e4\u00b7ren"], "token_info": ["word", "punct", "word", "word"], "pos": ["PWAV", "$.", "KON", "VAFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "die Sterne Perlen,", "tokens": ["die", "Ster\u00b7ne", "Per\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "tages\u00fcber", "tokens": ["ta\u00b7ges\u00b7\u00fc\u00b7ber"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "in Wolkenmuscheln gebettet:", "tokens": ["in", "Wol\u00b7ken\u00b7mu\u00b7scheln", "ge\u00b7bet\u00b7tet", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Aber des Nachts", "tokens": ["A\u00b7ber", "des", "Nachts"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "tuen die Schalen sich auf,", "tokens": ["tu\u00b7en", "die", "Scha\u00b7len", "sich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "und aus den schwarzen,", "tokens": ["und", "aus", "den", "schwar\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "angelspottenden Tiefen empor", "tokens": ["an\u00b7gel\u00b7spot\u00b7ten\u00b7den", "Tie\u00b7fen", "em\u00b7por"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "PTKVZ"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "lachen und funkeln", "tokens": ["la\u00b7chen", "und", "fun\u00b7keln"], "token_info": ["word", "word", "word"], "pos": ["VVINF", "KON", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "die schimmernden Sch\u00e4tze", "tokens": ["die", "schim\u00b7mern\u00b7den", "Sch\u00e4t\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "des Meers Unendlichkeit?", "tokens": ["des", "Meers", "Un\u00b7end\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Oft auch ist mir,", "tokens": ["Oft", "auch", "ist", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "ein m\u00e4chtig gew\u00f6lbter", "tokens": ["ein", "m\u00e4ch\u00b7tig", "ge\u00b7w\u00f6lb\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "kristallener Spiegel", "tokens": ["kris\u00b7tal\u00b7le\u00b7ner", "Spie\u00b7gel"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "sei dieser Himmel,", "tokens": ["sei", "die\u00b7ser", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "und was wir staunend", "tokens": ["und", "was", "wir", "stau\u00b7nend"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Gestirne nennen,", "tokens": ["Ge\u00b7stir\u00b7ne", "nen\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "das seien Millionen", "tokens": ["das", "sei\u00b7en", "Mil\u00b7lion\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VAFIN", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "and\u00e4chtiger Augen,", "tokens": ["an\u00b7d\u00e4ch\u00b7ti\u00b7ger", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "die strahlend", "tokens": ["die", "strah\u00b7lend"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "in seinem Dunkel sich spiegeln.", "tokens": ["in", "sei\u00b7nem", "Dun\u00b7kel", "sich", "spie\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Oder w\u00f6lbt", "tokens": ["O\u00b7der", "w\u00f6lbt"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "eines Kerkers bl\u00e4uliche Finsternis", "tokens": ["ei\u00b7nes", "Ker\u00b7kers", "bl\u00e4u\u00b7li\u00b7che", "Fins\u00b7ter\u00b7nis"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "feindlich sich \u00fcber uns?", "tokens": ["feind\u00b7lich", "sich", "\u00fc\u00b7ber", "uns", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPR", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Von ungez\u00e4hlten Gedankenpfeilen", "tokens": ["Von", "un\u00b7ge\u00b7z\u00e4hl\u00b7ten", "Ge\u00b7dan\u00b7ken\u00b7pfei\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "durchbohrt,", "tokens": ["durch\u00b7bohrt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "die von emp\u00f6rter Sehne", "tokens": ["die", "von", "em\u00b7p\u00f6r\u00b7ter", "Seh\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "der suchende Menschengeist", "tokens": ["der", "su\u00b7chen\u00b7de", "Men\u00b7schen\u00b7geist"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "rings um sich gestreut:", "tokens": ["rings", "um", "sich", "ge\u00b7streut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Das Licht der Erkenntnis aber,", "tokens": ["Das", "Licht", "der", "Er\u00b7kennt\u00b7nis", "a\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "die Sonne der Freiheit,", "tokens": ["die", "Son\u00b7ne", "der", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "quillt leuchtend", "tokens": ["quillt", "leuch\u00b7tend"], "token_info": ["word", "word"], "pos": ["VVFIN", "VVPP"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "durch die zerschossenen W\u00e4nde.", "tokens": ["durch", "die", "zer\u00b7schos\u00b7se\u00b7nen", "W\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Nein, nein! ...", "tokens": ["Nein", ",", "nein", "!", "..."], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$.", "$("], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Mit spottenden Augen", "tokens": ["Mit", "spot\u00b7ten\u00b7den", "Au\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "blinzt die Unendlichkeit", "tokens": ["blinzt", "die", "Un\u00b7end\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "auf den sterblichen R\u00e4tselrater ...", "tokens": ["auf", "den", "sterb\u00b7li\u00b7chen", "R\u00e4t\u00b7sel\u00b7ra\u00b7ter", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Und dennoch", "tokens": ["Und", "den\u00b7noch"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "rat ich das tiefe Geheimnis!", "tokens": ["rat", "ich", "das", "tie\u00b7fe", "Ge\u00b7heim\u00b7nis", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Denn bei Phanta", "tokens": ["Denn", "bei", "Phan\u00b7ta"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NE"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "ist nichts unm\u00f6glich.", "tokens": ["ist", "nichts", "un\u00b7m\u00f6g\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}}, "stanza.7": {"line.1": {"text": "In der leeren, dr\u00f6hnenden Halle des Alls", "tokens": ["In", "der", "lee\u00b7ren", ",", "dr\u00f6h\u00b7nen\u00b7den", "Hal\u00b7le", "des", "Alls"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "ART", "NN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "rauschte der Gott der Finsternis", "tokens": ["rauschte", "der", "Gott", "der", "Fins\u00b7ter\u00b7nis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "mit schwarzen, schleppenden Fittichen", "tokens": ["mit", "schwar\u00b7zen", ",", "schlep\u00b7pen\u00b7den", "Fit\u00b7ti\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "VVINF", "$,", "ADJA", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "grollend dahin.", "tokens": [