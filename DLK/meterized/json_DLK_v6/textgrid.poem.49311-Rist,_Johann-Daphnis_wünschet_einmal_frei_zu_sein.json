{"textgrid.poem.49311": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Daphnis w\u00fcnschet einmal frei zu sein", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie so selig mu\u00df doch sein", "tokens": ["O", "wie", "so", "se\u00b7lig", "mu\u00df", "doch", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADV", "ADJD", "VMFIN", "ADV", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein V\u00f6glein in den L\u00fcften,", "tokens": ["Ein", "V\u00f6\u00b7glein", "in", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Nachtigal beim B\u00e4chelein,", "tokens": ["Die", "Nach\u00b7ti\u00b7gal", "beim", "B\u00e4\u00b7che\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Fuchs im finstern Kl\u00fcften,", "tokens": ["Der", "Fuchs", "im", "fins\u00b7tern", "Kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Schlang' im Busch, ein Fisch im Meer,", "tokens": ["Die", "Schlang'", "im", "Busch", ",", "ein", "Fisch", "im", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Teucher in den Seen,", "tokens": ["Der", "Teu\u00b7cher", "in", "den", "Seen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der edler Hirsch, so hin und her", "tokens": ["Der", "ed\u00b7ler", "Hirsch", ",", "so", "hin", "und", "her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mag in den W\u00e4ldern gehen!", "tokens": ["Mag", "in", "den", "W\u00e4l\u00b7dern", "ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die Thier' in ihrer Einsamkeit", "tokens": ["Die", "Thier'", "in", "ih\u00b7rer", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die d\u00f6rfen sich nicht klagen,", "tokens": ["Die", "d\u00f6r\u00b7fen", "sich", "nicht", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Noch, wie ich mu\u00df, zu jeder Zeit", "tokens": ["Noch", ",", "wie", "ich", "mu\u00df", ",", "zu", "je\u00b7der", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PPER", "VMFIN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sich mit Gedanken plagen;", "tokens": ["Sich", "mit", "Ge\u00b7dan\u00b7ken", "pla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Sie suchen ihre Freud' und Lust", "tokens": ["Sie", "su\u00b7chen", "ih\u00b7re", "Freud'", "und", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "In Wassern und in Weiden,", "tokens": ["In", "Was\u00b7sern", "und", "in", "Wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Und ihrer keinen ist bewust,", "tokens": ["Und", "ih\u00b7rer", "kei\u00b7nen", "ist", "be\u00b7wust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "PIAT", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Was seufzen sei und leiden.", "tokens": ["Was", "seuf\u00b7zen", "sei", "und", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VAFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Die Freiheit ist ihr h\u00f6chstes Gut,", "tokens": ["Die", "Frei\u00b7heit", "ist", "ihr", "h\u00f6chs\u00b7tes", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ihr einig All, ihr Leben;", "tokens": ["Ihr", "ei\u00b7nig", "All", ",", "ihr", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Ich aber, wie ein Schlave thut,", "tokens": ["Ich", "a\u00b7ber", ",", "wie", "ein", "Schla\u00b7ve", "thut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Mu\u00df stets in Sorgen schweben;", "tokens": ["Mu\u00df", "stets", "in", "Sor\u00b7gen", "schwe\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Ich bin verstricket Tag und Nacht", "tokens": ["Ich", "bin", "ver\u00b7stri\u00b7cket", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Mit schweren Liebesbanden,", "tokens": ["Mit", "schwe\u00b7ren", "Lie\u00b7bes\u00b7ban\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Ja, werde durch der Sch\u00f6nsten Macht", "tokens": ["Ja", ",", "wer\u00b7de", "durch", "der", "Sch\u00f6ns\u00b7ten", "Macht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Fast ganz und gar zu Schanden.", "tokens": ["Fast", "ganz", "und", "gar", "zu", "Schan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Ach, m\u00f6cht' ich nur so gl\u00fccklich sein", "tokens": ["Ach", ",", "m\u00f6cht'", "ich", "nur", "so", "gl\u00fcck\u00b7lich", "sein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VMFIN", "PPER", "ADV", "ADV", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Wie die, so mit den Fl\u00fcgeln", "tokens": ["Wie", "die", ",", "so", "mit", "den", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Sich schwingen in die Luft hinein", "tokens": ["Sich", "schwin\u00b7gen", "in", "die", "Luft", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Und wohnen auf den H\u00fcgeln:", "tokens": ["Und", "woh\u00b7nen", "auf", "den", "H\u00fc\u00b7geln", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Die wi\u00dfen recht, was Freiheit ist,", "tokens": ["Die", "wi\u00b7\u00dfen", "recht", ",", "was", "Frei\u00b7heit", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "$,", "PWS", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Was scherzen hei\u00dft und lieben;", "tokens": ["Was", "scher\u00b7zen", "hei\u00dft", "und", "lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Ich aber mu\u00df, durch fremde List,", "tokens": ["Ich", "a\u00b7ber", "mu\u00df", ",", "durch", "frem\u00b7de", "List", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Ohn' Ende mich betr\u00fcben.", "tokens": ["Ohn'", "En\u00b7de", "mich", "be\u00b7tr\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "So w\u00fcnsch' ich, wie die Nachtigal", "tokens": ["So", "w\u00fcn\u00b7sch'", "ich", ",", "wie", "die", "Nach\u00b7ti\u00b7gal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.34": {"text": "In Einsamkeit zu singen", "tokens": ["In", "Ein\u00b7sam\u00b7keit", "zu", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Und wie ein Hirsch durch Berg und Thal", "tokens": ["Und", "wie", "ein", "Hirsch", "durch", "Berg", "und", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "In Freiheit herzuspringen,", "tokens": ["In", "Frei\u00b7heit", "her\u00b7zu\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Ja wie die Schlang' in finstrer H\u00f6hl'", "tokens": ["Ja", "wie", "die", "Schlang'", "in", "finst\u00b7rer", "H\u00f6hl'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Auch einst mich frei zu machen.", "tokens": ["Auch", "einst", "mich", "frei", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "So kan mein hochbetr\u00fcbte Seel'", "tokens": ["So", "kan", "mein", "hoch\u00b7be\u00b7tr\u00fcb\u00b7te", "Seel'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Im Wunsch auch herzlich lachen.", "tokens": ["Im", "Wunsch", "auch", "herz\u00b7lich", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}