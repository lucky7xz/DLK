{"textgrid.poem.57449": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "(auf das Beylager Herrn Christians, F\u00fcrsten zu Schwarzburg, mit einer Prinze\u00dfinn von Anhalt-Bernburg)", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf! schm\u00fccke dich mit Lust und Pracht,", "tokens": ["Auf", "!", "schm\u00fc\u00b7cke", "dich", "mit", "Lust", "und", "Pracht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du gl\u00fcckerf\u00fclltes Sondershausen!", "tokens": ["Du", "gl\u00fc\u00b7cker\u00b7f\u00fcll\u00b7tes", "Son\u00b7ders\u00b7hau\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn anderw\u00e4rts, bey dick gew\u00f6lkter Nacht,", "tokens": ["Wenn", "an\u00b7der\u00b7w\u00e4rts", ",", "bey", "dick", "ge\u00b7w\u00f6lk\u00b7ter", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Des Tr\u00fcbsals rauhe St\u00fcrme brausen.", "tokens": ["Des", "Tr\u00fcb\u00b7sals", "rau\u00b7he", "St\u00fcr\u00b7me", "brau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dir stralt ein heitres Freudenlicht,", "tokens": ["Dir", "stralt", "ein", "heit\u00b7res", "Freu\u00b7den\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit t\u00e4glich neuem Glanz entgegen,", "tokens": ["Mit", "t\u00e4g\u00b7lich", "neu\u00b7em", "Glanz", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und hindert dich, den Flor aufs Angesicht,", "tokens": ["Und", "hin\u00b7dert", "dich", ",", "den", "Flor", "aufs", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Den tr\u00fcben Trauerboy auf Brust und Arm zu legen.", "tokens": ["Den", "tr\u00fc\u00b7ben", "Trau\u00b7er\u00b7boy", "auf", "Brust", "und", "Arm", "zu", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Prinz ", "tokens": ["Prinz"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Empfindet Hymens hei\u00dfe Triebe,", "tokens": ["Emp\u00b7fin\u00b7det", "Hy\u00b7mens", "hei\u00b7\u00dfe", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Entschlie\u00dfet sich zu einer Hochzeitlust,", "tokens": ["Ent\u00b7schlie\u00b7\u00dfet", "sich", "zu", "ei\u00b7ner", "Hoch\u00b7zeit\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und folgt den Br\u00fcdern in der Liebe.", "tokens": ["Und", "folgt", "den", "Br\u00fc\u00b7dern", "in", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In Anhalt brennt der Angelstern,", "tokens": ["In", "An\u00b7halt", "brennt", "der", "An\u00b7gels\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Der sein entz\u00fccktes Auge leitet:", "tokens": ["Der", "sein", "ent\u00b7z\u00fcck\u00b7tes", "Au\u00b7ge", "lei\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Von diesem Stral ist Schwarzburg nicht zu fern,", "tokens": ["Von", "die\u00b7sem", "Stral", "ist", "Schwarz\u00b7burg", "nicht", "zu", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "NE", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da sich sein holder Glanz wohl weiter ausgebreitet.", "tokens": ["Da", "sich", "sein", "hol\u00b7der", "Glanz", "wohl", "wei\u00b7ter", "aus\u00b7ge\u00b7brei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So kr\u00f6nt der Eintracht wahrer Preis", "tokens": ["So", "kr\u00f6nt", "der", "Ein\u00b7tracht", "wah\u00b7rer", "Preis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das F\u00fcrstenkleeblatt dreyer Br\u00fcder!", "tokens": ["Das", "F\u00fcrs\u00b7ten\u00b7klee\u00b7blatt", "drey\u00b7er", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was sonst ein Land nicht leicht zu r\u00fchmen weis,", "tokens": ["Was", "sonst", "ein", "Land", "nicht", "leicht", "zu", "r\u00fch\u00b7men", "weis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sieht Sondershausen t\u00e4glich wieder.", "tokens": ["Sieht", "Son\u00b7ders\u00b7hau\u00b7sen", "t\u00e4g\u00b7lich", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Qu\u00e4lt sonst ein ungez\u00e4hmter Neid", "tokens": ["Qu\u00e4lt", "sonst", "ein", "un\u00b7ge\u00b7z\u00e4hm\u00b7ter", "Neid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Erben v\u00e4terlicher Thronen:", "tokens": ["Die", "Er\u00b7ben", "v\u00e4\u00b7ter\u00b7li\u00b7cher", "Thro\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sieht Schwarzburg doch den Sitz der Einigkeit,", "tokens": ["Sieht", "Schwarz\u00b7burg", "doch", "den", "Sitz", "der", "Ei\u00b7nig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wo seines Oberhaupts durchlauchte Br\u00fcder wohnen.", "tokens": ["Wo", "sei\u00b7nes", "O\u00b7ber\u00b7haupts", "durch\u00b7lauch\u00b7te", "Br\u00fc\u00b7der", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Seht! Amor selber leitet sie", "tokens": ["Seht", "!", "A\u00b7mor", "sel\u00b7ber", "lei\u00b7tet", "sie"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "NE", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht in verschiedne F\u00fcrstenh\u00e4user:", "tokens": ["Nicht", "in", "ver\u00b7schied\u00b7ne", "F\u00fcrs\u00b7ten\u00b7h\u00e4u\u00b7ser", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er selber kn\u00fcpft das Band der Eintracht hie,", "tokens": ["Er", "sel\u00b7ber", "kn\u00fcpft", "das", "Band", "der", "Ein\u00b7tracht", "hie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch dreyer F\u00fcrsten Myrthenreiser.", "tokens": ["Durch", "drey\u00b7er", "F\u00fcrs\u00b7ten", "Myr\u00b7then\u00b7rei\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gepriesnes Anhalt! dich allein", "tokens": ["Ge\u00b7pri\u00b7es\u00b7nes", "An\u00b7halt", "!", "dich", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Begl\u00fccken solche Prinze\u00dfinnen,", "tokens": ["Be\u00b7gl\u00fc\u00b7cken", "sol\u00b7che", "Prin\u00b7ze\u00b7\u00dfin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die an Verstand und Sch\u00f6nheit ungemein,", "tokens": ["Die", "an", "Ver\u00b7stand", "und", "Sch\u00f6n\u00b7heit", "un\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "An seltner Tugend reich, der Prinzen Herz gewinnen.", "tokens": ["An", "selt\u00b7ner", "Tu\u00b7gend", "reich", ",", "der", "Prin\u00b7zen", "Herz", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$,", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie sonst ein G\u00e4rtner Arm und Hand", "tokens": ["Wie", "sonst", "ein", "G\u00e4rt\u00b7ner", "Arm", "und", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am liebsten nach dem Baume strecket,", "tokens": ["Am", "liebs\u00b7ten", "nach", "dem", "Bau\u00b7me", "stre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ihm die Frucht, die er so k\u00f6stlich fand,", "tokens": ["Wo", "ihm", "die", "Frucht", ",", "die", "er", "so", "k\u00f6st\u00b7lich", "fand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Fast st\u00fcndlich neue Lust erwecket!", "tokens": ["Fast", "st\u00fcnd\u00b7lich", "neu\u00b7e", "Lust", "er\u00b7we\u00b7cket", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie sich des Winzers L\u00fcsternheit", "tokens": ["Wie", "sich", "des", "Win\u00b7zers", "L\u00fcs\u00b7tern\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Best\u00e4ndig nach den Reben sehnet,", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "nach", "den", "Re\u00b7ben", "seh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zu welchen sonst der Trauben S\u00fc\u00dfigkeit,", "tokens": ["Zu", "wel\u00b7chen", "sonst", "der", "Trau\u00b7ben", "S\u00fc\u00b7\u00dfig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Seit vielen Jahren schon den Leckermund gew\u00f6hnet.", "tokens": ["Seit", "vie\u00b7len", "Jah\u00b7ren", "schon", "den", "Le\u00b7cker\u00b7mund", "ge\u00b7w\u00f6h\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Sie blicken beyde ganz verliebt,", "tokens": ["Sie", "bli\u00b7cken", "bey\u00b7de", "ganz", "ver\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf ihren Zweig, auf ihre Reben,", "tokens": ["Auf", "ih\u00b7ren", "Zweig", ",", "auf", "ih\u00b7re", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wissens schon, was der f\u00fcr Aepfel giebt;", "tokens": ["Und", "wis\u00b7sens", "schon", ",", "was", "der", "f\u00fcr", "A\u00b7e\u00b7pfel", "giebt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "PRELS", "ART", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Was dieser pflegt f\u00fcr Most zu geben.", "tokens": ["Was", "die\u00b7ser", "pflegt", "f\u00fcr", "Most", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VVFIN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie bebt ihr Arm! Wie f\u00e4hrt er gleich", "tokens": ["Wie", "bebt", "ihr", "Arm", "!", "Wie", "f\u00e4hrt", "er", "gleich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$.", "PWAV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Macht nach den gewohnten Aesten;", "tokens": ["Mit", "Macht", "nach", "den", "ge\u00b7wohn\u00b7ten", "A\u00b7es\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+----", "measure": "unknown.measure.tri"}, "line.7": {"text": "Und fragt man sie: Was lockt, was reizet euch?", "tokens": ["Und", "fragt", "man", "sie", ":", "Was", "lockt", ",", "was", "rei\u00b7zet", "euch", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPER", "$.", "PWS", "VVFIN", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So ist die Antwort da: Hier schmeckt die Frucht am besten.", "tokens": ["So", "ist", "die", "Ant\u00b7wort", "da", ":", "Hier", "schmeckt", "die", "Frucht", "am", "bes\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "So, theurer F\u00fcrst, Prinz ", "tokens": ["So", ",", "theu\u00b7rer", "F\u00fcrst", ",", "Prinz"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "$,", "ADJD", "NN", "$,", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "So gieng es dir in deinem Lieben;", "tokens": ["So", "gieng", "es", "dir", "in", "dei\u00b7nem", "Lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du sahst das Gl\u00fcck der beyden Br\u00fcder an,", "tokens": ["Du", "sahst", "das", "Gl\u00fcck", "der", "bey\u00b7den", "Br\u00fc\u00b7der", "