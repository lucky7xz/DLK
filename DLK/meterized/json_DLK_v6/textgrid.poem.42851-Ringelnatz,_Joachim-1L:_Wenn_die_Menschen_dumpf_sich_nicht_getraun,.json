{"textgrid.poem.42851": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn die Menschen dumpf sich nicht getraun,", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn die Menschen dumpf sich nicht getraun,", "tokens": ["Wenn", "die", "Men\u00b7schen", "dumpf", "sich", "nicht", "ge\u00b7traun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "PRF", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn sie feig und heuchlerisch sich f\u00fcgen", "tokens": ["Wenn", "sie", "feig", "und", "heuch\u00b7le\u00b7risch", "sich", "f\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "PRF", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und ihr Gl\u00fcck auf ihre Schlauheit baun,", "tokens": ["Und", "ihr", "Gl\u00fcck", "auf", "ih\u00b7re", "Schlau\u00b7heit", "baun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Redliches bedr\u00fccken und betr\u00fcgen.", "tokens": ["Red\u00b7li\u00b7ches", "be\u00b7dr\u00fc\u00b7cken", "und", "be\u00b7tr\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn sie schleichen, fl\u00fcstern und sich ducken,", "tokens": ["Wenn", "sie", "schlei\u00b7chen", ",", "fl\u00fcs\u00b7tern", "und", "sich", "du\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVINF", "KON", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Andrerseits aus W\u00fcrde sich genieren, \u2013 \u2013", "tokens": ["A\u00b7ndrer\u00b7seits", "aus", "W\u00fcr\u00b7de", "sich", "ge\u00b7nie\u00b7ren", ",", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "APPR", "NN", "PRF", "VVINF", "$,", "$(", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "O dann m\u00fc\u00dfte etwas explodieren.", "tokens": ["O", "dann", "m\u00fc\u00df\u00b7te", "et\u00b7was", "ex\u00b7plo\u00b7die\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und ein Riese m\u00fc\u00dfte sich erheben", "tokens": ["Und", "ein", "Rie\u00b7se", "m\u00fc\u00df\u00b7te", "sich", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VMFIN", "PRF", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "\u00dcber sie und sie nicht etwa t\u00f6ten,", "tokens": ["\u00dc\u00b7ber", "sie", "und", "sie", "nicht", "et\u00b7wa", "t\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sondern saftig, kr\u00e4ftig sie bespucken,", "tokens": ["Son\u00b7dern", "saf\u00b7tig", ",", "kr\u00e4f\u00b7tig", "sie", "be\u00b7spu\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Um sie f\u00fcr ihr weitres Leben", "tokens": ["Um", "sie", "f\u00fcr", "ihr", "weit\u00b7res", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Als verschleimte, fette Warzenkr\u00f6ten", "tokens": ["Als", "ver\u00b7schleim\u00b7te", ",", "fet\u00b7te", "War\u00b7zen\u00b7kr\u00f6\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KOUS", "VVFIN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "In ein Glashaus einzusperrn.", "tokens": ["In", "ein", "Glas\u00b7haus", "ein\u00b7zu\u00b7sperrn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVIZU", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Und ich w\u00fcrde durch die Scheibe gucken", "tokens": ["Und", "ich", "w\u00fcr\u00b7de", "durch", "die", "Schei\u00b7be", "gu\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Und sie gr\u00fc\u00dfen: \u00bbHochverehrte Herrn!\u00ab", "tokens": ["Und", "sie", "gr\u00fc\u00b7\u00dfen", ":", "\u00bb", "Hoch\u00b7ver\u00b7ehr\u00b7te", "Herrn", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVINF", "$.", "$(", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}