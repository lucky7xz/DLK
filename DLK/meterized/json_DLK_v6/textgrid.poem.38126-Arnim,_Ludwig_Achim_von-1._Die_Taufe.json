{"textgrid.poem.38126": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1. Die Taufe", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hat gewohnt ein Edelmann,", "tokens": ["Es", "hat", "ge\u00b7wohnt", "ein", "E\u00b7del\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Tugend kannte jedermann", "tokens": ["Des", "Tu\u00b7gend", "kann\u00b7te", "je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht ferne vom Braunschweigschen Land,", "tokens": ["Nicht", "fer\u00b7ne", "vom", "Braun\u00b7schweig\u00b7schen", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Aschen von Walmoden genannt.", "tokens": ["A\u00b7schen", "von", "Wal\u00b7mo\u00b7den", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Gott segnete des Aschen Weib", "tokens": ["Gott", "seg\u00b7ne\u00b7te", "des", "A\u00b7schen", "Weib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im heilgen Stand mit fruchtbarem Leib,", "tokens": ["Im", "heil\u00b7gen", "Stand", "mit", "frucht\u00b7ba\u00b7rem", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Sie hat ein S\u00f6hnlein ihm geboren,", "tokens": ["Sie", "hat", "ein", "S\u00f6hn\u00b7lein", "ihm", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der war zu Grossem auserkoren.", "tokens": ["Der", "war", "zu", "Gros\u00b7sem", "au\u00b7ser\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die Aeltern sein aus Griechenland", "tokens": ["Die", "A\u00b7el\u00b7tern", "sein", "aus", "Grie\u00b7chen\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "APPR", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Theodulus ihn han genannt,", "tokens": ["Theo\u00b7du\u00b7lus", "ihn", "han", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Verk\u00fcrzt man aber Thedel spricht,", "tokens": ["Ver\u00b7k\u00fcrzt", "man", "a\u00b7ber", "The\u00b7del", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Von Gott ein Knecht, keins andern nicht.", "tokens": ["Von", "Gott", "ein", "Knecht", ",", "keins", "an\u00b7dern", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,", "PIAT", "ADJA", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Zur Schule ward er fr\u00fch gesandt,", "tokens": ["Zur", "Schu\u00b7le", "ward", "er", "fr\u00fch", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Die Sprachen lernt aus allem Land,", "tokens": ["Die", "Spra\u00b7chen", "lernt", "aus", "al\u00b7lem", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "In fremde Land ging nach Paris,", "tokens": ["In", "frem\u00b7de", "Land", "ging", "nach", "Pa\u00b7ris", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Damit er ward der Kunst gewi\u00df.", "tokens": ["Da\u00b7mit", "er", "ward", "der", "Kunst", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Da Thedel war so lange Zeit", "tokens": ["Da", "The\u00b7del", "war", "so", "lan\u00b7ge", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "VAFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "In fremdem Land gewesen weit,", "tokens": ["In", "frem\u00b7dem", "Land", "ge\u00b7we\u00b7sen", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAPP", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Kam endlich wieder heim nach Hau\u00df,", "tokens": ["Kam", "end\u00b7lich", "wie\u00b7der", "heim", "nach", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "PTKVZ", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Der Vater gab nen grossen Schmaus.", "tokens": ["Der", "Va\u00b7ter", "gab", "nen", "gros\u00b7sen", "Schmaus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Da ward getauft sein Schwesterlein,", "tokens": ["Da", "ward", "ge\u00b7tauft", "sein", "Schwes\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Er mu\u00df dabey Taufzeuge seyn.", "tokens": ["Er", "mu\u00df", "da\u00b7bey", "Tauf\u00b7zeu\u00b7ge", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Er konnt Latein, verstand so drat,", "tokens": ["Er", "konnt", "La\u00b7tein", ",", "ver\u00b7stand", "so", "drat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "$,", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Die Tauf, die Christus setzen that.", "tokens": ["Die", "Tauf", ",", "die", "Chris\u00b7tus", "set\u00b7zen", "that", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "NE", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Die Worte, die der Priester las,", "tokens": ["Die", "Wor\u00b7te", ",", "die", "der", "Pries\u00b7ter", "las", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Aus seinem Herzen nicht vergas,", "tokens": ["Aus", "sei\u00b7nem", "Her\u00b7zen", "nicht", "ver\u00b7gas", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und als die Mahlzeit war geschehen,", "tokens": ["Und", "als", "die", "Mahl\u00b7zeit", "war", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Lie\u00df er den Pfarrherrn zu sich gehen,", "tokens": ["Lie\u00df", "er", "den", "Pfar\u00b7rherrn", "zu", "sich", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Er sprach: \u00bbMir ist gezeiget an,", "tokens": ["Er", "sprach", ":", "\u00bb", "Mir", "ist", "ge\u00b7zei\u00b7get", "an", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPER", "VAFIN", "VVPP", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Da\u00df ihr mich auch getaufet han,", "tokens": ["Da\u00df", "ihr", "mich", "auch", "ge\u00b7tau\u00b7fet", "han", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Habt ihr da auch die Wort gelesen,", "tokens": ["Habt", "ihr", "da", "auch", "die", "Wort", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Die bey der Schwester Tauf gewesen?\u00ab \u2013", "tokens": ["Die", "bey", "der", "Schwes\u00b7ter", "Tauf", "ge\u00b7we\u00b7sen", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VAPP", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "\u00bbich sage euch bey Jesu Christ", "tokens": ["\u00bb", "ich", "sa\u00b7ge", "euch", "bey", "Je\u00b7su", "Christ"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "APPR", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Der unsrer aller Mittler ist,", "tokens": ["Der", "uns\u00b7rer", "al\u00b7ler", "Mitt\u00b7ler", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Bey euch sind keine andre Wort", "tokens": ["Bey", "euch", "sind", "kei\u00b7ne", "and\u00b7re", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Gebraucht als heut an diesem Ort,", "tokens": ["Ge\u00b7braucht", "als", "heut", "an", "die\u00b7sem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "So wird euch Gott vom Himmels Thron", "tokens": ["So", "wird", "euch", "Gott", "vom", "Him\u00b7mels", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "APPRART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Beystand geben durch seinen Sohn!\u00ab", "tokens": ["Beys\u00b7tand", "ge\u00b7ben", "durch", "sei\u00b7nen", "Sohn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVINF", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.39": {"text": "\u00bbehrw\u00fcrdger Herr, bin ich also", "tokens": ["\u00bb", "ehr\u00b7w\u00fcrd\u00b7ger", "Herr", ",", "bin", "ich", "al\u00b7so"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADJA", "NN", "$,", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Getauft, so bin ich herzlich froh,", "tokens": ["Ge\u00b7tauft", ",", "so", "bin", "ich", "herz\u00b7lich", "froh", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "VAFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Seit ich das bin von euch bericht,", "tokens": ["Seit", "ich", "das", "bin", "von", "euch", "be\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VAFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Ich f\u00fcrchte mich vor keinem nicht,", "tokens": ["Ich", "f\u00fcrch\u00b7te", "mich", "vor", "kei\u00b7nem", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIS", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "In Kampf und Streit in Gottes Namen,", "tokens": ["In", "Kampf", "und", "Streit", "in", "Got\u00b7tes", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Ich schlag den Teufel selbst zusammen.\u00ab", "tokens": ["Ich", "schlag", "den", "Teu\u00b7fel", "selbst", "zu\u00b7sam\u00b7men", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Den Teufel das gar sehr verdro\u00df,", "tokens": ["Den", "Teu\u00b7fel", "das", "gar", "sehr", "ver\u00b7dro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Da\u00df Thedels Glauben war so gro\u00df.", "tokens": ["Da\u00df", "The\u00b7dels", "Glau\u00b7ben", "war", "so", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}