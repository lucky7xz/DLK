{"textgrid.poem.65995": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Willst du dich \u00fcber die Menge erheben,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du dich \u00fcber die Menge erheben,", "tokens": ["Willst", "du", "dich", "\u00fc\u00b7ber", "die", "Men\u00b7ge", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Halte die Ehre blank und eben,", "tokens": ["Hal\u00b7te", "die", "Eh\u00b7re", "blank", "und", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PTKVZ", "KON", "ADV", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Den kleinsten Flecken, den kleinsten Belauf,", "tokens": ["Den", "kleins\u00b7ten", "Fle\u00b7cken", ",", "den", "kleins\u00b7ten", "Be\u00b7lauf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Leute zeigen mit Fingern darauf,", "tokens": ["Die", "Leu\u00b7te", "zei\u00b7gen", "mit", "Fin\u00b7gern", "da\u00b7rauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PAV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Froh eine Stelle gefunden zu haben,", "tokens": ["Froh", "ei\u00b7ne", "Stel\u00b7le", "ge\u00b7fun\u00b7den", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Worein sie ihre Haken graben,", "tokens": ["Wo\u00b7rein", "sie", "ih\u00b7re", "Ha\u00b7ken", "gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die dich aus deiner H\u00f6he wieder", "tokens": ["Die", "dich", "aus", "dei\u00b7ner", "H\u00f6\u00b7he", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zerren zu ihren S\u00fcmpfen nieder.", "tokens": ["Zer\u00b7ren", "zu", "ih\u00b7ren", "S\u00fcmp\u00b7fen", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Viel eher dulden sie schlecht dich, gemein,", "tokens": ["Viel", "e\u00b7her", "dul\u00b7den", "sie", "schlecht", "dich", ",", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "VVFIN", "PPER", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Als dass sie dein Besserseinwollen verzeihn.", "tokens": ["Als", "dass", "sie", "dein", "Bes\u00b7ser\u00b7sein\u00b7wol\u00b7len", "ver\u00b7zeihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}