{"textgrid.poem.32809": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Lied der Bauren beim Regen", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da sehn wir's wieder offenbar", "tokens": ["Da", "sehn", "wir's", "wie\u00b7der", "of\u00b7fen\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Gott uns nie vergi\u00dft,", "tokens": ["Da\u00df", "Gott", "uns", "nie", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jede Stund' im ganzen Jahr", "tokens": ["Und", "je\u00b7de", "Stund'", "im", "gan\u00b7zen", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der beste Vater ist.", "tokens": ["Der", "bes\u00b7te", "Va\u00b7ter", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da str\u00f6mt, nach langem Sonnenbrand,", "tokens": ["Da", "str\u00f6mt", ",", "nach", "lan\u00b7gem", "Son\u00b7nen\u00b7brand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sein Regengu\u00df aufs d\u00fcrre Land.", "tokens": ["Sein", "Re\u00b7gen\u00b7gu\u00df", "aufs", "d\u00fcr\u00b7re", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du lieber Gott, wie traurig sah's", "tokens": ["Du", "lie\u00b7ber", "Gott", ",", "wie", "trau\u00b7rig", "sah's"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "NN", "$,", "PWAV", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Garten, Wies' und Feld!", "tokens": ["Im", "Gar\u00b7ten", ",", "Wies'", "und", "Feld", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NE", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verdorrt war Korn, Gem\u00fcs' und Gras,", "tokens": ["Ver\u00b7dorrt", "war", "Korn", ",", "Ge\u00b7m\u00fcs'", "und", "Gras", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Laub von Hitz' entstellt;", "tokens": ["Das", "Laub", "von", "Hitz'", "ent\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vor dickem Staub erblickte man", "tokens": ["Vor", "di\u00b7ckem", "Staub", "er\u00b7blick\u00b7te", "man"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von ferne kaum den Wandersmann.", "tokens": ["Von", "fer\u00b7ne", "kaum", "den", "Wan\u00b7ders\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die welken \u00c4hren neigten sich,", "tokens": ["Die", "wel\u00b7ken", "\u00c4h\u00b7ren", "neig\u00b7ten", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und geblich ward ihr Gr\u00fcn;", "tokens": ["Und", "ge\u00b7blich", "ward", "ihr", "Gr\u00fcn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Der zarte Flachs schwand sichtbarlich", "tokens": ["Der", "zar\u00b7te", "Flachs", "schwand", "sicht\u00b7bar\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erstickt vom Unkraut hin;", "tokens": ["Er\u00b7stickt", "vom", "Un\u00b7kraut", "hin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Matt schlich das Vieh zur Weid' hinaus,", "tokens": ["Matt", "schlich", "das", "Vieh", "zur", "Weid'", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "APPRART", "NN", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und kehrte matter noch nach Haus.", "tokens": ["Und", "kehr\u00b7te", "mat\u00b7ter", "noch", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Des Futters und der Milch beraubt,", "tokens": ["Des", "Fut\u00b7ters", "und", "der", "Milch", "be\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verfielen Kalb und Kuh.", "tokens": ["Ver\u00b7fie\u00b7len", "Kalb", "und", "Kuh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie standen mit gesenktem Haupt,", "tokens": ["Sie", "stan\u00b7den", "mit", "ge\u00b7senk\u00b7tem", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sahn dem Jammer zu,", "tokens": ["Und", "sahn", "dem", "Jam\u00b7mer", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und seufzten: Ach du lieber Gott!", "tokens": ["Und", "seufz\u00b7ten", ":", "Ach", "du", "lie\u00b7ber", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ITJ", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gieb uns und unsern Kindern Brot!", "tokens": ["Gieb", "uns", "und", "un\u00b7sern", "Kin\u00b7dern", "Brot", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da hie\u00df, ger\u00fchrt von Mitleid, Er", "tokens": ["Da", "hie\u00df", ",", "ge\u00b7r\u00fchrt", "von", "Mit\u00b7leid", ",", "Er"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "$,", "VVPP", "APPR", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die bangen Sorgen fliehn;", "tokens": ["Die", "ban\u00b7gen", "Sor\u00b7gen", "fliehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schon sahn wir aus der Ferne her", "tokens": ["Schon", "sahn", "wir", "aus", "der", "Fer\u00b7ne", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gew\u00f6lk wie Sch\u00e4fchen ziehn;", "tokens": ["Ge\u00b7w\u00f6lk", "wie", "Sch\u00e4f\u00b7chen", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und n\u00e4her zog in raschem Lauf", "tokens": ["Und", "n\u00e4\u00b7her", "zog", "in", "ra\u00b7schem", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Wetter schwarzgedr\u00e4ngt herauf.", "tokens": ["Das", "Wet\u00b7ter", "schwarz\u00b7ge\u00b7dr\u00e4ngt", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Sonne sank in Grau geh\u00fcllt,", "tokens": ["Die", "Son\u00b7ne", "sank", "in", "Grau", "ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am schwarzen Wald hinab;", "tokens": ["Am", "schwar\u00b7zen", "Wald", "hin\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Regen rauschte k\u00fchl und mild", "tokens": ["Und", "Re\u00b7gen", "rauschte", "k\u00fchl", "und", "mild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aufs d\u00fcrre Feld herab.", "tokens": ["Aufs", "d\u00fcr\u00b7re", "Feld", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und Donner jauchzten durch die Nacht,", "tokens": ["Und", "Don\u00b7ner", "jauchz\u00b7ten", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und priesen Gottes Lieb' und Macht.", "tokens": ["Und", "prie\u00b7sen", "Got\u00b7tes", "Lieb'", "und", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie frisch hebt nun der Halm sein Haupt!", "tokens": ["Wie", "frisch", "hebt", "nun", "der", "Halm", "sein", "Haupt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie bl\u00fcht der Flachs so blau!", "tokens": ["Wie", "bl\u00fcht", "der", "Flachs", "so", "blau", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie steht der Baum so neubelaubt,", "tokens": ["Wie", "steht", "der", "Baum", "so", "neu\u00b7be\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So gl\u00e4nzendgr\u00fcn die Au'!", "tokens": ["So", "gl\u00e4n\u00b7zend\u00b7gr\u00fcn", "die", "Au'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und welch ein \u00fcbers\u00fc\u00dfer Duft", "tokens": ["Und", "welch", "ein", "\u00fc\u00b7ber\u00b7s\u00fc\u00b7\u00dfer", "Duft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcllt rings umher die k\u00fchle Luft!", "tokens": ["F\u00fcllt", "rings", "um\u00b7her", "die", "k\u00fch\u00b7le", "Luft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Das alles, lieber, guter Gott!", "tokens": ["Das", "al\u00b7les", ",", "lie\u00b7ber", ",", "gu\u00b7ter", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PIS", "$,", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat deine Macht gethan.", "tokens": ["Hat", "dei\u00b7ne", "Macht", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verschwunden ist des Winters Not,", "tokens": ["Ver\u00b7schwun\u00b7den", "ist", "des", "Win\u00b7ters", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die wir uns drohen sahn!", "tokens": ["Die", "wir", "uns", "dro\u00b7hen", "sahn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum la\u00df uns immer dir vertraun,", "tokens": ["Drum", "la\u00df", "uns", "im\u00b7mer", "dir", "ver\u00b7traun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nie so \u00e4ngstlich vorw\u00e4rts schaun!", "tokens": ["Und", "nie", "so", "\u00e4ngst\u00b7lich", "vor\u00b7w\u00e4rts", "schaun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}