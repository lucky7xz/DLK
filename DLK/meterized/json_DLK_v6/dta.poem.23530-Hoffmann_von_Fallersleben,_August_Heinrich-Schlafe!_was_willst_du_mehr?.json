{"dta.poem.23530": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schlafe! was willst du mehr?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo sind noch W\u00fcrm' und Drachen,", "tokens": ["Wo", "sind", "noch", "W\u00fcrm'", "und", "Dra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Riesen mit Schwert und Speer?", "tokens": ["Rie\u00b7sen", "mit", "Schwert", "und", "Speer", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Was kannst du weiter machen?", "tokens": ["Was", "kannst", "du", "wei\u00b7ter", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schlafe! was willst du mehr?", "tokens": ["Schla\u00b7fe", "!", "was", "willst", "du", "mehr", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Du hast genug gelitten", "tokens": ["Du", "hast", "ge\u00b7nug", "ge\u00b7lit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Qualen in Kampf und Strau\u00df;", "tokens": ["Qua\u00b7len", "in", "Kampf", "und", "Strau\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR