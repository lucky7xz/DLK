{"textgrid.poem.35901": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie r\u00fchrt' ich sonst die Leier", "genre": "verse", "period": "N.A.", "pub_year": 1828, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie r\u00fchrt' ich sonst die Leier", "tokens": ["Wie", "r\u00fchrt'", "ich", "sonst", "die", "Lei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So gern an diesem Tag", "tokens": ["So", "gern", "an", "die\u00b7sem", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sang zur Morgenfeier", "tokens": ["Und", "sang", "zur", "Mor\u00b7gen\u00b7fei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die muntern V\u00f6glein wach.", "tokens": ["Die", "mun\u00b7tern", "V\u00f6\u00b7glein", "wach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich sann in stillen Thr\u00e4nen", "tokens": ["Ich", "sann", "in", "stil\u00b7len", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Wehmuth und der Lust,", "tokens": ["Der", "Weh\u00b7muth", "und", "der", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Hoffen und von Sehnen", "tokens": ["Von", "Hof\u00b7fen", "und", "von", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bewegt in tiefer Brust.", "tokens": ["Be\u00b7wegt", "in", "tie\u00b7fer", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und Lied und holde Worte,", "tokens": ["Und", "Lied", "und", "hol\u00b7de", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was nur von Liebe spricht,", "tokens": ["Was", "nur", "von", "Lie\u00b7be", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ging aus dunkler Pforte", "tokens": ["Das", "ging", "aus", "dunk\u00b7ler", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An's helle Tageslicht.", "tokens": ["An's", "hel\u00b7le", "Ta\u00b7ges\u00b7licht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie gr\u00fc\u00dfte Dich so gerne", "tokens": ["Wie", "gr\u00fc\u00df\u00b7te", "Dich", "so", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auch heut' ein fr\u00f6hlich Lied,", "tokens": ["Auch", "heut'", "ein", "fr\u00f6h\u00b7lich", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das hell durch alle Ferne,", "tokens": ["Das", "hell", "durch", "al\u00b7le", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch alle Nebel zieht.", "tokens": ["Durch", "al\u00b7le", "Ne\u00b7bel", "zieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ach, aber all' mein Singen", "tokens": ["Ach", ",", "a\u00b7ber", "all'", "mein", "Sin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und all' mein Leierspiel,", "tokens": ["Und", "all'", "mein", "Lei\u00b7er\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das will mir nicht gelingen,", "tokens": ["Das", "will", "mir", "nicht", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das nutzet mir nicht viel.", "tokens": ["Das", "nut\u00b7zet", "mir", "nicht", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es lagern bange Sorgen", "tokens": ["Es", "la\u00b7gern", "ban\u00b7ge", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um mein ge\u00e4ngstet Herz;", "tokens": ["Um", "mein", "ge\u00b7\u00e4ngs\u00b7tet", "Herz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "VVPP", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Abend wie den Morgen", "tokens": ["Den", "A\u00b7bend", "wie", "den", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist immer wach der Schmerz:", "tokens": ["Ist", "im\u00b7mer", "wach", "der", "Schmerz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Bis jede Schuld sich b\u00fc\u00dfte", "tokens": ["Bis", "je\u00b7de", "Schuld", "sich", "b\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Gnadenworte wehn,", "tokens": ["Und", "Gna\u00b7den\u00b7wor\u00b7te", "wehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Willst, Armer, durch die W\u00fcste", "tokens": ["Willst", ",", "Ar\u00b7mer", ",", "durch", "die", "W\u00fcs\u00b7te"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "$,", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit wunden F\u00fc\u00dfen gehn.", "tokens": ["Mit", "wun\u00b7den", "F\u00fc\u00b7\u00dfen", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich denke Deines Leides", "tokens": ["Ich", "den\u00b7ke", "Dei\u00b7nes", "Lei\u00b7des"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und weine f\u00fcr und f\u00fcr;", "tokens": ["Und", "wei\u00b7ne", "f\u00fcr", "und", "f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich denke Deines Streites", "tokens": ["Ich", "den\u00b7ke", "Dei\u00b7nes", "Strei\u00b7tes"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und streite ihn mit Dir.", "tokens": ["Und", "strei\u00b7te", "ihn", "mit", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und, Freund! in solchen Stunden", "tokens": ["Und", ",", "Freund", "!", "in", "sol\u00b7chen", "Stun\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "NN", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da klingt kein Leierspiel;", "tokens": ["Da", "klingt", "kein", "Lei\u00b7er\u00b7spiel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum sei auch Du durch Wunden", "tokens": ["Drum", "sei", "auch", "Du", "durch", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ADV", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gegr\u00fc\u00dfet oft und viel;", "tokens": ["Ge\u00b7gr\u00fc\u00b7\u00dfet", "oft", "und", "viel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Durch ", "tokens": ["Durch"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Und durch Mari\u00e4 Schmerz,", "tokens": ["Und", "durch", "Ma\u00b7ri\u00e4", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Daran wohl mag gesunden", "tokens": ["Da\u00b7ran", "wohl", "mag", "ge\u00b7sun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ADV", "VMFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jedwedes kranke Herz;", "tokens": ["Jed\u00b7we\u00b7des", "kran\u00b7ke", "Herz", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Damit wohl \u00fcberwindet", "tokens": ["Da\u00b7mit", "wohl", "\u00fc\u00b7berw\u00b7in\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["PAV", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Jedweder K\u00e4mpfer gut,", "tokens": ["Jed\u00b7we\u00b7der", "K\u00e4mp\u00b7fer", "gut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Darin wohl Ruhe findet,", "tokens": ["Da\u00b7rin", "wohl", "Ru\u00b7he", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer nirgendwo geruht.", "tokens": ["Wer", "nir\u00b7gend\u00b7wo", "ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Und der im Dornenkranze", "tokens": ["Und", "der", "im", "Dor\u00b7nen\u00b7kran\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcr Dich am Kreuz verschied", "tokens": ["F\u00fcr", "Dich", "am", "Kreuz", "ver\u00b7schied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nun im ew'gen Glanze", "tokens": ["Und", "nun", "im", "ew'\u00b7gen", "Glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf Dich hernieder sieht:", "tokens": ["Auf", "Dich", "her\u00b7nie\u00b7der", "sieht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Der neige Seine Palmen", "tokens": ["Der", "nei\u00b7ge", "Sei\u00b7ne", "Pal\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem frommen Streiter hin", "tokens": ["Dem", "from\u00b7men", "Strei\u00b7ter", "hin"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wecke Dir zu Psalmen", "tokens": ["Und", "we\u00b7cke", "Dir", "zu", "Psal\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den schwer gedr\u00fcckten Sinn;", "tokens": ["Den", "schwer", "ge\u00b7dr\u00fcck\u00b7ten", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Der wolle Siegesfreude", "tokens": ["Der", "wol\u00b7le", "Sie\u00b7ges\u00b7freu\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Frieden Dir verleihn,", "tokens": ["Und", "Frie\u00b7den", "Dir", "ver\u00b7leihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der woll' im heil'gen Streite", "tokens": ["Der", "woll'", "im", "heil'\u00b7gen", "Strei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dir Schild und Harnisch sein!", "tokens": ["Dir", "Schild", "und", "Har\u00b7nisch", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Drum wende Deine Tritte", "tokens": ["Drum", "wen\u00b7de", "Dei\u00b7ne", "Trit\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom falschen Glanze fern", "tokens": ["Vom", "fal\u00b7schen", "Glan\u00b7ze", "fern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und baue Deine H\u00fctte", "tokens": ["Und", "bau\u00b7e", "Dei\u00b7ne", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Gnadenlicht des Herrn.", "tokens": ["Im", "Gna\u00b7den\u00b7licht", "des", "Herrn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Dann wird mit ew'ger Krone", "tokens": ["Dann", "wird", "mit", "ew'\u00b7ger", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein Ringen Dir gelohnt", "tokens": ["Dein", "Rin\u00b7gen", "Dir", "ge\u00b7lohnt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort, wo an Gottes Throne", "tokens": ["Dort", ",", "wo", "an", "Got\u00b7tes", "Thro\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Manch frommer K\u00e4mpfer wohnt.", "tokens": ["Manch", "from\u00b7mer", "K\u00e4mp\u00b7fer", "wohnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}