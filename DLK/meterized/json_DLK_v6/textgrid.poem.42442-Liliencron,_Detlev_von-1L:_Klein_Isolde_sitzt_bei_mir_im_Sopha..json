{"textgrid.poem.42442": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Klein Isolde sitzt bei mir im Sopha.", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Klein Isolde sitzt bei mir im Sopha.", "tokens": ["Klein", "I\u00b7sol\u00b7de", "sitzt", "bei", "mir", "im", "So\u00b7pha", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Klein Isolde z\u00e4hlt der Jahre vier erst.", "tokens": ["Klein", "I\u00b7sol\u00b7de", "z\u00e4hlt", "der", "Jah\u00b7re", "vier", "erst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "CARD", "ADV", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Ihre Puppen bringt sie mir in's Zimmer", "tokens": ["Ih\u00b7re", "Pup\u00b7pen", "bringt", "sie", "mir", "in's", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und berichtet mit dem feinen Stimmchen,", "tokens": ["Und", "be\u00b7rich\u00b7tet", "mit", "dem", "fei\u00b7nen", "Stimm\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Was mit ihnen letzthin sich begeben,", "tokens": ["Was", "mit", "ih\u00b7nen", "letz\u00b7thin", "sich", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "ADV", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Nun, die Resi, wie zeigt die sich aber:", "tokens": ["Nun", ",", "die", "Re\u00b7si", ",", "wie", "zeigt", "die", "sich", "a\u00b7ber", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "$,", "PWAV", "VVFIN", "ART", "PRF", "ADV", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Alle Glieder, alle Kleider, Strumpf und Schuhzeug", "tokens": ["Al\u00b7le", "Glie\u00b7der", ",", "al\u00b7le", "Klei\u00b7der", ",", "Strumpf", "und", "Schuh\u00b7zeug"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Sind ja schwarz, als h\u00e4tt' der Schornsteinfeger", "tokens": ["Sind", "ja", "schwarz", ",", "als", "h\u00e4tt'", "der", "Schorn\u00b7stein\u00b7fe\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "KOKOM", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sie beim Wickel grad gehabt, Isolde,", "tokens": ["Sie", "beim", "Wi\u00b7ckel", "grad", "ge\u00b7habt", ",", "I\u00b7sol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ADV", "VAPP", "$,", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Pfui, wie kommt's da\u00df sie so garstig aussieht?", "tokens": ["Pfui", ",", "wie", "kommt's", "da\u00df", "sie", "so", "gars\u00b7tig", "aus\u00b7sieht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Klein Isolde spricht mit zartem Stimmchen:", "tokens": ["Klein", "I\u00b7sol\u00b7de", "spricht", "mit", "zar\u00b7tem", "Stimm\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.7": {"text": "\u00bbresi fiel heut in den Kohlenkasten.\u00ab", "tokens": ["\u00bb", "re\u00b7si", "fiel", "heut", "in", "den", "Koh\u00b7len\u00b7kas\u00b7ten", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NE", "VVFIN", "ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Nein doch, was geschah mit Isidoren?", "tokens": ["Nein", "doch", ",", "was", "ge\u00b7schah", "mit", "I\u00b7si\u00b7do\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "PWS", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Abgeschlagen ist das rechte Beinchen,", "tokens": ["Ab\u00b7ge\u00b7schla\u00b7gen", "ist", "das", "rech\u00b7te", "Be\u00b7in\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und der linke Arm ist fortgeflogen,", "tokens": ["Und", "der", "lin\u00b7ke", "Arm", "ist", "fort\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und um N\u00e4schen ihr und Stirn und Augen", "tokens": ["Und", "um", "N\u00e4\u00b7schen", "ihr", "und", "Stirn", "und", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "PPER", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Tr\u00e4gt Verb\u00e4nde sie und wei\u00dfe T\u00fccher.", "tokens": ["Tr\u00e4gt", "Ver\u00b7b\u00e4n\u00b7de", "sie", "und", "wei\u00b7\u00dfe", "T\u00fc\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wie ereignete sich das, Isolde?", "tokens": ["Wie", "er\u00b7eig\u00b7ne\u00b7te", "sich", "das", ",", "I\u00b7sol\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "PDS", "$,", "NE", "$."], "meter": "+-+--++-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Und sie giebt mir weinerliche Antwort:", "tokens": ["Und", "sie", "giebt", "mir", "wei\u00b7ner\u00b7li\u00b7che", "Ant\u00b7wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "\u00bbvom Altane st\u00fcrzte Isidore.\u00ab", "tokens": ["\u00bb", "vom", "Al\u00b7ta\u00b7ne", "st\u00fcrz\u00b7te", "I\u00b7si\u00b7do\u00b7re", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPRART", "NN", "VVFIN", "NE", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Rosamundchen seh' ich nicht, Isolde.", "tokens": ["Ro\u00b7sa\u00b7mund\u00b7chen", "seh'", "ich", "nicht", ",", "I\u00b7sol\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "$,", "NE", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Allerdings ist sehr sie krank gewesen,", "tokens": ["Al\u00b7ler\u00b7dings", "ist", "sehr", "sie", "krank", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "ADJD", "VAPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hat die Cholera gehabt, die Aermste.", "tokens": ["Hat", "die", "Cho\u00b7le\u00b7ra", "ge\u00b7habt", ",", "die", "A\u00b7erms\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VAPP", "$,", "ART", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Doch ich hoffe, da\u00df sie wohl und munter.", "tokens": ["Doch", "ich", "hof\u00b7fe", ",", "da\u00df", "sie", "wohl", "und", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Klein Isolde nickt mit wichtiger Miene:", "tokens": ["Klein", "I\u00b7sol\u00b7de", "nickt", "mit", "wich\u00b7ti\u00b7ger", "Mie\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u00bbrosamundchen ist im Puppenhimmel.\u00ab", "tokens": ["\u00bb", "ro\u00b7sa\u00b7mund\u00b7chen", "ist", "im", "Pup\u00b7pen\u00b7him\u00b7mel", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "VAFIN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}