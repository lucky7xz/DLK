{"dta.poem.7590": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Perlen nicht allein, in deines Mundes Pforte", "tokens": ["Die", "Per\u00b7len", "nicht", "al\u00b7lein", ",", "in", "dei\u00b7nes", "Mun\u00b7des", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "ADV", "$,", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bewahren mu\u00dft du mehr als Perlen deine Worte.", "tokens": ["Be\u00b7wah\u00b7ren", "mu\u00dft", "du", "mehr", "als", "Per\u00b7len", "dei\u00b7ne", "Wor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PIAT", "KOKOM", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die gute Lehre nahm vordem ein weiser Mann", "tokens": ["Die", "gu\u00b7te", "Leh\u00b7re", "nahm", "vor\u00b7dem", "ein", "wei\u00b7ser", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von einem armen, der mit Schaden sie gewann.", "tokens": ["Von", "ei\u00b7nem", "ar\u00b7men", ",", "der", "mit", "Scha\u00b7den", "sie", "ge\u00b7wann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "PRELS", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Arme, der im Sand ein Dutzend Perlen fand,", "tokens": ["Der", "Ar\u00b7me", ",", "der", "im", "Sand", "ein", "Dut\u00b7zend", "Per\u00b7len", "fand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vern\u00e4hte zehn davon in sein zerlumpt Gewand.", "tokens": ["Ver\u00b7n\u00e4h\u00b7te", "zehn", "da\u00b7von", "in", "sein", "zer\u00b7lumpt", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "PAV", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die beiden \u00fcbrigen, weil ihm die List war kund", "tokens": ["Die", "bei\u00b7den", "\u00fcb\u00b7ri\u00b7gen", ",", "weil", "ihm", "die", "List", "war", "kund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "ADJA", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Diebe, nahm er und verwahrte sie im Mund.", "tokens": ["Der", "Die\u00b7be", ",", "nahm", "er", "und", "ver\u00b7wahr\u00b7te", "sie", "im", "Mund", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Diebe kamen ihm die Kleider durchzusehn,", "tokens": ["Die", "Die\u00b7be", "ka\u00b7men", "ihm", "die", "Klei\u00b7der", "durch\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nahmen die darin verborgnen alle zehn.", "tokens": ["Und", "nah\u00b7men", "die", "da\u00b7rin", "ver\u00b7borg\u00b7nen", "al\u00b7le", "zehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "PAV", "VVFIN", "PIAT", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Er dachte: Fahret hin! ihr seyd des Gl\u00fcckes Gabe;", "tokens": ["Er", "dach\u00b7te", ":", "Fah\u00b7ret", "hin", "!", "ihr", "seyd", "des", "Gl\u00fc\u00b7ckes", "Ga\u00b7be", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "PTKVZ", "$.", "PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mir gn\u00fcgen noch die zwei, die ich im Munde habe.", "tokens": ["Mir", "gn\u00fc\u00b7gen", "noch", "die", "zwei", ",", "die", "ich", "im", "Mun\u00b7de", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "CARD", "$,", "PRELS", "PPER", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Zum Gl\u00fccke dacht' ers nur, denn h\u00e4tt' er es gesprochen,", "tokens": ["Zum", "Gl\u00fc\u00b7cke", "dacht'", "ers", "nur", ",", "denn", "h\u00e4tt'", "er", "es", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "ADV", "$,", "KON", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie h\u00e4tten auch die zwei ihm aus dem Mund gebrochen.", "tokens": ["Sie", "h\u00e4t\u00b7ten", "auch", "die", "zwei", "ihm", "aus", "dem", "Mund", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "CARD", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Doch wenn sich dir das Gl\u00fcck verschworen hat zum B\u00f6sen,", "tokens": ["Doch", "wenn", "sich", "dir", "das", "Gl\u00fcck", "ver\u00b7schwo\u00b7ren", "hat", "zum", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "PPER", "ART", "NN", "VVPP", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wird zur Unzeit es dir schon die Zunge l\u00f6sen.", "tokens": ["So", "wird", "zur", "Un\u00b7zeit", "es", "dir", "schon", "die", "Zun\u00b7ge", "l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "PPER", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Er geht zur Stadt und will verkaufen seinen Schatz,", "tokens": ["Er", "geht", "zur", "Stadt", "und", "will", "ver\u00b7kau\u00b7fen", "sei\u00b7nen", "Schatz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VMFIN", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und denkt, beim Juwelier ist wohl der rechte Platz.", "tokens": ["Und", "denkt", ",", "beim", "Ju\u00b7we\u00b7lier", "ist", "wohl", "der", "rech\u00b7te", "Platz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPRART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Dem aber kamen heut zw\u00f6lf Perlen grad abhanden;", "tokens": ["Dem", "a\u00b7ber", "ka\u00b7men", "heut", "zw\u00f6lf", "Per\u00b7len", "grad", "ab\u00b7han\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "ADV", "CARD", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er freut sich, da\u00df sobald die zwei davon sich fanden.", "tokens": ["Er", "freut", "sich", ",", "da\u00df", "so\u00b7bald", "die", "zwei", "da\u00b7von", "sich", "fan\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KOUS", "ADV", "ART", "CARD", "PAV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Wie er das lump'ge Kleid des Finders angesehn,", "tokens": ["Wie", "er", "das", "lump'\u00b7ge", "Kleid", "des", "Fin\u00b7ders", "an\u00b7ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fragt er: Das sind die zwei, wo sind die andern zehn?", "tokens": ["Fragt", "er", ":", "Das", "sind", "die", "zwei", ",", "wo", "sind", "die", "an\u00b7dern", "zehn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PDS", "VAFIN", "ART", "CARD", "$,", "PWAV", "VAFIN", "ART", "ADJA", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Der Mann in Unschuld spricht: der Dieb hat sie genommen.", "tokens": ["Der", "Mann", "in", "Un\u00b7schuld", "spricht", ":", "der", "Dieb", "hat", "sie", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$.", "ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eganz recht! wer ist der Dieb? Du mu\u00dft zum Richter komm", "tokens": ["\u201e", "ganz", "recht", "!", "wer", "ist", "der", "Dieb", "?", "Du", "mu\u00dft", "zum", "Rich\u00b7ter", "komm"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADJD", "$.", "PWS", "VAFIN", "ART", "NN", "$.", "PPER", "VMFIN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Der arme Mann erschrickt, l\u00e4\u00dft seinen Schatz in Stich,", "tokens": ["Der", "ar\u00b7me", "Mann", "er\u00b7schrickt", ",", "l\u00e4\u00dft", "sei\u00b7nen", "Schatz", "in", "Stich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entflieht in Eil und nimmt die Lehr' allein mit sich:", "tokens": ["Ent\u00b7flieht", "in", "Eil", "und", "nimmt", "die", "Lehr'", "al\u00b7lein", "mit", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "VVFIN", "ART", "NN", "ADV", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.14": {"line.1": {"text": "Die Perlen nicht allein, in deines Mundes Pforte", "tokens": ["Die", "Per\u00b7len", "nicht", "al\u00b7lein", ",", "in", "dei\u00b7nes", "Mun\u00b7des", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "ADV", "$,", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bewahren mu\u00dft du mehr als Perlen deine Worte.", "tokens": ["Be\u00b7wah\u00b7ren", "mu\u00dft", "du", "mehr", "als", "Per\u00b7len", "dei\u00b7ne", "Wor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PIAT", "KOKOM", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}