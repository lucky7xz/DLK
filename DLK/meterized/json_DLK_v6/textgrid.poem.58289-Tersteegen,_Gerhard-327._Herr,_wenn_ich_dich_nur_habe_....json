{"textgrid.poem.58289": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "327. Herr, wenn ich dich nur habe ...", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man mag mir geben oder nehmen,", "tokens": ["Man", "mag", "mir", "ge\u00b7ben", "o\u00b7der", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich ehren oder mich besch\u00e4men,", "tokens": ["Mich", "eh\u00b7ren", "o\u00b7der", "mich", "be\u00b7sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Man mag mich lieben oder nicht,", "tokens": ["Man", "mag", "mich", "lie\u00b7ben", "o\u00b7der", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "VVINF", "KON", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es sei mir finster oder licht,", "tokens": ["Es", "sei", "mir", "fins\u00b7ter", "o\u00b7der", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sei betr\u00fcbt, ich sei in Freuden,", "tokens": ["Ich", "sei", "be\u00b7tr\u00fcbt", ",", "ich", "sei", "in", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In Angenehmheit oder Leiden \u2013", "tokens": ["In", "An\u00b7ge\u00b7nehm\u00b7heit", "o\u00b7der", "Lei\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Hab' ich nur Gott, so bin ich still", "tokens": ["Hab'", "ich", "nur", "Gott", ",", "so", "bin", "ich", "still"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und habe, was ich haben will.", "tokens": ["Und", "ha\u00b7be", ",", "was", "ich", "ha\u00b7ben", "will", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "PWS", "PPER", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}