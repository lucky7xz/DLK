{"textgrid.poem.60994": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Schmetterling", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Wetterf\u00e4hnlein ist mein Sinn,", "tokens": ["Ein", "Wet\u00b7ter\u00b7f\u00e4hn\u00b7lein", "ist", "mein", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er schwankt und wankt im Lieben,", "tokens": ["Er", "schwankt", "und", "wankt", "im", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er dreht sich her und dreht sich hin,", "tokens": ["Er", "dreht", "sich", "her", "und", "dreht", "sich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von jedem Wind getrieben.", "tokens": ["Von", "je\u00b7dem", "Wind", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich wei\u00df nicht, ist's mit mir allein,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "ist's", "mit", "mir", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VAFIN", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mag's andern auch so gehen?", "tokens": ["Mag's", "an\u00b7dern", "auch", "so", "ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "An jedem Fenster gro\u00df und klein", "tokens": ["An", "je\u00b7dem", "Fens\u00b7ter", "gro\u00df", "und", "klein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mu\u00df ich was Holdes sehen.", "tokens": ["Mu\u00df", "ich", "was", "Hol\u00b7des", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Heut klopf' ich bei der Blonden an", "tokens": ["Heut", "klopf'", "ich", "bei", "der", "Blon\u00b7den", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und morgen bei der Braunen,", "tokens": ["Und", "mor\u00b7gen", "bei", "der", "Brau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und \u00fcbermorgen mu\u00df ich dann", "tokens": ["Und", "\u00fc\u00b7ber\u00b7mor\u00b7gen", "mu\u00df", "ich", "dann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Schwarzen Reiz bestaunen.", "tokens": ["Der", "Schwar\u00b7zen", "Reiz", "be\u00b7stau\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nur kann ich nimmer allzulang", "tokens": ["Nur", "kann", "ich", "nim\u00b7mer", "all\u00b7zu\u00b7lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bei einer mich verweilen;", "tokens": ["Bei", "ei\u00b7ner", "mich", "ver\u00b7wei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Macht mich ein dunkles Auge krank,", "tokens": ["Macht", "mich", "ein", "dunk\u00b7les", "Au\u00b7ge", "krank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein blaues mu\u00df mich heilen.", "tokens": ["Ein", "blau\u00b7es", "mu\u00df", "mich", "hei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und leicht gewogen hier am Ort", "tokens": ["Und", "leicht", "ge\u00b7wo\u00b7gen", "hier", "am", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind mir die ros'gen Sch\u00f6nen,", "tokens": ["Sind", "mir", "die", "ros'\u00b7gen", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn jede h\u00f6rt ein Liebeswort", "tokens": ["Denn", "je\u00b7de", "h\u00f6rt", "ein", "Lie\u00b7bes\u00b7wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Zither gern ert\u00f6nen,", "tokens": ["Zur", "Zi\u00b7ther", "gern", "er\u00b7t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und jede schw\u00e4rmt auf ihre Art", "tokens": ["Und", "je\u00b7de", "schw\u00e4rmt", "auf", "ih\u00b7re", "Art"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Beim sanften Glanz der Sterne,", "tokens": ["Beim", "sanf\u00b7ten", "Glanz", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und machst du's nur ein wenig zart,", "tokens": ["Und", "machst", "du's", "nur", "ein", "we\u00b7nig", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "ART", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So k\u00fc\u00dft auch jede gerne.", "tokens": ["So", "k\u00fc\u00dft", "auch", "je\u00b7de", "ger\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So fliehn mir denn in leiser Spur", "tokens": ["So", "fliehn", "mir", "denn", "in", "lei\u00b7ser", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dahin die schnellen Stunden;", "tokens": ["Da\u00b7hin", "die", "schnel\u00b7len", "Stun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich seufze nicht, ich singe nur", "tokens": ["Ich", "seuf\u00b7ze", "nicht", ",", "ich", "sin\u00b7ge", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wei\u00df von keinen Wunden;", "tokens": ["Und", "wei\u00df", "von", "kei\u00b7nen", "Wun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bald bin ich dort, bald bin ich hier,", "tokens": ["Bald", "bin", "ich", "dort", ",", "bald", "bin", "ich", "hier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An Scherz und Spiel mich labend,", "tokens": ["An", "Scherz", "und", "Spiel", "mich", "la\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und jeder Tag bringt Lieder mir", "tokens": ["Und", "je\u00b7der", "Tag", "bringt", "Lie\u00b7der", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und K\u00fcsse jeder Abend.", "tokens": ["Und", "K\u00fcs\u00b7se", "je\u00b7der", "A\u00b7bend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}