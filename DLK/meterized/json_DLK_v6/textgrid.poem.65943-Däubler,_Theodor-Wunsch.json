{"textgrid.poem.65943": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Wunsch", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schl\u00fcrfe Freude aus des Freundes Schale,", "tokens": ["Ich", "schl\u00fcr\u00b7fe", "Freu\u00b7de", "aus", "des", "Freun\u00b7des", "Scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir, Sonne, bring ich k\u00fchnes Feuer dar;", "tokens": ["Dir", ",", "Son\u00b7ne", ",", "bring", "ich", "k\u00fch\u00b7nes", "Feu\u00b7er", "dar", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus wo-gebundner, ungenannter Schar,", "tokens": ["Aus", "wo\u00b7ge\u00b7bund\u00b7ner", ",", "un\u00b7ge\u00b7nann\u00b7ter", "Schar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verk\u00fcnden wir dein Lob, in offnem Saale.", "tokens": ["Ver\u00b7k\u00fcn\u00b7den", "wir", "dein", "Lob", ",", "in", "off\u00b7nem", "Saa\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du l\u00e4dst uns sorglos ein zu freiem Mahle,", "tokens": ["Du", "l\u00e4dst", "uns", "sorg\u00b7los", "ein", "zu", "frei\u00b7em", "Mah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ART", "PTKZU", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir nahen: Wachsame beim Lichtaltar.", "tokens": ["Wir", "na\u00b7hen", ":", "Wach\u00b7sa\u00b7me", "beim", "Lich\u00b7tal\u00b7tar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$.", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dir dankt, zur Hochzeit, das begl\u00fcckte Paar,", "tokens": ["Dir", "dankt", ",", "zur", "Hoch\u00b7zeit", ",", "das", "be\u00b7gl\u00fcck\u00b7te", "Paar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Auge funkt Beschlu\u00df vom Herzensstrahle:", "tokens": ["Im", "Au\u00b7ge", "funkt", "Be\u00b7schlu\u00df", "vom", "Her\u00b7zens\u00b7strah\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir schwanken nicht zur\u00fcck vor deinem Fordern,", "tokens": ["Wir", "schwan\u00b7ken", "nicht", "zu\u00b7r\u00fcck", "vor", "dei\u00b7nem", "For\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ergl\u00fchte H\u00fcter deiner Huld zu sein!", "tokens": ["Er\u00b7gl\u00fch\u00b7te", "H\u00fc\u00b7ter", "dei\u00b7ner", "Huld", "zu", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bew\u00e4hrt ich, dir zu taugen, mich bei Vordern!", "tokens": ["Be\u00b7w\u00e4hrt", "ich", ",", "dir", "zu", "tau\u00b7gen", ",", "mich", "bei", "Vor\u00b7dern", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Durch eigne G\u00fcte, Sonne, sind wir dein:", "tokens": ["Durch", "eig\u00b7ne", "G\u00fc\u00b7te", ",", "Son\u00b7ne", ",", "sind", "wir", "dein", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "$,", "VAFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du sollst mein Lied zum Morgengru\u00df beordern,", "tokens": ["Du", "sollst", "mein", "Lied", "zum", "Mor\u00b7gen\u00b7gru\u00df", "be\u00b7or\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Mittag mag des Mannes Mut gedeihn!", "tokens": ["Am", "Mit\u00b7tag", "mag", "des", "Man\u00b7nes", "Mut", "ge\u00b7deihn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}