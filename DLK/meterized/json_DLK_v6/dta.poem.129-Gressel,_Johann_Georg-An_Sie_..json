{"dta.poem.129": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An Sie .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Hertzens Doris deine Sch\u00f6ne", "tokens": ["Hert\u00b7zens", "Do\u00b7ris", "dei\u00b7ne", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist beliebet und ", "tokens": ["Ist", "be\u00b7lie\u00b7bet", "und"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "VVFIN", "KON"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Deine Augen sind ein Brand/", "tokens": ["Dei\u00b7ne", "Au\u00b7gen", "sind", "ein", "Brand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Haar ", "tokens": ["Und", "das", "Haar"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Deine Stimm\u2019 ein Lust Geth\u00f6ne;", "tokens": ["Dei\u00b7ne", "Stimm'", "ein", "Lust", "Ge\u00b7th\u00f6\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die angenehme Hand", "tokens": ["Und", "die", "an\u00b7ge\u00b7neh\u00b7me", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Macht den reinsten Schnee bekannt.", "tokens": ["Macht", "den", "reins\u00b7ten", "Schnee", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "G\u00f6nne/ da\u00df ich mehr erwehne?", "tokens": ["G\u00f6n\u00b7ne", "/", "da\u00df", "ich", "mehr", "er\u00b7weh\u00b7ne", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Deine Wangen sind wie Rosen/", "tokens": ["Dei\u00b7ne", "Wan\u00b7gen", "sind", "wie", "Ro\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Die einjeder lieb will kosen", "tokens": ["Die", "ein\u00b7je\u00b7der", "lieb", "will", "ko\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Deine Lippen sind Rubin/", "tokens": ["Dei\u00b7ne", "Lip\u00b7pen", "sind", "Ru\u00b7bin", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NE", "$("], "meter": "+-+-+++", "measure": "unknown.measure.penta"}, "line.12": {"text": "Deine Br\u00fcste Marmor-Ballen/", "tokens": ["Dei\u00b7ne", "Br\u00fcs\u00b7te", "Mar\u00b7mor\u00b7Bal\u00b7len", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und dein Schoo\u00df ist von Corallen;", "tokens": ["Und", "dein", "Schoo\u00df", "ist", "von", "Co\u00b7ral\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Nichtes ist dir f\u00fcrzuziehn.", "tokens": ["Nich\u00b7tes", "ist", "dir", "f\u00fcr\u00b7zu\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}