{"textgrid.poem.41720": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als Kind schon nahm die Leier ich zu Handen \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Kind schon nahm die Leier ich zu Handen \u2013", "tokens": ["Als", "Kind", "schon", "nahm", "die", "Lei\u00b7er", "ich", "zu", "Han\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn fr\u00fch verlernte ich der Kindheit Spiele;", "tokens": ["Denn", "fr\u00fch", "ver\u00b7lern\u00b7te", "ich", "der", "Kind\u00b7heit", "Spie\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich tr\u00e4umte nur in stillen Dichterlanden", "tokens": ["Ich", "tr\u00e4um\u00b7te", "nur", "in", "stil\u00b7len", "Dich\u00b7ter\u00b7lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entr\u00fcckt der Schwestern l\u00e4rmendem Gew\u00fchle.", "tokens": ["Ent\u00b7r\u00fcckt", "der", "Schwes\u00b7tern", "l\u00e4r\u00b7men\u00b7dem", "Ge\u00b7w\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ob auch mein Lied verstimmt und schrill geklungen", "tokens": ["Ob", "auch", "mein", "Lied", "ver\u00b7stimmt", "und", "schrill", "ge\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "VVFIN", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gleich einer Glocke, die entzwei gesprungen,", "tokens": ["Gleich", "ei\u00b7ner", "Glo\u00b7cke", ",", "die", "ent\u00b7zwei", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dumpfdr\u00f6hnend nur und unharmonisch l\u00e4utet:", "tokens": ["Dumpf\u00b7dr\u00f6h\u00b7nend", "nur", "und", "un\u00b7har\u00b7mo\u00b7nisch", "l\u00e4u\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich wu\u00dft es doch was Dichterlust bedeutet!", "tokens": ["Ich", "wu\u00dft", "es", "doch", "was", "Dich\u00b7ter\u00b7lust", "be\u00b7deu\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PWS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich sang von Schmetterlingen und von Sternen,", "tokens": ["Ich", "sang", "von", "Schmet\u00b7ter\u00b7lin\u00b7gen", "und", "von", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sang meinen Teuern, die im Jenseit wallen,", "tokens": ["Sang", "mei\u00b7nen", "Teu\u00b7ern", ",", "die", "im", "Jen\u00b7seit", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich sang von Gott und heilgen Himmelsfernen,", "tokens": ["Ich", "sang", "von", "Gott", "und", "heil\u00b7gen", "Him\u00b7mels\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bald auch von Rosen und von Nachtigallen,", "tokens": ["Bald", "auch", "von", "Ro\u00b7sen", "und", "von", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von Nachtigallen, denn im Liebeshaine", "tokens": ["Von", "Nach\u00b7ti\u00b7gal\u00b7len", ",", "denn", "im", "Lie\u00b7bes\u00b7hai\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fchlt ich der Liebe Wonne als die meine \u2013", "tokens": ["F\u00fchlt", "ich", "der", "Lie\u00b7be", "Won\u00b7ne", "als", "die", "mei\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "KOKOM", "ART", "PPOSAT", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "F\u00fchlt ich ein neues Wesen mich geworden \u2013", "tokens": ["F\u00fchlt", "ich", "ein", "neu\u00b7es", "We\u00b7sen", "mich", "ge\u00b7wor\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PPER", "VAPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da \u2013 ha! ein Schlag \u2013 ich stand an Grabespforten.", "tokens": ["Da", "\u2013", "ha", "!", "ein", "Schlag", "\u2013", "ich", "stand", "an", "Gra\u00b7be\u00b7spfor\u00b7ten", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "ITJ", "$.", "ART", "NN", "$(", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie g\u00e4hnten weit und schlossen dann sich wieder \u2013", "tokens": ["Sie", "g\u00e4hn\u00b7ten", "weit", "und", "schlos\u00b7sen", "dann", "sich", "wie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "ADV", "PRF", "ADV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich blieb zur\u00fcck auf thr\u00e4nenfeuchter Erde,", "tokens": ["Ich", "blieb", "zu\u00b7r\u00fcck", "auf", "thr\u00e4\u00b7nen\u00b7feuch\u00b7ter", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um mich verdorrte Kr\u00e4nze, Klagelieder,", "tokens": ["Um", "mich", "ver\u00b7dorr\u00b7te", "Kr\u00e4n\u00b7ze", ",", "Kla\u00b7ge\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In mir ein Feuer, das am Herzen zehrte! \u2013", "tokens": ["In", "mir", "ein", "Feu\u00b7er", ",", "das", "am", "Her\u00b7zen", "zehr\u00b7te", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was sing ich nun? \u2013 soll ich in eitlen Klagen,", "tokens": ["Was", "sing", "ich", "nun", "?", "\u2013", "soll", "ich", "in", "eit\u00b7len", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "$(", "VMFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der kalten Welt von hei\u00dfen Schmerzen sagen?", "tokens": ["Der", "kal\u00b7ten", "Welt", "von", "hei\u00b7\u00dfen", "Schmer\u00b7zen", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Soll ich um Mitleid singend betteln gehen?", "tokens": ["Soll", "ich", "um", "Mit\u00b7leid", "sin\u00b7gend", "bet\u00b7teln", "ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Soll feig den Tod ich um Erl\u00f6sung flehen? \u2013", "tokens": ["Soll", "feig", "den", "Tod", "ich", "um", "Er\u00b7l\u00f6\u00b7sung", "fle\u00b7hen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ADJD", "ART", "NN", "PPER", "APPR", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O Eines, Eines hab ich mir gerettet,", "tokens": ["O", "Ei\u00b7nes", ",", "Ei\u00b7nes", "hab", "ich", "mir", "ge\u00b7ret\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "$,", "PIS", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es ist der Stolz, der mit dem Schicksal ringet,", "tokens": ["Es", "ist", "der", "Stolz", ",", "der", "mit", "dem", "Schick\u00b7sal", "rin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der sich wohl auch auf einem Grabe bettet,", "tokens": ["Der", "sich", "wohl", "auch", "auf", "ei\u00b7nem", "Gra\u00b7be", "bet\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und doch im Leide festen Mut erzwinget.", "tokens": ["Und", "doch", "im", "Lei\u00b7de", "fes\u00b7ten", "Mut", "er\u00b7zwin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O der wei\u00df nichts von starren Ohnmachtskr\u00e4mpfen", "tokens": ["O", "der", "wei\u00df", "nichts", "von", "star\u00b7ren", "Ohn\u00b7machts\u00b7kr\u00e4mp\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "VVFIN", "PIS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er wagts noch um das h\u00f6chste Gut zu k\u00e4mpfen", "tokens": ["Er", "wagts", "noch", "um", "das", "h\u00f6chs\u00b7te", "Gut", "zu", "k\u00e4mp\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auf denn zum Lied! als Schwert solls Euch begegnen", "tokens": ["Auf", "denn", "zum", "Lied", "!", "als", "Schwert", "solls", "Euch", "be\u00b7geg\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPRART", "NN", "$.", "KOUS", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es ist gefeit zum R\u00e4chen und zum Segnen.", "tokens": ["Es", "ist", "ge\u00b7feit", "zum", "R\u00e4\u00b7chen", "und", "zum", "Seg\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das Lied der Freiheit ist mir noch geblieben \u2013", "tokens": ["Das", "Lied", "der", "Frei\u00b7heit", "ist", "mir", "noch", "ge\u00b7blie\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich will es k\u00fchn vor ihren Feinden singen;", "tokens": ["Ich", "will", "es", "k\u00fchn", "vor", "ih\u00b7ren", "Fein\u00b7den", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es soll mit Jubeln und mit heilgen Lieben", "tokens": ["Es", "soll", "mit", "Ju\u00b7beln", "und", "mit", "heil\u00b7gen", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu ihnen und des Volkes Freunden dringen.", "tokens": ["Zu", "ih\u00b7nen", "und", "des", "Vol\u00b7kes", "Freun\u00b7den", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie k\u00f6nnen h\u00f6hnen mich und schweigen hei\u00dfen,", "tokens": ["Sie", "k\u00f6n\u00b7nen", "h\u00f6h\u00b7nen", "mich", "und", "schwei\u00b7gen", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVFIN", "PPER", "KON", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Lieb zur Freiheit nimmer mir entrei\u00dfen.", "tokens": ["Die", "Lieb", "zur", "Frei\u00b7heit", "nim\u00b7mer", "mir", "ent\u00b7rei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In solchem Kampfe f\u00fchl ich mich gefunden:", "tokens": ["In", "sol\u00b7chem", "Kamp\u00b7fe", "f\u00fchl", "ich", "mich", "ge\u00b7fun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Streit der Freiheit heilt der Liebe Wunden.", "tokens": ["Der", "Streit", "der", "Frei\u00b7heit", "heilt", "der", "Lie\u00b7be", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}