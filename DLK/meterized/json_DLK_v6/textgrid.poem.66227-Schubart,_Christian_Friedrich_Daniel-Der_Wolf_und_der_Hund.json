{"textgrid.poem.66227": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Der Wolf und der Hund", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Hunde, der schon manche Nacht", "tokens": ["Zum", "Hun\u00b7de", ",", "der", "schon", "man\u00b7che", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An seiner Kette zugebracht,", "tokens": ["An", "sei\u00b7ner", "Ket\u00b7te", "zu\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn der Morgenstern am grauen Himmel hing,", "tokens": ["Und", "wenn", "der", "Mor\u00b7gens\u00b7tern", "am", "grau\u00b7en", "Him\u00b7mel", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf's Gay mit seinem Metzger ging,", "tokens": ["Auf's", "Gay", "mit", "sei\u00b7nem", "Metz\u00b7ger", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sprach einstens Isegrim: Ei Bruder wie so mager!", "tokens": ["Sprach", "eins\u00b7tens", "I\u00b7seg\u00b7rim", ":", "Ei", "Bru\u00b7der", "wie", "so", "ma\u00b7ger", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NE", "$.", "NN", "NN", "KOKOM", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie elend siehst du aus! wie sch\u00e4big und wie hager!", "tokens": ["Wie", "e\u00b7lend", "siehst", "du", "aus", "!", "wie", "sch\u00e4\u00b7big", "und", "wie", "ha\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$.", "PWAV", "ADJD", "KON", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Daurst mich, bist gar ein armer Hund.", "tokens": ["Daurst", "mich", ",", "bist", "gar", "ein", "ar\u00b7mer", "Hund", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sieh mich 'mal an! wie frisch und wie gesund", "tokens": ["Sieh", "mich", "'mal", "an", "!", "wie", "frisch", "und", "wie", "ge\u00b7sund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "PTKVZ", "$.", "PWAV", "ADJD", "KON", "PWAV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ich bin! Ich rieche nach der Luft.", "tokens": ["Ich", "bin", "!", "Ich", "rie\u00b7che", "nach", "der", "Luft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mein Balg ist parfumirt mit mancher Staude Duft.", "tokens": ["Mein", "Balg", "ist", "par\u00b7fu\u00b7mirt", "mit", "man\u00b7cher", "Stau\u00b7de", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich hab dir immer guten Fra\u00df,", "tokens": ["Ich", "hab", "dir", "im\u00b7mer", "gu\u00b7ten", "Fra\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Bald frisches Fleisch, bald fettes Aas.", "tokens": ["Bald", "fri\u00b7sches", "Fleisch", ",", "bald", "fet\u00b7tes", "Aas", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Drauf leck' ich klaren Quell und traun!", "tokens": ["Drauf", "leck'", "ich", "kla\u00b7ren", "Quell", "und", "traun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJA", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ich hab' dir immer gute Laun'.", "tokens": ["Ich", "hab'", "dir", "im\u00b7mer", "gu\u00b7te", "Laun'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Du aber \u2013 Ach, versetzte Melak, ach,", "tokens": ["Du", "a\u00b7ber", "\u2013", "Ach", ",", "ver\u00b7setz\u00b7te", "Me\u00b7lak", ",", "ach", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "$(", "ITJ", "$,", "VVFIN", "NE", "$,", "ITJ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Herr Bruder, nur gemach!", "tokens": ["Herr", "Bru\u00b7der", ",", "nur", "ge\u00b7mach", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Drum bist du Wolf; ich Hund! Du frei;", "tokens": ["Drum", "bist", "du", "Wolf", ";", "ich", "Hund", "!", "Du", "frei", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "NE", "$.", "PPER", "NN", "$.", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ich aber in der Sklaverei.", "tokens": ["Ich", "a\u00b7ber", "in", "der", "Skla\u00b7ve\u00b7rei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und die Moral? O die ist jedermann bekannt,", "tokens": ["Und", "die", "Mo\u00b7ral", "?", "O", "die", "ist", "je\u00b7der\u00b7mann", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$.", "NE", "ART", "VAFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Deutschland und in Engelland.", "tokens": ["In", "Deutschland", "und", "in", "En\u00b7gel\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}