{"textgrid.poem.40631": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gl\u00fcckauf! Die Stufen sind erklommen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fcckauf! Die Stufen sind erklommen", "tokens": ["Gl\u00fcc\u00b7kauf", "!", "Die", "Stu\u00b7fen", "sind", "er\u00b7klom\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum ersten Halt, zur sch\u00f6nen Rast!", "tokens": ["Zum", "ers\u00b7ten", "Halt", ",", "zur", "sch\u00f6\u00b7nen", "Rast", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Eurem Zug vorangeglommen,", "tokens": ["Die", "Eu\u00b7rem", "Zug", "vor\u00b7an\u00b7ge\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Leuchte flammt noch unverbla\u00dft;", "tokens": ["Die", "Leuch\u00b7te", "flammt", "noch", "un\u00b7ver\u00b7bla\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie ist im Aeltsten wie im J\u00fcngsten", "tokens": ["Sie", "ist", "im", "A\u00b7elts\u00b7ten", "wie", "im", "J\u00fcng\u00b7sten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "KOKOM", "APPRART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Zur Gluth des Einen Geists entbrannt,", "tokens": ["Zur", "Gluth", "des", "Ei\u00b7nen", "Geists", "ent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der einst, ein neues andres Pfingsten,", "tokens": ["Der", "einst", ",", "ein", "neu\u00b7es", "and\u00b7res", "Pfings\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Hader Babels siegreich bannt.", "tokens": ["Den", "Ha\u00b7der", "Ba\u00b7bels", "sieg\u00b7reich", "bannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn heut die Jubelbecher klingen,", "tokens": ["Wenn", "heut", "die", "Ju\u00b7bel\u00b7be\u00b7cher", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn heut die Bundesfahnen wehn,", "tokens": ["Wenn", "heut", "die", "Bun\u00b7des\u00b7fah\u00b7nen", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nachhallen rings wird Euer Singen", "tokens": ["Nach\u00b7hal\u00b7len", "rings", "wird", "Eu\u00b7er", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und jedes Herz auch wird's verstehn.", "tokens": ["Und", "je\u00b7des", "Herz", "auch", "wird's", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch von der Warte, die erklommen,", "tokens": ["Doch", "von", "der", "War\u00b7te", ",", "die", "er\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "O blickt aufs bunte Feld der Zeit,", "tokens": ["O", "blickt", "aufs", "bun\u00b7te", "Feld", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Seht die da gingen, die da kommen,", "tokens": ["Seht", "die", "da", "gin\u00b7gen", ",", "die", "da", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "VVFIN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Wandrer vor Euch weit, gar weit!", "tokens": ["Die", "Wand\u00b7rer", "vor", "Euch", "weit", ",", "gar", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das junge V\u00f6lklein Eurer Ahnen,", "tokens": ["Das", "jun\u00b7ge", "V\u00f6l\u00b7klein", "Eu\u00b7rer", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Tausend', die sich selbst verbannt,", "tokens": ["Die", "Tau\u00b7send'", ",", "die", "sich", "selbst", "ver\u00b7bannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie nahmen auf des Elends Bahnen", "tokens": ["Sie", "nah\u00b7men", "auf", "des", "E\u00b7lends", "Bah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Busen mit solch leuchtend Pfand;", "tokens": ["Im", "Bu\u00b7sen", "mit", "solch", "leuch\u00b7tend", "Pfand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum, wo sich ihre Pfade wanden,", "tokens": ["Drum", ",", "wo", "sich", "ih\u00b7re", "Pfa\u00b7de", "wan\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWAV", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zog Lichtgeleis die helle Spur,", "tokens": ["Zog", "Licht\u00b7ge\u00b7leis", "die", "hel\u00b7le", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wo ihr neues Heim sie fanden", "tokens": ["Und", "wo", "ihr", "neu\u00b7es", "Heim", "sie", "fan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gl\u00fchn Ruhmessterne im Azur.", "tokens": ["Gl\u00fchn", "Ruh\u00b7mess\u00b7ter\u00b7ne", "im", "A\u00b7zur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Die sp\u00e4tre Schaar an ihrer Stelle,", "tokens": ["Die", "sp\u00e4t\u00b7re", "Schaar", "an", "ih\u00b7rer", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die r\u00fcstig zu dem Bauwerk stand,", "tokens": ["Die", "r\u00fcs\u00b7tig", "zu", "dem", "Bau\u00b7werk", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie f\u00fchrt' in einer Hand die Kelle,", "tokens": ["Sie", "f\u00fchrt'", "in", "ei\u00b7ner", "Hand", "die", "Kel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Schwert doch in der andern Hand.", "tokens": ["Das", "Schwert", "doch", "in", "der", "an\u00b7dern", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zum m\u00e4cht'gen Quadernbau im Grunde", "tokens": ["Zum", "m\u00e4cht'\u00b7gen", "Qua\u00b7dern\u00b7bau", "im", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wahrzeichen legt' sie in den Stein:", "tokens": ["Wahr\u00b7zei\u00b7chen", "legt'", "sie", "in", "den", "Stein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gepr\u00e4g' vollg\u00fcltig noch zur Stunde,", "tokens": ["Ge\u00b7pr\u00e4g'", "voll\u00b7g\u00fcl\u00b7tig", "noch", "zur", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Freibrief f\u00fcr ein stolzes Sein.", "tokens": ["Den", "Frei\u00b7brief", "f\u00fcr", "ein", "stol\u00b7zes", "Sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie baut' in deutscher Art und Sitte;", "tokens": ["Sie", "baut'", "in", "deut\u00b7scher", "Art", "und", "Sit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der M\u00f6rtel, erst noch mild und weich,", "tokens": ["Der", "M\u00f6r\u00b7tel", ",", "erst", "noch", "mild", "und", "weich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erhartet bald zum festen Kitte,", "tokens": ["Er\u00b7har\u00b7tet", "bald", "zum", "fes\u00b7ten", "Kit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der aufrecht h\u00e4lt den Bau: die\u00df Reich. \u2013", "tokens": ["Der", "auf\u00b7recht", "h\u00e4lt", "den", "Bau", ":", "die\u00df", "Reich", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ART", "NN", "$.", "PDS", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So baut auch Ihr! Denn nicht verderbe", "tokens": ["So", "baut", "auch", "Ihr", "!", "Denn", "nicht", "ver\u00b7der\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$.", "KON", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der alte Feind das Werk aufs Neu;", "tokens": ["Der", "al\u00b7te", "Feind", "das", "Werk", "aufs", "Neu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der deutschen V\u00e4ter heilig Erbe", "tokens": ["Der", "deut\u00b7schen", "V\u00e4\u00b7ter", "hei\u00b7lig", "Er\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Beh\u00fcten wollt Ihr wach und treu.", "tokens": ["Be\u00b7h\u00fc\u00b7ten", "wollt", "Ihr", "wach", "und", "treu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein Erbe, nicht blos Einem Stamme,", "tokens": ["Ein", "Er\u00b7be", ",", "nicht", "blos", "Ei\u00b7nem", "Stam\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der ganzen Menschheit kostbar Gut,", "tokens": ["Der", "gan\u00b7zen", "Menschheit", "kost\u00b7bar", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Des V\u00f6lkerbundes Oriflamme,", "tokens": ["Des", "V\u00f6l\u00b7ker\u00b7bun\u00b7des", "O\u00b7ri\u00b7flam\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur anvertraut der treu'sten Hut.", "tokens": ["Nur", "an\u00b7ver\u00b7traut", "der", "treu'\u00b7sten", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Allen wollt Ihr freudig theilen", "tokens": ["Mit", "Al\u00b7len", "wollt", "Ihr", "freu\u00b7dig", "thei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das lautre Gold, wonach Ihr grabt;", "tokens": ["Das", "laut\u00b7re", "Gold", ",", "wo\u00b7nach", "Ihr", "grabt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Weh auch Andrer soll er heilen", "tokens": ["Das", "Weh", "auch", "A\u00b7ndrer", "soll", "er", "hei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PIS", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Jungborn, der Euch st\u00e4hlt und labt.", "tokens": ["Der", "Jung\u00b7born", ",", "der", "Euch", "st\u00e4hlt", "und", "labt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Deutsch sein hei\u00dft: offne Freundesarme", "tokens": ["Deutsch", "sein", "hei\u00dft", ":", "off\u00b7ne", "Freun\u00b7des\u00b7ar\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAINF", "VVFIN", "$.", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr alle Menschheit ausgespannt,", "tokens": ["F\u00fcr", "al\u00b7le", "Menschheit", "aus\u00b7ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Im Herzen doch die ewigwarme,", "tokens": ["Im", "Her\u00b7zen", "doch", "die", "e\u00b7wig\u00b7war\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die einz'ge Liebe: Vaterland!", "tokens": ["Die", "einz'\u00b7ge", "Lie\u00b7be", ":", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Deutsch sein hei\u00dft: sinnen, ringen, schaffen,", "tokens": ["Deutsch", "sein", "hei\u00dft", ":", "sin\u00b7nen", ",", "rin\u00b7gen", ",", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "VAINF", "VVFIN", "$.", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gedanken s\u00e4'n, nach Sternen sp\u00e4hn", "tokens": ["Ge\u00b7dan\u00b7ken", "s\u00e4'n", ",", "nach", "Ster\u00b7nen", "sp\