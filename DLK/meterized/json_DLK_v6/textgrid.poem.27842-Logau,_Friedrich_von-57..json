{"textgrid.poem.27842": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "57.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Deutsche Land ist arm; die Sprache kan es sagen,", "tokens": ["Das", "Deut\u00b7sche", "Land", "ist", "arm", ";", "die", "Spra\u00b7che", "kan", "es", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$.", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ietzt so mager ist, da\u00df ihr man zu mu\u00df tragen", "tokens": ["Die", "ietzt", "so", "ma\u00b7ger", "ist", ",", "da\u00df", "ihr", "man", "zu", "mu\u00df", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "PIS", "PTKZU", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Au\u00df Franckreich, was sie darff, und her vom Tiber-Strom,", "tokens": ["Au\u00df", "Fran\u00b7ck\u00b7reich", ",", "was", "sie", "darff", ",", "und", "her", "vom", "Ti\u00b7ber\u00b7Strom", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "PPER", "VMFIN", "$,", "KON", "ADV", "APPRART", "NN", "$,"], "meter": "-+----+-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Wo vor Latein starb auch mit dir, Unr\u00f6misch Rom!", "tokens": ["Wo", "vor", "La\u00b7tein", "starb", "auch", "mit", "dir", ",", "Un\u00b7r\u00f6\u00b7misch", "Rom", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "VVFIN", "ADV", "APPR", "PPER", "$,", "NN", "NE", "$."], "meter": "-+-----+-+-+", "measure": "dactylic.init"}, "line.5": {"text": "Zum Theil schickts der Iber. Das andre wird genummen,", "tokens": ["Zum", "Theil", "schickts", "der", "I\u00b7be\u00b7r.", "Das", "and\u00b7re", "wird", "ge\u00b7num\u00b7men", ","], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NE", "ART", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So gut es wird gezeugt und auff die Welt ist kummen", "tokens": ["So", "gut", "es", "wird", "ge\u00b7zeugt", "und", "auff", "die", "Welt", "ist", "kum\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "VAFIN", "VVPP", "KON", "APPR", "ART", "NN", "VAFIN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Durch einen Gerne-Klug, der, wenn der Geist ihn r\u00fcrt,", "tokens": ["Durch", "ei\u00b7nen", "Ger\u00b7ne\u00b7Klug", ",", "der", ",", "wenn", "der", "Geist", "ihn", "r\u00fcrt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ietzt dieses Prale-Wort, ietzt jenes rau\u00df gebiert.", "tokens": ["Ietzt", "die\u00b7ses", "Pra\u00b7le\u00b7Wort", ",", "ietzt", "je\u00b7nes", "rau\u00df", "ge\u00b7biert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$,", "ADV", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Musen w\u00fcrckten zwar durch kluge Tichter-Sinnen,", "tokens": ["Die", "Mu\u00b7sen", "w\u00fcrck\u00b7ten", "zwar", "durch", "klu\u00b7ge", "Tich\u00b7ter\u00b7Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das Deutschland solte Deutsch und artlich reden k\u00fcnnen;", "tokens": ["Das", "Deutschland", "sol\u00b7te", "Deutsch", "und", "art\u00b7lich", "re\u00b7den", "k\u00fcn\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "NN", "KON", "ADJD", "VVINF", "VMINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Mars aber schafft es ab und hat es so geschickt,", "tokens": ["Mars", "a\u00b7ber", "schafft", "es", "ab", "und", "hat", "es", "so", "ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df Deutschland ist Blut-arm; drum geht es so geflickt.", "tokens": ["Da\u00df", "Deutschland", "ist", "Blut\u00b7arm", ";", "drum", "geht", "es", "so", "ge\u00b7flickt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VAFIN", "NN", "$.", "PAV", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}