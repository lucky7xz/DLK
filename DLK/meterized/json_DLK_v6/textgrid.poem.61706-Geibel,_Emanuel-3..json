{"textgrid.poem.61706": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus allen Himmeln lieg' ich hergest\u00fcrzt", "tokens": ["Aus", "al\u00b7len", "Him\u00b7meln", "lieg'", "ich", "her\u00b7ge\u00b7st\u00fcrzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Schlangenturm, verfemt, ein Mann des Hohns;", "tokens": ["Im", "Schlan\u00b7gen\u00b7turm", ",", "ver\u00b7femt", ",", "ein", "Mann", "des", "Hohns", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich kann ihn nicht zerbrechen, weh, und auch", "tokens": ["Ich", "kann", "ihn", "nicht", "zer\u00b7bre\u00b7chen", ",", "weh", ",", "und", "auch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "PTKVZ", "$,", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vergessen nicht, was sonst war.", "tokens": ["Ver\u00b7ges\u00b7sen", "nicht", ",", "was", "sonst", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PRELS", "ADV", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich wollte K\u00f6nig sein und spielte drum", "tokens": ["Ich", "woll\u00b7te", "K\u00f6\u00b7nig", "sein", "und", "spiel\u00b7te", "drum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "NN", "VAINF", "KON", "VVFIN", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verwegnes Spiel \u2013 ich selbst zerschlug mein Gl\u00fcck,", "tokens": ["Ver\u00b7weg\u00b7nes", "Spiel", "\u2013", "ich", "selbst", "zer\u00b7schlug", "mein", "Gl\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich selbst, und nichts hab' ich gerettet, nichts", "tokens": ["Ich", "selbst", ",", "und", "nichts", "hab'", "ich", "ge\u00b7ret\u00b7tet", ",", "nichts"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "ADV", "$,", "KON", "PIS", "VAFIN", "PPER", "VVPP", "$,", "PIS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als meinen Stolz und meine Harfe.", "tokens": ["Als", "mei\u00b7nen", "Stolz", "und", "mei\u00b7ne", "Har\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zischt auf, ihr Nattern! Ringle, Qualenbrut!", "tokens": ["Zischt", "auf", ",", "ihr", "Nat\u00b7tern", "!", "Ring\u00b7le", ",", "Qua\u00b7len\u00b7brut", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hier bin ich; meine Seiten str\u00f6men schon", "tokens": ["Hier", "bin", "ich", ";", "mei\u00b7ne", "Sei\u00b7ten", "str\u00f6\u00b7men", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$.", "PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von euren Bissen, nagt! Ich singe drein,", "tokens": ["Von", "eu\u00b7ren", "Bis\u00b7sen", ",", "nagt", "!", "Ich", "sin\u00b7ge", "drein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "$.", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und singend will ich sterben.", "tokens": ["Und", "sin\u00b7gend", "will", "ich", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}