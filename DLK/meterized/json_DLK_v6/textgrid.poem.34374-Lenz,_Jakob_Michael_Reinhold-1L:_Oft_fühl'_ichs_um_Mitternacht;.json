{"textgrid.poem.34374": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Oft f\u00fchl' ichs um Mitternacht;", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft f\u00fchl' ichs um Mitternacht;", "tokens": ["Oft", "f\u00fchl'", "ichs", "um", "Mit\u00b7ter\u00b7nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dann stehn mir die Thr\u00e4nen im Auge,", "tokens": ["Dann", "stehn", "mir", "die", "Thr\u00e4\u00b7nen", "im", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und ich fall' im Dunkel vor dir aufs Knie, \u2013", "tokens": ["Und", "ich", "fall'", "im", "Dun\u00b7kel", "vor", "dir", "aufs", "Knie", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "APPR", "PPER", "APPRART", "NN", "$,", "$("], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Du pr\u00fcfst mir das Herz, und ich f\u00fchl' es noch w\u00e4rmer.", "tokens": ["Du", "pr\u00fcfst", "mir", "das", "Herz", ",", "und", "ich", "f\u00fchl'", "es", "noch", "w\u00e4r\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Heilig ist es \u2013 von Gott \u2013", "tokens": ["Hei\u00b7lig", "ist", "es", "\u2013", "von", "Gott", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "PPER", "$(", "APPR", "NN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Was im Herzen gl\u00fcht. Laut ruft es in mir,", "tokens": ["Was", "im", "Her\u00b7zen", "gl\u00fcht", ".", "Laut", "ruft", "es", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "VVFIN", "$.", "APPR", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "--+-+++--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Gott! \u2013 Laut ruft's dir entgegen. Es dringt", "tokens": ["Gott", "!", "\u2013", "Laut", "ruft's", "dir", "ent\u00b7ge\u00b7gen", ".", "Es", "dringt"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "$(", "APPR", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Durch die Gebeine, und auch die Gebeine f\u00fchlen's.", "tokens": ["Durch", "die", "Ge\u00b7bei\u00b7ne", ",", "und", "auch", "die", "Ge\u00b7bei\u00b7ne", "f\u00fch\u00b7len'", "s."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "ART", "NN", "$,", "KON", "ADV", "ART", "NN", "VVFIN", "NE"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}}, "stanza.3": {"line.1": {"text": "Wo ist's, die\u00df Bild? da\u00df ich's umfasse \u2013", "tokens": ["Wo", "ist's", ",", "die\u00df", "Bild", "?", "da\u00df", "ich's", "um\u00b7fas\u00b7se", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "$,", "PDS", "NN", "$.", "KOUS", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Bild Gottes, das meine Seele liebt?", "tokens": ["Das", "Bild", "Got\u00b7tes", ",", "das", "mei\u00b7ne", "See\u00b7le", "liebt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich wollt' es durchschauen; mein Arm sollt mit ihm verwachsen,", "tokens": ["Ich", "wollt'", "es", "durch\u00b7schau\u00b7en", ";", "mein", "Arm", "sollt", "mit", "ihm", "ver\u00b7wach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$.", "PPOSAT", "NN", "VMFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "Und tief pr\u00e4gt' ichs ins Herz.", "tokens": ["Und", "tief", "pr\u00e4gt'", "ichs", "ins", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PIS", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ach ein Bild! Gott du hie\u00dft es", "tokens": ["Ach", "ein", "Bild", "!", "Gott", "du", "hie\u00dft", "es"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "ART", "NN", "$.", "NN", "PPER", "VVFIN", "PPER"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Den Genius mir vor Augen halten.", "tokens": ["Den", "Ge\u00b7nius", "mir", "vor", "Au\u00b7gen", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wach ich fr\u00fch am Morgen, so steht es vor mir;", "tokens": ["Wach", "ich", "fr\u00fch", "am", "Mor\u00b7gen", ",", "so", "steht", "es", "vor", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Leg ich mich nieder, so schwebt es vor meiner Stirn.", "tokens": ["Leg", "ich", "mich", "nie\u00b7der", ",", "so", "schwebt", "es", "vor", "mei\u00b7ner", "Stirn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.5": {"line.1": {"text": "B\u00e4t' ich zu dir \u2013 wenn Himmel und Erde", "tokens": ["B\u00e4t'", "ich", "zu", "dir", "\u2013", "wenn", "Him\u00b7mel", "und", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$(", "KOUS", "NN", "KON", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Um mich vergeh'n \u2013 wenn du nur, und ich in dir", "tokens": ["Um", "mich", "ver\u00b7geh'n", "\u2013", "wenn", "du", "nur", ",", "und", "ich", "in", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "VVFIN", "$(", "KOUS", "PPER", "ADV", "$,", "KON", "PPER", "APPR", "PPER"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Noch bin \u2013 dann l\u00e4chelt die\u00df Bild in voller Klarheit", "tokens": ["Noch", "bin", "\u2013", "dann", "l\u00e4\u00b7chelt", "die\u00df", "Bild", "in", "vol\u00b7ler", "Klar\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$(", "ADV", "VVFIN", "PDS", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mir entgegen, da\u00df das Herz mir hinweg schmilzt.", "tokens": ["Mir", "ent\u00b7ge\u00b7gen", ",", "da\u00df", "das", "Herz", "mir", "hin\u00b7weg", "schmilzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "KOUS", "ART", "NN", "PPER", "APZR", "VVFIN", "$."], "meter": "+-+-+-+-+++", "measure": "unknown.measure.septa"}}, "stanza.6": {"line.1": {"text": "Weg! \u2013 da\u00df der Strom \u2013 er kocht mir im Herzen \u2013", "tokens": ["Weg", "!", "\u2013", "da\u00df", "der", "Strom", "\u2013", "er", "kocht", "mir", "im", "Her\u00b7zen", "\u2013"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "KOUS", "ART", "NN", "$(", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sich hier vor dem Herrn ergiesse!", "tokens": ["Sich", "hier", "vor", "dem", "Herrn", "er\u00b7gies\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Herr! ich will \u2013 ach! ich will es noch mehr!", "tokens": ["Herr", "!", "ich", "will", "\u2013", "ach", "!", "ich", "will", "es", "noch", "mehr", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VMFIN", "$(", "ITJ", "$.", "PPER", "VMFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Herr! die\u00df Verlangen \u2013 den himmlischen Zug!", "tokens": ["Herr", "!", "die\u00df", "Ver\u00b7lan\u00b7gen", "\u2013", "den", "himm\u00b7li\u00b7schen", "Zug", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PDS", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach vor dir! ja, nur dir \u2013 O, f\u00fchre mich hin!", "tokens": ["Ach", "vor", "dir", "!", "ja", ",", "nur", "dir", "\u2013", "O", ",", "f\u00fch\u00b7re", "mich", "hin", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$.", "PTKANT", "$,", "ADV", "PPER", "$(", "NE", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Es ist eine Seele, gleich gestimmt mit mir \u2013", "tokens": ["Es", "ist", "ei\u00b7ne", "See\u00b7le", ",", "gleich", "ge\u00b7stimmt", "mit", "mir", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADV", "VVPP", "APPR", "PPER", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich bin nicht ganz ohne sie \u2013 mit ihr", "tokens": ["Ich", "bin", "nicht", "ganz", "oh\u00b7ne", "sie", "\u2013", "mit", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "APPR", "PPER", "$(", "APPR", "PPOSAT"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Eins \u2013 soll ich die Ewigkeit genie\u00dfen.", "tokens": ["Eins", "\u2013", "soll", "ich", "die", "E\u00b7wig\u00b7keit", "ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Herr, ich sahe ein M\u00e4dchen \u2013 So wie die\u00df", "tokens": ["Herr", ",", "ich", "sa\u00b7he", "ein", "M\u00e4d\u00b7chen", "\u2013", "So", "wie", "die\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "ART", "NN", "$(", "ADV", "KOKOM", "PDS"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "M\u00fcss' ein M\u00e4dchen seyn.", "tokens": ["M\u00fcss'", "ein", "M\u00e4d\u00b7chen", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die edle Gottesseele flammt im Auge \u2013", "tokens": ["Die", "ed\u00b7le", "Got\u00b7tes\u00b7see\u00b7le", "flammt", "im", "Au\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Lieb', Unschuld, Gr\u00f6\u00dfe, W\u00e4rme, Adel!", "tokens": ["Lieb'", ",", "Un\u00b7schuld", ",", "Gr\u00f6\u00b7\u00dfe", ",", "W\u00e4r\u00b7me", ",", "A\u00b7del", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ach Gott! \u2013 Mich d\u00e4ucht, ich s\u00e4he das Bild", "tokens": ["Ach", "Gott", "!", "\u2013", "Mich", "d\u00e4ucht", ",", "ich", "s\u00e4\u00b7he", "das", "Bild"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$.", "$(", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Das vor meiner Seele schwebt.", "tokens": ["Das", "vor", "mei\u00b7ner", "See\u00b7le", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ganze Seele fing an sich zu heben,", "tokens": ["Die", "gan\u00b7ze", "See\u00b7le", "fing", "an", "sich", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch nie gef\u00fchlte heilige Ersch\u00fctterung", "tokens": ["Noch", "nie", "ge\u00b7f\u00fchl\u00b7te", "hei\u00b7li\u00b7ge", "Er\u00b7sch\u00fct\u00b7te\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Durchschauert' jede Nerve mir,", "tokens": ["Durch\u00b7schau\u00b7ert'", "je\u00b7de", "Ner\u00b7ve", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Geist wuchs. Ich liebte dich reiner,", "tokens": ["Der", "Geist", "wuchs", ".", "Ich", "lieb\u00b7te", "dich", "rei\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich f\u00fchlte mir Kraft, Tugend zu \u00fcben,", "tokens": ["Ich", "f\u00fchl\u00b7te", "mir", "Kraft", ",", "Tu\u00b7gend", "zu", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie ich zuvor nie sie gef\u00fchlt.", "tokens": ["Wie", "ich", "zu\u00b7vor", "nie", "sie", "ge\u00b7f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "PPER", "VVPP", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}}}}}