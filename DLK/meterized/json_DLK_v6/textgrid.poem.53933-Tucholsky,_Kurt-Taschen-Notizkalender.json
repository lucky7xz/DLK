{"textgrid.poem.53933": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Taschen-Notizkalender", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich gucke einmal,", "tokens": ["Ich", "gu\u00b7cke", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "ich gucke zweimal \u2013", "tokens": ["ich", "gu\u00b7cke", "zwei\u00b7mal", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ich denk: Nanu?", "tokens": ["Ich", "denk", ":", "Na\u00b7nu", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "da hat doch einer dran gedreht . . . ?", "tokens": ["da", "hat", "doch", "ei\u00b7ner", "dran", "ge\u00b7dreht", ".", ".", ".", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "PAV", "VVFIN", "$.", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}