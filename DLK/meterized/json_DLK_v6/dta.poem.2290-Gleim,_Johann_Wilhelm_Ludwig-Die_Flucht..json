{"dta.poem.2290": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Flucht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Br\u00fcder! seht doch durch die Gl\u00e4ser.", "tokens": ["Br\u00fc\u00b7der", "!", "seht", "doch", "durch", "die", "Gl\u00e4\u00b7ser", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seht doch, welche Menschenk\u00f6pfe!", "tokens": ["Seht", "doch", ",", "wel\u00b7che", "Men\u00b7schen\u00b7k\u00f6p\u00b7fe", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stehn doch K\u00f6pfe von den Thieren", "tokens": ["Stehn", "doch", "K\u00f6p\u00b7fe", "von", "den", "Thie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den H\u00e4lsen dieser M\u00e4nner!", "tokens": ["Auf", "den", "H\u00e4l\u00b7sen", "die\u00b7ser", "M\u00e4n\u00b7ner", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jener da, weist uns die Z\u00e4hne.", "tokens": ["Je\u00b7ner", "da", ",", "weist", "uns", "die", "Z\u00e4h\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADV", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Welcher Hund kan wol so bellen?", "tokens": ["Wel\u00b7cher", "Hund", "kan", "wol", "so", "bel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Welcher Hund ist ihm wol \u00e4hnlich?", "tokens": ["Wel\u00b7cher", "Hund", "ist", "ihm", "wol", "\u00e4hn\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dort im Winkel grunzt sein Bruder.", "tokens": ["Dort", "im", "Win\u00b7kel", "grunzt", "sein", "Bru\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "H\u00f6rt! nun f\u00e4ngt er an zu l\u00e4stern;", "tokens": ["H\u00f6rt", "!", "nun", "f\u00e4ngt", "er", "an", "zu", "l\u00e4s\u00b7tern", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "ADV", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Denn er l\u00e4stert auch im Beten.", "tokens": ["Denn", "er", "l\u00e4s\u00b7tert", "auch", "im", "Be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Welche schwarze L\u00e4sterworte", "tokens": ["Wel\u00b7che", "schwar\u00b7ze", "L\u00e4s\u00b7ter\u00b7wor\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Fliegen von den frommen Lippen!", "tokens": ["Flie\u00b7gen", "von", "den", "from\u00b7men", "Lip\u00b7pen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Br\u00fcder seht! die frommen Lippen", "tokens": ["Br\u00fc\u00b7der", "seht", "!", "die", "from\u00b7men", "Lip\u00b7pen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Sind so schwarz, wie Priesterr\u00f6kke.", "tokens": ["Sind", "so", "schwarz", ",", "wie", "Pries\u00b7ter\u00b7r\u00f6k\u00b7ke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "PWAV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Br\u00fcder komt, wir wollen laufen;", "tokens": ["Br\u00fc\u00b7der", "komt", ",", "wir", "wol\u00b7len", "lau\u00b7fen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Denn sie speien Ha\u00df und Geifer,", "tokens": ["Denn", "sie", "spei\u00b7en", "Ha\u00df", "und", "Gei\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Und er trift schon ihre Br\u00fcder.", "tokens": ["Und", "er", "trift", "schon", "ih\u00b7re", "Br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Komt, und la\u00dft die Narren l\u00e4stern,", "tokens": ["Komt", ",", "und", "la\u00dft", "die", "Nar\u00b7ren", "l\u00e4s\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVIMP", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Komt, wir wollen hier nicht trinken.", "tokens": ["Komt", ",", "wir", "wol\u00b7len", "hier", "nicht", "trin\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}