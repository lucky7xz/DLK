{"textgrid.poem.64132": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der Fiedelmann", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist der alte Fiedelmann,", "tokens": ["Das", "ist", "der", "al\u00b7te", "Fie\u00b7del\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umwallt vom grauen Bart:", "tokens": ["Um\u00b7wallt", "vom", "grau\u00b7en", "Bart", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hebt der sein machtvoll Liedel an,", "tokens": ["Hebt", "der", "sein", "macht\u00b7voll", "Lie\u00b7del", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PPOSAT", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "T\u00f6nt's ganz besondrer Art:", "tokens": ["T\u00f6nt's", "ganz", "be\u00b7sond\u00b7rer", "Art", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie Zauberzwang geschwinde", "tokens": ["Wie", "Zaub\u00b7er\u00b7zwang", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Lockt er vom Dorf die Kinde", "tokens": ["Lockt", "er", "vom", "Dorf", "die", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Heraus zur Heidenlinde.", "tokens": ["He\u00b7raus", "zur", "Hei\u00b7den\u00b7lin\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und spielt er auf zum Sunnwendtanz,", "tokens": ["Und", "spielt", "er", "auf", "zum", "Sunn\u00b7wend\u00b7tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lupft sich von selbst der Fu\u00df:", "tokens": ["Lupft", "sich", "von", "selbst", "der", "Fu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADV", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Des Burschen Haar, der Dirne Kranz", "tokens": ["Des", "Bur\u00b7schen", "Haar", ",", "der", "Dir\u00b7ne", "Kranz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tauscht knisternd hei\u00dfen Gru\u00df:", "tokens": ["Tauscht", "knis\u00b7ternd", "hei\u00b7\u00dfen", "Gru\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer ihrer nie ward inne,", "tokens": ["Wer", "ih\u00b7rer", "nie", "ward", "in\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADV", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dem weckt er f\u00fc\u00dfe Minne:", "tokens": ["Dem", "weckt", "er", "f\u00fc\u00b7\u00dfe", "Min\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bald gl\u00fchen alle Sinne.", "tokens": ["Bald", "gl\u00fc\u00b7hen", "al\u00b7le", "Sin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und singt er grau vergangne Zeit, \u2013", "tokens": ["Und", "singt", "er", "grau", "ver\u00b7gang\u00b7ne", "Zeit", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Heldentodgeschick,", "tokens": ["Von", "Hel\u00b7den\u00b7tod\u00b7ge\u00b7schick", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Heunensturm, vom V\u00f6lkerstreit: \u2013", "tokens": ["Vom", "Heu\u00b7nen\u00b7sturm", ",", "vom", "V\u00f6l\u00b7ker\u00b7streit", ":", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie spr\u00fcht der M\u00e4nner Blick!", "tokens": ["Wie", "spr\u00fcht", "der", "M\u00e4n\u00b7ner", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das hallt wie helle Harfen,", "tokens": ["Das", "hallt", "wie", "hel\u00b7le", "Har\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da K\u00f6nige noch die scharfen,", "tokens": ["Da", "K\u00f6\u00b7ni\u00b7ge", "noch", "die", "schar\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ART", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Die Schilddurchschmettrer warfen!", "tokens": ["Die", "Schild\u00b7durch\u00b7schmet\u00b7trer", "war\u00b7fen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und tiefer zieht den Schlappenhut", "tokens": ["Und", "tie\u00b7fer", "zieht", "den", "Schlap\u00b7pen\u00b7hut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wirrbart ins Gesicht:", "tokens": ["Der", "Wirr\u00b7bart", "ins", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hei, wie ihm lang verhaltne Glut", "tokens": ["Hei", ",", "wie", "ihm", "lang", "ver\u00b7halt\u00b7ne", "Glut"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "PPER", "ADJD", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Vom grauen Auge bricht:", "tokens": ["Vom", "grau\u00b7en", "Au\u00b7ge", "bricht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er singt, mit bittrem Leiden,", "tokens": ["Er", "singt", ",", "mit", "bitt\u00b7rem", "Lei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vom Gram der letzten Heiden", "tokens": ["Vom", "Gram", "der", "letz\u00b7ten", "Hei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und von der G\u00f6tter Scheiden.", "tokens": ["Und", "von", "der", "G\u00f6t\u00b7ter", "Schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbder Eichenhain in Flammen loht!", "tokens": ["\u00bb", "der", "Ei\u00b7chen\u00b7hain", "in", "Flam\u00b7men", "loht", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der heil'ge Quell ward blut'ger Pfuhl:", "tokens": ["Der", "heil'\u00b7ge", "Quell", "ward", "blut'\u00b7ger", "Pfuhl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Frau Berta klagt: \u00bbhilf Sassen\u00f4t:", "tokens": ["Frau", "Ber\u00b7ta", "klagt", ":", "\u00bb", "hilf", "Sas\u00b7sen\u00f4t", ":"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "$.", "$(", "VVIMP", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "In Tr\u00fcmmer barst die Irmensul!\u00ab", "tokens": ["In", "Tr\u00fcm\u00b7mer", "barst", "die", "Ir\u00b7men\u00b7sul", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf! lichtumfloss'ne Frauen", "tokens": ["Auf", "!", "lich\u00b7tum\u00b7floss'\u00b7ne", "Frau\u00b7en"], "token_info": ["word", "punct", "word", "word"], "pos": ["APPR", "$.", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Aus g\u00f6tterleeren Gauen", "tokens": ["Aus", "g\u00f6t\u00b7ter\u00b7lee\u00b7ren", "Gau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Empor zu Asgardhs Auen!\u00ab", "tokens": ["Em\u00b7por", "zu", "As\u00b7gardhs", "Au\u00b7en", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPR", "NE", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und Sehnsucht f\u00fcllt der H\u00f6rer Sinn. \u2013", "tokens": ["Und", "Sehn\u00b7sucht", "f\u00fcllt", "der", "H\u00f6\u00b7rer", "Sinn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da stirbt gemach der Fiedelton. \u2013", "tokens": ["Da", "stirbt", "ge\u00b7mach", "der", "Fie\u00b7del\u00b7ton", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo kam, wo schwand der Alte hin?", "tokens": ["Wo", "kam", ",", "wo", "schwand", "der", "Al\u00b7te", "hin", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "PWAV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Saum der Heide schwebt er schon!", "tokens": ["Am", "Saum", "der", "Hei\u00b7de", "schwebt", "er", "schon", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch fern klagt seine Weise:", "tokens": ["Noch", "fern", "klagt", "sei\u00b7ne", "Wei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es ziehn ums Haupt ihm leise", "tokens": ["Es", "ziehn", "ums", "Haupt", "ihm", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zwei Raben ihre Kreise! \u2013", "tokens": ["Zwei", "Ra\u00b7ben", "ih\u00b7re", "Krei\u00b7se", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["CARD", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}