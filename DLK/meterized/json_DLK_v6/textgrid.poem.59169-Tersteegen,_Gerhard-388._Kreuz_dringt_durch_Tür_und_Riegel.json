{"textgrid.poem.59169": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "388. Kreuz dringt durch T\u00fcr und Riegel", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh' ich in meine Zell' zur Ruh", "tokens": ["Geh'", "ich", "in", "mei\u00b7ne", "Zell'", "zur", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und denke sicher da zu sitzen,", "tokens": ["Und", "den\u00b7ke", "si\u00b7cher", "da", "zu", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schlie\u00df' auch mein T\u00fcrlein feste zu,", "tokens": ["Schlie\u00df'", "auch", "mein", "T\u00fcr\u00b7lein", "fes\u00b7te", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "NN", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du dringst herein gleich einer Nadelspitzen.", "tokens": ["Du", "dringst", "her\u00b7ein", "gleich", "ei\u00b7ner", "Na\u00b7del\u00b7spit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}