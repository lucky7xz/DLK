{"dta.poem.12084": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Warum l\u00e4\u00dft Volksmundart von Frauenlippen sich", "tokens": ["Wa\u00b7rum", "l\u00e4\u00dft", "Volks\u00b7mun\u00b7dart", "von", "Frau\u00b7en\u00b7lip\u00b7pen", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "NN", "APPR", "NN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lieblich h\u00f6ren, als von M\u00e4nnern widerlich?", "tokens": ["So", "lieb\u00b7lich", "h\u00f6\u00b7ren", ",", "als", "von", "M\u00e4n\u00b7nern", "wi\u00b7der\u00b7lich", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$,", "KOUS", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie rein der Reinheit, ist der Sch\u00f6nheit alles sch\u00f6n;", "tokens": ["Wie", "rein", "der", "Rein\u00b7heit", ",", "ist", "der", "Sch\u00f6n\u00b7heit", "al\u00b7les", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$,", "VAFIN", "ART", "NN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du h\u00f6rest, auch wenn sie's nicht spr\u00e4ch', ein Wohlget\u00f6n,", "tokens": ["Du", "h\u00f6\u00b7rest", ",", "auch", "wenn", "sie's", "nicht", "spr\u00e4ch'", ",", "ein", "Wohl\u00b7ge\u00b7t\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Anmuth ist es, die, alswie die Landestracht,", "tokens": ["Die", "An\u00b7muth", "ist", "es", ",", "die", ",", "als\u00b7wie", "die", "Lan\u00b7des\u00b7tracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "PRELS", "$,", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch Landesart und Sprach' am Weib anmuthig macht.", "tokens": ["Auch", "Lan\u00b7des\u00b7art", "und", "Sprach'", "am", "Weib", "an\u00b7mut\u00b7hig", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "APPRART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das Weib nat\u00fcrlich mag in der Natur verharren;", "tokens": ["Das", "Weib", "na\u00b7t\u00fcr\u00b7lich", "mag", "in", "der", "Na\u00b7tur", "ver\u00b7har\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Mann wird, wenn ers will, zum T\u00f6lpel oder Narren.", "tokens": ["Der", "Mann", "wird", ",", "wenn", "ers", "will", ",", "zum", "T\u00f6l\u00b7pel", "o\u00b7der", "Nar\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "KOUS", "PIS", "VMFIN", "$,", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Ein leichter Anflug nur von Mundart steht ihm gut,", "tokens": ["Ein", "leich\u00b7ter", "An\u00b7flug", "nur", "von", "Mund\u00b7art", "steht", "ihm", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alswie ein Erdgeschmack der Reben edlem Blut.", "tokens": ["Als\u00b7wie", "ein", "Erd\u00b7ge\u00b7schmack", "der", "Re\u00b7ben", "ed\u00b7lem", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}