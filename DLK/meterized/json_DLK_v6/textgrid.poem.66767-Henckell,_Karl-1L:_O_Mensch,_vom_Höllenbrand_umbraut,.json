{"textgrid.poem.66767": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Mensch, vom H\u00f6llenbrand umbraut,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Mensch, vom H\u00f6llenbrand umbraut,", "tokens": ["O", "Mensch", ",", "vom", "H\u00f6l\u00b7len\u00b7brand", "um\u00b7braut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ha\u00dfgiftigem Geschwele,", "tokens": ["Ha\u00df\u00b7gif\u00b7ti\u00b7gem", "Ge\u00b7schwe\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nach welchem fernen Ufer schaut", "tokens": ["Nach", "wel\u00b7chem", "fer\u00b7nen", "U\u00b7fer", "schaut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sehnsucht deiner Seele?", "tokens": ["Die", "Sehn\u00b7sucht", "dei\u00b7ner", "See\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wo von dem tiefen Blau getr\u00e4nkt", "tokens": ["Wo", "von", "dem", "tie\u00b7fen", "Blau", "ge\u00b7tr\u00e4nkt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein milder Himmel ruht,", "tokens": ["Ein", "mil\u00b7der", "Him\u00b7mel", "ruht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und du vergi\u00dft, was roh dich kr\u00e4nkt,", "tokens": ["Und", "du", "ver\u00b7gi\u00dft", ",", "was", "roh", "dich", "kr\u00e4nkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In \u00e4therklarer Flut.", "tokens": ["In", "\u00e4t\u00b7her\u00b7kla\u00b7rer", "Flut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gold rieselt um Basalt und Baum,", "tokens": ["Gold", "rie\u00b7selt", "um", "Ba\u00b7salt", "und", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "KON", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Hohe Zypressen steigen", "tokens": ["Ho\u00b7he", "Zyp\u00b7res\u00b7sen", "stei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "So feierstill zum reinen Raum,", "tokens": ["So", "fei\u00b7er\u00b7still", "zum", "rei\u00b7nen", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drin selige Genien geigen.", "tokens": ["Drin", "se\u00b7li\u00b7ge", "Ge\u00b7ni\u00b7en", "gei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Das heilige Schweigen wandelt hin", "tokens": ["Das", "hei\u00b7li\u00b7ge", "Schwei\u00b7gen", "wan\u00b7delt", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Auf Gr\u00e4serwogen sacht,", "tokens": ["Auf", "Gr\u00e4\u00b7ser\u00b7wo\u00b7gen", "sacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein dunkler Falter, schwebt dein Sinn", "tokens": ["Ein", "dunk\u00b7ler", "Fal\u00b7ter", ",", "schwebt", "dein", "Sinn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Erl\u00f6st in Lethes Nacht.", "tokens": ["Er\u00b7l\u00f6st", "in", "Le\u00b7thes", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}