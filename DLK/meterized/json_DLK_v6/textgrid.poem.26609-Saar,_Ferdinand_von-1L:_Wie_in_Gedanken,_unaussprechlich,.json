{"textgrid.poem.26609": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie in Gedanken, unaussprechlich,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie in Gedanken, unaussprechlich,", "tokens": ["Wie", "in", "Ge\u00b7dan\u00b7ken", ",", "un\u00b7aus\u00b7sprech\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gehen Arm in Arm sie einher;", "tokens": ["Ge\u00b7hen", "Arm", "in", "Arm", "sie", "ein\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "PPER", "PTKVZ", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Tief gebeugt und gebrochen ", "tokens": ["Tief", "ge\u00b7beugt", "und", "ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "KON", "VVPP"], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Schwer belastet mit Sammt und Seide,", "tokens": ["Schwer", "be\u00b7las\u00b7tet", "mit", "Sammt", "und", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "KON", "NE", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wankt ihr vornehm hagerer Leib", "tokens": ["Wankt", "ihr", "vor\u00b7nehm", "ha\u00b7ge\u00b7rer", "Leib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "ADJA", "NN"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.3": {"text": "Und es blinkt ihr gold'nes Geschmeide \u2013", "tokens": ["Und", "es", "blinkt", "ihr", "gold'\u00b7nes", "Ge\u00b7schmei\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ja, sie war einst ein sch\u00f6nes Weib!", "tokens": ["Ja", ",", "sie", "war", "einst", "ein", "sch\u00f6\u00b7nes", "Weib", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie im Nachglanz sieghafter Stunden", "tokens": ["Wie", "im", "Nach\u00b7glanz", "sieg\u00b7haf\u00b7ter", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Flammt noch zuweilen ihr dunkler Blick \u2013", "tokens": ["Flammt", "noch", "zu\u00b7wei\u00b7len", "ihr", "dunk\u00b7ler", "Blick", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Schlaff ergeben in sein Geschick.", "tokens": ["Schlaff", "er\u00b7ge\u00b7ben", "in", "sein", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Ach, was liegt nicht Alles dazwischen,", "tokens": ["Ach", ",", "was", "liegt", "nicht", "Al\u00b7les", "da\u00b7zwi\u00b7schen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "PTKNEG", "PIS", "PAV", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Seit die Beiden gewesen jung;", "tokens": ["Seit", "die", "Bei\u00b7den", "ge\u00b7we\u00b7sen", "jung", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAPP", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Wenn sie k\u00f6nnten, sie m\u00f6chten's verwischen", "tokens": ["Wenn", "sie", "k\u00f6nn\u00b7ten", ",", "sie", "m\u00f6ch\u00b7ten's", "ver\u00b7wi\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gern in ihrer Erinnerung.", "tokens": ["Gern", "in", "ih\u00b7rer", "E\u00b7rin\u00b7ne\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Halbes Finden im ersten Genusse,", "tokens": ["Hal\u00b7bes", "Fin\u00b7den", "im", "ers\u00b7ten", "Ge\u00b7nus\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Launen des Hochmuths, der Eitelkeit \u2013", "tokens": ["Lau\u00b7nen", "des", "Hoch\u00b7muths", ",", "der", "Ei\u00b7tel\u00b7keit", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "H\u00f6hnender Treubruch und seine Bu\u00dfe,", "tokens": ["H\u00f6h\u00b7nen\u00b7der", "Treu\u00b7bruch", "und", "sei\u00b7ne", "Bu\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+--++-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "B\u00f6se Jahre voll Ha\u00df und Streit.", "tokens": ["B\u00f6\u00b7se", "Jah\u00b7re", "voll", "Ha\u00df", "und", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Von der Natur auseinander getrieben,", "tokens": ["Von", "der", "Na\u00b7tur", "aus\u00b7ein\u00b7an\u00b7der", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "---+--+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Aber durch \u00bbR\u00fccksicht\u00ab immer vereint,", "tokens": ["A\u00b7ber", "durch", "\u00bb", "R\u00fcck\u00b7sicht", "\u00ab", "im\u00b7mer", "ver\u00b7eint", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "$(", "NN", "$(", "ADV", "VVPP", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Lernten sie kaum ihre Kinder lieben,", "tokens": ["Lern\u00b7ten", "sie", "kaum", "ih\u00b7re", "Kin\u00b7der", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die oft im Stillen dar\u00fcber geweint.", "tokens": ["Die", "oft", "im", "Stil\u00b7len", "da\u00b7r\u00fc\u00b7ber", "ge\u00b7weint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "PAV", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Dennoch sich t\u00e4glich in's Auge zu blicken,", "tokens": ["Den\u00b7noch", "sich", "t\u00e4g\u00b7lich", "in's", "Au\u00b7ge", "zu", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hatten die Beiden sich m\u00e4lig gew\u00f6hnt \u2013", "tokens": ["Hat\u00b7ten", "die", "Bei\u00b7den", "sich", "m\u00e4\u00b7lig", "ge\u00b7w\u00f6hnt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PRF", "ADJD", "VVPP", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Und nun hat, nach all' den Geschicken,", "tokens": ["Und", "nun", "hat", ",", "nach", "all'", "den", "Ge\u00b7schi\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "$,", "APPR", "PIS", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie noch zuletzt das Alter vers\u00f6hnt.", "tokens": ["Sie", "noch", "zu\u00b7letzt", "das", "Al\u00b7ter", "ver\u00b7s\u00f6hnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Wenn auch sp\u00e4t \u2013 ", "tokens": ["Wenn", "auch", "sp\u00e4t", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "\u00bbach, er war doch edel und gut!\u00ab", "tokens": ["\u00bb", "ach", ",", "er", "war", "doch", "e\u00b7del", "und", "gut", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "\u00bbach, sie hatte nur hei\u00dfes Blut!\u00ab", "tokens": ["\u00bb", "ach", ",", "sie", "hat\u00b7te", "nur", "hei\u00b7\u00dfes", "Blut", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "PPER", "VAFIN", "ADV", "ADJA", "NN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Und sie l\u00e4chelt, wenn er beflissen", "tokens": ["Und", "sie", "l\u00e4\u00b7chelt", ",", "wenn", "er", "be\u00b7flis\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVINF"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ihr den Shawl, die Mantille tr\u00e4gt,", "tokens": ["Ihr", "den", "Shawl", ",", "die", "Man\u00b7til\u00b7le", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Und er l\u00e4chelt, wenn sie die Kissen", "tokens": ["Und", "er", "l\u00e4\u00b7chelt", ",", "wenn", "sie", "die", "Kis\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Abends sorglich zurecht ihm legt ...", "tokens": ["A\u00b7bends", "sorg\u00b7lich", "zu\u00b7recht", "ihm", "legt", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "VVFIN", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "Wie in Gedanken, unaussprechlich,", "tokens": ["Wie", "in", "Ge\u00b7dan\u00b7ken", ",", "un\u00b7aus\u00b7sprech\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gehen Arm in Arm sie einher,", "tokens": ["Ge\u00b7hen", "Arm", "in", "Arm", "sie", "ein\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "PPER", "PTKVZ", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Tief gebeugt und gebrochen ", "tokens": ["Tief", "ge\u00b7beugt", "und", "ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "KON", "VVPP"], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}