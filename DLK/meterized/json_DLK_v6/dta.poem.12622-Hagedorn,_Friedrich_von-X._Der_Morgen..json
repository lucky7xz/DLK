{"dta.poem.12622": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "X.  \n  Der Morgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Uns lockt die Morgenr\u00f6the", "tokens": ["Uns", "lockt", "die", "Mor\u00b7gen\u00b7r\u00f6\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Busch und Wald,", "tokens": ["In", "Busch", "und", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wo schon der Hirten Fl\u00f6te", "tokens": ["Wo", "schon", "der", "Hir\u00b7ten", "Fl\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ins Land erschallt.", "tokens": ["Ins", "Land", "er\u00b7schallt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die Lerche steigt und schwirret,", "tokens": ["Die", "Ler\u00b7che", "steigt", "und", "schwir\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von Lust erregt:", "tokens": ["Von", "Lust", "er\u00b7regt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Die Taube lacht und girret:", "tokens": ["Die", "Tau\u00b7be", "lacht", "und", "gir\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Wachtel schl\u00e4gt.", "tokens": ["Die", "Wach\u00b7tel", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Die H\u00fcgel und die Weide", "tokens": ["Die", "H\u00fc\u00b7gel", "und", "die", "Wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Stehn aufgehellt,", "tokens": ["Stehn", "auf\u00b7ge\u00b7hellt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und Fruchtbarkeit und Freude", "tokens": ["Und", "Frucht\u00b7bar\u00b7keit", "und", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bebl\u00fcmt das Feld.", "tokens": ["Be\u00b7bl\u00fcmt", "das", "Feld", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Der Schmelz der gr\u00fcnen Fl\u00e4chen", "tokens": ["Der", "Schmelz", "der", "gr\u00fc\u00b7nen", "Fl\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gl\u00e4nzt voller Pracht,", "tokens": ["Gl\u00e4nzt", "vol\u00b7ler", "Pracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und von den klaren B\u00e4chen", "tokens": ["Und", "von", "den", "kla\u00b7ren", "B\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Entweicht die Nacht.", "tokens": ["Ent\u00b7weicht", "die", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Der H\u00fcgel weisse B\u00fcrde,", "tokens": ["Der", "H\u00fc\u00b7gel", "weis\u00b7se", "B\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Schafe Zucht", "tokens": ["Der", "Scha\u00b7fe", "Zucht"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dr\u00e4ngt sich aus Stall und H\u00fcrde", "tokens": ["Dr\u00e4ngt", "sich", "aus", "Stall", "und", "H\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit froher Flucht.", "tokens": ["Mit", "fro\u00b7her", "Flucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Seht wie der Mann der Herde", "tokens": ["Seht", "wie", "der", "Mann", "der", "Her\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Morgen f\u00fchlt", "tokens": ["Den", "Mor\u00b7gen", "f\u00fchlt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und auf der frischen Erde", "tokens": ["Und", "auf", "der", "fri\u00b7schen", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den Buhler spielt.", "tokens": ["Den", "Buh\u00b7ler", "spielt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Der J\u00e4ger macht schon rege", "tokens": ["Der", "J\u00e4\u00b7ger", "macht", "schon", "re\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und hetzt das Reh", "tokens": ["Und", "hetzt", "das", "Reh"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Durch blutbetriefte Wege,", "tokens": ["Durch", "blut\u00b7be\u00b7trief\u00b7te", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch Busch und Klee.", "tokens": ["Durch", "Busch", "und", "Klee", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sein Hifthorn gibt das Zeichen;", "tokens": ["Sein", "Hif\u00b7thorn", "gibt", "das", "Zei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Man eilt herbey:", "tokens": ["Man", "eilt", "her\u00b7bey", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Gleich schallt aus allen Str\u00e4uchen", "tokens": ["Gleich", "schallt", "aus", "al\u00b7len", "Str\u00e4u\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Jagd-Geschrey.", "tokens": ["Das", "Jagd\u00b7Ge\u00b7schrey", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Doch Phyllis Herz erbebet", "tokens": ["Doch", "Phyl\u00b7lis", "Herz", "er\u00b7be\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bey dieser Lust:", "tokens": ["Bey", "die\u00b7ser", "Lust", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nur Z\u00e4rtlichkeit belebet", "tokens": ["Nur", "Z\u00e4rt\u00b7lich\u00b7keit", "be\u00b7le\u00b7bet"], "token_info": ["word", "word", "word"], "pos": ["ADV", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die sanfte Brust.", "tokens": ["Die", "sanf\u00b7te", "Brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "La\u00df uns die Th\u00e4ler suchen,", "tokens": ["La\u00df", "uns", "die", "Th\u00e4\u00b7ler", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Geliebtes Kind!", "tokens": ["Ge\u00b7lieb\u00b7tes", "Kind", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wo wir von Berg und Buchen", "tokens": ["Wo", "wir", "von", "Berg", "und", "Bu\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Umschlossen sind.", "tokens": ["Um\u00b7schlos\u00b7sen", "sind", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Erkenne dich im Bilde", "tokens": ["Er\u00b7ken\u00b7ne", "dich", "im", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von jener Flur:", "tokens": ["Von", "je\u00b7ner", "Flur", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sey stets, wie die\u00df Gefilde,", "tokens": ["Sey", "stets", ",", "wie", "die\u00df", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "PWAV", "PDS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sch\u00f6n durch Natur.", "tokens": ["Sch\u00f6n", "durch", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Erw\u00fcnschter als der Morgen,", "tokens": ["Er\u00b7w\u00fcnschter", "als", "der", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Hold wie sein Stral,", "tokens": ["Hold", "wie", "sein", "Stral", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "So frey von Stolz und Sorgen", "tokens": ["So", "frey", "von", "Stolz", "und", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie dieses Thal.", "tokens": ["Wie", "die\u00b7ses", "Thal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}