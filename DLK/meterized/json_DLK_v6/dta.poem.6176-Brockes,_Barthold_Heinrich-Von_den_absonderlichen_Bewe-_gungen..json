{"dta.poem.6176": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den absonderlichen Bewe-  \n gungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In seinem eignen Stoff hat er nicht mehrern Raum.", "tokens": ["In", "sei\u00b7nem", "eig\u00b7nen", "Stoff", "hat", "er", "nicht", "meh\u00b7rern", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "PTKNEG", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie, wenn in kupffernen und gl\u00fchenden Gef\u00e4sse", "tokens": ["Wie", ",", "wenn", "in", "kupf\u00b7fer\u00b7nen", "und", "gl\u00fc\u00b7hen\u00b7den", "Ge\u00b7f\u00e4s\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "KOUS", "APPR", "ADJA", "KON", "ADJA", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Das Wasser \u00fcberbraust, und in dem weissen Schaum,", "tokens": ["Das", "Was\u00b7ser", "\u00fc\u00b7berb\u00b7raust", ",", "und", "in", "dem", "weis\u00b7sen", "Schaum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem Schein nach, seine Gr\u00f6sse", "tokens": ["Dem", "Schein", "nach", ",", "sei\u00b7ne", "Gr\u00f6s\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vermehrt; so hat doch in der That die Fluht", "tokens": ["Ver\u00b7mehrt", ";", "so", "hat", "doch", "in", "der", "That", "die", "Fluht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$.", "ADV", "VAFIN", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "An sich nicht gr\u00f6ssern Platz, indem von Lufft und Gluht", "tokens": ["An", "sich", "nicht", "gr\u00f6s\u00b7sern", "Platz", ",", "in\u00b7dem", "von", "Lufft", "und", "Gluht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "PTKNEG", "ADJA", "NN", "$,", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Die Theile mit derselben sich vereinen,", "tokens": ["Die", "Thei\u00b7le", "mit", "der\u00b7sel\u00b7ben", "sich", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wodurch die Fluhten denn, als wie vermehret, scheinen.", "tokens": ["Wo\u00b7durch", "die", "Fluh\u00b7ten", "denn", ",", "als", "wie", "ver\u00b7meh\u00b7ret", ",", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "$,", "KOUS", "KOKOM", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}