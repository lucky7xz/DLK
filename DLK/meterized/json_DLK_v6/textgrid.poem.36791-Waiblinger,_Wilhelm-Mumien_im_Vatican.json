{"textgrid.poem.36791": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Mumien im Vatican", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles sind' ich in dir, Laokoon, Zeus und Apollo,", "tokens": ["Al\u00b7les", "sind'", "ich", "in", "dir", ",", "Lao\u00b7ko\u00b7on", ",", "Zeus", "und", "A\u00b7pol\u00b7lo", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "PPER", "$,", "NE", "$,", "NE", "KON", "NE", "$,"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Aus dem gest\u00fcrzten Olymp flohen die G\u00f6tter zu dir.", "tokens": ["Aus", "dem", "ge\u00b7st\u00fcrz\u00b7ten", "O\u00b7lymp", "flo\u00b7hen", "die", "G\u00f6t\u00b7ter", "zu", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Welt der Griechen und R\u00f6mer, du zeigst auch \u00e4gyptische G\u00f6tzen,", "tokens": ["Welt", "der", "Grie\u00b7chen", "und", "R\u00f6\u00b7mer", ",", "du", "zeigst", "auch", "\u00e4\u00b7gypt\u00b7i\u00b7sche", "G\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und Brittania versorgt reichlich mit Mumien dich.", "tokens": ["Und", "Brit\u00b7ta\u00b7nia", "ver\u00b7sorgt", "reich\u00b7lich", "mit", "Mu\u00b7mi\u00b7en", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ADJD", "APPR", "NN", "PPER", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}