{"dta.poem.7941": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Traumdeutung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Gestern hatt\u2019 ich getr\u00e4umt, mein M\u00e4dchen am Fenster zu sehen;", "tokens": ["Ge\u00b7stern", "hatt'", "ich", "ge\u00b7tr\u00e4umt", ",", "mein", "M\u00e4d\u00b7chen", "am", "Fens\u00b7ter", "zu", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$,", "PPOSAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch was sah ich des Tags? Blumen der Lieblichen nur.", "tokens": ["Doch", "was", "sah", "ich", "des", "Tags", "?", "Blu\u00b7men", "der", "Lieb\u00b7li\u00b7chen", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "ART", "NN", "$.", "NN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Heute nun war mir im Traum, als s\u00e4h\u2019 ich am Fenster die", "tokens": ["Heu\u00b7te", "nun", "war", "mir", "im", "Traum", ",", "als", "s\u00e4h'", "ich", "am", "Fens\u00b7ter", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "APPRART", "NN", "$,", "KOUS", "VVFIN", "PPER", "APPRART", "NN", "ART"], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Blumen;", "tokens": ["Blu\u00b7men", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Darum schau\u2019 ich gewi\u00df heute die Liebliche selbst.", "tokens": ["Da\u00b7rum", "schau'", "ich", "ge\u00b7wi\u00df", "heu\u00b7te", "die", "Lieb\u00b7li\u00b7che", "selbst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "ADV", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}}}}}