{"textgrid.poem.58997": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "216. Reisegesetz", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel Wort, viel Last und viel Gesch\u00e4fte", "tokens": ["Viel", "Wort", ",", "viel", "Last", "und", "viel", "Ge\u00b7sch\u00e4f\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Benimmt dem Pilger seine Kr\u00e4fte;", "tokens": ["Be\u00b7nimmt", "dem", "Pil\u00b7ger", "sei\u00b7ne", "Kr\u00e4f\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wer ohn' Erm\u00fcden reisen will,", "tokens": ["Wer", "ohn'", "Er\u00b7m\u00fc\u00b7den", "rei\u00b7sen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der geh mit Gott ganz blo\u00df und still!", "tokens": ["Der", "geh", "mit", "Gott", "ganz", "blo\u00df", "und", "still", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "ADV", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}