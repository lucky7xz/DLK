{"dta.poem.13279": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hierauf erwies der Saal nicht viel Aufmercksamkeit,", "tokens": ["Hier\u00b7auf", "er\u00b7wies", "der", "Saal", "nicht", "viel", "Auf\u00b7merck\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PTKNEG", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man redte dort und da nur von der Seltenheit:", "tokens": ["Man", "red\u00b7te", "dort", "und", "da", "nur", "von", "der", "Sel\u00b7ten\u00b7heit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "KON", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Jezt aber gab man acht, vielleicht den Schlu\u00df zu h\u00f6ren,", "tokens": ["Jezt", "a\u00b7ber", "gab", "man", "acht", ",", "viel\u00b7leicht", "den", "Schlu\u00df", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "CARD", "$,", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil sich die K\u00f6niginn zu solchem schien zu kehren.", "tokens": ["Weil", "sich", "die", "K\u00f6\u00b7ni\u00b7ginn", "zu", "sol\u00b7chem", "schien", "zu", "keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "PIAT", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}