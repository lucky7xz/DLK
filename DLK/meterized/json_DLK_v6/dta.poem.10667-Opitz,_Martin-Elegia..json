{"dta.poem.10667": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Elegia.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Weil da\u00df die Sonne sich ins tieffe Meer begeben/", "tokens": ["Weil", "da\u00df", "die", "Son\u00b7ne", "sich", "ins", "tief\u00b7fe", "Meer", "be\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "PRF", "APPRART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd jhr gestirntes Haupt die Nacht hat vffgericht/", "tokens": ["Vnd", "jhr", "ge\u00b7stirn\u00b7tes", "Haupt", "die", "Nacht", "hat", "vff\u00b7ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein Menschen/ Vieh vnd Wild wie gleichsam ohne Leben/", "tokens": ["Sein", "Men\u00b7schen", "/", "Vieh", "vnd", "Wild", "wie", "gleich\u00b7sam", "oh\u00b7ne", "Le\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "NN", "KON", "NN", "KOKOM", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Monde scheinet auch gar kaum mit halbem Liecht.", "tokens": ["Der", "Mon\u00b7de", "schei\u00b7net", "auch", "gar", "kaum", "mit", "hal\u00b7bem", "Liecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich/ ob schon alles schl\u00e4fft/ mu\u00df ohn auffh\u00f6ren wachen/", "tokens": ["Ich", "/", "ob", "schon", "al\u00b7les", "schl\u00e4fft", "/", "mu\u00df", "ohn", "auff\u00b7h\u00f6\u00b7ren", "wa\u00b7chen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "KOUS", "ADV", "PIS", "VVFIN", "$(", "VMFIN", "APPR", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich/ ob schon allesruht/ mu\u00df ruhen ohne Ruh/", "tokens": ["Ich", "/", "ob", "schon", "al\u00b7les\u00b7ruht", "/", "mu\u00df", "ru\u00b7hen", "oh\u00b7ne", "Ruh", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "KOUS", "ADV", "ADJD", "$(", "VMFIN", "VVINF", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ob schon die gantze Welt frey ist von jhren sachen/", "tokens": ["Ob", "schon", "die", "gant\u00b7ze", "Welt", "frey", "ist", "von", "jhren", "sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Bring ich vor Liebes Brunst vnd Angst kein Auge zu.", "tokens": ["Bring", "ich", "vor", "Lie\u00b7bes", "Brunst", "vnd", "Angst", "kein", "Au\u00b7ge", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "KON", "NN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vnd dich/ Asterie/ hat auch der Schlaf vmbringet/", "tokens": ["Vnd", "dich", "/", "As\u00b7te\u00b7rie", "/", "hat", "auch", "der", "Schlaf", "vm\u00b7brin\u00b7get", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "NN", "$(", "VAFIN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Der Tages Arbeit furth/ de\u00df Todes Ebenbild/", "tokens": ["Der", "Ta\u00b7ges", "Ar\u00b7beit", "furth", "/", "de\u00df", "To\u00b7des", "E\u00b7ben\u00b7bild", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da mir der Z\u00e4hrenbach au\u00df beyden Augen dringet/", "tokens": ["Da", "mir", "der", "Z\u00e4h\u00b7ren\u00b7bach", "au\u00df", "bey\u00b7den", "Au\u00b7gen", "drin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bistu mit sanffter Ruh auff deinem Beth erf\u00fcllt.", "tokens": ["Bis\u00b7tu", "mit", "sanff\u00b7ter", "Ruh", "auff", "dei\u00b7nem", "Be\u00b7th", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Wie wann sich Delia hat in den Wald verborgen/", "tokens": ["Wie", "wann", "sich", "De\u00b7lia", "hat", "in", "den", "Wald", "ver\u00b7bor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PWAV", "PRF", "NE", "VAFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Wirdt durch den Schlaff erwischt/ vnd fellt ins gr\u00fcne Gra\u00df.", "tokens": ["Wirdt", "durch", "den", "Schlaff", "er\u00b7wischt", "/", "vnd", "fellt", "ins", "gr\u00fc\u00b7ne", "Gra\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$(", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Vnd wie die Nymfen auch sich legen gegen Morgen/", "tokens": ["Vnd", "wie", "die", "Nym\u00b7fen", "auch", "sich", "le\u00b7gen", "ge\u00b7gen", "Mor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "PRF", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wann der n\u00e4chtliche Tantz sie hat gemachet la\u00df.", "tokens": ["Wann", "der", "n\u00e4cht\u00b7li\u00b7che", "Tantz", "sie", "hat", "ge\u00b7ma\u00b7chet", "la\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPER", "VAFIN", "VVPP", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Sie ruhen sicherlich bey einem frischen Bronnen/", "tokens": ["Sie", "ru\u00b7hen", "si\u00b7cher\u00b7lich", "bey", "ei\u00b7nem", "fri\u00b7schen", "Bron\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die B\u00e4ume halten auff der Morgenr\u00f6the Licht/", "tokens": ["Die", "B\u00e4u\u00b7me", "hal\u00b7ten", "auff", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Das sie nicht also bald erwachen von der Sonnen/", "tokens": ["Das", "sie", "nicht", "al\u00b7so", "bald", "er\u00b7wa\u00b7chen", "von", "der", "Son\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKNEG", "ADV", "ADV", "VVINF", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Deckt sie der dicke Wald: Pan aber schl\u00e4ffet nicht.", "tokens": ["Deckt", "sie", "der", "di\u00b7cke", "Wald", ":", "Pan", "a\u00b7ber", "schl\u00e4f\u00b7fet", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "NE", "ADV", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Er geht/ er rufft/ er schreyt mit sehnlichem verlangen/", "tokens": ["Er", "geht", "/", "er", "rufft", "/", "er", "schreyt", "mit", "sehn\u00b7li\u00b7chem", "ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df seine Stimm erklingt durch P\u00fcsche/ Berg/ vnd Thal/", "tokens": ["Da\u00df", "sei\u00b7ne", "Stimm", "er\u00b7klingt", "durch", "P\u00fc\u00b7sche", "/", "Berg", "/", "vnd", "Thal", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$(", "NN", "$(", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Vnd sie sein sanfftiglich mit s\u00fcssem Traum vmbfangen/", "tokens": ["Vnd", "sie", "sein", "sanff\u00b7tig\u00b7lich", "mit", "s\u00fcs\u00b7sem", "Traum", "vmb\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "ADJD", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Dem Pan antwortet nur der blosse Widerschall.", "tokens": ["Dem", "Pan", "ant\u00b7wor\u00b7tet", "nur", "der", "blos\u00b7se", "Wi\u00b7der\u00b7schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du auch/ mein Leben/ schleffst/ ich mu\u00df in N\u00f6then wallen/", "tokens": ["Du", "auch", "/", "mein", "Le\u00b7ben", "/", "schleffst", "/", "ich", "mu\u00df", "in", "N\u00f6\u00b7then", "wal\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "PPOSAT", "NN", "$(", "VVFIN", "$(", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Du bist inguter Ruh/ ich wache f\u00fcr vnd f\u00fcr/", "tokens": ["Du", "bist", "in\u00b7gu\u00b7ter", "Ruh", "/", "ich", "wa\u00b7che", "f\u00fcr", "vnd", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "NN", "$(", "PPER", "ADJA", "APPR", "KON", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Bi\u00df mich der letzte Todt wirdt endlich vberfallen/", "tokens": ["Bi\u00df", "mich", "der", "letz\u00b7te", "Todt", "wirdt", "end\u00b7lich", "vber\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "VAFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Auff den ich sehnlich wart allhie bey deiner Th\u00fcr.", "tokens": ["Auff", "den", "ich", "sehn\u00b7lich", "wart", "all\u00b7hie", "bey", "dei\u00b7ner", "Th\u00fcr."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "ART", "PPER", "ADV", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}