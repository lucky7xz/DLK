{"textgrid.poem.64849": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: In der Brust die Schmerzenwunde,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der Brust die Schmerzenwunde,", "tokens": ["In", "der", "Brust", "die", "Schmer\u00b7zen\u00b7wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die dein Scheiden mir geschlagen,", "tokens": ["Die", "dein", "Schei\u00b7den", "mir", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lasse in tiefn\u00e4cht'ger Stunde", "tokens": ["Las\u00b7se", "in", "tief\u00b7n\u00e4cht'\u00b7ger", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Mich an deinem Sarge klagen.", "tokens": ["Mich", "an", "dei\u00b7nem", "Sar\u00b7ge", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df in deinem Angesichte", "tokens": ["La\u00df", "in", "dei\u00b7nem", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wiederfinden mich den Frieden", "tokens": ["Wie\u00b7der\u00b7fin\u00b7den", "mich", "den", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den Strahl aus h\u00f6h'rem Lichte,", "tokens": ["Und", "den", "Strahl", "aus", "h\u00f6h'\u00b7rem", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der mit dir von uns geschieden! \u2013", "tokens": ["Der", "mit", "dir", "von", "uns", "ge\u00b7schie\u00b7den", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "PPER", "APPR", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O in welchem Sch\u00f6nheitsglanze", "tokens": ["O", "in", "wel\u00b7chem", "Sch\u00f6n\u00b7heits\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "PWAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegst du da auf deiner Bahre,", "tokens": ["Liegst", "du", "da", "auf", "dei\u00b7ner", "Bah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem hellen Bl\u00fcthenkranze", "tokens": ["Mit", "dem", "hel\u00b7len", "Bl\u00fc\u00b7then\u00b7kran\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Lenznacht deiner Haare! \u2013", "tokens": ["In", "der", "Lenz\u00b7nacht", "dei\u00b7ner", "Haa\u00b7re", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mit den Blumen, die sich f\u00fcgen", "tokens": ["Mit", "den", "Blu\u00b7men", ",", "die", "sich", "f\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bunt zum Strau\u00df und D\u00fcfte f\u00e4cheln,", "tokens": ["Bunt", "zum", "Strau\u00df", "und", "D\u00fcf\u00b7te", "f\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NE", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem Ernst auf deinen Z\u00fcgen", "tokens": ["Mit", "dem", "Ernst", "auf", "dei\u00b7nen", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem heil'gen Siegesl\u00e4cheln!", "tokens": ["Mit", "dem", "heil'\u00b7gen", "Sie\u00b7ges\u00b7l\u00e4\u00b7cheln", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nimmer, nimmer werd' ich's fassen!", "tokens": ["Nim\u00b7mer", ",", "nim\u00b7mer", "werd'", "ich's", "fas\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich, du Lieblichste von Allen,", "tokens": ["Dich", ",", "du", "Lieb\u00b7lichs\u00b7te", "von", "Al\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Konnte dich das Leben lassen?", "tokens": ["Konn\u00b7te", "dich", "das", "Le\u00b7ben", "las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durftest du dem Tod verfallen?", "tokens": ["Durf\u00b7test", "du", "dem", "Tod", "ver\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja er h\u00e4lt dich nun umgittert,", "tokens": ["Ja", "er", "h\u00e4lt", "dich", "nun", "um\u00b7git\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tief und still, du Sondergleiche!", "tokens": ["Tief", "und", "still", ",", "du", "Son\u00b7derg\u00b7lei\u00b7che", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Deinen Gl\u00fcck, zersplittert", "tokens": ["Und", "der", "Dei\u00b7nen", "Gl\u00fcck", ",", "zer\u00b7split\u00b7tert"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "PPOSAT", "NN", "$,", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sank es hin an deiner Leiche!", "tokens": ["Sank", "es", "hin", "an", "dei\u00b7ner", "Lei\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach! und zu den Deinen z\u00e4hle", "tokens": ["Ach", "!", "und", "zu", "den", "Dei\u00b7nen", "z\u00e4h\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KON", "APPR", "ART", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich, die Qual- und Schmerzentbrannte,", "tokens": ["Ich", ",", "die", "Qua\u00b7l", "und", "Schmer\u00b7zent\u00b7brann\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "TRUNC", "KON", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Selber mich und jede Seele,", "tokens": ["Sel\u00b7ber", "mich", "und", "je\u00b7de", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welche, Engel! dich erkannte. \u2013", "tokens": ["Wel\u00b7che", ",", "En\u00b7gel", "!", "dich", "er\u00b7kann\u00b7te", ".", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWAT", "$,", "NE", "$.", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Heimgeschifft zum Vaterhause", "tokens": ["Heim\u00b7ge\u00b7schifft", "zum", "Va\u00b7ter\u00b7hau\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bist du in dem dunkeln Nachen \u2013", "tokens": ["Bist", "du", "in", "dem", "dun\u00b7keln", "Na\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlummre denn in deiner Klause!", "tokens": ["Schlumm\u00b7re", "denn", "in", "dei\u00b7ner", "Klau\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig wird die Klage wachen.", "tokens": ["E\u00b7wig", "wird", "die", "Kla\u00b7ge", "wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "F\u00fcr ein Herz, dem du entrissen,", "tokens": ["F\u00fcr", "ein", "Herz", ",", "dem", "du", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Giebt's nur ", "tokens": ["Giebt's", "nur"], "token_info": ["word", "word"], "pos": ["NE", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "F\u00fcr so schmerzliches Vermissen,", "tokens": ["F\u00fcr", "so", "schmerz\u00b7li\u00b7ches", "Ver\u00b7mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er hei\u00dft: um dich zu weinen!", "tokens": ["Und", "er", "hei\u00dft", ":", "um", "dich", "zu", "wei\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}