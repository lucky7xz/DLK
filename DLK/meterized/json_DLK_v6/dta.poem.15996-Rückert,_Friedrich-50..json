{"dta.poem.15996": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein F\u00fchrer kr\u00e4ftigt sich am Anblick seiner Treuen,", "tokens": ["Ein", "F\u00fch\u00b7rer", "kr\u00e4f\u00b7tigt", "sich", "am", "An\u00b7blick", "sei\u00b7ner", "Treu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie ihre Kr\u00e4fte sich an seinem Blick erneuen.", "tokens": ["Wie", "ih\u00b7re", "Kr\u00e4f\u00b7te", "sich", "an", "sei\u00b7nem", "Blick", "er\u00b7neu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie geben ihm Vertraun, und er gibt ihnen Muth,", "tokens": ["Sie", "ge\u00b7ben", "ihm", "Ver\u00b7traun", ",", "und", "er", "gibt", "ih\u00b7nen", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "KON", "PPER", "VVFIN", "PPER", "NN", "$,"], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sein Gut gibt er f\u00fcr sie, und sie f\u00fcr ihn ihr Blut.", "tokens": ["Sein", "Gut", "gibt", "er", "f\u00fcr", "sie", ",", "und", "sie", "f\u00fcr", "ihn", "ihr", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "PPER", "$,", "KON", "PPER", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Er f\u00fchlt in seinem Arm von tausenden die Macht,", "tokens": ["Er", "f\u00fchlt", "in", "sei\u00b7nem", "Arm", "von", "tau\u00b7sen\u00b7den", "die", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und tausend Sinn' hat er auf einen Sinn gebracht.", "tokens": ["Und", "tau\u00b7send", "Sinn'", "hat", "er", "auf", "ei\u00b7nen", "Sinn", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wo also Herr und Heer sich f\u00fchlet als ein Mann,", "tokens": ["Wo", "al\u00b7so", "Herr", "und", "Heer", "sich", "f\u00fch\u00b7let", "als", "ein", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "KON", "NN", "PRF", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Wunder ist es wenn der Wunder wirken kann.", "tokens": ["Kein", "Wun\u00b7der", "ist", "es", "wenn", "der", "Wun\u00b7der", "wir\u00b7ken", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "KOUS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Aus lockerm Staube wird ein Erdwall aufgesch\u00fcttet;", "tokens": ["Aus", "lo\u00b7ckerm", "Stau\u00b7be", "wird", "ein", "Erd\u00b7wall", "auf\u00b7ge\u00b7sch\u00fct\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sonst wehte weg ein Wind, was jetzt kein Sturm zerr\u00fcttet;", "tokens": ["Sonst", "weh\u00b7te", "weg", "ein", "Wind", ",", "was", "jetzt", "kein", "Sturm", "zer\u00b7r\u00fct\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "So fest und stark ist, was der Eintracht Kitt verk\u00fcttet.", "tokens": ["So", "fest", "und", "stark", "ist", ",", "was", "der", "Ein\u00b7tracht", "Kitt", "ver\u00b7k\u00fct\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "ADJD", "VAFIN", "$,", "PRELS", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}