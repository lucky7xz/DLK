{"textgrid.poem.48755": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "42. \u00dcber den Ort der Zusammenkunft", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft euch erschrecken nicht die jauchzenden Trombetten,", "tokens": ["La\u00dft", "euch", "er\u00b7schre\u00b7cken", "nicht", "die", "jauch\u00b7zen\u00b7den", "Trom\u00b7bet\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+--", "measure": "iambic.penta.invert"}, "line.2": {"text": "der Spiele vollen Lerm, der Flaggen hohen Flug,", "tokens": ["der", "Spie\u00b7le", "vol\u00b7len", "Lerm", ",", "der", "Flag\u00b7gen", "ho\u00b7hen", "Flug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "di\u00df ungew\u00f6hnte Schiff, das erst die ", "tokens": ["di\u00df", "un\u00b7ge\u00b7w\u00f6hn\u00b7te", "Schiff", ",", "das", "erst", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "$,", "PRELS", "ADV", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "der R\u00fcllen derben Knall, den Schlag der Falkeneten,", "tokens": ["der", "R\u00fcl\u00b7len", "der\u00b7ben", "Knall", ",", "den", "Schlag", "der", "Fal\u00b7ke\u00b7ne\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "den aufgefahrnen Dampf der blitzenden Musketen,", "tokens": ["den", "auf\u00b7ge\u00b7fahr\u00b7nen", "Dampf", "der", "blit\u00b7zen\u00b7den", "Mus\u00b7ke\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "ihr Nymphen dieses Orts, steigt sicher auf den Bug.", "tokens": ["ihr", "Nym\u00b7phen", "die\u00b7ses", "Orts", ",", "steigt", "si\u00b7cher", "auf", "den", "Bug", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDAT", "NN", "$,", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kommt her und seht uns zu, ihr habt es freien Fug.", "tokens": ["Kommt", "her", "und", "seht", "uns", "zu", ",", "ihr", "habt", "es", "frei\u00b7en", "Fug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir sind nicht Feind mit euch, wir kommen euch zu retten.", "tokens": ["Wir", "sind", "nicht", "Feind", "mit", "euch", ",", "wir", "kom\u00b7men", "euch", "zu", "ret\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "APPR", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Hier, da vor kurzer Zeit f\u00fcnfhundert ", "tokens": ["Hier", ",", "da", "vor", "kur\u00b7zer", "Zeit", "f\u00fcnf\u00b7hun\u00b7dert"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "APPR", "ADJA", "NN", "CARD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "durch der ", "tokens": ["durch", "der"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "wie viel Bericht k\u00f6mmt ein; hier, da der Tod itzt lebt,", "tokens": ["wie", "viel", "Be\u00b7richt", "k\u00f6mmt", "ein", ";", "hier", ",", "da", "der", "Tod", "itzt", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "PTKVZ", "$.", "ADV", "$,", "KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "die Ursach eurer Furcht, da werden mit versprechen,", "tokens": ["die", "Ur\u00b7sach", "eu\u00b7rer", "Furcht", ",", "da", "wer\u00b7den", "mit", "ver\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "zwei starke V\u00f6lker eins, die wilde Macht zu brechen,", "tokens": ["zwei", "star\u00b7ke", "V\u00f6l\u00b7ker", "eins", ",", "die", "wil\u00b7de", "Macht", "zu", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "PIS", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "f\u00fcr der di\u00df ganze Land in feigem Zittern bebt.", "tokens": ["f\u00fcr", "der", "di\u00df", "gan\u00b7ze", "Land", "in", "fei\u00b7gem", "Zit\u00b7tern", "bebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PDS", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}