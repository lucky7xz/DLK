{"dta.poem.832": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XiX.  Auf den Sontag de\u00df zu dem Tode gehen-  \n den Erl\u00f6sers/ oder  Quinquagesim\u00e6.  \n Luc. 18.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Olieb' ohn Ma\u00df! O gunst der nirgends wa\u00df zu gleichen/ ", "tokens": ["O\u00b7lieb'", "ohn", "Ma\u00df", "!", "O", "gunst", "der", "nir\u00b7gends", "wa\u00df", "zu", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$.", "NE", "NN", "ART", "ADV", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die GOtt in Todt: ins Creutz aus seinem Throne tr\u00e4gt!", "tokens": ["Die", "Gott", "in", "Todt", ":", "ins", "Creutz", "aus", "sei\u00b7nem", "Thro\u00b7ne", "tr\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$.", "APPRART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df sich der Lebens F\u00fcrst selbst in die schantze schlegt", "tokens": ["Da\u00df", "sich", "der", "Le\u00b7bens", "F\u00fcrst", "selbst", "in", "die", "schant\u00b7ze", "schlegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "NN", "ADV", "APPR", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verstehet kein verstand. Kein sinnen wirds erreichen/", "tokens": ["Ver\u00b7ste\u00b7het", "kein", "ver\u00b7stand", ".", "Kein", "sin\u00b7nen", "wirds", "er\u00b7rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "VVFIN", "$.", "PIAT", "NN", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ach/ sol das zartte Fleisch in schmach vnd geisselstreichen.", "tokens": ["Ach", "/", "sol", "das", "zart\u00b7te", "Fleisch", "in", "schmach", "vnd", "geis\u00b7sel\u00b7strei\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "VMFIN", "ART", "ADJA", "NN", "APPR", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In schwerer S\u00fcnden last die meine schuld aufflegt", "tokens": ["In", "schwe\u00b7rer", "S\u00fcn\u00b7den", "last", "die", "mei\u00b7ne", "schuld", "auf\u00b7flegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "PPOSAT", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In dem entbrandten grimm/ den Gottes fluch erreg\u2019t", "tokens": ["In", "dem", "ent\u00b7brand\u00b7ten", "grimm", "/", "den", "Got\u00b7tes", "fluch", "er\u00b7reg't"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJD", "$(", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd Mosis satzuug sterckt verschmachten vnd erbleichen?", "tokens": ["Vnd", "Mo\u00b7sis", "sat\u00b7zu\u00b7ug", "sterckt", "ver\u00b7schmach\u00b7ten", "vnd", "er\u00b7blei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ADJD", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie da\u00df ich doch mein HErr: so blind dein Leiden flih", "tokens": ["Wie", "da\u00df", "ich", "doch", "mein", "Herr", ":", "so", "blind", "dein", "Lei\u00b7den", "flih"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "$.", "ADV", "ADJD", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie da\u00df ich nicht mit dir eylfertig dahin zih?", "tokens": ["Wie", "da\u00df", "ich", "nicht", "mit", "dir", "eyl\u00b7fer\u00b7tig", "da\u00b7hin", "zih", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "PTKNEG", "APPR", "PPER", "ADJD", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo du durch angst vnd Creutz wirst in den Himmel gehen/", "tokens": ["Wo", "du", "durch", "angst", "vnd", "Creutz", "wirst", "in", "den", "Him\u00b7mel", "ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "KON", "NN", "VAFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Steh still vnd schaw mich an/ denn werd ich sehend seyn", "tokens": ["Steh", "still", "vnd", "schaw", "mich", "an", "/", "denn", "werd", "ich", "se\u00b7hend", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "PPER", "PTKVZ", "$(", "KON", "VAFIN", "PPER", "VVPP", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd deine Bruder trew\u2019 vnd Liebe recht verstehen.", "tokens": ["Vnd", "dei\u00b7ne", "Bru\u00b7der", "tre\u00b7w'", "vnd", "Lie\u00b7be", "recht", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "KON", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}