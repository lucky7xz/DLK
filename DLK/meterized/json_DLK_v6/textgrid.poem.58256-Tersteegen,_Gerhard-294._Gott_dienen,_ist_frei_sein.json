{"textgrid.poem.58256": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "294. Gott dienen, ist frei sein", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sein eigner Herr und Meister sein,", "tokens": ["Sein", "eig\u00b7ner", "Herr", "und", "Meis\u00b7ter", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach eignem Sinn und Willen wollen leben,", "tokens": ["Nach", "eig\u00b7nem", "Sinn", "und", "Wil\u00b7len", "wol\u00b7len", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Macht dich nicht frei, es bringt nur Angst und Pein,", "tokens": ["Macht", "dich", "nicht", "frei", ",", "es", "bringt", "nur", "Angst", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADJD", "$,", "PPER", "VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "So mu\u00df der edle Geist wie im Gef\u00e4ngnis schweben;", "tokens": ["So", "mu\u00df", "der", "ed\u00b7le", "Geist", "wie", "im", "Ge\u00b7f\u00e4ng\u00b7nis", "schwe\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "KOKOM", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer recht will frei sein hier auf Erden,", "tokens": ["Wer", "recht", "will", "frei", "sein", "hier", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "ADJD", "VAINF", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00df meines Gottes Sklave werden.", "tokens": ["Mu\u00df", "mei\u00b7nes", "Got\u00b7tes", "Skla\u00b7ve", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}