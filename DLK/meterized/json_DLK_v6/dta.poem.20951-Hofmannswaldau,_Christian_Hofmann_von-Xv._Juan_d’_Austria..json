{"dta.poem.20951": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Xv.  \n Juan d\u2019 Austria.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Des Vatern Tapfferkeit hab ich als Sohn geerbet/", "tokens": ["Des", "Va\u00b7tern", "Tapf\u00b7fer\u00b7keit", "hab", "ich", "als", "Sohn", "ge\u00b7er\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "KOUS", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und durch der T\u00fcrcken-Blut die wilde See gef\u00e4r-", "tokens": ["Und", "durch", "der", "T\u00fcr\u00b7cken\u00b7Blut", "die", "wil\u00b7de", "See", "ge\u00b7f\u00e4r"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "bet.", "tokens": ["bet", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Constantinopeln legt ich Trauer-Kleider an/", "tokens": ["Con\u00b7stan\u00b7ti\u00b7no\u00b7peln", "legt", "ich", "Trau\u00b7e\u00b7rKlei\u00b7der", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und zeigte/ da\u00df der Muth die Macht bezwingen", "tokens": ["Und", "zeig\u00b7te", "/", "da\u00df", "der", "Muth", "die", "Macht", "be\u00b7zwin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$(", "KOUS", "ART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "(kan.", "tokens": ["(", "kan", "."], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VMFIN", "$."], "meter": "-", "measure": "single.down"}}}}}