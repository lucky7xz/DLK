{"textgrid.poem.59114": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "333. La\u00df die Gl\u00e4ser unbemalet", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein eignes Denken und Gedicht", "tokens": ["Dein", "eig\u00b7nes", "Den\u00b7ken", "und", "Ge\u00b7dicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bedeckt das einfach reine Licht;", "tokens": ["Be\u00b7deckt", "das", "ein\u00b7fach", "rei\u00b7ne", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Werd wie ein Kind, kehr ein und schweige,", "tokens": ["Werd", "wie", "ein", "Kind", ",", "kehr", "ein", "und", "schwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "$,", "VVFIN", "PTKVZ", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Gott dir selbst die Wahrheit zeige!", "tokens": ["Da\u00df", "Gott", "dir", "selbst", "die", "Wahr\u00b7heit", "zei\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}