{"textgrid.poem.41804": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erinn'rung ruft in meiner Seele", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erinn'rung ruft in meiner Seele", "tokens": ["Er\u00b7inn'\u00b7rung", "ruft", "in", "mei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Bild herauf aus fr\u00fcher Zeit:", "tokens": ["Ein", "Bild", "her\u00b7auf", "aus", "fr\u00fc\u00b7her", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Herz war rein und ohne Fehle,", "tokens": ["Das", "Herz", "war", "rein", "und", "oh\u00b7ne", "Feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Schneegl\u00f6cklein im Fr\u00fchlingskleid.", "tokens": ["Ein", "Schnee\u00b7gl\u00f6c\u00b7klein", "im", "Fr\u00fch\u00b7lings\u00b7kleid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie das sich wiegt auf zartem Stengel,", "tokens": ["Wie", "das", "sich", "wiegt", "auf", "zar\u00b7tem", "Sten\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von jedem leisen Hauch ber\u00fchrt,", "tokens": ["Von", "je\u00b7dem", "lei\u00b7sen", "Hauch", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So bebt' ich, da der Kindheit Engel", "tokens": ["So", "bebt'", "ich", ",", "da", "der", "Kind\u00b7heit", "En\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zu dem der Jugend mich gef\u00fchrt.", "tokens": ["Zu", "dem", "der", "Ju\u00b7gend", "mich", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Am Osterfest im Feierkleide", "tokens": ["Am", "Os\u00b7ter\u00b7fest", "im", "Fei\u00b7er\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kniet ich am heiligen Altar,", "tokens": ["Kniet", "ich", "am", "hei\u00b7li\u00b7gen", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Engel schwebten mir zur Seite,", "tokens": ["Die", "En\u00b7gel", "schweb\u00b7ten", "mir", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Priester bot den Kelch mir dar:", "tokens": ["Der", "Pries\u00b7ter", "bot", "den", "Kelch", "mir", "dar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.6": {"text": "Den Spruch, den er als Segen bot \u2013", "tokens": ["Den", "Spruch", ",", "den", "er", "als", "Se\u00b7gen", "bot", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "KOUS", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}}, "stanza.3": {"line.1": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.2": {"text": "In meine Brust wie Blitzesstrahl,", "tokens": ["In", "mei\u00b7ne", "Brust", "wie", "Blit\u00b7zes\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Kampf des Lebens mir verk\u00fcndend", "tokens": ["Den", "Kampf", "des", "Le\u00b7bens", "mir", "ver\u00b7k\u00fcn\u00b7dend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Feindesm\u00e4chten ohne Zahl.", "tokens": ["Mit", "Fein\u00b7des\u00b7m\u00e4ch\u00b7ten", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der goldne Kelch mit seinem Blinken,", "tokens": ["Der", "gold\u00b7ne", "Kelch", "mit", "sei\u00b7nem", "Blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ward zum prophetischen Symbol \u2013,", "tokens": ["Ward", "zum", "pro\u00b7phe\u00b7ti\u00b7schen", "Sym\u00b7bol", "\u2013", ","], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vom Kelch des Leidens sollt' ich trinken", "tokens": ["Vom", "Kelch", "des", "Lei\u00b7dens", "sollt'", "ich", "trin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Noch oft und viel \u2013 ich ahnt' es wohl.", "tokens": ["Noch", "oft", "und", "viel", "\u2013", "ich", "ahnt'", "es", "wohl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "$(", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich ahnt' es wohl mit stillem Schauer \u2013", "tokens": ["Ich", "ahnt'", "es", "wohl", "mit", "stil\u00b7lem", "Schau\u00b7er", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir war, ich sei zum Kampf gefeit;", "tokens": ["Mir", "war", ",", "ich", "sei", "zum", "Kampf", "ge\u00b7feit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In meine Brust kam keine Trauer,", "tokens": ["In", "mei\u00b7ne", "Brust", "kam", "kei\u00b7ne", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Still ward mein Herz und gro\u00df und weit;", "tokens": ["Still", "ward", "mein", "Herz", "und", "gro\u00df", "und", "weit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es sehnte sich nach gro\u00dfen Stunden", "tokens": ["Es", "sehn\u00b7te", "sich", "nach", "gro\u00b7\u00dfen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von Kampfbegeisterung entbrannt,", "tokens": ["Von", "Kampf\u00b7be\u00b7geis\u00b7te\u00b7rung", "ent\u00b7brannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es bebte nicht vor tiefen Wunden", "tokens": ["Es", "beb\u00b7te", "nicht", "vor", "tie\u00b7fen", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Geschlagen von des Schicksals Hand.", "tokens": ["Ge\u00b7schla\u00b7gen", "von", "des", "Schick\u00b7sals", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich eilte vom Altar der Weihe", "tokens": ["Ich", "eil\u00b7te", "vom", "Al\u00b7tar", "der", "Wei\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hinaus in die erwachte Flur,", "tokens": ["Hin\u00b7aus", "in", "die", "er\u00b7wach\u00b7te", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Lerche schmetterte, die freie,", "tokens": ["Die", "Ler\u00b7che", "schmet\u00b7ter\u00b7te", ",", "die", "frei\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rings lachte hold des Lenzes Spur;", "tokens": ["Rings", "lach\u00b7te", "hold", "des", "Len\u00b7zes", "Spur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die ersten Veilchen sah ich bl\u00fchen \u2013", "tokens": ["Die", "ers\u00b7ten", "Veil\u00b7chen", "sah", "ich", "bl\u00fc\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Dornenstrauch dar\u00fcber stand,", "tokens": ["Ein", "Dor\u00b7nen\u00b7strauch", "da\u00b7r\u00fc\u00b7ber", "stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich pfl\u00fcckte sie mit stillem M\u00fchen \u2013", "tokens": ["Ich", "pfl\u00fcck\u00b7te", "sie", "mit", "stil\u00b7lem", "M\u00fc\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Blut entquoll der Kindeshand.", "tokens": ["Und", "Blut", "ent\u00b7quoll", "der", "Kin\u00b7des\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein neu' Symbol! ich kniete nieder", "tokens": ["Ein", "neu'", "Sym\u00b7bol", "!", "ich", "knie\u00b7te", "nie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und betete zu Gott empor:", "tokens": ["Und", "be\u00b7te\u00b7te", "zu", "Gott", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbo gieb mir Veilchen, gieb mir Lieder,", "tokens": ["\u00bb", "o", "gieb", "mir", "Veil\u00b7chen", ",", "gieb", "mir", "Lie\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VVIMP", "PPER", "NN", "$,", "VVIMP", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie frei sie singt der Lerchenchor", "tokens": ["Wie", "frei", "sie", "singt", "der", "Ler\u00b7chen\u00b7chor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vom Kelch der Leiden will ich trinken. \u2013", "tokens": ["Vom", "Kelch", "der", "Lei\u00b7den", "will", "ich", "trin\u00b7ken", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er ist der Kelch des Lebens auch!", "tokens": ["Er", "ist", "der", "Kelch", "des", "Le\u00b7bens", "auch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im Kampfe soll der Mut nicht sinken,", "tokens": ["Im", "Kamp\u00b7fe", "soll", "der", "Mut", "nicht", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Im Sturm weht der Begeistrung Hauch!\u00ab", "tokens": ["Im", "Sturm", "weht", "der", "Be\u00b7geis\u00b7trung", "Hauch", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbich m\u00f6chte Dich um Leiden flehen", "tokens": ["\u00bb", "ich", "m\u00f6ch\u00b7te", "Dich", "um", "Lei\u00b7den", "fle\u00b7hen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VMFIN", "PRF", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Statt sanfter Ruhe, die erschlafft,", "tokens": ["Statt", "sanf\u00b7ter", "Ru\u00b7he", ",", "die", "er\u00b7schlafft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich kann dem Schmerz ins Auge sehen,", "tokens": ["Ich", "kann", "dem", "Schmerz", "ins", "Au\u00b7ge", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn nur im Kampfe w\u00e4chst die Kraft\u00ab \u2013", "tokens": ["Denn", "nur", "im", "Kamp\u00b7fe", "w\u00e4chst", "die", "Kraft", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Himmel stand in Sonnenflammen,", "tokens": ["Der", "Him\u00b7mel", "stand", "in", "Son\u00b7nen\u00b7flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Westen gl\u00fchte Abendroth,", "tokens": ["Im", "Wes\u00b7ten", "gl\u00fch\u00b7te", "A\u00b7ben\u00b7droth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich schauerte in mir zusammen:", "tokens": ["Ich", "schau\u00b7er\u00b7te", "in", "mir", "zu\u00b7sam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}}, "stanza.8": {"line.1": {"text": "Ein tr\u00e4umend Kind von f\u00fcnfzehn Jahren,", "tokens": ["Ein", "tr\u00e4u\u00b7mend", "Kind", "von", "f\u00fcnf\u00b7zehn", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das solch Gebet mit Andacht sprach \u2013", "tokens": ["Das", "solch", "Ge\u00b7bet", "mit", "An\u00b7dacht", "sprach", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "War wohl ein Frevel solch Gebahren?", "tokens": ["War", "wohl", "ein", "Fre\u00b7vel", "solch", "Ge\u00b7bah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich sinne still der Frage nach.", "tokens": ["Ich", "sin\u00b7ne", "still", "der", "Fra\u00b7ge", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erh\u00f6rt ward des Gebetes Schauer,", "tokens": ["Er\u00b7h\u00f6rt", "ward", "des", "Ge\u00b7be\u00b7tes", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es kam das Leid und Kampf und Schmerz,", "tokens": ["Es", "kam", "das", "Leid", "und", "Kampf", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es kamen Tage tiefster Trauer,", "tokens": ["Es", "ka\u00b7men", "Ta\u00b7ge", "tiefs\u00b7ter", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und keine Ruhe fand das Herz!", "tokens": ["Und", "kei\u00b7ne", "Ru\u00b7he", "fand", "das", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wohl fliegen Engel auf und nieder", "tokens": ["Wohl", "flie\u00b7gen", "En\u00b7gel", "auf", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und standen ihm im Leide bei,", "tokens": ["Und", "stan\u00b7den", "ihm", "im", "Lei\u00b7de", "bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl fand es freie Lerchenlieder,", "tokens": ["Wohl", "fand", "es", "frei\u00b7e", "Ler\u00b7chen\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ward ihm zum Lied der Schmerzensschrei;", "tokens": ["Ward", "ihm", "zum", "Lied", "der", "Schmer\u00b7zens\u00b7schrei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohl bl\u00fchten Veilchen ihm im Lenze,", "tokens": ["Wohl", "bl\u00fch\u00b7ten", "Veil\u00b7chen", "ihm", "im", "Len\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Sommer Rosen wonnevoll \u2013", "tokens": ["Im", "Som\u00b7mer", "Ro\u00b7sen", "won\u00b7ne\u00b7voll", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch auf der Liebe sch\u00f6nste Kr\u00e4nze", "tokens": ["Doch", "auf", "der", "Lie\u00b7be", "sch\u00f6ns\u00b7te", "Kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des Auges Schmerzensthr\u00e4ne quoll.", "tokens": ["Des", "Au\u00b7ges", "Schmer\u00b7zenst\u00b7hr\u00e4\u00b7ne", "quoll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Du hast's erfleht, Du darfst nicht klagen,", "tokens": ["Du", "hast's", "er\u00b7fleht", ",", "Du", "darfst", "nicht", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "VVFIN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sei stark mein Herz im neuen Streit!", "tokens": ["Sei", "stark", "mein", "Herz", "im", "neu\u00b7en", "Streit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Ruhen gilt und kein Verzagen,", "tokens": ["Kein", "Ru\u00b7hen", "gilt", "und", "kein", "Ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist gest\u00e4hlt im Kampf und Leid.", "tokens": ["Du", "bist", "ge\u00b7st\u00e4hlt", "im", "Kampf", "und", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und kannst Du nicht mehr \u00fcberwinden", "tokens": ["Und", "kannst", "Du", "nicht", "mehr", "\u00fc\u00b7berw\u00b7in\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Schwerste, was das Schicksal bot,", "tokens": ["Das", "Schwers\u00b7te", ",", "was", "das", "Schick\u00b7sal", "bot", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann wird der letzte Kampf Dich finden:", "tokens": ["Dann", "wird", "der", "letz\u00b7te", "Kampf", "Dich", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du konntest \u00bbtreu sein bis zum Tod!\u00ab", "tokens": ["Du", "konn\u00b7test", "\u00bb", "treu", "sein", "bis", "zum", "Tod", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "$(", "ADJD", "VAINF", "APPR", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}