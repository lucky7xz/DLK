{"textgrid.poem.59034": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "253. Die kleinen Heiligen", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kannst du nicht, wie du w\u00fcnschest, rein sein,", "tokens": ["Kannst", "du", "nicht", ",", "wie", "du", "w\u00fcn\u00b7schest", ",", "rein", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADJD", "VAINF", "$,"], "meter": "+-+--++-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "So such die Heiligkeit im Kleinsein!", "tokens": ["So", "such", "die", "Hei\u00b7lig\u00b7keit", "im", "Klein\u00b7sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch s\u00fc\u00dfe Herzenskleinheit blo\u00df", "tokens": ["Durch", "s\u00fc\u00b7\u00dfe", "Her\u00b7zens\u00b7klein\u00b7heit", "blo\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wirst du bei Gott geliebt und gro\u00df.", "tokens": ["Wirst", "du", "bei", "Gott", "ge\u00b7liebt", "und", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}