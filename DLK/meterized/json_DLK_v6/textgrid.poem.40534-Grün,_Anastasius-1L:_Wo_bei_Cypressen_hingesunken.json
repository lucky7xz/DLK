{"textgrid.poem.40534": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo bei Cypressen hingesunken", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo bei Cypressen hingesunken", "tokens": ["Wo", "bei", "Cyp\u00b7res\u00b7sen", "hin\u00b7ge\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NE", "VVINF"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ich raste, schauend in den Schoo\u00df", "tokens": ["Ich", "ras\u00b7te", ",", "schau\u00b7end", "in", "den", "Schoo\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der ew'gen Roma, wehmuthtrunken", "tokens": ["Der", "ew'\u00b7gen", "Ro\u00b7ma", ",", "weh\u00b7muth\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Gl\u00f6cklein San Onofrio's;", "tokens": ["Vom", "Gl\u00f6c\u00b7klein", "San", "O\u00b7no\u00b7frio's", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hier sa\u00df einst Tasso. Der Cypressen", "tokens": ["Hier", "sa\u00df", "einst", "Tas\u00b7so", ".", "Der", "Cyp\u00b7res\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "NE", "$.", "ART", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Stand eine nur, sonst war's wie jetzt;", "tokens": ["Stand", "ei\u00b7ne", "nur", ",", "sonst", "wa\u00b7r's", "wie", "jetzt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADV", "$,", "ADV", "VAFIN", "KOKOM", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ob mancher Stein hinsank indessen,", "tokens": ["Ob", "man\u00b7cher", "Stein", "hin\u00b7sank", "in\u00b7des\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur Thau war's, der die\u00df Meer genetzt!", "tokens": ["Nur", "Thau", "wa\u00b7r's", ",", "der", "die\u00df", "Meer", "ge\u00b7netzt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "$,", "PRELS", "PDS", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wohl rauschte die Cypress' am H\u00fcgel", "tokens": ["Wohl", "rauschte", "die", "Cy\u00b7press'", "am", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihm die Cypress' im Herzen wach,", "tokens": ["Ihm", "die", "Cy\u00b7press'", "im", "Her\u00b7zen", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df, brechend seines Schweigens Siegel,", "tokens": ["Da\u00df", ",", "bre\u00b7chend", "sei\u00b7nes", "Schwei\u00b7gens", "Sie\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADJD", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der kranke Dichter zu sich sprach:", "tokens": ["Der", "kran\u00b7ke", "Dich\u00b7ter", "zu", "sich", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbo Menschenleben, Hauch im Winde,", "tokens": ["\u00bb", "o", "Men\u00b7schen\u00b7le\u00b7ben", ",", "Hauch", "im", "Win\u00b7de", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich \u00fcberdauert Stein und Thier!", "tokens": ["Dich", "\u00fc\u00b7berd\u00b7au\u00b7ert", "Stein", "und", "Thier", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fortlebt der Vater doch im Kinde,", "tokens": ["Fort\u00b7lebt", "der", "Va\u00b7ter", "doch", "im", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Lied, mein Kind, lebt' ich in dir!", "tokens": ["Mein", "Lied", ",", "mein", "Kind", ",", "lebt'", "ich", "in", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Komm, Rab' am Baum dort, dem zu Liebe", "tokens": ["Komm", ",", "Rab'", "am", "Baum", "dort", ",", "dem", "zu", "Lie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "APPRART", "NN", "ADV", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Enterbt ich um manch J\u00e4hrlein war,", "tokens": ["En\u00b7terbt", "ich", "um", "manch", "J\u00e4hr\u00b7lein", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich mein Lied dich plappern \u00fcbe,", "tokens": ["Da\u00df", "ich", "mein", "Lied", "dich", "plap\u00b7pern", "\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So t\u00f6nt's wohl noch ein hundert Jahr!", "tokens": ["So", "t\u00f6nt's", "wohl", "noch", "ein", "hun\u00b7dert", "Jahr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "CARD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dir, wei\u00dfer Zauberhirsch, durchsausend", "tokens": ["Dir", ",", "wei\u00b7\u00dfer", "Zau\u00b7ber\u00b7hirsch", ",", "durch\u00b7sau\u00b7send"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Apennin, schrieb' ich's mit Gold", "tokens": ["Den", "A\u00b7pen\u00b7nin", ",", "schrieb'", "ich's", "mit", "Gold"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NE", "$,", "VVFIN", "PIS", "APPR", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ins Halsband gern, da\u00df ein Jahrtausend", "tokens": ["Ins", "Hals\u00b7band", "gern", ",", "da\u00df", "ein", "Jahr\u00b7tau\u00b7send"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit dir es noch die Welt durchrollt!", "tokens": ["Mit", "dir", "es", "noch", "die", "Welt", "durch\u00b7rollt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dir, Stein am Wege, wollt ich's schlagen", "tokens": ["Dir", ",", "Stein", "am", "We\u00b7ge", ",", "wollt", "ich's", "schla\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "APPRART", "NN", "$,", "VMFIN", "PIS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In deine kalte Menschenbrust,", "tokens": ["In", "dei\u00b7ne", "kal\u00b7te", "Men\u00b7schen\u00b7brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df du es tausend Jahre tragen", "tokens": ["Da\u00df", "du", "es", "tau\u00b7send", "Jah\u00b7re", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "CARD", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und aber tausend Jahre mu\u00dft!", "tokens": ["Und", "a\u00b7ber", "tau\u00b7send", "Jah\u00b7re", "mu\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "CARD", "NN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Was ficht mich an? Wo sind die Thaten,", "tokens": ["Was", "ficht", "mich", "an", "?", "Wo", "sind", "die", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$.", "PWAV", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Da\u00df ich zu ragen bin gewillt,", "tokens": ["Da\u00df", "ich", "zu", "ra\u00b7gen", "bin", "ge\u00b7willt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Baume gleich, hoch \u00fcber Saaten,", "tokens": ["Dem", "Bau\u00b7me", "gleich", ",", "hoch", "\u00fc\u00b7ber", "Saa\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Thurm, hoch \u00fcberm Stadtgefild'?", "tokens": ["Dem", "Thurm", ",", "hoch", "\u00fc\u00b7berm", "Stadt\u00b7ge\u00b7fild'", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Dem Baum, wie mir, gibt Recht zu ragen", "tokens": ["Dem", "Baum", ",", "wie", "mir", ",", "gibt", "Recht", "zu", "ra\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "$,", "VVFIN", "NN", "PTKZU", "VVINF"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Furcht, Vogelsang und Bl\u00fcthenscherz!", "tokens": ["Furcht", ",", "Vo\u00b7gel\u00b7sang", "und", "Bl\u00fc\u00b7then\u00b7scherz", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Thurm, wie mir, gibt Recht zu ragen", "tokens": ["Dem", "Thurm", ",", "wie", "mir", ",", "gibt", "Recht", "zu", "ra\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "$,", "VVFIN", "NN", "PTKZU", "VVINF"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sein t\u00f6nend heilig Glockenherz!", "tokens": ["Sein", "t\u00f6\u00b7nend", "hei\u00b7lig", "Glo\u00b7cken\u00b7herz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Doch soll mein Lied hier stehn in Steinen,", "tokens": ["Doch", "soll", "mein", "Lied", "hier", "stehn", "in", "Stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Lieder nicht, nein, Ruhm und That", "tokens": ["Wo", "Lie\u00b7der", "nicht", ",", "nein", ",", "Ruhm", "und", "That"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "PTKNEG", "$,", "PTKANT", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und der Jahrtausend' Jauchzen, Weinen", "tokens": ["Und", "der", "Jahr\u00b7tau\u00b7send'", "Jauch\u00b7zen", ",", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Tr\u00fcmmern ruht, versteinte Saat?", "tokens": ["In", "Tr\u00fcm\u00b7mern", "ruht", ",", "ver\u00b7stein\u00b7te", "Saat", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Wo der Campagna W\u00fcst' ich sehe", "tokens": ["Wo", "der", "Cam\u00b7pag\u00b7na", "W\u00fcst'", "ich", "se\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und mich's kein Wunder mehr bed\u00fcnkt,", "tokens": ["Und", "mich's", "kein", "Wun\u00b7der", "mehr", "be\u00b7d\u00fcnkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df beim Anschaun von solchem Wehe", "tokens": ["Da\u00df", "beim", "An\u00b7schaun", "von", "sol\u00b7chem", "We\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "APPR", "PIAT", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Dem Pfl\u00fcgerarm der Pflug entsinkt?", "tokens": ["Dem", "Pfl\u00fc\u00b7ger\u00b7arm", "der", "Pflug", "ent\u00b7sinkt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Wo du selbst brachst, in Staub zerfallen,", "tokens": ["Wo", "du", "selbst", "brachst", ",", "in", "Staub", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Marmorgewordner Gott, entzwei!", "tokens": ["Mar\u00b7mor\u00b7ge\u00b7word\u00b7ner", "Gott", ",", "ent\u00b7zwei", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wo aus des Forums Tr\u00fcmmern allen", "tokens": ["Wo", "aus", "des", "Fo\u00b7rums", "Tr\u00fcm\u00b7mern", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch ragen Tempels\u00e4ulen drei;", "tokens": ["Noch", "ra\u00b7gen", "Tem\u00b7pel\u00b7s\u00e4u\u00b7len", "drei", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Furchtbar, drei Fingern gleich, erhoben", "tokens": ["Furcht\u00b7bar", ",", "drei", "Fin\u00b7gern", "gleich", ",", "er\u00b7ho\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "CARD", "NN", "ADV", "$,", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Schwur einst der Best\u00e4ndigkeit,", "tokens": ["Zum", "Schwur", "einst", "der", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch die verdorrt noch ragen oben,", "tokens": ["Doch", "die", "ver\u00b7dorrt", "noch", "ra\u00b7gen", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil sie beschworen falschen Eid!", "tokens": ["Weil", "sie", "be\u00b7schwo\u00b7ren", "fal\u00b7schen", "Eid", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Wo, zwar vom Siegesglanz umflossen,", "tokens": ["Wo", ",", "zwar", "vo