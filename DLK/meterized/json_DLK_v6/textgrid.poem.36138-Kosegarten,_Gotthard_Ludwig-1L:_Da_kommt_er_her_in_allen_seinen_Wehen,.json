{"textgrid.poem.36138": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da kommt er her in allen seinen Wehen,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.57", "da:0.42"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da kommt er her in allen seinen Wehen,", "tokens": ["Da", "kommt", "er", "her", "in", "al\u00b7len", "sei\u00b7nen", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der bange finst're Klagetag.", "tokens": ["Der", "ban\u00b7ge", "finst'\u00b7re", "Kla\u00b7ge\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schon lange sah'n wir ihn \u2013 ein fernes Wetter \u2013 stehen,", "tokens": ["Schon", "lan\u00b7ge", "sah'n", "wir", "ihn", "\u2013", "ein", "fer\u00b7nes", "Wet\u00b7ter", "\u2013", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "$(", "ART", "ADJA", "NN", "$(", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ahnten Blitz und Schlag.", "tokens": ["Und", "ahn\u00b7ten", "Blitz", "und", "Schlag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und str\u00f6mten Seufzer, da\u00df sein Falkenfl\u00fcgel", "tokens": ["Und", "str\u00f6m\u00b7ten", "Seuf\u00b7zer", ",", "da\u00df", "sein", "Fal\u00b7ken\u00b7fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gehemmt, und seiner Faust das Schwert", "tokens": ["Ge\u00b7hemmt", ",", "und", "sei\u00b7ner", "Faust", "das", "Schwert"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entrungen w\u00fcrd'! \u2013 Umsonst! Schon schw\u00e4rzet Wald und H\u00fcgel", "tokens": ["Ent\u00b7run\u00b7gen", "w\u00fcrd'", "!", "\u2013", "Um\u00b7sonst", "!", "Schon", "schw\u00e4r\u00b7zet", "Wald", "und", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$.", "$(", "NN", "$.", "ADV", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Wolke, d'rinn er f\u00e4hrt.", "tokens": ["Die", "Wol\u00b7ke", ",", "d'\u00b7rinn", "er", "f\u00e4hrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Sein Morgenroth ist tr\u00fcb' und bleich gestaltet,", "tokens": ["Sein", "Mor\u00b7gen\u00b7roth", "ist", "tr\u00fcb'", "und", "bleich", "ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und seines Odems Frost verdirbt \u2013", "tokens": ["Und", "sei\u00b7nes", "O\u00b7dems", "Frost", "ver\u00b7dirbt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Fr\u00fchlings Erstgeburt, das Veilchen, halb entfaltet,", "tokens": ["Des", "Fr\u00fch\u00b7lings", "Erst\u00b7ge\u00b7burt", ",", "das", "Veil\u00b7chen", ",", "halb", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "F\u00fchlt seinen Hauch und stirbt.", "tokens": ["F\u00fchlt", "sei\u00b7nen", "Hauch", "und", "stirbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und gr\u00e4bertr\u00fcb' \u2013 zween Sterne, von Gewittern", "tokens": ["Und", "gr\u00e4\u00b7ber\u00b7tr\u00fcb'", "\u2013", "zween", "Ster\u00b7ne", ",", "von", "Ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$(", "VVFIN", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Nebeln rund umdunkelt \u2013 stehn", "tokens": ["Und", "Ne\u00b7beln", "rund", "um\u00b7dun\u00b7kelt", "\u2013", "stehn"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "NN", "ADJD", "VVPP", "$(", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die J\u00fcnglinge, die uns sein Kommen raubt, und zittern", "tokens": ["Die", "J\u00fcng\u00b7lin\u00b7ge", ",", "die", "uns", "sein", "Kom\u00b7men", "raubt", ",", "und", "zit\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "KON", "VVFIN"], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Vor seiner Fl\u00fcgel Wehn \u2013 \u2013", "tokens": ["Vor", "sei\u00b7ner", "Fl\u00fc\u00b7gel", "Wehn", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was stehet ihr, ihr J\u00fcnglinge, und zittert,", "tokens": ["Was", "ste\u00b7het", "ihr", ",", "ihr", "J\u00fcng\u00b7lin\u00b7ge", ",", "und", "zit\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df ihr uns lassen sollt? \u2013 Verla\u00dft", "tokens": ["Da\u00df", "ihr", "uns", "las\u00b7sen", "sollt", "?", "\u2013", "Ver\u00b7la\u00dft"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$.", "$(", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns nicht. Der Morgen weht so kalt. Die Eiche splittert,", "tokens": ["Uns", "nicht", ".", "Der", "Mor\u00b7gen", "weht", "so", "kalt", ".", "Die", "Ei\u00b7che", "split\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$.", "ART", "NN", "VVFIN", "ADV", "ADJD", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von wildem Sturm gefa\u00dft.", "tokens": ["Von", "wil\u00b7dem", "Sturm", "ge\u00b7fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Verla\u00dft uns nicht. Gedenkt der Maientage,", "tokens": ["Ver\u00b7la\u00dft", "uns", "nicht", ".", "Ge\u00b7denkt", "der", "Mai\u00b7en\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$.", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ihr mit uns so froh geno\u00dft.", "tokens": ["Die", "ihr", "mit", "uns", "so", "froh", "ge\u00b7no\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie fliehn. Ihr laut Gejauchz verwandelt sich in Klage,", "tokens": ["Sie", "fliehn", ".", "Ihr", "laut", "Ge\u00b7jauchz", "ver\u00b7wan\u00b7delt", "sich", "in", "Kla\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "PPER", "APPR", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Jugendgluth in Frost.", "tokens": ["Und", "Ju\u00b7gend\u00b7gluth", "in", "Frost", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Gedenkt der Freuden, die im Jubelkreise", "tokens": ["Ge\u00b7denkt", "der", "Freu\u00b7den", ",", "die", "im", "Ju\u00b7bel\u00b7krei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr \u00f6fters uns entgegen sangt,", "tokens": ["Ihr", "\u00f6f\u00b7ters", "uns", "ent\u00b7ge\u00b7gen", "sangt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPER", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ihr, entbrannten Aug's, nach echter deutscher Weise,", "tokens": ["Wo", "ihr", ",", "ent\u00b7brann\u00b7ten", "Aug's", ",", "nach", "ech\u00b7ter", "deut\u00b7scher", "Wei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "VVFIN", "NE", "$,", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das volle Kelchglas schwangt.", "tokens": ["Das", "vol\u00b7le", "Kelchg\u00b7las", "schwangt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Gedenk', o goldgelockter Freund, der Wonne", "tokens": ["Ge\u00b7denk'", ",", "o", "gold\u00b7ge\u00b7lock\u00b7ter", "Freund", ",", "der", "Won\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "FM", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In deines M\u00e4dchens Minneblick \u2013", "tokens": ["In", "dei\u00b7nes", "M\u00e4d\u00b7chens", "Min\u00b7ne\u00b7blick", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Maienfr\u00fchroth sanft, wie Glanz der Sommersonne.", "tokens": ["Wie", "Mai\u00b7en\u00b7fr\u00fch\u00b7roth", "sanft", ",", "wie", "Glanz", "der", "Som\u00b7mer\u00b7son\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "$,", "PWAV", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gebietend strahlt ihr Blick.", "tokens": ["Ge\u00b7bie\u00b7tend", "strahlt", "ihr", "Blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Gedenk', o du, de\u00df Geist mit Flammenblicke", "tokens": ["Ge\u00b7denk'", ",", "o", "du", ",", "de\u00df", "Geist", "mit", "Flam\u00b7men\u00b7bli\u00b7cke"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "FM", "PPER", "$,", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Wahrheit Heiligthum durchflog,", "tokens": ["Der", "Wahr\u00b7heit", "Hei\u00b7lig\u00b7thum", "durch\u00b7flog", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sch\u00e4tze, die du grubst, der Blumen, die am Ricke", "tokens": ["Der", "Sch\u00e4t\u00b7ze", ",", "die", "du", "grubst", ",", "der", "Blu\u00b7men", ",", "die", "am", "Ri\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein Flei\u00df herauf erzog.", "tokens": ["Dein", "Flei\u00df", "her\u00b7auf", "er\u00b7zog", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Noch braus't, wie Bergstrom, der das Feld beschwemmet,", "tokens": ["Noch", "braus't", ",", "wie", "Berg\u00b7strom", ",", "der", "das", "Feld", "be\u00b7schwem\u00b7met", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So wild, so frei eu'r Herz dahin.", "tokens": ["So", "wild", ",", "so", "frei", "eu'r", "Herz", "da\u00b7hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "PPOSAT", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da, wo ihr hingeht, schnaubt die Sklaverey, und hemmet", "tokens": ["Da", ",", "wo", "ihr", "hin\u00b7geht", ",", "schnaubt", "die", "Skla\u00b7ve\u00b7rey", ",", "und", "hem\u00b7met"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des J\u00fcnglings stolzen Sinn.", "tokens": ["Des", "J\u00fcng\u00b7lings", "stol\u00b7zen", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Verla\u00dft uns nicht! \u2013 Noch steht ihr bleich und tr\u00fcbe?", "tokens": ["Ver\u00b7la\u00dft", "uns", "nicht", "!", "\u2013", "Noch", "steht", "ihr", "bleich", "und", "tr\u00fc\u00b7be", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$.", "$(", "ADV", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und klagt des Schicksals Steifsinn an,", "tokens": ["Und", "klagt", "des", "Schick\u00b7sals", "Steif\u00b7sinn", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Klagt, da\u00df sein Felsenwort euch von den Freunden triebe? \u2013", "tokens": ["Klagt", ",", "da\u00df", "sein", "Fel\u00b7sen\u00b7wort", "euch", "von", "den", "Freun\u00b7den", "trie\u00b7be", "?", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So gehet, gehet dann!", "tokens": ["So", "ge\u00b7het", ",", "ge\u00b7het", "dann", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Der sey nicht edel, sey nicht werth des Namens,", "tokens": ["Der", "sey", "nicht", "e\u00b7del", ",", "sey", "nicht", "werth", "des", "Na\u00b7mens", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADJD", "$,", "VAFIN", "PTKNEG", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Den eure Freundschaft ihm geschenkt,", "tokens": ["Den", "eu\u00b7re", "Freund\u00b7schaft", "ihm", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der nicht mit Seelendrang und W\u00e4rme eures Namens,", "tokens": ["Der", "nicht", "mit", "See\u00b7len\u00b7drang", "und", "W\u00e4r\u00b7me", "eu\u00b7res", "Na\u00b7mens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "NN", "KON", "NN", "PPOSAT", "NN", "$,"], "me