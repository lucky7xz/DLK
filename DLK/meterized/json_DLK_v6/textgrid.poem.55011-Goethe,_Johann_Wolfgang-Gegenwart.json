{"textgrid.poem.55011": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Gegenwart", "genre": "verse", "period": "N.A.", "pub_year": 1812, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles k\u00fcndet dich an!", "tokens": ["Al\u00b7les", "k\u00fcn\u00b7det", "dich", "an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Erscheinet die herrliche Sonne,", "tokens": ["Er\u00b7schei\u00b7net", "die", "herr\u00b7li\u00b7che", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Folgst du, so hoff ich es, bald.", "tokens": ["Folgst", "du", ",", "so", "hoff", "ich", "es", ",", "bald", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "PPER", "$,", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Trittst du im Garten hervor,", "tokens": ["Trittst", "du", "im", "Gar\u00b7ten", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "++-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "So bist du die Rose der Rosen,", "tokens": ["So", "bist", "du", "die", "Ro\u00b7se", "der", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Lilie der Lilien zugleich.", "tokens": ["Li\u00b7lie", "der", "Li\u00b7li\u00b7en", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn du im Tanze dich regst,", "tokens": ["Wenn", "du", "im", "Tan\u00b7ze", "dich", "regst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "So regen sich alle Gestirne", "tokens": ["So", "re\u00b7gen", "sich", "al\u00b7le", "Ge\u00b7stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "PRF", "PIAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Mit dir und um dich umher.", "tokens": ["Mit", "dir", "und", "um", "dich", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nacht! und so w\u00e4r es denn Nacht!", "tokens": ["Nacht", "!", "und", "so", "w\u00e4r", "es", "denn", "Nacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "ADV", "VAFIN", "PPER", "ADV", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Nun \u00fcberscheinst du des Mondes", "tokens": ["Nun", "\u00fc\u00b7bersc\u00b7heinst", "du", "des", "Mon\u00b7des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Lieblichen, ladenden Glanz.", "tokens": ["Lieb\u00b7li\u00b7chen", ",", "la\u00b7den\u00b7den", "Glanz", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Ladend und lieblich bist du,", "tokens": ["La\u00b7dend", "und", "lieb\u00b7lich", "bist", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "PPER", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Und Blumen, Mond und Gestirne", "tokens": ["Und", "Blu\u00b7men", ",", "Mond", "und", "Ge\u00b7stir\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Huldigen, Sonne, nur dir.", "tokens": ["Hul\u00b7di\u00b7gen", ",", "Son\u00b7ne", ",", "nur", "dir", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADV", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Sonne! so sei du auch mir", "tokens": ["Son\u00b7ne", "!", "so", "sei", "du", "auch", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VAFIN", "PPER", "ADV", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Sch\u00f6pferin herrlicher Tage;", "tokens": ["Die", "Sch\u00f6p\u00b7fe\u00b7rin", "herr\u00b7li\u00b7cher", "Ta\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Leben und Ewigkeit ist's.", "tokens": ["Le\u00b7ben", "und", "E\u00b7wig\u00b7keit", "ist'", "s."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["NN", "KON", "NN", "VAFIN", "NE"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}