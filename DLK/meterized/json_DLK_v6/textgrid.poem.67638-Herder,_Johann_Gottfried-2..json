{"textgrid.poem.67638": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bblieber Tod, was soll das Leben?", "tokens": ["\u00bb", "lie\u00b7ber", "Tod", ",", "was", "soll", "das", "Le\u00b7ben", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "$,", "PWS", "VMFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist doch nur ein B\u00fcrdetragen!", "tokens": ["Ist", "doch", "nur", "ein", "B\u00fcr\u00b7de\u00b7tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist doch nur ein \u00e4ngstlich Streben!", "tokens": ["Ist", "doch", "nur", "ein", "\u00e4ngst\u00b7lich", "Stre\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieber, h\u00f6rst Du nicht mein Klagen?", "tokens": ["Lie\u00b7ber", ",", "h\u00f6rst", "Du", "nicht", "mein", "Kla\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Komm, o Tod, und nimm mich hin!\u00ab", "tokens": ["Komm", ",", "o", "Tod", ",", "und", "nimm", "mich", "hin", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "FM", "NN", "$,", "KON", "VVIMP", "PPER", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Es kam der Tod und stellete sich hin;", "tokens": ["Es", "kam", "der", "Tod", "und", "stel\u00b7le\u00b7te", "sich", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Greis, erschrocken, \u00e4nderte", "tokens": ["Der", "Greis", ",", "er\u00b7schro\u00b7cken", ",", "\u00e4n\u00b7der\u00b7te"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Worte Sinn:", "tokens": ["Der", "Wor\u00b7te", "Sinn", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "\u00bbach, so h\u00f6rtest Du mein Klagen?", "tokens": ["\u00bb", "ach", ",", "so", "h\u00f6r\u00b7test", "Du", "mein", "Kla\u00b7gen", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sieh, die B\u00fcrde mu\u00df ich tragen;", "tokens": ["Sieh", ",", "die", "B\u00fcr\u00b7de", "mu\u00df", "ich", "tra\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Lieber Tod, drum hilf mir heben!", "tokens": ["Lie\u00b7ber", "Tod", ",", "drum", "hilf", "mir", "he\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PAV", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Hilf mir! s\u00fc\u00df ist doch das Leben!", "tokens": ["Hilf", "mir", "!", "s\u00fc\u00df", "ist", "doch", "das", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$.", "ADJD", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Nimmst mich fr\u00fch genug doch hin!\u00ab", "tokens": ["Nimmst", "mich", "fr\u00fch", "ge\u00b7nug", "doch", "hin", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "ADV", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Es half ihm auf der Tod und ging", "tokens": ["Es", "half", "ihm", "auf", "der", "Tod", "und", "ging"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Still seines Weges hin.", "tokens": ["Still", "sei\u00b7nes", "We\u00b7ges", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und rufet man nur so den Tod?", "tokens": ["Und", "ru\u00b7fet", "man", "nur", "so", "den", "Tod", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Ruft man nicht t\u00e4glich also Gott?", "tokens": ["Ruft", "man", "nicht", "t\u00e4g\u00b7lich", "al\u00b7so", "Gott", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "ADJD", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Itzt will man dies, itzt das im lieben Lebenslauf,", "tokens": ["Itzt", "will", "man", "dies", ",", "itzt", "das", "im", "lie\u00b7ben", "Le\u00b7bens\u00b7lauf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PDS", "$,", "ADV", "ART", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Er hilft immer auf.", "tokens": ["Und", "Er", "hilft", "im\u00b7mer", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}