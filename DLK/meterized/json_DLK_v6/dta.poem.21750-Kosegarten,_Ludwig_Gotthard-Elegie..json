{"dta.poem.21750": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Elegie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Krank f&#252;r Liebe zu seyn, getroffen vom Pfeil des", "tokens": ["Krank", "f", "&#252;", "r", "Lie\u00b7be", "zu", "seyn", ",", "ge\u00b7trof\u00b7fen", "vom", "Pfeil", "des"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$(", "ADJA", "NN", "PTKZU", "VAINF", "$,", "VVPP", "APPRART", "NN", "ART"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Verlangens", "tokens": ["Ver\u00b7lan\u00b7gens"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Speis' und Trank zu verschm\u00e4hn, Menschen", "tokens": ["Speis'", "und", "Trank", "zu", "ver\u00b7schm\u00e4hn", ",", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "KON", "NN", "PTKZU", "VVINF", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und Freuden zu fliehn \u2014", "tokens": ["und", "Freu\u00b7den", "zu", "fliehn"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKZU", "VVINF", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Thorheit d\u00e4uchte mich dies. Ich spottete strenge", "tokens": ["Thor\u00b7heit", "d\u00e4uch\u00b7te", "mich", "dies", ".", "Ich", "spot\u00b7te\u00b7te", "stren\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PDS", "$.", "PPER", "VVFIN", "ADJA"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "des Thoren;", "tokens": ["des", "Tho\u00b7ren", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Aber ich b\u00fcsse verdient f\u00fcr den unheiligen Spott.", "tokens": ["A\u00b7ber", "ich", "b\u00fcs\u00b7se", "ver\u00b7dient", "f\u00fcr", "den", "un\u00b7hei\u00b7li\u00b7gen", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.8": {"text": "Krank bin ich, wie keiner gewesen, vor sehnender", "tokens": ["Krank", "bin", "ich", ",", "wie", "kei\u00b7ner", "ge\u00b7we\u00b7sen", ",", "vor", "seh\u00b7nen\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$,", "PWAV", "PIS", "VAPP", "$,", "APPR", "NN"], "meter": "+-+-+--+--+--", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Liebe.", "tokens": ["Lie\u00b7be", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Schier aus den R\u00f6hren das Mark zehret die", "tokens": ["Schier", "aus", "den", "R\u00f6h\u00b7ren", "das", "Mark", "zeh\u00b7ret", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "ART"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.11": {"text": "sengende Glut.", "tokens": ["sen\u00b7gen\u00b7de", "Glut", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "---+", "measure": "unknown.measure.single"}, "line.12": {"text": "Nicht die Gaben der Ceres verm\u00f6gen zu st\u00e4rken", "tokens": ["Nicht", "die", "Ga\u00b7ben", "der", "Ce\u00b7res", "ver\u00b7m\u00f6\u00b7gen", "zu", "st\u00e4r\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "ART", "NE", "VVFIN", "PTKZU", "VVINF"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "den Matten,", "tokens": ["den", "Mat\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Jacchos erlesenster Most l\u00f6scht nicht den dur-", "tokens": ["Jac\u00b7chos", "er\u00b7le\u00b7sens\u00b7ter", "Most", "l\u00f6scht", "nicht", "den", "dur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "VVFIN", "PTKNEG", "ART", "TRUNC"], "meter": "+---+-+-+-+", "measure": "dactylic.init"}, "line.15": {"text": "Denn es fehlt mir der Einen belebende N\u00e4he, sie", "tokens": ["Denn", "es", "fehlt", "mir", "der", "Ei\u00b7nen", "be\u00b7le\u00b7ben\u00b7de", "N\u00e4\u00b7he", ",", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "ART", "ADJA", "NN", "$,", "PPER"], "meter": "--+--+---+-+-+", "measure": "anapaest.di.plus"}, "line.16": {"text": "fehlt mir,", "tokens": ["fehlt", "mir", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Deren ambrosischem Mund heilendes Labsal", "tokens": ["De\u00b7ren", "am\u00b7bro\u00b7si\u00b7schem", "Mund", "hei\u00b7len\u00b7des", "Lab\u00b7sal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "ADJA", "NN"], "meter": "+--+--++--+-", "measure": "dactylic.di.plus"}, "line.18": {"text": "entquillt.", "tokens": ["ent\u00b7quillt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Siebenmal thaute die Fr\u00fch', und siebenmal wehte", "tokens": ["Sie\u00b7ben\u00b7mal", "thau\u00b7te", "die", "Fr\u00fch'", ",", "und", "sie\u00b7ben\u00b7mal", "weh\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "ADV", "VVFIN"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.20": {"text": "die D\u00e4mmrung,", "tokens": ["die", "D\u00e4mm\u00b7rung", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Seit du, Geliebte, mir fehlst, seit ich ver-", "tokens": ["Seit", "du", ",", "Ge\u00b7lieb\u00b7te", ",", "mir", "fehlst", ",", "seit", "ich", "ver"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "NN", "$,", "PPER", "VVFIN", "$,", "KOUS", "PPER", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Ja ich verschmachte nach dir. Nicht l\u00e4nger zu mis-", "tokens": ["Ja", "ich", "ver\u00b7schmach\u00b7te", "nach", "dir", ".", "Nicht", "l\u00e4n\u00b7ger", "zu", "mis"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VVFIN", "APPR", "PPER", "$.", "PTKNEG", "ADJD", "APPR", "TRUNC"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Deinen erheiternden Blick, deinen erquicken-", "tokens": ["Dei\u00b7nen", "er\u00b7hei\u00b7tern\u00b7den", "Blick", ",", "dei\u00b7nen", "er\u00b7qui\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "TRUNC"], "meter": "+--+--++--+-", "measure": "dactylic.di.plus"}, "line.24": {"text": "Siehe die Aue draussen. Auch ihr ist die freund-", "tokens": ["Sie\u00b7he", "die", "Au\u00b7e", "draus\u00b7sen", ".", "Auch", "ihr", "ist", "die", "freun\u00b7d"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$.", "ADV", "PPER", "VAFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Untergegangen; schon l\u00e4ngst birgt sie ein nei-", "tokens": ["Un\u00b7ter\u00b7ge\u00b7gan\u00b7gen", ";", "schon", "l\u00e4ngst", "birgt", "sie", "ein", "nei"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "ADV", "VVFIN", "PPER", "ART", "TRUNC"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.26": {"text": "Glanzlos liegt nun die Flur und traurend der An-", "tokens": ["Glanz\u00b7los", "liegt", "nun", "die", "Flur", "und", "trau\u00b7rend", "der", "An"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "KON", "ADJD", "ART", "TRUNC"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.27": {"text": "Seit ich dein sonnig Gesicht, freundliche Ida,", "tokens": ["Seit", "ich", "dein", "son\u00b7nig", "Ge\u00b7sicht", ",", "freund\u00b7li\u00b7che", "I\u00b7da", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "NN", "$,", "ADJA", "NE", "$,"], "meter": "-+-+--++--+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "nicht sah.", "tokens": ["nicht", "sah", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.29": {"text": "O so strahle denn wieder hervor aus dem h\u00fcllen-", "tokens": ["O", "so", "strah\u00b7le", "denn", "wie\u00b7der", "her\u00b7vor", "aus", "dem", "h\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "ADV", "ADV", "PTKVZ", "APPR", "ART", "TRUNC"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.30": {"text": "Morgenr\u00f6thliches Licht, kl\u00e4re die Seele mir auf.", "tokens": ["Mor\u00b7gen\u00b7r\u00f6th\u00b7li\u00b7ches", "Licht", ",", "kl\u00e4\u00b7re", "die", "See\u00b7le", "mir", "auf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Tritt hervor, o siegende Sonn', in schimmernder", "tokens": ["Tritt", "her\u00b7vor", ",", "o", "sie\u00b7gen\u00b7de", "Sonn'", ",", "in", "schim\u00b7mern\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "FM", "ADJA", "NN", "$,", "APPR", "ADJA"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.32": {"text": "Sch\u00f6nheit.", "tokens": ["Sch\u00f6n\u00b7heit", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.33": {"text": "Bringe mir Klarheit und Glanz, Kraft und Ge-", "tokens": ["Brin\u00b7ge", "mir", "Klar\u00b7heit", "und", "Glanz", ",", "Kraft", "und", "Ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$,", "NN", "KON", "TRUNC"], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.34": {"text": "Die du zu lange verzogst, erscheine, Geliebte, er-", "tokens": ["Die", "du", "zu", "lan\u00b7ge", "ver\u00b7zogst", ",", "er\u00b7schei\u00b7ne", ",", "Ge\u00b7lieb\u00b7te", ",", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ART", "PPER", "APPR", "ADV", "VVFIN", "$,", "VVFIN", "$,", "NN", "$,", "TRUNC"], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "Stille das schlagende Herz, letze den lechzen-", "tokens": ["Stil\u00b7le", "das", "schla\u00b7gen\u00b7de", "Herz", ",", "let\u00b7ze", "den", "lech\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "VVFIN", "ART", "TRUNC"], "meter": "+--+--++--+-", "measure": "dactylic.di.plus"}, "line.36": {"text": "Dein beraubt verschmachtet das Herz, wie die wel-", "tokens": ["Dein", "be\u00b7raubt", "ver\u00b7schmach\u00b7tet", "das", "Herz", ",", "wie", "die", "wel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "VVPP", "VVFIN", "ART", "NN", "$,", "PWAV", "ART", "TRUNC"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.37": {"text": "In der Schw\u00fcle des Tags schmachtet nach", "tokens": ["In", "der", "Schw\u00fc\u00b7le", "des", "Tags", "schmach\u00b7tet", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "APPR"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.38": {"text": "Abend und Thau.", "tokens": ["A\u00b7bend", "und", "Thau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.39": {"text": "Lieber entbehr' ich des Lichts, als deines begei-", "tokens": ["Lie\u00b7ber", "ent\u00b7behr'", "ich", "des", "Lichts", ",", "als", "dei\u00b7nes", "be\u00b7gei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "PPOSAT", "TRUNC"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.40": {"text": "Lieber des Liedes Besuch, als dein entz\u00fccken-", "tokens": ["Lie\u00b7ber", "des", "Lie\u00b7des", "Be\u00b7such", ",", "als", "dein", "ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "NN", "$,", "KOUS", "PPOSAT", "TRUNC"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.41": {"text": "Silberner klingt mir dein Gruss, als der Lyra Lis-", "tokens": ["Sil\u00b7ber\u00b7ner", "klingt", "mir", "dein", "Gruss", ",", "als", "der", "Ly\u00b7ra", "Lis"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "TRUNC"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.42": {"text": "Lieber von dir mich genannt, als von der Zun-", "tokens": ["Lie\u00b7ber", "von", "dir", "mich", "ge\u00b7nannt", ",", "als", "von", "der", "Zun"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "PRF", "VVPP", "$,", "KOUS", "APPR", "ART", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.43": {"text": "Seliger ruht es sich, Edle, an deinem steigenden", "tokens": ["Se\u00b7li\u00b7ger", "ruht", "es", "sich", ",", "Ed\u00b7le", ",", "an", "dei\u00b7nem", "stei\u00b7gen\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PRF", "$,", "ADJA", "$,", "APPR", "PPOSAT", "ADJA"], "meter": "+--+--+--+-+--", "measure": "dactylic.tri.plus"}, "line.44": {"text": "Busen,", "tokens": ["Bu\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.45": {"text": "Als in der Mutter Natur bl\u00fcthenbeschneyetem", "tokens": ["Als", "in", "der", "Mut\u00b7ter", "Na\u00b7tur", "bl\u00fc\u00b7then\u00b7be\u00b7schne\u00b7ye\u00b7tem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Schooss.", "tokens": ["Scho\u00b7oss", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.47": {"text": "Gl\u00fccklicher bin ich, umweht von deinen goldenen", "tokens": ["Gl\u00fcck\u00b7li\u00b7cher", "bin", "ich", ",", "um\u00b7weht", "von", "dei\u00b7nen", "gol\u00b7de\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$,", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.48": {"text": "Locken,", "tokens": ["Lo\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.49": {"text": "Als von des r\u00f6thlichen May's schimmernden", "tokens": ["Als", "von", "des", "r\u00f6th\u00b7li\u00b7chen", "May's", "schim\u00b7mern\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+--+--++--", "measure": "dactylic.di.plus"}, "line.50": {"text": "Bl\u00fcthen beschneyt.", "tokens": ["Bl\u00fc\u00b7then", "be\u00b7schneyt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.51": {"text": "Tausend sind der Bl\u00fcthen des May's, und tausend-", "tokens": ["Tau\u00b7send", "sind", "der", "Bl\u00fc\u00b7then", "des", "May's", ",", "und", "tau\u00b7sen\u00b7d"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ART", "NE", "$,", "KON", "TRUNC"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.52": {"text": "W\u00fcrzige D\u00fcft' entwehn seinem balsamischen", "tokens": ["W\u00fcr\u00b7zi\u00b7ge", "D\u00fcft'", "ent\u00b7wehn", "sei\u00b7nem", "bal\u00b7sa\u00b7mi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.53": {"text": "Hauch.", "tokens": ["Hauch", "."], "token_info": ["word", "pu