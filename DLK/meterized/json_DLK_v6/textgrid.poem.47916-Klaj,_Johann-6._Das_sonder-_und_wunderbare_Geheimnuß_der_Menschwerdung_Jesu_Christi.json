{"textgrid.poem.47916": {"metadata": {"author": {"name": "Klaj, Johann", "birth": "N.A.", "death": "N.A."}, "title": "6. Das sonder- und wunderbare Geheimnu\u00df der Menschwerdung Jesu Christi", "genre": "verse", "period": "N.A.", "pub_year": 1636, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Sein", "tokens": ["Das", "Sein"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "N\u00fctz nichts/ wen der nicht der gibt Sein/ sein fein/", "tokens": ["N\u00fctz", "nichts", "/", "wen", "der", "nicht", "der", "gibt", "Sein", "/", "sein", "fein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PIS", "$(", "PWS", "ART", "PTKNEG", "ART", "VVFIN", "PPOSAT", "$(", "PPOSAT", "ADJD", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "ein Leben das da b\u00f6se/ ist es eines?", "tokens": ["ein", "Le\u00b7ben", "das", "da", "b\u00f6\u00b7se", "/", "ist", "es", "ei\u00b7nes", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "ADJD", "$(", "VAFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "keines.", "tokens": ["kei\u00b7nes", "."], "token_info": ["word", "punct"], "pos": ["PIS", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "sonst auch", "tokens": ["sonst", "auch"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "wird gute Gifft viel besser durch Gebrauch/", "tokens": ["wird", "gu\u00b7te", "Gifft", "viel", "bes\u00b7ser", "durch", "Ge\u00b7brauch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ADV", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wer wol lebt/ lebt. Was Adam hat verderbet/", "tokens": ["Wer", "wol", "lebt", "/", "lebt", ".", "Was", "A\u00b7dam", "hat", "ver\u00b7der\u00b7bet", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$(", "VVFIN", "$.", "PWS", "NN", "VAFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sterbet.", "tokens": ["Ster\u00b7bet", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "das Grab", "tokens": ["das", "Grab"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "das hat Er jhm und uns gebrochen ab/", "tokens": ["das", "hat", "Er", "jhm", "und", "uns", "ge\u00b7bro\u00b7chen", "ab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PPER", "KON", "PPER", "VVPP", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "der bittere Mund/ die Z\u00e4hne/ die gest\u00fcmpffet/", "tokens": ["der", "bit\u00b7te\u00b7re", "Mund", "/", "die", "Z\u00e4h\u00b7ne", "/", "die", "ge\u00b7st\u00fcmpf\u00b7fet", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "$(", "ART", "VVPP", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "r\u00fcmpffet.", "tokens": ["r\u00fcmpf\u00b7fet", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Er lag", "tokens": ["Er", "lag"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Stam vnd Geschlecht ba\u00df ohne Liecht/ und Tag/", "tokens": ["Stam", "vnd", "Ge\u00b7schlecht", "ba\u00df", "oh\u00b7ne", "Liecht", "/", "und", "Tag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ADV", "APPR", "NN", "$(", "KON", "NN", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.15": {"text": "So da\u00df er solt im hellen schwefel sitzen", "tokens": ["So", "da\u00df", "er", "solt", "im", "hel\u00b7len", "schwe\u00b7fel", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "VMFIN", "APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Schwitzen.", "tokens": ["Schwit\u00b7zen", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Doch Gott", "tokens": ["Doch", "Gott"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.18": {"text": "erbarmet sich dem Todt zu hohn und spott;", "tokens": ["er\u00b7bar\u00b7met", "sich", "dem", "Todt", "zu", "hohn", "und", "spott", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKZU", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Er selbst f\u00fcr uns/ mit Menschenfleisch/ bekleidet/", "tokens": ["Er", "selbst", "f\u00fcr", "uns", "/", "mit", "Men\u00b7schen\u00b7fleisch", "/", "be\u00b7klei\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPER", "$(", "APPR", "NN", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "leidet.", "tokens": ["lei\u00b7det", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "Der Sohn", "tokens": ["Der", "Sohn"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.22": {"text": "der Mutterlo\u00df nach seiner Gottheit Thron", "tokens": ["der", "Mut\u00b7ter\u00b7lo\u00df", "nach", "sei\u00b7ner", "Got\u00b7theit", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "auch Vatterlo\u00df/ hat m\u00fcssen in der Wiegen", "tokens": ["auch", "Vat\u00b7ter\u00b7lo\u00df", "/", "hat", "m\u00fcs\u00b7sen", "in", "der", "Wie\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "$(", "VAFIN", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "liegen.", "tokens": ["lie\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Der Schall", "tokens": ["Der", "Schall"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.26": {"text": "de\u00df Engels f\u00e4llt/ der Even Trauerfall", "tokens": ["de\u00df", "En\u00b7gels", "f\u00e4llt", "/", "der", "E\u00b7ven", "Trau\u00b7er\u00b7fall"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "In diesen Kind wir Alten in dem leben", "tokens": ["In", "die\u00b7sen", "Kind", "wir", "Al\u00b7ten", "in", "dem", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "PPER", "NN", "APPR", "ART", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "schweben.", "tokens": ["schwe\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Der Sch\u00f6pffer selbsten wird das/ was er hat geschaffen/", "tokens": ["Der", "Sch\u00f6pf\u00b7fer", "selbs\u00b7ten", "wird", "das", "/", "was", "er", "hat", "ge\u00b7schaf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VAFIN", "ART", "$(", "PWS", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Wettermacher Gott schl\u00e4fft ohne Blitz und Waffen.", "tokens": ["Der", "Wet\u00b7ter\u00b7ma\u00b7cher", "Gott", "schl\u00e4fft", "oh\u00b7ne", "Blitz", "und", "Waf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er ist mein Bruder? ja/ der alls in allem ist.", "tokens": ["Er", "ist", "mein", "Bru\u00b7der", "?", "ja", "/", "der", "alls", "in", "al\u00b7lem", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$.", "PTKANT", "$(", "ART", "ADV", "APPR", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es hat sich alls verkehrt in kurtz verwichner frist.", "tokens": ["Es", "hat", "sich", "alls", "ver\u00b7kehrt", "in", "kurtz", "ver\u00b7wich\u00b7ner", "frist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "VVPP", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Komm/ Kl\u00fcgelp\u00f6bel komm/ erwege Wundersachen/", "tokens": ["Komm", "/", "Kl\u00fc\u00b7gel\u00b7p\u00f6\u00b7bel", "komm", "/", "er\u00b7we\u00b7ge", "Wun\u00b7der\u00b7sa\u00b7chen", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$(", "NN", "VVFIN", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "die deinen albern Witz zum Aberwitze machen.", "tokens": ["die", "dei\u00b7nen", "al\u00b7bern", "Witz", "zum", "A\u00b7ber\u00b7wit\u00b7ze", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was ohne zeit/ wird zeit; was un\u00fcmschr\u00e4nckt/ \u00fcmschr\u00e4nckt;", "tokens": ["Was", "oh\u00b7ne", "zeit", "/", "wird", "zeit", ";", "was", "un\u00b7\u00fcm\u00b7schr\u00e4nckt", "/", "\u00fcm\u00b7schr\u00e4nckt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "APPR", "NN", "$(", "VAFIN", "NN", "$.", "PWS", "ADJD", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "der d\u00fcrstet/ der die Welt mit Wolckeng\u00fcssen tr\u00e4nckt;", "tokens": ["der", "d\u00fcrs\u00b7tet", "/", "der", "die", "Welt", "mit", "Wol\u00b7cken\u00b7g\u00fcs\u00b7sen", "tr\u00e4nckt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "$(", "ART", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "De\u00df Leibes Vfer hemt de\u00df Meeres Nasenl\u00f6cher;", "tokens": ["De\u00df", "Lei\u00b7bes", "Vfer", "hemt", "de\u00df", "Mee\u00b7res", "Na\u00b7sen\u00b7l\u00f6\u00b7cher", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Kein Crei\u00df \u00fcmschleust den Punct/ noch diesen Pfeil ein K\u00f6cher.", "tokens": ["Kein", "Crei\u00df", "\u00fcm\u00b7schleust", "den", "Punct", "/", "noch", "die\u00b7sen", "Pfeil", "ein", "K\u00f6\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$(", "ADV", "PDAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der alle Thier ern\u00e4hrt/ den hungert nach Gem\u00fc\u00df/", "tokens": ["Der", "al\u00b7le", "Thier", "er\u00b7n\u00e4hrt", "/", "den", "hun\u00b7gert", "nach", "Ge\u00b7m\u00fc\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$(", "ART", "CARD", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "der Himmel ist so klein als klarer Grie\u00df und Kie\u00df.", "tokens": ["der", "Him\u00b7mel", "ist", "so", "klein", "als", "kla\u00b7rer", "Grie\u00df", "und", "Kie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KOKOM", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der alls tr\u00e4gt auff der Hand/ wird auff der Hand getragen;", "tokens": ["Der", "alls", "tr\u00e4gt", "auff", "der", "Hand", "/", "wird", "auff", "der", "Hand", "ge\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "ART", "NN", "$(", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "der mit dem Donner kan die Welt in hauffen schlagen.", "tokens": ["der", "mit", "dem", "Don\u00b7ner", "kan", "die", "Welt", "in", "hauf\u00b7fen", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VMFIN", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "der weinet in der Welt. Der wie ein L\u00f6we br\u00fcllt", "tokens": ["der", "wei\u00b7net", "in", "der", "Welt", ".", "Der", "wie", "ein", "L\u00f6\u00b7we", "br\u00fcllt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "APPR", "ART", "NN", "$.", "ART", "KOKOM", "ART", "NE", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "erzittert f\u00fcr dem Stier in Windeln eingeh\u00fcllt.", "tokens": ["er\u00b7zit\u00b7tert", "f\u00fcr", "dem", "Stier", "in", "Win\u00b7deln", "ein\u00b7ge\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das Feuer leidet frost; es ist vom Reiff erkrummet/", "tokens": ["Das", "Feu\u00b7er", "lei\u00b7det", "frost", ";", "es", "ist", "vom", "Reiff", "er\u00b7krum\u00b7met", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$.", "PPER", "VAFIN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "die Hand die Sommer bringt; der Zungenschmid verstummet.", "tokens": ["die", "Hand", "die", "Som\u00b7mer", "bringt", ";", "der", "Zun\u00b7gen\u00b7schmid", "ver\u00b7stum\u00b7met", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der ligt im Schosse blo\u00df/ der alle Kleidung macht:", "tokens": ["Der", "ligt", "im", "Schos\u00b7se", "blo\u00df", "/", "der", "al\u00b7le", "Klei\u00b7dung", "macht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "ADV", "$(", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "die Freude gr\u00e4met sich; der bleiche Kummer lacht.", "tokens": ["die", "Freu\u00b7de", "gr\u00e4\u00b7met", "sich", ";", "der", "blei\u00b7che", "Kum\u00b7mer", "lacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$.", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}