{"textgrid.poem.67321": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Nachhall der Freundschaft", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoher Freundschaft Sympathieen singen,", "tokens": ["Ho\u00b7her", "Freund\u00b7schaft", "Sym\u00b7pa\u00b7thi\u00b7een", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "T\u00f6net edel; in den Saiten klingen", "tokens": ["T\u00f6\u00b7net", "e\u00b7del", ";", "in", "den", "Sai\u00b7ten", "klin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "$.", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hehr und stolz die laute Sympathie", "tokens": ["Hehr", "und", "stolz", "die", "lau\u00b7te", "Sym\u00b7pa\u00b7thie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Hoher Freundschaft; doch wo athmen sie?", "tokens": ["Ho\u00b7her", "Freund\u00b7schaft", ";", "doch", "wo", "ath\u00b7men", "sie", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "KON", "PWAV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ach, sie schieden l\u00e4ngst aus unsern H\u00fctten,", "tokens": ["Ach", ",", "sie", "schie\u00b7den", "l\u00e4ngst", "aus", "un\u00b7sern", "H\u00fct\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aus dem Taumel unsrer Affensitten,", "tokens": ["Aus", "dem", "Tau\u00b7mel", "uns\u00b7rer", "Af\u00b7fen\u00b7sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Gr\u00e4mten sich zu Luft und wurden Schall,", "tokens": ["Gr\u00e4m\u00b7ten", "sich", "zu", "Luft", "und", "wur\u00b7den", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und sind jetzt \u2013 was noch als Widerhall?", "tokens": ["Und", "sind", "jetzt", "\u2013", "was", "noch", "als", "Wi\u00b7der\u00b7hall", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "$(", "PWS", "ADV", "KOUS", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Widerhall, den jede Lipp' entweihet,", "tokens": ["Wi\u00b7der\u00b7hall", ",", "den", "je\u00b7de", "Lipp'", "ent\u00b7wei\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Widerhall, auf Sophas hingestreuet,", "tokens": ["Wi\u00b7der\u00b7hall", ",", "auf", "So\u00b7phas", "hin\u00b7ge\u00b7streu\u00b7et", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NE", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sind der Sprache Spielberloquen, sind", "tokens": ["Sind", "der", "Spra\u00b7che", "Spiel\u00b7ber\u00b7lo\u00b7quen", ",", "sind"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "ART", "NN", "NN", "$,", "VAFIN"], "meter": "+-+-+-++-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Unsrer sch\u00f6nen Kreise F\u00e4cherwind.", "tokens": ["Uns\u00b7rer", "sch\u00f6\u00b7nen", "Krei\u00b7se", "F\u00e4\u00b7cher\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Sympathie, als einst mit s\u00fc\u00dfen Schmerzen", "tokens": ["Sym\u00b7pa\u00b7thie", ",", "als", "einst", "mit", "s\u00fc\u00b7\u00dfen", "Schmer\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Du den S\u00e4ugling noch an Mutterherzen", "tokens": ["Du", "den", "S\u00e4ug\u00b7ling", "noch", "an", "Mut\u00b7ter\u00b7her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Bandest, als er an der Tugend Brust", "tokens": ["Ban\u00b7dest", ",", "als", "er", "an", "der", "Tu\u00b7gend", "Brust"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Leben trank, nicht sieche Lasterlust;", "tokens": ["Le\u00b7ben", "trank", ",", "nicht", "sie\u00b7che", "Las\u00b7ter\u00b7lust", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PTKNEG", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Als Du mit den Schwestern noch im Thale", "tokens": ["Als", "Du", "mit", "den", "Schwes\u00b7tern", "noch", "im", "Tha\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Spieltest und beim Heldenv\u00e4termahle", "tokens": ["Spiel\u00b7test", "und", "beim", "Hel\u00b7den\u00b7v\u00e4\u00b7ter\u00b7mah\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "J\u00fcnglinge beseeltest, sich mit Muth", "tokens": ["J\u00fcng\u00b7lin\u00b7ge", "be\u00b7seel\u00b7test", ",", "sich", "mit", "Muth"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PRF", "APPR", "NN"], "meter": "++--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Dir zu weihen, Dich in sch\u00f6nem Blut,", "tokens": ["Dir", "zu", "wei\u00b7hen", ",", "Dich", "in", "sch\u00f6\u00b7nem", "Blut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Sympathie, im Tode Dich zu singen,", "tokens": ["Sym\u00b7pa\u00b7thie", ",", "im", "To\u00b7de", "Dich", "zu", "sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sich auf Ruhmesfl\u00fcgeln aufzuschwingen,", "tokens": ["Sich", "auf", "Ruh\u00b7mes\u00b7fl\u00fc\u00b7geln", "auf\u00b7zu\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wo der Freund zu harren ihm verhie\u00df,", "tokens": ["Wo", "der", "Freund", "zu", "har\u00b7ren", "ihm", "ver\u00b7hie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKZU", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Hinterm Grab im V\u00e4terparadies.", "tokens": ["Hin\u00b7term", "Grab", "im", "V\u00e4\u00b7ter\u00b7pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Und, o Liebe, konntest Herzen binden,", "tokens": ["Und", ",", "o", "Lie\u00b7be", ",", "konn\u00b7test", "Her\u00b7zen", "bin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "FM", "NN", "$,", "VMFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In einander Ewigkeit zu finden,", "tokens": ["In", "ein\u00b7an\u00b7der", "E\u00b7wig\u00b7keit", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "F\u00fcr einander sich mit edler M\u00fch", "tokens": ["F\u00fcr", "ein\u00b7an\u00b7der", "sich", "mit", "ed\u00b7ler", "M\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Neu zu bilden \u2013 Herzenssympathie,", "tokens": ["Neu", "zu", "bil\u00b7den", "\u2013", "Her\u00b7zens\u00b7sym\u00b7pa\u00b7thie", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$(", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Reingel\u00e4utert in Dir zu zerflie\u00dfen,", "tokens": ["Rein\u00b7ge\u00b7l\u00e4u\u00b7tert", "in", "Dir", "zu", "zer\u00b7flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Alles, Alles in Dir zu genie\u00dfen,", "tokens": ["Al\u00b7les", ",", "Al\u00b7les", "in", "Dir", "zu", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Seelenth\u00fcllet sich zu zeigen, sich,", "tokens": ["See\u00b7lent\u00b7h\u00fcl\u00b7let", "sich", "zu", "zei\u00b7gen", ",", "sich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKZU", "VVINF", "$,", "PRF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wo der Blick verstummt, herzinniglich", "tokens": ["Wo", "der", "Blick", "ver\u00b7stummt", ",", "her\u00b7zin\u00b7nig\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Dein zu nennen. Auch die Thr\u00e4nen gie\u00dfen", "tokens": ["Dein", "zu", "nen\u00b7nen", ".", "Auch", "die", "Thr\u00e4\u00b7nen", "gie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "PTKZU", "VVINF", "$.", "ADV", "ART", "NN", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Balsam, wenn sie herzvereinet flie\u00dfen;", "tokens": ["Bal\u00b7sam", ",", "wenn", "sie", "herz\u00b7ver\u00b7ei\u00b7net", "flie\u00b7\u00dfen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Gram und Noth und Tod und Schicksal band", "tokens": ["Gram", "und", "Noth", "und", "Tod", "und", "Schick\u00b7sal", "band"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "KON", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Seelen fester als der Diamant,", "tokens": ["See\u00b7len", "fes\u00b7ter", "als", "der", "Di\u00b7a\u00b7mant", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Unsre Buhlerfessel. \u2013 Wilde Saiten,", "tokens": ["Uns\u00b7re", "Buh\u00b7ler\u00b7fes\u00b7sel", ".", "\u2013", "Wil\u00b7de", "Sai\u00b7ten", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "$(", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wohin irrt Ihr? \u2013 Wohin Euch begleiten", "tokens": ["Wo\u00b7hin", "irrt", "Ihr", "?", "\u2013", "Wo\u00b7hin", "Euch", "be\u00b7glei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$.", "$(", "PWAV", "PPER", "VVINF"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Nimmer kann der Zeiten Wahn; f\u00fcr Tand", "tokens": ["Nim\u00b7mer", "kann", "der", "Zei\u00b7ten", "Wahn", ";", "f\u00fcr", "Tand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "NN", "$.", "APPR", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Hat er, was Ihr singet, l\u00e4ngst erkannt;", "tokens": ["Hat", "er", ",", "was", "Ihr", "sin\u00b7get", ",", "l\u00e4ngst", "er\u00b7kannt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Mag auch seine Tempel nicht so h\u00f6hnen,", "tokens": ["Mag", "auch", "sei\u00b7ne", "Tem\u00b7pel", "nicht", "so", "h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "ADV", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df sie reiner Menschheit W\u00fcrde t\u00f6nen,", "tokens": ["Da\u00df", "sie", "rei\u00b7ner", "Menschheit", "W\u00fcr\u00b7de", "t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Der ja, reich ges\u00e4ttigt und geehrt,", "tokens": ["Der", "ja", ",", "reich", "ge\u00b7s\u00e4t\u00b7tigt", "und", "ge\u00b7ehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "ADJD", "VVPP", "KON", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Schw\u00e4tzer sucht und Freunde nicht begehrt,", "tokens": ["Schw\u00e4t\u00b7zer", "sucht", "und", "Freun\u00b7de", "nicht", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Nicht begehrt, noch haben kann. In Oede", "tokens": ["Nicht", "be\u00b7gehrt", ",", "noch", "ha\u00b7ben", "kann", ".", "In", "O\u00b7e\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "VVPP", "$,", "ADV", "VAINF", "VMFIN", "$.", "APPR", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Rings umher verstummt des Herzens Rede,", "tokens": ["Rings", "um\u00b7her", "ver\u00b7stummt", "des", "Her\u00b7zens", "Re\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schweigt sein lauter Pulsschlag. L\u00fcsteleer", "tokens": ["Schweigt", "sein", "lau\u00b7ter", "Puls\u00b7schlag", ".", "L\u00fcs\u00b7te\u00b7leer"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPOSAT", "PIAT", "NN", "$.", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ist es um mich; da ert\u00f6nt nicht mehr", "tokens": ["Ist", "es", "um", "mich", ";", "da", "er\u00b7t\u00f6nt", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "$.", "ADV", "VVFIN", "PTKNEG", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Herzens Silberklang. In armen H\u00fctten,", "tokens": ["Her\u00b7zens", "Sil\u00b7ber\u00b7klang", ".", "In", "ar\u00b7men", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In der Urzeit letzten heil'gen Sitten,", "tokens": ["In", "der", "Ur\u00b7zeit", "letz\u00b7ten", "heil'\u00b7gen", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da nur lebt