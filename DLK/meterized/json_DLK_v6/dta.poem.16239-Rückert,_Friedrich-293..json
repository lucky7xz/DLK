{"dta.poem.16239": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "293.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Forscher freuts da\u00df er den Vorrath nie verliert,", "tokens": ["Den", "For\u00b7scher", "freuts", "da\u00df", "er", "den", "Vor\u00b7rath", "nie", "ver\u00b7liert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil jeder Aufschlu\u00df ihm Aufgaben neu gebiert.", "tokens": ["Weil", "je\u00b7der", "Auf\u00b7schlu\u00df", "ihm", "Auf\u00b7ga\u00b7ben", "neu", "ge\u00b7biert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hier von der Wurzel dort zum Apfel kamst du kaum;", "tokens": ["Hier", "von", "der", "Wur\u00b7zel", "dort", "zum", "Ap\u00b7fel", "kamst", "du", "kaum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "APPRART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er hat ein Dutzend Kern', und jeder wird ein Baum.", "tokens": ["Er", "hat", "ein", "Dut\u00b7zend", "Kern'", ",", "und", "je\u00b7der", "wird", "ein", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,", "KON", "PIS", "VAFIN", "ART", "NN", "$."], "meter": "