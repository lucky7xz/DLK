{"dta.poem.8753": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxXII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Und f\u00fchlte mich f\u00fcrder gezogen,", "tokens": ["Und", "f\u00fchl\u00b7te", "mich", "f\u00fcr\u00b7der", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Gassen verlie\u00df ich, vom W\u00e4chter bewacht,", "tokens": ["Die", "Gas\u00b7sen", "ver\u00b7lie\u00df", "ich", ",", "vom", "W\u00e4ch\u00b7ter", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "APPRART", "NE", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Durchwandelte sacht", "tokens": ["Durch\u00b7wan\u00b7del\u00b7te", "sacht"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "In der Nacht, in der Nacht,", "tokens": ["In", "der", "Nacht", ",", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Das Thor mit dem gothischen Bogen.", "tokens": ["Das", "Thor", "mit", "dem", "go\u00b7thi\u00b7schen", "Bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der M\u00fchlbach rauschte durch felsigen Schacht,", "tokens": ["Der", "M\u00fchl\u00b7bach", "rauschte", "durch", "fel\u00b7si\u00b7gen", "Schacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ich lehnte mich \u00fcber die Br\u00fccke,", "tokens": ["Ich", "lehn\u00b7te", "mich", "\u00fc\u00b7ber", "die", "Br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Tief unter mir nahm ich der Wogen in Acht", "tokens": ["Tief", "un\u00b7ter", "mir", "nahm", "ich", "der", "Wo\u00b7gen", "in", "Acht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "CARD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die wallten so sacht", "tokens": ["Die", "wall\u00b7ten", "so", "sacht"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "In der Nacht, in der Nacht,", "tokens": ["In", "der", "Nacht", ",", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Doch wallte nicht eine zur\u00fccke.", "tokens": ["Doch", "wall\u00b7te", "nicht", "ei\u00b7ne", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Es drehte sich oben, unz\u00e4hlig entfacht,", "tokens": ["Es", "dreh\u00b7te", "sich", "o\u00b7ben", ",", "un\u00b7z\u00e4h\u00b7lig", "ent\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "$,", "ADJD", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Melodischer Wandel der Sterne,", "tokens": ["Me\u00b7lo\u00b7di\u00b7scher", "Wan\u00b7del", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Mit ihnen der Mond in beruhigter Pracht,", "tokens": ["Mit", "ih\u00b7nen", "der", "Mond", "in", "be\u00b7ru\u00b7hig\u00b7ter", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sie funkelten sacht", "tokens": ["Sie", "fun\u00b7kel\u00b7ten", "sacht"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "In der Nacht, in der Nacht,", "tokens": ["In", "der", "Nacht", ",", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Durch t\u00e4uschend entlegene Ferne.", "tokens": ["Durch", "t\u00e4u\u00b7schend", "ent\u00b7le\u00b7ge\u00b7ne", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich blickte hinauf in der Nacht, in der Nacht,", "tokens": ["Ich", "blick\u00b7te", "hin\u00b7auf", "in", "der", "Nacht", ",", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich blickte hinunter auf's Neue:", "tokens": ["Ich", "blick\u00b7te", "hin\u00b7un\u00b7ter", "auf's", "Neu\u00b7e", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "O wehe, wie hast du die Tage verbracht,", "tokens": ["O", "we\u00b7he", ",", "wie", "hast", "du", "die", "Ta\u00b7ge", "ver\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PWAV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Nun stille du sacht", "tokens": ["Nun", "stil\u00b7le", "du", "sacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "In der Nacht, in der Nacht,", "tokens": ["In", "der", "Nacht", ",", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Im pochenden Herzen die Reue!", "tokens": ["Im", "po\u00b7chen\u00b7den", "Her\u00b7zen", "die", "Reu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}