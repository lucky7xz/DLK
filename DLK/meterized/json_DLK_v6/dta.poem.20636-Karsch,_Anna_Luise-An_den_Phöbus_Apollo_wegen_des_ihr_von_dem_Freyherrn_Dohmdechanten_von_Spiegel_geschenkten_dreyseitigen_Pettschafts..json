{"dta.poem.20636": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  den Ph\u00f6bus Apollo  \n wegen  \n des ihr von dem Freyherrn Dohmdechanten  \n  von Spiegel  \n geschenkten dreyseitigen Pettschafts.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.71", "fr:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der du mir dieses Saytenspiel bespanntest,                 ", "tokens": ["Der", "du", "mir", "die\u00b7ses", "Say\u00b7ten\u00b7spiel", "be\u00b7spann\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df es die Seelen r\u00fchren soll,", "tokens": ["Da\u00df", "es", "die", "See\u00b7len", "r\u00fch\u00b7ren", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mir j\u00fcngst zum Geschenk ein dreyfach Wappen", "tokens": ["Und", "mir", "j\u00fcngst", "zum", "Ge\u00b7schenk", "ein", "drey\u00b7fach", "Wap\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "APPRART", "NN", "ART", "ADJD", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "sandtest,", "tokens": ["sand\u00b7test", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "O g\u00fctiger Apoll!", "tokens": ["O", "g\u00fc\u00b7ti\u00b7ger", "A\u00b7poll", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Erschienst du nicht im Traume deinem ", "tokens": ["Er\u00b7schienst", "du", "nicht", "im", "Trau\u00b7me", "dei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der dich in meinen Liedern liebt,", "tokens": ["Der", "dich", "in", "mei\u00b7nen", "Lie\u00b7dern", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und oftmals meinem mattgewordnen Geiste Fl\u00fcgel", "tokens": ["Und", "oft\u00b7mals", "mei\u00b7nem", "matt\u00b7ge\u00b7word\u00b7nen", "Geis\u00b7te", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch seine Freundschaft giebt?", "tokens": ["Durch", "sei\u00b7ne", "Freund\u00b7schaft", "giebt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ja, Ph\u00f6bus! seinem Geiste vorgemahlet", "tokens": ["Ja", ",", "Ph\u00f6\u00b7bus", "!", "sei\u00b7nem", "Geis\u00b7te", "vor\u00b7ge\u00b7mah\u00b7let"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "NE", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hast du die Bilder also fein;", "tokens": ["Hast", "du", "die", "Bil\u00b7der", "al\u00b7so", "fein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du, dessen gr\u00fcnumkr\u00e4nzte Locke pr\u00e4chtig strahlet", "tokens": ["Du", ",", "des\u00b7sen", "gr\u00fc\u00b7num\u00b7kr\u00e4nz\u00b7te", "Lo\u00b7cke", "pr\u00e4ch\u00b7tig", "strah\u00b7let"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELAT", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von Sonnenschein.", "tokens": ["Von", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "In kalte tiefe Fluthen sprang,", "tokens": ["In", "kal\u00b7te", "tie\u00b7fe", "Flut\u00b7hen", "sprang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und noch mit todtenblasser halberstorbner Lippe", "tokens": ["Und", "noch", "mit", "tod\u00b7ten\u00b7blas\u00b7ser", "hal\u00b7ber\u00b7storb\u00b7ner", "Lip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von Liebe sang;", "tokens": ["Von", "Lie\u00b7be", "sang", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Und dir geopfert, da\u00df es sey", "tokens": ["Und", "dir", "ge\u00b7o\u00b7pfert", ",", "da\u00df", "es", "sey"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVPP", "$,", "KOUS", "PPER", "VAFIN"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Einst nach Jahrtausenden mit Rosen frisch umwunden", "tokens": ["Einst", "nach", "Jahr\u00b7tau\u00b7sen\u00b7den", "mit", "Ro\u00b7sen", "frisch", "um\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "APPR", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "F\u00fcr mich wie neu;", "tokens": ["F\u00fcr", "mich", "wie", "neu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KOKOM", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Den Lorber, den ich so wie sie ersungen,", "tokens": ["Den", "Lor\u00b7ber", ",", "den", "ich", "so", "wie", "sie", "er\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "KOKOM", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der \u00fcber dieser Leyer schwebt;", "tokens": ["Der", "\u00fc\u00b7ber", "die\u00b7ser", "Le\u00b7yer", "schwebt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und meines ", "tokens": ["Und", "mei\u00b7nes"], "token_info": ["word", "word"], "pos": ["KON", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "In meinen webt:", "tokens": ["In", "mei\u00b7nen", "webt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Dies alles seh ich \u2014 o du Gott der Musen!", "tokens": ["Dies", "al\u00b7les", "seh", "ich", "o", "du", "Gott", "der", "Mu\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "$(", "FM", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sprich, ob mein Herz nicht schwellen soll?", "tokens": ["Sprich", ",", "ob", "mein", "Herz", "nicht", "schwel\u00b7len", "soll", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPOSAT", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So hoch schwoll vormals nicht des Sylla stolzer Busen", "tokens": ["So", "hoch", "schwoll", "vor\u00b7mals", "nicht", "des", "Syl\u00b7la", "stol\u00b7zer", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJD", "ADV", "PTKNEG", "ART", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von Freude voll,", "tokens": ["Von", "Freu\u00b7de", "voll", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Wenn er sein blaues Auge lachend wandte", "tokens": ["Wenn", "er", "sein", "blau\u00b7es", "Au\u00b7ge", "la\u00b7chend", "wand\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf seinen gro\u00dfen Siegelring,", "tokens": ["Auf", "sei\u00b7nen", "gro\u00b7\u00dfen", "Sie\u00b7gel\u00b7ring", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf dessen Schnitte man den K\u00f6nig Bachus kannte,", "tokens": ["Auf", "des\u00b7sen", "Schnit\u00b7te", "man", "den", "K\u00f6\u00b7nig", "Ba\u00b7chus", "kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PIS", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der treulos fing", "tokens": ["Der", "treu\u00b7los", "fing"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Den Gatten seines Kindes, und ihn brachte", "tokens": ["Den", "Gat\u00b7ten", "sei\u00b7nes", "Kin\u00b7des", ",", "und", "ihn", "brach\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gefesselt in des R\u00f6mers Hand,", "tokens": ["Ge\u00b7fes\u00b7selt", "in", "des", "R\u00f6\u00b7mers", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der den Jugurtha, den ein Heer nicht zittern machte,", "tokens": ["Der", "den", "Ju\u00b7gur\u00b7tha", ",", "den", "ein", "Heer", "nicht", "zit\u00b7tern", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NE", "$,", "PRELS", "ART", "NN", "PTKNEG", "VVINF", "VVFIN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Jezt \u00fcberwand,", "tokens": ["Jezt", "\u00fc\u00b7ber\u00b7wand", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Und im Triumph zum erstenmal gef\u00fchret", "tokens": ["Und", "im", "Tri\u00b7umph", "zum", "ers\u00b7ten\u00b7mal", "ge\u00b7f\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "APPRART", "ADV", "VVPP"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Den Sklaven, der sonst Kronen trug,", "tokens": ["Den", "Skla\u00b7ven", ",", "der", "sonst", "Kro\u00b7nen", "trug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf Purpur schlief, und unumschr\u00e4nkt regieret,", "tokens": ["Auf", "Pur\u00b7pur", "schlief", ",", "und", "un\u00b7um\u00b7schr\u00e4nkt", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Helden schlug.", "tokens": ["Und", "Hel\u00b7den", "schlug", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}