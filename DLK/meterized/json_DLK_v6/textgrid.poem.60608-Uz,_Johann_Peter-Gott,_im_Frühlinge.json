{"textgrid.poem.60608": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Gott, im Fr\u00fchlinge", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In seinem schimmernden Gewand", "tokens": ["In", "sei\u00b7nem", "schim\u00b7mern\u00b7den", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hast du den Fr\u00fchling uns gesandt,", "tokens": ["Hast", "du", "den", "Fr\u00fch\u00b7ling", "uns", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Rosen um sein Haupt gewunden.", "tokens": ["Und", "Ro\u00b7sen", "um", "sein", "Haupt", "ge\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Holdl\u00e4chelnd k\u00f6mmt er schon!", "tokens": ["Hold\u00b7l\u00e4\u00b7chelnd", "k\u00f6mmt", "er", "schon", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es f\u00fchren ihn die Stunden,", "tokens": ["Es", "f\u00fch\u00b7ren", "ihn", "die", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O Gott, auf seinen Bluhmenthron.", "tokens": ["O", "Gott", ",", "auf", "sei\u00b7nen", "Bluh\u00b7men\u00b7thron", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er geht in B\u00fcschen und sie bl\u00fchn;", "tokens": ["Er", "geht", "in", "B\u00fc\u00b7schen", "und", "sie", "bl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Fluren k\u00f6mmt ihr frisches Gr\u00fcn,", "tokens": ["Den", "Flu\u00b7ren", "k\u00f6mmt", "ihr", "fri\u00b7sches", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und W\u00e4ldern w\u00e4chst ihr Schatten wieder,", "tokens": ["Und", "W\u00e4l\u00b7dern", "w\u00e4chst", "ihr", "Schat\u00b7ten", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der West, liebkosend, schwingt", "tokens": ["Der", "West", ",", "lieb\u00b7ko\u00b7send", ",", "schwingt"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sein thauendes Gefieder,", "tokens": ["Sein", "thau\u00b7en\u00b7des", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und jeder frohe Vogel singt.", "tokens": ["Und", "je\u00b7der", "fro\u00b7he", "Vo\u00b7gel", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mit eurer Lieder s\u00fcssem Klang,", "tokens": ["Mit", "eu\u00b7rer", "Lie\u00b7der", "s\u00fcs\u00b7sem", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr V\u00f6gel, soll auch mein Gesang", "tokens": ["Ihr", "V\u00f6\u00b7gel", ",", "soll", "auch", "mein", "Ge\u00b7sang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VMFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Vater der Natur sich schwingen,", "tokens": ["Zum", "Va\u00b7ter", "der", "Na\u00b7tur", "sich", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entz\u00fcckung rei\u00dft mich hin!", "tokens": ["Ent\u00b7z\u00fc\u00b7ckung", "rei\u00dft", "mich", "hin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich will dem Herrn lobsingen,", "tokens": ["Ich", "will", "dem", "Herrn", "lob\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch den ich wurde, was ich bin!", "tokens": ["Durch", "den", "ich", "wur\u00b7de", ",", "was", "ich", "bin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VAFIN", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O G\u00fctigster! Denn wer ist gut,", "tokens": ["O", "G\u00fc\u00b7tigs\u00b7ter", "!", "Denn", "wer", "ist", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "KON", "PWS", "VAFIN", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Wie du, der allen Gutes thut?", "tokens": ["Wie", "du", ",", "der", "al\u00b7len", "Gu\u00b7tes", "thut", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du sorgtest auch f\u00fcr mein Vergn\u00fcgen,", "tokens": ["Du", "sorg\u00b7test", "auch", "f\u00fcr", "mein", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als aus dem gro\u00dfen Plan", "tokens": ["Als", "aus", "dem", "gro\u00b7\u00dfen", "Plan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Erstaunte Welten stiegen,", "tokens": ["Er\u00b7staun\u00b7te", "Wel\u00b7ten", "stie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Sonnen sich geschaffen sahn.", "tokens": ["Und", "Son\u00b7nen", "sich", "ge\u00b7schaf\u00b7fen", "sahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PRF", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sch\u00f6n ist die Erde, wann sie bl\u00fcht,", "tokens": ["Sch\u00f6n", "ist", "die", "Er\u00b7de", ",", "wann", "sie", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und, ganz um unsre Lust bem\u00fcht,", "tokens": ["Und", ",", "ganz", "um", "uns\u00b7re", "Lust", "be\u00b7m\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich in des Fr\u00fchlings Farben kleidet,", "tokens": ["Sich", "in", "des", "Fr\u00fch\u00b7lings", "Far\u00b7ben", "klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00fcberall voll Pracht,", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "voll", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Selbst, wo die Heerde weidet,", "tokens": ["Selbst", ",", "wo", "die", "Heer\u00b7de", "wei\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "In bunter Zierde d\u00fcftend lacht:", "tokens": ["In", "bun\u00b7ter", "Zier\u00b7de", "d\u00fcf\u00b7tend", "lacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Gottheit w\u00fcrdiger Altar,", "tokens": ["Der", "Got\u00b7theit", "w\u00fcr\u00b7di\u00b7ger", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Worauf das bluhmenreiche Jahr,", "tokens": ["Wo\u00b7rauf", "das", "bluh\u00b7men\u00b7rei\u00b7che", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Herr, zu deinem Wohlgefallen,", "tokens": ["O", "Herr", ",", "zu", "dei\u00b7nem", "Wohl\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein s\u00fcsses Rauchwerk bringt,", "tokens": ["Sein", "s\u00fcs\u00b7ses", "Rauch\u00b7werk", "bringt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Inde\u00df von Nachtigallen", "tokens": ["In\u00b7de\u00df", "von", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein froher Lobgesang erklingt!", "tokens": ["Ein", "fro\u00b7her", "Lob\u00b7ge\u00b7sang", "er\u00b7klingt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du hast mit Sch\u00f6nheit, die entz\u00fcckt,", "tokens": ["Du", "hast", "mit", "Sch\u00f6n\u00b7heit", ",", "die", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Antlitz der Natur geschm\u00fcckt,", "tokens": ["Das", "Ant\u00b7litz", "der", "Na\u00b7tur", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O aller Sch\u00f6nheit reiche Quelle!", "tokens": ["O", "al\u00b7ler", "Sch\u00f6n\u00b7heit", "rei\u00b7che", "Quel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dir geht kein Wesen vor!", "tokens": ["Dir", "geht", "kein", "We\u00b7sen", "vor", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die reinste Liebe schwelle", "tokens": ["Die", "reins\u00b7te", "Lie\u00b7be", "schwel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein ganzes Herz zu dir empor!", "tokens": ["Mein", "gan\u00b7zes", "Herz", "zu", "dir", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}