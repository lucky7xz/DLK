{"textgrid.poem.24836": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wald, der oftmals mein Gel\u00e4chter", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wald, der oftmals mein Gel\u00e4chter", "tokens": ["Wald", ",", "der", "oft\u00b7mals", "mein", "Ge\u00b7l\u00e4ch\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Freunde Kreis vernahm,", "tokens": ["In", "der", "Freun\u00b7de", "Kreis", "ver\u00b7nahm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeuge meiner frohen Tr\u00e4ume!", "tokens": ["Zeu\u00b7ge", "mei\u00b7ner", "fro\u00b7hen", "Tr\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "D\u00fcster sch\u00fctteln deine B\u00e4ume", "tokens": ["D\u00fcs\u00b7ter", "sch\u00fct\u00b7teln", "dei\u00b7ne", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun ihr Haupt, wie Totenw\u00e4chter,", "tokens": ["Nun", "ihr", "Haupt", ",", "wie", "To\u00b7ten\u00b7w\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "PWAV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ueber mir und meinem Gram!", "tokens": ["Ue\u00b7ber", "mir", "und", "mei\u00b7nem", "Gram", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lust'ge B\u00fccher, einst gelesen", "tokens": ["Lust'\u00b7ge", "B\u00fc\u00b7cher", ",", "einst", "ge\u00b7le\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der alten muntern Zeit,", "tokens": ["In", "der", "al\u00b7ten", "mun\u00b7tern", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wag' ich nun, euch aufzuschlagen;", "tokens": ["Wag'", "ich", "nun", ",", "euch", "auf\u00b7zu\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "PPER", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach! nur von vergangnen Tagen,", "tokens": ["Ach", "!", "nur", "von", "ver\u00b7gang\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur von dem, was ich gewesen,", "tokens": ["Nur", "von", "dem", ",", "was", "ich", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "$,", "PWS", "PPER", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sprecht ihr mir in dumpfem Leid!", "tokens": ["Sprecht", "ihr", "mir", "in", "dum\u00b7pfem", "Leid", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Saal, wo wir uns einst versammelt,", "tokens": ["Saal", ",", "wo", "wir", "uns", "einst", "ver\u00b7sam\u00b7melt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oede stehst du nun und leer!", "tokens": ["O\u00b7e\u00b7de", "stehst", "du", "nun", "und", "leer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nie mehr fliegt in heitrer Stunde", "tokens": ["Nie", "mehr", "fliegt", "in", "hei\u00b7trer", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das Gespr\u00e4ch von Mund zu Munde,", "tokens": ["Das", "Ge\u00b7spr\u00e4ch", "von", "Mund", "zu", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und nur ", "tokens": ["Und", "nur"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Schluchzend: Nimmer-, nimmermehr!", "tokens": ["Schluch\u00b7zend", ":", "Nim\u00b7mer", ",", "nim\u00b7mer\u00b7mehr", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$.", "TRUNC", "$,", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}