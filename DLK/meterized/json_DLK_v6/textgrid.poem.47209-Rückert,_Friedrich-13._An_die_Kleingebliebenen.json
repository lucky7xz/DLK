{"textgrid.poem.47209": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "13. An die Kleingebliebenen", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heranzualtern ist der Jugend Los,", "tokens": ["Her\u00b7an\u00b7zu\u00b7al\u00b7tern", "ist", "der", "Ju\u00b7gend", "Los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und kleine Kinder wachsen m\u00e4hlich gro\u00df,", "tokens": ["Und", "klei\u00b7ne", "Kin\u00b7der", "wach\u00b7sen", "m\u00e4h\u00b7lich", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann machen sie sich von den Eltern los,", "tokens": ["Dann", "ma\u00b7chen", "sie", "sich", "von", "den", "El\u00b7tern", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wiegen kannst du sie nicht mehr im Scho\u00df.", "tokens": ["Und", "wie\u00b7gen", "kannst", "du", "sie", "nicht", "mehr", "im", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VMFIN", "PPER", "PPER", "PTKNEG", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch ihr, die mir geraubt ein fr\u00fches Los,", "tokens": ["Doch", "ihr", ",", "die", "mir", "ge\u00b7raubt", "ein", "fr\u00fc\u00b7hes", "Los", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PRELS", "PPER", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bleibt immer klein, nie werdet ihr mir gro\u00df,", "tokens": ["Bleibt", "im\u00b7mer", "klein", ",", "nie", "wer\u00b7det", "ihr", "mir", "gro\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,", "ADV", "VAFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr rei\u00dft euch nie von meinem Herzen los,", "tokens": ["Ihr", "rei\u00dft", "euch", "nie", "von", "mei\u00b7nem", "Her\u00b7zen", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wiegen kann ich euch wie sonst im Scho\u00df.", "tokens": ["Und", "wie\u00b7gen", "kann", "ich", "euch", "wie", "sonst", "im", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VMFIN", "PPER", "PPER", "KOKOM", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}