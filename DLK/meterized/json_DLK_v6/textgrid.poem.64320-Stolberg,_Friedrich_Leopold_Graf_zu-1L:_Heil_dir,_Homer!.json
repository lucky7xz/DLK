{"textgrid.poem.64320": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heil dir, Homer!", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heil dir, Homer!", "tokens": ["Heil", "dir", ",", "Ho\u00b7mer", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Freudiger, entflammter, weinender Dank", "tokens": ["Freu\u00b7di\u00b7ger", ",", "ent\u00b7flamm\u00b7ter", ",", "wei\u00b7nen\u00b7der", "Dank"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJD", "$,", "ADJA", "NN"], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Bebt auf der Lippe,", "tokens": ["Bebt", "auf", "der", "Lip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Schimmert im Auge,", "tokens": ["Schim\u00b7mert", "im", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Tr\u00e4ufelt wie Tau", "tokens": ["Tr\u00e4u\u00b7felt", "wie", "Tau"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "KOKOM", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Hinab in deines Gesanges heiligen Strom!", "tokens": ["Hin\u00b7ab", "in", "dei\u00b7nes", "Ge\u00b7san\u00b7ges", "hei\u00b7li\u00b7gen", "Strom", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ihn go\u00df von Idas geweihtem Gipfel", "tokens": ["Ihn", "go\u00df", "von", "I\u00b7das", "ge\u00b7weih\u00b7tem", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mutter Natur!", "tokens": ["Mut\u00b7ter", "Na\u00b7tur", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Freute sich der str\u00f6menden Flut,", "tokens": ["Freu\u00b7te", "sich", "der", "str\u00f6\u00b7men\u00b7den", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die, voll Gottheit,", "tokens": ["Die", ",", "voll", "Got\u00b7theit", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wie der sonnenbes\u00e4te G\u00fcrtel der Nacht,", "tokens": ["Wie", "der", "son\u00b7nen\u00b7be\u00b7s\u00e4\u00b7te", "G\u00fcr\u00b7tel", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "T\u00f6nend mit himmlischen Harmonieen,", "tokens": ["T\u00f6\u00b7nend", "mit", "himm\u00b7li\u00b7schen", "Har\u00b7mo\u00b7ni\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "W\u00e4lzet ihre Wogen in das hallende Thal!", "tokens": ["W\u00e4l\u00b7zet", "ih\u00b7re", "Wo\u00b7gen", "in", "das", "hal\u00b7len\u00b7de", "Thal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.8": {"text": "Es freute sich die Natur,", "tokens": ["Es", "freu\u00b7te", "sich", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Rief ihre goldgelockte T\u00f6chter,", "tokens": ["Rief", "ih\u00b7re", "gold\u00b7ge\u00b7lock\u00b7te", "T\u00f6ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wahrheit und Sch\u00f6nheit beugten sich \u00fcber den Strom,", "tokens": ["Wahr\u00b7heit", "und", "Sch\u00f6n\u00b7heit", "beug\u00b7ten", "sich", "\u00fc\u00b7ber", "den", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.11": {"text": "Und erkannten in jeder Welle staunend ihr Bild!", "tokens": ["Und", "er\u00b7kann\u00b7ten", "in", "je\u00b7der", "Wel\u00b7le", "stau\u00b7nend", "ihr", "Bild", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Es liebte dich fr\u00fch", "tokens": ["Es", "lieb\u00b7te", "dich", "fr\u00fch"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Die heilige Natur!", "tokens": ["Die", "hei\u00b7li\u00b7ge", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da deine Mutter im Thale dich gebar,", "tokens": ["Da", "dei\u00b7ne", "Mut\u00b7ter", "im", "Tha\u00b7le", "dich", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wo Simois in den Skamander sich ergeu\u00dft,", "tokens": ["Wo", "Si\u00b7mois", "in", "den", "Ska\u00b7man\u00b7der", "sich", "er\u00b7geu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+---+-++-+", "measure": "dactylic.init"}, "line.5": {"text": "Und ermattet dich lie\u00df fallen in der Blumen Tau,", "tokens": ["Und", "er\u00b7mat\u00b7tet", "dich", "lie\u00df", "fal\u00b7len", "in", "der", "Blu\u00b7men", "Tau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "VVINF", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "Blicktest du schon mit Dichtergef\u00fchl", "tokens": ["Blick\u00b7test", "du", "schon", "mit", "Dich\u00b7ter\u00b7ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Der sinkenden Sonne,", "tokens": ["Der", "sin\u00b7ken\u00b7den", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Die vom Thrazischen Schneegebirg,", "tokens": ["Die", "vom", "Thra\u00b7zi\u00b7schen", "Schnee\u00b7ge\u00b7birg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "NE", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "\u00dcber purpurne Wallungen des Hellesponts,", "tokens": ["\u00dc\u00b7ber", "pur\u00b7pur\u00b7ne", "Wal\u00b7lun\u00b7gen", "des", "Hel\u00b7les\u00b7ponts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Dich begr\u00fc\u00dfte, in ihr flammendes Gesicht!", "tokens": ["Dich", "be\u00b7gr\u00fc\u00df\u00b7te", ",", "in", "ihr", "flam\u00b7men\u00b7des", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "Und es strebten sie zu greifen", "tokens": ["Und", "es", "streb\u00b7ten", "sie", "zu", "grei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Deine zarten H\u00e4nde,", "tokens": ["Dei\u00b7ne", "zar\u00b7ten", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Von ihrem Glanze r\u00f6tlicht, in die Luft empor!", "tokens": ["Von", "ih\u00b7rem", "Glan\u00b7ze", "r\u00f6t\u00b7licht", ",", "in", "die", "Luft", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da l\u00e4chelte die Natur,", "tokens": ["Da", "l\u00e4\u00b7chel\u00b7te", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Weihte dich, und s\u00e4ugte dich an ihrer Brust!", "tokens": ["Weih\u00b7te", "dich", ",", "und", "s\u00e4ug\u00b7te", "dich", "an", "ih\u00b7rer", "Brust", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Bildete, wie sie bildete die Himmel,", "tokens": ["Bil\u00b7de\u00b7te", ",", "wie", "sie", "bil\u00b7de\u00b7te", "die", "Him\u00b7mel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Wie sie bildete die Rose,", "tokens": ["Wie", "sie", "bil\u00b7de\u00b7te", "die", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und den Tau, der vom Himmel in die Rose tr\u00e4uft,", "tokens": ["Und", "den", "Tau", ",", "der", "vom", "Him\u00b7mel", "in", "die", "Ro\u00b7se", "tr\u00e4uft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Bildete sorgsam den Knaben und den J\u00fcngling so!", "tokens": ["Bil\u00b7de\u00b7te", "sorg\u00b7sam", "den", "Kna\u00b7ben", "und", "den", "J\u00fcng\u00b7ling", "so", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "KON", "ART", "NN", "ADV", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Gab dir der Empfindung", "tokens": ["Gab", "dir", "der", "Emp\u00b7fin\u00b7dung"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Flammenden Blick;", "tokens": ["Flam\u00b7men\u00b7den", "Blick", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Gab, was nur ihren Sch\u00f6\u00dflingen sie giebt,", "tokens": ["Gab", ",", "was", "nur", "ih\u00b7ren", "Sch\u00f6\u00df\u00b7lin\u00b7gen", "sie", "giebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.10": {"text": "Thr\u00e4nen jegliches Gef\u00fchls!", "tokens": ["Thr\u00e4\u00b7nen", "jeg\u00b7li\u00b7ches", "Ge\u00b7f\u00fchls", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Die st\u00fcrzende, welche gl\u00fchende Wangen netzt,", "tokens": ["Die", "st\u00fcr\u00b7zen\u00b7de", ",", "wel\u00b7che", "gl\u00fc\u00b7hen\u00b7de", "Wan\u00b7gen", "netzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PWAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Und die sanftre, die von zitternder Wimper", "tokens": ["Und", "die", "sanft\u00b7re", ",", "die", "von", "zit\u00b7tern\u00b7der", "Wim\u00b7per"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Rinnt aufs erbleichte Gesicht!", "tokens": ["Rinnt", "aufs", "er\u00b7bleich\u00b7te", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.14": {"text": "Gab deiner Seele", "tokens": ["Gab", "dei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Einfalt der Tauben und des Adlers Kraft!", "tokens": ["Ein\u00b7falt", "der", "Tau\u00b7ben", "und", "des", "Ad\u00b7lers", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.16": {"text": "Gleich deinem Liede", "tokens": ["Gleich", "dei\u00b7nem", "Lie\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Sanft nun, wie Quellen in des Mondes Schein,", "tokens": ["Sanft", "nun", ",", "wie", "Quel\u00b7len", "in", "des", "Mon\u00b7des", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Donnernd und stark nun, wie der Katarakte Sturz!", "tokens": ["Don\u00b7nernd", "und", "stark", "nun", ",", "wie", "der", "Ka\u00b7ta\u00b7rak\u00b7te", "Sturz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "ADV", "$,", "PWAV", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}