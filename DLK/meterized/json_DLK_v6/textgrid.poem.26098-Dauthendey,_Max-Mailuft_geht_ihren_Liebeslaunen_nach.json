{"textgrid.poem.26098": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Mailuft geht ihren Liebeslaunen nach", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mailuft geht \u00fcber alle H\u00e4userschwellen,", "tokens": ["Mai\u00b7luft", "geht", "\u00fc\u00b7ber", "al\u00b7le", "H\u00e4u\u00b7ser\u00b7schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mailuft flie\u00dft auf dem Wasser hin, auf gr\u00fcnen Wellen", "tokens": ["Mai\u00b7luft", "flie\u00dft", "auf", "dem", "Was\u00b7ser", "hin", ",", "auf", "gr\u00fc\u00b7nen", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,", "APPR", "ADJA", "NN"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "Und wird in einem Kahn zum frohgesungenen Reim", "tokens": ["Und", "wird", "in", "ei\u00b7nem", "Kahn", "zum", "froh\u00b7ge\u00b7sun\u00b7ge\u00b7nen", "Reim"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Und kommt wie junge Sehnsucht niemals heim.", "tokens": ["Und", "kommt", "wie", "jun\u00b7ge", "Sehn\u00b7sucht", "nie\u00b7mals", "heim", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mailuft sitzt nachts am offenen Fenster wach", "tokens": ["Mai\u00b7luft", "sitzt", "nachts", "am", "of\u00b7fe\u00b7nen", "Fens\u00b7ter", "wach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und \u00fcbt mit Nachtigallen bis zum Morgen.", "tokens": ["Und", "\u00fcbt", "mit", "Nach\u00b7ti\u00b7gal\u00b7len", "bis", "zum", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie hat kein Kissen und kein bleibend Dach", "tokens": ["Sie", "hat", "kein", "Kis\u00b7sen", "und", "kein", "blei\u00b7bend", "Dach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "KON", "PIAT", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und will auch nicht f\u00fcr lange Zukunft sorgen.", "tokens": ["Und", "will", "auch", "nicht", "f\u00fcr", "lan\u00b7ge", "Zu\u00b7kunft", "sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mailuft geht ihren Liebeslaunen nach.", "tokens": ["Mai\u00b7luft", "geht", "ih\u00b7ren", "Lie\u00b7bes\u00b7lau\u00b7nen", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}