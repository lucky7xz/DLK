{"textgrid.poem.41641": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Regenmonat str\u00f6mt, verfeindet allem Leben,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Regenmonat str\u00f6mt, verfeindet allem Leben,", "tokens": ["Der", "Re\u00b7gen\u00b7mo\u00b7nat", "str\u00f6mt", ",", "ver\u00b7fein\u00b7det", "al\u00b7lem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "PIS", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus seiner Urne Gu\u00df ein Dunkel frostergraut", "tokens": ["Aus", "sei\u00b7ner", "Ur\u00b7ne", "Gu\u00df", "ein", "Dun\u00b7kel", "fros\u00b7ter\u00b7graut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des Kirchhofs bleicher Schar im kalten D\u00e4mmerweben", "tokens": ["Des", "Kirch\u00b7hofs", "blei\u00b7cher", "Schar", "im", "kal\u00b7ten", "D\u00e4m\u00b7mer\u00b7we\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Sterben auf die Stadt, in der der Nebel braut.", "tokens": ["Und", "Ster\u00b7ben", "auf", "die", "Stadt", ",", "in", "der", "der", "Ne\u00b7bel", "braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es regt am Estrich sich in fr\u00f6stelndem Erbeben", "tokens": ["Es", "regt", "am", "Est\u00b7rich", "sich", "in", "fr\u00f6s\u00b7teln\u00b7dem", "Er\u00b7be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die magre Katze, die nach einem Lager schaut,", "tokens": ["Die", "mag\u00b7re", "Kat\u00b7ze", ",", "die", "nach", "ei\u00b7nem", "La\u00b7ger", "schaut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verstorbnen Dichters Geist f\u00fchl im Getropf ich schweben,", "tokens": ["Ver\u00b7storb\u00b7nen", "Dich\u00b7ters", "Geist", "f\u00fchl", "im", "Ge\u00b7tropf", "ich", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit eines irrenden Gespenstes Klagelaut.", "tokens": ["Mit", "ei\u00b7nes", "ir\u00b7ren\u00b7den", "Ge\u00b7spens\u00b7tes", "Kla\u00b7ge\u00b7laut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der dumpfe Brummba\u00df klagt, und rauchger Scheite Knistern", "tokens": ["Der", "dump\u00b7fe", "Brumm\u00b7ba\u00df", "klagt", ",", "und", "rauch\u00b7ger", "Schei\u00b7te", "Knis\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eint seiner Fistel Ton der Wanduhr heisrem Fl\u00fcstern,", "tokens": ["Eint", "sei\u00b7ner", "Fis\u00b7tel", "Ton", "der", "Wan\u00b7duhr", "heis\u00b7rem", "Fl\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Derweil im Kartenspiel, von schmutzgem Duft getr\u00e4nkt,", "tokens": ["Der\u00b7weil", "im", "Kar\u00b7ten\u00b7spiel", ",", "von", "schmutz\u00b7gem", "Duft", "ge\u00b7tr\u00e4nkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der eklen Erbschaft einer wassers\u00fcchtgen Alten,", "tokens": ["Der", "ek\u00b7len", "Erb\u00b7schaft", "ei\u00b7ner", "was\u00b7ser\u00b7s\u00fccht\u00b7gen", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich leis Piquedame und C\u0153rbube unterhalten", "tokens": ["Sich", "leis", "Pi\u00b7que\u00b7da\u00b7me", "und", "C\u0153r\u00b7bu\u00b7be", "un\u00b7ter\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und einstgen Liebesgl\u00fccks ihr Herz tr\u00fcbselig denkt.", "tokens": ["Und", "einst\u00b7gen", "Lie\u00b7bes\u00b7gl\u00fccks", "ihr", "Herz", "tr\u00fcb\u00b7se\u00b7lig", "denkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}