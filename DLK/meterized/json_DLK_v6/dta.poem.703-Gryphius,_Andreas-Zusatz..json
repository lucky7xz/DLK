{"dta.poem.703": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Zusatz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Die Z\u00e4hren die so h\u00e4uffig flissen/", "tokens": ["Die", "Z\u00e4h\u00b7ren", "die", "so", "h\u00e4uf\u00b7fig", "flis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Prest vn\u00df ergetzung au\u00df; Nicht vnlust/ wie man meynt.", "tokens": ["Prest", "vn\u00df", "er\u00b7get\u00b7zung", "au\u00df", ";", "Nicht", "vn\u00b7lust", "/", "wie", "man", "meynt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "PTKVZ", "$.", "PTKNEG", "ADJD", "$(", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich schaw/ ach! ach/ der Tag bricht an! ", "tokens": ["Ich", "schaw", "/", "ach", "!", "ach", "/", "der", "Tag", "bricht", "an", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "XY", "$.", "XY", "$(", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd die herbe Nacht verschwindet/", "tokens": ["Vnd", "die", "her\u00b7be", "Nacht", "ver\u00b7schwin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der Tag der vn\u00df ergetzen kan/", "tokens": ["Der", "Tag", "der", "vn\u00df", "er\u00b7get\u00b7zen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der die schwere band' entbindet. ", "tokens": ["Der", "die", "schwe\u00b7re", "band'", "ent\u00b7bin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ade nun Babilon. Jtzt bin ich nicht gefangen.", "tokens": ["A\u00b7de", "nun", "Ba\u00b7bi\u00b7lon", ".", "Jtzt", "bin", "ich", "nicht", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NE", "$.", "ADV", "VAFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Gl\u00fcck zu mein Vaterland ich bin der Augst entgangen", "tokens": ["Gl\u00fcck", "zu", "mein", "Va\u00b7ter\u00b7land", "ich", "bin", "der", "Augst", "ent\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PPER", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Frolockt jhr ", "tokens": ["Fro\u00b7lockt", "jhr"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+--", "measure": "dactylic.init"}, "line.10": {"text": "Die starcken Schl\u00f6sser sind entzwey.", "tokens": ["Die", "star\u00b7cken", "Schl\u00f6s\u00b7ser", "sind", "ent\u00b7zwey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Jhr W\u00e4lder den ich offt mein leiden anvertrawt! ", "tokens": ["Ihr", "W\u00e4l\u00b7der", "den", "ich", "offt", "mein", "lei\u00b7den", "an\u00b7ver\u00b7trawt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Jhr Zeugen meiner angst/ jhr Berg\u2019/ jhr Th\u00e4ler schawt", "tokens": ["Ihr", "Zeu\u00b7gen", "mei\u00b7ner", "angst", "/", "jhr", "Ber\u00b7g'", "/", "jhr", "Th\u00e4\u00b7ler", "schawt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Wie mich de\u00df ", "tokens": ["Wie", "mich", "de\u00df"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Ach nein! mir traumt ach nein ich wache!", "tokens": ["Ach", "nein", "!", "mir", "traumt", "ach", "nein", "ich", "wa\u00b7che", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$.", "PPER", "VVFIN", "ADV", "PTKANT", "PPER", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "O alzu s\u00fcsser wahn! was bild\u2019 ich mir doch eyn?", "tokens": ["O", "al\u00b7zu", "s\u00fcs\u00b7ser", "wahn", "!", "was", "bild'", "ich", "mir", "doch", "eyn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKA", "ADJD", "PTKVZ", "$.", "PWS", "VVFIN", "PPER", "PPER", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ich f\u00fchle ja da\u00df ich noch mu\u00df in schmertzen seyn!", "tokens": ["Ich", "f\u00fch\u00b7le", "ja", "da\u00df", "ich", "noch", "mu\u00df", "in", "schmert\u00b7zen", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOUS", "PPER", "ADV", "VMFIN", "APPR", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ach/ kan die Hoffnung mich so ohne ma\u00df\u2019 ergetzen!", "tokens": ["Ach", "/", "kan", "die", "Hoff\u00b7nung", "mich", "so", "oh\u00b7ne", "ma\u00df'", "er\u00b7get\u00b7zen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "VMFIN", "ART", "NN", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wie fr\u00f6lich werd\u2019 i", "tokens": ["Wie", "fr\u00f6\u00b7lich", "werd'", "i"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "setzen. ", "tokens": ["set\u00b7zen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}