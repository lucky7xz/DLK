{"textgrid.poem.67667": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "27. Das Leben", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Gastmahl ist Dein Leben.", "tokens": ["Ein", "Gast\u00b7mahl", "ist", "Dein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nimm, was Dir wird gegeben;", "tokens": ["Nimm", ",", "was", "Dir", "wird", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was nicht ist da,", "tokens": ["Was", "nicht", "ist", "da", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VAFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Was Dir nicht nah,", "tokens": ["Was", "Dir", "nicht", "nah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Erbettle nicht,", "tokens": ["Er\u00b7bett\u00b7le", "nicht", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Erwarte, bis es Dir gegeben;", "tokens": ["Er\u00b7war\u00b7te", ",", "bis", "es", "Dir", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sei froh, und wenn die Nacht anbricht,", "tokens": ["Sei", "froh", ",", "und", "wenn", "die", "Nacht", "an\u00b7bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "KON", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dann bange nicht,", "tokens": ["Dann", "ban\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Steh freudig auf und danke f\u00fcr Dein Leben!", "tokens": ["Steh", "freu\u00b7dig", "auf", "und", "dan\u00b7ke", "f\u00fcr", "Dein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}