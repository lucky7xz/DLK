{"dta.poem.15607": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weil du dich allerdings zu h\u00f6hern f\u00fchlst berufen,", "tokens": ["Weil", "du", "dich", "al\u00b7ler\u00b7dings", "zu", "h\u00f6\u00b7hern", "f\u00fchlst", "be\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PTKZU", "VVINF", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beklagest du, o Mensch, die stehn auf niedern Stufen;", "tokens": ["Be\u00b7kla\u00b7gest", "du", ",", "o", "Mensch", ",", "die", "stehn", "auf", "nie\u00b7dern", "Stu\u00b7fen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$,", "PRELS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Alsob Stein, Pflanz' und Thier todt oder taub und blind,", "tokens": ["Al\u00b7sob", "Stein", ",", "Pflanz'", "und", "Thier", "todt", "o\u00b7der", "taub", "und", "blind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "KON", "NN", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "--++-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Ungl\u00fccklich m\u00fc\u00dften seyn, weil sie wie du nicht sind.", "tokens": ["Un\u00b7gl\u00fcck\u00b7lich", "m\u00fc\u00df\u00b7ten", "seyn", ",", "weil", "sie", "wie", "du", "nicht", "sind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "VAINF", "$,", "KOUS", "PPER", "PWAV", "PPER", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So h\u00f6rest du das Thier wie nach Erl\u00f6sung st\u00f6hnen,", "tokens": ["So", "h\u00f6\u00b7rest", "du", "das", "Thier", "wie", "nach", "Er\u00b7l\u00f6\u00b7sung", "st\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "KOKOM", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00f6rst Weh- statt Wonnelaut in Nachtigallent\u00f6nen,", "tokens": ["H\u00f6rst", "Weh", "statt", "Won\u00b7ne\u00b7laut", "in", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Selbst einen Seufzerhauch im Fr\u00fchlingsfl\u00fcsterhain,", "tokens": ["Selbst", "ei\u00b7nen", "Seuf\u00b7zer\u00b7hauch", "im", "Fr\u00fch\u00b7lings\u00b7fl\u00fcs\u00b7ter\u00b7hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und einen Schmerzensklang aus jedem Erz und Stein.", "tokens": ["Und", "ei\u00b7nen", "Schmer\u00b7zens\u00b7klang", "aus", "je\u00b7dem", "Erz", "und", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "In dem, was ihn nicht f\u00fchlt, ist nicht der Widerspruch,", "tokens": ["In", "dem", ",", "was", "ihn", "nicht", "f\u00fchlt", ",", "ist", "nicht", "der", "Wi\u00b7der\u00b7spruch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,", "VAFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er ist in dir, du selbst belegst die Welt mit Fluch.", "tokens": ["Er", "ist", "in", "dir", ",", "du", "selbst", "be\u00b7legst", "die", "Welt", "mit", "Fluch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$,", "PPER", "ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Jemehr du in dir selbst zum Einklang bist gekommen,", "tokens": ["Je\u00b7mehr", "du", "in", "dir", "selbst", "zum", "Ein\u00b7klang", "bist", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jemehr wird er von dir auch au\u00dfenher vernommen.", "tokens": ["Je\u00b7mehr", "wird", "er", "von", "dir", "auch", "au\u00b7\u00dfen\u00b7her", "ver\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Befreie dich, o Mensch, vom Halben, Falschen, B\u00f6sen,", "tokens": ["Be\u00b7frei\u00b7e", "dich", ",", "o", "Mensch", ",", "vom", "Hal\u00b7ben", ",", "Fal\u00b7schen", ",", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$,", "APPRART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die gebundene Natur wird Gott erl\u00f6sen.", "tokens": ["Und", "die", "ge\u00b7bun\u00b7de\u00b7ne", "Na\u00b7tur", "wird", "Gott", "er\u00b7l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}