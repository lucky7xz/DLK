{"textgrid.poem.39462": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Lorbeer, dem du gl\u00fchend nachgerungen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Lorbeer, dem du gl\u00fchend nachgerungen,", "tokens": ["Der", "Lor\u00b7beer", ",", "dem", "du", "gl\u00fc\u00b7hend", "nach\u00b7ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Flemming! welke niemals deinen Haaren;", "tokens": ["O", "Flem\u00b7ming", "!", "wel\u00b7ke", "nie\u00b7mals", "dei\u00b7nen", "Haa\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der du durch Schiffbruch, W\u00fcstenei'n, Barbaren,", "tokens": ["Der", "du", "durch", "Schiff\u00b7bruch", ",", "W\u00fcs\u00b7te\u00b7nei'n", ",", "Bar\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fast bis zum Bett der Sonne hingedrungen.", "tokens": ["Fast", "bis", "zum", "Bett", "der", "Son\u00b7ne", "hin\u00b7ge\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du lie\u00dfest, wo kein deutscher Laut erklungen,", "tokens": ["Du", "lie\u00b7\u00dfest", ",", "wo", "kein", "deut\u00b7scher", "Laut", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Fremdlings-Poesie sich offenbaren.", "tokens": ["Die", "Fremd\u00b7lings\u00b7Po\u00b7e\u00b7sie", "sich", "of\u00b7fen\u00b7ba\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der L\u00e4nder mehr, als Alexanders Schaaren,", "tokens": ["Der", "L\u00e4n\u00b7der", "mehr", ",", "als", "A\u00b7lex\u00b7an\u00b7ders", "Schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KOUS", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hat dein Gesang verherrlichend bezwungen.", "tokens": ["Hat", "dein", "Ge\u00b7sang", "ver\u00b7herr\u00b7li\u00b7chend", "be\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du warst der Orpheus jener Argonauten,", "tokens": ["Du", "warst", "der", "Or\u00b7pheus", "je\u00b7ner", "Ar\u00b7go\u00b7nau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NE", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Deutschland, Friede w\u00fcnschend, aus der Wolgen", "tokens": ["Die", "Deutschland", ",", "Frie\u00b7de", "w\u00fcn\u00b7schend", ",", "aus", "der", "Wol\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "VVPP", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Auf Caspiens Flut gesendet zu den Persen.", "tokens": ["Auf", "Cas\u00b7pi\u00b7ens", "Flut", "ge\u00b7sen\u00b7det", "zu", "den", "Per\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch als auf dich der Heimat Musen schauten,", "tokens": ["Doch", "als", "auf", "dich", "der", "Hei\u00b7mat", "Mu\u00b7sen", "schau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PRF", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und du zur\u00fcck kamst, ihnen rasch zu folgen,", "tokens": ["Und", "du", "zu\u00b7r\u00fcck", "kamst", ",", "ih\u00b7nen", "rasch", "zu", "fol\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKVZ", "VVFIN", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da stach der Tod dich neidisch in die Fersen.", "tokens": ["Da", "stach", "der", "Tod", "dich", "nei\u00b7disch", "in", "die", "Fer\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}