{"dta.poem.8637": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Anmuth .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Die Hoffnung, eine arge Dirne,", "tokens": ["Die", "Hoff\u00b7nung", ",", "ei\u00b7ne", "ar\u00b7ge", "Dir\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verbuhlte mir den Augenblick,", "tokens": ["Ver\u00b7buhl\u00b7te", "mir", "den", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bestahl mit frecher L\u00fcgenstirne", "tokens": ["Be\u00b7stahl", "mit", "fre\u00b7cher", "L\u00fc\u00b7gen\u00b7stir\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein junges Leben um sein Gl\u00fcck.", "tokens": ["Mein", "jun\u00b7ges", "Le\u00b7ben", "um", "sein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun ist's vor\u00fcber; in den Tagen,", "tokens": ["Nun", "ist's", "vor\u00b7\u00fc\u00b7ber", ";", "in", "den", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "PTKVZ", "$.", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als ihr Betrug ins Herz mir schnitt,", "tokens": ["Als", "ihr", "Be\u00b7trug", "ins", "Herz", "mir", "schnitt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hab' ich das s\u00fc\u00dfe Kind erschlagen,", "tokens": ["Hab'", "ich", "das", "s\u00fc\u00b7\u00dfe", "Kind", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mit dem Leben bin ich quitt.", "tokens": ["Und", "mit", "dem", "Le\u00b7ben", "bin", "ich", "quitt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht mehr zum Lustschlo\u00df umgelogen,", "tokens": ["Nicht", "mehr", "zum", "Lust\u00b7schlo\u00df", "um\u00b7ge\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Scheint mir die Erde, was sie ist:", "tokens": ["Scheint", "mir", "die", "Er\u00b7de", ",", "was", "sie", "ist", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein schwankes Zelt, das wir bezogen", "tokens": ["Ein", "schwan\u00b7kes", "Zelt", ",", "das", "wir", "be\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u2014 Gott habe Dank! \u2014 auf kurze Frist.", "tokens": ["Gott", "ha\u00b7be", "Dank", "!", "auf", "kur\u00b7ze", "Frist", "."], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "VAFIN", "NN", "$.", "$(", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Zu lange doch d\u00fcnkt mir das Br\u00fcten", "tokens": ["Zu", "lan\u00b7ge", "doch", "d\u00fcnkt", "mir", "das", "Br\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Hier unter diesem schwanken Zelt;", "tokens": ["Hier", "un\u00b7ter", "die\u00b7sem", "schwan\u00b7ken", "Zelt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ergreif' es, Sturm, in deinem W\u00fcthen,", "tokens": ["Er\u00b7greif'", "es", ",", "Sturm", ",", "in", "dei\u00b7nem", "W\u00fct\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und streu die Lappen in die Welt!", "tokens": ["Und", "streu", "die", "Lap\u00b7pen", "in", "die", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}