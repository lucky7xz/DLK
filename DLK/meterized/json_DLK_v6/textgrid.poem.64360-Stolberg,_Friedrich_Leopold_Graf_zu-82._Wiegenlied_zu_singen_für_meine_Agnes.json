{"textgrid.poem.64360": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "82. Wiegenlied zu singen f\u00fcr meine Agnes", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieblicher Knab', ich wiege", "tokens": ["Lieb\u00b7li\u00b7cher", "Knab'", ",", "ich", "wie\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN"], "meter": "+--+-++", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Singend dich ein in Schlummer,", "tokens": ["Sin\u00b7gend", "dich", "ein", "in", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "APPR", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Knabe, l\u00e4chle noch einmal!", "tokens": ["Kna\u00b7be", ",", "l\u00e4ch\u00b7le", "noch", "ein\u00b7mal", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eh du die \u00c4uglein schlie\u00dfest,", "tokens": ["Eh", "du", "die", "\u00c4ug\u00b7lein", "schlie\u00b7\u00dfest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Eh du die H\u00e4ndchen streckest,", "tokens": ["Eh", "du", "die", "H\u00e4nd\u00b7chen", "stre\u00b7ckest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Knabe, l\u00e4chle noch einmal!", "tokens": ["Kna\u00b7be", ",", "l\u00e4ch\u00b7le", "noch", "ein\u00b7mal", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Trug ich dich nicht neun Monde", "tokens": ["Trug", "ich", "dich", "nicht", "neun", "Mon\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PRF", "PTKNEG", "CARD", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Unter gedr\u00fccktem Herzen?", "tokens": ["Un\u00b7ter", "ge\u00b7dr\u00fcck\u00b7tem", "Her\u00b7zen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Knabe, l\u00e4chle noch einmal!", "tokens": ["Kna\u00b7be", ",", "l\u00e4ch\u00b7le", "noch", "ein\u00b7mal", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und da der Storch dich brachte,", "tokens": ["Und", "da", "der", "Storch", "dich", "brach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bi\u00df er nicht deine Mutter?", "tokens": ["Bi\u00df", "er", "nicht", "dei\u00b7ne", "Mut\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Knabe, l\u00e4chle noch einmal!", "tokens": ["Kna\u00b7be", ",", "l\u00e4ch\u00b7le", "noch", "ein\u00b7mal", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "S\u00e4ug' ich dich nicht an meinem", "tokens": ["S\u00e4ug'", "ich", "dich", "nicht", "an", "mei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PRF", "PTKNEG", "APPR", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Klopfenden Mutterherzen?", "tokens": ["Klop\u00b7fen\u00b7den", "Mut\u00b7ter\u00b7her\u00b7zen", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Knabe, l\u00e4chle noch einmal!", "tokens": ["Kna\u00b7be", ",", "l\u00e4ch\u00b7le", "noch", "ein\u00b7mal", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn du der Mutter l\u00e4chelst,", "tokens": ["Wenn", "du", "der", "Mut\u00b7ter", "l\u00e4\u00b7chelst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wird dich der Vater lieben,", "tokens": ["Wird", "dich", "der", "Va\u00b7ter", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Knabe, l\u00e4chle noch einmal!", "tokens": ["Kna\u00b7be", ",", "l\u00e4ch\u00b7le", "noch", "ein\u00b7mal", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}