{"dta.poem.5517": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "E ilfertige  G edancken  \n bey Erblickung  \n  der ersten Zeilen des   p.  511.  befindlichen  \n Schlusses  \n  dieses f\u00fcnften  T heils.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht sorge, grosser Brocks, da\u00df Du zu viel ge-", "tokens": ["Nicht", "sor\u00b7ge", ",", "gros\u00b7ser", "Brocks", ",", "da\u00df", "Du", "zu", "viel", "ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "$,", "ADJA", "NN", "$,", "KOUS", "PPER", "APPR", "PIAT", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "schrieben;", "tokens": ["schrie\u00b7ben", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wer keine Verse liebt, mu\u00df doch die Deinen lieben.", "tokens": ["Wer", "kei\u00b7ne", "Ver\u00b7se", "liebt", ",", "mu\u00df", "doch", "die", "Dei\u00b7nen", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "$,", "VMFIN", "ADV", "ART", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wehlt iemand sich ein solches Ziel,", "tokens": ["Wehlt", "ie\u00b7mand", "sich", "ein", "sol\u00b7ches", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wei\u00df die Kunst so hoch zu treiben;", "tokens": ["Und", "wei\u00df", "die", "Kunst", "so", "hoch", "zu", "trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der schreibt, bey noch so \u00f6fterm Schreiben,", "tokens": ["Der", "schreibt", ",", "bey", "noch", "so", "\u00f6f\u00b7term", "Schrei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "APPR", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zwar ", "tokens": ["Zwar"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "M. A. W.", "tokens": ["M.", "A.", "W."], "token_info": ["abbreviation", "abbreviation", "abbreviation"], "pos": ["NE", "APPRART", "NE"]}}}}}