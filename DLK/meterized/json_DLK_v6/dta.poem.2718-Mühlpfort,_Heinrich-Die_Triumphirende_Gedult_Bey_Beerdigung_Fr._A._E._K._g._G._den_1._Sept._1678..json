{"dta.poem.2718": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Triumphirende Gedult/  \n  Bey Beerdigung Fr. A. E. K. g. G. den  1.  \n Sept. 1678.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zog das so stoltze Rom/ die Herrscherin der Erden/", "tokens": ["Zog", "das", "so", "stolt\u00b7ze", "Rom", "/", "die", "Herr\u00b7sche\u00b7rin", "der", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "ADV", "VVFIN", "NE", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn sie den Feind erlegt und seine Macht zerstreut/", "tokens": ["Wenn", "sie", "den", "Feind", "er\u00b7legt", "und", "sei\u00b7ne", "Macht", "zer\u00b7streut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "KON", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ins g\u00f6ldne Capitol mit weissen Sieges-Pferden/", "tokens": ["Ins", "g\u00f6ld\u00b7ne", "Ca\u00b7pi\u00b7tol", "mit", "weis\u00b7sen", "Sie\u00b7ges\u00b7P\u00b7fer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und hat den Lorber-Krantz de\u00df Jovis Scho\u00df geweyht:", "tokens": ["Und", "hat", "den", "Lor\u00b7ber\u00b7Krantz", "de\u00df", "Jo\u00b7vis", "Scho\u00df", "ge\u00b7weyht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So sag ich mit mehr Ruhm und besserem Gepr\u00e4nge", "tokens": ["So", "sag", "ich", "mit", "mehr", "Ruhm", "und", "bes\u00b7se\u00b7rem", "Ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PIAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zeucht die Frau ", "tokens": ["Zeucht", "die", "Frau"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Jhr ist das grosse Rund der weiten Welt zu enge/", "tokens": ["Ihr", "ist", "das", "gros\u00b7se", "Rund", "der", "wei\u00b7ten", "Welt", "zu", "en\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "APPR", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und ihr behertzter Fu\u00df zertritt das Rad der Zeit.", "tokens": ["Und", "ihr", "be\u00b7hertz\u00b7ter", "Fu\u00df", "zer\u00b7tritt", "das", "Rad", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zu dem so tr\u00e4gt ihr Haupt was mehr als Lorber-Kronen/", "tokens": ["Zu", "dem", "so", "tr\u00e4gt", "ihr", "Haupt", "was", "mehr", "als", "Lor\u00b7ber\u00b7Kro\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "VVFIN", "PPOSAT", "NN", "PWS", "PIS", "KOKOM", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Jhr Krantz ist sch\u00f6ner noch als Gold und Edelstein/", "tokens": ["Ihr", "Krantz", "ist", "sch\u00f6\u00b7ner", "noch", "als", "Gold", "und", "E\u00b7del\u00b7stein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ADV", "KOUS", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Jhr Sieg vollkommener/ denn derer die hie wohnen/", "tokens": ["Ihr", "Sieg", "voll\u00b7kom\u00b7me\u00b7ner", "/", "denn", "de\u00b7rer", "die", "hie", "woh\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$(", "KON", "PDS", "ART", "ADV", "VVINF", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Weil Glauben und Gedult ihr Schatz und Beute seyn.", "tokens": ["Weil", "Glau\u00b7ben", "und", "Ge\u00b7dult", "ihr", "Schatz", "und", "Beu\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPOSAT", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was hat in kurtzer Zeit f\u00fcr ", "tokens": ["Was", "hat", "in", "kurt\u00b7zer", "Zeit", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "ADJA", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Nun sie den Uberflu\u00df der Himmels-F\u00fcll erlangt.", "tokens": ["Nun", "sie", "den", "U\u00b7ber\u00b7flu\u00df", "der", "Him\u00b7mels\u00b7F\u00fcll", "er\u00b7langt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es scheint ein Augenblick darinnen sie gelitten/", "tokens": ["Es", "scheint", "ein", "Au\u00b7gen\u00b7blick", "da\u00b7rin\u00b7nen", "sie", "ge\u00b7lit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da sie auf ewig jetzt f\u00fcr Gottes Throne prangt.", "tokens": ["Da", "sie", "auf", "e\u00b7wig", "jetzt", "f\u00fcr", "Got\u00b7tes", "Thro\u00b7ne", "prangt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJD", "ADV", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und di\u00df macht die Gedult/ da\u00df sie so herrlich sieget/", "tokens": ["Und", "di\u00df", "macht", "die", "Ge\u00b7dult", "/", "da\u00df", "sie", "so", "herr\u00b7lich", "sie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ART", "NN", "$(", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Die Tugend so gewi\u00df der andern K\u00f6nigin.", "tokens": ["Die", "Tu\u00b7gend", "so", "ge\u00b7wi\u00df", "der", "an\u00b7dern", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die st\u00e4rckt den schwachen Geist/ da\u00df er nicht unten lieget/", "tokens": ["Die", "st\u00e4rckt", "den", "schwa\u00b7chen", "Geist", "/", "da\u00df", "er", "nicht", "un\u00b7ten", "lie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$(", "KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "F\u00fchrt von der Erden ab zum Himmel unsern Sinn.", "tokens": ["F\u00fchrt", "von", "der", "Er\u00b7den", "ab", "zum", "Him\u00b7mel", "un\u00b7sern", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ruhmseeligste Gedult/ wer preist nicht deine Wercke?", "tokens": ["Ruhm\u00b7see\u00b7ligs\u00b7te", "Ge\u00b7dult", "/", "wer", "preist", "nicht", "dei\u00b7ne", "Wer\u00b7cke", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PWS", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Und wem ist unbekand die Gr\u00f6sse deiner Macht?", "tokens": ["Und", "wem", "ist", "un\u00b7be\u00b7kand", "die", "Gr\u00f6s\u00b7se", "dei\u00b7ner", "Macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "ADJD", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Du giebst dem Hertzen Trost/ dem Glauben Licht und St\u00e4rcke", "tokens": ["Du", "giebst", "dem", "Hert\u00b7zen", "Trost", "/", "dem", "Glau\u00b7ben", "Licht", "und", "St\u00e4r\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$(", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und stehest unbewegt wenn Erd uud Welt erkracht.", "tokens": ["Und", "ste\u00b7hest", "un\u00b7be\u00b7wegt", "wenn", "Erd", "u\u00b7ud", "Welt", "er\u00b7kracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KOUS", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Regiert nicht deine Hand die Ketten von dem Frieden?", "tokens": ["Re\u00b7giert", "nicht", "dei\u00b7ne", "Hand", "die", "Ket\u00b7ten", "von", "dem", "Frie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "PPOSAT", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.26": {"text": "Baust du die Liebe nicht sambt der Bescheidenheit?", "tokens": ["Baust", "du", "die", "Lie\u00b7be", "nicht", "sambt", "der", "Be\u00b7schei\u00b7den\u00b7heit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.27": {"text": "Wenn du die Bu\u00df erwegst; wird S\u00fcnde nicht vermieden?", "tokens": ["Wenn", "du", "die", "Bu\u00df", "er\u00b7wegst", ";", "wird", "S\u00fcn\u00b7de", "nicht", "ver\u00b7mie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$.", "VAFIN", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Des Fleisches Lust ged\u00e4mpfft/ der Geist davon befreyt?", "tokens": ["Des", "Flei\u00b7sches", "Lust", "ge\u00b7d\u00e4mpfft", "/", "der", "Geist", "da\u00b7von", "be\u00b7freyt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$(", "ART", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und hemmst du nicht die Zung und bindest unsre H\u00e4nde?", "tokens": ["Und", "hemmst", "du", "nicht", "die", "Zung", "und", "bin\u00b7dest", "uns\u00b7re", "H\u00e4n\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Heist du uns nicht getrost auch bey Versuchung seyn?", "tokens": ["Heist", "du", "uns", "nicht", "ge\u00b7trost", "auch", "bey", "Ver\u00b7su\u00b7chung", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "PTKNEG", "VVPP", "ADV", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und machst dem Aergern\u00fc\u00df und was uns kr\u00e4nckt ein Ende/", "tokens": ["Und", "machst", "dem", "A\u00b7er\u00b7ger\u00b7n\u00fc\u00df", "und", "was", "uns", "kr\u00e4nckt", "ein", "En\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "PWS", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Da\u00df wir gleich M\u00e4rtyrern den Himmel gehen ein?", "tokens": ["Da\u00df", "wir", "gleich", "M\u00e4r\u00b7ty\u00b7rern", "den", "Him\u00b7mel", "ge\u00b7hen", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Du bist der Armen Trost/ ein Ziel und Ma\u00df des Reichen/", "tokens": ["Du", "bist", "der", "Ar\u00b7men", "Trost", "/", "ein", "Ziel", "und", "Ma\u00df", "des", "Rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$(", "ART", "NN", "KON", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Dem Schwachen hilffst du auf/ dem Starcken da\u00df er bleibt", "tokens": ["Dem", "Schwa\u00b7chen", "hilffst", "du", "auf", "/", "dem", "Star\u00b7cken", "da\u00df", "er", "bleibt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "$(", "ART", "NN", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Du bist de\u00df Gl\u00e4ubigen erw\u00fcnschtes Freuden-Zeichen/", "tokens": ["Du", "bist", "de\u00df", "Gl\u00e4u\u00b7bi\u00b7gen", "er\u00b7w\u00fcnschtes", "Freu\u00b7den\u00b7Zei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Des Heyden Angetrieb/ da\u00df er sich dir verschreibt.", "tokens": ["Des", "Hey\u00b7den", "An\u00b7ge\u00b7trieb", "/", "da\u00df", "er", "sich", "dir", "ver\u00b7schreibt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KOUS", "PPER", "PRF", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Du ziehrst das Frauen-Volck und wirst am Mann gepriesen/", "tokens": ["Du", "ziehrst", "das", "Frau\u00b7en\u00b7Volck", "und", "wirst", "am", "Mann", "ge\u00b7prie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VAFIN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "An J\u00fcnglingen gelobt/ an Knaben hoch geliebt.", "tokens": ["An", "J\u00fcng\u00b7lin\u00b7gen", "ge\u00b7lobt", "/", "an", "Kna\u00b7ben", "hoch", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$(", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.39": {"text": "Es ist kein Alter nicht/ da\u00df dir nicht Ehr\u2019 erwiesen", "tokens": ["Es", "ist", "kein", "Al\u00b7ter", "nicht", "/", "da\u00df", "dir", "nicht", "Ehr'", "er\u00b7wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "PTKNEG", "$(", "KOUS", "PPER", "PTKNEG", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und das dir willig Raum und Wohnst\u00e4tt bey sich gibt.", "tokens": ["Und", "das", "dir", "wil\u00b7lig", "Raum", "und", "Wohn\u00b7st\u00e4tt", "bey", "sich", "gibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADJD", "NN", "KON", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Vornemlich hat\u2019 ihr Hertz ", "tokens": ["Vor\u00b7nem\u00b7lich", "hat'", "ihr", "Hertz"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.42": {"text": "Du mustest Tag und Nacht bey ihr Ge