{"textgrid.poem.54481": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht! klage Rachel nicht! ob gleich dir zarte Reben", "tokens": ["Nicht", "!", "kla\u00b7ge", "Ra\u00b7chel", "nicht", "!", "ob", "gleich", "dir", "zar\u00b7te", "Re\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "$.", "VVFIN", "NE", "PTKNEG", "$.", "KOUS", "ADV", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Kinder deiner Brust in Auffgang ihrer Zeit/", "tokens": ["Die", "Kin\u00b7der", "dei\u00b7ner", "Brust", "in", "Auff\u00b7gang", "ih\u00b7rer", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von mehr als grausem Sturm/ der Schwerter abgem\u00e4yt!", "tokens": ["Von", "mehr", "als", "grau\u00b7sem", "Sturm", "/", "der", "Schwer\u00b7ter", "ab\u00b7ge\u00b7m\u00e4yt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "KOKOM", "ADJA", "NN", "$(", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es ist so gantz nicht au\u00df! ach traure nicht! sie leben.", "tokens": ["Es", "ist", "so", "gantz", "nicht", "au\u00df", "!", "ach", "trau\u00b7re", "nicht", "!", "sie", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PTKNEG", "PTKVZ", "$.", "ADV", "VVFIN", "PTKNEG", "$.", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die L\u00e4mblein so ihr Blutt f\u00fcrs wehrte Lamb gegeben/", "tokens": ["Die", "L\u00e4mb\u00b7lein", "so", "ihr", "Blutt", "f\u00fcrs", "wehr\u00b7te", "Lamb", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "ADJD", "APPRART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sind itzt nach kurtzer Angst/ vnd kaum erkandtem Leid/", "tokens": ["Sind", "itzt", "nach", "kurt\u00b7zer", "Angst", "/", "vnd", "kaum", "er\u00b7kand\u00b7tem", "Leid", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "NN", "$(", "KON", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In dem besternten Sitz der grossen Herrligkeit/", "tokens": ["In", "dem", "bes\u00b7tern\u00b7ten", "Sitz", "der", "gros\u00b7sen", "Herr\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "In dem sie Gottes Rath/ vnd hohes Lob erheben.", "tokens": ["In", "dem", "sie", "Got\u00b7tes", "Rath", "/", "vnd", "ho\u00b7hes", "Lob", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "NN", "$(", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O selig/ wer noch eh der Mund kan Christum nennen", "tokens": ["O", "se\u00b7lig", "/", "wer", "noch", "eh", "der", "Mund", "kan", "Chris\u00b7tum", "nen\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "$(", "PWS", "ADV", "KOUS", "ART", "NN", "VMFIN", "NE", "VVINF"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Die Glieder vor ihn gibt/ wer au\u00df der Mutter Scho\u00df", "tokens": ["Die", "Glie\u00b7der", "vor", "ihn", "gibt", "/", "wer", "au\u00df", "der", "Mut\u00b7ter", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$(", "PWS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Marter Kron ergreifft/ vnd tritt ins Himmels Schlo\u00df!", "tokens": ["Die", "Mar\u00b7ter", "Kron", "er\u00b7greifft", "/", "vnd", "tritt", "ins", "Him\u00b7mels", "Schlo\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$(", "KON", "VVFIN", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "O selig/ wer noch/ eh er seinen Feind kan kennen", "tokens": ["O", "se\u00b7lig", "/", "wer", "noch", "/", "eh", "er", "sei\u00b7nen", "Feind", "kan", "ken\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "$(", "PWS", "ADV", "$(", "KOUS", "PPER", "PPOSAT", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Schon \u00fcberwunden hat/ wer/ eh er S\u00fcnde sp\u00fcrt/", "tokens": ["Schon", "\u00fc\u00b7berw\u00b7un\u00b7den", "hat", "/", "wer", "/", "eh", "er", "S\u00fcn\u00b7de", "sp\u00fcrt", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "$(", "PWS", "$(", "KOUS", "PPER", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vnd eh er wei\u00df was tod/ von beyden triumphirt!", "tokens": ["Vnd", "eh", "er", "wei\u00df", "was", "tod", "/", "von", "bey\u00b7den", "tri\u00b7um\u00b7phirt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "PIS", "NN", "$(", "APPR", "PIAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}