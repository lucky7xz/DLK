{"textgrid.poem.25059": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Johannisnacht", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der sel'ge Abend, als inmitten", "tokens": ["Der", "sel'\u00b7ge", "A\u00b7bend", ",", "als", "in\u00b7mit\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bekr\u00e4nzter Nachen wir im Kahn", "tokens": ["Be\u00b7kr\u00e4nz\u00b7ter", "Na\u00b7chen", "wir", "im", "Kahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hin an Sevillas G\u00e4rten glitten", "tokens": ["Hin", "an", "Se\u00b7vil\u00b7las", "G\u00e4r\u00b7ten", "glit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "NN", "VVFIN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Auf sanft bewegter Wellen Bahn!", "tokens": ["Auf", "sanft", "be\u00b7weg\u00b7ter", "Wel\u00b7len", "Bahn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hell leuchteten die Ufer alle", "tokens": ["Hell", "leuch\u00b7te\u00b7ten", "die", "U\u00b7fer", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von der Johannisfeuer Glanz,", "tokens": ["Von", "der", "Jo\u00b7han\u00b7nis\u00b7feu\u00b7er", "Glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es schwang beim Kastagnettenschalle", "tokens": ["Es", "schwang", "beim", "Kas\u00b7tag\u00b7net\u00b7ten\u00b7schal\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Menge sich im muntern Tanz.", "tokens": ["Die", "Men\u00b7ge", "sich", "im", "mun\u00b7tern", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf stiegen flatternde Raketen,", "tokens": ["Auf", "stie\u00b7gen", "flat\u00b7tern\u00b7de", "Ra\u00b7ke\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "R\u00fcckstrahlend in des Stromes Flut,", "tokens": ["R\u00fcck\u00b7strah\u00b7lend", "in", "des", "Stro\u00b7mes", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schossen durch den sternbes\u00e4ten", "tokens": ["Und", "schos\u00b7sen", "durch", "den", "stern\u00b7be\u00b7s\u00e4\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lichthimmel hin mit dunkler Glut.", "tokens": ["Licht\u00b7him\u00b7mel", "hin", "mit", "dunk\u00b7ler", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch s\u00fc\u00dfer war's, als fern dem Feste", "tokens": ["Doch", "s\u00fc\u00b7\u00dfer", "wa\u00b7r's", ",", "als", "fern", "dem", "Fes\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "$,", "KOUS", "ADJD", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ans Ufer uns die Barke trug,", "tokens": ["Ans", "U\u00b7fer", "uns", "die", "Bar\u00b7ke", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00fcber uns der gr\u00fcnen Aeste", "tokens": ["Und", "\u00fc\u00b7ber", "uns", "der", "gr\u00fc\u00b7nen", "A\u00b7es\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Geheime Nacht zusammenschlug.", "tokens": ["Ge\u00b7hei\u00b7me", "Nacht", "zu\u00b7sam\u00b7men\u00b7schlug", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Erst dort, wo d\u00e4mmernd aus den Zweigen", "tokens": ["Erst", "dort", ",", "wo", "d\u00e4m\u00b7mernd", "aus", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "PWAV", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schimmer der Limonen quoll,", "tokens": ["Der", "Schim\u00b7mer", "der", "Li\u00b7mo\u00b7nen", "quoll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erschlo\u00df in Dunkel und in Schweigen", "tokens": ["Er\u00b7schlo\u00df", "in", "Dun\u00b7kel", "und", "in", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich unsre Wonne ganz und voll.", "tokens": ["Sich", "uns\u00b7re", "Won\u00b7ne", "ganz", "und", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O, da\u00df es oft noch so uns nachte!", "tokens": ["O", ",", "da\u00df", "es", "oft", "noch", "so", "uns", "nach\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "ADV", "ADV", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch jetzt auch la\u00df uns dankbar sein", "tokens": ["Doch", "jetzt", "auch", "la\u00df", "uns", "dank\u00b7bar", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVIMP", "PPER", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, weil er uns so treu bewachte,", "tokens": ["Und", ",", "weil", "er", "uns", "so", "treu", "be\u00b7wach\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PPER", "ADV", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem T\u00e4ufer eine Kerze weihn.", "tokens": ["Dem", "T\u00e4u\u00b7fer", "ei\u00b7ne", "Ker\u00b7ze", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}