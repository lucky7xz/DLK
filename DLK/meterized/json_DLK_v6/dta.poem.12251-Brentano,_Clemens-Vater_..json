{"dta.poem.12251": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Vater .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es fuhr ein M\u00e4gdlein \u00fcbern See,               ", "tokens": ["Es", "fuhr", "ein", "M\u00e4gd\u00b7lein", "\u00fc\u00b7bern", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wolt brechen den Feiel und gr\u00fcnen Klee,", "tokens": ["Wolt", "bre\u00b7chen", "den", "Fei\u00b7el", "und", "gr\u00fc\u00b7nen", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "ART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Mit ihrn schneweissen H\u00e4nden,", "tokens": ["Mit", "ihrn", "schne\u00b7weis\u00b7sen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Sommer hat schier ein Ende.", "tokens": ["Der", "Som\u00b7mer", "hat", "schier", "ein", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein Ritter kam dort her geritten,", "tokens": ["Ein", "Rit\u00b7ter", "kam", "dort", "her", "ge\u00b7rit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APZR", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er gr\u00fc\u00dfte sie nach Schw\u00e4bschen Sitten,", "tokens": ["Er", "gr\u00fc\u00df\u00b7te", "sie", "nach", "Schw\u00e4b\u00b7schen", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er gr\u00fc\u00dft sie da alleine:", "tokens": ["Er", "gr\u00fc\u00dft", "sie", "da", "al\u00b7lei\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eich f\u00fchr euch mit mir heime.\u201c", "tokens": ["\u201e", "ich", "f\u00fchr", "euch", "mit", "mir", "hei\u00b7me", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "APPR", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201each Ritter, ihr seyd hochgeborn,", "tokens": ["\u201e", "ach", "Rit\u00b7ter", ",", "ihr", "seyd", "hoch\u00b7ge\u00b7born", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "$,", "PPER", "VAFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eso f\u00fcrcht ich meines Vaters Zorn,", "tokens": ["\u201e", "so", "f\u00fcrcht", "ich", "mei\u00b7nes", "Va\u00b7ters", "Zorn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eich f\u00fcrcht ihn alzusehre,", "tokens": ["\u201e", "ich", "f\u00fcrcht", "ihn", "al\u00b7zu\u00b7seh\u00b7re", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201everliere vielleicht mein Ehre.", "tokens": ["\u201e", "ver\u00b7lie\u00b7re", "viel\u00b7leicht", "mein", "Eh\u00b7re", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "\u201each Vater lieber Vater mein,", "tokens": ["\u201e", "ach", "Va\u00b7ter", "lie\u00b7ber", "Va\u00b7ter", "mein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "ADV", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eso weck mich bei dem Mondeschein,", "tokens": ["\u201e", "so", "weck", "mich", "bei", "dem", "Mon\u00b7de\u00b7schein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVIMP", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eich wei\u00df gut L\u00e4mmer-Weide,", "tokens": ["\u201e", "ich", "wei\u00df", "gut", "L\u00e4m\u00b7mer\u00b7Wei\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eso fern auf jener Haide.\u201c", "tokens": ["\u201e", "so", "fern", "auf", "je\u00b7ner", "Hai\u00b7de", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADJD", "APPR", "PDAT", "NE", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u201edie L\u00e4mmerweid die du wohl weist,", "tokens": ["\u201e", "die", "L\u00e4m\u00b7mer\u00b7weid", "die", "du", "wohl", "weist", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ART", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201emacht mir mein L\u00e4mmer und Schaf nicht feist,", "tokens": ["\u201e", "macht", "mir", "mein", "L\u00e4m\u00b7mer", "und", "Schaf", "nicht", "feist", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "PTKNEG", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "\u201edu must hier heime bleiben,", "tokens": ["\u201e", "du", "must", "hier", "hei\u00b7me", "blei\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADV", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201emust spinnen die braune Seiden.\u201c", "tokens": ["\u201e", "must", "spin\u00b7nen", "die", "brau\u00b7ne", "Sei\u00b7den", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VMFIN", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "\u201edie Seide, die ich spinnen mu\u00df,", "tokens": ["\u201e", "die", "Sei\u00b7de", ",", "die", "ich", "spin\u00b7nen", "mu\u00df", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ebringt meinem Herzen schwere Bu\u00df,", "tokens": ["\u201e", "bringt", "mei\u00b7nem", "Her\u00b7zen", "schwe\u00b7re", "Bu\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eder Ritter mu\u00df mir werden,", "tokens": ["\u201e", "der", "Rit\u00b7ter", "mu\u00df", "mir", "wer\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201esein gleich lebt nicht auf Erden.\u201c", "tokens": ["\u201e", "sein", "gleich", "lebt", "nicht", "auf", "Er\u00b7den", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "ADV", "VVFIN", "PTKNEG", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der die\u00df Lied neu gesungen hat,", "tokens": ["Der", "die\u00df", "Lied", "neu", "ge\u00b7sun\u00b7gen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "NN", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Durch Lieb kam er in gro\u00dfe Noth,", "tokens": ["Durch", "Lieb", "kam", "er", "in", "gro\u00b7\u00dfe", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er ist gar kaum entronnen,", "tokens": ["Er", "ist", "gar", "kaum", "ent\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Magd hat er gewonnen.", "tokens": ["Die", "Magd", "hat", "er", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}