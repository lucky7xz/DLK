{"textgrid.poem.67228": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: S\u00fc\u00dfer duftet die Flur, und k\u00fchler hauchet der Abend;", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00dfer duftet die Flur, und k\u00fchler hauchet der Abend;", "tokens": ["S\u00fc\u00b7\u00dfer", "duf\u00b7tet", "die", "Flur", ",", "und", "k\u00fch\u00b7ler", "hau\u00b7chet", "der", "A\u00b7bend", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Nur ein welkendes Roth weilt am azurenen West.", "tokens": ["Nur", "ein", "wel\u00b7ken\u00b7des", "Roth", "weilt", "am", "a\u00b7zu\u00b7re\u00b7nen", "West", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "--+--++-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Stille thauet herab, und Ruh', und sanfte Begeist'rung", "tokens": ["Stil\u00b7le", "thau\u00b7et", "her\u00b7ab", ",", "und", "Ruh'", ",", "und", "sanf\u00b7te", "Be\u00b7geist'\u00b7rung"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "$,", "KON", "NN", "$,", "KON", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Auf den einsamen Pfad, welchen der Waller betritt.", "tokens": ["Auf", "den", "ein\u00b7sa\u00b7men", "Pfad", ",", "wel\u00b7chen", "der", "Wal\u00b7ler", "be\u00b7tritt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PWAT", "ART", "NN", "VVFIN", "$."], "meter": "---+-+---+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Hesperus schaut auf ihn mit freundlichen Blicken hernieder,", "tokens": ["Hes\u00b7pe\u00b7rus", "schaut", "auf", "ihn", "mit", "freund\u00b7li\u00b7chen", "Bli\u00b7cken", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "---+---+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Lispelt segnend ihm zu: Geh' in Frieden dahin!", "tokens": ["Lis\u00b7pelt", "seg\u00b7nend", "ihm", "zu", ":", "Geh'", "in", "Frie\u00b7den", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "PPER", "PTKVZ", "$.", "NE", "APPR", "NN", "PAV", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.7": {"text": "Ich auch wander' umher, und such' auf einsamen Pfaden", "tokens": ["Ich", "auch", "wan\u00b7der'", "um\u00b7her", ",", "und", "such'", "auf", "ein\u00b7sa\u00b7men", "Pfa\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Ruh' und lindernden Trost f\u00fcr mein sinkendes Herz.", "tokens": ["Ruh'", "und", "lin\u00b7dern\u00b7den", "Trost", "f\u00fcr", "mein", "sin\u00b7ken\u00b7des", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Ach vergebens! \u2013 O du der besten J\u00fcnglinge Bester,", "tokens": ["Ach", "ver\u00b7ge\u00b7bens", "!", "\u2013", "O", "du", "der", "bes\u00b7ten", "J\u00fcng\u00b7lin\u00b7ge", "Bes\u00b7ter", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "$(", "NE", "NE", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+--+-++-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Den ich liebe, so sehr, als ich zu lieben vermag;", "tokens": ["Den", "ich", "lie\u00b7be", ",", "so", "sehr", ",", "als", "ich", "zu", "lie\u00b7ben", "ver\u00b7mag", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "$,", "ADV", "ADV", "$,", "KOUS", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Dem die milde Natur der Gaben sch\u00f6nste, die selten", "tokens": ["Dem", "die", "mil\u00b7de", "Na\u00b7tur", "der", "Ga\u00b7ben", "sch\u00f6ns\u00b7te", ",", "die", "sel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,", "PRELS", "ADJD"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Sie verleiht, ein Herz zarter Empfindung, verlieh;", "tokens": ["Sie", "ver\u00b7leiht", ",", "ein", "Herz", "zar\u00b7ter", "Emp\u00b7fin\u00b7dung", ",", "ver\u00b7lieh", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "--+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Den sie der Freundschaft schuf, der Lieb', und stilleren Freuden;", "tokens": ["Den", "sie", "der", "Freund\u00b7schaft", "schuf", ",", "der", "Lieb'", ",", "und", "stil\u00b7le\u00b7ren", "Freu\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,", "ART", "NN", "$,", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Sanfte Melancholie, deine Feindinnen nicht!", "tokens": ["Sanf\u00b7te", "Me\u00b7lan\u00b7cho\u00b7lie", ",", "dei\u00b7ne", "Fein\u00b7din\u00b7nen", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.15": {"text": "Ach! du windest dich los aus deines Freundes Umarmung;", "tokens": ["Ach", "!", "du", "win\u00b7dest", "dich", "los", "aus", "dei\u00b7nes", "Freun\u00b7des", "Um\u00b7ar\u00b7mung", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Scheidest z\u00f6gernd von ihm \u2013 ach! auf ewig vielleicht! \u2013 \u2013", "tokens": ["Schei\u00b7dest", "z\u00f6\u00b7gernd", "von", "ihm", "\u2013", "ach", "!", "auf", "e\u00b7wig", "viel\u00b7leicht", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "$(", "ITJ", "$.", "APPR", "ADJD", "ADV", "$.", "$(", "$("], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.17": {"text": "Also sind sie dahin, der Freundschaft heilige Jahre,", "tokens": ["Al\u00b7so", "sind", "sie", "da\u00b7hin", ",", "der", "Freund\u00b7schaft", "hei\u00b7li\u00b7ge", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PAV", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.18": {"text": "Deren jeglicher Tag fester und fester uns band?", "tokens": ["De\u00b7ren", "jeg\u00b7li\u00b7cher", "Tag", "fes\u00b7ter", "und", "fes\u00b7ter", "uns", "band", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIAT", "NN", "ADJD", "KON", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Also sind sie verbl\u00fcht die Veilchen, welche mir oftmahl", "tokens": ["Al\u00b7so", "sind", "sie", "ver\u00b7bl\u00fcht", "die", "Veil\u00b7chen", ",", "wel\u00b7che", "mir", "oft\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV"], "meter": "---+-+-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.20": {"text": "Deine gef\u00e4llige Hand streut' in den m\u00fchsamen Weg?", "tokens": ["Dei\u00b7ne", "ge\u00b7f\u00e4l\u00b7li\u00b7ge", "Hand", "streut'", "in", "den", "m\u00fch\u00b7sa\u00b7men", "Weg", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.21": {"text": "Nein! sie sind nicht verbl\u00fcht! In jeder heiteren Stunde", "tokens": ["Nein", "!", "sie", "sind", "nicht", "ver\u00b7bl\u00fcht", "!", "In", "je\u00b7der", "hei\u00b7te\u00b7ren", "Stun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "PTKNEG", "VVPP", "$.", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Kehrt mir l\u00e4chelnd zur\u00fcck jede genossene Lust.", "tokens": ["Kehrt", "mir", "l\u00e4\u00b7chelnd", "zu\u00b7r\u00fcck", "je\u00b7de", "ge\u00b7nos\u00b7se\u00b7ne", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "PTKVZ", "PIAT", "ADJA", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.23": {"text": "O dann sollen mich oft Phantome der Abend' umschweben,", "tokens": ["O", "dann", "sol\u00b7len", "mich", "oft", "Phan\u00b7to\u00b7me", "der", "A\u00b7bend'", "um\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VMFIN", "PPER", "ADV", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.24": {"text": "Die, uns jeglichesmahl t\u00e4uschend, zu fl\u00fcchtig entflohn!", "tokens": ["Die", ",", "uns", "jeg\u00b7li\u00b7ches\u00b7mahl", "t\u00e4u\u00b7schend", ",", "zu", "fl\u00fcch\u00b7tig", "ent\u00b7flohn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PPER", "ADV", "ADJD", "$,", "PTKA", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "Jetzo wanderten wir, mit Fr\u00fchlingsruhe gesegnet,", "tokens": ["Jet\u00b7zo", "wan\u00b7der\u00b7ten", "wir", ",", "mit", "Fr\u00fch\u00b7lings\u00b7ru\u00b7he", "ge\u00b7seg\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-----+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Arm geschlungen in Arm, bl\u00fchende Th\u00e4ler hinab;", "tokens": ["Arm", "ge\u00b7schlun\u00b7gen", "in", "Arm", ",", "bl\u00fc\u00b7hen\u00b7de", "Th\u00e4\u00b7ler", "hin\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "NN", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Lagerten jetzo uns hin am moosigen Ufer des Baches,", "tokens": ["La\u00b7ger\u00b7ten", "jet\u00b7zo", "uns", "hin", "am", "moo\u00b7si\u00b7gen", "U\u00b7fer", "des", "Ba\u00b7ches", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ADV", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.28": {"text": "Und dem s\u00fc\u00dfen Geschw\u00e4tz horchte vertraulich der Mond.", "tokens": ["Und", "dem", "s\u00fc\u00b7\u00dfen", "Ge\u00b7schw\u00e4tz", "horch\u00b7te", "ver\u00b7trau\u00b7lich", "der", "Mond", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.29": {"text": "O, wie schmolz uns dann das Herz in sanfter Empfindung!", "tokens": ["O", ",", "wie", "schmolz", "uns", "dann", "das", "Herz", "in", "sanf\u00b7ter", "Emp\u00b7fin\u00b7dung", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADJD", "PPER", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.30": {"text": "O, wie schmeckten wir dich, himmlische Freundschaft, so s\u00fc\u00df!", "tokens": ["O", ",", "wie", "schmeck\u00b7ten", "wir", "dich", ",", "himm\u00b7li\u00b7sche", "Freund\u00b7schaft", ",", "so", "s\u00fc\u00df", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "PRF", "$,", "ADJA", "NN", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Einstens pfl\u00fcckt' ich zwo junge Vergi\u00dfmeinnicht, und streute,", "tokens": ["Eins\u00b7tens", "pfl\u00fcckt'", "ich", "zwo", "jun\u00b7ge", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", ",", "und", "streu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "CARD", "ADJA", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "--+--+--+-+-+-", "measure": "anapaest.tri.plus"}, "line.32": {"text": "Wo am kl\u00e4rsten er flo\u00df, sie in den kr\u00e4uselnden Bach.", "tokens": ["Wo", "am", "kl\u00e4rs\u00b7ten", "er", "flo\u00df", ",", "sie", "in", "den", "kr\u00e4u\u00b7seln\u00b7den", "Bach", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "PPER", "VVFIN", "$,", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.33": {"text": "Eine ri\u00df er hinweg; die andere weilt' am Ufer!", "tokens": ["Ei\u00b7ne", "ri\u00df", "er", "hin\u00b7weg", ";", "die", "an\u00b7de\u00b7re", "weilt'", "am", "U\u00b7fer", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "ADJA", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+--+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.34": {"text": "Und du starrtest mich an; Thr\u00e4nen bew\u00f6lkten den Blick!", "tokens": ["Und", "du", "starr\u00b7test", "mich", "an", ";", "Thr\u00e4\u00b7nen", "be\u00b7w\u00f6lk\u00b7ten", "den", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Ich verstand dich! Auch mich ergriff der b\u00e4ngste Gedanke:", "tokens": ["Ich", "ver\u00b7stand", "dich", "!", "Auch", "mich", "er\u00b7griff", "der", "b\u00e4ngs\u00b7te", "Ge\u00b7dan\u00b7ke", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "ADV", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.36": {"text": "Ach! wenn einst das Geschick uns wie die Blumen verstreut!", "tokens": ["Ach", "!", "wenn", "einst", "das", "Ge\u00b7schick", "uns", "wie", "die", "Blu\u00b7men", "ver\u00b7streut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "ADV", "ART", "NN", "PPER", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.37": {"text": "So schlich Wehmuth oft in unsere Freuden; so sprosset", "tokens": ["So", "schlich", "Weh\u00b7muth", "oft", "in", "un\u00b7se\u00b7re", "Freu\u00b7den", ";", "so", "spros\u00b7set"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "NN", "ADV", "APPR", "PPOSAT", "NN", "$.", "ADV", "VVFIN"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "In dem Myrtengeb\u00fcsch' eine Zypresse mit auf.", "tokens": ["In", "dem", "Myr\u00b7ten\u00b7ge\u00b7b\u00fcsch'", "ei\u00b7ne", "Zyp\u00b7res\u00b7se", "mit", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "APPR", "PTKVZ", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.39": {"text": "Oftmahl standen wir still am schroffen Hange des Felsen,", "tokens": ["Oft\u00b7mahl", "stan\u00b7den", "wir", "still", "am", "schrof\u00b7fen", "Han\u00b7ge", "des", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.40": {"text": "M\u00fcden Pilgern gleich, \u00fcber die St\u00e4be gelehnt;", "tokens": ["M\u00fc\u00b7den", "Pil\u00b7gern", "gleich", ",", "\u00fc\u00b7ber", "die", "St\u00e4\u00b7be", "ge\u00b7lehnt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,", "APPR", "ART", "NN", "VVPP", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.41": {"text": "Und umh\u00fcllte mich dann der Nebel der schwarzen Schwermuth,", "tokens": ["Und", "um\u00b7h\u00fcll\u00b7te", "mich", "dann", "der", "Ne\u00b7bel", "der", "schwar\u00b7zen", "Schwer\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+-++-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.42": {"text": "O so sch\u00fcttet' ich, Freund, dir in das deine mein Herz!", "tokens": ["O", "so", "sch\u00fct\u00b7tet'", "ich", ",", "Freund", ",", "dir", "in", "das", "dei\u00b7ne", "mein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "$,", "NN", "$,", "PPER", "APPR", "ART", "PPOSAT", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.43": {"text": "Seufzend h\u00f6rtest du mich, und jede Sorge, die theilend", "tokens": ["Seuf\u00b7zend", "h\u00f6r\u00b7test", "du", "mich", ",", "und", "je\u00b7de", "Sor\u00b7ge", ",", "die", "thei\u00b7lend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "$,", "KON", "PIAT", "NN", "$,", "PRELS", "ADJD"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.44": {"text": "Du mir nahmest, erhob meine beklommene Brust!", "tokens": ["Du", "mir", "nah\u00b7mest", ",", "er\u00b7hob", "mei\u00b7ne", "be\u00b7klom\u00b7me\u00b7ne", "Brust", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVFIN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.45": {"text": "Phantasie, wo gaukelst du hin? \u2013 O Bester, nun leichterst", "tokens": ["Phan\u00b7ta\u00b7sie", ",", "wo", "gau\u00b7kelst", "du", "hin", "?", "\u2013", "O", "Bes\u00b7ter", ",", "nun", "leich\u00b7terst"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "NE", "NN", "$,", "ADV", "ADV"], "meter": "-+-++--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.46": {"text": "Du nicht wieder die Last meiner beklommenen Brust!", "tokens": ["Du", "nicht", "wie\u00b7der", "die", "Last", "mei\u00b7ner", "be\u00b7klom\u00b7me\u00b7nen", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.47": {"text": "Ach, nun fliehst du! Verweil'! da\u00df in der letzten Umarmung", "tokens": ["Ach", ",", "nun", "fliehst", "du", "!", "Ver\u00b7weil'", "!", "da\u00df", "in", "der", "letz\u00b7ten", "Um\u00b7ar\u00b7mung"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "$.", "NN", "$.", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.48": {"text": "Eine Thr\u00e4ne nur noch misch' in die meinige sich.", "tokens": ["Ei\u00b7ne", "Thr\u00e4\u00b7ne", "nur", "noch", "misch'", "in", "die", "mei\u00b7ni\u00b7ge", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADJD", "APPR", "ART", "PPOSS", "PRF", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.49": {"text": "Segen geleite dich, Freund! O sei der Liebling des Gl\u00fcckes,", "tokens": ["Se\u00b7gen", "ge\u00b7lei\u00b7te", "dich", ",", "Freund", "!", "O", "sei", "der", "Lieb\u00b7ling", "des", "Gl\u00fc\u00b7ckes", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "NN", "$.", "NE", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.50": {"text": "Jenes reineren Gl\u00fcck's, welches der Weise nur kennt;", "tokens": ["Je\u00b7nes", "rei\u00b7ne\u00b7ren", "Gl\u00fc\u00b7ck's", ",", "wel\u00b7ches", "der", "Wei\u00b7se", "nur", "kennt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+--+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.51": {"text": "Sei de\u00df Liebling, wie du der menschenfreundlichen Tugend", "tokens": ["Sei", "de\u00df", "Lieb\u00b7ling", ",", "wie", "du", "der", "men\u00b7schen\u00b7freund\u00b7li\u00b7chen", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.52": {"text": "Und der Weisheit es bist! Segen geleite dich, Freund!", "tokens": ["Und", "der", "Weis\u00b7heit", "es", "bist", "!", "Se\u00b7gen", "ge\u00b7lei\u00b7te", "dich", ",", "Freu