{"dta.poem.10818": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Unverhoft kommt oft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist der Ausspruch wahr, da\u00df unverhoft\nerscheint,", "tokens": ["Es", "ist", "der", "Aus\u00b7spruch", "wahr", ",", "da\u00df", "un\u00b7ver\u00b7hoft", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKVZ", "$,", "KOUS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das was man nicht gehoft, und was", "tokens": ["Das", "was", "man", "nicht", "ge\u00b7hoft", ",", "und", "was"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "PRELS", "PIS", "PTKNEG", "VVPP", "$,", "KON", "PWS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So woll in gl\u00fcklichen, als auch in Un-", "tokens": ["So", "woll", "in", "gl\u00fck\u00b7li\u00b7chen", ",", "als", "auch", "in", "Un"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "$,", "KOUS", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kommt oft dasjenige, was wir uns nicht vorstellen.", "tokens": ["Kommt", "oft", "das\u00b7je\u00b7ni\u00b7ge", ",", "was", "wir", "uns", "nicht", "vor\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PDS", "$,", "PRELS", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Ursach davon ist, weil wir nicht allzeit sehn,", "tokens": ["Die", "Ur\u00b7sach", "da\u00b7von", "ist", ",", "weil", "wir", "nicht", "all\u00b7zeit", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VAFIN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Folgen solcher Ding die in der Welt entstehn.", "tokens": ["Die", "Fol\u00b7gen", "sol\u00b7cher", "Ding", "die", "in", "der", "Welt", "ent\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "ART", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Des Geistes Bl\u00f6digkeit, das irdische Get\u00fcmmel,", "tokens": ["Des", "Geis\u00b7tes", "Bl\u00f6\u00b7dig\u00b7keit", ",", "das", "ir\u00b7di\u00b7sche", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und auch bisweilen selbst der Rathschlus den im Him-", "tokens": ["Und", "auch", "bis\u00b7wei\u00b7len", "selbst", "der", "Rath\u00b7schlus", "den", "im", "Him"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADV", "ART", "NN", "ART", "APPRART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die ewge Vorsicht macht, behindert uns daran,", "tokens": ["Die", "ew\u00b7ge", "Vor\u00b7sicht", "macht", ",", "be\u00b7hin\u00b7dert", "uns", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das man nicht jederzeit zum voraus sehen kan,", "tokens": ["Das", "man", "nicht", "je\u00b7der\u00b7zeit", "zum", "vo\u00b7raus", "se\u00b7hen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "PTKNEG", "ADV", "APPRART", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was uns begegnen wird. Es ist uns stets verbor-", "tokens": ["Was", "uns", "be\u00b7geg\u00b7nen", "wird", ".", "Es", "ist", "uns", "stets", "ver\u00b7bor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVINF", "VAFIN", "$.", "PPER", "VAFIN", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was uns begegnen kan, auch an den nechsten Mor-", "tokens": ["Was", "uns", "be\u00b7geg\u00b7nen", "kan", ",", "auch", "an", "den", "nechs\u00b7ten", "Mor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "$,", "ADV", "APPR", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das Schiksahl da\u00df der Mensch zulezt zu hoffen hat,", "tokens": ["Das", "Schik\u00b7sahl", "da\u00df", "der", "Mensch", "zu\u00b7lezt", "zu", "hof\u00b7fen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "ART", "NN", "ADV", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das ist der Tod der kommt bald fr\u00fch, bald aber", "tokens": ["Das", "ist", "der", "Tod", "der", "kommt", "bald", "fr\u00fch", ",", "bald", "a\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Das Sterben ist gewis nach jenem alten Bunde,", "tokens": ["Das", "Ster\u00b7ben", "ist", "ge\u00b7wis", "nach", "je\u00b7nem", "al\u00b7ten", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Doch es bleibt ungewis die allerlezte Stunde,", "tokens": ["Doch", "es", "bleibt", "un\u00b7ge\u00b7wis", "die", "al\u00b7ler\u00b7lez\u00b7te", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Da unsre Lebens Uhr, die sich best\u00e4ndig dreht,", "tokens": ["Da", "uns\u00b7re", "Le\u00b7bens", "Uhr", ",", "die", "sich", "be\u00b7st\u00e4n\u00b7dig", "dreht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Bei dem erstarrten Blut im Pulsschlag stille steht:", "tokens": ["Bei", "dem", "er\u00b7starr\u00b7ten", "Blut", "im", "Puls\u00b7schlag", "stil\u00b7le", "steht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Sie kommt oft unverhoft. Es sind viel tausend Ar-", "tokens": ["Sie", "kommt", "oft", "un\u00b7ver\u00b7hoft", ".", "Es", "sind", "viel", "tau\u00b7send", "Ar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$.", "PPER", "VAFIN", "ADV", "CARD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da wir des Lebens End, das pl\u00f6zlich, nicht erwar-", "tokens": ["Da", "wir", "des", "Le\u00b7bens", "End", ",", "das", "pl\u00f6z\u00b7lich", ",", "nicht", "er\u00b7wa\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "$,", "PRELS", "ADJD", "$,", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Exempel sind genug in alt und neuer Zeit,", "tokens": ["Ex\u00b7em\u00b7pel", "sind", "ge\u00b7nug", "in", "alt", "und", "neu\u00b7er", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Die sezzen dieses fest, ohn allen Wiederstreit.", "tokens": ["Die", "sez\u00b7zen", "die\u00b7ses", "fest", ",", "ohn", "al\u00b7len", "Wie\u00b7der\u00b7streit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "PTKVZ", "$,", "KOUI", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Als der Held Bibulus sich im Triumph erg\u00f6zte,", "tokens": ["Als", "der", "Held", "Bi\u00b7bu\u00b7lus", "sich", "im", "Tri\u00b7umph", "er\u00b7g\u00f6z\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NE", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+--++-+--+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "So fiel ein Stein vom Dach der ihm gleich tod ver-", "tokens": ["So", "fiel", "ein", "Stein", "vom", "Dach", "der", "ihm", "gleich", "tod", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "ART", "PPER", "ADV", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Ein Valla trinket Meth; setzt kaum den Becher an;", "tokens": ["Ein", "Val\u00b7la", "trin\u00b7ket", "Me\u00b7th", ";", "setzt", "kaum", "den", "Be\u00b7cher", "an", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "NE", "$.", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "So trinkt er seinen Tod; und der gerechte Mann", "tokens": ["So", "trinkt", "er", "sei\u00b7nen", "Tod", ";", "und", "der", "ge\u00b7rech\u00b7te", "Mann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der grosse Baldus starb durch einem Bis vom Hun-", "tokens": ["Der", "gros\u00b7se", "Bal\u00b7dus", "starb", "durch", "ei\u00b7nem", "Bis", "vom", "Hun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "APPR", "APPRART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dem er in Schoos gen\u00e4hrt zu einer solchen Stunde,", "tokens": ["Dem", "er", "in", "Schoos", "ge\u00b7n\u00e4hrt", "zu", "ei\u00b7ner", "sol\u00b7chen", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVPP", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Da er es nie gedacht. Und jener Appius,", "tokens": ["Da", "er", "es", "nie", "ge\u00b7dacht", ".", "Und", "je\u00b7ner", "Ap\u00b7pius", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVPP", "$.", "KON", "PDAT", "NE", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.30": {"text": "Der i\u00dft ein weiches Ey, dran er erstikken mu\u00df.", "tokens": ["Der", "i\u00dft", "ein", "wei\u00b7ches", "Ey", ",", "dran", "er", "ers\u00b7tik\u00b7ken", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,", "PAV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wie viele hat ein Schlag im Augenblik ger\u00fchret", "tokens": ["Wie", "vie\u00b7le", "hat", "ein", "Schlag", "im", "Au\u00b7gen\u00b7blik", "ge\u00b7r\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VAFIN", "ART", "NN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und schnelle aus der Zeit zur Ewigkeit gef\u00fchret?", "tokens": ["Und", "schnel\u00b7le", "aus", "der", "Zeit", "zur", "E\u00b7wig\u00b7keit", "ge\u00b7f\u00fch\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wie viele hat ein Bliz durch seinen Dunst erstikt?", "tokens": ["Wie", "vie\u00b7le", "hat", "ein", "Bliz", "durch", "sei\u00b7nen", "Dunst", "ers\u00b7tikt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wie viele sind durchs Schwerd ganz unverhoft zer-", "tokens": ["Wie", "vie\u00b7le", "sind", "durchs", "Schwerd", "ganz", "un\u00b7ver\u00b7hoft", "zer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VAFIN", "APPRART", "NN", "ADV", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Wie viele sind im Fall, im Todesschlaf versunken,", "tokens": ["Wie", "vie\u00b7le", "sind", "im", "Fall", ",", "im", "To\u00b7des\u00b7schlaf", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "APPRART", "NN", "$,", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wie viele unverhoft in Wassers Noth vertrunken:", "tokens": ["Wie", "vie\u00b7le", "un\u00b7ver\u00b7hoft", "in", "Was\u00b7sers", "Noth", "ver\u00b7trun\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wie viele sind im Feur, in Dampf und Glut ver-", "tokens": ["Wie", "vie\u00b7le", "sind", "im", "Feur", ",", "in", "Dampf", "und", "Glut", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VAFIN", "APPRART", "NN", "$,", "APPR", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Wie viele durch den Zahn der wilden Thier verzehrt,", "tokens": ["Wie", "vie\u00b7le", "durch", "den", "Zahn", "der", "wil\u00b7den", "Thier", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die solches nie gedacht? Es kann gar leicht gesche-", "tokens": ["Die", "sol\u00b7ches", "nie", "ge\u00b7dacht", "?", "Es", "kann", "gar", "leicht", "ge\u00b7sche"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "ADV", "VVPP", "$.", "PPER", "VMFIN", "ADV", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Das wir den Tod sehr schnell in seinen Rachen ge-", "tokens": ["Das", "wir", "den", "Tod", "sehr", "schnell", "in", "sei\u00b7nen", "Ra\u00b7chen", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wer klug ist denkt daran, und machet sich bereit,", "tokens": ["Wer", "klug", "ist", "denkt", "da\u00b7ran", ",", "und", "ma\u00b7chet", "sich", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "VVFIN", "PAV", "$,", "KON", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Auf eine k\u00fcnftige gewisse Ewigkeit;", "tokens": ["Auf", "ei\u00b7ne", "k\u00fcnf\u00b7ti\u00b7ge", "ge\u00b7wis\u00b7se", "E\u00b7wig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Wer in Bereitschaft lebt, der kan nichts mehr ver-", "tokens": ["Wer", "in", "Be\u00b7reit\u00b7schaft", "lebt", ",", "der", "kan", "nichts", "mehr", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "VVFIN", "$,", "PRELS", "VMFIN", "PIS", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Als da\u00df er pl\u00f6zlich werd in Todes Garn gefangen.", "tokens": ["Als", "da\u00df", "er", "pl\u00f6z\u00b7lich", "werd", "in", "To\u00b7des", "Garn", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "VAFIN", "APPR", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "So ist ein schneller Tod, ein unverhoft Geschik,", "tokens": ["So", "ist", "ein", "schnel\u00b7ler", "Tod", ",", "ein", "un\u00b7ver\u00b7hoft", "Ge\u00b7schik", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Uns kein betr\u00fcbter Fall, vielmehr ein grosses Gl\u00fck.", "tokens": ["Uns", "kein", "be\u00b7tr\u00fcb\u00b7ter", "Fall", ",", "viel\u00b7mehr", "ein", "gros\u00b7ses", "Gl\u00fck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Die Vorsicht hat darum das Ende uns verborgen,", "tokens": ["Die", "Vor\u00b7sicht", "hat", "da\u00b7rum", "das", "En\u00b7de", "uns", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Da\u00df wir zu jederzeit vor dessen Ankunft sorgen.", "tokens": ["Da\u00df", "wir", "zu", "je\u00b7der\u00b7zeit", "vor", "des\u00b7sen", "An\u00b7kunft", "sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "APPR", "PRELAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Die Weisheit zeiget uns auch darin ihren Rath,", "tokens": ["Die", "Weis\u00b7heit", "zei\u00b7get", "uns", "auch", "da\u00b7rin", "ih\u00b7ren", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Da\u00df sie uns nicht vorher die Zeit bestimmet hat,", "tokens": ["Da\u00df", "sie", "uns", "nicht", "vor\u00b7her", "die", "Zeit", "be\u00b7stim\u00b7met", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "ADV", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Noch auch die Todes Art; damit wir ohne Zagen,", "tokens": ["Noch", "auch", "die", "To\u00b7des", "Art", ";", "da\u00b7mit", "wir", "oh\u00b7ne", "Za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$.", "KOUS", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Und einer innren Angst nicht vorher dr\u00fcber klagen.", "tokens": ["Und", "ei\u00b7ner", "inn\u00b7ren", "Angst", "nicht", "vor\u00b7her", "dr\u00fc\u00b7ber", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKNEG", "ADV", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Man meinet zwar es sei viel besser, wenn mans weis;", "tokens": ["Man", "mei\u00b7net", "zwar", "es", "sei", "viel", "bes\u00b7ser", ",", "wenn", "mans", "weis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PPER", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Allein was unbekandt das machet uns nicht hei\u00df.", "tokens": ["Al\u00b7lein", "was", "un\u00b7be\u00b7kandt", "das", "ma\u00b7chet", "uns", "nicht", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ADJD", "PDS", "VVFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Ein unverhoftes Gl\u00fck erwekt viel gr\u00f6\u00dfre Freude,", "tokens": ["Ein", "un\u00b7ver\u00b7hof\u00b7tes", "Gl\u00fck", "er\u00b7wekt", "viel", "gr\u00f6\u00df\u00b7re", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Was unerwartet k\u00f6mmt ist auch gut bei dem Leide,", "tokens": ["Was", "un\u00b7er\u00b7war\u00b7tet", "k\u00f6mmt", "ist", "auch", "gut", "bei", "dem", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "VAFIN", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.57": {"text": "Das Unverhoft kommt oft, und bringet uns viel", "tokens": ["Das", "Un\u00b7ver\u00b7hoft", "kommt", "oft", ",", "und", "brin\u00b7get", "uns", "viel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.58": {"text": "Es dient zur Linderung, es macht uns gutes Muths:", "tokens": ["Es", "dient", "zur", "Lin\u00b7de\u00b7rung", ",", "es", "macht", "uns", "gu\u00b7tes", "Muths", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Ein weiser braucht den Trost der in dem Worte", "tokens": ["Ein", "wei\u00b7ser", "braucht", "den", "Trost", "der", "in", "dem", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.60": {"text": "Und wird dadurch allzeit zur guten That erwekket.", "tokens": ["Und", "wird", "da\u00b7durch", "all\u00b7zeit", "zur", "gu\u00b7ten", "That", "er\u00b7wek\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PAV", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}