{"textgrid.poem.61224": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Die Sonnenblume", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Rosen, die mit Ruhme", "tokens": ["O", "Ro\u00b7sen", ",", "die", "mit", "Ruh\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr prangt in Duft und Licht,", "tokens": ["Ihr", "prangt", "in", "Duft", "und", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin die Sonnenblume,", "tokens": ["Ich", "bin", "die", "Son\u00b7nen\u00b7blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ich beneid' euch nicht.", "tokens": ["Und", "ich", "be\u00b7neid'", "euch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Des Falters flatternd Kosen,", "tokens": ["Des", "Fal\u00b7ters", "flat\u00b7ternd", "Ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Lieder im Gestr\u00e4uch,", "tokens": ["Die", "Lie\u00b7der", "im", "Ge\u00b7str\u00e4uch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Menschen Lob, ihr Rosen,", "tokens": ["Der", "Men\u00b7schen", "Lob", ",", "ihr", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie gerne g\u00f6nn' ich's euch!", "tokens": ["Wie", "ger\u00b7ne", "g\u00f6nn'", "ich's", "euch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PIS", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mir schafft es volle G'n\u00fcge,", "tokens": ["Mir", "schafft", "es", "vol\u00b7le", "G'\u00b7n\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vom Himmelstau getr\u00e4nkt", "tokens": ["Vom", "Him\u00b7mel\u00b7stau", "ge\u00b7tr\u00e4nkt"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In meines Liebsten Z\u00fcge", "tokens": ["In", "mei\u00b7nes", "Liebs\u00b7ten", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu schauen still versenkt.", "tokens": ["Zu", "schau\u00b7en", "still", "ver\u00b7senkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VMFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Zum Sonnenj\u00fcngling richte", "tokens": ["Zum", "Son\u00b7nen\u00b7j\u00fcng\u00b7ling", "rich\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Haupt ich fr\u00fch und sp\u00e4t", "tokens": ["Das", "Haupt", "ich", "fr\u00fch", "und", "sp\u00e4t"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und n\u00e4hre mich vom Lichte,", "tokens": ["Und", "n\u00e4h\u00b7re", "mich", "vom", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das sein Gelock umweht.", "tokens": ["Das", "sein", "Ge\u00b7lock", "um\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mein Auge bleibt dem Hohen", "tokens": ["Mein", "Au\u00b7ge", "bleibt", "dem", "Ho\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auch dann noch zugekehrt,", "tokens": ["Auch", "dann", "noch", "zu\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn er mit heil'gen Lohen", "tokens": ["Wenn", "er", "mit", "heil'\u00b7gen", "Lo\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zuletzt mich selbst verzehrt.", "tokens": ["Zu\u00b7letzt", "mich", "selbst", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O sprecht, wie lie\u00df' erwerben", "tokens": ["O", "sprecht", ",", "wie", "lie\u00df'", "er\u00b7wer\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "PWAV", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich k\u00f6stlicher Geschick,", "tokens": ["Sich", "k\u00f6st\u00b7li\u00b7cher", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als so dahinzusterben", "tokens": ["Als", "so", "da\u00b7hin\u00b7zu\u00b7ster\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "PAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sanft an des Lieblings Blick!", "tokens": ["Sanft", "an", "des", "Lieb\u00b7lings", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Drum bl\u00fcht in eurem Ruhme,", "tokens": ["Drum", "bl\u00fcht", "in", "eu\u00b7rem", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Rosen wonniglich!", "tokens": ["Ihr", "Ro\u00b7sen", "won\u00b7nig\u00b7lich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin die Sonnenblume,", "tokens": ["Ich", "bin", "die", "Son\u00b7nen\u00b7blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und selig bin auch ich.", "tokens": ["Und", "se\u00b7lig", "bin", "auch", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}