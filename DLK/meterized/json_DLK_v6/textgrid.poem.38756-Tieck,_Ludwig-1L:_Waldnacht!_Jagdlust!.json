{"textgrid.poem.38756": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Waldnacht! Jagdlust!", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Waldnacht! Jagdlust!", "tokens": ["Wald\u00b7nacht", "!", "Jagd\u00b7lust", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Leis' und ferner", "tokens": ["Leis'", "und", "fer\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Klingen H\u00f6rner,", "tokens": ["Klin\u00b7gen", "H\u00f6r\u00b7ner", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Hebt sich, jauchzt die freie Brust!", "tokens": ["Hebt", "sich", ",", "jauchzt", "die", "frei\u00b7e", "Brust", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "T\u00f6ne, t\u00f6ne nieder zum Thal,", "tokens": ["T\u00f6\u00b7ne", ",", "t\u00f6\u00b7ne", "nie\u00b7der", "zum", "Thal", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Freun sich, freun sich allzumal", "tokens": ["Freun", "sich", ",", "freun", "sich", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "$,", "VVFIN", "PRF", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Baum und Strauch beim muntern Schall.", "tokens": ["Baum", "und", "Strauch", "beim", "mun\u00b7tern", "Schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kling' nur Bergquell!", "tokens": ["Kling'", "nur", "Berg\u00b7quell", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Epheuranken", "tokens": ["E\u00b7pheu\u00b7ran\u00b7ken"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Dich umschwanken,", "tokens": ["Dich", "um\u00b7schwan\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Rieseln durch die Kl\u00fcfte schnell!", "tokens": ["Rie\u00b7seln", "durch", "die", "Kl\u00fcf\u00b7te", "schnell", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fliehet, flieht das Leben so fort,", "tokens": ["Flie\u00b7het", ",", "flieht", "das", "Le\u00b7ben", "so", "fort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Wandelt hier, dann ist es dort, \u2013", "tokens": ["Wan\u00b7delt", "hier", ",", "dann", "ist", "es", "dort", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADV", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hallt, zerschmilzt ein luftig Wort.", "tokens": ["Hallt", ",", "zer\u00b7schmilzt", "ein", "luf\u00b7tig", "Wort", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Waldnacht! Jagdlust!", "tokens": ["Wald\u00b7nacht", "!", "Jagd\u00b7lust", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Da\u00df die Liebe", "tokens": ["Da\u00df", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Bei uns bliebe,", "tokens": ["Bei", "uns", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Wohnen blieb' in treuer Brust!", "tokens": ["Woh\u00b7nen", "blieb'", "in", "treu\u00b7er", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wandelt, wandelt sich allzumal,", "tokens": ["Wan\u00b7delt", ",", "wan\u00b7delt", "sich", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PRF", "ADV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Fliehet gleich dem H\u00f6rnerschall: \u2013", "tokens": ["Flie\u00b7het", "gleich", "dem", "H\u00f6r\u00b7ner\u00b7schall", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Einsam, einsam gr\u00fcnes Thal.", "tokens": ["Ein\u00b7sam", ",", "ein\u00b7sam", "gr\u00fc\u00b7nes", "Thal", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kling' nur Bergquell!", "tokens": ["Kling'", "nur", "Berg\u00b7quell", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Ach, betrogen \u2013", "tokens": ["Ach", ",", "be\u00b7tro\u00b7gen", "\u2013"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "VVPP", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Wasserwogen", "tokens": ["Was\u00b7ser\u00b7wo\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Rauschen abw\u00e4rts nicht so schnell!", "tokens": ["Rau\u00b7schen", "ab\u00b7w\u00e4rts", "nicht", "so", "schnell", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe, Leben, sie eilen hin,", "tokens": ["Lie\u00b7be", ",", "Le\u00b7ben", ",", "sie", "ei\u00b7len", "hin", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Keins von beiden tr\u00e4gt Gewinn: \u2013", "tokens": ["Keins", "von", "bei\u00b7den", "tr\u00e4gt", "Ge\u00b7winn", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "PIAT", "VVFIN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, da\u00df ich geboren bin!", "tokens": ["Ach", ",", "da\u00df", "ich", "ge\u00b7bo\u00b7ren", "bin", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}