{"dta.poem.12911": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Er vergleicht sich mit dem Jcarus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin wie Jcarus bem\u00fcht,", "tokens": ["Ich", "bin", "wie", "Jca\u00b7rus", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "NE", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Mich aus dem kercker wegzubringen,", "tokens": ["Mich", "aus", "dem", "ker\u00b7cker", "weg\u00b7zu\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In welchen mich die ehrfurcht eingesperrt.", "tokens": ["In", "wel\u00b7chen", "mich", "die", "ehr\u00b7furcht", "ein\u00b7ge\u00b7sperrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verliebte bl\u00e4tter sind die schwingen,", "tokens": ["Ver\u00b7lieb\u00b7te", "bl\u00e4t\u00b7ter", "sind", "die", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Durch die mein hertze sich zu seiner sonne zieht.", "tokens": ["Durch", "die", "mein", "hert\u00b7ze", "sich", "zu", "sei\u00b7ner", "son\u00b7ne", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPOSAT", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch ach, verwegenheit! du bringest bittre fr\u00fcchte,", "tokens": ["Doch", "ach", ",", "ver\u00b7we\u00b7gen\u00b7heit", "!", "du", "brin\u00b7gest", "bitt\u00b7re", "fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "NN", "$.", "PPER", "VVFIN", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So hoch du dich erhebst und prangst;", "tokens": ["So", "hoch", "du", "dich", "er\u00b7hebst", "und", "prangst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PRF", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Denn ihr erhitzter strahl macht meinen flug zunichte,", "tokens": ["Denn", "ihr", "er\u00b7hitz\u00b7ter", "strahl", "macht", "mei\u00b7nen", "flug", "zu\u00b7nich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und \u00fcberliefert mich den wellen tiefer angst.", "tokens": ["Und", "\u00fc\u00b7berl\u00b7ie\u00b7fert", "mich", "den", "wel\u00b7len", "tie\u00b7fer", "angst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}