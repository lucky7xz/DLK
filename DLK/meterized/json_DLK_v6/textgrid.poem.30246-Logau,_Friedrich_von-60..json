{"textgrid.poem.30246": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "60.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amor sa\u00df zu nechst betr\u00fcbet,", "tokens": ["A\u00b7mor", "sa\u00df", "zu", "nechst", "be\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil sein Pfeil was mi\u00dfge\u00fcbet,", "tokens": ["Weil", "sein", "Pfeil", "was", "mi\u00df\u00b7ge\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PWS", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "So doch selten sich begibet;", "tokens": ["So", "doch", "sel\u00b7ten", "sich", "be\u00b7gi\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sahe drauff zwei M\u00fcndlein ringen,", "tokens": ["Sa\u00b7he", "drauff", "zwei", "M\u00fcnd\u00b7lein", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "CARD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00f6rte s\u00fcsse Schmatzer klingen,", "tokens": ["H\u00f6r\u00b7te", "s\u00fcs\u00b7se", "Schmat\u00b7zer", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da hub Amor an zu springen.", "tokens": ["Da", "hub", "A\u00b7mor", "an", "zu", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APZR", "PTKZU", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}}}}