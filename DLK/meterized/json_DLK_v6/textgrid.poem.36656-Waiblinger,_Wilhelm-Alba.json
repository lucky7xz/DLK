{"textgrid.poem.36656": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Alba", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "G\u00f6ttliches bietest dem Auge du dar in Thal und Gebirgen,", "tokens": ["G\u00f6tt\u00b7li\u00b7ches", "bie\u00b7test", "dem", "Au\u00b7ge", "du", "dar", "in", "Thal", "und", "Ge\u00b7bir\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Hier der Velino, und dort strahlt aus der Tiefe der See!", "tokens": ["Hier", "der", "Ve\u00b7li\u00b7no", ",", "und", "dort", "strahlt", "aus", "der", "Tie\u00b7fe", "der", "See", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NE", "$,", "KON", "ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+---+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Einst als Gefangener sa\u00df der K\u00f6nig Syphax in Alba,", "tokens": ["Einst", "als", "Ge\u00b7fan\u00b7ge\u00b7ner", "sa\u00df", "der", "K\u00f6\u00b7nig", "Syp\u00b7hax", "in", "Al\u00b7ba", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "VVFIN", "ART", "NN", "NE", "APPR", "NN", "$,"], "meter": "+--+--+-+-++-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Mir auch hat es, dem Frei'n, Wasser und Brod nur bescheert.", "tokens": ["Mir", "auch", "hat", "es", ",", "dem", "Frei'n", ",", "Was\u00b7ser", "und", "Brod", "nur", "be\u00b7scheert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "$,", "ART", "NN", "$,", "NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}}}}}