{"textgrid.poem.25956": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Als mein Schatz gegangen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Zimmer hat nur W\u00e4nde,", "tokens": ["Mein", "Zim\u00b7mer", "hat", "nur", "W\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Fenster hat es keine,", "tokens": ["Und", "Fens\u00b7ter", "hat", "es", "kei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "PIAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn als mein Schatz gegangen,", "tokens": ["Denn", "als", "mein", "Schatz", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sa\u00df ich mit nassen Wangen,", "tokens": ["Sa\u00df", "ich", "mit", "nas\u00b7sen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Fand, da\u00df die Sonne blende.", "tokens": ["Fand", ",", "da\u00df", "die", "Son\u00b7ne", "blen\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Ich schickte meine H\u00e4nde,", "tokens": ["Ich", "schick\u00b7te", "mei\u00b7ne", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie schleppten Mauersteine.", "tokens": ["Sie", "schlepp\u00b7ten", "Mau\u00b7er\u00b7stei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie bauten auf der Stelle", "tokens": ["Sie", "bau\u00b7ten", "auf", "der", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit M\u00f6rtel und mit Kelle", "tokens": ["Mit", "M\u00f6r\u00b7tel", "und", "mit", "Kel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr meine Seelenruh", "tokens": ["F\u00fcr", "mei\u00b7ne", "See\u00b7len\u00b7ruh"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die lauten Fenster zu.", "tokens": ["Die", "lau\u00b7ten", "Fens\u00b7ter", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Niemand sieht's, wenn ich weine.", "tokens": ["Nie\u00b7mand", "sieht's", ",", "wenn", "ich", "wei\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Statt Licht sind um mich Steine,", "tokens": ["Statt", "Licht", "sind", "um", "mich", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und tr\u00f6stend dunkle W\u00e4nde.", "tokens": ["Und", "tr\u00f6s\u00b7tend", "dunk\u00b7le", "W\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Tr\u00e4n' findet allein", "tokens": ["Die", "Tr\u00e4n'", "fin\u00b7det", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Den Weg in meine H\u00e4nde.", "tokens": ["Den", "Weg", "in", "mei\u00b7ne", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}