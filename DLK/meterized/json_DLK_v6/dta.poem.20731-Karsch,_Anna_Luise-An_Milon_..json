{"dta.poem.20731": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An Milon .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Einf\u00e4ltig machte die Natur               ", "tokens": ["Ein\u00b7f\u00e4l\u00b7tig", "mach\u00b7te", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz und meine Sinnen;", "tokens": ["Mein", "Herz", "und", "mei\u00b7ne", "Sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Best\u00e4ndig lieben kann ich nur,", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "lie\u00b7ben", "kann", "ich", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VMFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und alle mein Beginnen,", "tokens": ["Und", "al\u00b7le", "mein", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Dichten, Trachten, Wunsch und Flehn", "tokens": ["Mein", "Dich\u00b7ten", ",", "Trach\u00b7ten", ",", "Wunsch", "und", "Flehn"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bestehet blo\u00df darinnen", "tokens": ["Be\u00b7ste\u00b7het", "blo\u00df", "da\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dich aufzusuchen und zu sehn", "tokens": ["Dich", "auf\u00b7zu\u00b7su\u00b7chen", "und", "zu", "sehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Deinen Blick zu f\u00fchlen.", "tokens": ["Und", "Dei\u00b7nen", "Blick", "zu", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ich habe nie daran gedacht", "tokens": ["Ich", "ha\u00b7be", "nie", "da\u00b7ran", "ge\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PAV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dir einen Streich zu spielen;", "tokens": ["Dir", "ei\u00b7nen", "Streich", "zu", "spie\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Doch gestern hab ichs fein gemacht,", "tokens": ["Doch", "ge\u00b7stern", "hab", "ichs", "fein", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "O la\u00df Dirs nur gestehen:", "tokens": ["O", "la\u00df", "Dirs", "nur", "ge\u00b7ste\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "ADV", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.13": {"text": "Die Rose, die ich Dir gebracht", "tokens": ["Die", "Ro\u00b7se", ",", "die", "ich", "Dir", "ge\u00b7bracht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Fing schon an zu vergehen,", "tokens": ["Fing", "schon", "an", "zu", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.15": {"text": "Sie fiel dir endlich aus der Hand.", "tokens": ["Sie", "fiel", "dir", "end\u00b7lich", "aus", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Du hubst sie auf und bliesest", "tokens": ["Du", "hubst", "sie", "auf", "und", "blie\u00b7sest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Den Staub von ihr, und ich empfand,", "tokens": ["Den", "Staub", "von", "ihr", ",", "und", "ich", "emp\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Was du ihr jetzt erwiesest,", "tokens": ["Was", "du", "ihr", "jetzt", "er\u00b7wie\u00b7sest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Die Ehre, die ihr ward gethan.", "tokens": ["Die", "Eh\u00b7re", ",", "die", "ihr", "ward", "ge\u00b7than", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Sa\u00df hinter dir mit Lauschen", "tokens": ["Sa\u00df", "hin\u00b7ter", "dir", "mit", "Lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Scharfaugicht wie ein Falk, und san", "tokens": ["Schar\u00b7fau\u00b7gicht", "wie", "ein", "Falk", ",", "und", "san"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "$,", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Darauf, sie umzutauschen.", "tokens": ["Da\u00b7rauf", ",", "sie", "um\u00b7zu\u00b7tau\u00b7schen", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PAV", "$,", "PPER", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Und das gelang mir gar zu gut:", "tokens": ["Und", "das", "ge\u00b7lang", "mir", "gar", "zu", "gut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Sie lag vor meinem Blicke", "tokens": ["Sie", "lag", "vor", "mei\u00b7nem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Ganz s\u00e4uberlich in deinem Huth;", "tokens": ["Ganz", "s\u00e4u\u00b7ber\u00b7lich", "in", "dei\u00b7nem", "Huth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Und mir zum gro\u00dfen Gl\u00fccke", "tokens": ["Und", "mir", "zum", "gro\u00b7\u00dfen", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Sprachst du mit irgend einem Hirt.", "tokens": ["Sprachst", "du", "mit", "ir\u00b7gend", "ei\u00b7nem", "Hirt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Husch fuhr ich zu, und raubte", "tokens": ["Husch", "fuhr", "ich", "zu", ",", "und", "raub\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Die Rose, die mich st\u00e4rken wird,", "tokens": ["Die", "Ro\u00b7se", ",", "die", "mich", "st\u00e4r\u00b7ken", "wird", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Bey schon gesunknem Haupte.", "tokens": ["Bey", "schon", "ge\u00b7sun\u00b7knem", "Haup\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Husch legt ich eine gr\u00f6\u00dfre hin", "tokens": ["Husch", "legt", "ich", "ei\u00b7ne", "gr\u00f6\u00df\u00b7re", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "ADJA", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Mit unverwelkten Bl\u00e4ttern,", "tokens": ["Mit", "un\u00b7ver\u00b7welk\u00b7ten", "Bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Und dankete mit frohem Sinn", "tokens": ["Und", "dan\u00b7ke\u00b7te", "mit", "fro\u00b7hem", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Herzinnig allen G\u00f6ttern,", "tokens": ["Her\u00b7zin\u00b7nig", "al\u00b7len", "G\u00f6t\u00b7tern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Da\u00df ich den Streich so klug gespielt,", "tokens": ["Da\u00df", "ich", "den", "Streich", "so", "klug", "ge\u00b7spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Nicht ohne Furcht und Beben", "tokens": ["Nicht", "oh\u00b7ne", "Furcht", "und", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Hab ich den s\u00fc\u00dfen Raub erzielt,", "tokens": ["Hab", "ich", "den", "s\u00fc\u00b7\u00dfen", "Raub", "er\u00b7zielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Du wirst mirs doch vergeben?", "tokens": ["Du", "wirst", "mirs", "doch", "ver\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}