{"textgrid.poem.43618": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Drei Maik\u00e4fer kamen zusammen", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drei Maik\u00e4fer kamen zusammen", "tokens": ["Drei", "Mai\u00b7k\u00e4\u00b7fer", "ka\u00b7men", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PTKVZ"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und sprachen allerhand:", "tokens": ["Und", "spra\u00b7chen", "al\u00b7ler\u00b7hand", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie f\u00fchlten sich gar nicht zufrieden,", "tokens": ["Sie", "f\u00fchl\u00b7ten", "sich", "gar", "nicht", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Brumm brumm summ summ summ, zufrieden,", "tokens": ["Brumm", "brumm", "summ", "summ", "summ", ",", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie wollten sich machen bekannt", "tokens": ["Sie", "woll\u00b7ten", "sich", "ma\u00b7chen", "be\u00b7kannt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Mit einem besseren Land.", "tokens": ["Mit", "ei\u00b7nem", "bes\u00b7se\u00b7ren", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Da sprach dann der \u00e4lteste K\u00e4fer:", "tokens": ["Da", "sprach", "dann", "der", "\u00e4l\u00b7tes\u00b7te", "K\u00e4\u00b7fer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wohl m\u00f6cht' ich \u00fcber'n Rhein!", "tokens": ["Wohl", "m\u00f6cht'", "ich", "\u00fc\u00b7ber'n", "Rhein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch dr\u00fcben im herrlichen Frankreich,", "tokens": ["Doch", "dr\u00fc\u00b7ben", "im", "herr\u00b7li\u00b7chen", "Fran\u00b7kreich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Brumm brumm summ summ summ, ja Frankreich;", "tokens": ["Brumm", "brumm", "summ", "summ", "summ", ",", "ja", "Fran\u00b7kreich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "ADV", "NE", "$."], "meter": "+---++-+", "measure": "dactylic.init"}, "line.5": {"text": "Da macht man in Zucker uns ein,", "tokens": ["Da", "macht", "man", "in", "Zu\u00b7cker", "uns", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Uns arme K\u00e4ferlein.", "tokens": ["Uns", "ar\u00b7me", "K\u00e4\u00b7fer\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da meinte der zweite von ihnen:", "tokens": ["Da", "mein\u00b7te", "der", "zwei\u00b7te", "von", "ih\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "APPR", "PPER", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "In Ungarn w\u00e4r's gut sein,", "tokens": ["In", "Un\u00b7garn", "w\u00e4r's", "gut", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch wirft man uns dort vor die Schweine,", "tokens": ["Doch", "wirft", "man", "uns", "dort", "vor", "die", "Schwei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Brumm brumm summ summ summ, die Schweine,", "tokens": ["Brumm", "brumm", "summ", "summ", "summ", ",", "die", "Schwei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wir lebten in ewiger Pein,", "tokens": ["Wir", "leb\u00b7ten", "in", "e\u00b7wi\u00b7ger", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Wir arme K\u00e4ferlein.", "tokens": ["Wir", "ar\u00b7me", "K\u00e4\u00b7fer\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich sind' es am besten in Deutschland,", "tokens": ["Ich", "sind'", "es", "am", "bes\u00b7ten", "in", "Deutschland", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKA", "ADJD", "APPR", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "So sprach der dritte drein,", "tokens": ["So", "sprach", "der", "drit\u00b7te", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00e4ngt hier uns kein Spatz oder Bube,", "tokens": ["F\u00e4ngt", "hier", "uns", "kein", "Spatz", "o\u00b7der", "Bu\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "So k\u00f6nnen wir herrlich gedeihn,", "tokens": ["So", "k\u00f6n\u00b7nen", "wir", "herr\u00b7lich", "ge\u00b7deihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Wir arme K\u00e4ferlein.", "tokens": ["Wir", "ar\u00b7me", "K\u00e4\u00b7fer\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da fingen sie an zu schnurren", "tokens": ["Da", "fin\u00b7gen", "sie", "an", "zu", "schnur\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gar manche Melodei:", "tokens": ["Gar", "man\u00b7che", "Me\u00b7lo\u00b7dei", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir bleiben, wir bleiben in Deutschland,", "tokens": ["Wir", "blei\u00b7ben", ",", "wir", "blei\u00b7ben", "in", "Deutschland", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Brumm brumm summ summ summ, in Deutschland,", "tokens": ["Brumm", "brumm", "summ", "summ", "summ", ",", "in", "Deutschland", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "APPR", "NE", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Da leben wir lustig und frei,", "tokens": ["Da", "le\u00b7ben", "wir", "lus\u00b7tig", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Juchhe, juchhe, juchhei!", "tokens": ["Juch\u00b7he", ",", "juch\u00b7he", ",", "juch\u00b7hei", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ITJ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}