{"textgrid.poem.66046": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Morgengang", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Morgen warf, steh auf du Wicht,", "tokens": ["Der", "Mor\u00b7gen", "warf", ",", "steh", "auf", "du", "Wicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir goldne Perlen auf die Kissen,", "tokens": ["Mir", "gold\u00b7ne", "Per\u00b7len", "auf", "die", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da taumelte ich traumentrissen", "tokens": ["Da", "tau\u00b7mel\u00b7te", "ich", "trau\u00b7men\u00b7tris\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor sein ergl\u00fchtes Angesicht.", "tokens": ["Vor", "sein", "er\u00b7gl\u00fch\u00b7tes", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Blank lag die Welt in seinem Glanz.", "tokens": ["Blank", "lag", "die", "Welt", "in", "sei\u00b7nem", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Hinaus! Mein Herz war frisch und heiter!", "tokens": ["Hin\u00b7aus", "!", "Mein", "Herz", "war", "frisch", "und", "hei\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Duft und Farben, weiter, weiter!", "tokens": ["Durch", "Duft", "und", "Far\u00b7ben", ",", "wei\u00b7ter", ",", "wei\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ADV", "$,", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und jeder Schritt war Lust und Tanz.", "tokens": ["Und", "je\u00b7der", "Schritt", "war", "Lust", "und", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und auf und ab ein gro\u00dfes Kind,", "tokens": ["Und", "auf", "und", "ab", "ein", "gro\u00b7\u00dfes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lief ich durch all die Morgensch\u00f6ne,", "tokens": ["Lief", "ich", "durch", "all", "die", "Mor\u00b7gen\u00b7sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und oben schwammen Lerchent\u00f6ne", "tokens": ["Und", "o\u00b7ben", "schwam\u00b7men", "Ler\u00b7chen\u00b7t\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Bl\u00fctenbl\u00e4tter mit dem Wind.", "tokens": ["Und", "Bl\u00fc\u00b7ten\u00b7bl\u00e4t\u00b7ter", "mit", "dem", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da kam, da kam, durchs feuchte Kraut,", "tokens": ["Da", "kam", ",", "da", "kam", ",", "durchs", "feuch\u00b7te", "Kraut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die fr\u00fchen Freuden voll zu machen,", "tokens": ["Die", "fr\u00fc\u00b7hen", "Freu\u00b7den", "voll", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Schatz, und wurden Ku\u00df und Lachen", "tokens": ["Mein", "Schatz", ",", "und", "wur\u00b7den", "Ku\u00df", "und", "La\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KON", "VAFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und tausend s\u00fc\u00dfe Worte laut.", "tokens": ["Und", "tau\u00b7send", "s\u00fc\u00b7\u00dfe", "Wor\u00b7te", "laut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}