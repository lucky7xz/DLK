{"textgrid.poem.40549": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Ungleicher Kampf", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gigante du, willst mit dem Zwerg du ringen?", "tokens": ["Gi\u00b7gan\u00b7te", "du", ",", "willst", "mit", "dem", "Zwerg", "du", "rin\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VMFIN", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir ist es Schmach, den Schw\u00e4chling zu bezwingen,", "tokens": ["Dir", "ist", "es", "Schmach", ",", "den", "Schw\u00e4ch\u00b7ling", "zu", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihm ist es Ruhm, von deiner Hand zu fallen!", "tokens": ["Ihm", "ist", "es", "Ruhm", ",", "von", "dei\u00b7ner", "Hand", "zu", "fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "$,", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf gr\u00fcnem Alpensitz j\u00fcngst dacht' ich deiner:", "tokens": ["Auf", "gr\u00fc\u00b7nem", "Al\u00b7pen\u00b7sitz", "j\u00fcngst", "dacht'", "ich", "dei\u00b7ner", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zur Sonne flog der K\u00f6nigsadler einer", "tokens": ["Zur", "Son\u00b7ne", "flog", "der", "K\u00f6\u00b7ni\u00b7gsad\u00b7ler", "ei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein bl\u00f6kend Hammelthier in seinen Krallen.", "tokens": ["Ein", "bl\u00f6\u00b7kend", "Ham\u00b7mel\u00b7thier", "in", "sei\u00b7nen", "Kral\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "O Aar, dir l\u00e4\u00dft's nicht gut, am Schmutzvlie\u00df zerren,", "tokens": ["O", "Aar", ",", "dir", "l\u00e4\u00dft's", "nicht", "gut", ",", "am", "Schmutz\u00b7vlie\u00df", "zer\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "VVFIN", "PTKNEG", "ADJD", "$,", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Und Schmachtroph\u00e4'n sind dir des Hammels Flocken!", "tokens": ["Und", "Schmachtro\u00b7ph\u00e4'n", "sind", "dir", "des", "Ham\u00b7mels", "Flo\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Doch er, gewohnt auf niedrer Trift zu pl\u00e4rren,", "tokens": ["Doch", "er", ",", "ge\u00b7wohnt", "auf", "nie\u00b7drer", "Trift", "zu", "pl\u00e4r\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "VVPP", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Scheint selbst in deinen Krallen zu frohlocken,", "tokens": ["Scheint", "selbst", "in", "dei\u00b7nen", "Kral\u00b7len", "zu", "froh\u00b7lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Da\u00df er durch dich nun lernt den Flug nach oben,", "tokens": ["Da\u00df", "er", "durch", "dich", "nun", "lernt", "den", "Flug", "nach", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "VVFIN", "ART", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da\u00df er mit dir zur Wolkenh\u00f6h' erhoben!", "tokens": ["Da\u00df", "er", "mit", "dir", "zur", "Wol\u00b7ken\u00b7h\u00f6h'", "er\u00b7ho\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}