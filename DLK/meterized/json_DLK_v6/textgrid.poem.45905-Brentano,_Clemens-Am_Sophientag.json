{"textgrid.poem.45905": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Am Sophientag", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00dfer Mai du Quell des Lebens", "tokens": ["S\u00fc\u00b7\u00dfer", "Mai", "du", "Quell", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPER", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bist so s\u00fc\u00dfer Blumen voll", "tokens": ["Bist", "so", "s\u00fc\u00b7\u00dfer", "Blu\u00b7men", "voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebe sucht auch nicht vergebens", "tokens": ["Lie\u00b7be", "sucht", "auch", "nicht", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "PTKNEG", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wem sie Kr\u00e4nze winden soll.", "tokens": ["Wem", "sie", "Kr\u00e4n\u00b7ze", "win\u00b7den", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "S\u00fc\u00dfer Mai, mit Blumenglocken", "tokens": ["S\u00fc\u00b7\u00dfer", "Mai", ",", "mit", "Blu\u00b7men\u00b7glo\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4utest du das Fest mir ein", "tokens": ["L\u00e4u\u00b7test", "du", "das", "Fest", "mir", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPER", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich bekr\u00e4nze ihre Locken,", "tokens": ["Ich", "be\u00b7kr\u00e4n\u00b7ze", "ih\u00b7re", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will ein frommer Gast auch sein.", "tokens": ["Will", "ein", "from\u00b7mer", "Gast", "auch", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "ADV", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "S\u00fc\u00dfer Mai, zum Liebesmahle", "tokens": ["S\u00fc\u00b7\u00dfer", "Mai", ",", "zum", "Lie\u00b7bes\u00b7mah\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4gst du Blumenkelche ein", "tokens": ["Tr\u00e4gst", "du", "Blu\u00b7men\u00b7kel\u00b7che", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bl\u00fctens\u00e4ulen stehn im Saale", "tokens": ["Bl\u00fc\u00b7ten\u00b7s\u00e4u\u00b7len", "stehn", "im", "Saa\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dr\u00fcber w\u00f6lbt sich Sonnenschein.", "tokens": ["Dr\u00fc\u00b7ber", "w\u00f6lbt", "sich", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "S\u00fc\u00dfer Mai, in deinen Kelchen", "tokens": ["S\u00fc\u00b7\u00dfer", "Mai", ",", "in", "dei\u00b7nen", "Kel\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fcssen fromme Bienen sich", "tokens": ["K\u00fcs\u00b7sen", "from\u00b7me", "Bie\u00b7nen", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber unter allen welchen", "tokens": ["A\u00b7ber", "un\u00b7ter", "al\u00b7len", "wel\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "PWAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hast du eingef\u00fcllt f\u00fcr mich!", "tokens": ["Hast", "du", "ein\u00b7ge\u00b7f\u00fcllt", "f\u00fcr", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "S\u00fc\u00dfer Mai! du bringest nieder", "tokens": ["S\u00fc\u00b7\u00dfer", "Mai", "!", "du", "brin\u00b7gest", "nie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blume, Bl\u00fcte, Sonnenschein,", "tokens": ["Blu\u00b7me", ",", "Bl\u00fc\u00b7te", ",", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich wisse, wem die Lieder,", "tokens": ["Da\u00df", "ich", "wis\u00b7se", ",", "wem", "die", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PWS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wem das Herz, das Leben weihn.", "tokens": ["Wem", "das", "Herz", ",", "das", "Le\u00b7ben", "weihn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}