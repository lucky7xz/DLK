{"textgrid.poem.53552": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Das Heil von au\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was wir bereits gestorben glaubten,", "tokens": ["Was", "wir", "be\u00b7reits", "ge\u00b7stor\u00b7ben", "glaub\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ist, hols der Teufel, wieder da:", "tokens": ["ist", ",", "hols", "der", "Teu\u00b7fel", ",", "wie\u00b7der", "da", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "ART", "NN", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die alten achselst\u00fcckberaupten", "tokens": ["die", "al\u00b7ten", "ach\u00b7sel\u00b7st\u00fcck\u00b7be\u00b7raup\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kommis der Militaria.", "tokens": ["Kom\u00b7mis", "der", "Mi\u00b7li\u00b7ta\u00b7ria", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Das wandelt wie in alten Tagen,", "tokens": ["Das", "wan\u00b7delt", "wie", "in", "al\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "f\u00fcr alles Neue g\u00e4nzlich taub:", "tokens": ["f\u00fcr", "al\u00b7les", "Neu\u00b7e", "g\u00e4nz\u00b7lich", "taub", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "man tr\u00e4gt nur manches auf dem Kragen", "tokens": ["man", "tr\u00e4gt", "nur", "man\u00b7ches", "auf", "dem", "Kra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und ist ein Kerl mit Eichenlaub.", "tokens": ["und", "ist", "ein", "Kerl", "mit", "Ei\u00b7chen\u00b7laub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das sind doch alles Kleidermoden:", "tokens": ["Das", "sind", "doch", "al\u00b7les", "Klei\u00b7der\u00b7mo\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "der \u00c4rmelschmuck und wie das hei\u00dft . . .", "tokens": ["der", "\u00c4r\u00b7melsc\u00b7hmuck", "und", "wie", "das", "hei\u00dft", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "KON", "PWAV", "PDS", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man stellt sich einfach auf den Boden", "tokens": ["Man", "stellt", "sich", "ein\u00b7fach", "auf", "den", "Bo\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "der neuen Welt \u2013 im alten Geist.", "tokens": ["der", "neu\u00b7en", "Welt", "\u2013", "im", "al\u00b7ten", "Geist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und haben wir den Krieg verloren:", "tokens": ["Und", "ha\u00b7ben", "wir", "den", "Krieg", "ver\u00b7lo\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die Herren, silberig besternt,", "tokens": ["die", "Her\u00b7ren", ",", "sil\u00b7be\u00b7rig", "bes\u00b7ternt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "verschlie\u00dfen ihre langen Ohren \u2013", "tokens": ["ver\u00b7schlie\u00b7\u00dfen", "ih\u00b7re", "lan\u00b7gen", "Oh\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "sie haben nichts dazugelernt.", "tokens": ["sie", "ha\u00b7ben", "nichts", "da\u00b7zu\u00b7ge\u00b7lernt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und nur ein Friede kann uns retten,", "tokens": ["Und", "nur", "ein", "Frie\u00b7de", "kann", "uns", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ein Friede, der dies Heer zerbricht,", "tokens": ["ein", "Frie\u00b7de", ",", "der", "dies", "Heer", "zer\u00b7bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "zerbricht die alten Eisenketten \u2013", "tokens": ["zer\u00b7bricht", "die", "al\u00b7ten", "Ei\u00b7sen\u00b7ket\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "der Feind befreit uns von den Kletten.", "tokens": ["der", "Feind", "be\u00b7freit", "uns", "von", "den", "Klet\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Deutschen selber tun es nicht.", "tokens": ["Die", "Deut\u00b7schen", "sel\u00b7ber", "tun", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}