{"textgrid.poem.62803": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr schattichten Geb\u00fcsche", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr schattichten Geb\u00fcsche", "tokens": ["Ihr", "schat\u00b7tich\u00b7ten", "Ge\u00b7b\u00fc\u00b7sche"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Vernehmt mein Angstgeschrey!", "tokens": ["Ver\u00b7nehmt", "mein", "Angst\u00b7ge\u00b7schrey", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch ihr, ihr stummen Fische", "tokens": ["Auch", "ihr", ",", "ihr", "stum\u00b7men", "Fi\u00b7sche"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stimmt meinen Seufzern bey!", "tokens": ["Stimmt", "mei\u00b7nen", "Seuf\u00b7zern", "bey", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Beweint in bittern Klagen", "tokens": ["Be\u00b7weint", "in", "bit\u00b7tern", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Schicksals Tyranney:", "tokens": ["Des", "Schick\u00b7sals", "Ty\u00b7ran\u00b7ney", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Denn, ach, kaum kann ichs sagen,", "tokens": ["Denn", ",", "ach", ",", "kaum", "kann", "ichs", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$,", "ADV", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein Ba\u00dfglas ist entzwey.", "tokens": ["Mein", "Ba\u00df\u00b7glas", "ist", "ent\u00b7zwey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}