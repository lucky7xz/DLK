{"textgrid.poem.67574": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nur stets geputzt und stets geschminkt,", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur stets geputzt und stets geschminkt,", "tokens": ["Nur", "stets", "ge\u00b7putzt", "und", "stets", "ge\u00b7schminkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als prangte sie ins Hochzeitsmahl,", "tokens": ["Als", "prang\u00b7te", "sie", "ins", "Hoch\u00b7zeits\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Gold, in Seide! \u2013 Wie sie blinkt,", "tokens": ["In", "Gold", ",", "in", "Sei\u00b7de", "!", "\u2013", "Wie", "sie", "blinkt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NE", "$.", "$(", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hellgl\u00e4nzender als Sonnenstrahl!", "tokens": ["Hell\u00b7gl\u00e4n\u00b7zen\u00b7der", "als", "Son\u00b7nen\u00b7strahl", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So fein, so reich! Ganz Kunst! Geschmack! \u2013 Eins nur,", "tokens": ["So", "fein", ",", "so", "reich", "!", "Ganz", "Kunst", "!", "Ge\u00b7schmack", "!", "\u2013", "Eins", "nur", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$.", "ADV", "NN", "$.", "NN", "$.", "$(", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Eins fehlt Dir, reiche, feine Kunst \u2013 Natur!", "tokens": ["Eins", "fehlt", "Dir", ",", "rei\u00b7che", ",", "fei\u00b7ne", "Kunst", "\u2013", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "VVFIN", "$,", "ADJA", "NN", "$(", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein M\u00e4dchen nur in Morgentracht", "tokens": ["Mein", "M\u00e4d\u00b7chen", "nur", "in", "Mor\u00b7gen\u00b7tracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nachl\u00e4ssig sch\u00f6n! voll Einfalt nur", "tokens": ["Nach\u00b7l\u00e4s\u00b7sig", "sch\u00f6n", "!", "voll", "Ein\u00b7falt", "nur"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADJD", "$.", "ADJD", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Blick, ein Herzensku\u00df! \u2013 O Pracht,", "tokens": ["Ein", "Blick", ",", "ein", "Her\u00b7zens\u00b7ku\u00df", "!", "\u2013", "O", "Pracht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$.", "$(", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du Th\u00f6rin! \u2013 Sieh, Natur, Natur,", "tokens": ["Du", "Th\u00f6\u00b7rin", "!", "\u2013", "Sieh", ",", "Na\u00b7tur", ",", "Na\u00b7tur", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$.", "$(", "NE", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die r\u00fchrt das Herz! \u2013 Die Kunst, Dein Affentand", "tokens": ["Die", "r\u00fchrt", "das", "Herz", "!", "\u2013", "Die", "Kunst", ",", "Dein", "Af\u00b7fen\u00b7tand"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "$.", "$(", "ART", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ist \u2013 f\u00fcr den armen, kalten Greis \u2013 Verstand.", "tokens": ["Ist", "\u2013", "f\u00fcr", "den", "ar\u00b7men", ",", "kal\u00b7ten", "Greis", "\u2013", "Ver\u00b7stand", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "$(", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$(", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}