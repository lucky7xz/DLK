{"dta.poem.3459": {"metadata": {"author": {"name": "N.A.", "birth": "N.A.", "death": "N.A."}, "title": "Das .xxx.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1497", "urn": "urn:nbn:de:kobv:b4-30121-8", "language": ["de:0.99"], "booktitle": "Des Edlen Ritter Morgeners Walfart. Erfurt, 1497."}, "poem": {"stanza.1": {"line.1": {"text": "Eines schweigens het ich mir erdacht", "tokens": ["Ei\u00b7nes", "schwei\u00b7gens", "het", "ich", "mir", "er\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "VAFIN", "PPER", "PPER", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "so mu\u00df ich aber singen ee", "tokens": ["so", "mu\u00df", "ich", "a\u00b7ber", "sin\u00b7gen", "e\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "dartzu han mich die frawen pracht", "tokens": ["dart\u00b7zu", "han", "mich", "die", "fra\u00b7wen", "pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "die m\u00fcgen mir gepieten mee", "tokens": ["die", "m\u00fc\u00b7gen", "mir", "ge\u00b7pie\u00b7ten", "mee"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "ich pit e\u00fcch darumb iunger man", "tokens": ["ich", "pit", "e\u00b7\u00fcch", "da\u00b7rumb", "i\u00b7un\u00b7ger", "man"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NE", "ADJD", "PAV", "ADJA", "PIS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "gerecht mich an der alten praut", "tokens": ["ge\u00b7recht", "mich", "an", "der", "al\u00b7ten", "praut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "vnd schlacht mir summerlarchen an.", "tokens": ["vnd", "schlacht", "mir", "sum\u00b7mer\u00b7lar\u00b7chen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}