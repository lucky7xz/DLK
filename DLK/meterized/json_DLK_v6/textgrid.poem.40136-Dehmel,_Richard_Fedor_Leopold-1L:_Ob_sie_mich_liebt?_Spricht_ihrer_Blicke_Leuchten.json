{"textgrid.poem.40136": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ob sie mich liebt? Spricht ihrer Blicke Leuchten", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob sie mich liebt? Spricht ihrer Blicke Leuchten", "tokens": ["Ob", "sie", "mich", "liebt", "?", "Spricht", "ih\u00b7rer", "Bli\u00b7cke", "Leuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$.", "NN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "zu mir der Liebe selig stumme Frage?", "tokens": ["zu", "mir", "der", "Lie\u00b7be", "se\u00b7lig", "stum\u00b7me", "Fra\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist's nur die gro\u00dfe Hoffnung, die ich trage,", "tokens": ["Ist's", "nur", "die", "gro\u00b7\u00dfe", "Hoff\u00b7nung", ",", "die", "ich", "tra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "da\u00df mir erf\u00fcllt die bangen W\u00fcnsche d\u00e4uchten?", "tokens": ["da\u00df", "mir", "er\u00b7f\u00fcllt", "die", "ban\u00b7gen", "W\u00fcn\u00b7sche", "d\u00e4uch\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ob sie mich liebt?", "tokens": ["Ob", "sie", "mich", "liebt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ob sie mich liebt? Sie l\u00e4chelte so mild,", "tokens": ["Ob", "sie", "mich", "liebt", "?", "Sie", "l\u00e4\u00b7chel\u00b7te", "so", "mild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "als vor Verlangen stumm mein Gru\u00df verhauchte;", "tokens": ["als", "vor", "Ver\u00b7lan\u00b7gen", "stumm", "mein", "Gru\u00df", "ver\u00b7hauch\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "sie ", "tokens": ["sie"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "aus ihrem Auge mein verkl\u00e4rtes Bild ...", "tokens": ["aus", "ih\u00b7rem", "Au\u00b7ge", "mein", "ver\u00b7kl\u00e4r\u00b7tes", "Bild", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie liebt mich wol.", "tokens": ["Sie", "liebt", "mich", "wol", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}