{"dta.poem.13103": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eweit \u00fcber dem Gestirn fand ich oft meinen Geist;", "tokens": ["\u201e", "weit", "\u00fc\u00b7ber", "dem", "Ge\u00b7stirn", "fand", "ich", "oft", "mei\u00b7nen", "Geist", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eda sah ich, da\u00df er mir dort seinen Ursprung weist;", "tokens": ["\u201e", "da", "sah", "ich", ",", "da\u00df", "er", "mir", "dort", "sei\u00b7nen", "Ur\u00b7sprung", "weist", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201emithin befli\u00df\u2019 ich mich, die Sayten so zu stimmen,", "tokens": ["\u201e", "mi\u00b7thin", "be\u00b7fli\u00df'", "ich", "mich", ",", "die", "Say\u00b7ten", "so", "zu", "stim\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PRF", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "380\u201dDa\u00df ich verm\u00f6gend ward demselben nachzuklimmen.", "tokens": ["\"", "Da\u00df", "ich", "ver\u00b7m\u00f6\u00b7gend", "ward", "dem\u00b7sel\u00b7ben", "nach\u00b7zu\u00b7klim\u00b7men", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "VVPP", "VAFIN", "PDAT", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eda sang ich dessen Lob, von dem mir die Natur,", "tokens": ["\u201e", "da", "sang", "ich", "des\u00b7sen", "Lob", ",", "von", "dem", "mir", "die", "Na\u00b7tur", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PDS", "NN", "$,", "APPR", "PRELS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eund der Zusammenhang der Erden eine Spuhr", "tokens": ["\u201e", "und", "der", "Zu\u00b7sam\u00b7men\u00b7hang", "der", "Er\u00b7den", "ei\u00b7ne", "Spuhr"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "ART", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eschon vormahls eingepr\u00e4gt: Posaunen, Fl\u00f6ten, Cither,", "tokens": ["\u201e", "schon", "vor\u00b7mahls", "ein\u00b7ge\u00b7pr\u00e4gt", ":", "Po\u00b7sau\u00b7nen", ",", "Fl\u00f6\u00b7ten", ",", "Cit\u00b7her", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "ADV", "VVPP", "$.", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.8": {"text": "\u201edoch mehr der Harfe Klang besungen den Gebiether,", "tokens": ["\u201e", "doch", "mehr", "der", "Har\u00b7fe", "Klang", "be\u00b7sun\u00b7gen", "den", "Ge\u00b7bie\u00b7ther", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ART", "NN", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "385\u201dDen Sch\u00f6pfer meines Seyns, den Ursprung dieser Welt;", "tokens": ["\"", "Den", "Sch\u00f6p\u00b7fer", "mei\u00b7nes", "Seyns", ",", "den", "Ur\u00b7sprung", "die\u00b7ser", "Welt", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eda ward, sag ich, von mir desselben Lob erz\u00e4hlt;", "tokens": ["\u201e", "da", "ward", ",", "sag", "ich", ",", "von", "mir", "des\u00b7sel\u00b7ben", "Lob", "er\u00b7z\u00e4hlt", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "$,", "VVFIN", "PPER", "$,", "APPR", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eund was ich sang erschallt noch heut durch alle Zungen,", "tokens": ["\u201e", "und", "was", "ich", "sang", "er\u00b7schallt", "noch", "heut", "durch", "al\u00b7le", "Zun\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PWS", "PPER", "ADJD", "VVFIN", "ADV", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eseit dieser Zeit hat ihn kein Mund so sch\u00f6n besungen.", "tokens": ["\u201e", "seit", "die\u00b7ser", "Zeit", "hat", "ihn", "kein", "Mund", "so", "sch\u00f6n", "be\u00b7sun\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PDAT", "NN", "VAFIN", "PPER", "PIAT", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}