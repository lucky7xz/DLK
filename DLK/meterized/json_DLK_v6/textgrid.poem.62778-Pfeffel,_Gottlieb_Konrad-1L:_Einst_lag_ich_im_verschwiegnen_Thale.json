{"textgrid.poem.62778": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einst lag ich im verschwiegnen Thale", "genre": "verse", "period": "N.A.", "pub_year": 1763, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst lag ich im verschwiegnen Thale", "tokens": ["Einst", "lag", "ich", "im", "ver\u00b7schwieg\u00b7nen", "Tha\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An Chloens Brust im Abendroth,", "tokens": ["An", "Chloens", "Brust", "im", "A\u00b7ben\u00b7droth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und trank aus der bekr\u00e4nzten Schaale,", "tokens": ["Und", "trank", "aus", "der", "be\u00b7kr\u00e4nz\u00b7ten", "Schaa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die mir der Gott der Freude bot.", "tokens": ["Die", "mir", "der", "Gott", "der", "Freu\u00b7de", "bot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Schnell bebt ich; eine Thr\u00e4ne wallte", "tokens": ["Schnell", "bebt", "ich", ";", "ei\u00b7ne", "Thr\u00e4\u00b7ne", "wall\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hei\u00df in den Nektar. Armer Fant,", "tokens": ["Hei\u00df", "in", "den", "Nek\u00b7tar", ".", "Ar\u00b7mer", "Fant", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was schreckt dich? sprach der Gott. Ich lallte", "tokens": ["Was", "schreckt", "dich", "?", "sprach", "der", "Gott", ".", "Ich", "lall\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$.", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Zittern: Ach! dein Unbestand!", "tokens": ["Mit", "Zit\u00b7tern", ":", "Ach", "!", "dein", "Un\u00b7be\u00b7stand", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "ITJ", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der ist dein Gl\u00fcck, versetzt der Sp\u00f6tter,", "tokens": ["Der", "ist", "dein", "Gl\u00fcck", ",", "ver\u00b7setzt", "der", "Sp\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Stell immer deine Klagen ein,", "tokens": ["Stell", "im\u00b7mer", "dei\u00b7ne", "Kla\u00b7gen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "W\u00e4r ich best\u00e4ndig, traun, die G\u00f6tter", "tokens": ["W\u00e4r", "ich", "be\u00b7st\u00e4n\u00b7dig", ",", "traun", ",", "die", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "VVINF", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Behielten mich f\u00fcr sich allein.", "tokens": ["Be\u00b7hiel\u00b7ten", "mich", "f\u00fcr", "sich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PRF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mag seyn, doch wen kein Gut begl\u00fcckte,", "tokens": ["Mag", "seyn", ",", "doch", "wen", "kein", "Gut", "be\u00b7gl\u00fcck\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VAINF", "$,", "KON", "PWS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem droht auch keines Guts Verlust.", "tokens": ["Dem", "droht", "auch", "kei\u00b7nes", "Guts", "Ver\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So rief ich schluchzend aus und dr\u00fcckte", "tokens": ["So", "rief", "ich", "schluch\u00b7zend", "aus", "und", "dr\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Liebchen fester an die Brust.", "tokens": ["Mein", "Lieb\u00b7chen", "fes\u00b7ter", "an", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}