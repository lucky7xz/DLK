{"textgrid.poem.54995": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Wer kauft Liebesg\u00f6tter?", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von allen sch\u00f6nen Waren,", "tokens": ["Von", "al\u00b7len", "sch\u00f6\u00b7nen", "Wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum Markte hergefahren,", "tokens": ["Zum", "Mark\u00b7te", "her\u00b7ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wird keine mehr behagen,", "tokens": ["Wird", "kei\u00b7ne", "mehr", "be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als die wir euch getragen", "tokens": ["Als", "die", "wir", "euch", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "PPER", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Aus fremden L\u00e4ndern bringen.", "tokens": ["Aus", "frem\u00b7den", "L\u00e4n\u00b7dern", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O h\u00f6ret, was wir singen!", "tokens": ["O", "h\u00f6\u00b7ret", ",", "was", "wir", "sin\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und seht die sch\u00f6nen V\u00f6gel,", "tokens": ["Und", "seht", "die", "sch\u00f6\u00b7nen", "V\u00f6\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sie stehen zum Verkauf.", "tokens": ["Sie", "ste\u00b7hen", "zum", "Ver\u00b7kauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zuerst beseht den gro\u00dfen,", "tokens": ["Zu\u00b7erst", "be\u00b7seht", "den", "gro\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den lustigen, den losen!", "tokens": ["Den", "lus\u00b7ti\u00b7gen", ",", "den", "lo\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er h\u00fcpfet leicht und munter", "tokens": ["Er", "h\u00fcp\u00b7fet", "leicht", "und", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Baum und Busch herunter;", "tokens": ["Von", "Baum", "und", "Busch", "her\u00b7un\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gleich ist er wieder droben.", "tokens": ["Gleich", "ist", "er", "wie\u00b7der", "dro\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wir wollen ihn nicht loben.", "tokens": ["Wir", "wol\u00b7len", "ihn", "nicht", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "O seht den muntern Vogel!", "tokens": ["O", "seht", "den", "mun\u00b7tern", "Vo\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Er steht hier zum Verkauf.", "tokens": ["Er", "steht", "hier", "zum", "Ver\u00b7kauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Betrachtet nun den kleinen,", "tokens": ["Be\u00b7trach\u00b7tet", "nun", "den", "klei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er will bed\u00e4chtig scheinen,", "tokens": ["Er", "will", "be\u00b7d\u00e4ch\u00b7tig", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und doch ist er der lose,", "tokens": ["Und", "doch", "ist", "er", "der", "lo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "ADJA", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "So gut als wie der gro\u00dfe;", "tokens": ["So", "gut", "als", "wie", "der", "gro\u00b7\u00dfe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "KOKOM", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er zeiget meist im stillen", "tokens": ["Er", "zei\u00b7get", "meist", "im", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den allerbesten Willen.", "tokens": ["Den", "al\u00b7ler\u00b7bes\u00b7ten", "Wil\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der lose kleine Vogel,", "tokens": ["Der", "lo\u00b7se", "klei\u00b7ne", "Vo\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Er steht hier zum Verkauf.", "tokens": ["Er", "steht", "hier", "zum", "Ver\u00b7kauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O seht das kleine T\u00e4ubchen,", "tokens": ["O", "seht", "das", "klei\u00b7ne", "T\u00e4ub\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das liebe Turtelweibchen!", "tokens": ["Das", "lie\u00b7be", "Tur\u00b7tel\u00b7weib\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die M\u00e4dchen sind so zierlich,", "tokens": ["Die", "M\u00e4d\u00b7chen", "sind", "so", "zier\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verst\u00e4ndig und manierlich;", "tokens": ["Ver\u00b7st\u00e4n\u00b7dig", "und", "ma\u00b7nier\u00b7lich", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "-+-++--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Sie mag sich gerne putzen", "tokens": ["Sie", "mag", "sich", "ger\u00b7ne", "put\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und eure Liebe nutzen.", "tokens": ["Und", "eu\u00b7re", "Lie\u00b7be", "nut\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der kleine zarte Vogel,", "tokens": ["Der", "klei\u00b7ne", "zar\u00b7te", "Vo\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Er steht hier zum Verkauf.", "tokens": ["Er", "steht", "hier", "zum", "Ver\u00b7kauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wir wollen sie nicht loben,", "tokens": ["Wir", "wol\u00b7len", "sie", "nicht", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie stehn zu allen Proben.", "tokens": ["Sie", "stehn", "zu", "al\u00b7len", "Pro\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie lieben sich das Neue;", "tokens": ["Sie", "lie\u00b7ben", "sich", "das", "Neu\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch \u00fcber ihre Treue", "tokens": ["Doch", "\u00fc\u00b7ber", "ih\u00b7re", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Verlangt nicht Brief und Siegel;", "tokens": ["Ver\u00b7langt", "nicht", "Brief", "und", "Sie\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie haben alle Fl\u00fcgel.", "tokens": ["Sie", "ha\u00b7ben", "al\u00b7le", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie artig sind die V\u00f6gel,", "tokens": ["Wie", "ar\u00b7tig", "sind", "die", "V\u00f6\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie reizend ist der Kauf!", "tokens": ["Wie", "rei\u00b7zend", "ist", "der", "Kauf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}