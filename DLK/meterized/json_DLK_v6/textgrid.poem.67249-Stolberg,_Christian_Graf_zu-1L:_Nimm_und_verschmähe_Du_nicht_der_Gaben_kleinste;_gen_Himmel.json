{"textgrid.poem.67249": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nimm und verschm\u00e4he Du nicht der Gaben kleinste; gen Himmel", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimm und verschm\u00e4he Du nicht der Gaben kleinste; gen Himmel", "tokens": ["Nimm", "und", "ver\u00b7schm\u00e4\u00b7he", "Du", "nicht", "der", "Ga\u00b7ben", "kleins\u00b7te", ";", "gen", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$.", "APPR", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Hebt, der Gel\u00fcbde Bild, nichtiger Rauch sich ja selbst.", "tokens": ["Hebt", ",", "der", "Ge\u00b7l\u00fcb\u00b7de", "Bild", ",", "nich\u00b7ti\u00b7ger", "Rauch", "sich", "ja", "selbst", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "NN", "$,", "ADJA", "NN", "PRF", "ADV", "ADV", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Sieh, am Altare kniet das fromme H\u00e4uflein, die S\u00f6hne,", "tokens": ["Sieh", ",", "am", "Al\u00b7ta\u00b7re", "kniet", "das", "from\u00b7me", "H\u00e4uf\u00b7lein", ",", "die", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "T\u00f6chter, Eidame, Schn\u00fcr' und Enkelinnen umher.", "tokens": ["T\u00f6ch\u00b7ter", ",", "Ei\u00b7da\u00b7me", ",", "Schn\u00fcr'", "und", "En\u00b7ke\u00b7lin\u00b7nen", "um\u00b7her", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Alle flehen f\u00fcr Dich mit aufgehobenen H\u00e4nden,", "tokens": ["Al\u00b7le", "fle\u00b7hen", "f\u00fcr", "Dich", "mit", "auf\u00b7ge\u00b7ho\u00b7be\u00b7nen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und auf der Mutter Arm lallet des Enkels Gebet.", "tokens": ["Und", "auf", "der", "Mut\u00b7ter", "Arm", "lal\u00b7let", "des", "En\u00b7kels", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Auch wir Freunde dr\u00e4ngen uns dicht um die Opfernden, heben", "tokens": ["Auch", "wir", "Freun\u00b7de", "dr\u00e4n\u00b7gen", "uns", "dicht", "um", "die", "Op\u00b7fern\u00b7den", ",", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "PPER", "NN", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,", "VVFIN"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Flehende H\u00e4nd', und, wie sie, thr\u00e4nende Augen empor.", "tokens": ["Fle\u00b7hen\u00b7de", "H\u00e4nd'", ",", "und", ",", "wie", "sie", ",", "thr\u00e4\u00b7nen\u00b7de", "Au\u00b7gen", "em\u00b7por", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "$,", "PWAV", "PPER", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Edle, Dich segnete Gott mit der sch\u00f6nsten F\u00fclle der Segen,", "tokens": ["Ed\u00b7le", ",", "Dich", "seg\u00b7ne\u00b7te", "Gott", "mit", "der", "sch\u00f6ns\u00b7ten", "F\u00fcl\u00b7le", "der", "Se\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "PPER", "VVFIN", "NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Sage, was bleibet dem Wunsch, das er Dir w\u00fcnsche, zur\u00fcck?", "tokens": ["Sa\u00b7ge", ",", "was", "blei\u00b7bet", "dem", "Wunsch", ",", "das", "er", "Dir", "w\u00fcn\u00b7sche", ",", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Was Er Dir gab, erhalte Dir Gott, erhalte der Deinen", "tokens": ["Was", "Er", "Dir", "gab", ",", "er\u00b7hal\u00b7te", "Dir", "Gott", ",", "er\u00b7hal\u00b7te", "der", "Dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "---+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "H\u00e4uflein Dir, und Dich, ach, Er erhalte Dich uns!", "tokens": ["H\u00e4uf\u00b7lein", "Dir", ",", "und", "Dich", ",", "ach", ",", "Er", "er\u00b7hal\u00b7te", "Dich", "uns", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KON", "PPER", "$,", "ITJ", "$,", "PPER", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+-+---", "measure": "unknown.measure.penta"}}}}}