{"textgrid.poem.61155": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Den Verneinenden", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will es immerhin euch gern erlauben,", "tokens": ["Ich", "will", "es", "im\u00b7mer\u00b7hin", "euch", "gern", "er\u00b7lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ihr mich rechnet als der Schwachen einen,", "tokens": ["Da\u00df", "ihr", "mich", "rech\u00b7net", "als", "der", "Schwa\u00b7chen", "ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "KOKOM", "ART", "NN", "ART", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch sollt ihr meinem Auge nicht das Weinen", "tokens": ["Doch", "sollt", "ihr", "mei\u00b7nem", "Au\u00b7ge", "nicht", "das", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch meinem Mund der Freude L\u00e4cheln rauben.", "tokens": ["Noch", "mei\u00b7nem", "Mund", "der", "Freu\u00b7de", "L\u00e4\u00b7cheln", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zu eurer H\u00f6he kann ich mich nicht schrauben,", "tokens": ["Zu", "eu\u00b7rer", "H\u00f6\u00b7he", "kann", "ich", "mich", "nicht", "schrau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo statt der Sonne frost'ge Sterne scheinen;", "tokens": ["Wo", "statt", "der", "Son\u00b7ne", "frost'\u00b7ge", "Ster\u00b7ne", "schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich kann nicht hassen blo\u00df und blo\u00df verneinen;", "tokens": ["Ich", "kann", "nicht", "has\u00b7sen", "blo\u00df", "und", "blo\u00df", "ver\u00b7nei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVFIN", "ADV", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dies Herz bedarf's, zu lieben und zu glauben.", "tokens": ["Dies", "Herz", "be\u00b7da\u00b7rf's", ",", "zu", "lie\u00b7ben", "und", "zu", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "NE", "$,", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Da\u00df ihr euch Heiden nennet, h\u00f6r' ich sagen,", "tokens": ["Da\u00df", "ihr", "euch", "Hei\u00b7den", "nen\u00b7net", ",", "h\u00f6r'", "ich", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "VVFIN", "$,", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch jene sahn den Gott im Sturm der Meere,", "tokens": ["Doch", "je\u00b7ne", "sahn", "den", "Gott", "im", "Sturm", "der", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ART", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Gott im Donner und im Sonnenwagen.", "tokens": ["Den", "Gott", "im", "Don\u00b7ner", "und", "im", "Son\u00b7nen\u00b7wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr aber m\u00f6chtet frech mit erznem Speere", "tokens": ["Ihr", "a\u00b7ber", "m\u00f6ch\u00b7tet", "frech", "mit", "erz\u00b7nem", "Spee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Tr\u00fcmmer jedes G\u00f6tterbild zerschlagen -", "tokens": ["In", "Tr\u00fcm\u00b7mer", "je\u00b7des", "G\u00f6t\u00b7ter\u00b7bild", "zer\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So bleibt euch nichts denn als die gro\u00dfe Leere.", "tokens": ["So", "bleibt", "euch", "nichts", "denn", "als", "die", "gro\u00b7\u00dfe", "Lee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADV", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}