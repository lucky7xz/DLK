{"textgrid.poem.31172": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Als Wegzehrung", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott weiss, du bist ein braver Junge,", "tokens": ["Gott", "weiss", ",", "du", "bist", "ein", "bra\u00b7ver", "Jun\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch neune solcher machen zehn,", "tokens": ["Noch", "neu\u00b7ne", "sol\u00b7cher", "ma\u00b7chen", "zehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Herz ist rein wie deine Zunge", "tokens": ["Dein", "Herz", "ist", "rein", "wie", "dei\u00b7ne", "Zun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KOKOM", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schwerlich wirst du untergehn.", "tokens": ["Und", "schwer\u00b7lich", "wirst", "du", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du wogst noch niemals eine Lanze", "tokens": ["Du", "wogst", "noch", "nie\u00b7mals", "ei\u00b7ne", "Lan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und singst von Liebe nur und Lenz \u2013", "tokens": ["Und", "singst", "von", "Lie\u00b7be", "nur", "und", "Lenz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADV", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So geh denn hin, mein Freund, und tanze", "tokens": ["So", "geh", "denn", "hin", ",", "mein", "Freund", ",", "und", "tan\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Eiertanz der Convenienz!", "tokens": ["Den", "Ei\u00b7er\u00b7tanz", "der", "Con\u00b7ve\u00b7ni\u00b7enz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}