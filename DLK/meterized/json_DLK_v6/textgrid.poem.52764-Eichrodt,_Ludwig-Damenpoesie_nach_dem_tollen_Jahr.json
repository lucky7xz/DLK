{"textgrid.poem.52764": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Damenpoesie nach dem tollen Jahr", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo ist das Land, da noch Gesetze bl\u00fchn,", "tokens": ["Wo", "ist", "das", "Land", ",", "da", "noch", "Ge\u00b7set\u00b7ze", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "KOUS", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo Tugenden Millionen Herzen gl\u00fch'n,", "tokens": ["Wo", "Tu\u00b7gen\u00b7den", "Mil\u00b7lion\u00b7en", "Her\u00b7zen", "gl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Vaterauge sanft auf Alles blickt,", "tokens": ["Ein", "Va\u00b7te\u00b7rau\u00b7ge", "sanft", "auf", "Al\u00b7les", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und der Verf\u00fchrung einmal gar Nichts gl\u00fcckt?", "tokens": ["Und", "der", "Ver\u00b7f\u00fch\u00b7rung", "ein\u00b7mal", "gar", "Nichts", "gl\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wo ist das Schlo\u00df voll wundersch\u00f6ner Prinzen,", "tokens": ["Wo", "ist", "das", "Schlo\u00df", "voll", "wun\u00b7der\u00b7sch\u00f6\u00b7ner", "Prin\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da nicht Verr\u00e4thermienen teuflisch grinzen,", "tokens": ["Da", "nicht", "Ver\u00b7r\u00e4\u00b7ther\u00b7mie\u00b7nen", "teuf\u00b7lisch", "grin\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo nur die Treue wandelt aus und ein,", "tokens": ["Wo", "nur", "die", "Treu\u00b7e", "wan\u00b7delt", "aus", "und", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und stumme Ehrfurcht ist kein leerer Schein?", "tokens": ["Und", "stum\u00b7me", "Ehr\u00b7furcht", "ist", "kein", "lee\u00b7rer", "Schein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wo ist der Staat, da Sinn f\u00fcr Ordnung ist,", "tokens": ["Wo", "ist", "der", "Staat", ",", "da", "Sinn", "f\u00fcr", "Ord\u00b7nung", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "KOUS", "NN", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu W\u00fcrden nur gelangt der wahre Christ,", "tokens": ["Zu", "W\u00fcr\u00b7den", "nur", "ge\u00b7langt", "der", "wah\u00b7re", "Christ", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zufriedenheit bei Unterthanen wohnt,", "tokens": ["Zu\u00b7frie\u00b7den\u00b7heit", "bei", "Un\u00b7ter\u00b7tha\u00b7nen", "wohnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sich Verdienst in milder Gnade sonnt?", "tokens": ["Und", "sich", "Ver\u00b7dienst", "in", "mil\u00b7der", "Gna\u00b7de", "sonnt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wo Jeder mit den Seinen sich begn\u00fcgt,", "tokens": ["Wo", "Je\u00b7der", "mit", "den", "Sei\u00b7nen", "sich", "be\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "PPOSS", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und h\u00f6hern W\u00fcnschen man sich gerne f\u00fcgt,", "tokens": ["Und", "h\u00f6\u00b7hern", "W\u00fcn\u00b7schen", "man", "sich", "ger\u00b7ne", "f\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PIS", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo Neid und Habsucht, Scheelgier und Verrath", "tokens": ["Wo", "Neid", "und", "Hab\u00b7sucht", ",", "Scheel\u00b7gier", "und", "Ver\u00b7rath"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht \u00fcberwuchern darf des Guten Saat?", "tokens": ["Nicht", "\u00fc\u00b7berw\u00b7uc\u00b7hern", "darf", "des", "Gu\u00b7ten", "Saat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wo man allein noch Ritterlichkeit trifft,", "tokens": ["Wo", "man", "al\u00b7lein", "noch", "Rit\u00b7ter\u00b7lich\u00b7keit", "trifft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo nimmer wirkt das Demokratengift,", "tokens": ["Wo", "nim\u00b7mer", "wirkt", "das", "De\u00b7mo\u00b7kra\u00b7ten\u00b7gift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo nicht die Faulheit auf der Bierbank thront,", "tokens": ["Wo", "nicht", "die", "Faul\u00b7heit", "auf", "der", "Bier\u00b7bank", "thront", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Pflichterf\u00fcllung durch sich selbst belohnt?", "tokens": ["Und", "Pflich\u00b7ter\u00b7f\u00fcl\u00b7lung", "durch", "sich", "selbst", "be\u00b7lohnt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wo stille Demuth annoch findet Statt,", "tokens": ["Wo", "stil\u00b7le", "De\u00b7muth", "an\u00b7noch", "fin\u00b7det", "Statt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADV", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und noch der Adel was zu sagen hat,", "tokens": ["Und", "noch", "der", "A\u00b7del", "was", "zu", "sa\u00b7gen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PIS", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo Herr und Knecht kein leerer Name ist,", "tokens": ["Wo", "Herr", "und", "Knecht", "kein", "lee\u00b7rer", "Na\u00b7me", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PIAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kurz, wo Religion und Christenthum noch ist!?", "tokens": ["Kurz", ",", "wo", "Re\u00b7li\u00b7gi\u00b7on", "und", "Chris\u00b7ten\u00b7thum", "noch", "ist", "!?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "NN", "KON", "NN", "ADV", "VAFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}}}}