{"textgrid.poem.34965": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hatte mein Haupt die schwarze Frau", "tokens": ["Es", "hat\u00b7te", "mein", "Haupt", "die", "schwar\u00b7ze", "Frau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Z\u00e4rtlich ans Herz geschlossen;", "tokens": ["Z\u00e4rt\u00b7lich", "ans", "Herz", "ge\u00b7schlos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ach! meine Haare wurden grau,", "tokens": ["Ach", "!", "mei\u00b7ne", "Haa\u00b7re", "wur\u00b7den", "grau", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo ihre Tr\u00e4nen geflossen.", "tokens": ["Wo", "ih\u00b7re", "Tr\u00e4\u00b7nen", "ge\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie k\u00fc\u00dfte mich lahm, sie k\u00fc\u00dfte mich krank,", "tokens": ["Sie", "k\u00fc\u00df\u00b7te", "mich", "lahm", ",", "sie", "k\u00fc\u00df\u00b7te", "mich", "krank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie k\u00fc\u00dfte mir blind die Augen;", "tokens": ["Sie", "k\u00fc\u00df\u00b7te", "mir", "blind", "die", "Au\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Mark aus meinem R\u00fcckgrat trank", "tokens": ["Das", "Mark", "aus", "mei\u00b7nem", "R\u00fcck\u00b7grat", "trank"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Mund mit wildem Saugen.", "tokens": ["Ihr", "Mund", "mit", "wil\u00b7dem", "Sau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mein Leib ist jetzt ein Leichnam, worin", "tokens": ["Mein", "Leib", "ist", "jetzt", "ein", "Leich\u00b7nam", ",", "wo\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "$,", "PWAV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der Geist ist eingekerkert \u2013", "tokens": ["Der", "Geist", "ist", "ein\u00b7ge\u00b7ker\u00b7kert", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Manchmal wird ihm unwirsch zu Sinn,", "tokens": ["Manch\u00b7mal", "wird", "ihm", "un\u00b7wirsch", "zu", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Er tobt und rast und berserkert.", "tokens": ["Er", "tobt", "und", "rast", "und", "ber\u00b7ser\u00b7kert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ohnm\u00e4chtige Fl\u00fcche! Dein schlimmster Fluch", "tokens": ["Ohn\u00b7m\u00e4ch\u00b7ti\u00b7ge", "Fl\u00fc\u00b7che", "!", "Dein", "schlimms\u00b7ter", "Fluch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wird keine Fliege t\u00f6ten.", "tokens": ["Wird", "kei\u00b7ne", "Flie\u00b7ge", "t\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ertrage die Schickung, und versuch,", "tokens": ["Er\u00b7tra\u00b7ge", "die", "Schi\u00b7ckung", ",", "und", "ver\u00b7such", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gelinde zu flennen, zu beten.", "tokens": ["Ge\u00b7lin\u00b7de", "zu", "flen\u00b7nen", ",", "zu", "be\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}