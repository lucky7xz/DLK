{"textgrid.poem.37483": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Vorwort", "genre": "verse", "period": "N.A.", "pub_year": 1864, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, was mu\u00df man oft von b\u00f6sen", "tokens": ["Ach", ",", "was", "mu\u00df", "man", "oft", "von", "b\u00f6\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWS", "VMFIN", "PIS", "ADV", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kindern h\u00f6ren oder lesen!!", "tokens": ["Kin\u00b7dern", "h\u00f6\u00b7ren", "o\u00b7der", "le\u00b7sen", "!!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie zum Beispiel hier von diesen,", "tokens": ["Wie", "zum", "Bei\u00b7spiel", "hier", "von", "die\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ADV", "APPR", "PDAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welche Max und Moritz hie\u00dfen;", "tokens": ["Wel\u00b7che", "Max", "und", "Mo\u00b7ritz", "hie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NE", "KON", "NE", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die, anstatt durch weise Lehren", "tokens": ["Die", ",", "an\u00b7statt", "durch", "wei\u00b7se", "Leh\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUI", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sich zum Guten zu bekehren,", "tokens": ["Sich", "zum", "Gu\u00b7ten", "zu", "be\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oftmals noch dar\u00fcber lachten", "tokens": ["Oft\u00b7mals", "noch", "da\u00b7r\u00fc\u00b7ber", "lach\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "PAV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sich heimlich lustig machten. \u2013", "tokens": ["Und", "sich", "heim\u00b7lich", "lus\u00b7tig", "mach\u00b7ten", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PRF", "ADJD", "ADJD", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u2013 Ja, zur \u00dcbelt\u00e4tigkeit,", "tokens": ["\u2013", "Ja", ",", "zur", "\u00dc\u00b7bel\u00b7t\u00e4\u00b7tig\u00b7keit", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ja, dazu ist man bereit! \u2013", "tokens": ["Ja", ",", "da\u00b7zu", "ist", "man", "be\u00b7reit", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "PAV", "VAFIN", "PIS", "ADJD", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "\u2013 Menschen necken, Tiere qu\u00e4len,", "tokens": ["\u2013", "Men\u00b7schen", "ne\u00b7cken", ",", "Tie\u00b7re", "qu\u00e4\u00b7len", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NN", "VVINF", "$,", "ADJA", "VVINF", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.8": {"text": "\u00c4pfel, Birnen, Zwetschen stehlen \u2013\u2013", "tokens": ["\u00c4p\u00b7fel", ",", "Bir\u00b7nen", ",", "Zwet\u00b7schen", "steh\u00b7len", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVINF", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Das ist freilich angenehmer", "tokens": ["Das", "ist", "frei\u00b7lich", "an\u00b7ge\u00b7neh\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und dazu auch viel bequemer,", "tokens": ["Und", "da\u00b7zu", "auch", "viel", "be\u00b7que\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADV", "ADV", "ADJD", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.11": {"text": "Als in Kirche oder Schule", "tokens": ["Als", "in", "Kir\u00b7che", "o\u00b7der", "Schu\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Festzusitzen auf dem Stuhle. \u2013", "tokens": ["Fest\u00b7zu\u00b7sit\u00b7zen", "auf", "dem", "Stuh\u00b7le", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "\u2013 Aber wehe, wehe, wehe!", "tokens": ["\u2013", "A\u00b7ber", "we\u00b7he", ",", "we\u00b7he", ",", "we\u00b7he", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "KON", "ADJD", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Wenn ich auf das Ende sehe!! \u2013", "tokens": ["Wenn", "ich", "auf", "das", "En\u00b7de", "se\u00b7he", "!!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "\u2013 Ach, das war ein schlimmes Ding,", "tokens": ["\u2013", "Ach", ",", "das", "war", "ein", "schlim\u00b7mes", "Ding", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Wie es Max und Moritz ging.", "tokens": ["Wie", "es", "Max", "und", "Mo\u00b7ritz", "ging", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NE", "KON", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "\u2013 Drum ist hier, was sie getrieben,", "tokens": ["\u2013", "Drum", "ist", "hier", ",", "was", "sie", "ge\u00b7trie\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "VAFIN", "ADV", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Abgemalt und aufgeschrieben.", "tokens": ["Ab\u00b7ge\u00b7malt", "und", "auf\u00b7ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}