{"dta.poem.7351": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Dienstag .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Auf der breiten Tenne drehn", "tokens": ["Auf", "der", "brei\u00b7ten", "Ten\u00b7ne", "drehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Paar an Paar so nett,", "tokens": ["Paar", "an", "Paar", "so", "nett", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wo die Musikanten stehn,", "tokens": ["Wo", "die", "Mu\u00b7si\u00b7kan\u00b7ten", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Geig und Klarinett, \u2014", "tokens": ["Geig", "und", "Kla\u00b7ri\u00b7nett", ","], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "$,", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Auch der Brummba\u00df rumpelt drein,", "tokens": ["Auch", "der", "Brumm\u00b7ba\u00df", "rum\u00b7pelt", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sieht man noch den Br\u00e4ut'gamsschrein", "tokens": ["Sieht", "man", "noch", "den", "Br\u00e4ut'\u00b7gams\u00b7schrein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und das Hochzeitbett.", "tokens": ["Und", "das", "Hoch\u00b7zeit\u00b7bett", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Etwas eigen, etwas schlau,", "tokens": ["Et\u00b7was", "ei\u00b7gen", ",", "et\u00b7was", "schlau", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ein wenig bleich,", "tokens": ["Und", "ein", "we\u00b7nig", "bleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sittsam sieht die junge Frau,", "tokens": ["Sitt\u00b7sam", "sieht", "die", "jun\u00b7ge", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcrdevoll zugleich;", "tokens": ["W\u00fcr\u00b7de\u00b7voll", "zu\u00b7gleich", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Denn sie ist des Hauses Spro\u00df,", "tokens": ["Denn", "sie", "ist", "des", "Hau\u00b7ses", "Spro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn sie f\u00fchrt den Eh'geno\u00df", "tokens": ["Denn", "sie", "f\u00fchrt", "den", "Eh'\u00b7ge\u00b7no\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In ihr Erb' und Reich.", "tokens": ["In", "ihr", "Erb'", "und", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Sippschaft ist ein weites Band,", "tokens": ["Sipp\u00b7schaft", "ist", "ein", "wei\u00b7tes", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht gar viel hinein;", "tokens": ["Geht", "gar", "viel", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Hundert Kappen goldentbrannt,", "tokens": ["Hun\u00b7dert", "Kap\u00b7pen", "gol\u00b7dent\u00b7brannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kreuze funkeln drein;", "tokens": ["Kreu\u00b7ze", "fun\u00b7keln", "drein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wie das dr\u00e4ngt und wie das schiebt!", "tokens": ["Wie", "das", "dr\u00e4ngt", "und", "wie", "das", "schiebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVFIN", "KON", "PWAV", "PDS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was sich kennt und was sich liebt", "tokens": ["Was", "sich", "kennt", "und", "was", "sich", "liebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "VVFIN", "KON", "PWS", "PRF", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Will beisammen seyn.", "tokens": ["Will", "bei\u00b7sam\u00b7men", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "VVPP", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Nun ein schallend Vivat bricht", "tokens": ["Nun", "ein", "schal\u00b7lend", "Vi\u00b7vat", "bricht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJD", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Schwarme aus,", "tokens": ["In", "dem", "Schwar\u00b7me", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wo sogar die Thiere nicht", "tokens": ["Wo", "so\u00b7gar", "die", "Thie\u00b7re", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weigern den Applaus.", "tokens": ["Wei\u00b7gern", "den", "Ap\u00b7plaus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ja, wie an der Krippe fein", "tokens": ["Ja", ",", "wie", "an", "der", "Krip\u00b7pe", "fein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PWAV", "APPR", "ART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Br\u00fcllen Ochs und Eselein", "tokens": ["Br\u00fcl\u00b7len", "Ochs", "und", "E\u00b7sel\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ueber'n Trog hinaus.", "tokens": ["Ue\u00b7ber'n", "Trog", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Ganz verdutzt der junge Mann", "tokens": ["Ganz", "ver\u00b7dutzt", "der", "jun\u00b7ge", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kaum die Flasche h\u00e4lt,", "tokens": ["Kaum", "die", "Fla\u00b7sche", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sp\u00e4sse hageln drauf und dran,", "tokens": ["Sp\u00e4s\u00b7se", "ha\u00b7geln", "drauf", "und", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "KON", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Keiner neben f\u00e4llt;", "tokens": ["Kei\u00b7ner", "ne\u00b7ben", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "APPR", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Doch er lacht und reicht die Hand.", "tokens": ["Doch", "er", "lacht", "und", "reicht", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nun! er ist f\u00fcr seinen Stand", "tokens": ["Nun", "!", "er", "ist", "f\u00fcr", "sei\u00b7nen", "Stand"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schon ein Mann von Welt.", "tokens": ["Schon", "ein", "Mann", "von", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Alte Frauen schwei\u00dfbedeckt,", "tokens": ["Al\u00b7te", "Frau\u00b7en", "schwei\u00df\u00b7be\u00b7deckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Junge M\u00e4gd' im Lauf,", "tokens": ["Jun\u00b7ge", "M\u00e4gd'", "im", "Lauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Spenden was der Korb verdeckt,", "tokens": ["Spen\u00b7den", "was", "der", "Korb", "ver\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Reihen ab und auf.", "tokens": ["Rei\u00b7hen", "ab", "und", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Sieben Tische kann man sehn,", "tokens": ["Sie\u00b7ben", "Ti\u00b7sche", "kann", "man", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sieben Kaffeekessel stehn", "tokens": ["Sie\u00b7ben", "Kaf\u00b7fee\u00b7kes\u00b7sel", "stehn"], "token_info": ["word", "word", "word"], "pos": ["CARD", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Breit und gl\u00e4nzend drauf.", "tokens": ["Breit", "und", "gl\u00e4n\u00b7zend", "drauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Aber freundlich, wie er kam,", "tokens": ["A\u00b7ber", "freund\u00b7lich", ",", "wie", "er", "kam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sucht der Pfarrer gut", "tokens": ["Sucht", "der", "Pfar\u00b7rer", "gut"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Dr\u00fcben unter tausend Kram", "tokens": ["Dr\u00fc\u00b7ben", "un\u00b7ter", "tau\u00b7send", "Kram"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "CARD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seinen Stab und Hut;", "tokens": ["Sei\u00b7nen", "Stab", "und", "Hut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Dankt noch sch\u00f6n der Frau vom Haus;", "tokens": ["Dankt", "noch", "sch\u00f6n", "der", "Frau", "vom", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In die D\u00e4mmerung hinaus", "tokens": ["In", "die", "D\u00e4m\u00b7me\u00b7rung", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Trabt er wohlgemuth;", "tokens": ["Trabt", "er", "wohl\u00b7ge\u00b7muth", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Wandelt durch die Abendruh'", "tokens": ["Wan\u00b7delt", "durch", "die", "A\u00b7bendruh'"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sinnend allerlei:", "tokens": ["Sin\u00b7nend", "al\u00b7ler\u00b7lei", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PIAT", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "\u201eei, dort gieng es l\u00f6blich zu,", "tokens": ["\u201e", "ei", ",", "dort", "gieng", "es", "l\u00f6b\u00b7lich", "zu", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Munter, und nicht frei.", "tokens": ["Mun\u00b7ter", ",", "und", "nicht", "frei", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PTKNEG", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Aber \u2014 aber \u2014 aber doch \u2014\u201c", "tokens": ["A\u00b7ber", "a\u00b7ber", "a\u00b7ber", "doch", "\u201c"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "$(", "ADV", "$(", "ADV", "ADV", "$(", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Und ein langes Aber noch", "tokens": ["Und", "ein", "lan\u00b7ges", "A\u00b7ber", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "F\u00fcgt er seufzend bei.", "tokens": ["F\u00fcgt", "er", "seuf\u00b7zend", "bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "\u201ewie das flimmert! wie das lacht!", "tokens": ["\u201e", "wie", "das", "flim\u00b7mert", "!", "wie", "das", "lacht", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PDS", "VVFIN", "$.", "PWAV", "PDS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kanten H\u00e4ndebreit!\u201c", "tokens": ["Kan\u00b7ten", "H\u00e4n\u00b7de\u00b7breit", "!", "\u201c"], "token_info": ["word", "word", "punct", "punct"], "pos": ["VMFIN", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ach die schn\u00f6de Kleiderpracht", "tokens": ["Ach", "die", "schn\u00f6\u00b7de", "Klei\u00b7der\u00b7pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["ITJ", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Macht ihm tausend Leid.", "tokens": ["Macht", "ihm", "tau\u00b7send", "Leid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "CARD", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Und nun gar \u2014 er war nicht blind \u2014", "tokens": ["Und", "nun", "gar", "er", "war", "nicht", "blind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$(", "PPER", "VAFIN", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Eines armen Mannes Kind;", "tokens": ["Ei\u00b7nes", "ar\u00b7men", "Man\u00b7nes", "Kind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nein, das gieng zu weit.", "tokens": ["Nein", ",", "das", "gieng", "zu", "weit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VVFIN", "PTKA", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Kurz, er nimmt sich's ernstlich vor,", "tokens": ["Kurz", ",", "er", "nimmt", "sich's", "ernst\u00b7lich", "vor", ","], "token_info": ["word", "punct", "word", "word", "word", "wo