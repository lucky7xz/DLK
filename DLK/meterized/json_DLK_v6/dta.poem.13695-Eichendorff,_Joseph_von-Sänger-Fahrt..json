{"dta.poem.13695": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "S\u00e4nger-Fahrt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Euch m\u00f6cht' ich alles geben,", "tokens": ["Euch", "m\u00f6cht'", "ich", "al\u00b7les", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ich bin f\u00fcrstlich reich,", "tokens": ["Und", "ich", "bin", "f\u00fcrst\u00b7lich", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Herzblut und mein Leben,", "tokens": ["Mein", "Herz\u00b7blut", "und", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ihr Br\u00fcder, alles f\u00fcr Euch!", "tokens": ["Ihr", "Br\u00fc\u00b7der", ",", "al\u00b7les", "f\u00fcr", "Euch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PIS", "APPR", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}