{"dta.poem.10126": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Quelle des Materialischen und des Gei-  \n stigen Feuers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es scheint des Feuers reges Wesen, es scheinen Licht\nund heisse Flammen", "tokens": ["Es", "scheint", "des", "Feu\u00b7ers", "re\u00b7ges", "We\u00b7sen", ",", "es", "schei\u00b7nen", "Licht", "und", "heis\u00b7se", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,", "PPER", "VVFIN", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Von aller W\u00e4rm\u2019 und Lichter Quell, dem Sonnen-Feuer,", "tokens": ["Von", "al\u00b7ler", "W\u00e4rm'", "und", "Lich\u00b7ter", "Quell", ",", "dem", "Son\u00b7nen\u00b7Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und in gewisser C\u00f6rper Wesen, das sich dazu vor andern", "tokens": ["Und", "in", "ge\u00b7wis\u00b7ser", "C\u00f6r\u00b7per", "We\u00b7sen", ",", "das", "sich", "da\u00b7zu", "vor", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "NN", "$,", "PRELS", "PRF", "PAV", "APPR", "ADJA"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Und mehr und minder schweflicht ist, hinein gesenckt, hinein", "tokens": ["Und", "mehr", "und", "min\u00b7der", "schwef\u00b7licht", "ist", ",", "hin\u00b7ein", "ge\u00b7senckt", ",", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "KON", "ADV", "ADJD", "VAFIN", "$,", "ADV", "VVPP", "$,", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Die aber, wann sich andre Theile, die feurig sind, zu ihnen", "tokens": ["Die", "a\u00b7ber", ",", "wann", "sich", "and\u00b7re", "Thei\u00b7le", ",", "die", "feu\u00b7rig", "sind", ",", "zu", "ih\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "$,", "PWAV", "PRF", "ADJA", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "APPR", "PPER"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Sich schnell entz\u00fcnden, und in Eil\u2019, als lodernd, in die H\u00f6he", "tokens": ["Sich", "schnell", "ent\u00b7z\u00fcn\u00b7den", ",", "und", "in", "Eil'", ",", "als", "lo\u00b7dernd", ",", "in", "die", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "ADJD", "VVPP", "$,", "KON", "APPR", "NN", "$,", "KOUS", "VVPP", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Da\u00df wir dieselben Theilchen aber, bevor sie angez\u00fcndet", "tokens": ["Da\u00df", "wir", "die\u00b7sel\u00b7ben", "Theil\u00b7chen", "a\u00b7ber", ",", "be\u00b7vor", "sie", "an\u00b7ge\u00b7z\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDAT", "NN", "ADV", "$,", "KOUS", "PPER", "VVPP"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Nicht scheinen, gl\u00e4ntzen, leuchten, strahlen, und nicht, als lo-", "tokens": ["Nicht", "schei\u00b7nen", ",", "gl\u00e4nt\u00b7zen", ",", "leuch\u00b7ten", ",", "strah\u00b7len", ",", "und", "nicht", ",", "als", "lo"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "VVFIN", "$,", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,", "KON", "PTKNEG", "$,", "KOUS", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ist ja so wenig zu bewundern, als da\u00df man selber nicht", "tokens": ["Ist", "ja", "so", "we\u00b7nig", "zu", "be\u00b7wun\u00b7dern", ",", "als", "da\u00df", "man", "sel\u00b7ber", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "PIS", "PTKZU", "VVINF", "$,", "KOKOM", "KOUS", "PIS", "ADV", "PTKNEG"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Das Licht, so alle Ding\u2019 erhellet, und dessen eigentlichen", "tokens": ["Das", "Licht", ",", "so", "al\u00b7le", "Ding'", "er\u00b7hel\u00b7let", ",", "und", "des\u00b7sen", "ei\u00b7gent\u00b7li\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "PIAT", "NN", "VVFIN", "$,", "KON", "PRELAT", "ADJA"], "meter": "-+-+-+-+--+-++--", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "In seinem eignen Glantz allein, ohn andrer C\u00f6rper Gegen-", "tokens": ["In", "sei\u00b7nem", "eig\u00b7nen", "Glantz", "al\u00b7lein", ",", "ohn", "an\u00b7drer", "C\u00f6r\u00b7per", "Ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "$,", "KOUI", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Zu mercken, zu erblicken taugt, noch eigentlich zu sehn ver-", "tokens": ["Zu", "mer\u00b7cken", ",", "zu", "er\u00b7bli\u00b7cken", "taugt", ",", "noch", "ei\u00b7gent\u00b7lich", "zu", "sehn", "ver"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "VVFIN", "$,", "ADV", "ADV", "PTKZU", "VVINF", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Fast auf dieselbe Weise scheinet,", "tokens": ["Fast", "auf", "die\u00b7sel\u00b7be", "Wei\u00b7se", "schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch unsers Geistes Licht vereinet", "tokens": ["Auch", "un\u00b7sers", "Geis\u00b7tes", "Licht", "ver\u00b7ei\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit der allgegenw\u00e4rtgen GOTTHEIT, und Jhrem Licht,", "tokens": ["Mit", "der", "all\u00b7ge\u00b7gen\u00b7w\u00e4rt\u00b7gen", "GoT\u00b7T\u00b7HEIT", ",", "und", "Ih\u00b7rem", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}}, "stanza.4": {"line.1": {"text": "Die\u00df eingeschr\u00e4nckte Feur wird rege, wenn eine gleich-", "tokens": ["Die\u00df", "ein\u00b7ge\u00b7schr\u00e4nck\u00b7te", "Feur", "wird", "re\u00b7ge", ",", "wenn", "ei\u00b7ne", "gleich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "NN", "VAFIN", "ADJA", "$,", "KOUS", "ART", "TRUNC"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die, durch des Sch\u00f6pfers Wort und Willen, in Seinen Crea-", "tokens": ["Die", ",", "durch", "des", "Sch\u00f6p\u00b7fers", "Wort", "und", "Wil\u00b7len", ",", "in", "Sei\u00b7nen", "Crea"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "APPR", "ART", "NN", "NN", "KON", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sich zu ihm f\u00fcget; dann entstehet, durch die Verwundrung,", "tokens": ["Sich", "zu", "ihm", "f\u00fc\u00b7get", ";", "dann", "ent\u00b7ste\u00b7het", ",", "durch", "die", "Ver\u00b7wund\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.4": {"text": "Die Gluht der Andacht l\u00f6s\u2019t sie auf, wann Lieb\u2019 und Danck-", "tokens": ["Die", "Gluht", "der", "An\u00b7dacht", "l\u00f6s't", "sie", "auf", ",", "wann", "Lieb'", "und", "Dan\u00b7ck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da ihre geistge Flammen denn, mit unausdr\u00fccklichem Ver-", "tokens": ["Da", "ih\u00b7re", "geist\u00b7ge", "Flam\u00b7men", "denn", ",", "mit", "un\u00b7aus\u00b7dr\u00fcck\u00b7li\u00b7chem", "Ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "$,", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+---", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Sich zu der ersten H\u00f6he schwingen, sich wieder zu der Ur-", "tokens": ["Sich", "zu", "der", "ers\u00b7ten", "H\u00f6\u00b7he", "schwin\u00b7gen", ",", "sich", "wie\u00b7der", "zu", "der", "Ur"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,", "PRF", "ADV", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}}}}