{"textgrid.poem.55619": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "10", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Allersch\u00f6nste bist du anerkannt,", "tokens": ["Als", "Al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "bist", "du", "an\u00b7er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bist K\u00f6nigin des Blumenreichs genannt;", "tokens": ["Bist", "K\u00f6\u00b7ni\u00b7gin", "des", "Blu\u00b7men\u00b7reichs", "ge\u00b7nannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Unwidersprechlich allgemeines Zeugnis,", "tokens": ["Un\u00b7wi\u00b7der\u00b7sprech\u00b7lich", "all\u00b7ge\u00b7mei\u00b7nes", "Zeug\u00b7nis", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Streitsucht verbannend, wundersam Ereignis!", "tokens": ["Streit\u00b7sucht", "ver\u00b7ban\u00b7nend", ",", "wun\u00b7der\u00b7sam", "Er\u00b7eig\u00b7nis", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "ADJD", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Du bist es also, bist kein blo\u00dfer Schein,", "tokens": ["Du", "bist", "es", "al\u00b7so", ",", "bist", "kein", "blo\u00b7\u00dfer", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$,", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In dir trifft Schaun und Glauben \u00fcberein;", "tokens": ["In", "dir", "trifft", "Schaun", "und", "Glau\u00b7ben", "\u00fc\u00b7be\u00b7re\u00b7in", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Doch Forschung strebt und ringt, erm\u00fcdend nie,", "tokens": ["Doch", "For\u00b7schung", "strebt", "und", "ringt", ",", "er\u00b7m\u00fc\u00b7dend", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "KON", "VVFIN", "$,", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nach dem Gesetz, dem Grund, ", "tokens": ["Nach", "dem", "Ge\u00b7setz", ",", "dem", "Grund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}