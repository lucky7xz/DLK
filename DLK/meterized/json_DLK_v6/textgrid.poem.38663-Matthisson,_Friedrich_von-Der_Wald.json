{"textgrid.poem.38663": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Wald", "genre": "verse", "period": "N.A.", "pub_year": 1791, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herrlich ists im Gr\u00fcnen!", "tokens": ["Herr\u00b7lich", "ists", "im", "Gr\u00fc\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mehr als Opernb\u00fchnen", "tokens": ["Mehr", "als", "O\u00b7pern\u00b7b\u00fch\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "KOKOM", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ist mir Abends unser Wald,", "tokens": ["Ist", "mir", "A\u00b7bends", "un\u00b7ser", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn das Dorfgel\u00e4ute", "tokens": ["Wenn", "das", "Dorf\u00b7ge\u00b7l\u00e4u\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Dumpfig aus der Weite", "tokens": ["Dump\u00b7fig", "aus", "der", "Wei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Durch der Wipfel D\u00e4mmrung hallt.", "tokens": ["Durch", "der", "Wip\u00b7fel", "D\u00e4mm\u00b7rung", "hallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hoch aus mildem Glanze", "tokens": ["Hoch", "aus", "mil\u00b7dem", "Glan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Streut im leichten Tanze", "tokens": ["Streut", "im", "leich\u00b7ten", "Tan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mir das Eichhorn Laub und Moos;", "tokens": ["Mir", "das", "Eich\u00b7horn", "Laub", "und", "Moos", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fink' und Amsel rauschen", "tokens": ["Fink'", "und", "Am\u00b7sel", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KON", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Durch die Zweig' und lauschen", "tokens": ["Durch", "die", "Zweig'", "und", "lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Rings im jungen Maigespro\u00df.", "tokens": ["Rings", "im", "jun\u00b7gen", "Mai\u00b7ge\u00b7spro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Fern am Ellernholze", "tokens": ["Fern", "am", "El\u00b7lern\u00b7hol\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Grast in Ruh' der stolze", "tokens": ["Grast", "in", "Ruh'", "der", "stol\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Kronhirsch l\u00e4ngs dem Weidendamm;", "tokens": ["Kron\u00b7hirsch", "l\u00e4ngs", "dem", "Wei\u00b7den\u00b7damm", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueberh\u00fcllt von Laube", "tokens": ["Ue\u00b7ber\u00b7h\u00fcllt", "von", "Lau\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Girrt die Ringeltaube", "tokens": ["Girrt", "die", "Rin\u00b7gel\u00b7tau\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Im Gerank am Eichenstamm.", "tokens": ["Im", "Ge\u00b7rank", "am", "Ei\u00b7chen\u00b7stamm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "In der Abendhelle", "tokens": ["In", "der", "A\u00b7bend\u00b7hel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Funkelt die Libelle,", "tokens": ["Fun\u00b7kelt", "die", "Li\u00b7bel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sanft am Farrenkraut gewiegt;", "tokens": ["Sanft", "am", "Far\u00b7ren\u00b7kraut", "ge\u00b7wiegt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fcckenschw\u00e4rm' erheben", "tokens": ["M\u00fc\u00b7cken\u00b7schw\u00e4rm'", "er\u00b7he\u00b7ben"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sich aus Binsengr\u00e4ben", "tokens": ["Sich", "aus", "Bin\u00b7sen\u00b7gr\u00e4\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PRF", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und der braune Schr\u00f6ter fliegt.", "tokens": ["Und", "der", "brau\u00b7ne", "Schr\u00f6\u00b7ter", "fliegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Iris und Ranunkel", "tokens": ["I\u00b7ris", "und", "Ra\u00b7nun\u00b7kel"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Bl\u00fchn im Weidendunkel,", "tokens": ["Bl\u00fchn", "im", "Wei\u00b7den\u00b7dun\u00b7kel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wo durch Tuf die Quelle sch\u00e4umt,", "tokens": ["Wo", "durch", "Tuf", "die", "Quel\u00b7le", "sch\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die mit Spiegelgl\u00e4tte", "tokens": ["Die", "mit", "Spie\u00b7gel\u00b7gl\u00e4t\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Dort im Rasenbette", "tokens": ["Dort", "im", "Ra\u00b7sen\u00b7bet\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wies' und Birkenthal ums\u00e4umt.", "tokens": ["Wies'", "und", "Bir\u00b7ken\u00b7thal", "um\u00b7s\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ob dem Felsenpfade", "tokens": ["Ob", "dem", "Fel\u00b7sen\u00b7pfa\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schimmert die Kaskade,", "tokens": ["Schim\u00b7mert", "die", "Kas\u00b7ka\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie ein flatternd Silberband.", "tokens": ["Wie", "ein", "flat\u00b7ternd", "Sil\u00b7ber\u00b7band", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hell durch Laubgewimmel", "tokens": ["Hell", "durch", "Laub\u00b7ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Blinkt der Fr\u00fchlingshimmel,", "tokens": ["Blinkt", "der", "Fr\u00fch\u00b7lings\u00b7him\u00b7mel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und der Berge Schneegewand.", "tokens": ["Und", "der", "Ber\u00b7ge", "Schnee\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "Zauberisch erneuen", "tokens": ["Zau\u00b7be\u00b7risch", "er\u00b7neu\u00b7en"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sich die Phantaseyen", "tokens": ["Sich", "die", "Phan\u00b7ta\u00b7se\u00b7yen"], "token_info": ["word", "word", "word"], "pos": ["PRF", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Meiner Kindheit hier so licht!", "tokens": ["Mei\u00b7ner", "Kind\u00b7heit", "hier", "so", "licht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Rosenfarbig schweben", "tokens": ["Ro\u00b7sen\u00b7far\u00b7big", "schwe\u00b7ben"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Duftgebild', und weben", "tokens": ["Duft\u00b7ge\u00b7bild'", ",", "und", "we\u00b7ben"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "KON", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ein elysisch Traumgesicht.", "tokens": ["Ein", "e\u00b7ly\u00b7sisch", "Traum\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}