{"dta.poem.15724": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "57.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Freiheit ist im Kampf mit der Nothwendigkeit;", "tokens": ["Die", "Frei\u00b7heit", "ist", "im", "Kampf", "mit", "der", "Noth\u00b7wen\u00b7dig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geendet nicht, doch schon entschieden ist der Streit.", "tokens": ["Ge\u00b7en\u00b7det", "nicht", ",", "doch", "schon", "ent\u00b7schie\u00b7den", "ist", "der", "Streit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "ADV", "ADV", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn nie wird die Natur mehr st\u00e4rker als sie war,", "tokens": ["Denn", "nie", "wird", "die", "Na\u00b7tur", "mehr", "st\u00e4r\u00b7ker", "als", "sie", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "KOKOM", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch st\u00e4rker ward der Mensch und wird es immerdar.", "tokens": ["Doch", "st\u00e4r\u00b7ker", "ward", "der", "Mensch", "und", "wird", "es", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "KON", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Noch braucht wie sonst der Aar Klau, Schnabel, Fl\u00fcgelschlag,", "tokens": ["Noch", "braucht", "wie", "sonst", "der", "Aar", "Klau", ",", "Schna\u00b7bel", ",", "Fl\u00fc\u00b7gel\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ADV", "ART", "NN", "NE", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch Waffen tauscht der Mensch und wechselt, wie er mag.", "tokens": ["Doch", "Waf\u00b7fen", "tauscht", "der", "Mensch", "und", "wech\u00b7selt", ",", "wie", "er", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "KON", "VVFIN", "$,", "PWAV", "PPER", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}