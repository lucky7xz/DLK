{"textgrid.poem.36829": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf j\u00e4hen Felsen grauen alte Th\u00fcrme,", "tokens": ["Auf", "j\u00e4\u00b7hen", "Fel\u00b7sen", "grau\u00b7en", "al\u00b7te", "Th\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es g\u00e4hnt der Abgrund unter ihren F\u00fc\u00dfen,", "tokens": ["Es", "g\u00e4hnt", "der", "Ab\u00b7grund", "un\u00b7ter", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein halb Jahrtausend wehn um sie die St\u00fcrme.", "tokens": ["Ein", "halb", "Jahr\u00b7tau\u00b7send", "wehn", "um", "sie", "die", "St\u00fcr\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kaum schwingt der leichte Vogel sich zu ihnen,", "tokens": ["Kaum", "schwingt", "der", "leich\u00b7te", "Vo\u00b7gel", "sich", "zu", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch m\u00fchsam \u00fcber ungez\u00e4hlte Stufen", "tokens": ["Doch", "m\u00fch\u00b7sam", "\u00fc\u00b7ber", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gelangt der Mensch zu diesen Burgruinen.", "tokens": ["Ge\u00b7langt", "der", "Mensch", "zu", "die\u00b7sen", "Bur\u00b7gru\u00b7i\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sind's wohl aus r\u00f6m'scher Vorzeit Ueberreste,", "tokens": ["Sin\u00b7d's", "wohl", "aus", "r\u00f6m'\u00b7scher", "Vor\u00b7zeit", "Ue\u00b7ber\u00b7res\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Hat hier der Feind der Welt, die er beherrschte,", "tokens": ["Hat", "hier", "der", "Feind", "der", "Welt", ",", "die", "er", "be\u00b7herrschte", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Tiberius erbauet eine Veste?", "tokens": ["Ti\u00b7be\u00b7ri\u00b7us", "er\u00b7bau\u00b7et", "ei\u00b7ne", "Ves\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Ein andrer Kaiser ist's, der Held vom Norden,", "tokens": ["Ein", "an\u00b7drer", "Kai\u00b7ser", "ist's", ",", "der", "Held", "vom", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Hohenstauf' ist mit dem rothen Barte", "tokens": ["Der", "Ho\u00b7hen\u00b7stauf'", "ist", "mit", "dem", "ro\u00b7then", "Bar\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Insel Herr, des Schlosses Gr\u00fcnder worden.", "tokens": ["Der", "In\u00b7sel", "Herr", ",", "des", "Schlos\u00b7ses", "Gr\u00fcn\u00b7der", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und wo der Waiblinger in freiern Tagen", "tokens": ["Und", "wo", "der", "Waib\u00b7lin\u00b7ger", "in", "frei\u00b7ern", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Gethront, denkt oft ein Dichter dran, den Namen", "tokens": ["Ge\u00b7thront", ",", "denkt", "oft", "ein", "Dich\u00b7ter", "dran", ",", "den", "Na\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "$,", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Gro\u00dfen einst zu feiern, die ihn tragen.", "tokens": ["Der", "Gro\u00b7\u00dfen", "einst", "zu", "fei\u00b7ern", ",", "die", "ihn", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}