{"textgrid.poem.51385": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Danckg\u00e4biges Lobbedencken vor und von", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weg Pers, Weg Araber, Weg Griech, Weg R\u00f6mer weg,", "tokens": ["Weg", "Pers", ",", "Weg", "A\u00b7ra\u00b7ber", ",", "Weg", "Griech", ",", "Weg", "R\u00f6\u00b7mer", "weg", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "NN", "$,", "NN", "NE", "$,", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wahl, Iber, Britt! Ihr zielt und trefft doch nicht den Zweck:", "tokens": ["Wahl", ",", "I\u00b7ber", ",", "Britt", "!", "Ihr", "zielt", "und", "trefft", "doch", "nicht", "den", "Zweck", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "NN", "$.", "PPER", "VVFIN", "KON", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den ein Hochdeutscher Sch\u00fctz in Schlesien ohn Verhoffen", "tokens": ["Den", "ein", "Hoch\u00b7deut\u00b7scher", "Sch\u00fctz", "in", "Schle\u00b7si\u00b7en", "ohn", "Ver\u00b7hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "NN", "APPR", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Von Gott mit Licht und Recht erf\u00fcllet hat getroffen.", "tokens": ["Von", "Gott", "mit", "Licht", "und", "Recht", "er\u00b7f\u00fcl\u00b7let", "hat", "ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN", "VVPP", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Durch hoch erschwungnen Sinn, durch tieff durchbrochnen Geist", "tokens": ["Durch", "hoch", "er\u00b7schwung\u00b7nen", "Sinn", ",", "durch", "tieff", "durch\u00b7broch\u00b7nen", "Geist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In der Schlus Reimen Kunst, Kunst meisterlich erweist:", "tokens": ["In", "der", "Schlus", "Rei\u00b7men", "Kunst", ",", "Kunst", "meis\u00b7ter\u00b7lich", "er\u00b7weist", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "NN", "$,", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und, was der Haupt Punct ist, zur Seeligkeit gelencket,", "tokens": ["Und", ",", "was", "der", "Haupt", "Punct", "ist", ",", "zur", "See\u00b7lig\u00b7keit", "ge\u00b7len\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "ART", "NN", "NN", "VAFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Heist das den Deutschen nicht der Wei\u00dfheit Schatz geschencket?", "tokens": ["Heist", "das", "den", "Deut\u00b7schen", "nicht", "der", "Wei\u00df\u00b7heit", "Schatz", "ge\u00b7schen\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "PTKNEG", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mein CZEPKO glaube mir, Du wirst durch T.V.G.E.N.D Schein", "tokens": ["Mein", "CzEP\u00b7KO", "glau\u00b7be", "mir", ",", "Du", "wirst", "durch", "T.", "V.", "G.", "E.", "N.", "D", "Schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "abbreviation", "abbreviation", "abbreviation", "abbreviation", "abbreviation", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "APPR", "NE", "XY", "XY", "XY", "XY", "XY", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Weit \u00fcber OPITZ der dreymal Bekr\u00f6nte seyn:", "tokens": ["Weit", "\u00fc\u00b7ber", "O\u00b7pITZ", "der", "drey\u00b7mal", "Be\u00b7kr\u00f6n\u00b7te", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "ART", "ADV", "NN", "VAINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und ich wil dir den Prei\u00df der weisen Lehren geben,", "tokens": ["Und", "ich", "wil", "dir", "den", "Prei\u00df", "der", "wei\u00b7sen", "Leh\u00b7ren", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Da\u00df unser Deutschland sol nach deinen Reimen leben:", "tokens": ["Da\u00df", "un\u00b7ser", "Deutschland", "sol", "nach", "dei\u00b7nen", "Rei\u00b7men", "le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ja du mit ihm, so lang die Sonn am Himmel steht,", "tokens": ["Ja", "du", "mit", "ihm", ",", "so", "lang", "die", "Sonn", "am", "Him\u00b7mel", "steht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "APPR", "PPER", "$,", "ADV", "ADJD", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und auf ihr Wohnhaus zu der Reiger steigt und geht.", "tokens": ["Und", "auf", "ihr", "Wohn\u00b7haus", "zu", "der", "Rei\u00b7ger", "steigt", "und", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}