{"dta.poem.13254": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da sie der K\u00f6niginn das Kreuz von weiten zeigte,", "tokens": ["Da", "sie", "der", "K\u00f6\u00b7ni\u00b7ginn", "das", "Kreuz", "von", "wei\u00b7ten", "zeig\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Stuffen n\u00e4her kam und sich mit Demuth neigte;", "tokens": ["Den", "Stuf\u00b7fen", "n\u00e4\u00b7her", "kam", "und", "sich", "mit", "De\u00b7muth", "neig\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "KON", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Alsdann es ihrer Hand zum \u00fcbernehmen gab;", "tokens": ["Als\u00b7dann", "es", "ih\u00b7rer", "Hand", "zum", "\u00fc\u00b7ber\u00b7neh\u00b7men", "gab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "NN", "APPRART", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So sprach die K\u00f6niginn: \u201eSieh! dieses ist der Stab,", "tokens": ["So", "sprach", "die", "K\u00f6\u00b7ni\u00b7ginn", ":", "\u201e", "Sieh", "!", "die\u00b7ses", "ist", "der", "Stab", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "NE", "$.", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eder ists, durch welchen ich mich und mein Haus besch\u00fczte,", "tokens": ["\u201e", "der", "ists", ",", "durch", "wel\u00b7chen", "ich", "mich", "und", "mein", "Haus", "be\u00b7sch\u00fcz\u00b7te", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "$,", "APPR", "PWAT", "PPER", "PRF", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "300\u201dDie L\u00e4nder schirmete, den Grund des Thrones st\u00fczte;", "tokens": ["\"", "Die", "L\u00e4n\u00b7der", "schir\u00b7me\u00b7te", ",", "den", "Grund", "des", "Thro\u00b7nes", "st\u00fcz\u00b7te", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eder ists, durch dessen Kraft ich meinem Heer befahl.", "tokens": ["\u201e", "der", "ists", ",", "durch", "des\u00b7sen", "Kraft", "ich", "mei\u00b7nem", "Heer", "be\u00b7fahl", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "$,", "APPR", "PRELAT", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hier \u00fcberreichte sie das Kreuz dem Ehgemahl,", "tokens": ["Hier", "\u00fc\u00b7berr\u00b7eich\u00b7te", "sie", "das", "Kreuz", "dem", "Eh\u00b7ge\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der es mit Ehrfurcht nahm, und sich erreget zeigte,", "tokens": ["Der", "es", "mit", "Ehr\u00b7furcht", "nahm", ",", "und", "sich", "er\u00b7re\u00b7get", "zeig\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$,", "KON", "PRF", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Indem er sich darum mit zarten Mienen neigte.", "tokens": ["In\u00b7dem", "er", "sich", "da\u00b7rum", "mit", "zar\u00b7ten", "Mie\u00b7nen", "neig\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PAV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}