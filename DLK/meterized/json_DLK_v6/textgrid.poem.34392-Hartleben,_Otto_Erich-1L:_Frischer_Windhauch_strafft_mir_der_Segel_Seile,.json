{"textgrid.poem.34392": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Frischer Windhauch strafft mir der Segel Seile,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frischer Windhauch strafft mir der Segel Seile,", "tokens": ["Fri\u00b7scher", "Wind\u00b7hauch", "strafft", "mir", "der", "Se\u00b7gel", "Sei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "l\u00e4sst die Fluth aufspritzen in hellen K\u00e4mmen,", "tokens": ["l\u00e4sst", "die", "Fluth", "auf\u00b7sprit\u00b7zen", "in", "hel\u00b7len", "K\u00e4m\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVIZU", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "hei! der sturmschnell eilende leichte Nachen", "tokens": ["hei", "!", "der", "sturm\u00b7schnell", "ei\u00b7len\u00b7de", "leich\u00b7te", "Na\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKVZ", "$.", "ART", "ADJD", "ADJA", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "hebt sich und senkt sich.", "tokens": ["hebt", "sich", "und", "senkt", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "VVFIN", "PRF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "An der Stirne kleben die feuchten Locken,", "tokens": ["An", "der", "Stir\u00b7ne", "kle\u00b7ben", "die", "feuch\u00b7ten", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "und das Hirn durchbohren die Gluthgedanken.", "tokens": ["und", "das", "Hirn", "durch\u00b7boh\u00b7ren", "die", "Gluth\u00b7ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wirr und rastlos flattert das Haar der Furie,", "tokens": ["Wirr", "und", "rast\u00b7los", "flat\u00b7tert", "das", "Haar", "der", "Fu\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "z\u00fcngeln die Nattern! \u2013", "tokens": ["z\u00fcn\u00b7geln", "die", "Nat\u00b7tern", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Dass ein Gott w\u00e4r, dem ich mich beugen k\u00f6nnte!", "tokens": ["Dass", "ein", "Gott", "w\u00e4r", ",", "dem", "ich", "mich", "beu\u00b7gen", "k\u00f6nn\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "$,", "PRELS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dass ein Gott w\u00e4r, welcher mich strafen d\u00fcrfte!", "tokens": ["Dass", "ein", "Gott", "w\u00e4r", ",", "wel\u00b7cher", "mich", "stra\u00b7fen", "d\u00fcrf\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Jauchzend wollt ich, s\u00fchneberauscht und b\u00fcssend,", "tokens": ["Jauch\u00b7zend", "wollt", "ich", ",", "s\u00fch\u00b7ne\u00b7be\u00b7rauscht", "und", "b\u00fcs\u00b7send", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "tauchen ins Weltmeer!", "tokens": ["tau\u00b7chen", "ins", "Welt\u00b7meer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}