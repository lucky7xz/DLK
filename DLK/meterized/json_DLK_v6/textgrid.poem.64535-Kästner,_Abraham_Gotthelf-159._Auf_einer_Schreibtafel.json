{"textgrid.poem.64535": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "159. Auf einer Schreibtafel", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sorgt ja, da\u00df auch von euren Z\u00fcgen", "tokens": ["Sorgt", "ja", ",", "da\u00df", "auch", "von", "eu\u00b7ren", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein gutes Bild der Nachwelt \u00fcbrig ist:", "tokens": ["Ein", "gu\u00b7tes", "Bild", "der", "Nach\u00b7welt", "\u00fcb\u00b7rig", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So steht sie euch, Autoren, mit Vergn\u00fcgen,", "tokens": ["So", "steht", "sie", "euch", ",", "Au\u00b7to\u00b7ren", ",", "mit", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn sie euch lange nicht mehr liest.", "tokens": ["Wenn", "sie", "euch", "lan\u00b7ge", "nicht", "mehr", "liest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}