{"textgrid.poem.63971": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Brautmorgen", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Erwachens Knospe schwillt,", "tokens": ["Des", "Er\u00b7wa\u00b7chens", "Knos\u00b7pe", "schwillt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hochrosig t\u00f6nt sich der regere Schlummer.", "tokens": ["Hoch\u00b7ro\u00b7sig", "t\u00f6nt", "sich", "der", "re\u00b7ge\u00b7re", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Z\u00f6gernd, selig bang,", "tokens": ["Z\u00f6\u00b7gernd", ",", "se\u00b7lig", "bang", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Lange, lange.", "tokens": ["Lan\u00b7ge", ",", "lan\u00b7ge", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADV", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Weit offen die lauschende Seele.", "tokens": ["Weit", "of\u00b7fen", "die", "lau\u00b7schen\u00b7de", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "War es, war es nicht?", "tokens": ["War", "es", ",", "war", "es", "nicht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Das schreckende M\u00e4rchen,", "tokens": ["Das", "schre\u00b7cken\u00b7de", "M\u00e4r\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "So hold und so wild!", "tokens": ["So", "hold", "und", "so", "wild", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Ein leiser Blick stiehlt sich um.", "tokens": ["Ein", "lei\u00b7ser", "Blick", "stiehlt", "sich", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Ja, es ist da", "tokens": ["Ja", ",", "es", "ist", "da"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Und sieht doch gar nicht gef\u00e4hrlich aus \u2013", "tokens": ["Und", "sieht", "doch", "gar", "nicht", "ge\u00b7f\u00e4hr\u00b7lich", "aus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PTKNEG", "ADJD", "PTKVZ", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Und wie ruhig es atmen kann!", "tokens": ["Und", "wie", "ru\u00b7hig", "es", "at\u00b7men", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Als sei nichts,", "tokens": ["Als", "sei", "nichts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PIS", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.14": {"text": "Aber auch gar nichts passiert.", "tokens": ["A\u00b7ber", "auch", "gar", "nichts", "pas\u00b7siert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "War das da denn so furchtbar,", "tokens": ["War", "das", "da", "denn", "so", "furcht\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "So unversch\u00e4mt \u2013 und scheu\u00dflich,", "tokens": ["So", "un\u00b7ver\u00b7sch\u00e4mt", "\u2013", "und", "scheu\u00df\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "So zu sich zwingend \u2013", "tokens": ["So", "zu", "sich", "zwin\u00b7gend", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "ADJD", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Und kehrte sich an nichts.", "tokens": ["Und", "kehr\u00b7te", "sich", "an", "nichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "M\u00f6glich, da\u00df nur's Dunkel so drauf wirkt.", "tokens": ["M\u00f6g\u00b7lich", ",", "da\u00df", "nur's", "Dun\u00b7kel", "so", "drauf", "wirkt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "ADV", "ADJD", "ADV", "PAV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.20": {"text": "Dieses gute schlummernde Kind,", "tokens": ["Die\u00b7ses", "gu\u00b7te", "schlum\u00b7mern\u00b7de", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.21": {"text": "Dieser schlummernde Friede", "tokens": ["Die\u00b7ser", "schlum\u00b7mern\u00b7de", "Frie\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.22": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.23": {"text": "Und wieder sieht sie starr und steif nach oben,", "tokens": ["Und", "wie\u00b7der", "sieht", "sie", "starr", "und", "steif", "nach", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Wie die Toten ihre Heimat sehen.", "tokens": ["Wie", "die", "To\u00b7ten", "ih\u00b7re", "Hei\u00b7mat", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.26": {"text": "Nun wird es sich regen das Kind,", "tokens": ["Nun", "wird", "es", "sich", "re\u00b7gen", "das", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Das Kind mit dem seidenen Schnurrbart.", "tokens": ["Das", "Kind", "mit", "dem", "sei\u00b7de\u00b7nen", "Schnurr\u00b7bart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Etwas m\u00fcde, selige Sterne", "tokens": ["Et\u00b7was", "m\u00fc\u00b7de", ",", "se\u00b7li\u00b7ge", "Ster\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.29": {"text": "Sind still noch im verwunderten Gl\u00fcck.", "tokens": ["Sind", "still", "noch", "im", "ver\u00b7wun\u00b7der\u00b7ten", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.30": {"text": "Ja, das, das ist die Liebe,", "tokens": ["Ja", ",", "das", ",", "das", "ist", "die", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Die lebensinnige, seelenvolle Liebe,", "tokens": ["Die", "le\u00b7ben\u00b7sin\u00b7ni\u00b7ge", ",", "see\u00b7len\u00b7vol\u00b7le", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "So still, so traulich still,", "tokens": ["So", "still", ",", "so", "trau\u00b7lich", "still", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "So mit der vollen Seele angesprengt!", "tokens": ["So", "mit", "der", "vol\u00b7len", "See\u00b7le", "an\u00b7ge\u00b7sprengt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Ja, das andere \u2013 fr\u00fcher \u2013", "tokens": ["Ja", ",", "das", "an\u00b7de\u00b7re", "\u2013", "fr\u00fc\u00b7her", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "PIS", "$(", "ADJD", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.35": {"text": "Wie f\u00fcr die Knaben \u2013", "tokens": ["Wie", "f\u00fcr", "die", "Kna\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.36": {"text": "Wie mochte man nur?", "tokens": ["Wie", "moch\u00b7te", "man", "nur", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.37": {"text": "Nun kann man haben", "tokens": ["Nun", "kann", "man", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "VAFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.38": {"text": "Die liebe lange Nacht", "tokens": ["Die", "lie\u00b7be", "lan\u00b7ge", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "In inniger Macht", "tokens": ["In", "in\u00b7ni\u00b7ger", "Macht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.40": {"text": "Bezaubernde Gaben,", "tokens": ["Be\u00b7zau\u00b7bern\u00b7de", "Ga\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.41": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.42": {"text": "Die sich nur bieten dem Mann,", "tokens": ["Die", "sich", "nur", "bie\u00b7ten", "dem", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.43": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.44": {"text": "Und nach des Dunkels", "tokens": ["Und", "nach", "des", "Dun\u00b7kels"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.45": {"text": "St\u00fcrmender Wildheit \u2013", "tokens": ["St\u00fcr\u00b7men\u00b7der", "Wild\u00b7heit", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.46": {"text": "Leisheit scheu und zart,", "tokens": ["Leis\u00b7heit", "scheu", "und", "zart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.47": {"text": "Unter der ein Schelm liegt verwahrt.", "tokens": ["Un\u00b7ter", "der", "ein", "Schelm", "lie