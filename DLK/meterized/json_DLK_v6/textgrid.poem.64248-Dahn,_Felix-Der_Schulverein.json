{"textgrid.poem.64248": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der Schulverein", "genre": "verse", "period": "N.A.", "pub_year": 1885, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbder Schulverein, der Schulverein!\u00ab", "tokens": ["\u00bb", "der", "Schul\u00b7ver\u00b7ein", ",", "der", "Schul\u00b7ver\u00b7ein", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "$,", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was Wort ist fein gefunden:", "tokens": ["Was", "Wort", "ist", "fein", "ge\u00b7fun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir alle wollen Sch\u00fcler sein,", "tokens": ["Wir", "al\u00b7le", "wol\u00b7len", "Sch\u00fc\u00b7ler", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VMFIN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu ", "tokens": ["Zu"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Die Schule hei\u00dft: das Vaterland,", "tokens": ["Die", "Schu\u00b7le", "hei\u00dft", ":", "das", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das gro\u00dfe, teure, ", "tokens": ["Das", "gro\u00b7\u00dfe", ",", "teu\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Das Vaterland vom Bernsteinstrand", "tokens": ["Das", "Va\u00b7ter\u00b7land", "vom", "Bern\u00b7stein\u00b7strand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis zu der Gletscher Glanze,", "tokens": ["Bis", "zu", "der", "Glet\u00b7scher", "Glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und von der Adria Gebraus", "tokens": ["Und", "von", "der", "Ad\u00b7ria", "Ge\u00b7braus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NE", "NN"], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Bis an des Nordlands Thule:", "tokens": ["Bis", "an", "des", "Nord\u00b7lands", "Thu\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir lernen nun und nimmer aus", "tokens": ["Wir", "ler\u00b7nen", "nun", "und", "nim\u00b7mer", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KON", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In dieser gro\u00dfen Schule!", "tokens": ["In", "die\u00b7ser", "gro\u00b7\u00dfen", "Schu\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie Gold- und Erzklang nah und weit", "tokens": ["Wie", "Gold", "und", "Erz\u00b7klang", "nah", "und", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "TRUNC", "KON", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erklingt seit grauen Tagen", "tokens": ["Er\u00b7klingt", "seit", "grau\u00b7en", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der deutschen Sprache Herrlichkeit", "tokens": ["Der", "deut\u00b7schen", "Spra\u00b7che", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ist nicht auszusagen!", "tokens": ["Und", "ist", "nicht", "aus\u00b7zu\u00b7sa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das deutsche Volkstum ist ein Hort", "tokens": ["Das", "deut\u00b7sche", "Volks\u00b7tum", "ist", "ein", "Hort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Gold und Erz und Eisen;", "tokens": ["Von", "Gold", "und", "Erz", "und", "Ei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er wird sich: \u2013 sch\u00f6pft nur fort und fort! \u2013", "tokens": ["Er", "wird", "sich", ":", "\u2013", "sch\u00f6pft", "nur", "fort", "und", "fort", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PRF", "$.", "$(", "VVFIN", "ADV", "PTKVZ", "KON", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als unaussch\u00f6pfbar weisen.", "tokens": ["Als", "un\u00b7aus\u00b7sch\u00f6pf\u00b7bar", "wei\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wir alle wollen Sch\u00fcler sein,", "tokens": ["Wir", "al\u00b7le", "wol\u00b7len", "Sch\u00fc\u00b7ler", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VMFIN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Jungen und die Alten,", "tokens": ["Die", "Jun\u00b7gen", "und", "die", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch was wir lernten, gro\u00df und klein,", "tokens": ["Doch", "was", "wir", "lern\u00b7ten", ",", "gro\u00df", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht stumm f\u00fcr uns behalten:", "tokens": ["Nicht", "stumm", "f\u00fcr", "uns", "be\u00b7hal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nein! Was wir lernten, ", "tokens": ["Nein", "!", "Was", "wir", "lern\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wir sch\u00f6pfen, um zu ", "tokens": ["Wir", "sch\u00f6p\u00b7fen", ",", "um", "zu"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "PTKZU"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die heil'ge Aussaat mehren wir", "tokens": ["Die", "heil'\u00b7ge", "Aus\u00b7saat", "meh\u00b7ren", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit nimmerm\u00fcden H\u00e4nden!", "tokens": ["Mit", "nim\u00b7mer\u00b7m\u00fc\u00b7den", "H\u00e4n\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und wer verg\u00e4\u00dfe solcher Pflicht,", "tokens": ["Und", "wer", "ver\u00b7g\u00e4\u00b7\u00dfe", "sol\u00b7cher", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Der Deutsche, der kein Deutscher nicht \u2013 \u2013,", "tokens": ["Der", "Deut\u00b7sche", ",", "der", "kein", "Deut\u00b7scher", "nicht", "\u2013", "\u2013", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "PTKNEG", "$(", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Name sei die Schande!", "tokens": ["Des", "Na\u00b7me", "sei", "die", "Schan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "An unsrer Treue bricht die Flut", "tokens": ["An", "uns\u00b7rer", "Treu\u00b7e", "bricht", "die", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rings feindlicher Umtosung:", "tokens": ["Rings", "feind\u00b7li\u00b7cher", "Um\u00b7to\u00b7sung", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbsein Volk des Mannes h\u00f6chstes Gut!\u00ab", "tokens": ["\u00bb", "sein", "Volk", "des", "Man\u00b7nes", "h\u00f6chs\u00b7tes", "Gut", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist unsre stolze Losung! \u2013 \u2013", "tokens": ["Ist", "uns\u00b7re", "stol\u00b7ze", "Lo\u00b7sung", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}