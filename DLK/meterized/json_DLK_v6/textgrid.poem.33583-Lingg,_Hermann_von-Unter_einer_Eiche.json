{"textgrid.poem.33583": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Unter einer Eiche", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eiche, deine dunkeln Zweige ragen", "tokens": ["Ei\u00b7che", ",", "dei\u00b7ne", "dun\u00b7keln", "Zwei\u00b7ge", "ra\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stolz empor aus l\u00e4ngst vergangnen Tagen,", "tokens": ["Stolz", "em\u00b7por", "aus", "l\u00e4ngst", "ver\u00b7gang\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Geister wandeln durch dein \u00e4stig Haus;", "tokens": ["Geis\u00b7ter", "wan\u00b7deln", "durch", "dein", "\u00e4s\u00b7tig", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sieben Menschenalter sahst du schreiten,", "tokens": ["Sie\u00b7ben", "Men\u00b7schen\u00b7al\u00b7ter", "sahst", "du", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und wie Harfen aus den alten Zeiten", "tokens": ["Und", "wie", "Har\u00b7fen", "aus", "den", "al\u00b7ten", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Rauscht es durch dein Laub im Sturmgebraus.", "tokens": ["Rauscht", "es", "durch", "dein", "Laub", "im", "Sturm\u00b7ge\u00b7braus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "O wie oft in deiner Schattenk\u00fchle", "tokens": ["O", "wie", "oft", "in", "dei\u00b7ner", "Schat\u00b7ten\u00b7k\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Haben M\u00e4hder bei des Sommers Schw\u00fcle", "tokens": ["Ha\u00b7ben", "M\u00e4h\u00b7der", "bei", "des", "Som\u00b7mers", "Schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ausgerastet von des Tages M\u00fchn;", "tokens": ["Aus\u00b7ge\u00b7ras\u00b7tet", "von", "des", "Ta\u00b7ges", "M\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Deine friedlichen Gezweige kr\u00e4nzten", "tokens": ["Dei\u00b7ne", "fried\u00b7li\u00b7chen", "Ge\u00b7zwei\u00b7ge", "kr\u00e4nz\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Keine Siegeshelme, hier ergl\u00e4nzten", "tokens": ["Kei\u00b7ne", "Sie\u00b7ge\u00b7shel\u00b7me", ",", "hier", "er\u00b7gl\u00e4nz\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Hirtenfeuer nur und Alpengl\u00fchn.", "tokens": ["Hir\u00b7ten\u00b7feu\u00b7er", "nur", "und", "Al\u00b7pen\u00b7gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Hirsche nur und junge Rehe sprangen", "tokens": ["Hir\u00b7sche", "nur", "und", "jun\u00b7ge", "Re\u00b7he", "spran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "KON", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aus dem Wald herauf, und Lerchen sangen", "tokens": ["Aus", "dem", "Wald", "her\u00b7auf", ",", "und", "Ler\u00b7chen", "san\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "KON", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Unter deinen Blumen auf der Flur.", "tokens": ["Un\u00b7ter", "dei\u00b7nen", "Blu\u00b7men", "auf", "der", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "W\u00e4hrend ringsum Kriegsgesch\u00fctze dr\u00f6hnten,", "tokens": ["W\u00e4h\u00b7rend", "ring\u00b7sum", "Kriegs\u00b7ge\u00b7sch\u00fct\u00b7ze", "dr\u00f6hn\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Feindesbanner flatterten, ert\u00f6nten", "tokens": ["Fein\u00b7des\u00b7ban\u00b7ner", "flat\u00b7ter\u00b7ten", ",", "er\u00b7t\u00f6n\u00b7ten"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Hier des Sonntags fromme Glocken nur.", "tokens": ["Hier", "des", "Sonn\u00b7tags", "from\u00b7me", "Glo\u00b7cken", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Aus der Wunde deiner harten Adern", "tokens": ["Aus", "der", "Wun\u00b7de", "dei\u00b7ner", "har\u00b7ten", "A\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Quillt ein Honig, summenden Geschwadern", "tokens": ["Quillt", "ein", "Ho\u00b7nig", ",", "sum\u00b7men\u00b7den", "Ge\u00b7schwa\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wilder Bienen dient dein Holz zum Bau:", "tokens": ["Wil\u00b7der", "Bie\u00b7nen", "dient", "dein", "Holz", "zum", "Bau", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "So quillt Sanftmut aus der tiefen Wunde,", "tokens": ["So", "quillt", "Sanft\u00b7mut", "aus", "der", "tie\u00b7fen", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Die vernarbt in unsres Herzens Grunde,", "tokens": ["Die", "ver\u00b7narbt", "in", "uns\u00b7res", "Her\u00b7zens", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Aus dem Schmerz des Liedes milder Tau.", "tokens": ["Aus", "dem", "Schmerz", "des", "Lie\u00b7des", "mil\u00b7der", "Tau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Sturm und Blitz verschonten dich, o Eiche,", "tokens": ["Sturm", "und", "Blitz", "ver\u00b7schon\u00b7ten", "dich", ",", "o", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Vor des Beils verh\u00e4ngnisvollem Streiche,", "tokens": ["Vor", "des", "Beils", "ver\u00b7h\u00e4ng\u00b7nis\u00b7vol\u00b7lem", "Strei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schirmend soll mein Segen dich umwehn.", "tokens": ["Schir\u00b7mend", "soll", "mein", "Se\u00b7gen", "dich", "um\u00b7wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Lebe wohl, und seh' ich einst dich wieder,", "tokens": ["Le\u00b7be", "wohl", ",", "und", "seh'", "ich", "einst", "dich", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "KON", "VVFIN", "PPER", "ADV", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "La\u00df aufs neue dann durch meine Lieder", "tokens": ["La\u00df", "aufs", "neu\u00b7e", "dann", "durch", "mei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPRART", "ADJA", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Deiner Wipfel dunkles Rauschen gehn!", "tokens": ["Dei\u00b7ner", "Wip\u00b7fel", "dunk\u00b7les", "Rau\u00b7schen", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}