{"textgrid.poem.32584": {"metadata": {"author": {"name": "Wieland, Christoph Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: H\u00fcpfend, wie das Blut in deinen Adern, scherzet,", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00fcpfend, wie das Blut in deinen Adern, scherzet,", "tokens": ["H\u00fcp\u00b7fend", ",", "wie", "das", "Blut", "in", "dei\u00b7nen", "A\u00b7dern", ",", "scher\u00b7zet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Chloe, deine Seel ihr Dasein hin;", "tokens": ["Chloe", ",", "dei\u00b7ne", "Seel", "ihr", "Da\u00b7sein", "hin", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Keine Ahndung ferner \u00dcbel schw\u00e4rzet", "tokens": ["Kei\u00b7ne", "Ahn\u00b7dung", "fer\u00b7ner", "\u00dc\u00b7bel", "schw\u00e4r\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADV", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deinen freien unbew\u00f6lkten Sinn;", "tokens": ["Dei\u00b7nen", "frei\u00b7en", "un\u00b7be\u00b7w\u00f6lk\u00b7ten", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Alles, deucht dir, ist wie deine Wangen", "tokens": ["Al\u00b7les", ",", "deucht", "dir", ",", "ist", "wie", "dei\u00b7ne", "Wan\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "$,", "VVFIN", "PPER", "$,", "VAFIN", "KOKOM", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Rosenrot; gleich Liebesg\u00f6ttern hangen", "tokens": ["Ro\u00b7sen\u00b7rot", ";", "gleich", "Lie\u00b7bes\u00b7g\u00f6t\u00b7tern", "han\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Tausend Hoffnungen, von br\u00fctender Begier", "tokens": ["Tau\u00b7send", "Hoff\u00b7nun\u00b7gen", ",", "von", "br\u00fc\u00b7ten\u00b7der", "Be\u00b7gier"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.8": {"text": "Sanft entfaltet, gaukelnd \u00fcber dir.", "tokens": ["Sanft", "ent\u00b7fal\u00b7tet", ",", "gau\u00b7kelnd", "\u00fc\u00b7ber", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVPP", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Jeder Wunsch, der mit Vergn\u00fcgen schmeichelt,", "tokens": ["Je\u00b7der", "Wunsch", ",", "der", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "schmei\u00b7chelt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Scheint dir schuldlos: du erfuhrst noch nicht", "tokens": ["Scheint", "dir", "schuld\u00b7los", ":", "du", "er\u00b7fuhrst", "noch", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "$.", "PPER", "VVFIN", "ADV", "PTKNEG"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Da\u00df der Schmerz sich oft zu Wollust heuchelt,", "tokens": ["Da\u00df", "der", "Schmerz", "sich", "oft", "zu", "Wol\u00b7lust", "heu\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Und die Hoffnung stets zu viel verspricht.", "tokens": ["Und", "die", "Hoff\u00b7nung", "stets", "zu", "viel", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "PTKA", "PIS", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ach! warum, o Chloe, sind's nur Tr\u00e4ume,", "tokens": ["Ach", "!", "wa\u00b7rum", ",", "o", "Chloe", ",", "sin\u00b7d's", "nur", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "$,", "FM", "NE", "$,", "VAFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn die Phantasie, mit eitler Sch\u00f6pfungskraft,", "tokens": ["Wenn", "die", "Phan\u00b7ta\u00b7sie", ",", "mit", "eit\u00b7ler", "Sch\u00f6p\u00b7fungs\u00b7kraft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Goldne Welten um uns her erschafft?", "tokens": ["Gold\u00b7ne", "Wel\u00b7ten", "um", "uns", "her", "er\u00b7schafft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PRF", "APZR", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Lauter Lust, wohin das Auge gafft,", "tokens": ["Lau\u00b7ter", "Lust", ",", "wo\u00b7hin", "das", "Au\u00b7ge", "gafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Lauter Rosen, lauter Myrtenb\u00e4ume;", "tokens": ["Lau\u00b7ter", "Ro\u00b7sen", ",", "lau\u00b7ter", "Myr\u00b7ten\u00b7b\u00e4u\u00b7me", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "G\u00f6ttertisch von Grazien gedeckt,", "tokens": ["G\u00f6t\u00b7ter\u00b7tisch", "von", "Gra\u00b7zi\u00b7en", "ge\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Nektar aus Tokay in allen Fl\u00fcssen,", "tokens": ["Nek\u00b7tar", "aus", "To\u00b7kay", "in", "al\u00b7len", "Fl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "APPR", "PIAT", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Schlaf auf Schwanen, den zu stillen K\u00fcssen", "tokens": ["Schlaf", "auf", "Schwa\u00b7nen", ",", "den", "zu", "stil\u00b7len", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Amor oft, die Sorge niemals, weckt;", "tokens": ["A\u00b7mor", "oft", ",", "die", "Sor\u00b7ge", "nie\u00b7mals", ",", "weckt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADV", "$,", "ART", "NN", "ADV", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Lauter Feste, T\u00e4nze, frohe Spiele,", "tokens": ["Lau\u00b7ter", "Fes\u00b7te", ",", "T\u00e4n\u00b7ze", ",", "fro\u00b7he", "Spie\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Lauter Unschuld, Eintracht, Z\u00e4rtlichkeit,", "tokens": ["Lau\u00b7ter", "Un\u00b7schuld", ",", "Ein\u00b7tracht", ",", "Z\u00e4rt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Kurz, der Menschen ganze Lebenszeit", "tokens": ["Kurz", ",", "der", "Men\u00b7schen", "gan\u00b7ze", "Le\u00b7bens\u00b7zeit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Ein Gewebe lieblicher Gef\u00fchle \u2013", "tokens": ["Ein", "Ge\u00b7we\u00b7be", "lieb\u00b7li\u00b7cher", "Ge\u00b7f\u00fch\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Welch ein Traum! \u2013", "tokens": ["Welch", "ein", "Traum", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PIAT", "ART", "NN", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "\u00bbwarum (so ruft, entz\u00fcckt", "tokens": ["\u00bb", "wa\u00b7rum", "(", "so", "ruft", ",", "ent\u00b7z\u00fcckt"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["$(", "PWAV", "$(", "ADV", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Von Nanett im kurzen Unterrocke,", "tokens": ["Von", "Na\u00b7nett", "im", "kur\u00b7zen", "Un\u00b7ter\u00b7ro\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Tristram aus, indem des M\u00e4dchens schwarze Locke", "tokens": ["Trist\u00b7ram", "aus", ",", "in\u00b7dem", "des", "M\u00e4d\u00b7chens", "schwar\u00b7ze", "Lo\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Sich im ungelernten Tanz entstrickt,", "tokens": ["Sich", "im", "un\u00b7ge\u00b7lern\u00b7ten", "Tanz", "ent\u00b7strickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und ihr l\u00e4chelnd Aug unwissend Liebe blickt)", "tokens": ["Und", "ihr", "l\u00e4\u00b7chelnd", "Aug", "un\u00b7wis\u00b7send", "Lie\u00b7be", "blickt", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "NN", "ADJD", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Ach! warum, du, dessen Wohlbehagen", "tokens": ["Ach", "!", "wa\u00b7rum", ",", "du", ",", "des\u00b7sen", "Wohl\u00b7be\u00b7ha\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "$,", "PPER", "$,", "PRELAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Unsre Freuden schafft und unsre Plagen,", "tokens": ["Uns\u00b7re", "Freu\u00b7den", "schafft", "und", "uns\u00b7re", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Kann nicht hier ein Mann sich in der Freude Scho\u00df", "tokens": ["Kann", "nicht", "hier", "ein", "Mann", "sich", "in", "der", "Freu\u00b7de", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "ADV", "ART", "NN", "PRF", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.9": {"text": "Niederlegen, tanzen, singen, und sein Pater sagen,", "tokens": ["Nie\u00b7der\u00b7le\u00b7gen", ",", "tan\u00b7zen", ",", "sin\u00b7gen", ",", "und", "sein", "Pa\u00b7ter", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "Und gen Himmel mit Nanetten gehn?\u00ab", "tokens": ["Und", "gen", "Him\u00b7mel", "mit", "Na\u00b7net\u00b7ten", "gehn", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Eitler Wunsch! vielleicht verzeihlich im Entstehn,", "tokens": ["Eit\u00b7ler", "Wunsch", "!", "viel\u00b7leicht", "ver\u00b7zeih\u00b7lich", "im", "Ent\u00b7stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Aber dem Gesetz der ernsten Weisheit \u2013 S\u00fcnde!", "tokens": ["A\u00b7ber", "dem", "Ge\u00b7setz", "der", "erns\u00b7ten", "Weis\u00b7heit", "\u2013", "S\u00fcn\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "$(", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Ein Verh\u00e4ngnis, dessen dunkle Gr\u00fcnde", "tokens": ["Ein", "Ver\u00b7h\u00e4ng\u00b7nis", ",", "des\u00b7sen", "dunk\u00b7le", "Gr\u00fcn\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wir vielleicht in bessern Welten sehn,", "tokens": ["Wir", "viel\u00b7leicht", "in", "bes\u00b7sern", "Wel\u00b7ten", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Findt f\u00fcr ", "tokens": ["Findt", "f\u00fcr"], "token_info": ["word", "word"], "pos": ["NE", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Mischt in jeden Tropfen Lust geschwinde", "tokens": ["Mischt", "in", "je\u00b7den", "Trop\u00b7fen", "Lust", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "NN", "NN", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Zwei von Bitterkeit, gef\u00e4llt sich, (wie es scheint)", "tokens": ["Zwei", "von", "Bit\u00b7ter\u00b7keit", ",", "ge\u00b7f\u00e4llt", "sich", ",", "(", "wie", "es", "scheint", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "APPR", "NN", "$,", "VVFIN", "PRF", "$,", "$(", "PWAV", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Jede Hoffnung ", "tokens": ["Je\u00b7de", "Hoff\u00b7nung"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Wenn zu unsern W\u00fcnschen alles sich vereint,", "tokens": ["Wenn", "zu", "un\u00b7sern", "W\u00fcn\u00b7schen", "al\u00b7les", "sich", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "PIS", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "Pl\u00f6tzlich zu verwehn, erfindet jedem Morgen,", "tokens": ["Pl\u00f6tz\u00b7lich", "zu", "ver\u00b7wehn", ",", "er\u00b7fin\u00b7det", "je\u00b7dem", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.11": {"text": "Der uns Lust verhie\u00df, unvorgesehne Sorgen,", "tokens": ["Der", "uns", "Lust", "ver\u00b7hie\u00df", ",", "un\u00b7vor\u00b7ge\u00b7seh\u00b7ne", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.12": {"text": "Gibt die Unschuld oft der Bosheit, dem Betrug", "tokens": ["Gibt", "die", "Un\u00b7schuld", "oft", "der", "Bos\u00b7heit", ",", "dem", "Be\u00b7trug"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.13": {"text": "Preis, und lohnt die Treu mit einem Aschenkrug.", "tokens": ["Preis", ",", "und", "lohnt", "die", "Treu", "mit", "ei\u00b7nem", "A\u00b7schen\u00b7krug", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Chloe, hoffe nicht, da\u00df innerhalb dem Kreise,", "tokens": ["Chloe", ",", "hof\u00b7fe", "nicht", ",", "da\u00df", "in\u00b7ner\u00b7halb", "dem", "Krei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PTKNEG", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der den Erdball von dem Sternenfeld", "tokens": ["Der", "den", "Erd\u00b7ball", "von", "dem", "Ster\u00b7nen\u00b7feld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Trennt, die ", "tokens": ["Trennt", ",", "die"], "token_info": ["word", "punct", "word"], "pos": ["VVFIN", "$,", "PRELS"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Ach! sie sinkt nicht bis zur Unterwelt!", "tokens": ["Ach", "!", "sie", "sinkt", "nicht", "bis", "zur", "Un\u00b7ter\u00b7welt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "PTKNEG", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Alle diese sch\u00f6nen Luftgesichte,", "tokens": ["Al\u00b7le", "die\u00b7se", "sch\u00f6\u00b7nen", "Luft\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Deren Name deine junge Brust", "tokens": ["De\u00b7ren", "Na\u00b7me", "dei\u00b7ne", "jun\u00b7ge", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "\u00dcberwallend macht, sind blo\u00dfe Schaugerichte,", "tokens": ["\u00dc\u00b7berw\u00b7al\u00b7lend", "macht", ",", "sind", "blo\u00b7\u00dfe", "Schau\u00b7ge\u00b7rich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Leichte Tr\u00e4um unwesentlicher Lust!", "tokens": ["Leich\u00b7te", "Tr\u00e4um", "un\u00b7we\u00b7sent\u00b7li\u00b7cher", "Lust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Nur von fern aus offnem Himmel sehn;", "tokens": ["Nur", "von", "fern", "aus", "off\u00b7nem", "Him\u00b7mel", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Diesseits her versetzt, sind eure Fr\u00fcchte \u2013 Bl\u00e4tter,", "tokens": ["Dies\u00b7seits", "her", "ver\u00b7setzt", ",", "sind", "eu\u00b7re", "Fr\u00fcch\u00b7te", "\u2013", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APZR", "VVPP", "$,", "VAFIN", "PPOSAT", "NN", "$(", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Die mit leerem Schmuck das Auge hintergehn!", "tokens": ["Die", "mit", "lee\u00b7rem", "Schmuck", "das", "Au\u00b7ge", "hin\u00b7ter\u00b7gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}