{"textgrid.poem.47222": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Espe", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als den Herrn ans Kreuz geschlagen", "tokens": ["Als", "den", "Herrn", "ans", "Kreuz", "ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun des Feldes B\u00e4ume sahn,", "tokens": ["Nun", "des", "Fel\u00b7des", "B\u00e4u\u00b7me", "sahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kam ein Zittern und ein Zagen", "tokens": ["Kam", "ein", "Zit\u00b7tern", "und", "ein", "Za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Allen fernen, allen nah'n.", "tokens": ["Al\u00b7len", "fer\u00b7nen", ",", "al\u00b7len", "nah'", "n."], "token_info": ["word", "word", "punct", "word", "word", "abbreviation"], "pos": ["PIS", "VVINF", "$,", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur der Espe Krone", "tokens": ["Nur", "der", "Es\u00b7pe", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Lie\u00df die Bl\u00e4tter ohne", "tokens": ["Lie\u00df", "die", "Bl\u00e4t\u00b7ter", "oh\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Beben in die L\u00fcfte ragen,", "tokens": ["Be\u00b7ben", "in", "die", "L\u00fcf\u00b7te", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gleich als ging sie das nicht an.", "tokens": ["Gleich", "als", "ging", "sie", "das", "nicht", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VVFIN", "PPER", "PDS", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Damals ward der Fluch gesprochen,", "tokens": ["Da\u00b7mals", "ward", "der", "Fluch", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ihn h\u00f6rte Berg und Kluft:", "tokens": ["Und", "ihn", "h\u00f6r\u00b7te", "Berg", "und", "Kluft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbda\u00df dir sei dein Stolz gebrochen,", "tokens": ["\u00bb", "da\u00df", "dir", "sei", "dein", "Stolz", "ge\u00b7bro\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zittre k\u00fcnftig jeder Luft!", "tokens": ["Zitt\u00b7re", "k\u00fcnf\u00b7tig", "je\u00b7der", "Luft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Andre B\u00e4ume zittern", "tokens": ["And\u00b7re", "B\u00e4u\u00b7me", "zit\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Nur in Ungewittern,", "tokens": ["Nur", "in", "Un\u00b7ge\u00b7wit\u00b7tern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Zitternd soll das Herz dir pochen,", "tokens": ["Zit\u00b7ternd", "soll", "das", "Herz", "dir", "po\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn im Wald ein Vogel ruft.", "tokens": ["Wenn", "im", "Wald", "ein", "Vo\u00b7gel", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zittre, wo im Erdenkreise", "tokens": ["Zitt\u00b7re", ",", "wo", "im", "Er\u00b7den\u00b7krei\u00b7se"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fcnftig du entkeimst dem Staub!", "tokens": ["K\u00fcnf\u00b7tig", "du", "ent\u00b7keimst", "dem", "Staub", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jedes Blatt soll zittern leise,", "tokens": ["Je\u00b7des", "Blatt", "soll", "zit\u00b7tern", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis es wird des Herbstwinds Raub.", "tokens": ["Bis", "es", "wird", "des", "Herbst\u00b7winds", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und in allen Tagen", "tokens": ["Und", "in", "al\u00b7len", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Soll man h\u00f6ren sagen", "tokens": ["Soll", "man", "h\u00f6\u00b7ren", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "VVINF", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Dir zur Strafe sprichwortweise:", "tokens": ["Dir", "zur", "Stra\u00b7fe", "sprich\u00b7wort\u00b7wei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zittern wie ein Espenlaub!\u00ab", "tokens": ["Zit\u00b7tern", "wie", "ein", "E\u00b7spen\u00b7laub", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}