{"textgrid.poem.42208": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Lulu", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich liebe nicht den Hundetrab", "tokens": ["Ich", "lie\u00b7be", "nicht", "den", "Hun\u00b7de\u00b7trab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Allt\u00e4glichen Verkehres;", "tokens": ["All\u00b7t\u00e4g\u00b7li\u00b7chen", "Ver\u00b7keh\u00b7res", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich liebe das wogende Auf und Ab", "tokens": ["Ich", "lie\u00b7be", "das", "wo\u00b7gen\u00b7de", "Auf", "und", "Ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "KON", "APPR"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Des tosenden Weltenmeeres.", "tokens": ["Des", "to\u00b7sen\u00b7den", "Wel\u00b7ten\u00b7mee\u00b7res", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich liebe die Liebe, die ernste Kunst,", "tokens": ["Ich", "lie\u00b7be", "die", "Lie\u00b7be", ",", "die", "erns\u00b7te", "Kunst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Urewige Wissenschaft ist,", "tokens": ["U\u00b7re\u00b7wi\u00b7ge", "Wis\u00b7sen\u00b7schaft", "ist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Die Liebe, die heilige Himmelsgunst,", "tokens": ["Die", "Lie\u00b7be", ",", "die", "hei\u00b7li\u00b7ge", "Him\u00b7mels\u00b7gunst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die irdische Riesenkraft ist.", "tokens": ["Die", "ir\u00b7di\u00b7sche", "Rie\u00b7sen\u00b7kraft", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+--+---", "measure": "iambic.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Mein ganzes Innre erf\u00fclle der Mann", "tokens": ["Mein", "gan\u00b7zes", "Inn\u00b7re", "er\u00b7f\u00fcl\u00b7le", "der", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit Wucht und mit seelischer Gr\u00f6\u00dfe.", "tokens": ["Mit", "Wucht", "und", "mit", "see\u00b7li\u00b7scher", "Gr\u00f6\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Aufjauchzend vor Stolz enth\u00fcll ich ihm dann,", "tokens": ["Auf\u00b7jauch\u00b7zend", "vor", "Stolz", "ent\u00b7h\u00fcll", "ich", "ihm", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aufjauchzend vor Gl\u00fcck meine Bl\u00f6\u00dfe.", "tokens": ["Auf\u00b7jauch\u00b7zend", "vor", "Gl\u00fcck", "mei\u00b7ne", "Bl\u00f6\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}