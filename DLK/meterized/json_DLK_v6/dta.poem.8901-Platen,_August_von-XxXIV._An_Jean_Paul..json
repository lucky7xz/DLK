{"dta.poem.8901": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxXIV.  \n  An Jean Paul.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Bereut' ich, da\u00df ich meine Pflicht verschoben,", "tokens": ["Be\u00b7reut'", "ich", ",", "da\u00df", "ich", "mei\u00b7ne", "Pflicht", "ver\u00b7scho\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nie zu dir ein Wort des Danks erhoben", "tokens": ["Und", "nie", "zu", "dir", "ein", "Wort", "des", "Danks", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPER", "ART", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr deine seelenvolle Lieb' und Milde.", "tokens": ["F\u00fcr", "dei\u00b7ne", "see\u00b7len\u00b7vol\u00b7le", "Lieb'", "und", "Mil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun hat der Tod mit seinem Gorgoschilde", "tokens": ["Nun", "hat", "der", "Tod", "mit", "sei\u00b7nem", "Gor\u00b7go\u00b7schil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Blick erstarrt, der gern geschaut nach oben,", "tokens": ["Den", "Blick", "er\u00b7starrt", ",", "der", "gern", "ge\u00b7schaut", "nach", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "ADV", "VVPP", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und was ich Freundliches f\u00fcr dich gewoben,", "tokens": ["Und", "was", "ich", "Freund\u00b7li\u00b7ches", "f\u00fcr", "dich", "ge\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Send' ich dir nach in fremdere Gefilde.", "tokens": ["Send'", "ich", "dir", "nach", "in", "frem\u00b7de\u00b7re", "Ge\u00b7fil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es hat den J\u00fcngling deine Gunst belebet,", "tokens": ["Es", "hat", "den", "J\u00fcng\u00b7ling", "dei\u00b7ne", "Gunst", "be\u00b7le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir galt f\u00fcr k\u00fcnft'ge Gluth der erste Zunder,", "tokens": ["Dir", "galt", "f\u00fcr", "k\u00fcnft'\u00b7ge", "Gluth", "der", "ers\u00b7te", "Zun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf dem noch kaum ein Funke schwach gebebet.", "tokens": ["Auf", "dem", "noch", "kaum", "ein", "Fun\u00b7ke", "schwach", "ge\u00b7be\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADV", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun weilt dein ewig wonniger, gesunder,", "tokens": ["Nun", "weilt", "dein", "e\u00b7wig", "won\u00b7ni\u00b7ger", ",", "ge\u00b7sun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJD", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verj\u00fcngter Geist, wohin er stets geschwebet,", "tokens": ["Ver\u00b7j\u00fcng\u00b7ter", "Geist", ",", "wo\u00b7hin", "er", "stets", "ge\u00b7schwe\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im \u00fcberschw\u00e4nglichen Gebiet der Wunder.", "tokens": ["Im", "\u00fc\u00b7bersc\u00b7hw\u00e4ng\u00b7li\u00b7chen", "Ge\u00b7biet", "der", "Wun\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}