{"textgrid.poem.50825": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der K\u00fcrassier", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich spute mich nach Hause in kalter Regennacht:", "tokens": ["Ich", "spu\u00b7te", "mich", "nach", "Hau\u00b7se", "in", "kal\u00b7ter", "Re\u00b7gen\u00b7nacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da stehet d\u00fcster schimmernd und lautlos auf der Wacht", "tokens": ["Da", "ste\u00b7het", "d\u00fcs\u00b7ter", "schim\u00b7mernd", "und", "laut\u00b7los", "auf", "der", "Wacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ADJD", "KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der schlanke, der blanke, der schwere K\u00fcrassier.", "tokens": ["Der", "schlan\u00b7ke", ",", "der", "blan\u00b7ke", ",", "der", "schwe\u00b7re", "K\u00fc\u00b7ras\u00b7sier", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Er fl\u00fcstert leis: \u00bbMich hungert, ein Kreuzer, Herr, zu Brot!\u00ab", "tokens": ["Er", "fl\u00fcs\u00b7tert", "leis", ":", "\u00bb", "Mich", "hun\u00b7gert", ",", "ein", "Kreu\u00b7zer", ",", "Herr", ",", "zu", "Brot", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "$(", "PPER", "VVFIN", "$,", "ART", "NN", "$,", "NN", "$,", "APPR", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich stehe still, erschrocken, und werde f\u00fcr ihn rot.", "tokens": ["Ich", "ste\u00b7he", "still", ",", "er\u00b7schro\u00b7cken", ",", "und", "wer\u00b7de", "f\u00fcr", "ihn", "rot", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "VVPP", "$,", "KON", "VAFIN", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der schlanke, der blanke, der schwere K\u00fcrassier!", "tokens": ["Der", "schlan\u00b7ke", ",", "der", "blan\u00b7ke", ",", "der", "schwe\u00b7re", "K\u00fc\u00b7ras\u00b7sier", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Doch wie ich meinen Bettel will teilen mit ihm drauf,", "tokens": ["Doch", "wie", "ich", "mei\u00b7nen", "Bet\u00b7tel", "will", "tei\u00b7len", "mit", "ihm", "drauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPOSAT", "NN", "VMFIN", "VVINF", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da rasselt die Karosse vorbei im schnellen Lauf.", "tokens": ["Da", "ras\u00b7selt", "die", "Ka\u00b7ros\u00b7se", "vor\u00b7bei", "im", "schnel\u00b7len", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+--++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Auf, schlanker und blanker, du schwerer K\u00fcrassier!", "tokens": ["Auf", ",", "schlan\u00b7ker", "und", "blan\u00b7ker", ",", "du", "schwe\u00b7rer", "K\u00fc\u00b7ras\u00b7sier", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "ADJD", "KON", "ADJD", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Drin sitzt ein abgeflattert, blutlos Ministerweib:", "tokens": ["Drin", "sitzt", "ein", "ab\u00b7ge\u00b7flat\u00b7tert", ",", "blut\u00b7los", "Mi\u00b7nis\u00b7ter\u00b7weib", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "VVPP", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Reiter l\u00e4\u00dft erklirren den starren Riesenleib,", "tokens": ["Der", "Rei\u00b7ter", "l\u00e4\u00dft", "er\u00b7klir\u00b7ren", "den", "star\u00b7ren", "Rie\u00b7sen\u00b7leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der schlanke, der blanke, der schwere K\u00fcrassier.", "tokens": ["Der", "schlan\u00b7ke", ",", "der", "blan\u00b7ke", ",", "der", "schwe\u00b7re", "K\u00fc\u00b7ras\u00b7sier", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Dann nimmt er meine Gabe und bittet demutsvoll,", "tokens": ["Dann", "nimmt", "er", "mei\u00b7ne", "Ga\u00b7be", "und", "bit\u00b7tet", "de\u00b7muts\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df ich doch unsern Handel niemanden sagen soll \u2013", "tokens": ["Da\u00df", "ich", "doch", "un\u00b7sern", "Han\u00b7del", "nie\u00b7man\u00b7den", "sa\u00b7gen", "soll", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "PIS", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der schlanke, der blanke, der schwere K\u00fcrassier.", "tokens": ["Der", "schlan\u00b7ke", ",", "der", "blan\u00b7ke", ",", "der", "schwe\u00b7re", "K\u00fc\u00b7ras\u00b7sier", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "So steht er noch ein St\u00fcndlein und gr\u00fcbelt sonder Harm,", "tokens": ["So", "steht", "er", "noch", "ein", "St\u00fcnd\u00b7lein", "und", "gr\u00fc\u00b7belt", "son\u00b7der", "Harm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "KON", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Etwa: Im K\u00f6nigssaale, da ist es wohl recht warm.", "tokens": ["Et\u00b7wa", ":", "Im", "K\u00f6\u00b7nigs\u00b7saa\u00b7le", ",", "da", "ist", "es", "wohl", "recht", "warm", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "APPRART", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der schlanke, der blanke, der schwere K\u00fcrassier.", "tokens": ["Der", "schlan\u00b7ke", ",", "der", "blan\u00b7ke", ",", "der", "schwe\u00b7re", "K\u00fc\u00b7ras\u00b7sier", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Bis einsmals er im Fieber von seinem Posten geht \u2013", "tokens": ["Bis", "eins\u00b7mals", "er", "im", "Fie\u00b7ber", "von", "sei\u00b7nem", "Pos\u00b7ten", "geht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "APPRART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Drauf hei\u00dft es: \u00bbNummer Neune liegt tot im Lazareth.\u00ab", "tokens": ["Drauf", "hei\u00dft", "es", ":", "\u00bb", "Num\u00b7mer", "Neu\u00b7ne", "liegt", "tot", "im", "La\u00b7za\u00b7reth", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$.", "$(", "NE", "NE", "VVFIN", "ADJD", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der schlanke, der kranke, der arme K\u00fcrassier!", "tokens": ["Der", "schlan\u00b7ke", ",", "der", "kran\u00b7ke", ",", "der", "ar\u00b7me", "K\u00fc\u00b7ras\u00b7sier", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "Es wird an seiner Treue zuschanden jeder Spott;", "tokens": ["Es", "wird", "an", "sei\u00b7ner", "Treu\u00b7e", "zu\u00b7schan\u00b7den", "je\u00b7der", "Spott", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er starb ja f\u00fcr den K\u00f6nig, f\u00fcr Vaterland und Gott!", "tokens": ["Er", "starb", "ja", "f\u00fcr", "den", "K\u00f6\u00b7nig", ",", "f\u00fcr", "Va\u00b7ter\u00b7land", "und", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der schlanke, der tote, der arme K\u00fcrassier! \u2013", "tokens": ["Der", "schlan\u00b7ke", ",", "der", "to\u00b7te", ",", "der", "ar\u00b7me", "K\u00fc\u00b7ras\u00b7sier", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}