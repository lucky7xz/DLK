{"textgrid.poem.37556": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der weise Schuhu", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Schuhu h\u00f6rte stets mit Ruh,", "tokens": ["Der", "Schu\u00b7hu", "h\u00f6r\u00b7te", "stets", "mit", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wenn zwei sich disputierten, zu. \u2013", "tokens": ["wenn", "zwei", "sich", "dis\u00b7pu\u00b7tier\u00b7ten", ",", "zu", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KOUS", "CARD", "PRF", "VVINF", "$,", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mal stritten sich der Storch und Rabe,", "tokens": ["Mal", "strit\u00b7ten", "sich", "der", "Storch", "und", "Ra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "was Gott der Herr zuerst erschaffen habe,", "tokens": ["was", "Gott", "der", "Herr", "zu\u00b7erst", "er\u00b7schaf\u00b7fen", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ART", "NN", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "ob erst den Vogel oder erst das Ei.", "tokens": ["ob", "erst", "den", "Vo\u00b7gel", "o\u00b7der", "erst", "das", "Ei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NE", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbden Vogel!\u00ab \u2013 schrie der Storch \u2013", "tokens": ["\u00bb", "den", "Vo\u00b7gel", "!", "\u00ab", "\u2013", "schrie", "der", "Storch", "\u2013"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NE", "$.", "$(", "$(", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbdas ist so klar wie Brei!\u00ab", "tokens": ["\u00bb", "das", "ist", "so", "klar", "wie", "Brei", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VAFIN", "ADV", "ADJD", "KOKOM", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Rabe kr\u00e4chzt: \u00bbDas Ei, wobei ich bleibe;", "tokens": ["Der", "Ra\u00b7be", "kr\u00e4chzt", ":", "\u00bb", "Das", "Ei", ",", "wo\u00b7bei", "ich", "blei\u00b7be", ";"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "wer's nicht begreift, hat kein Gehirn im Leibe!\u00ab", "tokens": ["wer's", "nicht", "be\u00b7greift", ",", "hat", "kein", "Ge\u00b7hirn", "im", "Lei\u00b7be", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PTKNEG", "VVFIN", "$,", "VAFIN", "PIAT", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da fingen an zu quacken", "tokens": ["Da", "fin\u00b7gen", "an", "zu", "qua\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "zwei Fr\u00f6sch in gr\u00fcnen Jacken.", "tokens": ["zwei", "Fr\u00f6sch", "in", "gr\u00fc\u00b7nen", "Ja\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der eine quackt: \u00bbDer Storch hat recht!\u00ab", "tokens": ["Der", "ei\u00b7ne", "quackt", ":", "\u00bb", "Der", "Storch", "hat", "recht", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PIS", "VVFIN", "$.", "$(", "ART", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der zweite quackt: \u00bbDer Rab hat recht!\u00ab", "tokens": ["Der", "zwei\u00b7te", "quackt", ":", "\u00bb", "Der", "Rab", "hat", "recht", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$.", "$(", "ART", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbwas?\u00ab \u2013 schrien die beiden Disputaxe \u2013", "tokens": ["\u00bb", "was", "?", "\u00ab", "\u2013", "schri\u00b7en", "die", "bei\u00b7den", "Dis\u00b7pu\u00b7ta\u00b7xe", "\u2013"], "token_info": ["punct", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "$.", "$(", "$(", "VVFIN", "ART", "PIAT", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbwas ist das da f\u00fcr ein Gequackse??\u00ab \u2013", "tokens": ["\u00bb", "was", "ist", "das", "da", "f\u00fcr", "ein", "Ge\u00b7quack\u00b7se", "??", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PWS", "VAFIN", "ART", "ADV", "APPR", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Streit erlosch. \u2013", "tokens": ["Der", "Streit", "er\u00b7losch", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ein jeder nimmt sich seinen Frosch,", "tokens": ["Ein", "je\u00b7der", "nimmt", "sich", "sei\u00b7nen", "Frosch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der schmeckt ihm gar nicht schlecht.", "tokens": ["der", "schmeckt", "ihm", "gar", "nicht", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbja\u00ab \u2013 denkt der Schuhu \u2013 \u00bbso bin ich!", "tokens": ["\u00bb", "ja", "\u00ab", "\u2013", "denkt", "der", "Schu\u00b7hu", "\u2013", "\u00bb", "so", "bin", "ich", "!"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$(", "$(", "VVFIN", "ART", "NN", "$(", "$(", "ADV", "VAFIN", "PPER", "$."], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Der Weise schweigt und r\u00e4uspert sich!\u00ab", "tokens": ["Der", "Wei\u00b7se", "schweigt", "und", "r\u00e4us\u00b7pert", "sich", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PRF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}