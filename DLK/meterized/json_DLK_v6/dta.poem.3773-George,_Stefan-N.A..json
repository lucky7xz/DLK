{"dta.poem.3773": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1892", "urn": "urn:nbn:de:kobv:b4-200905191524", "language": ["de:0.99"], "booktitle": "George, Stefan: Algabal. Paris u. a., 1892."}, "poem": {"stanza.1": {"line.1": {"text": "Der weisses licht und weissen glanz vereint", "tokens": ["Der", "weis\u00b7ses", "licht", "und", "weis\u00b7sen", "glanz", "ver\u00b7eint"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das dach ist glas die streu gebleichter felle", "tokens": ["Das", "dach", "ist", "glas", "die", "streu", "ge\u00b7bleich\u00b7ter", "fel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PAV", "VAFIN", "VVFIN", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am boden schnee und oben wolke scheint", "tokens": ["Am", "bo\u00b7den", "schnee", "und", "o\u00b7ben", "wol\u00b7ke", "scheint"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "KON", "ADV", "NE", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der w\u00e4nde matte t\u00e4felung aus zedern \u00b7", "tokens": ["Der", "w\u00e4n\u00b7de", "mat\u00b7te", "t\u00e4\u00b7fe\u00b7lung", "aus", "ze\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "APPR", "XY", "XY"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die dreissig pfauen stehen dran im kreis", "tokens": ["Die", "dreis\u00b7sig", "pfau\u00b7en", "ste\u00b7hen", "dran", "im", "kreis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "CARD", "NN", "VVFIN", "PAV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie tragen daunen blank wie schwanenfedern", "tokens": ["Sie", "tra\u00b7gen", "dau\u00b7nen", "blank", "wie", "schwa\u00b7nen\u00b7fe\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "PTKVZ", "KOKOM", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ihre schleppen schimmern wie das eis", "tokens": ["Und", "ih\u00b7re", "schlep\u00b7pen", "schim\u00b7mern", "wie", "das", "eis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "F\u00fcr jede zier die freunden farbenstrahlen:", "tokens": ["F\u00fcr", "je\u00b7de", "zier", "die", "freun\u00b7den", "far\u00b7ben\u00b7strah\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus blitzendem und blinderem metall", "tokens": ["Aus", "blit\u00b7zen\u00b7dem", "und", "blin\u00b7de\u00b7rem", "me\u00b7tall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus elfenbein und milchigen opalen", "tokens": ["Aus", "el\u00b7fen\u00b7bein", "und", "mil\u00b7chi\u00b7gen", "o\u00b7pa\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus demant alabaster und kristall", "tokens": ["Aus", "de\u00b7mant", "a\u00b7la\u00b7bas\u00b7ter", "und", "kris\u00b7tall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "KON", "NN"], "meter": "---+-+-+++", "measure": "zehnsilber"}}, "stanza.4": {"line.1": {"text": "Und perlen! klare gaben dumpfer st\u00e4tte", "tokens": ["Und", "per\u00b7len", "!", "kla\u00b7re", "ga\u00b7ben", "dum\u00b7pfer", "st\u00e4t\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "$.", "ADJA", "VVFIN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ihr wie menschliche gebilde rollt", "tokens": ["Die", "ihr", "wie", "menschli\u00b7che", "ge\u00b7bil\u00b7de", "rollt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "KOKOM", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und doch an einer wange warmer gl\u00e4tte", "tokens": ["Und", "doch", "an", "ei\u00b7ner", "wan\u00b7ge", "war\u00b7mer", "gl\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das nasse k\u00fchl beharrlich wahren sollt", "tokens": ["Das", "nas\u00b7se", "k\u00fchl", "be\u00b7harr\u00b7lich", "wah\u00b7ren", "sollt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "ADJD", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Da lag die kugel auch von murra-stein", "tokens": ["Da", "lag", "die", "ku\u00b7gel", "auch", "von", "mur\u00b7ra\u00b7stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit der in fr\u00fcher jugend Er gespielt", "tokens": ["Mit", "der", "in", "fr\u00fc\u00b7her", "ju\u00b7gend", "Er", "ge\u00b7spielt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "ADJA", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des kaisers finger war am tage rein", "tokens": ["Des", "kai\u00b7sers", "fin\u00b7ger", "war", "am", "ta\u00b7ge", "rein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo thr\u00e4nend er sie vor das auge hielt", "tokens": ["Wo", "thr\u00e4\u00b7nend", "er", "sie", "vor", "das", "au\u00b7ge", "hielt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}