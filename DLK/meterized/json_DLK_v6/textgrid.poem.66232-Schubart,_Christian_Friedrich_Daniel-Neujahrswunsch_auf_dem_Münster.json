{"textgrid.poem.66232": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Neujahrswunsch auf dem M\u00fcnster", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schau hinab, o Gott, auf deine Erde,", "tokens": ["Schau", "hin\u00b7ab", ",", "o", "Gott", ",", "auf", "dei\u00b7ne", "Er\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "FM", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sieh der Menschen \u00e4ngstliches Gew\u00fchl.", "tokens": ["Sieh", "der", "Men\u00b7schen", "\u00e4ngst\u00b7li\u00b7ches", "Ge\u00b7w\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach da gibt's, du wei\u00dft's ja, viel Beschwerde.", "tokens": ["Ach", "da", "gibt's", ",", "du", "wei\u00dft's", "ja", ",", "viel", "Be\u00b7schwer\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$,", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und des Stoffs zu Thr\u00e4nen gibt es viel.", "tokens": ["Und", "des", "Stoffs", "zu", "Thr\u00e4\u00b7nen", "gibt", "es", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Christen gibt es, die sich scheun zu sagen,", "tokens": ["Chris\u00b7ten", "gibt", "es", ",", "die", "sich", "scheun", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PRELS", "PRF", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df sie Christus, da\u00df sie Gottes sind;", "tokens": ["Da\u00df", "sie", "Chris\u00b7tus", ",", "da\u00df", "sie", "Got\u00b7tes", "sind", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "$,", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Weise gibt es, die die Thoren tragen,", "tokens": ["Wei\u00b7se", "gibt", "es", ",", "die", "die", "Tho\u00b7ren", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und mit ihren Seufzern spielt der Wind;", "tokens": ["Und", "mit", "ih\u00b7ren", "Seuf\u00b7zern", "spielt", "der", "Wind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Tugendhafte, die den Strom der Laster", "tokens": ["Tu\u00b7gend\u00b7haf\u00b7te", ",", "die", "den", "Strom", "der", "Las\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00fcrchterlich vor\u00fcberziehen sehn,", "tokens": ["F\u00fcrch\u00b7ter\u00b7lich", "vor\u00b7\u00fc\u00b7ber\u00b7zie\u00b7hen", "sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Auf dem Strome segelt ein verha\u00dfter", "tokens": ["Auf", "dem", "Stro\u00b7me", "se\u00b7gelt", "ein", "ver\u00b7ha\u00df\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "W\u00fctherich, taub f\u00fcr der Menschen Flehn;", "tokens": ["W\u00fct\u00b7he\u00b7rich", ",", "taub", "f\u00fcr", "der", "Men\u00b7schen", "Flehn", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Greise, die mit d\u00fcnnen wei\u00dfen Haaren,", "tokens": ["Grei\u00b7se", ",", "die", "mit", "d\u00fcn\u00b7nen", "wei\u00b7\u00dfen", "Haa\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mit des Fluches schrecklichem Gewicht", "tokens": ["Mit", "des", "Flu\u00b7ches", "schreck\u00b7li\u00b7chem", "Ge\u00b7wicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach hinunter in die Grube fahren,", "tokens": ["Ach", "hin\u00b7un\u00b7ter", "in", "die", "Gru\u00b7be", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Denn ihr Enkel ist ein B\u00f6sewicht!", "tokens": ["Denn", "ihr", "En\u00b7kel", "ist", "ein", "B\u00f6\u00b7se\u00b7wicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Unschuld, die am Todesh\u00fcgel jammert,", "tokens": ["Un\u00b7schuld", ",", "die", "am", "To\u00b7des\u00b7h\u00fc\u00b7gel", "jam\u00b7mert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo der Vater, wo die Mutter ruht;", "tokens": ["Wo", "der", "Va\u00b7ter", ",", "wo", "die", "Mut\u00b7ter", "ruht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie sie da das Todtenkreuz umklammert,", "tokens": ["Wie", "sie", "da", "das", "Tod\u00b7ten\u00b7kreuz", "um\u00b7klam\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie sie \u00e4chzt: Ach, rettet euer Blut!", "tokens": ["Wie", "sie", "\u00e4chzt", ":", "Ach", ",", "ret\u00b7tet", "eu\u00b7er", "Blut", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$.", "ITJ", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Denn sie scheucht der L\u00fcstling, der zum Raube", "tokens": ["Denn", "sie", "scheucht", "der", "L\u00fcst\u00b7ling", ",", "der", "zum", "Rau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Im Gebeinhaus t\u00fcckisch sich verbirgt:", "tokens": ["Im", "Ge\u00b7bein\u00b7haus", "t\u00fc\u00b7ckisch", "sich", "ver\u00b7birgt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie der Geier, der die fromme Taube", "tokens": ["Wie", "der", "Gei\u00b7er", ",", "der", "die", "from\u00b7me", "Tau\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Selbst auf Tempelzinnen niederw\u00fcrgt.", "tokens": ["Selbst", "auf", "Tem\u00b7pel\u00b7zin\u00b7nen", "nie\u00b7der\u00b7w\u00fcrgt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Patrioten, die am Eichenstamme", "tokens": ["Pat\u00b7ri\u00b7o\u00b7ten", ",", "die", "am", "Ei\u00b7chen\u00b7stam\u00b7me"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mit gesenktem, tr\u00fcbem Blicke stehn:", "tokens": ["Mit", "ge\u00b7senk\u00b7tem", ",", "tr\u00fc\u00b7bem", "Bli\u00b7cke", "stehn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach, sie sehn mit unterdr\u00fcckter Flamme", "tokens": ["Ach", ",", "sie", "sehn", "mit", "un\u00b7ter\u00b7dr\u00fcck\u00b7ter", "Flam\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deutsche Sitt' und Freiheit untergehn.", "tokens": ["Deut\u00b7sche", "Sitt'", "und", "Frei\u00b7heit", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "J\u00fcnglinge, beim dumpfen Traurgel\u00e4ute", "tokens": ["J\u00fcng\u00b7lin\u00b7ge", ",", "beim", "dum\u00b7pfen", "Traur\u00b7ge\u00b7l\u00e4u\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPRART", "ADJA", "NN"], "meter": "++--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Langsam schreitend zu der schwarzen Gruft,", "tokens": ["Lang\u00b7sam", "schrei\u00b7tend", "zu", "der", "schwar\u00b7zen", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Um die sch\u00f6nste, edelste der Br\u00e4ute", "tokens": ["Um", "die", "sch\u00f6ns\u00b7te", ",", "e\u00b7dels\u00b7te", "der", "Br\u00e4u\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "$,", "ADJA", "ART", "NN"], "meter": "--+-+---+-", "measure": "anapaest.init"}, "line.4": {"text": "Jammert ihre Klage in die Luft.", "tokens": ["Jam\u00b7mert", "ih\u00b7re", "Kla\u00b7ge", "in", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Vater! alle diese Menschen unten", "tokens": ["Va\u00b7ter", "!", "al\u00b7le", "die\u00b7se", "Men\u00b7schen", "un\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PIS", "PDAT", "NN", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "M\u00fcssen sterben \u2013 deine Engel nicht!", "tokens": ["M\u00fcs\u00b7sen", "ster\u00b7ben", "\u2013", "dei\u00b7ne", "En\u00b7gel", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$(", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sterben \u2013 ach mit hei\u00dfen offnen Wunden,", "tokens": ["Ster\u00b7ben", "\u2013", "ach", "mit", "hei\u00b7\u00dfen", "off\u00b7nen", "Wun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "XY", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zittern vor Verwesung und Gericht.", "tokens": ["Zit\u00b7tern", "vor", "Ver\u00b7we\u00b7sung", "und", "Ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Sch\u00f6pfer! Vater, ach erbarm dich ihrer,", "tokens": ["Sch\u00f6p\u00b7fer", "!", "Va\u00b7ter", ",", "ach", "er\u00b7barm", "dich", "ih\u00b7rer", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$,", "ADV", "ADJD", "PPER", "PPOSAT", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sieh dies Wimmeln deiner Kinder an;", "tokens": ["Sieh", "dies", "Wim\u00b7meln", "dei\u00b7ner", "Kin\u00b7der", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Auf des Lebens dornenvoller Bahn.", "tokens": ["Auf", "des", "Le\u00b7bens", "dor\u00b7nen\u00b7vol\u00b7ler", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Sieh, auf dieses Thurmes luft'gen H\u00f6hen", "tokens": ["Sieh", ",", "auf", "die\u00b7ses", "Thur\u00b7mes", "luft'\u00b7gen", "H\u00f6\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "PDAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bitt' ich dich mit hochgehobner Hand:", "tokens": ["Bitt'", "ich", "dich", "mit", "hoch\u00b7ge\u00b7hob\u00b7ner", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie die Eiche tiefgewurzelt stehen", "tokens": ["Wie", "die", "Ei\u00b7che", "tief\u00b7ge\u00b7wur\u00b7zelt", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "La\u00df mein Vaterland, mein Vaterland,", "tokens": ["La\u00df", "mein", "Va\u00b7ter\u00b7land", ",", "mein", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Unsern Kaiser! La\u00df die F\u00fcrsten leben,", "tokens": ["Un\u00b7sern", "Kai\u00b7ser", "!", "La\u00df", "die", "F\u00fcrs\u00b7ten", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVIMP", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dir nachahmend, ohne blut'gen Zwist;", "tokens": ["Dir", "nac\u00b7hah\u00b7mend", ",", "oh\u00b7ne", "blut'\u00b7gen", "Zwist", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "KOUI", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aber la\u00df sie vor dem Donner beben:", "tokens": ["A\u00b7ber", "la\u00df", "sie", "vor", "dem", "Don\u00b7ner", "be\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df du Richter aller F\u00fcrsten bist.", "tokens": ["Da\u00df", "du", "Rich\u00b7ter", "al\u00b7ler", "F\u00fcrs\u00b7ten", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PIAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Rei\u00df dem Heuchler in der Wahrheit Lichte", "tokens": ["Rei\u00df", "dem", "Heuch\u00b7ler", "in", "der", "Wahr\u00b7heit", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Seine schwarze Larve vom Gesicht,", "tokens": ["Sei\u00b7ne", "schwar\u00b7ze", "Lar\u00b7ve", "vom", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aber ist die Larve vom Gesichte,", "tokens": ["A\u00b7ber", "ist", "die", "Lar\u00b7ve", "vom", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.pen