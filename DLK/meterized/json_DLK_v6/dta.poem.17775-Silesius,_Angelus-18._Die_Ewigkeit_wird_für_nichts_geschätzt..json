{"dta.poem.17775": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "18.  Die Ewigkeit wird f\u00fcr nichts gesch\u00e4tzt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "O Thorheit/ umb die zeit wagt man sich bi\u00df inn Tod!", "tokens": ["O", "Thor\u00b7heit", "/", "umb", "die", "zeit", "wagt", "man", "sich", "bi\u00df", "inn", "Tod", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "APPR", "ART", "NN", "VVFIN", "PIS", "PRF", "APPR", "APPR", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und auf die Ewigkeit setzt man nur einen Spott!", "tokens": ["Und", "auf", "die", "E\u00b7wig\u00b7keit", "setzt", "man", "nur", "ei\u00b7nen", "Spott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PIS", "ADV", "ART", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}