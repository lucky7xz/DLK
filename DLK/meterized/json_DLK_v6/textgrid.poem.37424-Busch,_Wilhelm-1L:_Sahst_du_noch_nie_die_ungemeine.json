{"textgrid.poem.37424": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sahst du noch nie die ungemeine", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sahst du noch nie die ungemeine", "tokens": ["Sahst", "du", "noch", "nie", "die", "un\u00b7ge\u00b7mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und hohe Kunstgelenkigkeit,", "tokens": ["Und", "ho\u00b7he", "Kunst\u00b7ge\u00b7len\u00b7kig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sowohl der Fl\u00fcgel, wie der Beine,", "tokens": ["So\u00b7wohl", "der", "Fl\u00fc\u00b7gel", ",", "wie", "der", "Bei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Tierbereich mit stillem Neid?", "tokens": ["Im", "Tier\u00b7be\u00b7reich", "mit", "stil\u00b7lem", "Neid", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh nur, wie aus dem Feldgekl\u00fcfte", "tokens": ["Sieh", "nur", ",", "wie", "aus", "dem", "Feld\u00b7ge\u00b7kl\u00fcf\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf seinen Schwingen wunderbar", "tokens": ["Auf", "sei\u00b7nen", "Schwin\u00b7gen", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis zu den Wolken durch die L\u00fcfte", "tokens": ["Bis", "zu", "den", "Wol\u00b7ken", "durch", "die", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In stolzen Kreisen schwebt der Aar.", "tokens": ["In", "stol\u00b7zen", "Krei\u00b7sen", "schwebt", "der", "Aar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sieh nur das Tierchen, das geringe,", "tokens": ["Sieh", "nur", "das", "Tier\u00b7chen", ",", "das", "ge\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das zu benennen sich nicht ziemt,", "tokens": ["Das", "zu", "be\u00b7nen\u00b7nen", "sich", "nicht", "ziemt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "VVINF", "PRF", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist durch seine Meisterspr\u00fcnge,", "tokens": ["Es", "ist", "durch", "sei\u00b7ne", "Meis\u00b7ter\u00b7spr\u00fcn\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn nicht beliebt, so doch ber\u00fchmt.", "tokens": ["Wenn", "nicht", "be\u00b7liebt", ",", "so", "doch", "be\u00b7r\u00fchmt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ADJD", "$,", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Leicht zu erlegen diese beiden,", "tokens": ["Leicht", "zu", "er\u00b7le\u00b7gen", "die\u00b7se", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "PDAT", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das schlag dir lieber aus dem Sinn.", "tokens": ["Das", "schlag", "dir", "lie\u00b7ber", "aus", "dem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer es versucht, der wird bescheiden,", "tokens": ["Wer", "es", "ver\u00b7sucht", ",", "der", "wird", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "PRELS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sei's J\u00e4ger oder J\u00e4gerin.", "tokens": ["Sei's", "J\u00e4\u00b7ger", "o\u00b7der", "J\u00e4\u00b7ge\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}