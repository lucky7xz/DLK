{"textgrid.poem.36175": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: F\u00fcrst sonder Furcht und Trug, du kommst zum lauern S\u00fcden", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrst sonder Furcht und Trug, du kommst zum lauern S\u00fcden", "tokens": ["F\u00fcrst", "son\u00b7der", "Furcht", "und", "Trug", ",", "du", "kommst", "zum", "lau\u00b7ern", "S\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fernher vom starren Nord, zu f\u00f6rdern Ruh und Frieden,", "tokens": ["Fern\u00b7her", "vom", "star\u00b7ren", "Nord", ",", "zu", "f\u00f6r\u00b7dern", "Ruh", "und", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu schlichten jeden Zwist, zu s\u00fchnen alle Wuth!", "tokens": ["Zu", "schlich\u00b7ten", "je\u00b7den", "Zwist", ",", "zu", "s\u00fch\u00b7nen", "al\u00b7le", "Wuth", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PIAT", "NN", "$,", "APPR", "ADJA", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Trutz willst du bek\u00e4mpfen,", "tokens": ["Den", "Trutz", "willst", "du", "be\u00b7k\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gewalt und Frevel d\u00e4mpfen,", "tokens": ["Ge\u00b7walt", "und", "Fre\u00b7vel", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Unschuld Hort und Hut!", "tokens": ["Der", "Un\u00b7schuld", "Hort", "und", "Hut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bis uns willkommen dann! Und Gott der Herr sey gn\u00e4dig", "tokens": ["Bis", "uns", "will\u00b7kom\u00b7men", "dann", "!", "Und", "Gott", "der", "Herr", "sey", "gn\u00e4\u00b7dig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADJD", "ADV", "$.", "KON", "NN", "ART", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem, das du willst und w\u00fcrkst! Der Bande los und ledig", "tokens": ["Dem", ",", "das", "du", "willst", "und", "w\u00fcrkst", "!", "Der", "Ban\u00b7de", "los", "und", "le\u00b7dig"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "PPER", "VMFIN", "KON", "VAFIN", "$.", "ART", "NN", "PTKVZ", "KON", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Werd' unser Volk durch dich! M\u00f6gst du mit reiner Hand", "tokens": ["Werd'", "un\u00b7ser", "Volk", "durch", "dich", "!", "M\u00f6gst", "du", "mit", "rei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "PPER", "$.", "VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Zwietracht Thor verriegeln,", "tokens": ["Der", "Zwiet\u00b7racht", "Thor", "ver\u00b7rie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Rache Schlund versiegeln,", "tokens": ["Der", "Ra\u00b7che", "Schlund", "ver\u00b7sie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Held, bis zum Weltenbrand!", "tokens": ["Held", ",", "bis", "zum", "Wel\u00b7ten\u00b7brand", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "M\u00f6gst du vom Untergang der Menschheit Tr\u00fcmmer retten!", "tokens": ["M\u00f6gst", "du", "vom", "Un\u00b7ter\u00b7gang", "der", "Menschheit", "Tr\u00fcm\u00b7mer", "ret\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Geling' es dir, den Feind, den Argen, anzuketten!", "tokens": ["Ge\u00b7ling'", "es", "dir", ",", "den", "Feind", ",", "den", "Ar\u00b7gen", ",", "an\u00b7zu\u00b7ket\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "PPER", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Stift' ein atlantisch Reich! Pflanz' einen Gottesstaat!", "tokens": ["Stift'", "ein", "at\u00b7lan\u00b7tisch", "Reich", "!", "Pflanz'", "ei\u00b7nen", "Got\u00b7tes\u00b7staat", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJD", "NN", "$.", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und schimmern soll dein Name,", "tokens": ["Und", "schim\u00b7mern", "soll", "dein", "Na\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wuchern soll dein Same,", "tokens": ["Und", "wu\u00b7chern", "soll", "dein", "Sa\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcrst, gleich der Sternensaat!", "tokens": ["F\u00fcrst", ",", "gleich", "der", "Ster\u00b7nen\u00b7saat", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Wer ist es, sagt mirs an, wer ist der Recht' und Eine,", "tokens": ["Wer", "ist", "es", ",", "sagt", "mirs", "an", ",", "wer", "ist", "der", "Recht'", "und", "Ei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "VVFIN", "NE", "PTKVZ", "$,", "PWS", "VAFIN", "ART", "NN", "KON", "ART", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Gottes Bild noch tr\u00e4gt in seiner Treu und Reine?", "tokens": ["Der", "Got\u00b7tes", "Bild", "noch", "tr\u00e4gt", "in", "sei\u00b7ner", "Treu", "und", "Rei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Held ist's, der zugleich kindlich und fromm und mild!", "tokens": ["Der", "Held", "ist's", ",", "der", "zu\u00b7gleich", "kind\u00b7lich", "und", "fromm", "und", "mild", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der W\u00fcthrich wird zerst\u00e4uben;", "tokens": ["Der", "W\u00fcth\u00b7rich", "wird", "zer\u00b7st\u00e4u\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der G\u00fctige wird bleiben;", "tokens": ["Der", "G\u00fc\u00b7ti\u00b7ge", "wird", "blei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Denn er tr\u00e4gt Gottes Bild!", "tokens": ["Denn", "er", "tr\u00e4gt", "Got\u00b7tes", "Bild", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Die ihr auf Thronen prangt, um bald im Staub zu modern,", "tokens": ["Die", "ihr", "auf", "Thro\u00b7nen", "prangt", ",", "um", "bald", "im", "Staub", "zu", "mo\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$,", "KOUI", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der K\u00f6nige K\u00f6nig wird vor seinen Stuhl euch fodern,", "tokens": ["Der", "K\u00f6\u00b7ni\u00b7ge", "K\u00f6\u00b7nig", "wird", "vor", "sei\u00b7nen", "Stuhl", "euch", "fo\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Pr\u00fcfend so Schrot als Korn, ein strenger Waradein.", "tokens": ["Pr\u00fc\u00b7fend", "so", "Schrot", "als", "Korn", ",", "ein", "stren\u00b7ger", "Wa\u00b7ra\u00b7dein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADJD", "KOKOM", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Er sch\u00fcrt der Schmelzgluth Flammen,", "tokens": ["Er", "sch\u00fcrt", "der", "Schmelz\u00b7gluth", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nur er mag euch verdammen,", "tokens": ["Nur", "er", "mag", "euch", "ver\u00b7dam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Nur er mag euch verzeihn!", "tokens": ["Nur", "er", "mag", "euch", "ver\u00b7zeihn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Weh, Krieger, \u00fcber dich, dem ob dem kalten Morden", "tokens": ["Weh", ",", "Krie\u00b7ger", ",", "\u00fc\u00b7ber", "dich", ",", "dem", "ob", "dem", "kal\u00b7ten", "Mor\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "APPR", "PPER", "$,", "PRELS", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Eingeweid' zu Stein, zu Stahl die Brust geworden,", "tokens": ["Das", "Ein\u00b7ge\u00b7weid'", "zu", "Stein", ",", "zu", "Stahl", "die", "Brust", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "APPR", "NN", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem nie die Wimper na\u00df und nie das Herz wird weich!", "tokens": ["Dem", "nie", "die", "Wim\u00b7per", "na\u00df", "und", "nie", "das", "Herz", "wird", "weich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "ADJD", "KON", "ADV", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht mag der Lorbeer dauern,", "tokens": ["Nicht", "mag", "der", "Lor\u00b7beer", "dau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Um den die V\u00f6lker trauern;", "tokens": ["Um", "den", "die", "V\u00f6l\u00b7ker", "trau\u00b7ern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr Jammer w\u00e4scht ihn bleich!", "tokens": ["Ihr", "Jam\u00b7mer", "w\u00e4scht", "ihn", "bleich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "F\u00fcrst sonder Trutz und Trug, magst du den Ruhm erneuern", "tokens": ["F\u00fcrst", "son\u00b7der", "Trutz", "und", "Trug", ",", "magst", "du", "den", "Ruhm", "er\u00b7neu\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$,", "VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "De\u00df, der bei L\u00fctzen stritt, den Deutschlands Hymnen feiern,", "tokens": ["De\u00df", ",", "der", "bei", "L\u00fct\u00b7zen", "stritt", ",", "den", "Deutschlands", "Hym\u00b7nen", "fei\u00b7ern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Weil er den Unhold schlug, vor dem der Welt gegraut!", "tokens": ["Weil", "er", "den", "Un\u00b7hold", "schlug", ",", "vor", "dem", "der", "Welt", "ge\u00b7graut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "APPR", "ART", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Held, la\u00df die Banner fliegen;", "tokens": ["Held", ",", "la\u00df", "die", "Ban\u00b7ner", "flie\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Denn schier in letzten Z\u00fcgen", "tokens": ["Denn", "schier", "in", "letz\u00b7ten", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Liegt Deutschland, deine Braut!", "tokens": ["Liegt", "Deutschland", ",", "dei\u00b7ne", "Braut", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}