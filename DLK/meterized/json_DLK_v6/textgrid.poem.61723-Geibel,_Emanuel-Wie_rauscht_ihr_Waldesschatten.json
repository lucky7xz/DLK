{"textgrid.poem.61723": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Wie rauscht ihr Waldesschatten", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie rauscht ihr Waldesschatten", "tokens": ["Wie", "rauscht", "ihr", "Wal\u00b7des\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So k\u00fchl noch weit und breit!", "tokens": ["So", "k\u00fchl", "noch", "weit", "und", "breit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie schaut im bunten Kleid", "tokens": ["Wie", "schaut", "im", "bun\u00b7ten", "Kleid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Blumen nur so lustig aus den Matten!", "tokens": ["Ihr", "Blu\u00b7men", "nur", "so", "lus\u00b7tig", "aus", "den", "Mat\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie m\u00f6gt ihr V\u00f6glein pfeifen", "tokens": ["Wie", "m\u00f6gt", "ihr", "V\u00f6\u00b7glein", "pfei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In dieser argen Zeit! \u2013", "tokens": ["In", "die\u00b7ser", "ar\u00b7gen", "Zeit", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mir ist so tr\u00fcb, ich kann es kaum begreifen.", "tokens": ["Mir", "ist", "so", "tr\u00fcb", ",", "ich", "kann", "es", "kaum", "be\u00b7grei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ist's doch ein Traum gewesen,", "tokens": ["Ist's", "doch", "ein", "Traum", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der sonder Spur verschwand,", "tokens": ["Der", "son\u00b7der", "Spur", "ver\u00b7schwand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df du, mein deutsches Land,", "tokens": ["Da\u00df", "du", ",", "mein", "deut\u00b7sches", "Land", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Noch einmal seist zu Ehren auserlesen.", "tokens": ["Noch", "ein\u00b7mal", "seist", "zu", "Eh\u00b7ren", "au\u00b7ser\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wo in vor'gen Tagen", "tokens": ["Und", "wo", "in", "vor'\u00b7gen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Stuhl des Kaisers stand,", "tokens": ["Der", "Stuhl", "des", "Kai\u00b7sers", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "W\u00e4chst fort das Gras; das mu\u00df ich ewig klagen.", "tokens": ["W\u00e4chst", "fort", "das", "Gras", ";", "das", "mu\u00df", "ich", "e\u00b7wig", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "ART", "NN", "$.", "PDS", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}