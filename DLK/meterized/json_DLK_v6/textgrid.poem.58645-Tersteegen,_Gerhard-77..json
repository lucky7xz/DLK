{"textgrid.poem.58645": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "77.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Fischer sucht durchs Lockaas nur die Fische fest zu kriegen,", "tokens": ["Der", "Fi\u00b7scher", "sucht", "durchs", "Loc\u00b7kaas", "nur", "die", "Fi\u00b7sche", "fest", "zu", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ADV", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "So f\u00e4ngst du, Herr, auch manche Seel' mit s\u00fc\u00dfen Liebesz\u00fcgen;", "tokens": ["So", "f\u00e4ngst", "du", ",", "Herr", ",", "auch", "man\u00b7che", "Seel'", "mit", "s\u00fc\u00b7\u00dfen", "Lie\u00b7bes\u00b7z\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "ADV", "PIAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Hernach schickst du viel J\u00e4ger oft, die Hunde laufen zu,", "tokens": ["Her\u00b7nach", "schickst", "du", "viel", "J\u00e4\u00b7ger", "oft", ",", "die", "Hun\u00b7de", "lau\u00b7fen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "ADV", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Man wird geplagt, man wird gejagt, man findet nirgends Ruh.", "tokens": ["Man", "wird", "ge\u00b7plagt", ",", "man", "wird", "ge\u00b7jagt", ",", "man", "fin\u00b7det", "nir\u00b7gends", "Ruh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "VVPP", "$,", "PIS", "VAFIN", "VVPP", "$,", "PIS", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Doch, Herr, du bist in allem gut, ich will auch nimmer klagen,", "tokens": ["Doch", ",", "Herr", ",", "du", "bist", "in", "al\u00b7lem", "gut", ",", "ich", "will", "auch", "nim\u00b7mer", "kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "PPER", "VAFIN", "APPR", "PIS", "ADJD", "$,", "PPER", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Ich will mich lassen fischen gern, ich will mich lassen jagen;", "tokens": ["Ich", "will", "mich", "las\u00b7sen", "fi\u00b7schen", "gern", ",", "ich", "will", "mich", "las\u00b7sen", "ja\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "VVFIN", "ADV", "$,", "PPER", "VMFIN", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Fahr fort und jag die Eigenheit aus allen Winkeln aus, \u2013", "tokens": ["Fahr", "fort", "und", "jag", "die", "Ei\u00b7gen\u00b7heit", "aus", "al\u00b7len", "Win\u00b7keln", "aus", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$,", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Nichts kann sich ja verbergen dir \u2013 so find' ich Ruh im Haus!", "tokens": ["Nichts", "kann", "sich", "ja", "ver\u00b7ber\u00b7gen", "dir", "\u2013", "so", "find'", "ich", "Ruh", "im", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "ADV", "VVFIN", "PPER", "$(", "ADV", "VVFIN", "PPER", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}