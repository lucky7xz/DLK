{"textgrid.poem.37492": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Hans Huckebein der Ungl\u00fccksrabe", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier sieht man Fritz, den muntern Knaben,", "tokens": ["Hier", "sieht", "man", "Fritz", ",", "den", "mun\u00b7tern", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nebst Huckebein, dem jungen Raben.", "tokens": ["Nebst", "Huc\u00b7ke\u00b7bein", ",", "dem", "jun\u00b7gen", "Ra\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und dieser Fritz, wie alle Knaben,", "tokens": ["Und", "die\u00b7ser", "Fritz", ",", "wie", "al\u00b7le", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NE", "$,", "PWAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Will einen Raben gerne haben.", "tokens": ["Will", "ei\u00b7nen", "Ra\u00b7ben", "ger\u00b7ne", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schon rutscht er auf dem Ast daher,", "tokens": ["Schon", "rutscht", "er", "auf", "dem", "Ast", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Vogel, der mi\u00dftraut ihm sehr.", "tokens": ["Der", "Vo\u00b7gel", ",", "der", "mi\u00df\u00b7traut", "ihm", "sehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schlapp! macht der Fritz von seiner Kappe", "tokens": ["Schlapp", "!", "macht", "der", "Fritz", "von", "sei\u00b7ner", "Kap\u00b7pe"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "VVFIN", "ART", "NE", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Listen eine Vogelklappe.", "tokens": ["Mit", "Lis\u00b7ten", "ei\u00b7ne", "Vo\u00b7gel\u00b7klap\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Beinahe h\u00e4tt' er ihn! \u2013 Doch ach!", "tokens": ["Bei\u00b7na\u00b7he", "h\u00e4tt'", "er", "ihn", "!", "\u2013", "Doch", "ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$.", "$(", "KON", "XY", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Ast zerbricht mit einem Krach.", "tokens": ["Der", "Ast", "zer\u00b7bricht", "mit", "ei\u00b7nem", "Krach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "In schwarzen Beeren sitzt der Fritze,", "tokens": ["In", "schwar\u00b7zen", "Bee\u00b7ren", "sitzt", "der", "Frit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der schwarze Vogel in der M\u00fctze.", "tokens": ["Der", "schwar\u00b7ze", "Vo\u00b7gel", "in", "der", "M\u00fct\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Knabe Fritz ist schwarz betupft;", "tokens": ["Der", "Kna\u00b7be", "Fritz", "ist", "schwarz", "be\u00b7tupft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Rabe ist in Angst und hupft.", "tokens": ["Der", "Ra\u00b7be", "ist", "in", "Angst", "und", "hupft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der schwarze Vogel ist gefangen,", "tokens": ["Der", "schwar\u00b7ze", "Vo\u00b7gel", "ist", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er bleibt im Unterfutter hangen.", "tokens": ["Er", "bleibt", "im", "Un\u00b7ter\u00b7fut\u00b7ter", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "\u00bbjetzt hab' ich dich, Hans Huckebein,", "tokens": ["\u00bb", "jetzt", "hab'", "ich", "dich", ",", "Hans", "Huc\u00b7ke\u00b7bein", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "PRF", "$,", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie wird sich Tante Lotte freun!\u00ab", "tokens": ["Wie", "wird", "sich", "Tan\u00b7te", "Lot\u00b7te", "freun", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PRF", "NN", "NE", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Die Tante kommt aus ihrer T\u00fcr;", "tokens": ["Die", "Tan\u00b7te", "kommt", "aus", "ih\u00b7rer", "T\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbei!\u00ab \u2013 spricht sie \u2013 \u00bbwelch ein gutes Tier!\u00ab", "tokens": ["\u00bb", "ei", "!", "\u00ab", "\u2013", "spricht", "sie", "\u2013", "\u00bb", "welch", "ein", "gu\u00b7tes", "Tier", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$.", "$(", "$(", "VVFIN", "PPER", "$(", "$(", "PWAT", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Kaum ist das Wort dem Mund entflohn,", "tokens": ["Kaum", "ist", "das", "Wort", "dem", "Mund", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schnapp! hat er ihren Finger schon.", "tokens": ["Schnapp", "!", "hat", "er", "ih\u00b7ren", "Fin\u00b7ger", "schon", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "\u00bbach!\u00ab \u2013 ruft sie \u2013 \u00bber ist doch nicht gut!", "tokens": ["\u00bb", "ach", "!", "\u00ab", "\u2013", "ruft", "sie", "\u2013", "\u00bb", "er", "ist", "doch", "nicht", "gut", "!"], "token_info": ["punct", "word", "punct", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$.", "$(", "$(", "VVFIN", "PPER", "$(", "$(", "PPER", "VAFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Weil er mir was zuleide tut!!\u00ab", "tokens": ["Weil", "er", "mir", "was", "zu\u00b7lei\u00b7de", "tut", "!!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIS", "VVFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Hier lauert in des Topfes H\u00f6hle", "tokens": ["Hier", "lau\u00b7ert", "in", "des", "Top\u00b7fes", "H\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hans Huckebein, die schwarze Seele.", "tokens": ["Hans", "Huc\u00b7ke\u00b7bein", ",", "die", "schwar\u00b7ze", "See\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Den Knochen, den er Spitz gestohlen,", "tokens": ["Den", "Kno\u00b7chen", ",", "den", "er", "Spitz", "ge\u00b7stoh\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Will dieser jetzt sich wiederholen.", "tokens": ["Will", "die\u00b7ser", "jetzt", "sich", "wie\u00b7der\u00b7ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Sie ziehn mit Knurren und Gekr\u00e4chz,", "tokens": ["Sie", "ziehn", "mit", "Knur\u00b7ren", "und", "Ge\u00b7kr\u00e4chz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der eine links, der andre rechts.", "tokens": ["Der", "ei\u00b7ne", "links", ",", "der", "and\u00b7re", "rechts", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "$,", "PRELS", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Schon denkt der Spitz, da\u00df er gewinnt,", "tokens": ["Schon", "denkt", "der", "Spitz", ",", "da\u00df", "er", "ge\u00b7winnt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da zwickt der Rabe ihn von hint'.", "tokens": ["Da", "zwickt", "der", "Ra\u00b7be", "ihn", "von", "hint'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "O weh! Er springt auf Spitzens Nacken,", "tokens": ["O", "weh", "!", "Er", "springt", "auf", "Spit\u00b7zens", "Na\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PPER", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um ihm die Haare auszuzwacken.", "tokens": ["Um", "ihm", "die", "Haa\u00b7re", "aus\u00b7zu\u00b7zwa\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Der Spitz, der \u00e4rgert sich bereits", "tokens": ["Der", "Spitz", ",", "der", "\u00e4r\u00b7gert", "sich", "be\u00b7reits"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und rupft den Raben seinerseits.", "tokens": ["Und", "rupft", "den", "Ra\u00b7ben", "sei\u00b7ner\u00b7seits", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Derweil springt mit dem Schinkenbein", "tokens": ["Der\u00b7weil", "springt", "mit", "dem", "Schin\u00b7ken\u00b7bein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Kater in den Topf hinein.", "tokens": ["Der", "Ka\u00b7ter", "in", "den", "Topf", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Da sitzen sie und schaun und schaun. \u2013", "tokens": ["Da", "sit\u00b7zen", "sie", "und", "schaun", "und", "schaun", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "ADJD", "KON", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Kater ist nicht sehr zu traun.", "tokens": ["Dem", "Ka\u00b7ter", "ist", "nicht", "sehr", "zu", "traun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.21": {"line.1": {"text": "Der Kater hackt den Spitz, der schreit,", "tokens": ["Der", "Ka\u00b7ter", "hackt", "den", "Spitz", ",", "der", "schreit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Rabe ist voll Freudigkeit.", "tokens": ["Der", "Ra\u00b7be", "ist", "voll", "Freu\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.22": {"line.1": {"text": "Schnell fa\u00dft er, weil der Topf nicht ganz,", "tokens": ["Schnell", "fa\u00dft", "er", ",", "weil", "der", "Topf", "nicht", "ganz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit schlauer List den Katerschwanz.", "tokens": ["Mit", "schlau\u00b7er", "List", "den", "Ka\u00b7ter\u00b7schwanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.23": {"line.1": {"text": "Es rollt der Topf. Es kr\u00fcmmt voll Quale", "tokens": ["Es", "rollt", "der", "Topf", ".", "Es", "kr\u00fcmmt", "voll", "Qua\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Katers Schweif sich zur Spirale.", "tokens": ["Des", "Ka\u00b7ters", "Schweif", "sich", "zur", "Spi\u00b7ra\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}}, "stanza.24": {"line.1": {"text": "Und Spitz und Kater fliehn im Lauf. \u2013", "tokens": ["Und", "Spitz", "und", "Ka\u00b7ter", "fliehn", "im", "Lauf", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVINF", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der gr\u00f6\u00dfte Lump bleibt obenauf!! \u2013", "tokens": ["Der", "gr\u00f6\u00df\u00b7te", "Lump", "bleibt", "o\u00b7ben\u00b7auf", "!!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.25": {"line.1": {"text": "Nichts Sch\u00f6nres gab's f\u00fcr Tante Lotte", "tokens": ["Nichts", "Sch\u00f6n\u00b7res", "gab's", "f\u00fcr", "Tan\u00b7te", "Lot\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "NN", "VVFIN", "APPR", "NN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als schwarze Heidelbeerkompotte.", "tokens": ["Als", "schwar\u00b7ze", "Hei\u00b7del\u00b7beer\u00b7kom\u00b7pot\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.26": {"line.1": {"text": "Doch Huckebein verschleudert nur", "tokens": ["Doch", "Huc\u00b7ke\u00b7bein", "ver\u00b7schleu\u00b7dert", "nur"], "token_info": ["word", "word", "word", "word"], "