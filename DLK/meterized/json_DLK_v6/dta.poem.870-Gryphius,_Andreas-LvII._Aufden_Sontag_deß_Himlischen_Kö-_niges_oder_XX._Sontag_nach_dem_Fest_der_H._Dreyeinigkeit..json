{"dta.poem.870": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "LvII.  Aufden Sontag de\u00df Himlischen K\u00f6-  \n niges/ oder  XX.  Sontag nach dem Fest der  \n H. Dreyeinigkeit.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Soelen Br\u00e4utigamm/ der du mich stets geliebt; ", "tokens": ["Mein", "Soe\u00b7len", "Br\u00e4u\u00b7ti\u00b7gamm", "/", "der", "du", "mich", "stets", "ge\u00b7liebt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$(", "PRELS", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd schon vor ewigkeit zu deiner Braut erw\u00e4hlet/", "tokens": ["Vnd", "schon", "vor", "e\u00b7wig\u00b7keit", "zu", "dei\u00b7ner", "Braut", "er\u00b7w\u00e4h\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd dich mit mir in fried v\u00f1 glauben fest verm\u00e4hlet;", "tokens": ["Vnd", "dich", "mit", "mir", "in", "fried", "v\u00f1", "glau\u00b7ben", "fest", "ver\u00b7m\u00e4h\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "APPR", "NN", "NE", "VVFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ja da ich dich mit schand vnd Lastern hoch betr\u00fcbt", "tokens": ["Ja", "da", "ich", "dich", "mit", "schand", "vnd", "Las\u00b7tern", "hoch", "be\u00b7tr\u00fcbt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "KOUS", "PPER", "PRF", "APPR", "VVFIN", "KON", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vnd S\u00fcnden Hurerey ohn vnterla\u00df ver\u00fcbt/", "tokens": ["Vnd", "S\u00fcn\u00b7den", "Hu\u00b7re\u00b7rey", "ohn", "vn\u00b7ter\u00b7la\u00df", "ver\u00b7\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich durch dein reines Blut von schulden lo\u00df gezehlet", "tokens": ["Mich", "durch", "dein", "rei\u00b7nes", "Blut", "von", "schul\u00b7den", "lo\u00df", "ge\u00b7zeh\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hilff! da\u00df: ob gleich der ", "tokens": ["Hilff", "!", "da\u00df", ":", "ob", "gleich", "der"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "KOUS", "$.", "KOUS", "ADV", "ART"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Vnd Spott/ schmach/ angst vnd todt mir zu dem Brautschatz", "tokens": ["Vnd", "Spott", "/", "schmach", "/", "angst", "vnd", "todt", "mir", "zu", "dem", "Braut\u00b7schatz"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "$(", "VVFIN", "$(", "VVPP", "KON", "ADJD", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "giebt: ", "tokens": ["giebt", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Ich vnverzagt doch steh. Hilff da\u00df kein Schwerd noch", "tokens": ["Ich", "vn\u00b7ver\u00b7zagt", "doch", "steh", ".", "Hilff", "da\u00df", "kein", "Schwerd", "noch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "$.", "NN", "KOUS", "PIAT", "NN", "ADV"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Pein ", "tokens": ["Pein"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "M\u00f6g' h\u00e4rtter als die glutt der keuschen Liebe seyn: ", "tokens": ["M\u00f6g'", "h\u00e4rt\u00b7ter", "als", "die", "glutt", "der", "keu\u00b7schen", "Lie\u00b7be", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "KOKOM", "ART", "ADJD", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bi\u00df du mein h\u00f6chster Trost mich wirst zur Hochzeit f\u00fchren:", "tokens": ["Bi\u00df", "du", "mein", "h\u00f6chs\u00b7ter", "Trost", "mich", "wirst", "zur", "Hoch\u00b7zeit", "f\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "PPER", "VAFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Da werd ich sch\u00f6n geschm\u00fcckt mich frewen f\u00fcr vnd f\u00fcr:", "tokens": ["Da", "werd", "ich", "sch\u00f6n", "ge\u00b7schm\u00fcckt", "mich", "fre\u00b7wen", "f\u00fcr", "vnd", "f\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "PPER", "VVFIN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn di\u00df was vnrem ist/ in ", "tokens": ["Wenn", "di\u00df", "was", "vn\u00b7rem", "ist", "/", "in"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PDS", "PRELS", "ADJA", "VAFIN", "$(", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "In Hunger/ hohn vnd Durst ohn vnterla\u00df wird frieren.", "tokens": ["In", "Hun\u00b7ger", "/", "hohn", "vnd", "Durst", "ohn", "vn\u00b7ter\u00b7la\u00df", "wird", "frie\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "ADJA", "KON", "NN", "APPR", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}