{"textgrid.poem.55034": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Herbstgef\u00fchl", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fetter gr\u00fcne, du Laub,", "tokens": ["Fet\u00b7ter", "gr\u00fc\u00b7ne", ",", "du", "Laub", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPER", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Am Rebengel\u00e4nder", "tokens": ["Am", "Re\u00b7ben\u00b7ge\u00b7l\u00e4n\u00b7der"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Hier mein Fenster herauf!", "tokens": ["Hier", "mein", "Fens\u00b7ter", "her\u00b7auf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Gedr\u00e4ngter quellet,", "tokens": ["Ge\u00b7dr\u00e4ng\u00b7ter", "quel\u00b7let", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Zwillingsbeeren, und reifet", "tokens": ["Zwil\u00b7lings\u00b7bee\u00b7ren", ",", "und", "rei\u00b7fet"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "KON", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Schneller und gl\u00e4nzend voller!", "tokens": ["Schnel\u00b7ler", "und", "gl\u00e4n\u00b7zend", "vol\u00b7ler", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ADJA", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Euch br\u00fctet der Mutter Sonne", "tokens": ["Euch", "br\u00fc\u00b7tet", "der", "Mut\u00b7ter", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Scheideblick; euch ums\u00e4uselt", "tokens": ["Schei\u00b7de\u00b7blick", ";", "euch", "um\u00b7s\u00e4u\u00b7selt"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "Des holden Himmels", "tokens": ["Des", "hol\u00b7den", "Him\u00b7mels"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Fruchtende F\u00fclle;", "tokens": ["Fruch\u00b7ten\u00b7de", "F\u00fcl\u00b7le", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Euch k\u00fchlet des Mondes", "tokens": ["Euch", "k\u00fch\u00b7let", "des", "Mon\u00b7des"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Freundlicher Zauberhauch,", "tokens": ["Freund\u00b7li\u00b7cher", "Zau\u00b7ber\u00b7hauch", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.13": {"text": "Und euch betauen, ach!", "tokens": ["Und", "euch", "be\u00b7tau\u00b7en", ",", "ach", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$,", "ITJ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Aus diesen Augen", "tokens": ["Aus", "die\u00b7sen", "Au\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Der ewig belebenden Liebe", "tokens": ["Der", "e\u00b7wig", "be\u00b7le\u00b7ben\u00b7den", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.16": {"text": "Vollschwellende Tr\u00e4nen.", "tokens": ["Voll\u00b7schwel\u00b7len\u00b7de", "Tr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}