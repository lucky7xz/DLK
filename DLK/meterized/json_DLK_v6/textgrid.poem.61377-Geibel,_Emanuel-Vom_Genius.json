{"textgrid.poem.61377": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Vom Genius", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt wohl, da\u00df ein ber\u00fchmter Mann", "tokens": ["Kommt", "wohl", ",", "da\u00df", "ein", "be\u00b7r\u00fchm\u00b7ter", "Mann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat seinen Fehler dann und wann,", "tokens": ["Hat", "sei\u00b7nen", "Feh\u00b7ler", "dann", "und", "wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "KON", "PWAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df er aufs Geld sich nicht versteht,", "tokens": ["Da\u00df", "er", "aufs", "Geld", "sich", "nicht", "ver\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PRF", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wirtschaft gehn l\u00e4\u00dft, wie sie geht,", "tokens": ["Die", "Wirt\u00b7schaft", "gehn", "l\u00e4\u00dft", ",", "wie", "sie", "geht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Beim Weine Zeit und Ma\u00df vergi\u00dft", "tokens": ["Beim", "Wei\u00b7ne", "Zeit", "und", "Ma\u00df", "ver\u00b7gi\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sonst tut, was nicht sauber ist.", "tokens": ["Und", "sonst", "tut", ",", "was", "nicht", "sau\u00b7ber", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "PRELS", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das alles wird nun nimmer fein,", "tokens": ["Das", "al\u00b7les", "wird", "nun", "nim\u00b7mer", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch mag man's solchem Mann verzeihn,", "tokens": ["Doch", "mag", "man's", "sol\u00b7chem", "Mann", "ver\u00b7zeihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wiewohl er ohne das auf Ehr'", "tokens": ["Wie\u00b7wohl", "er", "oh\u00b7ne", "das", "auf", "Ehr'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Einem noch zehnmal lieber w\u00e4r'.", "tokens": ["Ei\u00b7nem", "noch", "zehn\u00b7mal", "lie\u00b7ber", "w\u00e4r'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADV", "ADV", "VAFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Doch nun meint manch ein Hasenfu\u00df,", "tokens": ["Doch", "nun", "meint", "manch", "ein", "Ha\u00b7sen\u00b7fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Dreck da sitzt der Genius,", "tokens": ["Im", "Dreck", "da", "sitzt", "der", "Ge\u00b7nius", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und Unordnung und loses Wesen", "tokens": ["Und", "Un\u00b7ord\u00b7nung", "und", "lo\u00b7ses", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist so recht vom Geist erlesen;", "tokens": ["Das", "ist", "so", "recht", "vom", "Geist", "er\u00b7le\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Versucht's auch lustig hinterdrein,", "tokens": ["Ver\u00b7sucht's", "auch", "lus\u00b7tig", "hin\u00b7ter\u00b7drein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf solche Art genial zu sein,", "tokens": ["Auf", "sol\u00b7che", "Art", "ge\u00b7ni\u00b7al", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Verdirbt bei Dirnen sich das Blut,", "tokens": ["Ver\u00b7dirbt", "bei", "Dir\u00b7nen", "sich", "das", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Schlampampt, vertut sein Hab und Gut;", "tokens": ["Schlam\u00b7pampt", ",", "ver\u00b7tut", "sein", "Hab", "und", "Gut", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und weil ihm das denn ba\u00df gelingt,", "tokens": ["Und", "weil", "ihm", "das", "denn", "ba\u00df", "ge\u00b7lingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PDS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Er's bald zu Rausch und Schulden bringt,", "tokens": ["Er's", "bald", "zu", "Rausch", "und", "Schul\u00b7den", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So bl\u00e4st mein Narr die N\u00fcstern auf,", "tokens": ["So", "bl\u00e4st", "mein", "Narr", "die", "N\u00fcs\u00b7tern", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Als w\u00e4r' die Welt bei ihm zu Kauf,", "tokens": ["Als", "w\u00e4r'", "die", "Welt", "bei", "ihm", "zu", "Kauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "APPR", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und sieht jedweden Ehrenmann", "tokens": ["Und", "sieht", "jed\u00b7we\u00b7den", "Eh\u00b7ren\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "F\u00fcr einen Lumpenhund nur an.", "tokens": ["F\u00fcr", "ei\u00b7nen", "Lum\u00b7pen\u00b7hund", "nur", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch zehnfach arg wird's und verkehrt,", "tokens": ["Doch", "zehn\u00b7fach", "arg", "wird's", "und", "ver\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "VAFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn in ein Weib ", "tokens": ["Wenn", "in", "ein", "Weib"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Gleich ist ihr zu gemein das Leben,", "tokens": ["Gleich", "ist", "ihr", "zu", "ge\u00b7mein", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKA", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df immer in den Wolken schweben,", "tokens": ["Mu\u00df", "im\u00b7mer", "in", "den", "Wol\u00b7ken", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kriegt die Vapeurs und hat das Maul", "tokens": ["Kriegt", "die", "Va\u00b7peurs", "und", "hat", "das", "Maul"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "VAFIN", "ART", "NN"], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Voll Redensarten von Jean Paul,", "tokens": ["Voll", "Re\u00b7dens\u00b7ar\u00b7ten", "von", "Jean", "Paul", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Studiert den Hegel zum Zeitvertreib", "tokens": ["Stu\u00b7diert", "den", "He\u00b7gel", "zum", "Zeit\u00b7ver\u00b7treib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Und tr\u00e4gt kein reines Hemd am Leib.", "tokens": ["Und", "tr\u00e4gt", "kein", "rei\u00b7nes", "Hemd", "am", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Am Feu'r der Braten brennt zu Aschen,", "tokens": ["Am", "Feu'r", "der", "Bra\u00b7ten", "brennt", "zu", "A\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Kinder laufen ungewaschen,", "tokens": ["Die", "Kin\u00b7der", "lau\u00b7fen", "un\u00b7ge\u00b7wa\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und kommst du erst zu ihr ins Haus:", "tokens": ["Und", "kommst", "du", "erst", "zu", "ihr", "ins", "Haus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "So sieht's in keinem Saustall aus.", "tokens": ["So", "sieht's", "in", "kei\u00b7nem", "Sau\u00b7stall", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und mu\u00df ich solche Unbild sehn", "tokens": ["Und", "mu\u00df", "ich", "sol\u00b7che", "Un\u00b7bild", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem armen Genius geschehn,", "tokens": ["Dem", "ar\u00b7men", "Ge\u00b7nius", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie frech in seine schlechtsten Lappen", "tokens": ["Wie", "frech", "in", "sei\u00b7ne", "schlechts\u00b7ten", "Lap\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die eitlen Affen sich verkappen,", "tokens": ["Die", "eit\u00b7len", "Af\u00b7fen", "sich", "ver\u00b7kap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die doch \u2013 zu reden gar gelind \u2013", "tokens": ["Die", "doch", "\u2013", "zu", "re\u00b7den", "gar", "ge\u00b7lind", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$(", "PTKZU", "VVINF", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mi\u00dfratene Philister sind,", "tokens": ["Mi\u00df\u00b7ra\u00b7te\u00b7ne", "Phi\u00b7lis\u00b7ter", "sind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da seufzt mein Herz voll Ingrimm auf:", "tokens": ["Da", "seufzt", "mein", "Herz", "voll", "In\u00b7grimm", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "O Simson, Simson, steig herauf", "tokens": ["O", "Sim\u00b7son", ",", "Sim\u00b7son", ",", "steig", "her\u00b7auf"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "NE", "$,", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und fahre mit dem Eselsbacken", "tokens": ["Und", "fah\u00b7re", "mit", "dem", "E\u00b7sels\u00b7ba\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dem Volk allm\u00e4chtig in den Nacken,", "tokens": ["Dem", "Volk", "all\u00b7m\u00e4ch\u00b7tig", "in", "den", "Na\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Bis ihm die Genialit\u00e4t", "tokens": ["Bis", "ihm", "die", "Ge\u00b7ni\u00b7a\u00b7li\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "F\u00fcr heut und immerdar vergeht!", "tokens": ["F\u00fcr", "heut", "und", "im\u00b7mer\u00b7dar", "ver\u00b7geht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}