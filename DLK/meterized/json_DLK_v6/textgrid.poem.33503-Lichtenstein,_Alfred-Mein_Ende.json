{"textgrid.poem.33503": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Mein Ende", "genre": "verse", "period": "N.A.", "pub_year": 1911, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Halbe H\u00e4nde halten mein Schicksal.", "tokens": ["Hal\u00b7be", "H\u00e4n\u00b7de", "hal\u00b7ten", "mein", "Schick\u00b7sal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wo wird es sinken ...", "tokens": ["Wo", "wird", "es", "sin\u00b7ken", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Meine Schritte sind winzig wie die Schritte von Fraun.", "tokens": ["Mei\u00b7ne", "Schrit\u00b7te", "sind", "win\u00b7zig", "wie", "die", "Schrit\u00b7te", "von", "Fraun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein Abend hat alle Tr\u00e4ume verw\u00fcstet.", "tokens": ["Ein", "A\u00b7bend", "hat", "al\u00b7le", "Tr\u00e4u\u00b7me", "ver\u00b7w\u00fcs\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kein Schlaf f\u00e4llt mir ein \u2013", "tokens": ["Kein", "Schlaf", "f\u00e4llt", "mir", "ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ART", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}