{"dta.poem.8164": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Die Zeit ist hin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Und leise mehr und mehr von meiner Brust;", "tokens": ["Und", "lei\u00b7se", "mehr", "und", "mehr", "von", "mei\u00b7ner", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "KON", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich suche dich mit sanftem Druck zu fassen,", "tokens": ["Ich", "su\u00b7che", "dich", "mit", "sanf\u00b7tem", "Druck", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch f\u00fchl' ich wohl, ich mu\u00df dich gehen lassen.", "tokens": ["Doch", "f\u00fchl'", "ich", "wohl", ",", "ich", "mu\u00df", "dich", "ge\u00b7hen", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PPER", "VMFIN", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So la\u00df mich denn, bevor du weit von mir", "tokens": ["So", "la\u00df", "mich", "denn", ",", "be\u00b7vor", "du", "weit", "von", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "$,", "KOUS", "PPER", "ADJD", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Leben gehst, noch einmal danken dir;", "tokens": ["Im", "Le\u00b7ben", "gehst", ",", "noch", "ein\u00b7mal", "dan\u00b7ken", "dir", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "ADV", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und magst du nie, was rettungslos vergangen,", "tokens": ["Und", "magst", "du", "nie", ",", "was", "ret\u00b7tungs\u00b7los", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "$,", "PRELS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In schlummerlosen N\u00e4chten heim verlangen.", "tokens": ["In", "schlum\u00b7mer\u00b7lo\u00b7sen", "N\u00e4ch\u00b7ten", "heim", "ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hier steh' ich nun und schaue bang zur\u00fcck;", "tokens": ["Hier", "steh'", "ich", "nun", "und", "schau\u00b7e", "bang", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vor\u00fcber rinnt auch dieser Augenblick,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "rinnt", "auch", "die\u00b7ser", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wie viel Stunden dir und mir gegeben,", "tokens": ["Und", "wie", "viel", "Stun\u00b7den", "dir", "und", "mir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIAT", "NN", "PPER", "KON", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir werden keine mehr zusammen leben.", "tokens": ["Wir", "wer\u00b7den", "kei\u00b7ne", "mehr", "zu\u00b7sam\u00b7men", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}