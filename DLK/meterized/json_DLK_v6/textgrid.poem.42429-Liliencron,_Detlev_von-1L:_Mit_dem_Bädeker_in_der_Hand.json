{"textgrid.poem.42429": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit dem B\u00e4deker in der Hand", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit dem B\u00e4deker in der Hand", "tokens": ["Mit", "dem", "B\u00e4\u00b7de\u00b7ker", "in", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Bin ich durch eine Stadt gerannt,", "tokens": ["Bin", "ich", "durch", "ei\u00b7ne", "Stadt", "ge\u00b7rannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die weithin th\u00e4t nach allen Enden", "tokens": ["Die", "weit\u00b7hin", "th\u00e4t", "nach", "al\u00b7len", "En\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihre Stra\u00dfen und Z\u00fcge senden.", "tokens": ["Ih\u00b7re", "Stra\u00b7\u00dfen", "und", "Z\u00fc\u00b7ge", "sen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Auf den Zeilen und M\u00e4rkten und Pl\u00e4tzen und Br\u00fccken", "tokens": ["Auf", "den", "Zei\u00b7len", "und", "M\u00e4rk\u00b7ten", "und", "Pl\u00e4t\u00b7zen", "und", "Br\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "NN", "KON", "NN", "KON", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Konnt' ich mich kaum durch die Menge dr\u00fccken.", "tokens": ["Konnt'", "ich", "mich", "kaum", "durch", "die", "Men\u00b7ge", "dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "In all dem Gew\u00fchl, in all dem Gesuche", "tokens": ["In", "all", "dem", "Ge\u00b7w\u00fchl", ",", "in", "all", "dem", "Ge\u00b7su\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ART", "NN", "$,", "APPR", "PIAT", "ART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Lief ich umher mit dem roten Buche,", "tokens": ["Lief", "ich", "um\u00b7her", "mit", "dem", "ro\u00b7ten", "Bu\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.9": {"text": "Bis ich mich unter dem hei\u00dfen Himmel", "tokens": ["Bis", "ich", "mich", "un\u00b7ter", "dem", "hei\u00b7\u00dfen", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "G\u00e4nzlich verlor im Volksgewimmel:", "tokens": ["G\u00e4nz\u00b7lich", "ver\u00b7lor", "im", "Volks\u00b7ge\u00b7wim\u00b7mel", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Karroussel und Affentheater,", "tokens": ["Kar\u00b7rous\u00b7sel", "und", "Af\u00b7fen\u00b7the\u00b7a\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "Pudeldressur und gelehrte Kater,", "tokens": ["Pu\u00b7del\u00b7dres\u00b7sur", "und", "ge\u00b7lehr\u00b7te", "Ka\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Seilt\u00e4nzer, Schie\u00dfbuden, Mechanik,", "tokens": ["Seil\u00b7t\u00e4n\u00b7zer", ",", "Schie\u00df\u00b7bu\u00b7den", ",", "Me\u00b7cha\u00b7nik", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Orgelgedreh, Musikantengequiek:", "tokens": ["Or\u00b7gel\u00b7ge\u00b7dreh", ",", "Mu\u00b7si\u00b7kan\u00b7ten\u00b7ge\u00b7qui\u00b7ek", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Gelangweilt halb, halb angeregt,", "tokens": ["Ge\u00b7lang\u00b7weilt", "halb", ",", "halb", "an\u00b7ge\u00b7regt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Hab' ich mich im Gewoge bewegt,", "tokens": ["Hab'", "ich", "mich", "im", "Ge\u00b7wo\u00b7ge", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.17": {"text": "Lie\u00df mich treiben und lie\u00df mich schieben,", "tokens": ["Lie\u00df", "mich", "trei\u00b7ben", "und", "lie\u00df", "mich", "schie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "KON", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "Bin hier gegangen und dort geblieben,", "tokens": ["Bin", "hier", "ge\u00b7gan\u00b7gen", "und", "dort", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "KON", "ADV", "VVPP", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.19": {"text": "Und war endlich zufrieden und froh,", "tokens": ["Und", "war", "end\u00b7lich", "zu\u00b7frie\u00b7den", "und", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.20": {"text": "Als ich dem Zetermordio", "tokens": ["Als", "ich", "dem", "Ze\u00b7ter\u00b7mor\u00b7dio"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "Den R\u00fccken wandte; doch halt noch einmal,", "tokens": ["Den", "R\u00fc\u00b7cken", "wand\u00b7te", ";", "doch", "halt", "noch", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Da gab's noch einen Zaubersaal.", "tokens": ["Da", "gab's", "noch", "ei\u00b7nen", "Zau\u00b7ber\u00b7saal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Sehns\u00fcchtig harrten der Kinder viel", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "harr\u00b7ten", "der", "Kin\u00b7der", "viel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADV"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.24": {"text": "Und s\u00e4hen zu gerne das Puppenspiel.", "tokens": ["Und", "s\u00e4\u00b7hen", "zu", "ger\u00b7ne", "das", "Pup\u00b7pen\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADV", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.25": {"text": "Und wie's so geht, ich l\u00fcfte mein Geld,", "tokens": ["Und", "wie's", "so", "geht", ",", "ich", "l\u00fcf\u00b7te", "mein", "Geld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.26": {"text": "Und lie\u00df sie hinein in die Wunderwelt.", "tokens": ["Und", "lie\u00df", "sie", "hin\u00b7ein", "in", "die", "Wun\u00b7der\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.27": {"text": "Das war ein Jauchzen und war ein Jubel,", "tokens": ["Das", "war", "ein", "Jauch\u00b7zen", "und", "war", "ein", "Ju\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Nur eine wagt es nicht im Trubel", "tokens": ["Nur", "ei\u00b7ne", "wagt", "es", "nicht", "im", "Tru\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Mit zu dr\u00e4ngen im polternden Heer,", "tokens": ["Mit", "zu", "dr\u00e4n\u00b7gen", "im", "pol\u00b7tern\u00b7den", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PTKZU", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.30": {"text": "Und auch, sie war ein Kind nicht mehr:", "tokens": ["Und", "auch", ",", "sie", "war", "ein", "Kind", "nicht", "mehr", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "VAFIN", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Ein M\u00e4del von siebenzehn, achtzehn Jahren,", "tokens": ["Ein", "M\u00e4\u00b7del", "von", "sie\u00b7ben\u00b7zehn", ",", "acht\u00b7zehn", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "CARD", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.32": {"text": "Mit braunen Augen und blonden Haaren,", "tokens": ["Mit", "brau\u00b7nen", "Au\u00b7gen", "und", "blon\u00b7den", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.33": {"text": "Die sandte mir Blicke, ich w\u00e4r' nicht von Stein,", "tokens": ["Die", "sand\u00b7te", "mir", "Bli\u00b7cke", ",", "ich", "w\u00e4r'", "nicht", "von", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Ob sie nicht auch dabei k\u00f6nnte sein.", "tokens": ["Ob", "sie", "nicht", "auch", "da\u00b7bei", "k\u00f6nn\u00b7te", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "PAV", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.35": {"text": "Gewi\u00df, nur zu, und ich geh' mit,", "tokens": ["Ge\u00b7wi\u00df", ",", "nur", "zu", ",", "und", "ich", "geh'", "mit", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Und halte mit ihr den gleichen Schritt.", "tokens": ["Und", "hal\u00b7te", "mit", "ihr", "den", "glei\u00b7chen", "Schritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.37": {"text": "Und w\u00e4hrend sich zeigten Adam und Eva,", "tokens": ["Und", "w\u00e4h\u00b7rend", "sich", "zeig\u00b7ten", "A\u00b7dam", "und", "E\u00b7va", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-++-", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "Hanswurst, der Pfalzgraf und Genofeva,", "tokens": ["Hans\u00b7wurst", ",", "der", "Pfalz\u00b7graf", "und", "Ge\u00b7no\u00b7fe\u00b7va", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Blieb fast stetig mein Auge hangen", "tokens": ["Blieb", "fast", "ste\u00b7tig", "mein", "Au\u00b7ge", "han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD", "PPOSAT", "NN", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.40": {"text": "An meiner Nachbarin weichen Wangen.", "tokens": ["An", "mei\u00b7ner", "Nach\u00b7ba\u00b7rin", "wei\u00b7chen", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.41": {"text": "Wie zart und bla\u00df war ihr Gesicht,", "tokens": ["Wie", "zart", "und", "bla\u00df", "war", "ihr", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Die hat im Leben viel Freude nicht.", "tokens": ["Die", "hat", "im", "Le\u00b7ben", "viel", "Freu\u00b7de", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.43": {"text": "Einen Hauch schon entdeckt' ich der t\u00e4glichen Sorgen,", "tokens": ["Ei\u00b7nen", "Hauch", "schon", "ent\u00b7deckt'", "ich", "der", "t\u00e4g\u00b7li\u00b7chen", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.44": {"text": "Den die Angst ihr gab vor dem n\u00e4chsten Morgen.", "tokens": ["Den", "die", "Angst", "ihr", "gab", "vor", "dem", "n\u00e4chs\u00b7ten", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.45": {"text": "Sie aber f\u00fchlte nicht meine Gedanken,", "tokens": ["Sie", "a\u00b7ber", "f\u00fchl\u00b7te", "nicht", "mei\u00b7ne", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.46": {"text": "Sah auf der B\u00fchne dem Keifen und Zanken", "tokens": ["Sah", "auf", "der", "B\u00fch\u00b7ne", "dem", "Kei\u00b7fen", "und", "Zan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "KON", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.47": {"text": "Voll Neugier zu und lacht und klatscht,", "tokens": ["Voll", "Neu\u00b7gier", "zu", "und", "lacht", "und", "klatscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PTKVZ", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Wenn Kasperl den b\u00f6sen Teufel klabatscht.", "tokens": ["Wenn", "Kas\u00b7perl", "den", "b\u00f6\u00b7sen", "Teu\u00b7fel", "kla\u00b7batscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.49": {"text": "Der Vorhang f\u00e4llt, das St\u00fcck ist aus,", "tokens": ["Der", "Vor\u00b7hang", "f\u00e4llt", ",", "das", "St\u00fcck", "ist", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Und alles str\u00f6mt in's Freie hinaus.", "tokens": ["Und", "al\u00b7les", "str\u00f6mt", "in's", "Frei\u00b7e", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"},