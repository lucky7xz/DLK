{"textgrid.poem.53408": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich h\u00f6rte, spricht Johannes, ein Geschrey,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00f6rte, spricht Johannes, ein Geschrey,", "tokens": ["Ich", "h\u00f6r\u00b7te", ",", "spricht", "Jo\u00b7han\u00b7nes", ",", "ein", "Ge\u00b7schrey", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als eine Stimm der Engel vielerley,", "tokens": ["Als", "ei\u00b7ne", "Stimm", "der", "En\u00b7gel", "vie\u00b7ler\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die umb den Stul, das Thier, die \u00c4ltsten stunden,", "tokens": ["Die", "umb", "den", "Stul", ",", "das", "Thier", ",", "die", "\u00c4lts\u00b7ten", "stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vnd ihre Zahl ward unzehlbar gefunden,", "tokens": ["Vnd", "ih\u00b7re", "Zahl", "ward", "un\u00b7zehl\u00b7bar", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die sungen so mit vollen Stimmen fort:", "tokens": ["Die", "sun\u00b7gen", "so", "mit", "vol\u00b7len", "Stim\u00b7men", "fort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das Lamb, an dem ver\u00fcbt ist Tod und Mord,", "tokens": ["Das", "Lamb", ",", "an", "dem", "ver\u00b7\u00fcbt", "ist", "Tod", "und", "Mord", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "VVPP", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ist einig werth Krafft, Reichthumb, Wei\u00dfheit, Prangen,", "tokens": ["Ist", "ei\u00b7nig", "werth", "Krafft", ",", "Reicht\u00b7humb", ",", "Wei\u00df\u00b7heit", ",", "Pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Lob, St\u00e4rcke, Prei\u00df und Ehre zu empfangen,", "tokens": ["Lob", ",", "St\u00e4r\u00b7cke", ",", "Prei\u00df", "und", "Eh\u00b7re", "zu", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Vnd was Gesch\u00f6pff bewohnt des Himmels Feld,", "tokens": ["Vnd", "was", "Ge\u00b7sch\u00f6pff", "be\u00b7wohnt", "des", "Him\u00b7mels", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Was umb den Krei\u00df der Erden sich enth\u00e4lt,", "tokens": ["Was", "umb", "den", "Krei\u00df", "der", "Er\u00b7den", "sich", "ent\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Mehr welches steckt tieff in der Erden Gr\u00fcnden,", "tokens": ["Mehr", "wel\u00b7ches", "steckt", "tieff", "in", "der", "Er\u00b7den", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "VVFIN", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Das Meer und was darinn sich kan befinden.", "tokens": ["Das", "Meer", "und", "was", "da\u00b7rinn", "sich", "kan", "be\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PWS", "PAV", "PRF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Das alles h\u00f6rt ich sagen solcher Arth", "tokens": ["Das", "al\u00b7les", "h\u00f6rt", "ich", "sa\u00b7gen", "sol\u00b7cher", "Ar\u00b7th"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Zu dem, der auff dem Stul gesehen ward,", "tokens": ["Zu", "dem", ",", "der", "auff", "dem", "Stul", "ge\u00b7se\u00b7hen", "ward", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "APPR", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Vnd zu dem Lamm: Lob, Prei\u00df und Ehren-Nahmen", "tokens": ["Vnd", "zu", "dem", "Lamm", ":", "Lob", ",", "Prei\u00df", "und", "Eh\u00b7ren\u00b7Nah\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$.", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Von Ewigkeit zu Ewigkeit hin Amen.", "tokens": ["Von", "E\u00b7wig\u00b7keit", "zu", "E\u00b7wig\u00b7keit", "hin", "A\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sey, spricht der Herr: getrost in deinem Sinn,", "tokens": ["Sey", ",", "spricht", "der", "Herr", ":", "ge\u00b7trost", "in", "dei\u00b7nem", "Sinn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "VVFIN", "ART", "NN", "$.", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vnd wi\u00df da\u00df ich der Erst und Letzte bin,", "tokens": ["Vnd", "wi\u00df", "da\u00df", "ich", "der", "Erst", "und", "Letz\u00b7te", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "PPER", "ART", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Lebendig. Ich liesse mich ermorden,", "tokens": ["Der", "Le\u00b7ben\u00b7dig", ".", "Ich", "lies\u00b7se", "mich", "er\u00b7mor\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$.", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vnd sieh ich bin nun wieder lebend worden,", "tokens": ["Vnd", "sieh", "ich", "bin", "nun", "wie\u00b7der", "le\u00b7bend", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vnd sterb hinfort in Ewigkeit nicht mehr,", "tokens": ["Vnd", "sterb", "hin\u00b7fort", "in", "E\u00b7wig\u00b7keit", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mir ist vertraut des Sieges Pracht und Ehr,", "tokens": ["Mir", "ist", "ver\u00b7traut", "des", "Sie\u00b7ges", "Pracht", "und", "Ehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Schl\u00fcssel-Macht des Todes und der Hellen,", "tokens": ["Die", "Schl\u00fcs\u00b7sel\u00b7Macht", "des", "To\u00b7des", "und", "der", "Hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die meinem Fu\u00df sich dienstbar unterstellen.", "tokens": ["Die", "mei\u00b7nem", "Fu\u00df", "sich", "dienst\u00b7bar", "un\u00b7ter\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Vnd siehe da ein jegliches der Thier,", "tokens": ["Vnd", "sie\u00b7he", "da", "ein", "jeg\u00b7li\u00b7ches", "der", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der waren, spricht Johannes ferner, vier,", "tokens": ["Der", "wa\u00b7ren", ",", "spricht", "Jo\u00b7han\u00b7nes", "fer\u00b7ner", ",", "vier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "VVFIN", "NE", "ADV", "$,", "CARD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Das hatt' umbher sechs Fl\u00fcgel, und von innen", "tokens": ["Das", "hatt'", "um\u00b7bher", "sechs", "Fl\u00fc\u00b7gel", ",", "und", "von", "in\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "CARD", "NN", "$,", "KON", "APPR", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Der Augen viel. Sie kunten nicht gewinnen,", "tokens": ["Der", "Au\u00b7gen", "viel", ".", "Sie", "kun\u00b7ten", "nicht", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Bey Tagen, nicht bey N\u00e4chten Ruh und Frist,", "tokens": ["Bey", "Ta\u00b7gen", ",", "nicht", "bey", "N\u00e4ch\u00b7ten", "Ruh", "und", "Frist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PTKNEG", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Vnd sprachen: Heilig, Heilig, Heilig ist", "tokens": ["Vnd", "spra\u00b7chen", ":", "Hei\u00b7lig", ",", "Hei\u00b7lig", ",", "Hei\u00b7lig", "ist"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$.", "ADJA", "$,", "ADJA", "$,", "ADJA", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Gott unser Herr, durch welchen alles webet,", "tokens": ["Gott", "un\u00b7ser", "Herr", ",", "durch", "wel\u00b7chen", "al\u00b7les", "we\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "APPR", "PWAT", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Der welcher war und ist und ewig lebet.", "tokens": ["Der", "wel\u00b7cher", "war", "und", "ist", "und", "e\u00b7wig", "le\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PWAT", "VAFIN", "KON", "VAFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vnd siehe, sagt Johannes: eine Schar", "tokens": ["Vnd", "sie\u00b7he", ",", "sagt", "Jo\u00b7han\u00b7nes", ":", "ei\u00b7ne", "Schar"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVIMP", "$,", "VVFIN", "NE", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von grosser Zahl die umb den Stul her war,", "tokens": ["Von", "gros\u00b7ser", "Zahl", "die", "umb", "den", "Stul", "her", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "APPR", "ART", "NN", "APZR", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vnd umb das Lamb, die sahe man sich frewen,", "tokens": ["Vnd", "umb", "das", "Lamb", ",", "die", "sa\u00b7he", "man", "sich", "fre\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "PIS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vnd \u00fcberlaut mit voller Stimme schreyen:", "tokens": ["Vnd", "\u00fc\u00b7berl\u00b7aut", "mit", "vol\u00b7ler", "Stim\u00b7me", "schre\u00b7yen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Heyl bleibe dem, der sitzet auff dem Thron,", "tokens": ["Heyl", "blei\u00b7be", "dem", ",", "der", "sit\u00b7zet", "auff", "dem", "Thron", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "$,", "PRELS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ihm unserm Gott, dem Lamb auch Gottes Sohn,", "tokens": ["Ihm", "un\u00b7serm", "Gott", ",", "dem", "Lamb", "auch", "Got\u00b7tes", "Sohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "ART", "NN", "ADV", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das unsre Schuld gedultig hat getragen", "tokens": ["Das", "uns\u00b7re", "Schuld", "ge\u00b7dul\u00b7tig", "hat", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "NN", "ADJD", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und sich ans Creutz unschuldig lassen schlagen.", "tokens": ["Und", "sich", "ans", "Creutz", "un\u00b7schul\u00b7dig", "las\u00b7sen", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPRART", "NN", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Vnd alles Chor der Engel fiel aus Pflicht", "tokens": ["Vnd", "al\u00b7les", "Chor", "der", "En\u00b7gel", "fiel", "aus", "Pflicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Recht vor den Stul auff ihre Angesicht", "tokens": ["Recht", "vor", "den", "Stul", "auff", "ih\u00b7re", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.11": {"text": "Und betheten den h\u00f6chsten an, den Nahmen,", "tokens": ["Und", "be\u00b7the\u00b7ten", "den", "h\u00f6chs\u00b7ten", "an", ",", "den", "Nah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "PTKVZ", "$,", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Den niemand kennt, und sprachen s\u00e4mptlich Amen,", "tokens": ["Den", "nie\u00b7mand", "kennt", ",", "und", "spra\u00b7chen", "s\u00e4mpt\u00b7lich", "A\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "KON", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Lob, Wei\u00dfheit, Ehr und St\u00e4rck und Sieg im Streit", "tokens": ["Lob", ",", "Wei\u00df\u00b7heit", ",", "Ehr", "und", "St\u00e4rck", "und", "Sieg", "im", "Streit"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "KON", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Sey unserm Gott in ewig' Ewigkeit. Amen.", "tokens": ["Sey", "un\u00b7serm", "Gott", "in", "e\u00b7wig'", "E\u00b7wig\u00b7keit", ".", "A\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}