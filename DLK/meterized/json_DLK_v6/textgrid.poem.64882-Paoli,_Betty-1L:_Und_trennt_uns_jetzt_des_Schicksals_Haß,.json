{"textgrid.poem.64882": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und trennt uns jetzt des Schicksals Ha\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und trennt uns jetzt des Schicksals Ha\u00df,", "tokens": ["Und", "trennt", "uns", "jetzt", "des", "Schick\u00b7sals", "Ha\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df fort ich ziehn allein;", "tokens": ["Mu\u00df", "fort", "ich", "ziehn", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKVZ", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So leb denn wohl, mein Freund, doch la\u00df", "tokens": ["So", "leb", "denn", "wohl", ",", "mein", "Freund", ",", "doch", "la\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "$,", "PPOSAT", "NN", "$,", "KON", "VVIMP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich nicht vergessen sein!", "tokens": ["Mich", "nicht", "ver\u00b7ges\u00b7sen", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gehst ohne mich du nun durch's Thal", "tokens": ["Gehst", "oh\u00b7ne", "mich", "du", "nun", "durch's", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und durch den frischen Wald,", "tokens": ["Und", "durch", "den", "fri\u00b7schen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo wir beim Morgensonnenstrahl", "tokens": ["Wo", "wir", "beim", "Mor\u00b7gen\u00b7son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So oft vereint gewallt;", "tokens": ["So", "oft", "ver\u00b7eint", "ge\u00b7wallt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und trittst du hin zum gr\u00fcnen Strand,", "tokens": ["Und", "trittst", "du", "hin", "zum", "gr\u00fc\u00b7nen", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der liebend uns umschlo\u00df,", "tokens": ["Der", "lie\u00b7bend", "uns", "um\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hin, wo auf meine bleiche Hand", "tokens": ["Hin", ",", "wo", "auf", "mei\u00b7ne", "blei\u00b7che", "Hand"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hei\u00df deine Thr\u00e4ne flo\u00df:", "tokens": ["Hei\u00df", "dei\u00b7ne", "Thr\u00e4\u00b7ne", "flo\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dann schenke, wenn's so kommen mu\u00df,", "tokens": ["Dann", "schen\u00b7ke", ",", "wenn's", "so", "kom\u00b7men", "mu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich ersetzt bei dir,", "tokens": ["Da\u00df", "ich", "er\u00b7setzt", "bei", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Andern jeden Liebesgru\u00df,", "tokens": ["Den", "An\u00b7dern", "je\u00b7den", "Lie\u00b7bes\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch einen Seufzer mir!", "tokens": ["Doch", "ei\u00b7nen", "Seuf\u00b7zer", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}