{"dta.poem.2133": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Herbst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die, in dem gro\u00dfen Wunderzirkel, das Jahr, und", "tokens": ["Die", ",", "in", "dem", "gro\u00b7\u00dfen", "Wun\u00b7der\u00b7zir\u00b7kel", ",", "das", "Jahr", ",", "und"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "KON"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "dadurch, unsre Welt,", "tokens": ["da\u00b7durch", ",", "uns\u00b7re", "Welt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PAV", "$,", "PPOSAT", "NN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "Auf eine, nicht verneinliche, vern\u00fcnftigm\u00e4chtge Weis\u2019", "tokens": ["Auf", "ei\u00b7ne", ",", "nicht", "ver\u00b7nein\u00b7li\u00b7che", ",", "ver\u00b7n\u00fcnf\u00b7tig\u00b7m\u00e4cht\u00b7ge", "Weis'"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "$,", "PTKNEG", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "erhalt!", "tokens": ["er\u00b7halt", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Sey inniglich daf\u00fcr gepriesen, da\u00df du Gesch\u00f6pfe werden", "tokens": ["Sey", "in\u00b7nig\u00b7lich", "da\u00b7f\u00fcr", "ge\u00b7prie\u00b7sen", ",", "da\u00df", "du", "Ge\u00b7sch\u00f6p\u00b7fe", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "PAV", "VVPP", "$,", "KOUS", "PPER", "NN", "VAINF"], "meter": "+--+-+-+--+-+-+-", "measure": "iambic.septa.invert"}, "line.6": {"text": "lassen,", "tokens": ["las\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Die, da\u00df hierinn von deinem Wesen ein Stral verbor-", "tokens": ["Die", ",", "da\u00df", "hie\u00b7rinn", "von", "dei\u00b7nem", "We\u00b7sen", "ein", "Stral", "ver\u00b7bor"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "gen, k\u00f6nnen fassen.", "tokens": ["gen", ",", "k\u00f6n\u00b7nen", "fas\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Gieb, da\u00df, mit froher Dankbarkeit, sie dich, in diesem", "tokens": ["Gieb", ",", "da\u00df", ",", "mit", "fro\u00b7her", "Dank\u00b7bar\u00b7keit", ",", "sie", "dich", ",", "in", "die\u00b7sem"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "$,", "KOUS", "$,", "APPR", "ADJA", "NN", "$,", "PPER", "PPER", "$,", "APPR", "PDAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "deinen Segen,", "tokens": ["dei\u00b7nen", "Se\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Erkennen, dich bewundernd loben, bekennen und ver-", "tokens": ["Er\u00b7ken\u00b7nen", ",", "dich", "be\u00b7wun\u00b7dernd", "lo\u00b7ben", ",", "be\u00b7ken\u00b7nen", "und", "ver"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRF", "ADJD", "VVINF", "$,", "VVFIN", "KON", "TRUNC"], "meter": "-+-+-+-+--+-++", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "ehren m\u00f6gen!", "tokens": ["eh\u00b7ren", "m\u00f6\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "La\u00df ihr, in dieser frohen Zeit durch Dank und Lust erreg-", "tokens": ["La\u00df", "ihr", ",", "in", "die\u00b7ser", "fro\u00b7hen", "Zeit", "durch", "Dank", "und", "Lust", "er\u00b7reg"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "APPR", "PDAT", "ADJA", "NN", "APPR", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "tes, Lallen,", "tokens": ["tes", ",", "Lal\u00b7len", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJA", "$,", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Der Ton durch dich vergn\u00fcgter Kinder, dir, ew\u2019gem", "tokens": ["Der", "Ton", "durch", "dich", "ver\u00b7gn\u00fcg\u00b7ter", "Kin\u00b7der", ",", "dir", ",", "ew'\u00b7gem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "APPR", "PPER", "ADJA", "NN", "$,", "PPER", "$,", "ADJA"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Vater, doch gefallen!", "tokens": ["Va\u00b7ter", ",", "doch", "ge\u00b7fal\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}