{"textgrid.poem.42637": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn dich mein Ohr den Schwanenarm,", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn dich mein Ohr den Schwanenarm,", "tokens": ["Wenn", "dich", "mein", "Ohr", "den", "Schwa\u00b7nen\u00b7arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Rosenhals Amynts, Nadine, preisen h\u00f6ret,", "tokens": ["Den", "Ro\u00b7sen\u00b7hals", "A\u00b7mynts", ",", "Na\u00b7di\u00b7ne", ",", "prei\u00b7sen", "h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "NN", "$,", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie f\u00fchl' ich dann mit bangem Harm", "tokens": ["Wie", "f\u00fchl'", "ich", "dann", "mit", "ban\u00b7gem", "Harm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von reger Eifersucht mein pochend Herz emp\u00f6ret!", "tokens": ["Von", "re\u00b7ger", "Ei\u00b7fer\u00b7sucht", "mein", "po\u00b7chend", "Herz", "em\u00b7p\u00f6\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Farbe weicht, es starrt mein Blut:", "tokens": ["Die", "Far\u00b7be", "weicht", ",", "es", "starrt", "mein", "Blut", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Manch Tr\u00f6pfchen, das geheim zur Wange niedergleitet,", "tokens": ["Manch", "Tr\u00f6pf\u00b7chen", ",", "das", "ge\u00b7heim", "zur", "Wan\u00b7ge", "nie\u00b7derg\u00b7lei\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verr\u00e4th in mir die wilde Glut,", "tokens": ["Ver\u00b7r\u00e4\u00b7th", "in", "mir", "die", "wil\u00b7de", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die, langsam nagend, sich bis in das Mark verbreitet.", "tokens": ["Die", ",", "lang\u00b7sam", "na\u00b7gend", ",", "sich", "bis", "in", "das", "Mark", "ver\u00b7brei\u00b7tet", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "VVPP", "$,", "PRF", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich brenne, wenn, vom trunknen Streit", "tokens": ["Ich", "bren\u00b7ne", ",", "wenn", ",", "vom", "trunk\u00b7nen", "Streit"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entstellt, dein Busen oft unb\u00e4ndig sich beweget,", "tokens": ["Ent\u00b7stellt", ",", "dein", "Bu\u00b7sen", "oft", "un\u00b7b\u00e4n\u00b7dig", "sich", "be\u00b7we\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "ADV", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und mit verw\u00e4gner L\u00fcsternheit", "tokens": ["Und", "mit", "ver\u00b7w\u00e4g\u00b7ner", "L\u00fcs\u00b7tern\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Satyr einen Kuss dir auf die Lippe pr\u00e4get.", "tokens": ["Der", "Sa\u00b7tyr", "ei\u00b7nen", "Kuss", "dir", "auf", "die", "Lip\u00b7pe", "pr\u00e4\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "O traue nicht der Leidenschaft", "tokens": ["O", "trau\u00b7e", "nicht", "der", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Fl\u00fcchtlings, dessen Herz ein dauernd B\u00fcndniss scheuet,", "tokens": ["Des", "Fl\u00fccht\u00b7lings", ",", "des\u00b7sen", "Herz", "ein", "dau\u00b7ernd", "B\u00fcnd\u00b7niss", "scheu\u00b7et", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und der den Kuss, den in den Saft", "tokens": ["Und", "der", "den", "Kuss", ",", "den", "in", "den", "Saft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Nektars Cypris taucht, so faunenhaft entweihet!", "tokens": ["Des", "Nek\u00b7tars", "Cyp\u00b7ris", "taucht", ",", "so", "fau\u00b7nen\u00b7haft", "ent\u00b7wei\u00b7het", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$,", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Begl\u00fccktes Paar, das Unbestand", "tokens": ["Be\u00b7gl\u00fcck\u00b7tes", "Paar", ",", "das", "Un\u00b7be\u00b7stand"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Leichtsinn nie entzweyt, das Hadersucht nicht kennet,", "tokens": ["Und", "Leicht\u00b7sinn", "nie", "ent\u00b7zweyt", ",", "das", "Ha\u00b7der\u00b7sucht", "nicht", "ken\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVFIN", "$,", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und dessen festes Liebesband", "tokens": ["Und", "des\u00b7sen", "fes\u00b7tes", "Lie\u00b7bes\u00b7band"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gott Amor erst am Rand des Schattenreiches trennet!", "tokens": ["Gott", "A\u00b7mor", "erst", "am", "Rand", "des", "Schat\u00b7ten\u00b7rei\u00b7ches", "tren\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}