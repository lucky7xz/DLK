{"dta.poem.7575": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "74.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kann jeder doch die Welt nur seinem Sinn anpassen;", "tokens": ["Kann", "je\u00b7der", "doch", "die", "Welt", "nur", "sei\u00b7nem", "Sinn", "an\u00b7pas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "ART", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was ich fassen soll, mu\u00df ich in Verse fassen.", "tokens": ["Und", "was", "ich", "fas\u00b7sen", "soll", ",", "mu\u00df", "ich", "in", "Ver\u00b7se", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVINF", "VMFIN", "$,", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Drum, ob an manchem Vers von mir du habest nichts,", "tokens": ["Drum", ",", "ob", "an", "man\u00b7chem", "Vers", "von", "mir", "du", "ha\u00b7best", "nichts", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "APPR", "PIAT", "NN", "APPR", "PPER", "PPER", "VAFIN", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So denk: Den hat f\u00fcr sich der Meister des Gedichts.", "tokens": ["So", "denk", ":", "Den", "hat", "f\u00fcr", "sich", "der", "Meis\u00b7ter", "des", "Ge\u00b7dichts", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "NE", "VAFIN", "APPR", "PRF", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "H\u00e4tt' ich den Vers, an dem du nichts hast, nicht gemacht,", "tokens": ["H\u00e4tt'", "ich", "den", "Vers", ",", "an", "dem", "du", "nichts", "hast", ",", "nicht", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "PIS", "VAFIN", "$,", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00e4tt' ich auch die, woran du viel hast, nicht erdacht.", "tokens": ["H\u00e4tt'", "ich", "auch", "die", ",", "wo\u00b7ran", "du", "viel", "hast", ",", "nicht", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$,", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}