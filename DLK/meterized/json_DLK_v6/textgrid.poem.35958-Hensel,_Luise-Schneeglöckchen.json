{"textgrid.poem.35958": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "Schneegl\u00f6ckchen", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, so eisig ist's im Thale,", "tokens": ["Du", ",", "so", "ei\u00b7sig", "ist's", "im", "Tha\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "ADJD", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schnee bedeckt das gr\u00fcne Moos,", "tokens": ["Schnee", "be\u00b7deckt", "das", "gr\u00fc\u00b7ne", "Moos", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die zarten Schwestern alle", "tokens": ["Und", "die", "zar\u00b7ten", "Schwes\u00b7tern", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruh'n noch in der Mutter Schoo\u00df.", "tokens": ["Ruh'n", "noch", "in", "der", "Mut\u00b7ter", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mich nur trieb ein warmes Lieben", "tokens": ["Mich", "nur", "trieb", "ein", "war\u00b7mes", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu des Tages ros'gem Licht \u2013", "tokens": ["Zu", "des", "Ta\u00b7ges", "ros'\u00b7gem", "Licht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4r ich nur daheim geblieben,", "tokens": ["W\u00e4r", "ich", "nur", "da\u00b7heim", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, da unten st\u00fcrmt es nicht! \u2013", "tokens": ["Ach", ",", "da", "un\u00b7ten", "st\u00fcrmt", "es", "nicht", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "KOUS", "ADV", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Trauernd mu\u00df zur Erde nicken", "tokens": ["Trau\u00b7ernd", "mu\u00df", "zur", "Er\u00b7de", "ni\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meines Kelches reiner Stern. \u2013", "tokens": ["Mei\u00b7nes", "Kel\u00b7ches", "rei\u00b7ner", "Stern", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber willst ", "tokens": ["A\u00b7ber", "willst"], "token_info": ["word", "word"], "pos": ["KON", "VMFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Brich mich nur, ", "tokens": ["Brich", "mich", "nur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}}}}}