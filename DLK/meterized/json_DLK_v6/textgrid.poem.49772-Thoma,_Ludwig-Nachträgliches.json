{"textgrid.poem.49772": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Nachtr\u00e4gliches", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Schlusse ist ein altes Jahr", "tokens": ["Zum", "Schlus\u00b7se", "ist", "ein", "al\u00b7tes", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verbraucht und arg verrostet.", "tokens": ["Ver\u00b7braucht", "und", "arg", "ver\u00b7ros\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Man f\u00fchlt, wie wenig sch\u00f6n es war", "tokens": ["Man", "f\u00fchlt", ",", "wie", "we\u00b7nig", "sch\u00f6n", "es", "war"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "PIS", "ADJD", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und was es uns gekostet.", "tokens": ["Und", "was", "es", "uns", "ge\u00b7kos\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Kurz vor es scheidet, liest man noch", "tokens": ["Kurz", "vor", "es", "schei\u00b7det", ",", "liest", "man", "noch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Liste seiner Toten", "tokens": ["Die", "Lis\u00b7te", "sei\u00b7ner", "To\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und denkt mit Seufzen, wie es doch", "tokens": ["Und", "denkt", "mit", "Seuf\u00b7zen", ",", "wie", "es", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "PWAV", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur Tr\u00fcbes uns geboten.", "tokens": ["Nur", "Tr\u00fc\u00b7bes", "uns", "ge\u00b7bo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das neue l\u00e4\u00dft sich anders an,", "tokens": ["Das", "neu\u00b7e", "l\u00e4\u00dft", "sich", "an\u00b7ders", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es bringt nur eitel Wonne", "tokens": ["Es", "bringt", "nur", "ei\u00b7tel", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und dem und jenem Untertan", "tokens": ["Und", "dem", "und", "je\u00b7nem", "Un\u00b7ter\u00b7tan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "KON", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Strahl der Gnadensonne.", "tokens": ["Den", "Strahl", "der", "Gna\u00b7den\u00b7son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es streut die bunten Orden aus", "tokens": ["Es", "streut", "die", "bun\u00b7ten", "Or\u00b7den", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus wohlgef\u00fcllten Taschen,", "tokens": ["Aus", "wohl\u00b7ge\u00b7f\u00fcll\u00b7ten", "Ta\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und l\u00e4\u00dft so manches alte Haus", "tokens": ["Und", "l\u00e4\u00dft", "so", "man\u00b7ches", "al\u00b7te", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Begierig danach haschen.", "tokens": ["Be\u00b7gie\u00b7rig", "da\u00b7nach", "ha\u00b7schen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Seht, wie die Treue st\u00e4rker ist", "tokens": ["Seht", ",", "wie", "die", "Treu\u00b7e", "st\u00e4r\u00b7ker", "ist"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wie sie sich verj\u00fcnget,", "tokens": ["Und", "wie", "sie", "sich", "ver\u00b7j\u00fcn\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie Spargel, den mit Pferdemist", "tokens": ["Wie", "Spar\u00b7gel", ",", "den", "mit", "Pfer\u00b7de\u00b7mist"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der weise G\u00e4rtner d\u00fcnget.", "tokens": ["Der", 