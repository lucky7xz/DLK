{"textgrid.poem.55765": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Mondeslichter rinnen", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Mondeslichter rinnen", "tokens": ["Die", "Mon\u00b7des\u00b7lich\u00b7ter", "rin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus sterndurchsprengtem Raum", "tokens": ["Aus", "stern\u00b7durch\u00b7spreng\u00b7tem", "Raum"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zur regungslosen Erde,", "tokens": ["Zur", "re\u00b7gungs\u00b7lo\u00b7sen", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die m\u00fcde atmet kaum.", "tokens": ["Die", "m\u00fc\u00b7de", "at\u00b7met", "kaum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie schlummertrunken schweigen", "tokens": ["Wie", "schlum\u00b7mer\u00b7trun\u00b7ken", "schwei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Linden rund umher,", "tokens": ["Die", "Lin\u00b7den", "rund", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Rauschens m\u00fcde, neigen", "tokens": ["Des", "Rau\u00b7schens", "m\u00fc\u00b7de", ",", "nei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herab sie bl\u00fctenschwer.", "tokens": ["Her\u00b7ab", "sie", "bl\u00fc\u00b7ten\u00b7schwer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nur manchmal, traumhaft leise,", "tokens": ["Nur", "manch\u00b7mal", ",", "traum\u00b7haft", "lei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Rauscht auf der Wipfel Lied,", "tokens": ["Rauscht", "auf", "der", "Wip\u00b7fel", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn schaurig durchs Ge\u00e4ste", "tokens": ["Wenn", "schau\u00b7rig", "durchs", "Ge\u00b7\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein k\u00fchler Nachthauch zieht.", "tokens": ["Ein", "k\u00fch\u00b7ler", "Nacht\u00b7hauch", "zieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mein Herz ist ruh-umfangen,", "tokens": ["Mein", "Herz", "ist", "ruh\u00b7um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist weltvergessen still,", "tokens": ["Ist", "welt\u00b7ver\u00b7ges\u00b7sen", "still", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Sehnen und Verlangen", "tokens": ["Kein", "Seh\u00b7nen", "und", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Brust bewegen will.", "tokens": ["Die", "Brust", "be\u00b7we\u00b7gen", "will", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nur manchmal, traumhaft leise,", "tokens": ["Nur", "manch\u00b7mal", ",", "traum\u00b7haft", "lei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durchzieht der alte Schmerz,", "tokens": ["Durch\u00b7zieht", "der", "al\u00b7te", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie Nachtwind durchs Ge\u00e4ste,", "tokens": ["Wie", "Nacht\u00b7wind", "durchs", "Ge\u00b7\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das m\u00fcdgeliebte Herz.", "tokens": ["Das", "m\u00fcd\u00b7ge\u00b7lieb\u00b7te", "Herz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}