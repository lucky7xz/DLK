ART", "ART", "NN", "$(", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gantz ersch\u00f6pfft/ die mattes sehnen", "tokens": ["Gantz", "er\u00b7sch\u00f6pfft", "/", "die", "mat\u00b7tes", "seh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$(", "ART", "ADJA", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur noch allein mit schwachem ", "tokens": ["Nur", "noch", "al\u00b7lein", "mit", "schwa\u00b7chem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die wie ein Turtelt\u00e4ublein girret:", "tokens": ["Die", "wie", "ein", "Tur\u00b7tel\u00b7t\u00e4u\u00b7blein", "gir\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sitzt/ in geschwinde lust verwirret.", "tokens": ["Sitzt", "/", "in", "ge\u00b7schwin\u00b7de", "lust", "ver\u00b7wir\u00b7ret", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie lacht/ sie jauchtzt/ sie r\u00fchmbt/ sie siegt. ", "tokens": ["Sie", "lacht", "/", "sie", "jauchtzt", "/", "sie", "r\u00fchmbt", "/", "sie", "siegt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df Thal vnd Berg davon erklingt.", "tokens": ["Da\u00df", "Thal", "vnd", "Berg", "da\u00b7von", "er\u00b7klingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Vnd billich! denn wer dieser wunder schawt:", "tokens": ["Vnd", "bil\u00b7lich", "!", "denn", "wer", "die\u00b7ser", "wun\u00b7der", "schawt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "KON", "PWS", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vnd die gantz zersprengten Bande.", "tokens": ["Vnd", "die", "gantz", "zer\u00b7spreng\u00b7ten", "Ban\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die in Ruhmb verkehrte Schande/ ", "tokens": ["Die", "in", "Ruhmb", "ver\u00b7kehr\u00b7te", "Schan\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die rawe klufft/ f\u00fcr der der ", "tokens": ["Die", "ra\u00b7we", "klufft", "/", "f\u00fcr", "der", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "APPR", "ART", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "L\u00e4st mich best\u00fcrtzung von sich h\u00f6ren:", "tokens": ["L\u00e4st", "mich", "be\u00b7st\u00fcrt\u00b7zung", "von", "sich", "h\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Wieviel hat GOTT der H", "tokens": ["Wie\u00b7viel", "hat", "GoTT", "der", "H"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "NE", "ART", "XY"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "An jhr gethan: ", "tokens": ["An", "jhr", "ge\u00b7than", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$."], "meter": "---+", "measure": "unknown.measure.single"}, "line.8": {"text": "Was weder Ost noch West verschweiget. ", "tokens": ["Was", "we\u00b7der", "Ost", "noch", "West", "ver\u00b7schwei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "KON", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja freylich spricht Sie: Thut er viel an mir", "tokens": ["Ja", "frey\u00b7lich", "spricht", "Sie", ":", "Thut", "er", "viel", "an", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Drumb soll: weil mir Blutt vnd Leben", "tokens": ["Drumb", "soll", ":", "weil", "mir", "Blutt", "vnd", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "$.", "KOUS", "PPER", "ADJD", "KON", "NN"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wird durch Hertz vnd Adern schweben", "tokens": ["Wird", "durch", "Hertz", "vnd", "A\u00b7dern", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein Mund vnd Hand/ vnd ", "tokens": ["Mein", "Mund", "vnd", "Hand", "/", "vnd"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "$(", "KON"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "HeRR rette was sich noch nicht