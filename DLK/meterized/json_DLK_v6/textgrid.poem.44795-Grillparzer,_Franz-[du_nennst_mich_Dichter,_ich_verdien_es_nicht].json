{"textgrid.poem.44795": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[du nennst mich Dichter, ich verdien es nicht]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du nennst mich Dichter, ich verdien es nicht.", "tokens": ["Du", "nennst", "mich", "Dich\u00b7ter", ",", "ich", "ver\u00b7di\u00b7en", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ein andrer sitzt, ich f\u00fchls, und schreibt mein Leben.", "tokens": ["Ein", "an\u00b7drer", "sitzt", ",", "ich", "f\u00fchls", ",", "und", "schreibt", "mein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "PPER", "ADV", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und soll die Poesie den Namen geben,", "tokens": ["Und", "soll", "die", "Poe\u00b7sie", "den", "Na\u00b7men", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Statt Dichter, f\u00fchl ich h\u00f6chstens mich Gedicht.", "tokens": ["Statt", "Dich\u00b7ter", ",", "f\u00fchl", "ich", "h\u00f6chs\u00b7tens", "mich", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "PPER", "ADV", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}