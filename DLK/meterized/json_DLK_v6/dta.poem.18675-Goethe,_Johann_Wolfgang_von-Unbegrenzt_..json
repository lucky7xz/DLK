{"dta.poem.18675": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Unbegrenzt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Dass du nicht enden kannst das macht dich gross,", "tokens": ["Dass", "du", "nicht", "en\u00b7den", "kannst", "das", "macht", "dich", "gross", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "PDS", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und dass du nie beginnst das ist dein Loos.", "tokens": ["Und", "dass", "du", "nie", "be\u00b7ginnst", "das", "ist", "dein", "Loos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dein Lied ist drehend wie das Sterngew\u00f6lbe,", "tokens": ["Dein", "Lied", "ist", "dre\u00b7hend", "wie", "das", "Stern\u00b7ge\u00b7w\u00f6l\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Anfang und Ende immer fort dasselbe,", "tokens": ["An\u00b7fang", "und", "En\u00b7de", "im\u00b7mer", "fort", "das\u00b7sel\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "PTKVZ", "PDAT", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Und was die Mitte bringt ist offenbar,", "tokens": ["Und", "was", "die", "Mit\u00b7te", "bringt", "ist", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das was zu Ende bleibt und Anfangs war.", "tokens": ["Das", "was", "zu", "En\u00b7de", "bleibt", "und", "An\u00b7fangs", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "APPR", "NN", "VVFIN", "KON", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du bist der Freuden \u00e4chte Dichterquelle,", "tokens": ["Du", "bist", "der", "Freu\u00b7den", "\u00e4ch\u00b7te", "Dich\u00b7ter\u00b7quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ungez\u00e4hlt entfliesst dir Well\u2019 auf Welle.", "tokens": ["Und", "un\u00b7ge\u00b7z\u00e4hlt", "ent\u00b7fliesst", "dir", "Well'", "auf", "Wel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum K\u00fcssen stets bereiter Mund,", "tokens": ["Zum", "K\u00fcs\u00b7sen", "stets", "be\u00b7rei\u00b7ter", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Brustgesang der lieblich fliesset,", "tokens": ["Ein", "Brust\u00b7ge\u00b7sang", "der", "lieb\u00b7lich", "flies\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Trinken stets gereizter Schlund,", "tokens": ["Zum", "Trin\u00b7ken", "stets", "ge\u00b7reiz\u00b7ter", "Schlund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein gutes Herz das sich ergiesset.", "tokens": ["Ein", "gu\u00b7tes", "Herz", "das", "sich", "er\u00b7gies\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und mag die ganze Welt versinken,", "tokens": ["Und", "mag", "die", "gan\u00b7ze", "Welt", "ver\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hafis mit dir, mit dir allein", "tokens": ["Ha\u00b7fis", "mit", "dir", ",", "mit", "dir", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPR", "PPER", "$,", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Will ich wetteifern! Lust und Pein", "tokens": ["Will", "ich", "wett\u00b7ei\u00b7fern", "!", "Lust", "und", "Pein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF", "$.", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sey uns den Zwillingen gemein!", "tokens": ["Sey", "uns", "den", "Zwil\u00b7lin\u00b7gen", "ge\u00b7mein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "++-++--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Wie du zu lieben und zu trinken", "tokens": ["Wie", "du", "zu", "lie\u00b7ben", "und", "zu", "trin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das soll mein Stolz, mein Leben seyn.", "tokens": ["Das", "soll", "mein", "Stolz", ",", "mein", "Le\u00b7ben", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun t\u00f6ne Lied mit eignem Feuer!", "tokens": ["Nun", "t\u00f6\u00b7ne", "Lied", "mit", "eig\u00b7nem", "Feu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn du bist \u00e4lter, du bist neuer.", "tokens": ["Denn", "du", "bist", "\u00e4l\u00b7ter", ",", "du", "bist", "neu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}