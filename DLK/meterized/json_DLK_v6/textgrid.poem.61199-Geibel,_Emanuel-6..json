{"textgrid.poem.61199": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwas schautest gestern du so finster drein,", "tokens": ["\u00bb", "was", "schau\u00b7test", "ge\u00b7stern", "du", "so", "fins\u00b7ter", "drein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "ADV", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da schwarz aufs Meer die Wolken niederzogen,", "tokens": ["Da", "schwarz", "aufs", "Meer", "die", "Wol\u00b7ken", "nie\u00b7der\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und kreischend vor dem Sturm die M\u00f6wen flogen,", "tokens": ["Und", "krei\u00b7schend", "vor", "dem", "Sturm", "die", "M\u00f6\u00b7wen", "flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Schwingen tauchend in den Wetterschein?", "tokens": ["Die", "Schwin\u00b7gen", "tau\u00b7chend", "in", "den", "Wet\u00b7ter\u00b7schein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mir war's, als w\u00fcrd' ich ledig jeder Pein,", "tokens": ["Mir", "wa\u00b7r's", ",", "als", "w\u00fcrd'", "ich", "le\u00b7dig", "je\u00b7der", "Pein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "PPER", "ADJD", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Und jauchzen mu\u00dft' ich ins Geroll der Wogen,", "tokens": ["Und", "jauch\u00b7zen", "mu\u00dft'", "ich", "ins", "Ge\u00b7roll", "der", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch tr\u00fcbe standest du, das Haupt gebogen -", "tokens": ["Doch", "tr\u00fc\u00b7be", "stan\u00b7dest", "du", ",", "das", "Haupt", "ge\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "VVFIN", "PPER", "$,", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was war's? Du siehst, die Luft ist wieder rein.\u00ab", "tokens": ["Was", "wa\u00b7r's", "?", "Du", "siehst", ",", "die", "Luft", "ist", "wie\u00b7der", "rein", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "$.", "PPER", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADV", "ADJD", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Nicht schelt' ich deinen ungest\u00fcmen Drang,", "tokens": ["Nicht", "schelt'", "ich", "dei\u00b7nen", "un\u00b7ge\u00b7st\u00fc\u00b7men", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem Knaben wird im Sturm die Brust erweitert,", "tokens": ["Dem", "Kna\u00b7ben", "wird", "im", "Sturm", "die", "Brust", "er\u00b7wei\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Fluten Donner deucht ihm wie Gesang;", "tokens": ["Der", "Flu\u00b7ten", "Don\u00b7ner", "deucht", "ihm", "wie", "Ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wohl hast du recht, der Himmel gl\u00e4nzt erheitert,", "tokens": ["Wohl", "hast", "du", "recht", ",", "der", "Him\u00b7mel", "gl\u00e4nzt", "er\u00b7hei\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sonne wandelt ruhig ihren Gang -", "tokens": ["Die", "Son\u00b7ne", "wan\u00b7delt", "ru\u00b7hig", "ih\u00b7ren", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch wei\u00dft du auch, wieviel heut nacht gescheitert?", "tokens": ["Doch", "wei\u00dft", "du", "auch", ",", "wie\u00b7viel", "heut", "nacht", "ge\u00b7schei\u00b7tert", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}