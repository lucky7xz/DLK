{"textgrid.poem.47429": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[es schm\u00fcckt die Herrschaft hohe Throne nicht]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schm\u00fcckt die Herrschaft hohe Throne nicht;", "tokens": ["Es", "schm\u00fcckt", "die", "Herr\u00b7schaft", "ho\u00b7he", "Thro\u00b7ne", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie wohnt, o Lieb, in deinem Angesicht.", "tokens": ["Sie", "wohnt", ",", "o", "Lieb", ",", "in", "dei\u00b7nem", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Den Tag erschafft die Sonne nicht; es tagt", "tokens": ["Den", "Tag", "er\u00b7schafft", "die", "Son\u00b7ne", "nicht", ";", "es", "tagt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch deiner Augen zauberisches Licht.", "tokens": ["Durch", "dei\u00b7ner", "Au\u00b7gen", "zau\u00b7be\u00b7ri\u00b7sches", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dir huldiget gefesselt alles Sein,", "tokens": ["Dir", "hul\u00b7di\u00b7get", "ge\u00b7fes\u00b7selt", "al\u00b7les", "Sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "PIS", "PPOSAT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und keines ist, das seine Bande bricht,", "tokens": ["Und", "kei\u00b7nes", "ist", ",", "das", "sei\u00b7ne", "Ban\u00b7de", "bricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "In deiner Hand ist Leben und Verderb,", "tokens": ["In", "dei\u00b7ner", "Hand", "ist", "Le\u00b7ben", "und", "Ver\u00b7derb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie deine Willk\u00fchr unser Urtheil spricht.", "tokens": ["Wie", "dei\u00b7ne", "Will\u00b7k\u00fchr", "un\u00b7ser", "Ur\u00b7theil", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Zwar wenig ist des Guten, das du thust,", "tokens": ["Zwar", "we\u00b7nig", "ist", "des", "Gu\u00b7ten", ",", "das", "du", "thust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des B\u00f6sen ein unendliches Gewicht;", "tokens": ["Des", "B\u00f6\u00b7sen", "ein", "un\u00b7end\u00b7li\u00b7ches", "Ge\u00b7wicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Doch morde nur und trinke Blut auf Blut!", "tokens": ["Doch", "mor\u00b7de", "nur", "und", "trin\u00b7ke", "Blut", "auf", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "KON", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht geht mit dir der Himmel in's Gericht;", "tokens": ["Nicht", "geht", "mit", "dir", "der", "Him\u00b7mel", "in's", "Ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "APPR", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Nicht schreiben Engel deine Schulden auf;", "tokens": ["Nicht", "schrei\u00b7ben", "En\u00b7gel", "dei\u00b7ne", "Schul\u00b7den", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie selbst entflammt, sie brechen ihre Pflicht.", "tokens": ["Sie", "selbst", "ent\u00b7flammt", ",", "sie", "bre\u00b7chen", "ih\u00b7re", "Pflicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}