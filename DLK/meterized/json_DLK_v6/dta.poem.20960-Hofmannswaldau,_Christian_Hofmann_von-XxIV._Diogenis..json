{"dta.poem.20960": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XxIV.  \n Diogenis.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Pracht/ Reichthum/ falscher Schein/ ward von mir", "tokens": ["Pracht", "/", "Reicht\u00b7hum", "/", "fal\u00b7scher", "Schein", "/", "ward", "von", "mir"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "NN", "$(", "ADJA", "NN", "$(", "VAFIN", "APPR", "PPER"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "au\u00dfgelacht/", "tokens": ["au\u00df\u00b7ge\u00b7lacht", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Ich habe mehr nach Ruh als grossen Ruhm getracht.", "tokens": ["Ich", "ha\u00b7be", "mehr", "nach", "Ruh", "als", "gros\u00b7sen", "Ruhm", "ge\u00b7tracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "KOUS", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Wohnhau\u00df war ein Fa\u00df/ mein Becher war", "tokens": ["Mein", "Wohn\u00b7hau\u00df", "war", "ein", "Fa\u00df", "/", "mein", "Be\u00b7cher", "war"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$(", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "die Hand/", "tokens": ["die", "Hand", "/"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Die weite Welt mein Buch/ und auch mein Vater-", "tokens": ["Die", "wei\u00b7te", "Welt", "mein", "Buch", "/", "und", "auch", "mein", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$(", "KON", "ADV", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "land.", "tokens": ["land", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-", "measure": "single.down"}}}}}