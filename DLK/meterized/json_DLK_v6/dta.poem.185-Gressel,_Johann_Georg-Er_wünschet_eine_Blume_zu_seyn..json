{"dta.poem.185": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er w\u00fcnschet eine Blume zu seyn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Florens Meister-St\u00fcck/", "tokens": ["Ihr", "Flo\u00b7rens", "Meis\u00b7ter\u00b7St\u00fcck", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Jhr angenehmen Rosen/", "tokens": ["Ihr", "an\u00b7ge\u00b7neh\u00b7men", "Ro\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die ihr das holde Gl\u00fcck", "tokens": ["Die", "ihr", "das", "hol\u00b7de", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Da\u00df ihr die weisse Haut", "tokens": ["Da\u00df", "ihr", "die", "weis\u00b7se", "Haut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Vor welcher die Zeitlosen", "tokens": ["Vor", "wel\u00b7cher", "die", "Zeit\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN"], "meter": "-+-+++-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Gleich als besch\u00e4met stehn.", "tokens": ["Gleich", "als", "be\u00b7sch\u00e4\u00b7met", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VVFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Die euch ein warmer Schnee", "tokens": ["Die", "euch", "ein", "war\u00b7mer", "Schnee"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Der zarten Br\u00fcste labt/", "tokens": ["Der", "zar\u00b7ten", "Br\u00fcs\u00b7te", "labt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Die einen ", "tokens": ["Die", "ei\u00b7nen"], "token_info": ["word", "word"], "pos": ["ART", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Zur K\u00fchlung bey sich sehn/", "tokens": ["Zur", "K\u00fch\u00b7lung", "bey", "sich", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PRF", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Seyd doch darauf bedacht/", "tokens": ["Seyd", "doch", "da\u00b7rauf", "be\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "PAV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Da\u00df gleicher Bl\u00e4tter-Pracht", "tokens": ["Da\u00df", "glei\u00b7cher", "Bl\u00e4t\u00b7ter\u00b7Pracht"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Mit euch mich h\u00fclle ein/", "tokens": ["Mit", "euch", "mich", "h\u00fcl\u00b7le", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "VVFIN", "ART", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Da\u00df ich mag bey euch seyn.", "tokens": ["Da\u00df", "ich", "mag", "bey", "euch", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Euch G\u00f6tter bitte ich!", "tokens": ["Euch", "G\u00f6t\u00b7ter", "bit\u00b7te", "ich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Verwandelt mich in Blumen", "tokens": ["Ver\u00b7wan\u00b7delt", "mich", "in", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "In kr\u00e4fftige ", "tokens": ["In", "kr\u00e4ff\u00b7ti\u00b7ge"], "token_info": ["word", "word"], "pos": ["APPR", "ADJA"], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "In riechende Violen/", "tokens": ["In", "rie\u00b7chen\u00b7de", "Vi\u00b7o\u00b7len", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "In pr\u00e4chtige ", "tokens": ["In", "pr\u00e4ch\u00b7ti\u00b7ge"], "token_info": ["word", "word"], "pos": ["APPR", "ADJA"], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "Macht fort/ verwandelt mich!", "tokens": ["Macht", "fort", "/", "ver\u00b7wan\u00b7delt", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$(", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "La\u00dft ", "tokens": ["La\u00dft"], "token_info": ["word"], "pos": ["VVIMP"], "meter": "-", "measure": "single.down"}, "line.24": {"text": "Und bey den Br\u00fcsten wissen/", "tokens": ["Und", "bey", "den", "Br\u00fcs\u00b7ten", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "So lebe ich vergn\u00fcgt.", "tokens": ["So", "le\u00b7be", "ich", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Wo nicht/ so soll mein Blut/", "tokens": ["Wo", "nicht", "/", "so", "soll", "mein", "Blut", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$(", "ADV", "VMFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.28": {"text": "Doch sch\u00f6ne Rosen zeugen/", "tokens": ["Doch", "sch\u00f6\u00b7ne", "Ro\u00b7sen", "zeu\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Die sich zu ", "tokens": ["Die", "sich", "zu"], "token_info": ["word", "word", "word"], "pos": ["ART", "PRF", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.30": {"text": "Denn bin ich auch entseelt", "tokens": ["Denn", "bin", "ich", "auch", "ent\u00b7seelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Bey ihrer Brust vergn\u00fcgt/", "tokens": ["Bey", "ih\u00b7rer", "Brust", "ver\u00b7gn\u00fcgt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.32": {"text": "Weil es das Gl\u00fccke so gef\u00fcgt/", "tokens": ["Weil", "es", "das", "Gl\u00fc\u00b7cke", "so", "ge\u00b7f\u00fcgt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Da\u00df ich das sch\u00f6ne Feld", "tokens": ["Da\u00df", "ich", "das", "sch\u00f6\u00b7ne", "Feld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.34": {"text": "Der Brust zum Blumen-Bett gekriegt.", "tokens": ["Der", "Brust", "zum", "Blu\u00b7men\u00b7Bett", "ge\u00b7kriegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}