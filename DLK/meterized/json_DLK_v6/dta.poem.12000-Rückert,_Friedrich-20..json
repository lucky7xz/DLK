{"dta.poem.12000": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Solang du lebend bist, komm halte dich ans Leben,", "tokens": ["So\u00b7lang", "du", "le\u00b7bend", "bist", ",", "komm", "hal\u00b7te", "dich", "ans", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VAFIN", "$,", "VVFIN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und la\u00df die Todten sich ab mit den Todten geben.", "tokens": ["Und", "la\u00df", "die", "Tod\u00b7ten", "sich", "ab", "mit", "den", "Tod\u00b7ten", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "PRF", "PTKVZ", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wieviele starben, doch des Lebens bleibt genug;", "tokens": ["Wie\u00b7vie\u00b7le", "star\u00b7ben", ",", "doch", "des", "Le\u00b7bens", "bleibt", "ge\u00b7nug", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADV", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie einer abtritt, folgt ein andrer Maskenzug.", "tokens": ["Wie", "ei\u00b7ner", "ab\u00b7tritt", ",", "folgt", "ein", "an\u00b7drer", "Mas\u00b7ken\u00b7zug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und tritst du selber ab, so thu's mit Lust, zufrieden,", "tokens": ["Und", "tritst", "du", "sel\u00b7ber", "ab", ",", "so", "thu's", "mit", "Lust", ",", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "ADV", "VVFIN", "APPR", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du gelebt und nicht mehr leben mu\u00dft hienieden.", "tokens": ["Da\u00df", "du", "ge\u00b7lebt", "und", "nicht", "mehr", "le\u00b7ben", "mu\u00dft", "hien\u00b7ie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "KON", "PTKNEG", "ADV", "VVINF", "VMFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}