{"dta.poem.15721": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "54.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Eigenth\u00fcmlichkeit, des Menschen sch\u00f6nste Bl\u00fcte,", "tokens": ["Die", "Ei\u00b7gent\u00b7h\u00fcm\u00b7lich\u00b7keit", ",", "des", "Men\u00b7schen", "sch\u00f6ns\u00b7te", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In seinem Thun und Seyn, im Antlitz und Gem\u00fcte;", "tokens": ["In", "sei\u00b7nem", "Thun", "und", "Seyn", ",", "im", "Ant\u00b7litz", "und", "Ge\u00b7m\u00fc\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "PPOSAT", "$,", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wodurch der Einzelne zu einem Ganzen ward,", "tokens": ["Wo\u00b7durch", "der", "Ein\u00b7zel\u00b7ne", "zu", "ei\u00b7nem", "Gan\u00b7zen", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Indes ein Thier nichts hat voraus vor seiner Art.", "tokens": ["In\u00b7des", "ein", "Thier", "nichts", "hat", "vo\u00b7raus", "vor", "sei\u00b7ner", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PIS", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch unterscheidet selbst am Thiere, was ein tr\u00e4ger", "tokens": ["Doch", "un\u00b7ter\u00b7schei\u00b7det", "selbst", "am", "Thie\u00b7re", ",", "was", "ein", "tr\u00e4\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "$,", "PRELS", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Blick unterschiedlos fand, ein Hirte, Reuter, J\u00e4ger.", "tokens": ["Blick", "un\u00b7ter\u00b7schied\u00b7los", "fand", ",", "ein", "Hir\u00b7te", ",", "Reu\u00b7ter", ",", "J\u00e4\u00b7ger", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "$,", "ART", "NN", "$,", "NE", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gez\u00e4hmte Thiere sind, wie Menschen, wechselreich,", "tokens": ["Ge\u00b7z\u00e4hm\u00b7te", "Thie\u00b7re", "sind", ",", "wie", "Men\u00b7schen", ",", "wech\u00b7sel\u00b7reich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$,", "PWAV", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Halbwilde Menschen am Gepr\u00e4g, wie Thiere, gleich.", "tokens": ["Halb\u00b7wil\u00b7de", "Men\u00b7schen", "am", "Ge\u00b7pr\u00e4g", ",", "wie", "Thie\u00b7re", ",", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,", "PWAV", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wol gibts Familiengesichter, Volksgesichter,", "tokens": ["Wol", "gibts", "Fa\u00b7mi\u00b7li\u00b7en\u00b7ge\u00b7sich\u00b7ter", ",", "Volks\u00b7ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "NN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Doch Menschenangesicht besticht allein den Richter,", "tokens": ["Doch", "Men\u00b7schen\u00b7an\u00b7ge\u00b7sicht", "be\u00b7sticht", "al\u00b7lein", "den", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und Menschenangesichts h\u00f6chster Verkl\u00e4rungstral,", "tokens": ["Und", "Men\u00b7schen\u00b7an\u00b7ge\u00b7sichts", "h\u00f6chs\u00b7ter", "Ver\u00b7kl\u00e4\u00b7rungs\u00b7tral", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Eigenth\u00fcmlichkeit Vollendung, Ideal;", "tokens": ["Der", "Ei\u00b7gent\u00b7h\u00fcm\u00b7lich\u00b7keit", "Vol\u00b7len\u00b7dung", ",", "I\u00b7deal", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Wodurch Besondres wird zur\u00fcck zur Allgemeinheit", "tokens": ["Wo\u00b7durch", "Be\u00b7sond\u00b7res", "wird", "zu\u00b7r\u00fcck", "zur", "All\u00b7ge\u00b7mein\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VAFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gebracht, und Menschliches mit G\u00f6ttlichem zur Einheit.", "tokens": ["Ge\u00b7bracht", ",", "und", "Menschli\u00b7ches", "mit", "G\u00f6tt\u00b7li\u00b7chem", "zur", "Ein\u00b7heit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "NN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}}}}