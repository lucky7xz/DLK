{"textgrid.poem.64524": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "148. Uebersetzung in Verse einer Stelle aus H\u00fcbners poetischem Handbuche", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbund reimte man nun auch nicht mehr,", "tokens": ["\u00bb", "und", "reim\u00b7te", "man", "nun", "auch", "nicht", "mehr", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PIS", "ADV", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was w\u00fcrde man sich denn ersparen?", "tokens": ["Was", "w\u00fcr\u00b7de", "man", "sich", "denn", "er\u00b7spa\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PIS", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn wo wir jetzt zufrieden Reime sehn,", "tokens": ["Denn", "wo", "wir", "jetzt", "zu\u00b7frie\u00b7den", "Rei\u00b7me", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da m\u00fc\u00dften doch alsdann Gedanken stehn.\u00ab", "tokens": ["Da", "m\u00fc\u00df\u00b7ten", "doch", "als\u00b7dann", "Ge\u00b7dan\u00b7ken", "stehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ADV", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So meinte ", "tokens": ["So", "mein\u00b7te"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Der gute Mann! lebt' er in unsern Jahren,", "tokens": ["Der", "gu\u00b7te", "Mann", "!", "lebt'", "er", "in", "un\u00b7sern", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Er w\u00fcrde tausend Dichter sehn,", "tokens": ["Er", "w\u00fcr\u00b7de", "tau\u00b7send", "Dich\u00b7ter", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Reim und Denken sich ersparen.", "tokens": ["Die", "Reim", "und", "Den\u00b7ken", "sich", "er\u00b7spa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}