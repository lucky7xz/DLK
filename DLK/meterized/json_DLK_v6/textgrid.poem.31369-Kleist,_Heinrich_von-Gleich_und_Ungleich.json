{"textgrid.poem.31369": {"metadata": {"author": {"name": "Kleist, Heinrich von", "birth": "N.A.", "death": "N.A."}, "title": "Gleich und Ungleich", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herr, als er auf Erden noch einherging,", "tokens": ["Der", "Herr", ",", "als", "er", "auf", "Er\u00b7den", "noch", "ein\u00b7her\u00b7ging", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kam mit Sankt Peter einst an einen Scheideweg,", "tokens": ["Kam", "mit", "Sankt", "Pe\u00b7ter", "einst", "an", "ei\u00b7nen", "Schei\u00b7de\u00b7weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "VVFIN", "NE", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und fragte, unbekannt des Landes,", "tokens": ["Und", "frag\u00b7te", ",", "un\u00b7be\u00b7kannt", "des", "Lan\u00b7des", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das er durchstreifte, einen Bauersknecht,", "tokens": ["Das", "er", "durch\u00b7streif\u00b7te", ",", "ei\u00b7nen", "Bau\u00b7ers\u00b7knecht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PPER", "VMFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der faul, da, wo der Rain sich spaltete, gestreckt", "tokens": ["Der", "faul", ",", "da", ",", "wo", "der", "Rain", "sich", "spal\u00b7te\u00b7te", ",", "ge\u00b7streckt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJD", "$,", "KOUS", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "$,", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In eines Birnbaums Schatten lag:", "tokens": ["In", "ei\u00b7nes", "Birn\u00b7baums", "Schat\u00b7ten", "lag", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was f\u00fcr ein Weg nach Jericho ihn f\u00fchre?", "tokens": ["Was", "f\u00fcr", "ein", "Weg", "nach", "Je\u00b7ri\u00b7cho", "ihn", "f\u00fch\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "APPR", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Kerl, die M\u00e4nner nicht beachtend,", "tokens": ["Der", "Kerl", ",", "die", "M\u00e4n\u00b7ner", "nicht", "be\u00b7ach\u00b7tend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Verdrie\u00dflich, sich zu regen, hob ein Bein,", "tokens": ["Ver\u00b7drie\u00df\u00b7lich", ",", "sich", "zu", "re\u00b7gen", ",", "hob", "ein", "Bein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PRF", "APPR", "ADJA", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.10": {"text": "Zeigt' auf ein Haus im Feld, und g\u00e4hnt' und sprach: da unten!", "tokens": ["Zeigt'", "auf", "ein", "Haus", "im", "Feld", ",", "und", "g\u00e4hnt'", "und", "sprach", ":", "da", "un\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$,", "KON", "VVFIN", "KON", "VVFIN", "$.", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Zerrt sich die M\u00fctze \u00fcbers Ohr zurecht,", "tokens": ["Zerrt", "sich", "die", "M\u00fct\u00b7ze", "\u00fc\u00b7bers", "Ohr", "zu\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Kehrt sich, und schnarcht schon wieder ein.", "tokens": ["Kehrt", "sich", ",", "und", "schnarcht", "schon", "wie\u00b7der", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "KON", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die M\u00e4nner drauf, wohin das Bein gewiesen,", "tokens": ["Die", "M\u00e4n\u00b7ner", "drauf", ",", "wo\u00b7hin", "das", "Bein", "ge\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Gehn ihre Stra\u00dfe fort; jedoch nicht lange w\u00e4hrts,", "tokens": ["Gehn", "ih\u00b7re", "Stra\u00b7\u00dfe", "fort", ";", "je\u00b7doch", "nicht", "lan\u00b7ge", "w\u00e4hrts", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$.", "ADV", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Von Menschen leer, wie sie das Haus befinden,", "tokens": ["Von", "Men\u00b7schen", "leer", ",", "wie", "sie", "das", "Haus", "be\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Sind sie im Land schon wieder irr.", "tokens": ["Sind", "sie", "im", "Land", "schon", "wie\u00b7der", "irr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Da steht, im hei\u00dfen Strahl der Mittagssonne,", "tokens": ["Da", "steht", ",", "im", "hei\u00b7\u00dfen", "Strahl", "der", "Mit\u00b7tags\u00b7son\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Bedeckt von \u00c4hren, eine Magd,", "tokens": ["Be\u00b7deckt", "von", "\u00c4h\u00b7ren", ",", "ei\u00b7ne", "Magd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Die schneidet, frisch und wacker, Korn,", "tokens": ["Die", "schnei\u00b7det", ",", "frisch", "und", "wa\u00b7cker", ",", "Korn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Der Schwei\u00df rollt ihr vom Angesicht herab.", "tokens": ["Der", "Schwei\u00df", "rollt", "ihr", "vom", "An\u00b7ge\u00b7sicht", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Der Herr, nachdem er sich gef\u00e4llig drob ergangen,", "tokens": ["Der", "Herr", ",", "nach\u00b7dem", "er", "sich", "ge\u00b7f\u00e4l\u00b7lig", "drob", "er\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PRF", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Kehrt also sich mit Freundlichkeit zu ihr:", "tokens": ["Kehrt", "al\u00b7so", "sich", "mit", "Freund\u00b7lich\u00b7keit", "zu", "ihr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PRF", "APPR", "NN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "\u00bbmein T\u00f6chterchen, gehn wir auch recht,", "tokens": ["\u00bb", "mein", "T\u00f6ch\u00b7ter\u00b7chen", ",", "gehn", "wir", "auch", "recht", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.24": {"text": "So wie wir stehn, den Weg nach Jericho?\u00ab", "tokens": ["So", "wie", "wir", "stehn", ",", "den", "Weg", "nach", "Je\u00b7ri\u00b7cho", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "KOKOM", "PPER", "VVFIN", "$,", "ART", "NN", "APPR", "NE", "$.", "$("], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.25": {"text": "Die Magd antwortet flink: \u00bbEi, Herr!", "tokens": ["Die", "Magd", "ant\u00b7wor\u00b7tet", "flink", ":", "\u00bb", "Ei", ",", "Herr", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "$(", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Da seid ihr weit vom Wege irr gegangen;", "tokens": ["Da", "seid", "ihr", "weit", "vom", "We\u00b7ge", "irr", "ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Dort hinterm Walde liegt der Turm von Jericho,", "tokens": ["Dort", "hin\u00b7term", "Wal\u00b7de", "liegt", "der", "Turm", "von", "Je\u00b7ri\u00b7cho", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.28": {"text": "Kommt her, ich will den Weg euch zeigen.\u00ab", "tokens": ["Kommt", "her", ",", "ich", "will", "den", "Weg", "euch", "zei\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "VMFIN", "ART", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Und legt die Sichel weg, und f\u00fchrt, geschickt und emsig,", "tokens": ["Und", "legt", "die", "Si\u00b7chel", "weg", ",", "und", "f\u00fchrt", ",", "ge\u00b7schickt", "und", "em\u00b7sig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Durch \u00c4cker die der Rain durchschneidet,", "tokens": ["Durch", "\u00c4\u00b7cker", "die", "der", "Rain", "durch\u00b7schnei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Die M\u00e4nner auf die rechte Stra\u00dfe hin,", "tokens": ["Die", "M\u00e4n\u00b7ner", "auf", "die", "rech\u00b7te", "Stra\u00b7\u00dfe", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Zeigt noch, wo schon der Turm von Jericho ergl\u00e4nzet,", "tokens": ["Zeigt", "noch", ",", "wo", "schon", "der", "Turm", "von", "Je\u00b7ri\u00b7cho", "er\u00b7gl\u00e4n\u00b7zet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "ADV", "ART", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Gr\u00fc\u00dft sie und eilt zur\u00fccke wieder,", "tokens": ["Gr\u00fc\u00dft", "sie", "und", "eilt", "zu\u00b7r\u00fc\u00b7cke", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Auf da\u00df sie schneid, in R\u00fcstigkeit, und raffe,", "tokens": ["Auf", "da\u00df", "sie", "schneid", ",", "in", "R\u00fcs\u00b7tig\u00b7keit", ",", "und", "raf\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "$,", "APPR", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Von Schwei\u00df betrieft, im Weizenfelde,", "tokens": ["Von", "Schwei\u00df", "be\u00b7trieft", ",", "im", "Wei\u00b7zen\u00b7fel\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "So nach wie vor.", "tokens": ["So", "nach", "wie", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "KOKOM", "APPR", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.37": {"text": "Sankt Peter spricht: \u00bbO Meister mein!", "tokens": ["Sankt", "Pe\u00b7ter", "spricht", ":", "\u00bb", "O", "Meis\u00b7ter", "mein", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "VVFIN", "$.", "$(", "NE", "NE", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Ich bitte dich, um deiner G\u00fcte willen,", "tokens": ["Ich", "bit\u00b7te", "dich", ",", "um", "dei\u00b7ner", "G\u00fc\u00b7te", "wil\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUI", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Du wollest dieser Maid die Tat der Liebe lohnen,", "tokens": ["Du", "wol\u00b7lest", "die\u00b7ser", "Maid", "die", "Tat", "der", "Lie\u00b7be", "loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDAT", "NN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und, flink und wacker, wie sie ist,", "tokens": ["Und", ",", "flink", "und", "wa\u00b7cker", ",", "wie", "sie", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "KON", "ADJD", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Ihr einen Mann, flink auch und wacker, schenken.\u00ab", "tokens": ["Ihr", "ei\u00b7nen", "Mann", ",", "flink", "auch", "und", "wa\u00b7cker", ",", "schen\u00b7ken", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "ART", "NN", "$,", "VVFIN", "ADV", "KON", "ADJD", "$,", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "\u00bbdie Maid\u00ab, versetzt der Herr voll Ernst,", "tokens": ["\u00bb", "die", "Maid", "\u00ab", ",", "ver\u00b7setzt", "der", "Herr", "voll", "Ernst", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$(", "$,", "VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "\u00bbdie soll den faulen Schelmen nehmen,", "tokens": ["\u00bb", "die", "soll", "den", "fau\u00b7len", "Schel\u00b7men", "neh\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Den wir am Scheideweg im Birnbaumsschatten trafen;", "tokens": ["Den", "wir", "am", "Schei\u00b7de\u00b7weg", "im", "Birn\u00b7baums\u00b7schat\u00b7ten", "tra\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Also beschlo\u00df ichs gleich im Herzen,", "tokens": ["Al\u00b7so", "be\u00b7schlo\u00df", "ichs", "gleich", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPRART", "NN", "$,"], "meter": "+--+-+-+-",