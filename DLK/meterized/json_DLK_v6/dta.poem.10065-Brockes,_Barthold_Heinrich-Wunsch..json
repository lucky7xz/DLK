{"dta.poem.10065": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wunsch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sahe j\u00fcngst im lauen Leutzen,", "tokens": ["Ich", "sa\u00b7he", "j\u00fcngst", "im", "lau\u00b7en", "Leut\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu ihr- und meines Sch\u00f6pfers Ehr,", "tokens": ["Zu", "ih\u00b7r", "und", "mei\u00b7nes", "Sch\u00f6p\u00b7fers", "Ehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Jm bunten Schmuck, ein gantzes Heer", "tokens": ["Jm", "bun\u00b7ten", "Schmuck", ",", "ein", "gant\u00b7zes", "Heer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Farben-reichen Bluhmen gl\u00e4ntzen,", "tokens": ["Von", "Fa\u00b7rben\u00b7rei\u00b7chen", "Bluh\u00b7men", "gl\u00e4nt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nachdem der Morgen-Sonne Glantz,", "tokens": ["Nach\u00b7dem", "der", "Mor\u00b7gen\u00b7Son\u00b7ne", "Glantz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als wie ein \u00fcberschwemmend Meer", "tokens": ["Als", "wie", "ein", "\u00fc\u00b7bersc\u00b7hwem\u00b7mend", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "VVPP", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von W\u00e4rm und Licht, dieselben gantz", "tokens": ["Von", "W\u00e4rm", "und", "Licht", ",", "die\u00b7sel\u00b7ben", "gantz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PDAT", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bedecket, sie \u00fcmgeben, sie beflossen,", "tokens": ["Be\u00b7de\u00b7cket", ",", "sie", "\u00fcm\u00b7ge\u00b7ben", ",", "sie", "be\u00b7flos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVINF", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und mit gef\u00e4rbter Gluht sich \u00fcber sie ergossen.", "tokens": ["Und", "mit", "ge\u00b7f\u00e4rb\u00b7ter", "Gluht", "sich", "\u00fc\u00b7ber", "sie", "er\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich sah\u2019 sie mit vergn\u00fcgten Blicken,", "tokens": ["Ich", "sah'", "sie", "mit", "ver\u00b7gn\u00fcg\u00b7ten", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ja mit fast innigem Entz\u00fccken,", "tokens": ["Ja", "mit", "fast", "in\u00b7ni\u00b7gem", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Bald \u00fcberhaupt, bald einzeln, an.", "tokens": ["Bald", "\u00fc\u00b7ber\u00b7haupt", ",", "bald", "ein\u00b7zeln", ",", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADJD", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jhr gleichsam feuriges Gepr\u00e4nge,", "tokens": ["Ihr", "gleich\u00b7sam", "feu\u00b7ri\u00b7ges", "Ge\u00b7pr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die unterschiedne Gr\u00f6ss\u2019 und L\u00e4nge,", "tokens": ["Die", "un\u00b7ter\u00b7schied\u00b7ne", "Gr\u00f6ss'", "und", "L\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Form und Farben Pracht und Menge,", "tokens": ["Der", "Form", "und", "Far\u00b7ben", "Pracht", "und", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die selbst ein G\u00e4rtner-Aug\u2019 unm\u00f6glich zehlen kann,", "tokens": ["Die", "selbst", "ein", "G\u00e4rt\u00b7ner\u00b7Aug'", "un\u00b7m\u00f6g\u00b7lich", "zeh\u00b7len", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Befassten mein erstaunt Gem\u00fcthe.", "tokens": ["Be\u00b7fass\u00b7ten", "mein", "er\u00b7staunt", "Ge\u00b7m\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ich stutzt\u2019, und glaubt\u2019, in stiller Lust,", "tokens": ["Ich", "stutzt'", ",", "und", "glaubt'", ",", "in", "stil\u00b7ler", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und mit von Andacht heisser Brust", "tokens": ["Und", "mit", "von", "An\u00b7dacht", "heis\u00b7ser", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Des Sch\u00f6pfers Allmacht, Weisheit, G\u00fcte,", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "All\u00b7macht", ",", "Weis\u00b7heit", ",", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "In diesen Wundern Wunder-sch\u00f6n,", "tokens": ["In", "die\u00b7sen", "Wun\u00b7dern", "Wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Zu Seinen Ehren, anzusehn.", "tokens": ["Zu", "Sei\u00b7nen", "Eh\u00b7ren", ",", "an\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es regte sich so gleich in mir", "tokens": ["Es", "reg\u00b7te", "sich", "so", "gleich", "in", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Trieb gerechter Danck-Begier.", "tokens": ["Ein", "Trieb", "ge\u00b7rech\u00b7ter", "Dan\u00b7ck\u00b7Be\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ach! dacht ich, m\u00f6gte meine Freude", "tokens": ["Ach", "!", "dacht", "ich", ",", "m\u00f6g\u00b7te", "mei\u00b7ne", "Freu\u00b7de"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$,", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ob dieser sch\u00f6nen Augen-Weide", "tokens": ["Ob", "die\u00b7ser", "sch\u00f6\u00b7nen", "Au\u00b7gen\u00b7Wei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und aller Wunder Pracht und Schein,", "tokens": ["Und", "al\u00b7ler", "Wun\u00b7der", "Pracht", "und", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Vater, Dir gef\u00e4llig seyn!", "tokens": ["Mein", "Va\u00b7ter", ",", "Dir", "ge\u00b7f\u00e4l\u00b7lig", "seyn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ach m\u00f6gte alles das, was mein,", "tokens": ["Ach", "m\u00f6g\u00b7te", "al\u00b7les", "das", ",", "was", "mein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "PDS", "$,", "PRELS", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "HeRR, Deinen Ruhm zu mehren t\u00fcgen!", "tokens": ["HeRR", ",", "Dei\u00b7nen", "Ruhm", "zu", "meh\u00b7ren", "t\u00fc\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "HeRR, la\u00df, aus Lieb\u2019 und Gnad allein,", "tokens": ["HeRR", ",", "la\u00df", ",", "aus", "Lieb'", "und", "Gnad", "al\u00b7lein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "$,", "APPR", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Auch mein Vergn\u00fcgen Dich vergn\u00fcgen!", "tokens": ["Auch", "mein", "Ver\u00b7gn\u00fc\u00b7gen", "Dich", "ver\u00b7gn\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}