{"textgrid.poem.28019": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich f\u00fcrchte nicht den Tod, der mich zu nemen k\u00fcmmt;", "tokens": ["Ich", "f\u00fcrch\u00b7te", "nicht", "den", "Tod", ",", "der", "mich", "zu", "ne\u00b7men", "k\u00fcmmt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,", "PRELS", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich f\u00fcrchte mehr den Tod, der mir die Meinen nimmt.", "tokens": ["Ich", "f\u00fcrch\u00b7te", "mehr", "den", "Tod", ",", "der", "mir", "die", "Mei\u00b7nen", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}