{"textgrid.poem.24372": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Hertzog von Marlboroug", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich zeuget Engelland/ und Teutschland macht dich gro\u00df.", "tokens": ["Dich", "zeu\u00b7get", "En\u00b7gel\u00b7land", "/", "und", "Teutschland", "macht", "dich", "gro\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$(", "KON", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Dir sitzt die Tapferkeit/ und du dem Gl\u00fcck im Schoo\u00df.", "tokens": ["Dir", "sitzt", "die", "Tap\u00b7fer\u00b7keit", "/", "und", "du", "dem", "Gl\u00fcck", "im", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "KON", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die K\u00f6nigin gab dir/ o Held/ darum die Waffen/", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "gab", "dir", "/", "o", "Held", "/", "da\u00b7rum", "die", "Waf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "FM", "NN", "$(", "PAV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dir einen Hertzogs Hut/ Ihr Sicherheit zu schaffen/", "tokens": ["Dir", "ei\u00b7nen", "Hert\u00b7zogs", "Hut", "/", "Ihr", "Si\u00b7cher\u00b7heit", "zu", "schaf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "$(", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Engels Held zu seyn/ der so auf Franckreich schl\u00e4gt/", "tokens": ["Ein", "En\u00b7gels", "Held", "zu", "seyn", "/", "der", "so", "auf", "Fran\u00b7ck\u00b7reich", "schl\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VAINF", "$(", "ART", "ADV", "APPR", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "Als wie den Sanherib ein Engel hat erlegt.", "tokens": ["Als", "wie", "den", "San\u00b7he\u00b7rib", "ein", "En\u00b7gel", "hat", "er\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}