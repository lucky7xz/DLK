{"dta.poem.14311": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn zwei geschieden sind von Herz und Munde,", "tokens": ["Wenn", "zwei", "ge\u00b7schie\u00b7den", "sind", "von", "Herz", "und", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "VVPP", "VAFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da zieh'n Gedanken \u00fcber Berg' und Schl\u00fcfte", "tokens": ["Da", "zieh'n", "Ge\u00b7dan\u00b7ken", "\u00fc\u00b7ber", "Ber\u00b7g'", "und", "Schl\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie Tauben s\u00e4uselnd durch die blauen L\u00fcfte,", "tokens": ["Wie", "Tau\u00b7ben", "s\u00e4u\u00b7selnd", "durch", "die", "blau\u00b7en", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und tragen hin und wieder s\u00fc\u00dfe Kunde.", "tokens": ["Und", "tra\u00b7gen", "hin", "und", "wie\u00b7der", "s\u00fc\u00b7\u00dfe", "Kun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}