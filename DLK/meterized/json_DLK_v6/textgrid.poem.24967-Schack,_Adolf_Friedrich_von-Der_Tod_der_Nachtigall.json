{"textgrid.poem.24967": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Tod der Nachtigall", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, die unsterblich, vom Geschlechte", "tokens": ["Du", ",", "die", "uns\u00b7terb\u00b7lich", ",", "vom", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ADJD", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Feen und Elfen ich geglaubt,", "tokens": ["Der", "Feen", "und", "El\u00b7fen", "ich", "ge\u00b7glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O holde Freundin meiner N\u00e4chte,", "tokens": ["O", "hol\u00b7de", "Freun\u00b7din", "mei\u00b7ner", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So hat der Tod dich mir geraubt!", "tokens": ["So", "hat", "der", "Tod", "dich", "mir", "ge\u00b7raubt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im weichen Mondlicht vom Balkone", "tokens": ["Im", "wei\u00b7chen", "Mond\u00b7licht", "vom", "Bal\u00b7ko\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+++-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Wie oft dir lauscht' ich andachtsvoll,", "tokens": ["Wie", "oft", "dir", "lauscht'", "ich", "an\u00b7dachts\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn aus der gr\u00fcnen Bl\u00e4tterkrone", "tokens": ["Wenn", "aus", "der", "gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7ter\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein heil'ges Lied her\u00fcberscholl.", "tokens": ["Dein", "heil'\u00b7ges", "Lied", "her\u00b7\u00fc\u00b7bersc\u00b7holl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf horchte selbst das Seelenlose", "tokens": ["Auf", "horch\u00b7te", "selbst", "das", "See\u00b7len\u00b7lo\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den T\u00f6nen deiner Melodie;", "tokens": ["Den", "T\u00f6\u00b7nen", "dei\u00b7ner", "Me\u00b7lo\u00b7die", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die bleiche Lilie, die Rose", "tokens": ["Die", "blei\u00b7che", "Li\u00b7lie", ",", "die", "Ro\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In ihrem Schlummer h\u00f6rten sie.", "tokens": ["In", "ih\u00b7rem", "Schlum\u00b7mer", "h\u00f6r\u00b7ten", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zu Abgrundtiefen bald versunken,", "tokens": ["Zu", "Ab\u00b7grund\u00b7tie\u00b7fen", "bald", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo kein Gestirn des Lichtes kreist,", "tokens": ["Wo", "kein", "Ge\u00b7stirn", "des", "Lich\u00b7tes", "kreist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald von des Himmels Wonne trunken", "tokens": ["Bald", "von", "des", "Him\u00b7mels", "Won\u00b7ne", "trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schien im Gesang dein Sehergeist.", "tokens": ["Schien", "im", "Ge\u00b7sang", "dein", "Se\u00b7her\u00b7geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein Hoffen quoll aus ihm, ein Ahnen", "tokens": ["Ein", "Hof\u00b7fen", "quoll", "aus", "ihm", ",", "ein", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von H\u00f6herm, als die Erde giebt;", "tokens": ["Von", "H\u00f6\u00b7herm", ",", "als", "die", "Er\u00b7de", "giebt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Hauch, so wollte mich's gemahnen,", "tokens": ["Ein", "Hauch", ",", "so", "woll\u00b7te", "mich's", "ge\u00b7mah\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VMFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Liebe, die in allen liebt.", "tokens": ["Der", "Lie\u00b7be", ",", "die", "in", "al\u00b7len", "liebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nicht schwieg dein Schmettern, dein Gefl\u00f6te,", "tokens": ["Nicht", "schwieg", "dein", "Schmet\u00b7tern", ",", "dein", "Ge\u00b7fl\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seitdem das Abendlicht vergl\u00fcht;", "tokens": ["Seit\u00b7dem", "das", "A\u00b7bend\u00b7licht", "ver\u00b7gl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erst sp\u00e4t beim Schein der Morgenr\u00f6te", "tokens": ["Erst", "sp\u00e4t", "beim", "Schein", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sank dir das K\u00f6pfchen schlummerm\u00fcd.", "tokens": ["Sank", "dir", "das", "K\u00f6pf\u00b7chen", "schlum\u00b7mer\u00b7m\u00fcd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Im Dunkel gestern auch zum Singen", "tokens": ["Im", "Dun\u00b7kel", "ge\u00b7stern", "auch", "zum", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ADV", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Auf deinem Zweig warst du erwacht;", "tokens": ["Auf", "dei\u00b7nem", "Zweig", "warst", "du", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gew\u00f6lk stieg auf; verloren gingen", "tokens": ["Ge\u00b7w\u00f6lk", "stieg", "auf", ";", "ver\u00b7lo\u00b7ren", "gin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ", "$.", "VVPP", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schlaftrunkne Donner durch die Nacht.", "tokens": ["Schlaf\u00b7trunk\u00b7ne", "Don\u00b7ner", "durch", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Sanft glitt dein Lied, das leisgehauchte,", "tokens": ["Sanft", "glitt", "dein", "Lied", ",", "das", "leis\u00b7ge\u00b7hauch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Rosen- und Jasminenduft,", "tokens": ["Auf", "Ro\u00b7sen", "und", "Jas\u00b7mi\u00b7nen\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Der ringsher aus den Kelchen rauchte,", "tokens": ["Der", "rings\u00b7her", "aus", "den", "Kel\u00b7chen", "rauch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu mir durch sommerschw\u00fcle Luft.", "tokens": ["Zu", "mir", "durch", "som\u00b7mer\u00b7schw\u00fc\u00b7le", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Doch st\u00e4rker war der Aeste Sausen,", "tokens": ["Doch", "st\u00e4r\u00b7ker", "war", "der", "A\u00b7es\u00b7te", "Sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Donnerkrachens Wiederhall;", "tokens": ["Des", "Don\u00b7ner\u00b7kra\u00b7chens", "Wie\u00b7der\u00b7hall", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Laut, immer lauter durch das Brausen", "tokens": ["Laut", ",", "im\u00b7mer", "lau\u00b7ter", "durch", "das", "Brau\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "$,", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Sturms quoll deiner Stimme Schall;", "tokens": ["Des", "Sturms", "quoll", "dei\u00b7ner", "Stim\u00b7me", "Schall", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und ob der Blitz mit lohem Strahle", "tokens": ["Und", "ob", "der", "Blitz", "mit", "lo\u00b7hem", "Strah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hernieder auf die Wipfel fuhr,", "tokens": ["Her\u00b7nie\u00b7der", "auf", "die", "Wip\u00b7fel", "fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hoch jauchztest du in dem Chorale", "tokens": ["Hoch", "jauchz\u00b7test", "du", "in", "dem", "Cho\u00b7ra\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der um dich jubelnden Natur.", "tokens": ["Der", "um", "dich", "ju\u00b7beln\u00b7den", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}}, "stanza.11": {"line.1": {"text": "Mit Geistern war's ein Zwiesprachhalten,", "tokens": ["Mit", "Geis\u00b7tern", "wa\u00b7r's", "ein", "Zwie\u00b7sprach\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein St\u00fcrzen in das ew'ge Licht,", "tokens": ["Ein", "St\u00fcr\u00b7zen", "in", "das", "ew'\u00b7ge", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Schauen himmlischer Gestalten,", "tokens": ["Ein", "Schau\u00b7en", "himm\u00b7li\u00b7scher", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie in Ezechiels Gesicht.", "tokens": ["Wie", "in", "E\u00b7zec\u00b7hiels", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NE", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.12": {"line.1": {"text": "Und, wo selbst der Prophet mit Zagen", "tokens": ["Und", ",", "wo", "selbst", "der", "Pro\u00b7phet", "mit", "Za\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "ADV", "ART", "NN", "APPR", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Den Blick gesenkt und heil'gem Graun,", "tokens": ["Den", "Blick", "ge\u00b7senkt", "und", "heil'\u00b7gem", "Graun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie wolltest du's, o Kleine, tragen,", "tokens": ["Wie", "woll\u00b7test", "du's", ",", "o", "Klei\u00b7ne", ",", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "$,", "FM", "ADJA", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Gottheit unverh\u00fcllt zu schaun?", "tokens": ["Die", "Got\u00b7theit", "un\u00b7ver\u00b7h\u00fcllt", "zu", "schaun", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Beim Fr\u00fchrot rollte durch das Wetter", "tokens": ["Beim", "Fr\u00fch\u00b7rot", "roll\u00b7te", "durch", "das", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein letzter, m\u00e4cht'ger Donnerklang,", "tokens": ["Ein", "letz\u00b7ter", ",", "m\u00e4cht'\u00b7ger", "Don\u00b7ner\u00b7klang", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch den ein jubelndes Geschmetter", "tokens": ["Durch", "den", "ein", "ju\u00b7beln\u00b7des", "Ge\u00b7schmet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "In hohem, vollem Hymnus drang.", "tokens": ["In", "ho\u00b7hem", ",", "vol\u00b7lem", "Hym\u00b7nus", "drang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Glorreich durchs Dunkel stieg die Sonne;", "tokens": ["Glor\u00b7reich", "durchs", "Dun\u00b7kel", "stieg", "die", "Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Da sankst du zuckend erdenw\u00e4rts;", "tokens": ["Da", "sankst", "du", "zu\u00b7ckend", "er\u00b7den\u00b7w\u00e4rts", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Donner schwieg; im Sturm der Wonne", "tokens": ["Der", "Don\u00b7ner", "schwieg", ";", "im", "Sturm", "der", "Won\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gebrochen war dein kleines Herz.", "tokens": ["Ge\u00b7bro\u00b7chen", "war", "dein", "klei\u00b7nes", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}