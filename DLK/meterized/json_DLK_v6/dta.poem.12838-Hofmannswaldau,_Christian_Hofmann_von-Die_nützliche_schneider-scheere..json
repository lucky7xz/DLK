{"dta.poem.12838": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die n\u00fctzliche schneider-scheere.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr ausgehangnen schneider-scheeren!", "tokens": ["Ihr", "aus\u00b7ge\u00b7hang\u00b7nen", "schnei\u00b7der\u00b7schee\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der so genannten b\u00f6cke schild!", "tokens": ["Der", "so", "ge\u00b7nann\u00b7ten", "b\u00f6\u00b7cke", "schild", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr werdet mirs ja nicht verwehren,", "tokens": ["Ihr", "wer\u00b7det", "mirs", "ja", "nicht", "ver\u00b7weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil euch doch alles gleiche gilt,", "tokens": ["Weil", "euch", "doch", "al\u00b7les", "glei\u00b7che", "gilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df ich mag euer bild auf diese bl\u00e4tter mahlen,", "tokens": ["Da\u00df", "ich", "mag", "eu\u00b7er", "bild", "auf", "die\u00b7se", "bl\u00e4t\u00b7ter", "mah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "PPOSAT", "NN", "APPR", "PDAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es wird weit pr\u00e4chtiger, als Piters flecke, pralen.", "tokens": ["Es", "wird", "weit", "pr\u00e4ch\u00b7ti\u00b7ger", ",", "als", "Pi\u00b7ters", "fle\u00b7cke", ",", "pra\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADJA", "$,", "KOUS", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Jhr seyd der schneider alchymisten,", "tokens": ["Ihr", "seyd", "der", "schnei\u00b7der", "al\u00b7chy\u00b7mis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr macht und bringet ihnen geld;", "tokens": ["Ihr", "macht", "und", "brin\u00b7get", "ih\u00b7nen", "geld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sie nicht eure h\u00fclffe w\u00fc\u00dften,", "tokens": ["Wenn", "sie", "nicht", "eu\u00b7re", "h\u00fclf\u00b7fe", "w\u00fc\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So w\u00e4r es schlecht um sie bestellt.", "tokens": ["So", "w\u00e4r", "es", "schlecht", "um", "sie", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das b\u00fcgel-eisen giebt den meisten zwars gewichte,", "tokens": ["Das", "b\u00fc\u00b7gel\u00b7ei\u00b7sen", "giebt", "den", "meis\u00b7ten", "zwars", "ge\u00b7wich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "PIAT", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch machet euer nutz desselben krafft zu nichte.", "tokens": ["Doch", "ma\u00b7chet", "eu\u00b7er", "nutz", "des\u00b7sel\u00b7ben", "krafft", "zu", "nich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PDAT", "NN", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es f\u00fchren euch die kleider-b\u00f6cke;", "tokens": ["Es", "f\u00fch\u00b7ren", "euch", "die", "klei\u00b7der\u00b7b\u00f6\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch dis vermehret euren ruhm,", "tokens": ["Doch", "dis", "ver\u00b7meh\u00b7ret", "eu\u00b7ren", "ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr lacht offt \u00fcber das gemecke,", "tokens": ["Ihr", "lacht", "offt", "\u00fc\u00b7ber", "das", "ge\u00b7me\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der herren schneider eigenth", "tokens": ["Der", "her\u00b7ren", "schnei\u00b7der", "ei\u00b7genth"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn diese meister sich, vor jene schlitz-krabaten", "tokens": ["Wenn", "die\u00b7se", "meis\u00b7ter", "sich", ",", "vor", "je\u00b7ne", "schlitz\u00b7kra\u00b7ba\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "PRF", "$,", "APPR", "PDAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die moden zu ersehn, in ihrer zunfft berathen.", "tokens": ["Die", "mo\u00b7den", "zu", "er\u00b7sehn", ",", "in", "ih\u00b7rer", "zunfft", "be\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PTKZU", "VVINF", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Es lebe demnach dieses mittel,", "tokens": ["Es", "le\u00b7be", "dem\u00b7nach", "die\u00b7ses", "mit\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das sch\u00f6ne pluder-hosen macht,", "tokens": ["Das", "sch\u00f6\u00b7ne", "plu\u00b7der\u00b7ho\u00b7sen", "macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wird noch mancher m\u00e4gde-kittel", "tokens": ["So", "wird", "noch", "man\u00b7cher", "m\u00e4g\u00b7de\u00b7kit\u00b7tel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ans helle tage-licht gebracht:", "tokens": ["Ans", "hel\u00b7le", "ta\u00b7ge\u00b7licht", "ge\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch leben sie, so sollt ihr scheeren gleichfalls leben,", "tokens": ["Doch", "le\u00b7ben", "sie", ",", "so", "sollt", "ihr", "schee\u00b7ren", "gleich\u00b7falls", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADV", "VMFIN", "PPER", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Damit man euer lob zum capricorn kan heben.", "tokens": ["Da\u00b7mit", "man", "eu\u00b7er", "lob", "zum", "cap\u00b7ri\u00b7corn", "kan", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}