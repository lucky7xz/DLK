{"textgrid.poem.57181": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "[o wie gerne lern ich Milde]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie gerne lern ich Milde,", "tokens": ["O", "wie", "ger\u00b7ne", "lern", "ich", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "liebes Herz, von deinem Munde,", "tokens": ["lie\u00b7bes", "Herz", ",", "von", "dei\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "folge dir in stillem Bunde", "tokens": ["fol\u00b7ge", "dir", "in", "stil\u00b7lem", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "in gel\u00e4uterte Gefilde!", "tokens": ["in", "ge\u00b7l\u00e4u\u00b7ter\u00b7te", "Ge\u00b7fil\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wir schaun zur\u00fcck zusammen", "tokens": ["Und", "wir", "schaun", "zu\u00b7r\u00fcck", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJD", "PTKVZ", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "auf die Welt, samt ihrem Schelten,", "tokens": ["auf", "die", "Welt", ",", "samt", "ih\u00b7rem", "Schel\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und anstatt sie zu verdammen,", "tokens": ["und", "an\u00b7statt", "sie", "zu", "ver\u00b7dam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "lassen wir sie gehn und gelten.", "tokens": ["las\u00b7sen", "wir", "sie", "gehn", "und", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}