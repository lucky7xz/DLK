{"textgrid.poem.56075": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die Entf\u00fchrung", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft war sie als Kind ihren Dienerinnen", "tokens": ["Oft", "war", "sie", "als", "Kind", "ih\u00b7ren", "Die\u00b7ne\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "KOUS", "NN", "PPOSAT", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "entwichen, um die Nacht und den Wind", "tokens": ["ent\u00b7wi\u00b7chen", ",", "um", "die", "Nacht", "und", "den", "Wind"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUI", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "(weil sie drinnen so anders sind)", "tokens": ["(", "weil", "sie", "drin\u00b7nen", "so", "an\u00b7ders", "sind", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "ADV", "ADV", "VAFIN", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "drau\u00dfen zu sehn an ihrem Beginnen;", "tokens": ["drau\u00b7\u00dfen", "zu", "sehn", "an", "ih\u00b7rem", "Be\u00b7gin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "doch keine Sturmnacht hatte gewi\u00df", "tokens": ["doch", "kei\u00b7ne", "Sturm\u00b7nacht", "hat\u00b7te", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "den riesigen Park so in St\u00fccke gerissen,", "tokens": ["den", "rie\u00b7si\u00b7gen", "Park", "so", "in", "St\u00fc\u00b7cke", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "wie ihn jetzt ihr Gewissen zerri\u00df,", "tokens": ["wie", "ihn", "jetzt", "ihr", "Ge\u00b7wis\u00b7sen", "zer\u00b7ri\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "da er sie nahm von der seidenen Leiter", "tokens": ["da", "er", "sie", "nahm", "von", "der", "sei\u00b7de\u00b7nen", "Lei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und sie weitertrug, weiter, weiter...:", "tokens": ["und", "sie", "wei\u00b7ter\u00b7trug", ",", "wei\u00b7ter", ",", "wei\u00b7ter", "...", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "ADV", "$,", "ADV", "$(", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "bis der Wagen alles war.", "tokens": ["bis", "der", "Wa\u00b7gen", "al\u00b7les", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIS", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und sie roch ihn, den schwarzen Wagen,", "tokens": ["Und", "sie", "roch", "ihn", ",", "den", "schwar\u00b7zen", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "um den verhalten das Jagen stand", "tokens": ["um", "den", "ver\u00b7hal\u00b7ten", "das", "Ja\u00b7gen", "stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "und die Gefahr.", "tokens": ["und", "die", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und sie fand ihn mit Kaltem ausgeschlagen;", "tokens": ["Und", "sie", "fand", "ihn", "mit", "Kal\u00b7tem", "aus\u00b7ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "und das Schwarze und Kalte war auch in ihr.", "tokens": ["und", "das", "Schwar\u00b7ze", "und", "Kal\u00b7te", "war", "auch", "in", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "NN", "VAFIN", "ADV", "APPR", "PPOSAT", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Sie kroch in ihren Mantelkragen", "tokens": ["Sie", "kroch", "in", "ih\u00b7ren", "Man\u00b7tel\u00b7kra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "und bef\u00fchlte ihr Haar, als bliebe es hier,", "tokens": ["und", "be\u00b7f\u00fchl\u00b7te", "ihr", "Haar", ",", "als", "blie\u00b7be", "es", "hier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "VVFIN", "PPER", "ADV", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "und h\u00f6rte fremd einen Fremden sagen:", "tokens": ["und", "h\u00f6r\u00b7te", "fremd", "ei\u00b7nen", "Frem\u00b7den", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Ichbinbeidir.", "tokens": ["Ich\u00b7bin\u00b7bei\u00b7dir", "."], "token_info": ["word", "punct"], "pos": ["PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}