{"dta.poem.21404": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da war der Wille, dessen Gesicht", "tokens": ["Da", "war", "der", "Wil\u00b7le", ",", "des\u00b7sen", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ein Stammbuch, worin mit Hieben", "tokens": ["Ein", "Stamm\u00b7buch", ",", "wo\u00b7rin", "mit", "Hie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die akademischen Feinde sich", "tokens": ["Die", "a\u00b7kad\u00b7e\u00b7mi\u00b7schen", "Fein\u00b7de", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PRF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Recht leserlich eingeschrieben.", "tokens": ["Recht", "le\u00b7ser\u00b7lich", "ein\u00b7ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da war der Fucks, ein blinder Heid,", "tokens": ["Da", "war", "der", "Fucks", ",", "ein", "blin\u00b7der", "Heid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und pers\u00f6nlicher Feind des Jehovah,", "tokens": ["Und", "per\u00b7s\u00f6n\u00b7li\u00b7cher", "Feind", "des", "Je\u00b7ho\u00b7vah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+--", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Glaubt nur an Hegel und etwa noch", "tokens": ["Glaubt", "nur", "an", "He\u00b7gel", "und", "et\u00b7wa", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NE", "KON", "ADV", "ADV"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "An die Venus des Canova.", "tokens": ["An", "die", "Ve\u00b7nus", "des", "Ca\u00b7no\u00b7va", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein Campe war Amphytrio", "tokens": ["Mein", "Cam\u00b7pe", "war", "Am\u00b7phy\u00b7trio"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und l\u00e4chelte vor Wonne;", "tokens": ["Und", "l\u00e4\u00b7chel\u00b7te", "vor", "Won\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein Auge stralte Seligkeit,", "tokens": ["Sein", "Au\u00b7ge", "stral\u00b7te", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie eine verkl\u00e4rte Madonne.", "tokens": ["Wie", "ei\u00b7ne", "ver\u00b7kl\u00e4r\u00b7te", "Ma\u00b7don\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Ich a\u00df und trank, mit gutem Ap\u2019tit,", "tokens": ["Ich", "a\u00df", "und", "trank", ",", "mit", "gu\u00b7tem", "Ap'\u00b7tit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und dachte in meinem Gem\u00fcthe:", "tokens": ["Und", "dach\u00b7te", "in", "mei\u00b7nem", "Ge\u00b7m\u00fc\u00b7the", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "\u201eder Campe ist wirklich ein gro\u00dfer Mann,", "tokens": ["\u201e", "der", "Cam\u00b7pe", "ist", "wirk\u00b7lich", "ein", "gro\u00b7\u00dfer", "Mann", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ist aller Verleger Bl\u00fcthe.", "tokens": ["Ist", "al\u00b7ler", "Ver\u00b7le\u00b7ger", "Bl\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}