{"textgrid.poem.64030": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Pfingstgewitter", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "L\u00f6we, Adler ihre st\u00fcrzenden Grausamkeiten sind mehr als", "tokens": ["L\u00f6\u00b7we", ",", "Ad\u00b7ler", "ih\u00b7re", "st\u00fcr\u00b7zen\u00b7den", "Grau\u00b7sam\u00b7kei\u00b7ten", "sind", "mehr", "als"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "PPOSAT", "ADJA", "NN", "VAFIN", "PIAT", "KOKOM"], "meter": "+-+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Lammverspeisen und Verzehren von Ziegen. Sie haben Sein.", "tokens": ["Lamm\u00b7ver\u00b7spei\u00b7sen", "und", "Ver\u00b7zeh\u00b7ren", "von", "Zie\u00b7gen", ".", "Sie", "ha\u00b7ben", "Sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "$.", "PPER", "VAFIN", "PPOSAT", "$."], "meter": "+-+-+-+--+--+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Sein der H\u00f6he.", "tokens": ["Sein", "der", "H\u00f6\u00b7he", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Ihr fragenden Grausamkeiten halte ich in mir, ein geduldiger L\u00f6we.", "tokens": ["Ihr", "fra\u00b7gen\u00b7den", "Grau\u00b7sam\u00b7kei\u00b7ten", "hal\u00b7te", "ich", "in", "mir", ",", "ein", "ge\u00b7dul\u00b7di\u00b7ger", "L\u00f6\u00b7we", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPER", "$,", "ART", "ADJA", "NE", "$."], "meter": "-+--+-+-+--+-+-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Ich st\u00f6hne den Rager, den Weltvergie\u00dfer", "tokens": ["Ich", "st\u00f6h\u00b7ne", "den", "Ra\u00b7ger", ",", "den", "Welt\u00b7ver\u00b7gie\u00b7\u00dfer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Ich br\u00fclle den Rei\u00dfenden.", "tokens": ["Ich", "br\u00fcl\u00b7le", "den", "Rei\u00b7\u00dfen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.7": {"text": "Bin Gewitter wie das, das da oben kommt", "tokens": ["Bin", "Ge\u00b7wit\u00b7ter", "wie", "das", ",", "das", "da", "o\u00b7ben", "kommt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "KOKOM", "PDS", "$,", "PRELS", "ADV", "ADV", "VVFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich behalte meine Blitze:", "tokens": ["Ich", "be\u00b7hal\u00b7te", "mei\u00b7ne", "Blit\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zerrissener Himmel", "tokens": ["Zer\u00b7ris\u00b7se\u00b7ner", "Him\u00b7mel"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Gewaltiges Wort.", "tokens": ["Ge\u00b7wal\u00b7ti\u00b7ges", "Wort", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und ist da oben wer Schriftgelehrter:", "tokens": ["Und", "ist", "da", "o\u00b7ben", "wer", "Schrift\u00b7ge\u00b7lehr\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "PWS", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Mag sein Gewand zerrei\u00dfen.", "tokens": ["Mag", "sein", "Ge\u00b7wand", "zer\u00b7rei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bardenwart der!", "tokens": ["Bar\u00b7den\u00b7wart", "der", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ART", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ja, ja du Bardenwart der L\u00fcfte, und wenn du noch so brummst.", "tokens": ["Ja", ",", "ja", "du", "Bar\u00b7den\u00b7wart", "der", "L\u00fcf\u00b7te", ",", "und", "wenn", "du", "noch", "so", "brummst", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "PPER", "NN", "ART", "NN", "$,", "KON", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Ragender,", "tokens": ["Ra\u00b7gen\u00b7der", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.9": {"text": "Weltvergie\u00dfer,", "tokens": ["Welt\u00b7ver\u00b7gie\u00b7\u00dfer", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Frierst du nicht, so oben?", "tokens": ["Frierst", "du", "nicht", ",", "so", "o\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "ADV", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Wirst du nicht wahnsinnig,", "tokens": ["Wirst", "du", "nicht", "wahn\u00b7sin\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Da so gar nichts dein ist.", "tokens": ["Da", "so", "gar", "nichts", "dein", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PIS", "PPOSAT", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Wer alles hat, hat wieder nichts.", "tokens": ["Wer", "al\u00b7les", "hat", ",", "hat", "wie\u00b7der", "nichts", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VAFIN", "$,", "VAFIN", "ADV", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Sollen wir?", "tokens": ["Sol\u00b7len", "wir", "?"], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Dir Gesellschaft leisten?", "tokens": ["Dir", "Ge\u00b7sell\u00b7schaft", "leis\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "Mit dir spielen?", "tokens": ["Mit", "dir", "spie\u00b7len", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Bist du nicht Kind?", "tokens": ["Bist", "du", "nicht", "Kind", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "So mu\u00dft du es werden.", "tokens": ["So", "mu\u00dft", "du", "es", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VAINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Und besonders wir Dichter.", "tokens": ["Und", "be\u00b7son\u00b7ders", "wir", "Dich\u00b7ter."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["KON", "ADV", "PPER", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.20": {"text": "Wir?", "tokens": ["Wir", "?"], "token_info": ["word", "punct"], "pos": ["PPER", "$."], "meter": "-", "measure": "single.down"}, "line.21": {"text": "Was wei\u00df ich von anderen,", "tokens": ["Was", "wei\u00df", "ich", "von", "an\u00b7de\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ADJA", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "Bin ich nicht auch wie du?", "tokens": ["Bin", "ich", "nicht", "auch", "wie", "du", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "KOKOM", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "So eigen allein!", "tokens": ["So", "ei\u00b7gen", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.24": {"text": "Ob auch nicht ganz", "tokens": ["Ob", "auch", "nicht", "ganz"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PTKNEG", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.25": {"text": "So m\u00e4chtig.", "tokens": ["So", "m\u00e4ch\u00b7tig", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Ich will dich unterhalten:", "tokens": ["Ich", "will", "dich", "un\u00b7ter\u00b7hal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wei\u00dfe Flammen taumeln,", "tokens": ["Wei\u00b7\u00dfe", "Flam\u00b7men", "tau\u00b7meln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Tanzen der jauchzenden Feuerreigen", "tokens": ["Tan\u00b7zen", "der", "jauch\u00b7zen\u00b7den", "Feu\u00b7er\u00b7rei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Gl\u00fchender Welt.", "tokens": ["Gl\u00fc\u00b7hen\u00b7der", "Welt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Leuchtende Gewitter bl\u00fchen,", "tokens": ["Leuch\u00b7ten\u00b7de", "Ge\u00b7wit\u00b7ter", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Klar\u00e4ugige St\u00fcrme, Wolkenj\u00e4ger", "tokens": ["Kla\u00b7r\u00e4u\u00b7gi\u00b7ge", "St\u00fcr\u00b7me", ",", "Wol\u00b7ken\u00b7j\u00e4\u00b7ger"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wischen den spr\u00fchenden Schwei\u00df", "tokens": ["Wi\u00b7schen", "den", "spr\u00fc\u00b7hen\u00b7den", "Schwei\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Von h\u00e4mmernder Stirn.", "tokens": ["Von", "h\u00e4m\u00b7mern\u00b7der", "Stirn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Und wilder Segen ist,", "tokens": ["Und", "wil\u00b7der", "Se\u00b7gen", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Himmeltaumelnde Trunkenheit,", "tokens": ["Him\u00b7mel\u00b7tau\u00b7meln\u00b7de", "Trun\u00b7ken\u00b7heit", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "Zausen starker Neckerei.", "tokens": ["Zau\u00b7sen", "star\u00b7ker", "Ne\u00b7cke\u00b7rei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Dankbar bl\u00fcht da L\u00e4cheln aus tauig taumelndem Grunde.", "tokens": ["Dank\u00b7bar", "bl\u00fcht", "da", "L\u00e4\u00b7cheln", "aus", "tau\u00b7ig", "tau\u00b7meln\u00b7dem", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "NN", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Safttollende Kelche strotzend frischer Feuer bluten.", "tokens": ["Saft\u00b7tol\u00b7len\u00b7de", "Kel\u00b7che", "strot\u00b7zend", "fri\u00b7scher", "Feu\u00b7er", "blu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "++--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "Wei\u00df gesch\u00fcrzte Reigen,", "tokens": ["Wei\u00df", "ge\u00b7sch\u00fcrz\u00b7te", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Dr\u00e4ngend leuchtende Gewitter", "tokens": ["Dr\u00e4n\u00b7gend", "leuch\u00b7ten\u00b7de", "Ge\u00b7wit\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Dr\u00fccken ihrer schwellenden Fr\u00fcchte", "tokens": ["Dr\u00fc\u00b7cken", "ih\u00b7rer", "schwel\u00b7len\u00b7den", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "Berauschend erquickenden Saft", "tokens": ["Be\u00b7rau\u00b7schend", "er\u00b7qui\u00b7cken\u00b7den", "Saft"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.17": {"text": "Auf diese wei\u00df gesch\u00fcrzten selig auf-", "tokens": ["Auf", "die\u00b7se", "wei\u00df", "ge\u00b7sch\u00fcrz\u00b7ten", "se\u00b7lig", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "VVFIN", "ADV", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "schmachtenden Reigen,", "tokens": ["schmach\u00b7ten\u00b7den", "Rei\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Warme Wolken gleiten gl\u00fcckleuchtend spazieren.", "tokens": ["War\u00b7me", "Wol\u00b7ken", "glei\u00b7ten", "gl\u00fcck\u00b7leuch\u00b7tend", "spa\u00b7zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Umtaumelnd Mutwill, fromm die Erde, f\u00fcrchtende Freude.", "tokens": ["Um\u00b7tau\u00b7melnd", "Mut\u00b7will", ",", "fromm", "die", "Er\u00b7de", ",", "f\u00fcrch\u00b7ten\u00b7de", "Freu\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "NN", "$,", "ADJD", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Wie sie ausbricht, die jubelstrotzende", "tokens": ["Wie", "sie", "aus\u00b7bricht", ",", "die", "ju\u00b7bel\u00b7strot\u00b7zen\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.22": {"text": "Leidenschaft zusammenziehender H\u00f6hen.", "tokens": ["Lei\u00b7den\u00b7schaft", "zu\u00b7sam\u00b7men\u00b7zie\u00b7hen\u00b7der", "H\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.23": {"text": "Nachtigallenst\u00fcrme aus wonnewankenden W\u00e4ldern.", "tokens": ["Nach\u00b7ti\u00b7gal\u00b7len\u00b7st\u00fcr\u00b7me", "aus", "won\u00b7ne\u00b7wan\u00b7ken\u00b7den", "W\u00e4l\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Weichstark dringen klingender Seele \u2013", "tokens": ["Weichs\u00b7tark", "drin\u00b7gen", "klin\u00b7gen\u00b7der", "See\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.25": {"text": "Jubelnd stirbt sich's am Lied.", "tokens": ["Ju\u00b7belnd", "stirbt", "sich's", "am", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Adler schreien und schlagen nieder", "tokens": ["Ad\u00b7ler", "schrei\u00b7en", "und", "schla\u00b7gen", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "PTKVZ"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "mit jauchzendem Gefieder", "tokens": ["mit", "jauch\u00b7zen\u00b