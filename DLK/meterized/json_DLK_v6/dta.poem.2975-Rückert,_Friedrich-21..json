{"dta.poem.2975": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die H\u00f6lle Dante's hat mich weiland sehr emp\u00f6rt,", "tokens": ["Die", "H\u00f6l\u00b7le", "Dan\u00b7te's", "hat", "mich", "wei\u00b7land", "sehr", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nun gef\u00e4llt mir die Mohammed's, unerh\u00f6rt!", "tokens": ["Und", "nun", "ge\u00b7f\u00e4llt", "mir", "die", "Mo\u00b7ham\u00b7me\u00b7d's", ",", "un\u00b7er\u00b7h\u00f6rt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NE", "$,", "ADJD", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ist minder gr\u00e4\u00dflich ein Gebilde die als jene?", "tokens": ["Ist", "min\u00b7der", "gr\u00e4\u00df\u00b7lich", "ein", "Ge\u00b7bil\u00b7de", "die", "als", "je\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "ART", "NN", "ART", "KOKOM", "PDS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nein, aber weiter ist hinaus gelegt die Scene.", "tokens": ["Nein", ",", "a\u00b7ber", "wei\u00b7ter", "ist", "hin\u00b7aus", "ge\u00b7legt", "die", "Sce\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ADV", "VAFIN", "APZR", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gewaltig heizt er sie, doch macht sie mir nicht hei\u00df,", "tokens": ["Ge\u00b7wal\u00b7tig", "heizt", "er", "sie", ",", "doch", "macht", "sie", "mir", "nicht", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil ich sie nicht bestimmt f\u00fcr meinesgleichen wei\u00df.", "tokens": ["Weil", "ich", "sie", "nicht", "be\u00b7stimmt", "f\u00fcr", "mei\u00b7nes\u00b7glei\u00b7chen", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVFIN", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Zwar hat er grade f\u00fcr Ungl\u00e4ubige sie bestimmt,", "tokens": ["Zwar", "hat", "er", "gra\u00b7de", "f\u00fcr", "Un\u00b7gl\u00e4u\u00b7bi\u00b7ge", "sie", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch mu\u00df ein Gl\u00e4ubiger schon seyn, wer sie annimmt.", "tokens": ["Doch", "mu\u00df", "ein", "Gl\u00e4u\u00b7bi\u00b7ger", "schon", "seyn", ",", "wer", "sie", "an\u00b7nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ADV", "VAINF", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}