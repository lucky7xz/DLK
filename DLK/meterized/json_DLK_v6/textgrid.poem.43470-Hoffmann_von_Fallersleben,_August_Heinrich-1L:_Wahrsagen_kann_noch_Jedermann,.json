{"textgrid.poem.43470": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wahrsagen kann noch Jedermann,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wahrsagen kann noch Jedermann,", "tokens": ["Wahr\u00b7sa\u00b7gen", "kann", "noch", "Je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch willst du Wahrheit sagen,", "tokens": ["Doch", "willst", "du", "Wahr\u00b7heit", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So kommt die Polizei heran", "tokens": ["So", "kommt", "die", "Po\u00b7li\u00b7zei", "he\u00b7ran"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fa\u00dft dich gleich beim Kragen.", "tokens": ["Und", "fa\u00dft", "dich", "gleich", "beim", "Kra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wahrsagen steht noch jedem frei,", "tokens": ["Wahr\u00b7sa\u00b7gen", "steht", "noch", "je\u00b7dem", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch mit der Wahrheit ist's vorbei.", "tokens": ["Doch", "mit", "der", "Wahr\u00b7heit", "ist's", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So h\u00f6rt mich an, ich sage wahr", "tokens": ["So", "h\u00f6rt", "mich", "an", ",", "ich", "sa\u00b7ge", "wahr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sag's mit d\u00fcrren Worten,", "tokens": ["Und", "sag's", "mit", "d\u00fcr\u00b7ren", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sag' es heut' und immerdar", "tokens": ["Und", "sag'", "es", "heut'", "und", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sag' es aller Orten:", "tokens": ["Und", "sag'", "es", "al\u00b7ler", "Or\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit der Censur und Polizei", "tokens": ["Mit", "der", "Cen\u00b7sur", "und", "Po\u00b7li\u00b7zei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Ist es, so Gott will, bald vorbei.", "tokens": ["Ist", "es", ",", "so", "Gott", "will", ",", "bald", "vor\u00b7bei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ADV", "NN", "VMFIN", "$,", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}