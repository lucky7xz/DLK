{"textgrid.poem.48710": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "11. An Deutschland, nach Vernehmung des Todesfalles Herrn Opitzens", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bis hieher und an uns hast du nur einen Preis,", "tokens": ["Bis", "hie\u00b7her", "und", "an", "uns", "hast", "du", "nur", "ei\u00b7nen", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PAV", "KON", "APPR", "PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "o gro\u00dfes Vaterland, da\u00df man so weit der Erden", "tokens": ["o", "gro\u00b7\u00dfes", "Va\u00b7ter\u00b7land", ",", "da\u00df", "man", "so", "weit", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "ADJA", "NN", "$,", "KOUS", "PIS", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "gevierter Kreis bisher gewu\u00dft hat k\u00f6nnen werden,", "tokens": ["ge\u00b7vier\u00b7ter", "Kreis", "bis\u00b7her", "ge\u00b7wu\u00dft", "hat", "k\u00f6n\u00b7nen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVPP", "VAFIN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "so Ritterliches nichts, als dich zu nennen wei\u00df.", "tokens": ["so", "Rit\u00b7ter\u00b7li\u00b7ches", "nichts", ",", "als", "dich", "zu", "nen\u00b7nen", "wei\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "PIS", "$,", "KOUS", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Di\u00df ist dein eigner Ruhm f\u00fcr deinen eignen Schwei\u00df,", "tokens": ["Di\u00df", "ist", "dein", "eig\u00b7ner", "Ruhm", "f\u00fcr", "dei\u00b7nen", "eig\u00b7nen", "Schwei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df Rom, da\u00df Spanien, da\u00df aller V\u00f6lker Herden,", "tokens": ["da\u00df", "Rom", ",", "da\u00df", "Spa\u00b7ni\u00b7en", ",", "da\u00df", "al\u00b7ler", "V\u00f6l\u00b7ker", "Her\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "KOUS", "NE", "$,", "KOUS", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die sich an dich gewagt, f\u00fcr dir und deinen Pferden", "tokens": ["die", "sich", "an", "dich", "ge\u00b7wagt", ",", "f\u00fcr", "dir", "und", "dei\u00b7nen", "Pfer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRELS", "PRF", "APPR", "PPER", "VVPP", "$,", "APPR", "PPER", "KON", "PPOSAT", "NN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "bestunden besser nicht als ein gew\u00e4rmtes Eis.", "tokens": ["be\u00b7stun\u00b7den", "bes\u00b7ser", "nicht", "als", "ein", "ge\u00b7w\u00e4rm\u00b7tes", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PTKNEG", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Weiln denn (unangesehn, wie lange du den Ruhm", "tokens": ["Weiln", "denn", "(", "un\u00b7an\u00b7ge\u00b7sehn", ",", "wie", "lan\u00b7ge", "du", "den", "Ruhm"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "$(", "ADJD", "$,", "PWAV", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "des Unbezwungnen hast, dein ewigs Eigentum)", "tokens": ["des", "Un\u00b7be\u00b7zwung\u00b7nen", "hast", ",", "dein", "e\u00b7wigs", "Ei\u00b7gen\u00b7tum", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "du durch nichts Zeitlichs itzt zu brechen wirst gezwungen,", "tokens": ["du", "durch", "nichts", "Zeit\u00b7lichs", "itzt", "zu", "bre\u00b7chen", "wirst", "ge\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "NN", "ADV", "PTKZU", "VVINF", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "und nun auch ", "tokens": ["und", "nun", "auch"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "so falle froh und frei und, der du bist, ein Held.", "tokens": ["so", "fal\u00b7le", "froh", "und", "frei", "und", ",", "der", "du", "bist", ",", "ein", "Held", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "ADJD", "KON", "$,", "PRELS", "PPER", "VAFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du hast aufs Edelste gerungen und gesungen.", "tokens": ["Du", "hast", "aufs", "E\u00b7dels\u00b7te", "ge\u00b7run\u00b7gen", "und", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}