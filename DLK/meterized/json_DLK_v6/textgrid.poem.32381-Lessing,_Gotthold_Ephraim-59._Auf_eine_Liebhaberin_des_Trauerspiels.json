{"textgrid.poem.32381": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "59. Auf eine Liebhaberin des Trauerspiels", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00f6re, Freund, dein ernstes, sch\u00f6nes Kind", "tokens": ["Ich", "h\u00f6\u00b7re", ",", "Freund", ",", "dein", "erns\u00b7tes", ",", "sch\u00f6\u00b7nes", "Kind"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Will sich des Lachens ganz entw\u00f6hnen,", "tokens": ["Will", "sich", "des", "La\u00b7chens", "ganz", "ent\u00b7w\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "K\u00f6mmt in den Schauplatz nur, wenn s\u00fc\u00dfe Tr\u00e4nen", "tokens": ["K\u00f6mmt", "in", "den", "Schau\u00b7platz", "nur", ",", "wenn", "s\u00fc\u00b7\u00dfe", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "$,", "KOUS", "ADJA", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Da zu vergie\u00dfen sind. \u2013", "tokens": ["Da", "zu", "ver\u00b7gie\u00b7\u00dfen", "sind", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VAFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie? fehlt es ihr bereits an sch\u00f6nen Z\u00e4hnen?", "tokens": ["Wie", "?", "fehlt", "es", "ihr", "be\u00b7reits", "an", "sch\u00f6\u00b7nen", "Z\u00e4h\u00b7nen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}