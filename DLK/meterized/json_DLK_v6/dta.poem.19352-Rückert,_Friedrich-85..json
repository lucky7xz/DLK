{"dta.poem.19352": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "85.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer ist freigebig? Wer, dis sagt das Wort, gibt frei,", "tokens": ["Wer", "ist", "frei\u00b7ge\u00b7big", "?", "Wer", ",", "dis", "sagt", "das", "Wort", ",", "gibt", "frei", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "$.", "PWS", "$,", "PDS", "VVFIN", "ART", "NN", "$,", "VVFIN", "ADJD", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Frei, ohne da\u00df er selbst dazu gezwungen sei,", "tokens": ["Frei", ",", "oh\u00b7ne", "da\u00df", "er", "selbst", "da\u00b7zu", "ge\u00b7zwun\u00b7gen", "sei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "KOUS", "PPER", "ADV", "PAV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gezwungen weder durch Gewalt, noch R\u00fccksicht auch,", "tokens": ["Ge\u00b7zwun\u00b7gen", "we\u00b7der", "durch", "Ge\u00b7walt", ",", "noch", "R\u00fcck\u00b7sicht", "auch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "APPR", "NN", "$,", "ADV", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die gleichgewaltig ist, auf Ruhm, Stand oder Brauch.", "tokens": ["Die", "gleich\u00b7ge\u00b7wal\u00b7tig", "ist", ",", "auf", "Ruhm", ",", "Stand", "o\u00b7der", "Brauch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Freigebig ist, dem Wort wohnt dieser Sinn auch bei,", "tokens": ["Frei\u00b7ge\u00b7big", "ist", ",", "dem", "Wort", "wohnt", "die\u00b7ser", "Sinn", "auch", "bei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "ART", "NN", "VVFIN", "PDAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Wer den Unfreien gibt, den Schuldverbundnen, frei.", "tokens": ["Wer", "den", "Un\u00b7frei\u00b7en", "gibt", ",", "den", "Schuld\u00b7ver\u00b7bund\u00b7nen", ",", "frei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "ART", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Freigebig ist, wer frei dir gibt, da\u00df, wie du magst,", "tokens": ["Frei\u00b7ge\u00b7big", "ist", ",", "wer", "frei", "dir", "gibt", ",", "da\u00df", ",", "wie", "du", "magst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "PWS", "ADJD", "PPER", "VVFIN", "$,", "KOUS", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Du hinnimmst, was er gibt, Dank oder nicht ihm sagst.", "tokens": ["Du", "hin\u00b7nimmst", ",", "was", "er", "gibt", ",", "Dank", "o\u00b7der", "nicht", "ihm", "sagst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "NN", "KON", "PTKNEG", "PPER", "VVFIN", "$."], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Freigebig, wer als Mann, als freier, kund sich gibt", "tokens": ["Frei\u00b7ge\u00b7big", ",", "wer", "als", "Mann", ",", "als", "frei\u00b7er", ",", "kund", "sich", "gibt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "PWS", "KOUS", "NN", "$,", "KOUS", "ADJD", "$,", "PTKVZ", "PRF", "VVFIN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Durch Geben, weil er kann hingeben, was er liebt.", "tokens": ["Durch", "Ge\u00b7ben", ",", "weil", "er", "kann", "hin\u00b7ge\u00b7ben", ",", "was", "er", "liebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Denn Sklav' ist seines Guts, wers nicht hingeben kann;", "tokens": ["Denn", "Skla\u00b7v'", "ist", "sei\u00b7nes", "Guts", ",", "wers", "nicht", "hin\u00b7ge\u00b7ben", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPOSAT", "NN", "$,", "PWS", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Frei f\u00fchlt sich vom Besitz nur der freigeb'ge Mann.", "tokens": ["Frei", "f\u00fchlt", "sich", "vom", "Be\u00b7sitz", "nur", "der", "frei\u00b7ge\u00b7b'\u00b7ge", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPRART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}