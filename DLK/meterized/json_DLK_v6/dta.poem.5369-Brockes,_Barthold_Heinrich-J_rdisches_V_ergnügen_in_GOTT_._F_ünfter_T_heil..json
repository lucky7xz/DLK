{"dta.poem.5369": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch f\u00fcr die, o ew\u2019ge Liebe! meine Seufzer auszusch\u00fctten,", "tokens": ["Auch", "f\u00fcr", "die", ",", "o", "ew'\u00b7ge", "Lie\u00b7be", "!", "mei\u00b7ne", "Seuf\u00b7zer", "aus\u00b7zu\u00b7sch\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "$,", "FM", "ADJA", "NN", "$.", "PPOSAT", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und, von dir, Barmhertzigkeit f\u00fcr die Armen zu erbitten.", "tokens": ["Und", ",", "von", "dir", ",", "Barm\u00b7hert\u00b7zig\u00b7keit", "f\u00fcr", "die", "Ar\u00b7men", "zu", "er\u00b7bit\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "PPER", "$,", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ach, wie mancher Mensch mu\u00df sich jetzt auf harten Boden", "tokens": ["Ach", ",", "wie", "man\u00b7cher", "Mensch", "mu\u00df", "sich", "jetzt", "auf", "har\u00b7ten", "Bo\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "PIAT", "NN", "VMFIN", "PRF", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "legen,", "tokens": ["le\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Drauf er, sonder Dach und Fach, sich f\u00fcr K\u00e4lte, Sturm", "tokens": ["Drauf", "er", ",", "son\u00b7der", "Dach", "und", "Fach", ",", "sich", "f\u00fcr", "K\u00e4l\u00b7te", ",", "Sturm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PAV", "PPER", "$,", "KON", "NN", "KON", "NN", "$,", "PRF", "APPR", "NN", "$,", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "und Regen", "tokens": ["und", "Re\u00b7gen"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Kaum mit alten Lumpen sch\u00fctzet! da ich, mit Beqvemlichkeit,", "tokens": ["Kaum", "mit", "al\u00b7ten", "Lum\u00b7pen", "sch\u00fct\u00b7zet", "!", "da", "ich", ",", "mit", "Be\u00b7qvem\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$.", "KOUS", "PPER", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "In so weichen Feder-Decken mich gem\u00e4chlich legen kann;", "tokens": ["In", "so", "wei\u00b7chen", "Fe\u00b7der\u00b7De\u00b7cken", "mich", "ge\u00b7m\u00e4ch\u00b7lich", "le\u00b7gen", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Ach erbarm dich ihrer auch, sieh\u2019 ihr Elend gn\u00e4dig an!", "tokens": ["Ach", "er\u00b7barm", "dich", "ih\u00b7rer", "auch", ",", "sieh'", "ihr", "E\u00b7lend", "gn\u00e4\u00b7dig", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJD", "PPER", "PPOSAT", "ADV", "$,", "VVFIN", "PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Schencke wenigstens Gedult! und la\u00df, nebst Gelassenheit,", "tokens": ["Schen\u00b7cke", "we\u00b7nigs\u00b7tens", "Ge\u00b7dult", "!", "und", "la\u00df", ",", "nebst", "Ge\u00b7las\u00b7sen\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "$.", "KON", "VVIMP", "$,", "APPR", "NN", "$,"], "meter": "+-+---+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Die Gewohnheit ihre Plagen, wo nicht gantz und gar", "tokens": ["Die", "Ge\u00b7wohn\u00b7heit", "ih\u00b7re", "Pla\u00b7gen", ",", "wo", "nicht", "gantz", "und", "gar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PWAV", "PTKNEG", "ADV", "KON", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.12": {"text": "vermindern,", "tokens": ["ver\u00b7min\u00b7dern", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Doch in Hofnung und Vertrauen, wenigstens in etwas", "tokens": ["Doch", "in", "Hof\u00b7nung", "und", "Ver\u00b7trau\u00b7en", ",", "we\u00b7nigs\u00b7tens", "in", "et\u00b7was"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN", "$,", "ADV", "APPR", "PIS"], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "lindern!", "tokens": ["lin\u00b7dern", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Nun so schlie\u00df ich meiner Augen Schlaf-begier\u2019ge Lieder zu,", "tokens": ["Nun", "so", "schlie\u00df", "ich", "mei\u00b7ner", "Au\u00b7gen", "Schlaf\u00b7be\u00b7gier'\u00b7ge", "Lie\u00b7der", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Und, indem die m\u00fcden Glieder, sammt den matt-gewor-", "tokens": ["Und", ",", "in\u00b7dem", "die", "m\u00fc\u00b7den", "Glie\u00b7der", ",", "sammt", "den", "mat\u00b7tge\u00b7wor"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "ART", "ADJA", "NN", "$,", "VVFIN", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.17": {"text": "dnen Sehnen,", "tokens": ["dnen", "Seh\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.18": {"text": "Sich, mit einer neuen Lust, sanfte von einander dehnen,", "tokens": ["Sich", ",", "mit", "ei\u00b7ner", "neu\u00b7en", "Lust", ",", "sanf\u00b7te", "von", "ein\u00b7an\u00b7der", "deh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$,", "APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "PRF", "PDS", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Denck\u2019 ich, mich in dich versenckend, auf die Lust der ew\u2019-", "tokens": ["Denck", "ich", ",", "mich", "in", "dich", "ver\u00b7sen\u00b7ckend", ",", "auf", "die", "Lust", "der", "ew'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "PRF", "APPR", "PPER", "VVPP", "$,", "APPR", "ART", "NN", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.20": {"text": "gen Ruh.", "tokens": ["gen", "Ruh", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}