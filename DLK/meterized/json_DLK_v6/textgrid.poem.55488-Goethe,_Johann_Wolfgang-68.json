{"textgrid.poem.55488": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "68", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verirrtes B\u00fcchlein! kannst unsichre Tritte", "tokens": ["Ver\u00b7irr\u00b7tes", "B\u00fcch\u00b7lein", "!", "kannst", "un\u00b7sich\u00b7re", "Trit\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da- oder dorthin keineswegs vermeiden;", "tokens": ["Da", "o\u00b7der", "dor\u00b7thin", "kei\u00b7nes\u00b7wegs", "ver\u00b7mei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Irrsternen zu bewegst du deine Schritte,", "tokens": ["Irrs\u00b7ter\u00b7nen", "zu", "be\u00b7wegst", "du", "dei\u00b7ne", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und vor dem Kommen bist bereit zu scheiden.", "tokens": ["Und", "vor", "dem", "Kom\u00b7men", "bist", "be\u00b7reit", "zu", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr diesmal aber wollen wir dich fesseln,", "tokens": ["F\u00fcr", "dies\u00b7mal", "a\u00b7ber", "wol\u00b7len", "wir", "dich", "fes\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du sollst mir diese Botschaft nicht verfehlen;", "tokens": ["Du", "sollst", "mir", "die\u00b7se", "Bot\u00b7schaft", "nicht", "ver\u00b7feh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PDAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sei es durch Rosen, Dornen, Veilchen, Nesseln,", "tokens": ["Sei", "es", "durch", "Ro\u00b7sen", ",", "Dor\u00b7nen", ",", "Veil\u00b7chen", ",", "Nes\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nur immer grade zu, geh zu Adelen!", "tokens": ["Nur", "im\u00b7mer", "gra\u00b7de", "zu", ",", "geh", "zu", "A\u00b7de\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PTKVZ", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}}}}}