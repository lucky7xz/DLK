{"textgrid.poem.36304": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An Novalis", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O la\u00df mich, lieber Freund, nicht l\u00e4nger leiden,", "tokens": ["O", "la\u00df", "mich", ",", "lie\u00b7ber", "Freund", ",", "nicht", "l\u00e4n\u00b7ger", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "ADV", "NN", "$,", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df wieder friedlich mich dein Wort erfreue,", "tokens": ["Da\u00df", "wie\u00b7der", "fried\u00b7lich", "mich", "dein", "Wort", "er\u00b7freu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vergangenheitsgespr\u00e4ch sich uns erneue,", "tokens": ["Ver\u00b7gan\u00b7gen\u00b7heits\u00b7ge\u00b7spr\u00e4ch", "sich", "uns", "er\u00b7neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Augen an der Augen Licht sich weiden.", "tokens": ["Die", "Au\u00b7gen", "an", "der", "Au\u00b7gen", "Licht", "sich", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie konnt'st, mein ander Ich, du von mir scheiden?", "tokens": ["Wie", "konnt'st", ",", "mein", "an\u00b7der", "Ich", ",", "du", "von", "mir", "schei\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "$,", "PPOSAT", "ADJD", "PPER", "$,", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du strahlst in heiterm Frieden, fern von Reue,", "tokens": ["Du", "strahlst", "in", "hei\u00b7term", "Frie\u00b7den", ",", "fern", "von", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich bin derselbe noch in gleicher Treue,", "tokens": ["Ich", "bin", "der\u00b7sel\u00b7be", "noch", "in", "glei\u00b7cher", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDAT", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur Freude mu\u00df den Freundelosen meiden.", "tokens": ["Nur", "Freu\u00b7de", "mu\u00df", "den", "Freun\u00b7de\u00b7lo\u00b7sen", "mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Musik, unsterbliche, die sprachst du sterbend,", "tokens": ["Mu\u00b7sik", ",", "uns\u00b7terb\u00b7li\u00b7che", ",", "die", "sprachst", "du", "ster\u00b7bend", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "PRELS", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Mir ist der Mund verstummt in Herzens Sehnen,", "tokens": ["Mir", "ist", "der", "Mund", "ver\u00b7stummt", "in", "Her\u00b7zens", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In \u00d6d' allein mit mir und meiner Liebe;", "tokens": ["In", "\u00d6d'", "al\u00b7lein", "mit", "mir", "und", "mei\u00b7ner", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nach dir sich dr\u00e4ngen, streben alle Triebe,", "tokens": ["Nach", "dir", "sich", "dr\u00e4n\u00b7gen", ",", "stre\u00b7ben", "al\u00b7le", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "VVFIN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein liebes Wort, ich h\u00f6r' es noch im W\u00e4hnen,", "tokens": ["Dein", "lie\u00b7bes", "Wort", ",", "ich", "h\u00f6r'", "es", "noch", "im", "W\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus aller Jugend die Erinn'rung erbend.", "tokens": ["Aus", "al\u00b7ler", "Ju\u00b7gend", "die", "Er\u00b7inn'\u00b7rung", "er\u00b7bend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}