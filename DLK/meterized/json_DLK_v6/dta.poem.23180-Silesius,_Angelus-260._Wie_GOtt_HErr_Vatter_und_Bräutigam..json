{"dta.poem.23180": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "260. Wie GOtt/ HErr/ Vatter/ und  \n Br\u00e4utigam.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Den Knechten ist Gott HErr/ dir Vatter wo du Kind/", "tokens": ["Den", "Knech\u00b7ten", "ist", "Gott", "Herr", "/", "dir", "Vat\u00b7ter", "wo", "du", "Kind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "NN", "$(", "PPER", "NN", "PWAV", "PPER", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mir ist Er Br\u00e4utigam/ wenn er mich Jungfrau findt.", "tokens": ["Mir", "ist", "Er", "Br\u00e4u\u00b7ti\u00b7gam", "/", "wenn", "er", "mich", "Jung\u00b7frau", "findt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NE", "$(", "KOUS", "PPER", "PRF", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}