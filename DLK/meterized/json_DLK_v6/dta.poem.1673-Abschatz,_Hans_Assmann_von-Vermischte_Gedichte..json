{"dta.poem.1673": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Man findet wenig Pracht in alten Adels Schilden/ ", "tokens": ["Man", "fin\u00b7det", "we\u00b7nig", "Pracht", "in", "al\u00b7ten", "A\u00b7dels", "Schil\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIAT", "NN", "APPR", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Ahnen tapffre Faust hat solche selbst gemahlt/", "tokens": ["Der", "Ah\u00b7nen", "tapf\u00b7fre", "Faust", "hat", "sol\u00b7che", "selbst", "ge\u00b7mahlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "PIAT", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Denckmahl ihres Sinns den Erben einzubilden/", "tokens": ["Ein", "Denck\u00b7mahl", "ih\u00b7res", "Sinns", "den", "Er\u00b7ben", "ein\u00b7zu\u00b7bil\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ART", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da izt die neue Welt mit vielem Schmucke prahlt.", "tokens": ["Da", "izt", "die", "neu\u00b7e", "Welt", "mit", "vie\u00b7lem", "Schmu\u00b7cke", "prahlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "APPR", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo man den Vogel kan aus seinen Federn kennen/", "tokens": ["Wo", "man", "den", "Vo\u00b7gel", "kan", "aus", "sei\u00b7nen", "Fe\u00b7dern", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So mag man manchen wohl des Adels Mi\u00dfbrutt nennen.", "tokens": ["So", "mag", "man", "man\u00b7chen", "wohl", "des", "A\u00b7dels", "Mi\u00df\u00b7brutt", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PIAT", "ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Alten Einfalt wie\u00df die ungef\u00e4lschten Sitten", "tokens": ["Der", "Al\u00b7ten", "Ein\u00b7falt", "wie\u00df", "die", "un\u00b7ge\u00b7f\u00e4lschten", "Sit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit schlechten Farben aus: das unbefleckte Wei\u00df", "tokens": ["Mit", "schlech\u00b7ten", "Far\u00b7ben", "aus", ":", "das", "un\u00b7be\u00b7fleck\u00b7te", "Wei\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die rothe Tapfferkeit/ die manchen Sieg erstritten/", "tokens": ["Die", "ro\u00b7the", "Tapf\u00b7fer\u00b7keit", "/", "die", "man\u00b7chen", "Sieg", "er\u00b7strit\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das treu-best\u00e4ndge Schwartz behielt damahls den Prei\u00df.", "tokens": ["Das", "treu\u00b7be\u00b7st\u00e4nd\u00b7ge", "Schwartz", "be\u00b7hielt", "da\u00b7mahls", "den", "Prei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Izt mu\u00df offt alles voll und bund gemahlet stecken/", "tokens": ["Izt", "mu\u00df", "offt", "al\u00b7les", "voll", "und", "bund", "ge\u00b7mah\u00b7let", "ste\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PIS", "ADJD", "KON", "ADJD", "VVPP", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der \u00e4usserliche Pracht den innern Mangel decken.", "tokens": ["Der", "\u00e4us\u00b7ser\u00b7li\u00b7che", "Pracht", "den", "in\u00b7nern", "Man\u00b7gel", "de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn izt ein Alter solt in seine H\u00f6rner blasen/", "tokens": ["Wenn", "izt", "ein", "Al\u00b7ter", "solt", "in", "sei\u00b7ne", "H\u00f6r\u00b7ner", "bla\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit er seinen Feind zu schrecken war ger\u00fcst/", "tokens": ["Da\u00b7mit", "er", "sei\u00b7nen", "Feind", "zu", "schre\u00b7cken", "war", "ge\u00b7r\u00fcst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und manchen edlen Held auff Blut-gef\u00e4rbten Rasen", "tokens": ["Und", "man\u00b7chen", "ed\u00b7len", "Held", "auff", "Blut\u00b7ge\u00b7f\u00e4rb\u00b7ten", "Ra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zum Streite fodern aus/ zu weisen/ was er ist/", "tokens": ["Zum", "Strei\u00b7te", "fo\u00b7dern", "aus", "/", "zu", "wei\u00b7sen", "/", "was", "er", "ist", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKVZ", "$(", "PTKZU", "VVINF", "$(", "PWS", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie w\u00fcrde Schild und Helm offt auff die Seite fliegen/", "tokens": ["Wie", "w\u00fcr\u00b7de", "Schild", "und", "Helm", "offt", "auff", "die", "Sei\u00b7te", "flie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "KON", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der neu-erworbne Stand und Schmuck im Kothe liegen.", "tokens": ["Der", "neu\u00b7er\u00b7worb\u00b7ne", "Stand", "und", "Schmuck", "im", "Ko\u00b7the", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch mu\u00df der Ahnen Rauch nicht so zu Kopffe steigen/", "tokens": ["Doch", "mu\u00df", "der", "Ah\u00b7nen", "Rauch", "nicht", "so", "zu", "Kopf\u00b7fe", "stei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "NN", "PTKNEG", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df man beschimpffen will/ was Tugend edel acht/", "tokens": ["Da\u00df", "man", "be\u00b7schimpf\u00b7fen", "will", "/", "was", "Tu\u00b7gend", "e\u00b7del", "acht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "$(", "PWS", "NN", "ADJD", "CARD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Wappen/ nicht der Ruhm der V\u00e4ter/ ist uns eigen.", "tokens": ["Das", "Wap\u00b7pen", "/", "nicht", "der", "Ruhm", "der", "V\u00e4\u00b7ter", "/", "ist", "uns", "ei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PTKNEG", "ART", "NN", "ART", "NN", "$(", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Man setze weiter nach/ wie sie die Bahn gemacht;", "tokens": ["Man", "set\u00b7ze", "wei\u00b7ter", "nach", "/", "wie", "sie", "die", "Bahn", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "$(", "PWAV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wann vieler Jahre Schwei\u00df nicht soll umsonst zerrinnen:", "tokens": ["Wann", "vie\u00b7ler", "Jah\u00b7re", "Schwei\u00df", "nicht", "soll", "um\u00b7sonst", "zer\u00b7rin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "NN", "PTKNEG", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erhalten sch\u00e4tzet man so k\u00fcnstlich als gewinnen.", "tokens": ["Er\u00b7hal\u00b7ten", "sch\u00e4t\u00b7zet", "man", "so", "k\u00fcnst\u00b7lich", "als", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "ADV", "ADJD", "KOKOM", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}