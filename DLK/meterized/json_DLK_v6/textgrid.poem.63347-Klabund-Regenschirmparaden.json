{"textgrid.poem.63347": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Regenschirmparaden", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor unserm Feldmarschall, dem Ruppert:", "tokens": ["Vor", "un\u00b7serm", "Feld\u00b7mar\u00b7schall", ",", "dem", "Rup\u00b7pert", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie manches Heldenherz da puppert.", "tokens": ["Wie", "man\u00b7ches", "Hel\u00b7den\u00b7herz", "da", "pup\u00b7pert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Man sieht mit Schirmen und mit St\u00f6cken", "tokens": ["Man", "sieht", "mit", "Schir\u00b7men", "und", "mit", "St\u00f6\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vorbeimarschier'n die alten Recken.", "tokens": ["Vor\u00b7bei\u00b7mar\u00b7schier'n", "die", "al\u00b7ten", "Re\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Mit achtzig und mit neunzig Jahren", "tokens": ["Mit", "acht\u00b7zig", "und", "mit", "neun\u00b7zig", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "KON", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sind sie von weitem hergefahren,", "tokens": ["Sind", "sie", "von", "wei\u00b7tem", "her\u00b7ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Um mit den wackeligen Gliedern", "tokens": ["Um", "mit", "den", "wa\u00b7cke\u00b7li\u00b7gen", "Glie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den K\u00f6nigsgru\u00df steif zu erwidern.", "tokens": ["Den", "K\u00f6\u00b7nigs\u00b7gru\u00df", "steif", "zu", "er\u00b7wi\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, besser w\u00e4r's, ihr alten Knaben,", "tokens": ["Ach", ",", "bes\u00b7ser", "w\u00e4r's", ",", "ihr", "al\u00b7ten", "Kna\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJD", "VAFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein R\u00fcckgrat \u00fcberhaupt zu haben", "tokens": ["Ein", "R\u00fcck\u00b7grat", "\u00fc\u00b7ber\u00b7haupt", "zu", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PTKZU", "VAINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Leben und daheim im Laden", "tokens": ["Im", "Le\u00b7ben", "und", "da\u00b7heim", "im", "La\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und nicht bei v\u00f6lkischen Paraden.", "tokens": ["Und", "nicht", "bei", "v\u00f6l\u00b7ki\u00b7schen", "Pa\u00b7ra\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn ihr im Feld spazieren tut,", "tokens": ["Wenn", "ihr", "im", "Feld", "spa\u00b7zie\u00b7ren", "tut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zieht ihr da euren Sonntagshut", "tokens": ["Zieht", "ihr", "da", "eu\u00b7ren", "Sonn\u00b7tags\u00b7hut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und reckt ihr euch aus den Gestr\u00e4uchen", "tokens": ["Und", "reckt", "ihr", "euch", "aus", "den", "Ge\u00b7str\u00e4u\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vor den (zum Beispiel) Vogelscheuchen?", "tokens": ["Vor", "den", "(", "zum", "Bei\u00b7spiel", ")", "Vo\u00b7gel\u00b7scheu\u00b7chen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "$(", "APPRART", "NN", "$(", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}