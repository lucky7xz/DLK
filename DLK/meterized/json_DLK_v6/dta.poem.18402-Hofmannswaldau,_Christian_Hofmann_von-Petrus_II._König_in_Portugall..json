{"dta.poem.18402": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Petrus  II.  K\u00f6nig in Portugall.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zeuch wider Franckreich aus/ und zeige mit der that/", "tokens": ["Zeuch", "wi\u00b7der", "Fran\u00b7ck\u00b7reich", "aus", "/", "und", "zei\u00b7ge", "mit", "der", "that", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "PTKVZ", "$(", "KON", "VVFIN", "APPR", "ART", "VVFIN", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df auch dein Portugall noch helden in sich hat;", "tokens": ["Da\u00df", "auch", "dein", "Por\u00b7tu\u00b7gall", "noch", "hel\u00b7den", "in", "sich", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ADV", "NN", "APPR", "PRF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man m\u00f6chte sonst von dir in den Geschichten schreiben:", "tokens": ["Man", "m\u00f6ch\u00b7te", "sonst", "von", "dir", "in", "den", "Ge\u00b7schich\u00b7ten", "schrei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "APPR", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du k\u00f6ntest weiter nichts/ als br\u00fcder nur vertreiben.", "tokens": ["Du", "k\u00f6n\u00b7test", "wei\u00b7ter", "nichts", "/", "als", "br\u00fc\u00b7der", "nur", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word",