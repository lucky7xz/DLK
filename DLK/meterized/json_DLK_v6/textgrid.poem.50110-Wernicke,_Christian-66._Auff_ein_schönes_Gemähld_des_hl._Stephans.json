{"textgrid.poem.50110": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "66. Auff ein sch\u00f6nes Gem\u00e4hld des hl. Stephans", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich den theuren Mann so wol find' abgebildt,", "tokens": ["Wenn", "ich", "den", "theu\u00b7ren", "Mann", "so", "wol", "find'", "ab\u00b7ge\u00b7bildt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "ADV", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So r\u00fchrt mehr als sein ", "tokens": ["So", "r\u00fchrt", "mehr", "als", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "KOKOM", "PPOSAT"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und Stephan's ", "tokens": ["Und", "Ste\u00b7phan's"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Doch d\u00fcnckt mich auch, wenn ich den Himmel offen find',", "tokens": ["Doch", "d\u00fcnckt", "mich", "auch", ",", "wenn", "ich", "den", "Him\u00b7mel", "of\u00b7fen", "find'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihn zu ", "tokens": ["Ihn", "zu"], "token_info": ["word", "word"], "pos": ["PPER", "PTKZU"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Und dass die ", "tokens": ["Und", "dass", "die"], "token_info": ["word", "word", "word"], "pos": ["KON", "KOUS", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "So sch\u00f6n als ", "tokens": ["So", "sch\u00f6n", "als"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM"], "meter": "-+-", "measure": "amphibrach.single"}}}}}