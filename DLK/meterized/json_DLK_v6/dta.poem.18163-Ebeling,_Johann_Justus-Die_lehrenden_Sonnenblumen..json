{"dta.poem.18163": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n lehrenden Sonnenblumen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sonnenblumen! eure Lehren,", "tokens": ["Son\u00b7nen\u00b7blu\u00b7men", "!", "eu\u00b7re", "Leh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ich jetzo sehn und h\u00f6ren,", "tokens": ["Will", "ich", "jet\u00b7zo", "sehn", "und", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ihr in verg\u00fcldter Pracht", "tokens": ["Die", "ihr", "in", "ver\u00b7g\u00fcld\u00b7ter", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem rund geformten Kreise", "tokens": ["Mit", "dem", "rund", "ge\u00b7form\u00b7ten", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nach der Sonnen Licht und Reise", "tokens": ["Nach", "der", "Son\u00b7nen", "Licht", "und", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Euren Stand und Wendung macht.", "tokens": ["Eu\u00b7ren", "Stand", "und", "Wen\u00b7dung", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Stellet man euch vors Gesichte,", "tokens": ["Stel\u00b7let", "man", "euch", "vors", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "APPRART", "NN", "$,"], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.8": {"text": "So k\u00f6nt ihr dem Himmels Lichte", "tokens": ["So", "k\u00f6nt", "ihr", "dem", "Him\u00b7mels", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "An Figur und an dem Schein", "tokens": ["An", "Fi\u00b7gur", "und", "an", "dem", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "ART", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.10": {"text": "An dem Majest\u00e4tschen Prangen,", "tokens": ["An", "dem", "Ma\u00b7jes\u00b7t\u00e4t\u00b7schen", "Pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Auf den gr\u00fcn bewachsnen Stangen", "tokens": ["Auf", "den", "gr\u00fcn", "be\u00b7wachs\u00b7nen", "Stan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wo nicht gleich doch \u00e4hnlich sein.", "tokens": ["Wo", "nicht", "gleich", "doch", "\u00e4hn\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Auf der Erden Lustgefilde", "tokens": ["Auf", "der", "Er\u00b7den", "Lust\u00b7ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Traget ihr der Sonnenbilde", "tokens": ["Tra\u00b7get", "ihr", "der", "Son\u00b7nen\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Dieses m\u00fcssen wir gestehn:", "tokens": ["Die\u00b7ses", "m\u00fcs\u00b7sen", "wir", "ge\u00b7stehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "M\u00f6chten wir uns auch befleissen,", "tokens": ["M\u00f6ch\u00b7ten", "wir", "uns", "auch", "be\u00b7fleis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Da wir ", "tokens": ["Da", "wir"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Da\u00df sein Bild an uns zu sehn.", "tokens": ["Da\u00df", "sein", "Bild", "an", "uns", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Jhr heist recht die Sonnenwende", "tokens": ["Ihr", "heist", "recht", "die", "Son\u00b7nen\u00b7wen\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Weil ihr mit dem spizzen Ende", "tokens": ["Weil", "ihr", "mit", "dem", "spiz\u00b7zen", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Euch zur Mittags Gegend neigt,", "tokens": ["Euch", "zur", "Mit\u00b7tags", "Ge\u00b7gend", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Dadurch wird, wie wir auf Erden,", "tokens": ["Da\u00b7durch", "wird", ",", "wie", "wir", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "$,", "PWAV", "PPER", "APPR", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.23": {"text": "Auch schon sollen himlisch werden", "tokens": ["Auch", "schon", "sol\u00b7len", "him\u00b7lisch", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "ADJD", "VAINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Uns zur Lehre angezeigt.", "tokens": ["Uns", "zur", "Leh\u00b7re", "an\u00b7ge\u00b7zeigt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "M\u00f6chten sich der Menschen Sinne,", "tokens": ["M\u00f6ch\u00b7ten", "sich", "der", "Men\u00b7schen", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Zu dem HErrn der Himmels Zinne", "tokens": ["Zu", "dem", "Herrn", "der", "Him\u00b7mels", "Zin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Also lenken wie die Blum", "tokens": ["Al\u00b7so", "len\u00b7ken", "wie", "die", "Blum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "KOKOM", "ART", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Ach! so w\u00fcrden unsre Seelen,", "tokens": ["Ach", "!", "so", "w\u00fcr\u00b7den", "uns\u00b7re", "See\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Sich mit ", "tokens": ["Sich", "mit"], "token_info": ["word", "word"], "pos": ["PRF", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.30": {"text": "Der ihr h\u00f6chstes Eigenthum.", "tokens": ["Der", "ihr", "h\u00f6chs\u00b7tes", "Ei\u00b7gen\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Euer Nahm stammt von der Sonne,", "tokens": ["Eu\u00b7er", "Nahm", "stammt", "von", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.32": {"text": "Weil ihr euch mit eurer Wonne,", "tokens": ["Weil", "ihr", "euch", "mit", "eu\u00b7rer", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Nach derselben Strahlen lenkt:", "tokens": ["Nach", "der\u00b7sel\u00b7ben", "Strah\u00b7len", "lenkt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "Nahm und That stimmt hier zusammen", "tokens": ["Nahm", "und", "That", "stimmt", "hier", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Wenn bei einen Christen Nahmen", "tokens": ["Wenn", "bei", "ei\u00b7nen", "Chris\u00b7ten", "Nah\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Dies ein Christe auch bedenkt:", "tokens": ["Dies", "ein", "Chris\u00b7te", "auch", "be\u00b7denkt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Ach! so w\u00fcrden auf der Erden", "tokens": ["Ach", "!", "so", "w\u00fcr\u00b7den", "auf", "der", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Nicht so viel gefunden werden", "tokens": ["Nicht", "so", "viel", "ge\u00b7fun\u00b7den", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADV", "VVPP", "VAINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Die nur Christen nach den Schein:", "tokens": ["Die", "nur", "Chris\u00b7ten", "nach", "den", "Schein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Aber durch ihr ganzes Leben,", "tokens": ["A\u00b7ber", "durch", "ihr", "gan\u00b7zes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Deutlich zu verstehen geben,", "tokens": ["Deut\u00b7lich", "zu", "ver\u00b7ste\u00b7hen", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Da\u00df sie warlich Heiden sein.", "tokens": ["Da\u00df", "sie", "war\u00b7lich", "Hei\u00b7den", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}