{"textgrid.poem.62655": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Uber die Aufnahme Sr. Hochwohlgebohrnen, Herrn Ludewig Wilhelm von Langenau,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Musen! la\u00dft mich jetzt nicht schweigen!", "tokens": ["Ihr", "Mu\u00b7sen", "!", "la\u00dft", "mich", "jetzt", "nicht", "schwei\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVIMP", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kommt! stehet mir mit Nachdruck bey,", "tokens": ["Kommt", "!", "ste\u00b7het", "mir", "mit", "Nach\u00b7druck", "bey", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich geschickt und: f\u00e4hig sey,", "tokens": ["Da\u00df", "ich", "ge\u00b7schickt", "und", ":", "f\u00e4\u00b7hig", "sey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "KON", "$.", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von eurer S\u00f6hne Ruhm zu zeugen.", "tokens": ["Von", "eu\u00b7rer", "S\u00f6h\u00b7ne", "Ruhm", "zu", "zeu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Trinkt mir aus eurem Brunnen zu,", "tokens": ["Trinkt", "mir", "aus", "eu\u00b7rem", "Brun\u00b7nen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erquickt die matten Lebens-S\u00e4fte,", "tokens": ["Er\u00b7quickt", "die", "mat\u00b7ten", "Le\u00b7bens\u00b7S\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und gebt mir Feuer, Muth und Kr\u00e4fte,", "tokens": ["Und", "gebt", "mir", "Feu\u00b7er", ",", "Muth", "und", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df ich nach Wunsch und Pflichten thu.", "tokens": ["Da\u00df", "ich", "nach", "Wunsch", "und", "Pflich\u00b7ten", "thu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Kommt! helfet mir vor allen Dingen,", "tokens": ["Kommt", "!", "hel\u00b7fet", "mir", "vor", "al\u00b7len", "Din\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Ehre Schlesiens besiegen.", "tokens": ["Die", "Eh\u00b7re", "Schle\u00b7si\u00b7ens", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du Mutter derer gr\u00f6\u00dften Weisen!", "tokens": ["Du", "Mut\u00b7ter", "de\u00b7rer", "gr\u00f6\u00df\u00b7ten", "Wei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Elysien, du g\u00fcldnes Land!", "tokens": ["E\u00b7ly\u00b7si\u00b7en", ",", "du", "g\u00fcld\u00b7nes", "Land", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Ruhm ist ja schon l\u00e4ngst bekannt;", "tokens": ["Dein", "Ruhm", "ist", "ja", "schon", "l\u00e4ngst", "be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was such ich dich alhier zu preisen?", "tokens": ["Was", "such", "ich", "dich", "al\u00b7hier", "zu", "prei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Ort wird in der Welt gesehn,", "tokens": ["Kein", "Ort", "wird", "in", "der", "Welt", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In welchem so ber\u00fchmte Lichter,", "tokens": ["In", "wel\u00b7chem", "so", "be\u00b7r\u00fchm\u00b7te", "Lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So gro\u00df- und Feuer-reiche Dichter,", "tokens": ["So", "gro\u00df", "und", "Feu\u00b7er\u00b7rei\u00b7che", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als hier in Menge auferstehn.", "tokens": ["Als", "hier", "in", "Men\u00b7ge", "auf\u00b7er\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Auf euch, ihr m\u00e4chtigen Sudeten,", "tokens": ["Auf", "euch", ",", "ihr", "m\u00e4ch\u00b7ti\u00b7gen", "Su\u00b7de\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "H\u00f6rt man den Klang der sch\u00f6nsten Fl\u00f6ten.", "tokens": ["H\u00f6rt", "man", "den", "Klang", "der", "sch\u00f6ns\u00b7ten", "Fl\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Du bist wie Deutschland selbst bekennet,", "tokens": ["Du", "bist", "wie", "Deutschland", "selbst", "be\u00b7ken\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sein angenehmer Helicon.", "tokens": ["Sein", "an\u00b7ge\u00b7neh\u00b7mer", "He\u00b7li\u00b7con", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In dir hat Ph\u00f6bus seinen Thron,", "tokens": ["In", "dir", "hat", "Ph\u00f6\u00b7bus", "sei\u00b7nen", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den man nach deinem Namen nennet.", "tokens": ["Den", "man", "nach", "dei\u00b7nem", "Na\u00b7men", "nen\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+--+--", "measure": "dactylic.tri"}, "line.5": {"text": "Es scheint als wenn der Mutter Brust", "tokens": ["Es", "scheint", "als", "wenn", "der", "Mut\u00b7ter", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den ersten Trieb zum Dichten schenkte,", "tokens": ["Den", "ers\u00b7ten", "Trieb", "zum", "Dich\u00b7ten", "schenk\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als ob sie Geist und Sinne lenkte.", "tokens": ["Als", "ob", "sie", "Geist", "und", "Sin\u00b7ne", "lenk\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So kr\u00e4ftig zeigt sich hier die Lust:", "tokens": ["So", "kr\u00e4f\u00b7tig", "zeigt", "sich", "hier", "die", "Lust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PRF", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "In dir, du bist darzu erkohren,", "tokens": ["In", "dir", ",", "du", "bist", "dar\u00b7zu", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "VAFIN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wird mancher G\u00f6tter-Sohn gebohren.", "tokens": ["Wird", "man\u00b7cher", "G\u00f6t\u00b7ter\u00b7Sohn", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Ein Weltber\u00fchmter ", "tokens": ["Ein", "Welt\u00b7be\u00b7r\u00fchm\u00b7ter"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Verg\u00f6ssert deinen Glanz und Schein,", "tokens": ["Ver\u00b7g\u00f6s\u00b7sert", "dei\u00b7nen", "Glanz", "und", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wei\u00df ihn h\u00f6her aufzuf\u00fchren.", "tokens": ["Und", "wei\u00df", "ihn", "h\u00f6\u00b7her", "auf\u00b7zu\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Durch seinen hohen Geist geschenket.", "tokens": ["Durch", "sei\u00b7nen", "ho\u00b7hen", "Geist", "ge\u00b7schen\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ja, wer an ", "tokens": ["Ja", ",", "wer", "an"], "token_info": ["word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "PWS", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Der wei\u00df, wie hoch er dich erhob.", "tokens": ["Der", "wei\u00df", ",", "wie", "hoch", "er", "dich", "er\u00b7hob", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "ADJD", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Ruhm, den ", "tokens": ["Den", "Ruhm", ",", "den"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Ist noch bis jetzo nicht erstorben.", "tokens": ["Ist", "noch", "bis", "jet\u00b7zo", "nicht", "ers\u00b7tor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich werd entz\u00fcckt! mir f\u00e4llt jetzunder", "tokens": ["Ich", "werd", "ent\u00b7z\u00fcckt", "!", "mir", "f\u00e4llt", "je\u00b7tzun\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des grossen ", "tokens": ["Des", "gros\u00b7sen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und fragt der Vorwitz: wer er sey?", "tokens": ["Und", "fragt", "der", "Vor\u00b7witz", ":", "wer", "er", "sey", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$.", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So folgt die Antwort drauf: Ein Wunder!", "tokens": ["So", "folgt", "die", "Ant\u00b7wort", "drauf", ":", "Ein", "Wun\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Wunder bey dem Dichter Chor,", "tokens": ["Ein", "Wun\u00b7der", "bey", "dem", "Dich\u00b7ter", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ja ein Poet, dem keiner gleichet,", "tokens": ["Ja", "ein", "Po\u00b7et", ",", "dem", "kei\u00b7ner", "glei\u00b7chet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und dessen Geist kein Witz erreichet.", "tokens": ["Und", "des\u00b7sen", "Geist", "kein", "Witz", "er\u00b7rei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So steigt Elysien empor!", "tokens": ["So", "steigt", "E\u00b7ly\u00b7si\u00b7en", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nur du weist Dichter aufzubringen.", "tokens": ["Nur", "du", "weist", "Dich\u00b7ter", "auf\u00b7zu\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die z\u00e4rtlich und erhaben singen.", "tokens": ["Die", "z\u00e4rt\u00b7lich", "und", "er\u00b7ha\u00b7ben", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "In Ober- und Nieder-Sachsen", "tokens": ["In", "O\u00b7ber", "und", "Nie\u00b7der\u00b7Sach\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "TRUNC", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gr\u00fcnt zwar auch jetzt der Musen Hayn;", "tokens": ["Gr\u00fcnt", "zwar", "auch", "jetzt", "der", "Mu\u00b7sen", "Hayn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es zeiget uns der Augenschein,", "tokens": ["Es", "zei\u00b7get", "uns", "der", "Au\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was hier vor edle Dichter wachsen.", "tokens": ["Was", "hier", "vor", "ed\u00b7le", "Dich\u00b7ter", "wach\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch Schlesien beh\u00e4lt den Ruhm;", "tokens": ["Doch", "Schle\u00b7si\u00b7en", "be\u00b7h\u00e4lt", "den", "Ruhm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ihrer S\u00f6hne muntre S\u00e4yten", "tokens": ["Da\u00df", "ih\u00b7rer", "S\u00f6h\u00b7ne", "mun\u00b7tre", "S\u00e4y\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den Vorzug aller Welt bestreiten:", "tokens": ["Den", "Vor\u00b7zug", "al\u00b7ler", "Welt", "be\