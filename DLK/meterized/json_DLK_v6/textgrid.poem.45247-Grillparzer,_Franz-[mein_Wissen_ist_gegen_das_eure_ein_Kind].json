{"textgrid.poem.45247": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[mein Wissen ist gegen das eure ein Kind]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Wissen ist gegen das eure ein Kind,", "tokens": ["Mein", "Wis\u00b7sen", "ist", "ge\u00b7gen", "das", "eu\u00b7re", "ein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ART", "PPOSAT", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Fern sei, da\u00df ich es leugne,", "tokens": ["Fern", "sei", ",", "da\u00df", "ich", "es", "leug\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nur da\u00df eure Gedanken fremde sind,", "tokens": ["Nur", "da\u00df", "eu\u00b7re", "Ge\u00b7dan\u00b7ken", "frem\u00b7de", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "NN", "ADJA", "VAFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Die meinen aber eigne.", "tokens": ["Die", "mei\u00b7nen", "a\u00b7ber", "eig\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}