"word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.91": {"text": "Und bleiben, w\u00fcnscht' ich gleich zu gehen,", "tokens": ["Und", "blei\u00b7ben", ",", "w\u00fcnscht'", "ich", "gleich", "zu", "ge\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.92": {"text": "Und lachen, wenn ich traurig bin,", "tokens": ["Und", "la\u00b7chen", ",", "wenn", "ich", "trau\u00b7rig", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.93": {"text": "Und, was mein Lachen reitzt, beklagen,", "tokens": ["Und", ",", "was", "mein", "La\u00b7chen", "reitzt", ",", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.94": {"text": "Und trotz der vielen Gall' im Magen,", "tokens": ["Und", "trotz", "der", "vie\u00b7len", "Gall'", "im", "Ma\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.95": {"text": "F\u00fcr jeden Schurken immerhin", "tokens": ["F\u00fcr", "je\u00b7den", "Schur\u00b7ken", "im\u00b7mer\u00b7hin"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.96": {"text": "Nur Honig auf der Zunge tragen;", "tokens": ["Nur", "Ho\u00b7nig", "auf", "der", "Zun\u00b7ge", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.97": {"text": "Mu\u00df leiden, wenn's dem Marschall' so gef\u00e4llt,", "tokens": ["Mu\u00df", "lei\u00b7den", ",", "wenn's", "dem", "Mar\u00b7schall'", "so", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "KOUS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.98": {"text": "Da\u00df Schw\u00e4tzer mich zum Spieltisch' f\u00fchren,", "tokens": ["Da\u00df", "Schw\u00e4t\u00b7zer", "mich", "zum", "Spiel\u00b7tisch'", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.99": {"text": "Und meine Zeit, sogar mein Geld,", "tokens": ["Und", "mei\u00b7ne", "Zeit", ",", "so\u00b7gar", "mein", "Geld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.100": {"text": "Nur niemals die Geduld verlieren.", "tokens": ["Nur", "nie\u00b7mals", "die", "Ge\u00b7duld", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.101": {"text": "Die\u00df Opfer ist f\u00fcrwahr nicht klein;", "tokens": ["Die\u00df", "Op\u00b7fer", "ist", "f\u00fcr\u00b7wahr", "nicht", "klein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.102": {"text": "Sollt' ich es ", "tokens": ["Sollt'", "ich", "es"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.103": {"text": "So m\u00fc\u00dft' er, wenn die Figuranten gingen,", "tokens": ["So", "m\u00fc\u00dft'", "er", ",", "wenn", "die", "Fi\u00b7gu\u00b7ran\u00b7ten", "gin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.104": {"text": "Und dann, mit Wenigen allein,", "tokens": ["Und", "dann", ",", "mit", "We\u00b7ni\u00b7gen", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.105": {"text": "Die Herzen ihm sich an zu \u00f6ffnen fingen,", "tokens": ["Die", "Her\u00b7zen", "ihm", "sich", "an", "zu", "\u00f6ff\u00b7nen", "fin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PRF", "APZR", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.106": {"text": "Nicht F\u00fcrst, wie bei der Cour, mehr seyn.", "tokens": ["Nicht", "F\u00fcrst", ",", "wie", "bei", "der", "Cour", ",", "mehr", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PWAV", "APPR", "ART", "NN", "$,", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.107": {"text": "Denn, wenn er die Despoten-Miene,", "tokens": ["Denn", ",", "wenn", "er", "die", "Despo\u00b7ten\u00b7Mie\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.108": {"text": "Die bald dem\u00fcthigt, bald besch\u00fctzt,", "tokens": ["Die", "bald", "de\u00b7m\u00fct\u00b7higt", ",", "bald", "be\u00b7sch\u00fctzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.109": {"text": "Auch hier noch nicht verliert, noch steif und ernsthaft sitzt,", "tokens": ["Auch", "hier", "noch", "nicht", "ver\u00b7liert", ",", "noch", "steif", "und", "ernst\u00b7haft", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PTKNEG", "VVFIN", "$,", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.110": {"text": "Damit sich ja kein Mensch erk\u00fchne,", "tokens": ["Da\u00b7mit", "sich", "ja", "kein", "Mensch", "er\u00b7k\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.111": {"text": "Nur mit dem Schnupftuch' sich zu wedeln, wenn ihn schwitzt,", "tokens": ["Nur", "mit", "dem", "Schnupf\u00b7tuch'", "sich", "zu", "we\u00b7deln", ",", "wenn", "ihn", "schwitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PRF", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.112": {"text": "So spielt er, ", "tokens": ["So", "spielt", "er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.113": {"text": "Und ich erduldete so was,", "tokens": ["Und", "ich", "er\u00b7dul\u00b7de\u00b7te", "so", "was", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.114": {"text": "Um Kaviar und Ananas", "tokens": ["Um", "Ka\u00b7vi\u00b7ar", "und", "A\u00b7na\u00b7nas"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.115": {"text": "Zu schmausen, und um Chierwein zu trinken? \u2013", "tokens": ["Zu", "schmau\u00b7sen", ",", "und", "um", "Chier\u00b7wein", "zu", "trin\u00b7ken", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KON", "APPR", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.116": {"text": "Denn, Freundin, w\u00fcrd' es weise seyn,", "tokens": ["Denn", ",", "Freun\u00b7din", ",", "w\u00fcrd'", "es", "wei\u00b7se", "seyn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "VAFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.117": {"text": "Geehrt schon dadurch ganz allein,", "tokens": ["Ge\u00b7ehrt", "schon", "da\u00b7durch", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PAV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.118": {"text": "Weil jener F\u00fcrst ist, sich zu d\u00fcnken?", "tokens": ["Weil", "je\u00b7ner", "F\u00fcrst", "ist", ",", "sich", "zu", "d\u00fcn\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VAFIN", "$,", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.119": {"text": "La\u00df