{"textgrid.poem.25468": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nein! la\u00df du mich in meiner Einsamkeit!", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein! la\u00df du mich in meiner Einsamkeit!", "tokens": ["Nein", "!", "la\u00df", "du", "mich", "in", "mei\u00b7ner", "Ein\u00b7sam\u00b7keit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VVIMP", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hier plaudr' ich, ohne mich an Zeit", "tokens": ["Hier", "plaudr'", "ich", ",", "oh\u00b7ne", "mich", "an", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUI", "PRF", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Ort zu kehren, in der M\u00fctze,", "tokens": ["Und", "Ort", "zu", "keh\u00b7ren", ",", "in", "der", "M\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKZU", "VVINF", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis Mitternacht mit dir, und horche deinem Witze.", "tokens": ["Bis", "Mit\u00b7ter\u00b7nacht", "mit", "dir", ",", "und", "hor\u00b7che", "dei\u00b7nem", "Wit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hier schleudr' ich oft, ein \u00e4chter Sohn des Teut,", "tokens": ["Hier", "schleudr'", "ich", "oft", ",", "ein", "\u00e4ch\u00b7ter", "Sohn", "des", "Teut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Auf das Tyrannenvolk, das barsch vom Thron' gebeut,", "tokens": ["Auf", "das", "Ty\u00b7ran\u00b7nen\u00b7volk", ",", "das", "barsch", "vom", "Thron'", "ge\u00b7beut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und w\u00e4hnt, der Rest der Menschen sey nichts n\u00fctze,", "tokens": ["Und", "w\u00e4hnt", ",", "der", "Rest", "der", "Men\u00b7schen", "sey", "nichts", "n\u00fct\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "ART", "NN", "VAFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als Sklav zu seyn von ihrer Herrlichkeit,", "tokens": ["Als", "Sklav", "zu", "seyn", "von", "ih\u00b7rer", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKZU", "VAINF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der Wahrheit Donner und des Spottes Blitze.", "tokens": ["Der", "Wahr\u00b7heit", "Don\u00b7ner", "und", "des", "Spot\u00b7tes", "Blit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Hier kann ich beim Johannisbeerenwein'", "tokens": ["Hier", "kann", "ich", "beim", "Jo\u00b7han\u00b7nis\u00b7bee\u00b7ren\u00b7wein'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Aus meinem Garten, und der Gr\u00fctze", "tokens": ["Aus", "mei\u00b7nem", "Gar\u00b7ten", ",", "und", "der", "Gr\u00fct\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Von meinem Felde, gl\u00fccklich seyn.", "tokens": ["Von", "mei\u00b7nem", "Fel\u00b7de", ",", "gl\u00fcck\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Allein zu arm, mich immer neu zu kleiden,", "tokens": ["Al\u00b7lein", "zu", "arm", ",", "mich", "im\u00b7mer", "neu", "zu", "klei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "$,", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Zu froh, den Hofmann zu beneiden,", "tokens": ["Zu", "froh", ",", "den", "Hof\u00b7mann", "zu", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Zu alt, um in die Lehre noch zu gehn,", "tokens": ["Zu", "alt", ",", "um", "in", "die", "Leh\u00b7re", "noch", "zu", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "KOUI", "APPR", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Hat Euer Hof f\u00fcr mich zu wenig Freuden.", "tokens": ["Hat", "Eu\u00b7er", "Hof", "f\u00fcr", "mich", "zu", "we\u00b7nig", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Sonst, offenherzig zu gestehn,", "tokens": ["Sonst", ",", "of\u00b7fen\u00b7her\u00b7zig", "zu", "ge\u00b7stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Wollt' ich f\u00fcr dich wohl zehnfach leiden,", "tokens": ["Wollt'", "ich", "f\u00fcr", "dich", "wohl", "zehn\u00b7fach", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.20": {"text": "Am Hofe litt; an dir sein Auge weiden,", "tokens": ["Am", "Ho\u00b7fe", "litt", ";", "an", "dir", "sein", "Au\u00b7ge", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$.", "APPR", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "W\u00e4r' auch ein ungleich gr\u00f6\u00dfrer Sold.", "tokens": ["W\u00e4r'", "auch", "ein", "un\u00b7gleich", "gr\u00f6\u00df\u00b7rer", "Sold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "La\u00df immer meinen Eigensinn", "tokens": ["La\u00df", "im\u00b7mer", "mei\u00b7nen", "Ei\u00b7gen\u00b7sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Noch ferner nach Belieben mit mir schalten,", "tokens": ["Noch", "fer\u00b7ner", "nach", "Be\u00b7lie\u00b7ben", "mit", "mir", "schal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Denn dieser l\u00e4\u00dft es immerhin", "tokens": ["Denn", "die\u00b7ser", "l\u00e4\u00dft", "es", "im\u00b7mer\u00b7hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Mit der Bequemlichkeit beim alten,", "tokens": ["Mit", "der", "Be\u00b7quem\u00b7lich\u00b7keit", "beim", "al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Und, wie du siehst, besteht mein Gl\u00fcck darin.", "tokens": ["Und", ",", "wie", "du", "siehst", ",", "be\u00b7steht", "mein", "Gl\u00fcck", "da\u00b7rin", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Ich, den in Stiefeln und in Spornen,", "tokens": ["Ich", ",", "den", "in", "Stie\u00b7feln", "und", "in", "Spor\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Die Haare schlicht zur\u00fcckgek\u00e4mmt,", "tokens": ["Die", "Haa\u00b7re", "schlicht", "zu\u00b7r\u00fcck\u00b7ge\u00b7k\u00e4mmt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Kein Bach, kein Sumpf und kein Geb\u00fcsch voll Dornen,", "tokens": ["Kein", "Bach", ",", "kein", "Sumpf", "und", "kein", "Ge\u00b7b\u00fcsch", "voll", "Dor\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "KON", "PIAT", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Auf seinen Wanderungen hemmt:", "tokens": ["Auf", "sei\u00b7nen", "Wan\u00b7de\u00b7run\u00b7gen", "hemmt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Ich soll mir Stunden lang die Haare", "tokens": ["Ich", "soll", "mir", "Stun\u00b7den", "lang", "die", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "NN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Zerraufen lassen, und die Zeh'n", "tokens": ["Zer\u00b7rau\u00b7fen", "las\u00b7sen", ",", "und", "die", "Zeh'n"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVINF", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Auf d\u00fcnnen Sohlen wund mir gehn? \u2013", "tokens": ["Auf", "d\u00fcn\u00b7nen", "Soh\u00b7len", "wund", "mir", "gehn", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Wer fahren kann, ei nun, der fahre!", "tokens": ["Wer", "fah\u00b7ren", "kann", ",", "ei", "nun", ",", "der", "fah\u00b7re", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VMFIN", "$,", "ITJ", "ADV", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Doch wer wie ich wohl gehen mu\u00df,", "tokens": ["Doch", "wer", "wie", "ich", "wohl", "ge\u00b7hen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "KOKOM", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Sey wenigstens so klug und spare", "tokens": ["Sey", "we\u00b7nigs\u00b7tens", "so", "klug", "und", "spa\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Sich jeden Schritt zum Ekel und Verdru\u00df.", "tokens": ["Sich", "je\u00b7den", "Schritt", "zum", "E\u00b7kel", "und", "Ver\u00b7dru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Mich immer sch\u00fcchtern umzusehn,", "tokens": ["Mich", "im\u00b7mer", "sch\u00fcch\u00b7tern", "um\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Ob mir das fromme Ding, mein Degen,", "tokens": ["Ob", "mir", "das", "from\u00b7me", "Ding", ",", "mein", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Auch richtig folg'? und meiner Locken wegen", "tokens": ["Auch", "rich\u00b7tig", "fol\u00b7g'", "?", "und", "mei\u00b7ner", "Lo\u00b7cken", "we\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PTKVZ", "$.", "KON", "PPOSAT", "NN", "APPR"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "Bei jedem Wind' in Furcht zu stehn?", "tokens": ["Bei", "je\u00b7dem", "Wind'", "in", "Furcht", "zu", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Da w\u00e4r' ich wohl ein braver Thor!", "tokens": ["Da", "w\u00e4r'", "ich", "wohl", "ein", "bra\u00b7ver", "Thor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Was ist mir itzt an einem Schirm' gelegen,", "tokens": ["Was", "ist", "mir", "itzt", "an", "ei\u00b7nem", "Schirm'", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Mir, der durch keinen Wind und keinen Regen", "tokens": ["Mir", ",", "der", "durch", "kei\u00b7nen", "Wind", "und", "kei\u00b7nen", "Re\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPR", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Das mindeste bisher verlor?", "tokens": ["Das", "min\u00b7des\u00b7te", "bis\u00b7her", "ver\u00b7lor", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.46": {"text": "Und wozu soll mir gar ein Degen?", "tokens": ["Und", "wo\u00b7zu", "soll", "mir", "gar", "ein", "De\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VMFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Auf einer Treppe Arm und Bein,", "tokens": ["Auf", "ei\u00b7ner", "Trep\u00b7pe", "Arm", "und", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Aus H\u00f6flichkeit, im Umdrehn, zu zerbrechen?", "tokens": ["Aus", "H\u00f6f\u00b7lich\u00b7keit", ",", "im", "Um\u00b7drehn", ",", "zu", "zer\u00b7bre\u00b7chen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPRART", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.49": {"text": "Denn auch nur einen Frosch der Wiese zu erstechen,", "tokens": ["Denn", "auch", "nur", "ei\u00b7nen", "Frosch", "der", "Wie\u00b7se", "zu", "er\u00b7ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "W\u00fcrd' ich gewi\u00df zu billig seyn.", "tokens": ["W\u00fcrd'", "ich", "ge\u00b7wi\u00df", "zu", "bil\u00b7lig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", 