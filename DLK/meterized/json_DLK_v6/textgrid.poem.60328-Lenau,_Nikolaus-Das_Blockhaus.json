{"textgrid.poem.60328": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Das Blockhaus", "genre": "verse", "period": "N.A.", "pub_year": 1835, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00fcdgeritten auf langer Tagesreise", "tokens": ["M\u00fcd\u00b7ge\u00b7rit\u00b7ten", "auf", "lan\u00b7ger", "Ta\u00b7ges\u00b7rei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Durch die hohen W\u00e4lder der Republik,", "tokens": ["Durch", "die", "ho\u00b7hen", "W\u00e4l\u00b7der", "der", "Re\u00b7pub\u00b7lik", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "F\u00fchrte zu einem Gastwirt mein Geschick;", "tokens": ["F\u00fchr\u00b7te", "zu", "ei\u00b7nem", "Gast\u00b7wirt", "mein", "Ge\u00b7schick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Der empfing mich kalt, auf freundliche Weise,", "tokens": ["Der", "emp\u00b7fing", "mich", "kalt", ",", "auf", "freund\u00b7li\u00b7che", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Sprach gelassen, mit ungekr\u00fcmmtem R\u00fccken:", "tokens": ["Sprach", "ge\u00b7las\u00b7sen", ",", "mit", "un\u00b7ge\u00b7kr\u00fcmm\u00b7tem", "R\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "\u00bbguten Abend!\u00ab und bot mir seine Hand,", "tokens": ["\u00bb", "gu\u00b7ten", "A\u00b7bend", "!", "\u00ab", "und", "bot", "mir", "sei\u00b7ne", "Hand", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$.", "$(", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Gleichsam guten Empfangs ein leblos Pfand,", "tokens": ["Gleich\u00b7sam", "gu\u00b7ten", "Emp\u00b7fangs", "ein", "leb\u00b7los", "Pfand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Denn er r\u00fchrte sie nicht, die meine zu dr\u00fccken.", "tokens": ["Denn", "er", "r\u00fchr\u00b7te", "sie", "nicht", ",", "die", "mei\u00b7ne", "zu", "dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "PPOSAT", "PTKZU", "VVINF", "$."], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Lesen konnt ich in seinen festen Z\u00fcgen", "tokens": ["Le\u00b7sen", "konnt", "ich", "in", "sei\u00b7nen", "fes\u00b7ten", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.10": {"text": "Seinen lang und treu bewahrten Entschlu\u00df:", "tokens": ["Sei\u00b7nen", "lang", "und", "treu", "be\u00b7wahr\u00b7ten", "Ent\u00b7schlu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "KON", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Auch mit keinem Fingerdrucke zu l\u00fcgen;", "tokens": ["Auch", "mit", "kei\u00b7nem", "Fin\u00b7ger\u00b7dru\u00b7cke", "zu", "l\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Sicher und wohl ward mir bei seinem Gru\u00df.", "tokens": ["Si\u00b7cher", "und", "wohl", "ward", "mir", "bei", "sei\u00b7nem", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.13": {"text": "Wenig eilte der Mann, mich zu bedienen,", "tokens": ["We\u00b7nig", "eil\u00b7te", "der", "Mann", ",", "mich", "zu", "be\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Doch nicht fand ich die Kost so d\u00fcrr und mager", "tokens": ["Doch", "nicht", "fand", "ich", "die", "Kost", "so", "d\u00fcrr", "und", "ma\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "KON", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Wie sein Wort, ich sollte bei ihm ein Lager", "tokens": ["Wie", "sein", "Wort", ",", "ich", "soll\u00b7te", "bei", "ihm", "ein", "La\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "PPER", "VMFIN", "APPR", "PPER", "ART", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Finden weicher und w\u00e4rmer als seine Mienen.", "tokens": ["Fin\u00b7den", "wei\u00b7cher", "und", "w\u00e4r\u00b7mer", "als", "sei\u00b7ne", "Mie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "KON", "ADJD", "KOKOM", "PPOSAT", "NN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Winter wars, ich starrte vom Urwaldfroste;", "tokens": ["Win\u00b7ter", "wars", ",", "ich", "starr\u00b7te", "vom", "Ur\u00b7wald\u00b7fros\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.18": {"text": "Als ich eintrat in die geheizte Stube,", "tokens": ["Als", "ich", "ein\u00b7trat", "in", "die", "ge\u00b7heiz\u00b7te", "Stu\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Sprang mit Fragen heran des Farmers Bube,", "tokens": ["Sprang", "mit", "Fra\u00b7gen", "he\u00b7ran", "des", "Far\u00b7mers", "Bu\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PTKVZ", "ART", "NN", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Was von meinem Gep\u00e4ck dies, jenes koste?", "tokens": ["Was", "von", "mei\u00b7nem", "Ge\u00b7p\u00e4ck", "dies", ",", "je\u00b7nes", "kos\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "PDS", "$,", "PDS", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "Emsig am Tisch sah ich die Weiber schalten;", "tokens": ["Em\u00b7sig", "am", "Tisch", "sah", "ich", "die", "Wei\u00b7ber", "schal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.22": {"text": "Und es wurde die Mahlzeit rasch gehalten.", "tokens": ["Und", "es", "wur\u00b7de", "die", "Mahl\u00b7zeit", "rasch", "ge\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.23": {"text": "Sp\u00e4ter schwatzten die m\u00e4nnlichen Hausgenossen", "tokens": ["Sp\u00e4\u00b7ter", "schwatz\u00b7ten", "die", "m\u00e4nn\u00b7li\u00b7chen", "Haus\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.24": {"text": "Am Kamin, die scharfe Zigarr im Munde,", "tokens": ["Am", "Ka\u00b7min", ",", "die", "schar\u00b7fe", "Zi\u00b7garr", "im", "Mun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.25": {"text": "Von Gesch\u00e4ft und Betrieb, bis eine Stunde", "tokens": ["Von", "Ge\u00b7sch\u00e4ft", "und", "Be\u00b7trieb", ",", "bis", "ei\u00b7ne", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "KOUS", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.26": {"text": "Mir in traulicher Langweil hingeflossen.", "tokens": ["Mir", "in", "trau\u00b7li\u00b7cher", "Lang\u00b7weil", "hin\u00b7ge\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.27": {"text": "H\u00f6rbar vor allen sprach des Hauses Vater,", "tokens": ["H\u00f6r\u00b7bar", "vor", "al\u00b7len", "sprach", "des", "Hau\u00b7ses", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIS", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Als ein vielerfahrner Lenker und Rater,", "tokens": ["Als", "ein", "vie\u00b7ler\u00b7fahr\u00b7ner", "Len\u00b7ker", "und", "Ra\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Wechselnd raucht' er und sprach, und aller Augen", "tokens": ["Wech\u00b7selnd", "raucht'", "er", "und", "sprach", ",", "und", "al\u00b7ler", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "KON", "VVFIN", "$,", "KON", "PIAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.30": {"text": "Hingen an seinen Lippen, der Alte schien", "tokens": ["Hin\u00b7gen", "an", "sei\u00b7nen", "Lip\u00b7pen", ",", "der", "Al\u00b7te", "schien"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.31": {"text": "Aus dem Zigarrenstumpf Erfindung zu saugen;", "tokens": ["Aus", "dem", "Zi\u00b7gar\u00b7ren\u00b7stumpf", "Er\u00b7fin\u00b7dung", "zu", "sau\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Schweigend lie\u00df ich die Reden vor\u00fcberziehn.", "tokens": ["Schwei\u00b7gend", "lie\u00df", "ich", "die", "Re\u00b7den", "vor\u00b7\u00fc\u00b7ber\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.33": {"text": "Endlich gewann der Schlaf den stillen Sieg,", "tokens": ["End\u00b7lich", "ge\u00b7wann", "der", "Schlaf", "den", "stil\u00b7len", "Sieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.34": {"text": "Und sie gingen zu Bett; ich blieb allein,", "tokens": ["Und", "sie", "gin\u00b7gen", "zu", "Bett", ";", "ich", "blieb", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$.", "PPER", "VVFIN", "ADV", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.35": {"text": "Trank noch eine Flasche vom lieben Rhein,", "tokens": ["Trank", "noch", "ei\u00b7ne", "Fla\u00b7sche", "vom", "lie\u00b7ben", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "APPRART", "ADJA", "NE", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.36": {"text": "Als das englische Talergelispel schwieg.", "tokens": ["Als", "das", "eng\u00b7li\u00b7sche", "Ta\u00b7ler\u00b7ge\u00b7lis\u00b7pel", "schwieg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.37": {"text": "Und zur weit gewanderten deutschen Flasche", "tokens": ["Und", "zur", "weit", "ge\u00b7wan\u00b7der\u00b7ten", "deut\u00b7schen", "Fla\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJD", "ADJA", "ADJA", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.38": {"text": "Holt ich den Uhland aus meiner Satteltasche.", "tokens": ["Holt", "ich", "den", "Uh\u00b7land", "aus", "mei\u00b7ner", "Sat\u00b7tel\u00b7ta\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.39": {"text": "Ferne der Heimat, tiefst im fremden Wald,", "tokens": ["Fer\u00b7ne", "der", "Hei\u00b7mat", ",", "tiefst", "im", "frem\u00b7den", "Wald", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.40": {"text": "Las ich mir laut den herrlichen \u203aHeld Harald\u2039.", "tokens": ["Las", "ich", "mir", "laut", "den", "herr\u00b7li\u00b7chen", "\u203a", "Held", "Ha\u00b7rald", "\u2039", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "PPER", "PRF", "APPR", "ART", "ADJA", "ADJA", "NN", "NE", "$(", "$."], "meter": "+--+-+--++-", "measure": "iambic.penta.invert"}, "line.41": {"text": "Eichenst\u00e4mme warf ich ins lustige Feuer,", "tokens": ["Ei\u00b7chen\u00b7st\u00e4m\u00b7me", "warf", "ich", "ins", "lus\u00b7ti\u00b7ge", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.42": {"text": "Mir die Stube zu hellen und zu w\u00e4rmen,", "tokens": ["Mir", "die", "Stu\u00b7be", "zu", "hel\u00b7len", "und", "zu", "w\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.43": {"text": "Denn die Elfen Haralds sind nicht geheuer,", "tokens": ["Denn", "die", "El\u00b7fen", "Ha\u00b7ralds", "sind", "nicht", "ge\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "--+-++-+-+-", "measure": "anapaest.init"}, "line.44": {"text": "Lockend h\u00f6rte ich sie schon im Walde schw\u00e4rmen.", "tokens": ["Lo\u00b7ckend", "h\u00f6r\u00b7te", "ich", "sie", "schon", "im", "Wal\u00b7de", "schw\u00e4r\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.45": {"text": "Aber mit einmal war die Freude geschwunden,", "tokens": ["A\u00b7ber", "mit", "ein\u00b7mal", "war", "die", "Freu\u00b7de", "ge\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.46": {"text": "Und mir wollte der Rheinwein nicht mehr munden.", "tokens": ["Und", "mir", "woll\u00b7te", "der", "Rhein\u00b7wein", "nicht", "mehr", "mun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.47": {"text": "\u203auhland! wie stehts mit der Freiheit daheim?\u2039 die Frage", "tokens": ["\u203a", "uh\u00b7land", "!", "wie", "stehts", "mit", "der", "Frei\u00b7heit", "da\u00b7heim", "?", "\u2039", "die", "Fra\u00b7ge"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["$(", "NN", "$.", "PWAV", "PIS", "APPR", "ART", "NN", "PTKVZ", "$.", "$(", "ART", "NN"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.48": {"text": "Sandt ich \u00fcber W\u00e4lder und Meer ihm zu.", "tokens": ["Sandt", "ich", "\u00fc\u00b7ber", "W\u00e4l\u00b7der", "und", "Meer", "ihm", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "PPER", "PTKVZ", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.49": {"text": "Pl\u00f6tzlich erwachte der Sturm aus stiller Ruh,", "tokens": ["Pl\u00f6tz\u00b7lich", "er\u00b7wach\u00b7te", "der", "Sturm", "aus", "stil\u00b7ler", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.50": {"text": "Und im Walde h\u00f6rt ich die Antwortklage:", "tokens": ["Und", "im", "Wal\u00b7de", "h\u00f6rt", "ich", "die", "Ant\u00b7wort\u00b7kla\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.51": {"text": "Krachend st\u00fcrzten drau\u00dfen die nacktgesch\u00e4lten", "tokens": ["Kra\u00b7chend", "st\u00fcrz\u00b7ten", "drau\u00b7\u00dfen", "die", "nackt\u00b7ge\u00b7sch\u00e4l\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN"], "meter": "+-