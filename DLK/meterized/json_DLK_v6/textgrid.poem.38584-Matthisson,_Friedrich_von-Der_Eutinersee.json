{"textgrid.poem.38584": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Eutinersee", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herrlich, o See, sind deine Silberfluthen,", "tokens": ["Herr\u00b7lich", ",", "o", "See", ",", "sind", "dei\u00b7ne", "Sil\u00b7ber\u00b7flut\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "FM", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sanft vom goldenen Abendschein ger\u00f6thet", "tokens": ["Sanft", "vom", "gol\u00b7de\u00b7nen", "A\u00b7bend\u00b7schein", "ge\u00b7r\u00f6\u00b7thet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "VVPP"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Oder mild, in N\u00e4chten des Mais, vom stillen", "tokens": ["O\u00b7der", "mild", ",", "in", "N\u00e4ch\u00b7ten", "des", "Mais", ",", "vom", "stil\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "$,", "APPR", "NN", "ART", "NN", "$,", "APPRART", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Monde beleuchtet!", "tokens": ["Mon\u00b7de", "be\u00b7leuch\u00b7tet", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "L\u00fcfte des Fr\u00fchlings bebten durch die Wipfel!", "tokens": ["L\u00fcf\u00b7te", "des", "Fr\u00fch\u00b7lings", "beb\u00b7ten", "durch", "die", "Wip\u00b7fel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "V\u00f6gel sangen im Gr\u00fcnen! Wolkenbilder", "tokens": ["V\u00f6\u00b7gel", "san\u00b7gen", "im", "Gr\u00fc\u00b7nen", "!", "Wol\u00b7ken\u00b7bil\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$.", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Schwebten, hell vom westlichen Stral, in deiner", "tokens": ["Schweb\u00b7ten", ",", "hell", "vom", "west\u00b7li\u00b7chen", "Stral", ",", "in", "dei\u00b7ner"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJD", "APPRART", "ADJA", "NN", "$,", "APPR", "PPOSAT"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Wallenden Klarheit!", "tokens": ["Wal\u00b7len\u00b7den", "Klar\u00b7heit", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Rinnen, o Vo\u00df, wird sp\u00e4t noch der Erinnrung", "tokens": ["Rin\u00b7nen", ",", "o", "Vo\u00df", ",", "wird", "sp\u00e4t", "noch", "der", "E\u00b7rinn\u00b7rung"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "FM", "NE", "$,", "VAFIN", "ADJD", "ADV", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Schwermuthsvollere Thr\u00e4ne, ach! den Freuden,", "tokens": ["Schwer\u00b7muths\u00b7vol\u00b7le\u00b7re", "Thr\u00e4\u00b7ne", ",", "ach", "!", "den", "Freu\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ITJ", "$.", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Die durch dich im Thale des Sees, in diesem", "tokens": ["Die", "durch", "dich", "im", "Tha\u00b7le", "des", "Sees", ",", "in", "die\u00b7sem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "PPER", "APPRART", "NN", "ART", "NN", "$,", "APPR", "PDAT"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Irdischen Himmel,", "tokens": ["Ir\u00b7di\u00b7schen", "Him\u00b7mel", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Unter den Linden, die den gr\u00fcnumschilften", "tokens": ["Un\u00b7ter", "den", "Lin\u00b7den", ",", "die", "den", "gr\u00fc\u00b7num\u00b7schilf\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "$,", "PRELS", "ART", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Agneswerder beschatten, in der Laube", "tokens": ["Ag\u00b7nes\u00b7wer\u00b7der", "be\u00b7schat\u00b7ten", ",", "in", "der", "Lau\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Stillvertrautem Dunkel und in des Gartens", "tokens": ["Still\u00b7ver\u00b7trau\u00b7tem", "Dun\u00b7kel", "und", "in", "des", "Gar\u00b7tens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "APPR", "ART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "K\u00fchle mich kr\u00e4nzten!", "tokens": ["K\u00fch\u00b7le", "mich", "kr\u00e4nz\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}