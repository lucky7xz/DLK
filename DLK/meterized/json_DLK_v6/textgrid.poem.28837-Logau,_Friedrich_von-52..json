{"textgrid.poem.28837": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es werden viel F\u00fcchse bey Hofe gefressen;", "tokens": ["Es", "wer\u00b7den", "viel", "F\u00fcch\u00b7se", "bey", "Ho\u00b7fe", "ge\u00b7fres\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Noch sind sie doch h\u00e4uffig daselbsten gesessen;", "tokens": ["Noch", "sind", "sie", "doch", "h\u00e4uf\u00b7fig", "da\u00b7selbs\u00b7ten", "ge\u00b7ses\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "PAV", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Das machet, sie wissen, mit waserley Namen", "tokens": ["Das", "ma\u00b7chet", ",", "sie", "wis\u00b7sen", ",", "mit", "wa\u00b7ser\u00b7ley", "Na\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "PPER", "VVINF", "$,", "APPR", "PIAT", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die Schw\u00e4ntze von ihnen zur Herrligkeit kamen.", "tokens": ["Die", "Schw\u00e4nt\u00b7ze", "von", "ih\u00b7nen", "zur", "Herr\u00b7lig\u00b7keit", "ka\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}