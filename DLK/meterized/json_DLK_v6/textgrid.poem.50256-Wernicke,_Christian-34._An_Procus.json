{"textgrid.poem.50256": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "34. An Procus", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil dich die ", "tokens": ["Weil", "dich", "die"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "++-", "measure": "unknown.measure.di"}, "line.2": {"text": "So urtheil ich im kalten Blut;", "tokens": ["So", "ur\u00b7theil", "ich", "im", "kal\u00b7ten", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du heissest ein Ding ", "tokens": ["Du", "heis\u00b7sest", "ein", "Ding"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ich aber lass' es ", "tokens": ["Ich", "a\u00b7ber", "lass'", "es"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}}}}}