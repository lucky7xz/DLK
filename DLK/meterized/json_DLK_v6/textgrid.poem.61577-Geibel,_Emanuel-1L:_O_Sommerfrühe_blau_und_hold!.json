{"textgrid.poem.61577": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Sommerfr\u00fche blau und hold!", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Sommerfr\u00fche blau und hold!", "tokens": ["O", "Som\u00b7mer\u00b7fr\u00fc\u00b7he", "blau", "und", "hold", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es trieft der Wald von Sonnengold,", "tokens": ["Es", "trieft", "der", "Wald", "von", "Son\u00b7nen\u00b7gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Blumen steht die Wiese;", "tokens": ["In", "Blu\u00b7men", "steht", "die", "Wie\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Rosen bl\u00fchen rot und wei\u00df,", "tokens": ["Die", "Ro\u00b7sen", "bl\u00fc\u00b7hen", "rot", "und", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und durch die Fluren wandelt leis", "tokens": ["Und", "durch", "die", "Flu\u00b7ren", "wan\u00b7delt", "leis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Hauch vom Paradiese.", "tokens": ["Ein", "Hauch", "vom", "Pa\u00b7ra\u00b7die\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die ganze Welt ist Glanz und Freud',", "tokens": ["Die", "gan\u00b7ze", "Welt", "ist", "Glanz", "und", "Freud'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bist du jung, so liebe heut", "tokens": ["Und", "bist", "du", "jung", ",", "so", "lie\u00b7be", "heut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Rosen brich mit Wonnen!", "tokens": ["Und", "Ro\u00b7sen", "brich", "mit", "Won\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wardst du alt, vergi\u00df der Pein", "tokens": ["Und", "wardst", "du", "alt", ",", "ver\u00b7gi\u00df", "der", "Pein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "VVIMP", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und lerne dich am Widerschein", "tokens": ["Und", "ler\u00b7ne", "dich", "am", "Wi\u00b7der\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Gl\u00fcck der Jugend sonnen!", "tokens": ["Vom", "Gl\u00fcck", "der", "Ju\u00b7gend", "son\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}