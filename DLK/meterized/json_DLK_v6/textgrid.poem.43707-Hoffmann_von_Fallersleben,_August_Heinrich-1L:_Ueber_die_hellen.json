{"textgrid.poem.43707": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ueber die hellen", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ueber die hellen", "tokens": ["Ue\u00b7ber", "die", "hel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Funkelnden Wellen", "tokens": ["Fun\u00b7keln\u00b7den", "Wel\u00b7len"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Tanzen Libellen den lustigen Tanz,", "tokens": ["Tan\u00b7zen", "Li\u00b7bel\u00b7len", "den", "lus\u00b7ti\u00b7gen", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Fr\u00f6hlich und munter,", "tokens": ["Fr\u00f6h\u00b7lich", "und", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Auf und hinunter,", "tokens": ["Auf", "und", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "KON", "PTKVZ", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Schweben und weben im sonnigen Glanz.", "tokens": ["Schwe\u00b7ben", "und", "we\u00b7ben", "im", "son\u00b7ni\u00b7gen", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "K\u00fchlende L\u00fcfte,", "tokens": ["K\u00fch\u00b7len\u00b7de", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Liebliche D\u00fcfte,", "tokens": ["Lieb\u00b7li\u00b7che", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wonniges Wetter und V\u00f6gelgesang!", "tokens": ["Won\u00b7ni\u00b7ges", "Wet\u00b7ter", "und", "V\u00f6\u00b7gel\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Gleich den Libellen", "tokens": ["Gleich", "den", "Li\u00b7bel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ueber die Wellen", "tokens": ["Ue\u00b7ber", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Schwebt unser Nachen am Ufer entlang.", "tokens": ["Schwebt", "un\u00b7ser", "Na\u00b7chen", "am", "U\u00b7fer", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Blumen und B\u00e4ume", "tokens": ["Blu\u00b7men", "und", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Fliegen wie Tr\u00e4ume,", "tokens": ["Flie\u00b7gen", "wie", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Alles enteilet und schwindet dahin.", "tokens": ["Al\u00b7les", "ent\u00b7ei\u00b7let", "und", "schwin\u00b7det", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "PAV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Doch im Enteilen", "tokens": ["Doch", "im", "Ent\u00b7ei\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Lassen wir weilen,", "tokens": ["Las\u00b7sen", "wir", "wei\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Weilen im Herzen den fr\u00f6hlichen Sinn.", "tokens": ["Wei\u00b7len", "im", "Her\u00b7zen", "den", "fr\u00f6h\u00b7li\u00b7chen", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}