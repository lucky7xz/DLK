{"dta.poem.6404": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geschmack.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn es mit scharffen Druck die Nerven r\u00fchret;", "tokens": ["Wenn", "es", "mit", "scharf\u00b7fen", "Druck", "die", "Ner\u00b7ven", "r\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird mehrentheils auch Lust dadurch versp\u00fcret.", "tokens": ["Wird", "meh\u00b7ren\u00b7theils", "auch", "Lust", "da\u00b7durch", "ver\u00b7sp\u00fc\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es schmeichelt dem Geschmack, es reitzt ihn, es erweckt", "tokens": ["Es", "schmei\u00b7chelt", "dem", "Ge\u00b7schmack", ",", "es", "reitzt", "ihn", ",", "es", "er\u00b7weckt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und machet, da\u00df es dann den meisten schmeckt.", "tokens": ["Und", "ma\u00b7chet", ",", "da\u00df", "es", "dann", "den", "meis\u00b7ten", "schmeckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wei\u00df man an Speisen Saltz, ohn Uberflu\u00df, zu geben;", "tokens": ["Wei\u00df", "man", "an", "Spei\u00b7sen", "Saltz", ",", "ohn", "U\u00b7ber\u00b7flu\u00df", ",", "zu", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NN", "NN", "$,", "KOUI", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist es derselben Seel und Leben.", "tokens": ["Ist", "es", "der\u00b7sel\u00b7ben", "Seel", "und", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}