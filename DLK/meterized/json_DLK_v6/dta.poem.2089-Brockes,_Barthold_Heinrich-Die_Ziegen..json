{"dta.poem.2089": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Ziegen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu dieser Art geh\u00f6ren noch die auch betrachtungswer-", "tokens": ["Zu", "die\u00b7ser", "Art", "ge\u00b7h\u00f6\u00b7ren", "noch", "die", "auch", "be\u00b7trach\u00b7tungs\u00b7wer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ADV", "ART", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "then Ziegen.", "tokens": ["then", "Zie\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Wie mancherley Bequemlichkeit, Trank, Nahrung,", "tokens": ["Wie", "man\u00b7cher\u00b7ley", "Be\u00b7quem\u00b7lich\u00b7keit", ",", "Trank", ",", "Nah\u00b7rung", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nutzen und Vergn\u00fcgen", "tokens": ["Nut\u00b7zen", "und", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Verschaffet uns auch dieses Thier, durch Fleisch, Milch,", "tokens": ["Ver\u00b7schaf\u00b7fet", "uns", "auch", "die\u00b7ses", "Thier", ",", "durch", "Fleisch", ",", "Milch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PDAT", "NN", "$,", "APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.6": {"text": "K\u00e4se, Fell und Haar!", "tokens": ["K\u00e4\u00b7se", ",", "Fell", "und", "Haar", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Das Haar wird K\u00fcssen auszustopfen, zu Filzen, und ver-", "tokens": ["Das", "Haar", "wird", "K\u00fcs\u00b7sen", "aus\u00b7zu\u00b7stop\u00b7fen", ",", "zu", "Fil\u00b7zen", ",", "und", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NN", "VVPP", "$,", "APPR", "NN", "$,", "KON", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "schiednen Zeugen,", "tokens": ["schied\u00b7nen", "Zeu\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Zu Stricken gleichfalls oft genutzt, nebst andern Dingen,", "tokens": ["Zu", "Stri\u00b7cken", "gleich\u00b7falls", "oft", "ge\u00b7nutzt", ",", "nebst", "an\u00b7dern", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADV", "VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "auch sogar", "tokens": ["auch", "so\u00b7gar"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Zu den Parucken mit verbraucht. Dann ist der Nutz", "tokens": ["Zu", "den", "Pa\u00b7ru\u00b7cken", "mit", "ver\u00b7braucht", ".", "Dann", "ist", "der", "Nutz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "VVPP", "$.", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "nicht zu verschweigen,", "tokens": ["nicht", "zu", "ver\u00b7schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Den uns die Fell, im Corduan und Pergament, imgleichen", "tokens": ["Den", "uns", "die", "Fell", ",", "im", "Cor\u00b7du\u00b7an", "und", "Per\u00b7ga\u00b7ment", ",", "im\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PPER", "ART", "NN", "$,", "APPRART", "NE", "KON", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "auch", "tokens": ["auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.15": {"text": "Jm Handschuh, G\u00fcrteln, Nesteln, S\u00e4ckeln und man-", "tokens": ["Jm", "Hand\u00b7schuh", ",", "G\u00fcr\u00b7teln", ",", "Nes\u00b7teln", ",", "S\u00e4\u00b7ckeln", "und", "man"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "cher Art von Kleidung reichen.", "tokens": ["cher", "Art", "von", "Klei\u00b7dung", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Die Milch dient, au\u00dfer ihrer Nahrung, zu einem heil-", "tokens": ["Die", "Milch", "dient", ",", "au\u00b7\u00dfer", "ih\u00b7rer", "Nah\u00b7rung", ",", "zu", "ei\u00b7nem", "heil"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "samen Gebrauch", "tokens": ["sa\u00b7men", "Ge\u00b7brauch"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.19": {"text": "In magrer Schwind- und Lungensucht, sie ist ein Mittel", "tokens": ["In", "mag\u00b7rer", "Schwin\u00b7d", "und", "Lun\u00b7gen\u00b7sucht", ",", "sie", "ist", "ein", "Mit\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "TRUNC", "KON", "NN", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "sonder Gleichen.", "tokens": ["son\u00b7der", "Glei\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "Die K\u00e4se sind gesund und nahrsam; auch ist die Butter,", "tokens": ["Die", "K\u00e4\u00b7se", "sind", "ge\u00b7sund", "und", "nahr\u00b7sam", ";", "auch", "ist", "die", "But\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$.", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "sonderlich", "tokens": ["son\u00b7der\u00b7lich"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "Zu Salben und zur Heilung, gut. Nicht weniger curirt", "tokens": ["Zu", "Sal\u00b7ben", "und", "zur", "Hei\u00b7lung", ",", "gut", ".", "Nicht", "we\u00b7ni\u00b7ger", "cu\u00b7rirt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPRART", "NN", "$,", "ADJD", "$.", "PTKNEG", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.24": {"text": "man sich", "tokens": ["man", "sich"], "token_info": ["word", "word"], "pos": ["PIS", "PRF"], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Jm Scharbock durch die Ziegenmolken. Das Horn, wenns", "tokens": ["Jm", "Schar\u00b7bock", "durch", "die", "Zie\u00b7gen\u00b7mol\u00b7ken", ".", "Das", "Horn", ",", "wenns"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$.", "ART", "NN", "$,", "KOUS"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "auf dem Feuer raucht,", "tokens": ["auf", "dem", "Feu\u00b7er", "raucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.27": {"text": "Wird, in der Pest und andern Seuchen, mit vieler Nutz-", "tokens": ["Wird", ",", "in", "der", "Pest", "und", "an\u00b7dern", "Seu\u00b7chen", ",", "mit", "vie\u00b7ler", "Nutz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "$,", "APPR", "ART", "NN", "KON", "ADJA", "NN", "$,", "APPR", "PIAT", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "barkeit gebraucht.", "tokens": ["bar\u00b7keit", "ge\u00b7braucht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}