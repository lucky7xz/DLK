{"textgrid.poem.23959": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "L\u00e4ndler des Verliebten", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein M\u00e4del h\u00e4lts B\u00e4ndel,", "tokens": ["Mein", "M\u00e4\u00b7del", "h\u00e4lts", "B\u00e4n\u00b7del", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "So lauf ich durchs L\u00e4ndel", "tokens": ["So", "lauf", "ich", "durchs", "L\u00e4n\u00b7del"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Hurr her und hurr hin;", "tokens": ["Hurr", "her", "und", "hurr", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "NE", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Verbotene Wege,", "tokens": ["Ver\u00b7bo\u00b7te\u00b7ne", "We\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Verwachsene Stege,", "tokens": ["Ver\u00b7wach\u00b7se\u00b7ne", "Ste\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Nichts hemmt unsern Sinn,", "tokens": ["Nichts", "hemmt", "un\u00b7sern", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und starren Verh\u00e4ge:", "tokens": ["Und", "star\u00b7ren", "Ver\u00b7h\u00e4\u00b7ge", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Hupp dr\u00fcber und drin!", "tokens": ["Hupp", "dr\u00fc\u00b7ber", "und", "drin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PAV", "KON", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Mein M\u00e4del h\u00e4lts B\u00e4ndel,", "tokens": ["Mein", "M\u00e4\u00b7del", "h\u00e4lts", "B\u00e4n\u00b7del", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Wir laufen durchs L\u00e4ndel", "tokens": ["Wir", "lau\u00b7fen", "durchs", "L\u00e4n\u00b7del"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Hurr her und hurr hin.", "tokens": ["Hurr", "her", "und", "hurr", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "NE", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Durch W\u00e4lder und Wiesen,", "tokens": ["Durch", "W\u00e4l\u00b7der", "und", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Es giebt kein Verdrie\u00dfen,", "tokens": ["Es", "giebt", "kein", "Ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wie sch\u00f6n ist die Welt!", "tokens": ["Wie", "sch\u00f6n", "ist", "die", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "