{"textgrid.poem.33445": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Der Lackschuh", "genre": "verse", "period": "N.A.", "pub_year": 1911, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Dichter dachte:", "tokens": ["Der", "Dich\u00b7ter", "dach\u00b7te", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ach was, ich hab den Plunder satt!", "tokens": ["Ach", "was", ",", "ich", "hab", "den", "Plun\u00b7der", "satt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "$,", "PPER", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Dirnen, das Theater und den Stadtmond,", "tokens": ["Die", "Dir\u00b7nen", ",", "das", "The\u00b7a\u00b7ter", "und", "den", "Stadt\u00b7mond", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Oberhemden, Stra\u00dfen und Ger\u00fcche,", "tokens": ["Die", "O\u00b7ber\u00b7hem\u00b7den", ",", "Stra\u00b7\u00dfen", "und", "Ge\u00b7r\u00fc\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die N\u00e4chte und die Kutscher und die Fenster,", "tokens": ["Die", "N\u00e4ch\u00b7te", "und", "die", "Kut\u00b7scher", "und", "die", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das Lachen, die Laternen und die Morde \u2013", "tokens": ["Das", "La\u00b7chen", ",", "die", "La\u00b7ter\u00b7nen", "und", "die", "Mor\u00b7de", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Den ganzen Dreck hab ich nun wirklich satt,", "tokens": ["Den", "gan\u00b7zen", "Dreck", "hab", "ich", "nun", "wirk\u00b7lich", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Beim Teufel!", "tokens": ["Beim", "Teu\u00b7fel", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Mag werden, was da will ... mir ist es gleich:", "tokens": ["Mag", "wer\u00b7den", ",", "was", "da", "will", "...", "mir", "ist", "es", "gleich", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VAINF", "$,", "PRELS", "ADV", "VMFIN", "$(", "PPER", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Lackschuh dr\u00fcckt mich. Und ich zieh ihn aus \u2013", "tokens": ["Der", "Lack\u00b7schuh", "dr\u00fcckt", "mich", ".", "Und", "ich", "zieh", "ihn", "aus", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "KON", "PPER", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Leute m\u00f6gen sich verwundert wenden.", "tokens": ["Die", "Leu\u00b7te", "m\u00f6\u00b7gen", "sich", "ver\u00b7wun\u00b7dert", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur schade ists um meinen seidnen Strumpf ...", "tokens": ["Nur", "scha\u00b7de", "ists", "um", "mei\u00b7nen", "seid\u00b7nen", "Strumpf", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}