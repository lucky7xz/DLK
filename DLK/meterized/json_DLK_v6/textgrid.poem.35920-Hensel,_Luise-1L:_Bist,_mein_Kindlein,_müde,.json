{"textgrid.poem.35920": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bist, mein Kindlein, m\u00fcde,", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bist, mein Kindlein, m\u00fcde,", "tokens": ["Bist", ",", "mein", "Kin\u00b7dlein", ",", "m\u00fc\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "$,", "PPOSAT", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Bist so welk und krank!", "tokens": ["Bist", "so", "welk", "und", "krank", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Schlaf' bei meinem Wiegenliede,", "tokens": ["Schlaf'", "bei", "mei\u00b7nem", "Wie\u00b7gen\u00b7lie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Nacht ist lang und bang. \u2013", "tokens": ["Dei\u00b7ne", "Nacht", "ist", "lang", "und", "bang", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als Jesus auf der Erde ging,", "tokens": ["Als", "Je\u00b7sus", "auf", "der", "Er\u00b7de", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da kamen viele Kindlein flink,", "tokens": ["Da", "ka\u00b7men", "vie\u00b7le", "Kin\u00b7dlein", "flink", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die lie\u00dfen all ihr Spielzeug stehn", "tokens": ["Die", "lie\u00b7\u00dfen", "all", "ihr", "Spiel\u00b7zeug", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PIAT", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und wollten nur mit Jesu gehn.", "tokens": ["Und", "woll\u00b7ten", "nur", "mit", "Je\u00b7su", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Armes Herz, bist m\u00fcde,", "tokens": ["Ar\u00b7mes", "Herz", ",", "bist", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sei nicht schwer und bang,", "tokens": ["Sei", "nicht", "schwer", "und", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ueber dir sei Himmelsfriede,", "tokens": ["Ue\u00b7ber", "dir", "sei", "Him\u00b7mels\u00b7frie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um dich Engelsang und Klang.", "tokens": ["Um", "dich", "En\u00b7gel\u00b7sang", "und", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als Jesus zu den Menschen kam,", "tokens": ["Als", "Je\u00b7sus", "zu", "den", "Men\u00b7schen", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Kindlein an den Busen nahm;", "tokens": ["Die", "Kin\u00b7dlein", "an", "den", "Bu\u00b7sen", "nahm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ach, h\u00e4tt' Er dich, mein Herz, erblickt,", "tokens": ["Ach", ",", "h\u00e4tt'", "Er", "dich", ",", "mein", "Herz", ",", "er\u00b7blickt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "PRF", "$,", "PPOSAT", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dich h\u00e4tt' Er auch an's Herz gedr\u00fcckt.", "tokens": ["Dich", "h\u00e4tt'", "Er", "auch", "an's", "Herz", "ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So, mein Kindlein, liege", "tokens": ["So", ",", "mein", "Kin\u00b7dlein", ",", "lie\u00b7ge"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Selig, unbewu\u00dft;", "tokens": ["Se\u00b7lig", ",", "un\u00b7be\u00b7wu\u00dft", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Friede ist nur in der Wiege,", "tokens": ["Frie\u00b7de", "ist", "nur", "in", "der", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur an Mutterbrust ist Lust. \u2013", "tokens": ["Nur", "an", "Mut\u00b7ter\u00b7brust", "ist", "Lust", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O theures Wort, so hoch und werth,", "tokens": ["O", "theu\u00b7res", "Wort", ",", "so", "hoch", "und", "werth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df Gott sich zu den Menschen kehrt!", "tokens": ["Da\u00df", "Gott", "sich", "zu", "den", "Men\u00b7schen", "kehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O, wie ist diese Welt so sch\u00f6n,", "tokens": ["O", ",", "wie", "ist", "die\u00b7se", "Welt", "so", "sch\u00f6n", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VAFIN", "PDAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wo Gott und Mensch mitsammen gehn!", "tokens": ["Wo", "Gott", "und", "Mensch", "mit\u00b7sam\u00b7men", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Magst unschuldig tr\u00e4umen", "tokens": ["Magst", "un\u00b7schul\u00b7dig", "tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Manchen s\u00fc\u00dfen Traum,", "tokens": ["Man\u00b7chen", "s\u00fc\u00b7\u00dfen", "Traum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wirst indessen nichts vers\u00e4umen,", "tokens": ["Wirst", "in\u00b7des\u00b7sen", "nichts", "ver\u00b7s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist doch Alles Traum und Schaum. \u2013", "tokens": ["Ist", "doch", "Al\u00b7les", "Traum", "und", "Schaum", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo Jesus ging, war's sch\u00f6n und gr\u00fcn,", "tokens": ["Wo", "Je\u00b7sus", "ging", ",", "wa\u00b7r's", "sch\u00f6n", "und", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "$,", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da thaten tausend Bl\u00fcmlein bl\u00fchn;", "tokens": ["Da", "tha\u00b7ten", "tau\u00b7send", "Bl\u00fcm\u00b7lein", "bl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ach! als man an's Kreuz Ihn schlug,", "tokens": ["Und", "ach", "!", "als", "man", "an's", "Kreuz", "Ihn", "schlug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "KOUS", "PIS", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Erde h\u00fcllt' ein Leichentuch.", "tokens": ["Die", "Er\u00b7de", "h\u00fcllt'", "ein", "Lei\u00b7chen\u00b7tuch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Schlaf' am Mutterherzen,", "tokens": ["Schlaf'", "am", "Mut\u00b7ter\u00b7her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mutterherz ist treu;", "tokens": ["Mut\u00b7ter\u00b7herz", "ist", "treu", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Schlaf', vertr\u00e4ume Deine Schmerzen,", "tokens": ["Schlaf'", ",", "ver\u00b7tr\u00e4u\u00b7me", "Dei\u00b7ne", "Schmer\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Morgen bist Du neu und frei.", "tokens": ["Mor\u00b7gen", "bist", "Du", "neu", "und", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df von der falschen Amme los,", "tokens": ["La\u00df", "von", "der", "fal\u00b7schen", "Am\u00b7me", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Dann f\u00e4llst Du in Mariens Schoo\u00df;", "tokens": ["Dann", "f\u00e4llst", "Du", "in", "Ma\u00b7ri\u00b7ens", "Schoo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Mariens Schoo\u00df ist treu und warm", "tokens": ["Ma\u00b7ri\u00b7ens", "Schoo\u00df", "ist", "treu", "und", "warm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Und Friede nur in ", "tokens": ["Und", "Frie\u00b7de", "nur", "in"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "APPR"], "meter": "-+---", "measure": "dactylic.init"}}}}}