{"textgrid.poem.36463": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Hirsch, mit pr\u00e4chtigem Geweih", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Hirsch, mit pr\u00e4chtigem Geweih", "tokens": ["Ein", "Hirsch", ",", "mit", "pr\u00e4ch\u00b7ti\u00b7gem", "Ge\u00b7weih"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von achtzehn Enden, ging spazieren.", "tokens": ["Von", "acht\u00b7zehn", "En\u00b7den", ",", "ging", "spa\u00b7zie\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Hase lief vorbei,", "tokens": ["Ein", "Ha\u00b7se", "lief", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sah ihn und stutzte.", "tokens": ["Sah", "ihn", "und", "stutz\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "$."], "meter": "++-+-", "measure": "iambic.di"}, "line.5": {"text": "Starr auf allen Vieren", "tokens": ["Starr", "auf", "al\u00b7len", "Vie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Steht er, und gafft ihn an,", "tokens": ["Steht", "er", ",", "und", "gafft", "ihn", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Macht M\u00e4nnchen, geht heran,", "tokens": ["Macht", "M\u00e4nn\u00b7chen", ",", "geht", "he\u00b7ran", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Sagt:", "tokens": ["Sagt", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Ein Esel h\u00f6rte zu,", "tokens": ["Ein", "E\u00b7sel", "h\u00f6r\u00b7te", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sprach: ", "tokens": ["Sprach", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Der Hirsch that einen Seitenblick,", "tokens": ["Der", "Hirsch", "that", "ei\u00b7nen", "Sei\u00b7ten\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ging in seinen Wald zur\u00fcck!", "tokens": ["Und", "ging", "in", "sei\u00b7nen", "Wald", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}