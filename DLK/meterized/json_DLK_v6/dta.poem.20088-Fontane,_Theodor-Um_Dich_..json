{"dta.poem.20088": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Um Dich .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Beutst Du dem Geiste seine Nahrung,", "tokens": ["Beutst", "Du", "dem", "Geis\u00b7te", "sei\u00b7ne", "Nah\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So la\u00df nicht darben Dein Gem\u00fcth,", "tokens": ["So", "la\u00df", "nicht", "dar\u00b7ben", "Dein", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PTKNEG", "PAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Lebens h\u00f6chste Offenbarung", "tokens": ["Des", "Le\u00b7bens", "h\u00f6chs\u00b7te", "Of\u00b7fen\u00b7ba\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch immer aus dem Herzen bl\u00fcht.", "tokens": ["Doch", "im\u00b7mer", "aus", "dem", "Her\u00b7zen", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Gru\u00df aus frischer Knabenkehle,", "tokens": ["Ein", "Gru\u00df", "aus", "fri\u00b7scher", "Kna\u00b7ben\u00b7keh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ja mehr noch, eines Kindes Lall\u2019n,", "tokens": ["Ja", "mehr", "noch", ",", "ei\u00b7nes", "Kin\u00b7des", "Lall'n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ADV", "$,", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kann leuchtender in Deine Seele", "tokens": ["Kann", "leuch\u00b7ten\u00b7der", "in", "Dei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Weisheit aller Weisen fall\u2019n.", "tokens": ["Wie", "Weis\u00b7heit", "al\u00b7ler", "Wei\u00b7sen", "fall'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "NN", "PIAT", "NN", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Erst unter Ku\u00df und Spiel und Scherzen", "tokens": ["Erst", "un\u00b7ter", "Ku\u00df", "und", "Spiel", "und", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erkennst Du ", "tokens": ["Er\u00b7kennst", "Du"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "O lerne denken mit dem Herzen,", "tokens": ["O", "ler\u00b7ne", "den\u00b7ken", "mit", "dem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und lerne f\u00fchlen mit dem Geist.", "tokens": ["Und", "ler\u00b7ne", "f\u00fch\u00b7len", "mit", "dem", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}