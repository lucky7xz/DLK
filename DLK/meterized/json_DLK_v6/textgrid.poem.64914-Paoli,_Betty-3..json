{"textgrid.poem.64914": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwas hat mein armes Herz denn gegen Gott verbrochen,", "tokens": ["\u00bb", "was", "hat", "mein", "ar\u00b7mes", "Herz", "denn", "ge\u00b7gen", "Gott", "ver\u00b7bro\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPOSAT", "ADJA", "NN", "KON", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df, da es kaum geheilt, von ihm auf's Neu gebrochen?", "tokens": ["Da\u00df", ",", "da", "es", "kaum", "ge\u00b7heilt", ",", "von", "ihm", "auf's", "Neu", "ge\u00b7bro\u00b7chen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,", "APPR", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn neu sich \u00f6ffnete der alten Wunde Ri\u00df,", "tokens": ["Wenn", "neu", "sich", "\u00f6ff\u00b7ne\u00b7te", "der", "al\u00b7ten", "Wun\u00b7de", "Ri\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PRF", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die du vernarbt geglaubt, dann halte f\u00fcr gewi\u00df:", "tokens": ["Die", "du", "ver\u00b7narbt", "ge\u00b7glaubt", ",", "dann", "hal\u00b7te", "f\u00fcr", "ge\u00b7wi\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "VVPP", "$,", "ADV", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es war nur obenhin und schlecht geheilt dein Herz,", "tokens": ["Es", "war", "nur", "o\u00b7ben\u00b7hin", "und", "schlecht", "ge\u00b7heilt", "dein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ertrag' nun klug und stark gr\u00fcndlicher Heilung Schmerz.", "tokens": ["Er\u00b7trag'", "nun", "klug", "und", "stark", "gr\u00fcnd\u00b7li\u00b7cher", "Hei\u00b7lung", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "KON", "ADJD", "ADJA", "NN", "NN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}}}}}