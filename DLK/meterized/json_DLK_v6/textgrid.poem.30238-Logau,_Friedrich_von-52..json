{"textgrid.poem.30238": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Virulenta, dein Verstand", "tokens": ["Vi\u00b7ru\u00b7len\u00b7ta", ",", "dein", "Ver\u00b7stand"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ist wie ein seel\u00e4ndisch Land;", "tokens": ["Ist", "wie", "ein", "see\u00b7l\u00e4n\u00b7disch", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "ADJD", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wann die Gall es \u00fcberd\u00e4mmet,", "tokens": ["Wann", "die", "Gall", "es", "\u00fc\u00b7berd\u00b7\u00e4m\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist das gantze Land verschw\u00e4mmet.", "tokens": ["Ist", "das", "gant\u00b7ze", "Land", "ver\u00b7schw\u00e4m\u00b7met", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}