{"textgrid.poem.45244": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Kalender-Wahrheit", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vorm\u00e4rzlich ist der Februar,", "tokens": ["Vor\u00b7m\u00e4rz\u00b7lich", "ist", "der", "Feb\u00b7ru\u00b7ar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es preis ihn, wer da will,", "tokens": ["Es", "preis", "ihn", ",", "wer", "da", "will", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWS", "ADV", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch auf den M\u00e4rz unmittelbar", "tokens": ["Doch", "auf", "den", "M\u00e4rz", "un\u00b7mit\u00b7tel\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Folgt auch nur der April.", "tokens": ["Folgt", "auch", "nur", "der", "Ap\u00b7ril", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}