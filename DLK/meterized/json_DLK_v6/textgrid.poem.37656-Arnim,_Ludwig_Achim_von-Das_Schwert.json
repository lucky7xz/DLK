{"textgrid.poem.37656": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Das Schwert", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lange hat das Schwert gestanden,", "tokens": ["Lan\u00b7ge", "hat", "das", "Schwert", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist magnetisch so geworden,", "tokens": ["Ist", "mag\u00b7ne\u00b7tisch", "so", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "VAPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und es braucht nicht mehr zu morden,", "tokens": ["Und", "es", "braucht", "nicht", "mehr", "zu", "mor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle Noth ist \u00fcberstanden,", "tokens": ["Al\u00b7le", "Noth", "ist", "\u00fc\u00b7bers\u00b7tan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles Eisen legt sich an,", "tokens": ["Al\u00b7les", "Ei\u00b7sen", "legt", "sich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwerter wie Feilstaub h\u00e4ngen daran.", "tokens": ["Schwer\u00b7ter", "wie", "Feil\u00b7staub", "h\u00e4n\u00b7gen", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "VVFIN", "PAV", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Ruhig l\u00e4\u00dft's der K\u00f6nig stehen,", "tokens": ["Ru\u00b7hig", "l\u00e4\u00dft's", "der", "K\u00f6\u00b7nig", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will die Ordnung ja nicht st\u00f6ren,", "tokens": ["Will", "die", "Ord\u00b7nung", "ja", "nicht", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kein Metall will man mehr ehren,", "tokens": ["Kein", "Me\u00b7tall", "will", "man", "mehr", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das magnetisch nicht will gehen,", "tokens": ["Das", "mag\u00b7ne\u00b7tisch", "nicht", "will", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Das verschm\u00e4hete Metall", "tokens": ["Das", "ver\u00b7schm\u00e4\u00b7he\u00b7te", "Me\u00b7tall"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sammeln die Feinde nun \u00fcberall.", "tokens": ["Sam\u00b7meln", "die", "Fein\u00b7de", "nun", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ADV", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Pl\u00f6tzlich mit den neuen Waffen", "tokens": ["Pl\u00f6tz\u00b7lich", "mit", "den", "neu\u00b7en", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind wie Pilze sie entstanden,", "tokens": ["Sind", "wie", "Pil\u00b7ze", "sie", "ent\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch es t\u00f6nt aus allen Landen,", "tokens": ["Doch", "es", "t\u00f6nt", "aus", "al\u00b7len", "Lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df aus Blei sie Schwerter schaffen;", "tokens": ["Da\u00df", "aus", "Blei", "sie", "Schwer\u00b7ter", "schaf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Das Verachten ist so leicht,", "tokens": ["Das", "Ver\u00b7ach\u00b7ten", "ist", "so", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In dem Beachten der Meister sich zeigt.", "tokens": ["In", "dem", "Be\u00b7ach\u00b7ten", "der", "Meis\u00b7ter", "sich", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun mit gar gewaltger Feier", "tokens": ["Nun", "mit", "gar", "ge\u00b7walt\u00b7ger", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird das Eisenschwert erhoben,", "tokens": ["Wird", "das", "Ei\u00b7sen\u00b7schwert", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder mu\u00df es wieder loben,", "tokens": ["Je\u00b7der", "mu\u00df", "es", "wie\u00b7der", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer nach der alten Leier,", "tokens": ["Im\u00b7mer", "nach", "der", "al\u00b7ten", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lobten's, weil es Eisen sei,", "tokens": ["Lob\u00b7ten's", ",", "weil", "es", "Ei\u00b7sen", "sei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Rostiges Eisen springt gegen Blei.", "tokens": ["Ros\u00b7ti\u00b7ges", "Ei\u00b7sen", "springt", "ge\u00b7gen", "Blei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}}}}