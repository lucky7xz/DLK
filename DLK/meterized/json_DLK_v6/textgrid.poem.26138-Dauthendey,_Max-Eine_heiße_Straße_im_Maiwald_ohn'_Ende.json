{"textgrid.poem.26138": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Eine hei\u00dfe Stra\u00dfe im Maiwald ohn' Ende", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine hei\u00dfe Stra\u00dfe im Maiwald ohn' Ende;", "tokens": ["Ei\u00b7ne", "hei\u00b7\u00dfe", "Stra\u00b7\u00dfe", "im", "Mai\u00b7wald", "ohn'", "En\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "APPR", "NN", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Gr\u00fcnwachsende W\u00e4nde der Buchen und Eichen;", "tokens": ["Gr\u00fcn\u00b7wach\u00b7sen\u00b7de", "W\u00e4n\u00b7de", "der", "Bu\u00b7chen", "und", "Ei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "+---+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Alle Bl\u00e4tter sind tausend arbeitende H\u00e4nde.", "tokens": ["Al\u00b7le", "Bl\u00e4t\u00b7ter", "sind", "tau\u00b7send", "ar\u00b7bei\u00b7ten\u00b7de", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "CARD", "ADJA", "NN", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}}, "stanza.2": {"line.1": {"text": "Die Bl\u00e4tter todstill bei der Arbeit jetzt leben", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "tod\u00b7still", "bei", "der", "Ar\u00b7beit", "jetzt", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "ADV", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und senken und heben sich manchmal voll Zeichen,", "tokens": ["Und", "sen\u00b7ken", "und", "he\u00b7ben", "sich", "manch\u00b7mal", "voll", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KON", "VVFIN", "PRF", "ADV", "ADJD", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Als ob Wollustgedanken vor\u00fcberstreichen.", "tokens": ["Als", "ob", "Wol\u00b7lust\u00b7ge\u00b7dan\u00b7ken", "vor\u00b7\u00fc\u00b7bers\u00b7trei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "NN", "VVINF", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Ein Steinklopfer sitzt an der Stra\u00dfe daneben,", "tokens": ["Ein", "Stein\u00b7klop\u00b7fer", "sitzt", "an", "der", "Stra\u00b7\u00dfe", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "PAV", "$,"], "meter": "-+--+--+-+--", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Dem schwer die Brust \u00fcberm H\u00e4mmern schwitzt,", "tokens": ["Dem", "schwer", "die", "Brust", "\u00fc\u00b7berm", "H\u00e4m\u00b7mern", "schwitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Damit die Steine ihm mehr als nur's Essen geben.", "tokens": ["Da\u00b7mit", "die", "Stei\u00b7ne", "ihm", "mehr", "als", "nur's", "Es\u00b7sen", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PPER", "PIAT", "KOKOM", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}