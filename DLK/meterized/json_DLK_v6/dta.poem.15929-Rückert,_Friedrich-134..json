{"dta.poem.15929": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "134.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Am letzten Tag des Jahrs blick' ich zur\u00fcck aufs ganze,", "tokens": ["Am", "letz\u00b7ten", "Tag", "des", "Jahrs", "blick'", "ich", "zu\u00b7r\u00fcck", "aufs", "gan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und leuchten seh' ich es gleich einem Gottesglanze.", "tokens": ["Und", "leuch\u00b7ten", "seh'", "ich", "es", "gleich", "ei\u00b7nem", "Got\u00b7tes\u00b7glan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es war nicht lauter Licht, nicht lauter reines Gl\u00fcck,", "tokens": ["Es", "war", "nicht", "lau\u00b7ter", "Licht", ",", "nicht", "lau\u00b7ter", "rei\u00b7nes", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PIAT", "NN", "$,", "PTKNEG", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch nicht ein Schatten blieb in meinem Sinn zur\u00fcck.", "tokens": ["Doch", "nicht", "ein", "Schat\u00b7ten", "blieb", "in", "mei\u00b7nem", "Sinn", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Freuden bl\u00fchn mir noch, die Leiden sind erblichen,", "tokens": ["Die", "Freu\u00b7den", "bl\u00fchn", "mir", "noch", ",", "die", "Lei\u00b7den", "sind", "er\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ins Gef\u00fchl des Danks ist alles ausgeglichen.", "tokens": ["Und", "ins", "Ge\u00b7f\u00fchl", "des", "Danks", "ist", "al\u00b7les", "aus\u00b7ge\u00b7gli\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VAFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich gab mit Lust der Welt das beste was ich hatte,", "tokens": ["Ich", "gab", "mit", "Lust", "der", "Welt", "das", "bes\u00b7te", "was", "ich", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "ART", "ADJA", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und freute mich zu sehn, da\u00df sie's mit Dank erstatte.", "tokens": ["Und", "freu\u00b7te", "mich", "zu", "sehn", ",", "da\u00df", "sie's", "mit", "Dank", "er\u00b7stat\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$,", "KOUS", "PIS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nichts be\u00dfres w\u00fcnsch' ich mir, als da\u00df so hell und klar,", "tokens": ["Nichts", "be\u00df\u00b7res", "w\u00fcn\u00b7sch'", "ich", "mir", ",", "als", "da\u00df", "so", "hell", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJA", "VVFIN", "PPER", "PPER", "$,", "KOKOM", "KOUS", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie das vergangne, mir sei jedes k\u00fcnft'ge Jahr.", "tokens": ["Wie", "das", "ver\u00b7gang\u00b7ne", ",", "mir", "sei", "je\u00b7des", "k\u00fcnft'\u00b7ge", "Jahr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "$,", "PPER", "VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}