{"dta.poem.13282": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hiermit ward alles still; so machte man den Schlu\u00df;", "tokens": ["Hier\u00b7mit", "ward", "al\u00b7les", "still", ";", "so", "mach\u00b7te", "man", "den", "Schlu\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PTKVZ", "$.", "ADV", "VVFIN", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Herold gab hierzu das Zeichen mit dem Fu\u00df:", "tokens": ["Der", "He\u00b7rold", "gab", "hier\u00b7zu", "das", "Zei\u00b7chen", "mit", "dem", "Fu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da regte sich der Saal; die Paucken und Trompeten", "tokens": ["Da", "reg\u00b7te", "sich", "der", "Saal", ";", "die", "Pau\u00b7cken", "und", "Trom\u00b7pe\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$.", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+--++-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Erhohlten den Befehl: den Abzug anzutreten.", "tokens": ["Er\u00b7hohl\u00b7ten", "den", "Be\u00b7fehl", ":", "den", "Ab\u00b7zug", "an\u00b7zu\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "625Man stellte wiederum die beyden Reihen dar,", "tokens": ["stell\u00b7te", "wie\u00b7de\u00b7rum", "die", "bey\u00b7den", "Rei\u00b7hen", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "So wie die K\u00f6niginn zuvor gekommen war.", "tokens": ["So", "wie", "die", "K\u00f6\u00b7ni\u00b7ginn", "zu\u00b7vor", "ge\u00b7kom\u00b7men", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Drauf wurde nach und nach der ganze Saal ger\u00e4umet,", "tokens": ["Drauf", "wur\u00b7de", "nach", "und", "nach", "der", "gan\u00b7ze", "Saal", "ge\u00b7r\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "APPR", "KON", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil niemand, mit dem Hof zu gehen, sich ges\u00e4umet.", "tokens": ["Weil", "nie\u00b7mand", ",", "mit", "dem", "Hof", "zu", "ge\u00b7hen", ",", "sich", "ge\u00b7s\u00e4u\u00b7met", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich folgte dem Geleit, und ware bey der Th\u00fcr", "tokens": ["Ich", "folg\u00b7te", "dem", "Ge\u00b7leit", ",", "und", "wa\u00b7re", "bey", "der", "Th\u00fcr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "630Auch schon hinein zu gehn; so gab Thalia mir", "tokens": ["schon", "hin\u00b7ein", "zu", "gehn", ";", "so", "gab", "Tha\u00b7lia", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "$.", "ADV", "VVFIN", "NE", "PPER"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Durch ihren Fingerzeig bedeutlich zu verstehen:", "tokens": ["Durch", "ih\u00b7ren", "Fin\u00b7ger\u00b7zeig", "be\u00b7deut\u00b7lich", "zu", "ver\u00b7ste\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df nur die Tugenden in jene Zimmer gehen.", "tokens": ["Da\u00df", "nur", "die", "Tu\u00b7gen\u00b7den", "in", "je\u00b7ne", "Zim\u00b7mer", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}