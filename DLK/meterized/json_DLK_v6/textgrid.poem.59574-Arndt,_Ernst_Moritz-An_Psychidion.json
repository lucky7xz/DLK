{"textgrid.poem.59574": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "An Psychidion", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Psyche Psychidion, mein s\u00fc\u00dfes Seelchen,", "tokens": ["Psy\u00b7che", "Psy\u00b7chi\u00b7di\u00b7on", ",", "mein", "s\u00fc\u00b7\u00dfes", "Seel\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Himmlisches V\u00f6glein mit den goldnen Fl\u00fcgeln,", "tokens": ["Himm\u00b7li\u00b7sches", "V\u00f6\u00b7glein", "mit", "den", "gold\u00b7nen", "Fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Locket der Lenz dich wieder in die Freude?", "tokens": ["Lo\u00b7cket", "der", "Lenz", "dich", "wie\u00b7der", "in", "die", "Freu\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Lockt dich die Liebe?", "tokens": ["Lockt", "dich", "die", "Lie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Locket dich mehr als Lenz und mehr als Liebe,", "tokens": ["Lo\u00b7cket", "dich", "mehr", "als", "Lenz", "und", "mehr", "als", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KOUS", "NN", "KON", "ADV", "KOUS", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wie sie im Staub des Erdentals geh\u00f6rt wird?", "tokens": ["Wie", "sie", "im", "Staub", "des", "Er\u00b7den\u00b7tals", "ge\u00b7h\u00f6rt", "wird", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lockt dich die Sehnsucht wieder zu der Heimat", "tokens": ["Lockt", "dich", "die", "Sehn\u00b7sucht", "wie\u00b7der", "zu", "der", "Hei\u00b7mat"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sel'gen Gespielen?", "tokens": ["Sel'\u00b7gen", "Ge\u00b7spie\u00b7len", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Hin, wo das ", "tokens": ["Hin", ",", "wo", "das"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Sein wird, des Name Liebe klingt und Freude,", "tokens": ["Sein", "wird", ",", "des", "Na\u00b7me", "Lie\u00b7be", "klingt", "und", "Freu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "$,", "ART", "NN", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Welcher die Seelen ausgo\u00df aus der Urne,", "tokens": ["Wel\u00b7cher", "die", "See\u00b7len", "aus\u00b7go\u00df", "aus", "der", "Ur\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Ausgo\u00df die Sterne?", "tokens": ["Aus\u00b7go\u00df", "die", "Ster\u00b7ne", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "La\u00df sie dich locken, la\u00df die Engelfl\u00fcgel", "tokens": ["La\u00df", "sie", "dich", "lo\u00b7cken", ",", "la\u00df", "die", "En\u00b7gel\u00b7fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PRF", "VVINF", "$,", "VVIMP", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Klingen zum \u00c4ther, alter G\u00f6tterheimat,", "tokens": ["Klin\u00b7gen", "zum", "\u00c4\u00b7ther", ",", "al\u00b7ter", "G\u00f6t\u00b7ter\u00b7hei\u00b7mat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Da\u00df du uns unten das von oben deutest,", "tokens": ["Da\u00df", "du", "uns", "un\u00b7ten", "das", "von", "o\u00b7ben", "deu\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ART", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Himmlische Tr\u00e4ume;", "tokens": ["Himm\u00b7li\u00b7sche", "Tr\u00e4u\u00b7me", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Da\u00df du uns lehrst, warum die Demut droben", "tokens": ["Da\u00df", "du", "uns", "lehrst", ",", "wa\u00b7rum", "die", "De\u00b7mut", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lieblingin Gottes, herrscht vor hohen Thronen,", "tokens": ["Lieb\u00b7lin\u00b7gin", "Got\u00b7tes", ",", "herrscht", "vor", "ho\u00b7hen", "Thro\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was in der Unschuld schweigt und was in holder", "tokens": ["Was", "in", "der", "Un\u00b7schuld", "schweigt", "und", "was", "in", "hol\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "KON", "PWS", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Scham sich verh\u00fcllet.", "tokens": ["Scham", "sich", "ver\u00b7h\u00fcl\u00b7let", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Aber fliege nicht von uns, s\u00fc\u00dfer Vogel,", "tokens": ["A\u00b7ber", "flie\u00b7ge", "nicht", "von", "uns", ",", "s\u00fc\u00b7\u00dfer", "Vo\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Komm zu der Erde gr\u00fcnen Fluren wieder,", "tokens": ["Komm", "zu", "der", "Er\u00b7de", "gr\u00fc\u00b7nen", "Flu\u00b7ren", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Damit auch wir, was auf den Sternen wandelt,", "tokens": ["Da\u00b7mit", "auch", "wir", ",", "was", "auf", "den", "Ster\u00b7nen", "wan\u00b7delt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hoffen und sehnen.", "tokens": ["Hof\u00b7fen", "und", "seh\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Psyche Psychidion, mein s\u00fc\u00dfes Seelchen,", "tokens": ["Psy\u00b7che", "Psy\u00b7chi\u00b7di\u00b7on", ",", "mein", "s\u00fc\u00b7\u00dfes", "Seel\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Himmlisches V\u00f6glein mit den goldnen Fl\u00fcgeln,", "tokens": ["Himm\u00b7li\u00b7sches", "V\u00f6\u00b7glein", "mit", "den", "gold\u00b7nen", "Fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "M\u00f6ge liebende Sehnsucht nie dich lassen!", "tokens": ["M\u00f6\u00b7ge", "lie\u00b7ben\u00b7de", "Sehn\u00b7sucht", "nie", "dich", "las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "ADV", "PPER", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Liebender Wahn nie!", "tokens": ["Lie\u00b7ben\u00b7der", "Wahn", "nie", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}