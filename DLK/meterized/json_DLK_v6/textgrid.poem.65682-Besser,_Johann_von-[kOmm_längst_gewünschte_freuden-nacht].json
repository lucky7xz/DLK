{"textgrid.poem.65682": {"metadata": {"author": {"name": "Besser, Johann von", "birth": "N.A.", "death": "N.A."}, "title": "[kOmm l\u00e4ngst gew\u00fcnschte freuden-nacht]", "genre": "verse", "period": "N.A.", "pub_year": 1691, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm l\u00e4ngst gew\u00fcnschte freuden-nacht /", "tokens": ["Komm", "l\u00e4ngst", "ge\u00b7w\u00fcnschte", "freu\u00b7den\u00b7nacht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Als zeugin meiner Liebe /", "tokens": ["Als", "zeu\u00b7gin", "mei\u00b7ner", "Lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verh\u00fclle des gestirnes pracht /", "tokens": ["Ver\u00b7h\u00fcl\u00b7le", "des", "ge\u00b7stir\u00b7nes", "pracht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mach alles schwartz und tr\u00fcbe /", "tokens": ["Mach", "al\u00b7les", "schwartz", "und", "tr\u00fc\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "KON", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df mir an statt der g\u00fcldnen sterne seyn", "tokens": ["La\u00df", "mir", "an", "statt", "der", "g\u00fcld\u00b7nen", "ster\u00b7ne", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zwey unbefleckter augen schein.", "tokens": ["Zwey", "un\u00b7be\u00b7fleck\u00b7ter", "au\u00b7gen", "schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich w\u00fcnsche nicht dein bleiches licht /", "tokens": ["Ich", "w\u00fcn\u00b7sche", "nicht", "dein", "blei\u00b7ches", "licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Nord-stern darff mich f\u00fchren /", "tokens": ["Kein", "Nord\u00b7stern", "darff", "mich", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PRF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein glantz darff meinen augen nicht", "tokens": ["Kein", "glantz", "darff", "mei\u00b7nen", "au\u00b7gen", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den weiten himmel zieren /", "tokens": ["Den", "wei\u00b7ten", "him\u00b7mel", "zie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dieweil die glut / die mich zuvor betr\u00fcbt /", "tokens": ["Die\u00b7weil", "die", "glut", "/", "die", "mich", "zu\u00b7vor", "be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$(", "PRELS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mir ietzt die beste klarheit giebt.", "tokens": ["Mir", "ietzt", "die", "bes\u00b7te", "klar\u00b7heit", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ist das geschwinde wunder-kind /", "tokens": ["Ist", "das", "ge\u00b7schwin\u00b7de", "wun\u00b7der\u00b7kind", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So uns die noth erreget /", "tokens": ["So", "uns", "die", "noth", "er\u00b7re\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nach aller k\u00f6pffe meynung blind /", "tokens": ["Nach", "al\u00b7ler", "k\u00f6pf\u00b7fe", "mey\u00b7nung", "blind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So werd ich nichts beweget /", "tokens": ["So", "werd", "ich", "nichts", "be\u00b7we\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df ich auff dieser unbekandten bahn", "tokens": ["Da\u00df", "ich", "auff", "die\u00b7ser", "un\u00b7be\u00b7kand\u00b7ten", "bahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nicht wie bey tage schauen kan.", "tokens": ["Nicht", "wie", "bey", "ta\u00b7ge", "schau\u00b7en", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOKOM", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und ist mein auge gleich bedeckt /", "tokens": ["Und", "ist", "mein", "au\u00b7ge", "gleich", "be\u00b7deckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So schau ich doch im hertzen /", "tokens": ["So", "schau", "ich", "doch", "im", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df mir die Venus auffgesteckt", "tokens": ["Da\u00df", "mir", "die", "Ve\u00b7nus", "auff\u00b7ge\u00b7steckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Viel flammen-reiche kertzen /", "tokens": ["Viel", "flam\u00b7men\u00b7rei\u00b7che", "kert\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durch welcher glantz itzt mein gem\u00fcth erkiest /", "tokens": ["Durch", "wel\u00b7cher", "glantz", "itzt", "mein", "ge\u00b7m\u00fcth", "er\u00b7kiest", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Was lieben und geniessen ist.", "tokens": ["Was", "lie\u00b7ben", "und", "ge\u00b7nies\u00b7sen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Genug / die nacht erzeiget sich", "tokens": ["Ge\u00b7nug", "/", "die", "nacht", "er\u00b7zei\u00b7get", "sich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$(", "ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auff ihrem schwartzen throne /", "tokens": ["Auff", "ih\u00b7rem", "schwart\u00b7zen", "thro\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Venus selbst ermahnet mich", "tokens": ["Die", "Ve\u00b7nus", "selbst", "er\u00b7mah\u00b7net", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihrem kleinen sohne", "tokens": ["Mit", "ih\u00b7rem", "klei\u00b7nen", "soh\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zu suchen die / so meine freyheit f\u00e4llt /", "tokens": ["Zu", "su\u00b7chen", "die", "/", "so", "mei\u00b7ne", "frey\u00b7heit", "f\u00e4llt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "$(", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und meinen sinn gefangen h\u00e4lt.", "tokens": ["Und", "mei\u00b7nen", "sinn", "ge\u00b7fan\u00b7gen", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Drum liebste komm / und sey bereit /", "tokens": ["Drum", "liebs\u00b7te", "komm", "/", "und", "sey", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "VVFIN", "$(", "KON", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die stunden haben fl\u00fcgel /", "tokens": ["Die", "stun\u00b7den", "ha\u00b7ben", "fl\u00fc\u00b7gel", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Ph\u00f6bus ist gewi\u00df nicht weit /", "tokens": ["Der", "Ph\u00f6\u00b7bus", "ist", "ge\u00b7wi\u00df", "nicht", "weit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er r\u00fchret seine z\u00fcgel /", "tokens": ["Er", "r\u00fch\u00b7ret", "sei\u00b7ne", "z\u00fc\u00b7gel", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dieweil es sich nicht allzuwohl geb\u00fchrt /", "tokens": ["Die\u00b7weil", "es", "sich", "nicht", "all\u00b7zu\u00b7wohl", "ge\u00b7b\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df uns der tag nach bette f\u00fchrt.", "tokens": ["Da\u00df", "uns", "der", "tag", "nach", "bet\u00b7te", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}}}}