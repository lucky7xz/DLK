{"dta.poem.8372": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Martin Opitzen Sch\u00e4fferey  \n von der Nimfen Hercinie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1630", "urn": "urn:nbn:de:kobv:b4-200905197863", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Sch\u00e4fferey Von der Nimfen Hercinie. Breslau, 1630."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr hirten/ die jhr kompt zue schawen", "tokens": ["Ihr", "hir\u00b7ten", "/", "die", "jhr", "kompt", "zue", "scha\u00b7wen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PPER", "VVFIN", "PTKZU", "VVINF"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die quelle/ diese berg' vndt awen/", "tokens": ["Die", "quel\u00b7le", "/", "die\u00b7se", "ber\u00b7g'", "vndt", "a\u00b7wen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PDS", "VVFIN", "KON", "NE", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihr hirten/ lauffer nicht vor mir/", "tokens": ["Ihr", "hir\u00b7ten", "/", "lauf\u00b7fer", "nicht", "vor", "mir", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ADJD", "PTKNEG", "APPR", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bin des ortes Nimfe hier.", "tokens": ["Ich", "bin", "des", "or\u00b7tes", "Nim\u00b7fe", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Zacken den jhr mich seht gie\u00dfen/", "tokens": ["Der", "Za\u00b7cken", "den", "jhr", "mich", "seht", "gie\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "PRF", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der minste von den kleinen fl\u00fc\u00dfen/", "tokens": ["Der", "mins\u00b7te", "von", "den", "klei\u00b7nen", "fl\u00fc\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fchrt oben silber klare flut/", "tokens": ["F\u00fchrt", "o\u00b7ben", "sil\u00b7ber", "kla\u00b7re", "flut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sein reiner sandt tregt goldt vndt guet.", "tokens": ["Sein", "rei\u00b7ner", "sandt", "tregt", "goldt", "vndt", "guet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJD", "VVFIN", "NE", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Warumb sich freundt vndt feinde neiden/", "tokens": ["Wa\u00b7rumb", "sich", "freundt", "vndt", "fein\u00b7de", "nei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADJD", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Darbey k\u00f6nnt jhr die schaffe weiden.", "tokens": ["Dar\u00b7bey", "k\u00f6nnt", "jhr", "die", "schaf\u00b7fe", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wer goldt zue waschen erst gelehrt/", "tokens": ["Wer", "goldt", "zue", "wa\u00b7schen", "erst", "ge\u00b7lehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PTKZU", "VVINF", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Hatt ja die menschen hoch versehrt!", "tokens": ["Hatt", "ja", "die", "men\u00b7schen", "hoch", "ver\u00b7sehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.13": {"text": "Die g\u00f6tter lieben solche sinnen/", "tokens": ["Die", "g\u00f6t\u00b7ter", "lie\u00b7ben", "sol\u00b7che", "sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Die g\u00fcldinn' einfalt lieben k\u00f6nnen;", "tokens": ["Die", "g\u00fcl\u00b7dinn'", "ein\u00b7falt", "lie\u00b7ben", "k\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "So kompt/ jhr hirten/ schawet an/", "tokens": ["So", "kompt", "/", "jhr", "hir\u00b7ten", "/", "scha\u00b7wet", "an", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "PPOSAT", "NN", "$(", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Was ich/ vndt kein mensch zeigen kan.", "tokens": ["Was", "ich", "/", "vndt", "kein", "mensch", "zei\u00b7gen", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "$(", "KON", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}