{"textgrid.poem.64949": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Eule tr\u00e4ger Flug umschwirret mich so schaurig;", "tokens": ["Der", "Eu\u00b7le", "tr\u00e4\u00b7ger", "Flug", "um\u00b7schwir\u00b7ret", "mich", "so", "schau\u00b7rig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es braus't der dunkle Wald, die Wolken blicken traurig.", "tokens": ["Es", "braus't", "der", "dunk\u00b7le", "Wald", ",", "die", "Wol\u00b7ken", "bli\u00b7cken", "trau\u00b7rig", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es scheint, als ob Natur tief in das Menschenherz", "tokens": ["Es", "scheint", ",", "als", "ob", "Na\u00b7tur", "tief", "in", "das", "Men\u00b7schen\u00b7herz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "NN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Ahnung bringen wollt' von nahem, hei\u00dfem Schmerz.", "tokens": ["Die", "Ah\u00b7nung", "brin\u00b7gen", "wollt'", "von", "na\u00b7hem", ",", "hei\u00b7\u00dfem", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Als wollte ernst und streng sie Strafe aller S\u00fcnden,", "tokens": ["Als", "woll\u00b7te", "ernst", "und", "streng", "sie", "Stra\u00b7fe", "al\u00b7ler", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "ADJD", "KON", "VVFIN", "PPER", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und allen Gl\u00fccklichen des Gl\u00fcckes Tod verk\u00fcnden.", "tokens": ["Und", "al\u00b7len", "Gl\u00fcck\u00b7li\u00b7chen", "des", "Gl\u00fc\u00b7ckes", "Tod", "ver\u00b7k\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Allein was k\u00fcmmert mich die\u00df warnende Gesicht?", "tokens": ["Al\u00b7lein", "was", "k\u00fcm\u00b7mert", "mich", "die\u00df", "war\u00b7nen\u00b7de", "Ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PPER", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich wei\u00df von keiner Schuld und gl\u00fccklich bin ich nicht.", "tokens": ["Ich", "wei\u00df", "von", "kei\u00b7ner", "Schuld", "und", "gl\u00fcck\u00b7lich", "bin", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "KON", "ADJD", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}