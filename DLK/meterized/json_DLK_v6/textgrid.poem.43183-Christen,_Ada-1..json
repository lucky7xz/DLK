{"textgrid.poem.43183": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur Du allein, Du schautest wie ich litt,", "tokens": ["Nur", "Du", "al\u00b7lein", ",", "Du", "schau\u00b7test", "wie", "ich", "litt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "$,", "PPER", "VVFIN", "KOKOM", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur Du allein hast meiner Qual geglaubt,", "tokens": ["Nur", "Du", "al\u00b7lein", "hast", "mei\u00b7ner", "Qual", "ge\u00b7glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du schirmtest die Gedanken mir im Haupt \u2013", "tokens": ["Du", "schirm\u00b7test", "die", "Ge\u00b7dan\u00b7ken", "mir", "im", "Haupt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als Nacht mit Licht in meiner Seele stritt.", "tokens": ["Als", "Nacht", "mit", "Licht", "in", "mei\u00b7ner", "See\u00b7le", "stritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nur Du allein, Du lieh'st mir Deine Hand,", "tokens": ["Nur", "Du", "al\u00b7lein", ",", "Du", "lieh'st", "mir", "Dei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als ich einst kam, geschm\u00e4het und bedroht \u2013", "tokens": ["Als", "ich", "einst", "kam", ",", "ge\u00b7schm\u00e4\u00b7het", "und", "be\u00b7droht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "VVPP", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als sich kein heimathlicher Heerd mir bot,", "tokens": ["Als", "sich", "kein", "hei\u00b7math\u00b7li\u00b7cher", "Heerd", "mir", "bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PIAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Als ich allein auf weiter Erde stand ...", "tokens": ["Als", "ich", "al\u00b7lein", "auf", "wei\u00b7ter", "Er\u00b7de", "stand", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nur Du allein, Du hast mich nie betr\u00fcbt,", "tokens": ["Nur", "Du", "al\u00b7lein", ",", "Du", "hast", "mich", "nie", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Seit Du erschaut, wie ich so tiefverarmt \u2013", "tokens": ["Seit", "Du", "er\u00b7schaut", ",", "wie", "ich", "so", "tief\u00b7ver\u00b7armt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "PWAV", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur Du allein hast Dich einst mein ", "tokens": ["Nur", "Du", "al\u00b7lein", "hast", "Dich", "einst", "mein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADV", "VAFIN", "PPER", "ADV", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hast mich besch\u00fctzt \u2013 und hast mich ", "tokens": ["Hast", "mich", "be\u00b7sch\u00fctzt", "\u2013", "und", "hast", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$(", "KON", "VAFIN", "PPER"], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}