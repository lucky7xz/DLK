{"textgrid.poem.54499": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleich wie das Heil der Welt/ mit hellem Glantz vmbgeben/", "tokens": ["Gleich", "wie", "das", "Heil", "der", "Welt", "/", "mit", "hel\u00b7lem", "Glantz", "vmb\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "ART", "NN", "$(", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auff Thabors Spitze steht/ wie seiner Kleider Licht/", "tokens": ["Auff", "Tha\u00b7bors", "Spit\u00b7ze", "steht", "/", "wie", "sei\u00b7ner", "Klei\u00b7der", "Licht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "$(", "KOKOM", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hell-Stralende verblend't/ der J\u00fcnger Angesicht/", "tokens": ["Hell\u00b7S\u00b7tra\u00b7len\u00b7de", "ver\u00b7blend't", "/", "der", "J\u00fcn\u00b7ger", "An\u00b7ge\u00b7sicht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So scheint/ wer Christum liebt/ in neu-verkl\u00e4rtem Leben.", "tokens": ["So", "scheint", "/", "wer", "Chris\u00b7tum", "liebt", "/", "in", "neu\u00b7ver\u00b7kl\u00e4r\u00b7tem", "Le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "PWS", "NE", "VVFIN", "$(", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hier schaust du Mosen nur mit dem The\u00dfbiten schweben", "tokens": ["Hier", "schaust", "du", "Mo\u00b7sen", "nur", "mit", "dem", "The\u00df\u00b7bi\u00b7ten", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ADV", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Kaum einen Augenblick/ dort flihn die Engel nicht;", "tokens": ["Kaum", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", "/", "dort", "flihn", "die", "En\u00b7gel", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$(", "ADV", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dort ist der Hauffen der geheime Ding' au\u00dfspricht;", "tokens": ["Dort", "ist", "der", "Hauf\u00b7fen", "der", "ge\u00b7hei\u00b7me", "Ding'", "au\u00df\u00b7spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dort sind die Gottes Ruhm mit freyem Mund erheben;", "tokens": ["Dort", "sind", "die", "Got\u00b7tes", "Ruhm", "mit", "frey\u00b7em", "Mund", "er\u00b7he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hier hat de\u00df Himmels F\u00fcrst mit Wolcken sich vmbdeckt/", "tokens": ["Hier", "hat", "de\u00df", "Him\u00b7mels", "F\u00fcrst", "mit", "Wol\u00b7cken", "sich", "vmb\u00b7deckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "APPR", "NN", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Hier wird durch seine Stimm der J\u00fcnger Hertz erschreckt;", "tokens": ["Hier", "wird", "durch", "sei\u00b7ne", "Stimm", "der", "J\u00fcn\u00b7ger", "Hertz", "er\u00b7schreckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dort h\u00f6rt man seinen Trost/ dort kan man klar Ihn schauen/", "tokens": ["Dort", "h\u00f6rt", "man", "sei\u00b7nen", "Trost", "/", "dort", "kan", "man", "klar", "Ihn", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPOSAT", "NN", "$(", "ADV", "VMFIN", "PIS", "ADJD", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn hier ein einig nun/ so fr\u00f6lich Petrum macht:", "tokens": ["Wenn", "hier", "ein", "ei\u00b7nig", "nun", "/", "so", "fr\u00f6\u00b7lich", "Pet\u00b7rum", "macht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJD", "ADV", "$(", "ADV", "ADJD", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was wird wol ewig thun? drumb eilt ihr Leut' vnd wacht/", "tokens": ["Was", "wird", "wol", "e\u00b7wig", "thun", "?", "drumb", "eilt", "ihr", "Leut'", "vnd", "wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "VVINF", "$.", "PAV", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vnd last vns fr\u00f6lich dort/ nicht hier die H\u00fctten bauen.", "tokens": ["Vnd", "last", "vns", "fr\u00f6\u00b7lich", "dort", "/", "nicht", "hier", "die", "H\u00fct\u00b7ten", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "$(", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}