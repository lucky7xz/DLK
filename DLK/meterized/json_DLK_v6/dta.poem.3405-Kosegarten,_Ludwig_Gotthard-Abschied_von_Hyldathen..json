{"dta.poem.3405": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Abschied von Hyldathen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Goldne Tag', ihr entfloht, ehe mein lauschend", "tokens": ["Gold\u00b7ne", "Tag'", ",", "ihr", "ent\u00b7floht", ",", "e\u00b7he", "mein", "lau\u00b7schend"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ohr", "tokens": ["Ohr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Eures reissenden Flugs sausenden Schwung vernahm,", "tokens": ["Eu\u00b7res", "reis\u00b7sen\u00b7den", "Flugs", "sau\u00b7sen\u00b7den", "Schwung", "ver\u00b7nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.4": {"text": "Eh' die lechzende Zunge", "tokens": ["Eh'", "die", "lech\u00b7zen\u00b7de", "Zun\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Eures Kelches zur Gn\u00fcge trank.", "tokens": ["Eu\u00b7res", "Kel\u00b7ches", "zur", "Gn\u00fc\u00b7ge", "trank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Goldne Tage, darin jegliches Morgenroth", "tokens": ["Gold\u00b7ne", "Ta\u00b7ge", ",", "da\u00b7rin", "jeg\u00b7li\u00b7ches", "Mor\u00b7gen\u00b7roth"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PAV", "ADJA", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Mir es wehte, darin jegliches Abendk\u00fchl", "tokens": ["Mir", "es", "weh\u00b7te", ",", "da\u00b7rin", "jeg\u00b7li\u00b7ches", "A\u00b7bend\u00b7k\u00fchl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PPER", "VVFIN", "$,", "PAV", "ADJA", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Mir es s\u00e4uselt: \u201eNoch bist du", "tokens": ["Mir", "es", "s\u00e4u\u00b7selt", ":", "\u201e", "Noch", "bist", "du"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "PPER", "VVFIN", "$.", "$(", "ADV", "VAFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schuldlos, J\u00fcngling, und fessellos.", "tokens": ["Schuld\u00b7los", ",", "J\u00fcng\u00b7ling", ",", "und", "fes\u00b7sel\u00b7los", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$,", "KON", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Klaget, Br\u00fcder! Sie flohn. Klaget die Fl\u00fcchtlinge!", "tokens": ["Kla\u00b7get", ",", "Br\u00fc\u00b7der", "!", "Sie", "flohn", ".", "Kla\u00b7get", "die", "Fl\u00fccht\u00b7lin\u00b7ge", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "PPER", "VVFIN", "$.", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Jammernd klagt sie mein Lied. Jeglicher Laut wird", "tokens": ["Jam\u00b7mernd", "klagt", "sie", "mein", "Lied", ".", "Jeg\u00b7li\u00b7cher", "Laut", "wird"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "PIS", "APPR", "VAFIN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ach,", "tokens": ["Ach", ","], "token_info": ["word", "punct"], "pos": ["ITJ", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Und verweht in den Herbstwind,", "tokens": ["Und", "ver\u00b7weht", "in", "den", "Herbst\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Der die Stoppel her\u00fcberbl\u00e4s't.", "tokens": ["Der", "die", "Stop\u00b7pel", "her\u00b7\u00fcber\u00b7bl\u00e4s'", "t."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ART", "ART", "NN", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kalt bl\u00e4s't, Br\u00fcder, der Wind; Nebel entdampft", "tokens": ["Kalt", "bl\u00e4s't", ",", "Br\u00fc\u00b7der", ",", "der", "Wind", ";", "Ne\u00b7bel", "ent\u00b7dampft"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "NN", "$,", "ART", "NN", "$.", "NN", "VVFIN"], "meter": "+-+--++--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "der Flur;", "tokens": ["der", "Flur", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Wolken klimmen herauf; Schauer und St\u00fcrme drohn.", "tokens": ["Wol\u00b7ken", "klim\u00b7men", "her\u00b7auf", ";", "Schau\u00b7er", "und", "St\u00fcr\u00b7me", "drohn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$.", "NE", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Auf im Schauern und St\u00fcrmen,", "tokens": ["Auf", "im", "Schau\u00b7ern", "und", "St\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Und geleitet den Scheidenden!", "tokens": ["Und", "ge\u00b7lei\u00b7tet", "den", "Schei\u00b7den\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "ART", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Mich entbeut mein Geschick hin, wo des Oceans", "tokens": ["Mich", "ent\u00b7beut", "mein", "Ge\u00b7schick", "hin", ",", "wo", "des", "O\u00b7ceans"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "PWAV", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wogendonner ein Land furchtbar und sch\u00f6n umbr\u00fcllt,", "tokens": ["Wo\u00b7gen\u00b7don\u00b7ner", "ein", "Land", "furcht\u00b7bar", "und", "sch\u00f6n", "um\u00b7br\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wo die moosige Scheitel", "tokens": ["Wo", "die", "moo\u00b7si\u00b7ge", "Schei\u00b7tel"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Weit umschauend der Rugard hebt.", "tokens": ["Weit", "um\u00b7schau\u00b7end", "der", "Ru\u00b7gard", "hebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ART", "NE", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Schickung, warum so rasch? Warum so fl\u00fcchtig, Zeit?", "tokens": ["Schi\u00b7ckung", ",", "wa\u00b7rum", "so", "rasch", "?", "Wa\u00b7rum", "so", "fl\u00fcch\u00b7tig", ",", "Zeit", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "ADJD", "$.", "PWAV", "ADV", "ADJD", "$,", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Sehnend ruft dir mein Lied. \u2014 Nicht, wie der", "tokens": ["Seh\u00b7nend", "ruft", "dir", "mein", "Lied", ".", "Nicht", ",", "wie", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$(", "PTKNEG", "$,", "PWAV", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Thor dir ruft,", "tokens": ["Thor", "dir", "ruft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Der verzweifelnd zum Gestern,", "tokens": ["Der", "ver\u00b7zwei\u00b7felnd", "zum", "Ge\u00b7stern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dem verlornen: Sey heute! spricht.", "tokens": ["Dem", "ver\u00b7lor\u00b7nen", ":", "Sey", "heu\u00b7te", "!", "spricht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVINF", "$.", "VAFIN", "ADV", "$.", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Euren strudelnden Sturz staunt' ich nicht m\u00fcssig an,", "tokens": ["Eu\u00b7ren", "stru\u00b7deln\u00b7den", "Sturz", "staunt'", "ich", "nicht", "m\u00fcs\u00b7sig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "ADJD", "PTKVZ", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Sch\u00f6ne Tage! Ich hielt \u2014 zeugt es, die ihr mich", "tokens": ["Sch\u00f6\u00b7ne", "Ta\u00b7ge", "!", "Ich", "hielt", "zeugt", "es", ",", "die", "ihr", "mich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "$(", "VVFIN", "PPER", "$,", "PRELS", "PPER", "PRF"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "kennt \u2014", "tokens": ["kennt"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Mit der Rechten die Weisheit,", "tokens": ["Mit", "der", "Rech\u00b7ten", "die", "Weis\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Mit der Linken die Freud' umarmt.", "tokens": ["Mit", "der", "Lin\u00b7ken", "die", "Freud'", "um\u00b7armt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "Nimmer lechzte mein Schwert Hader noch Bruderblut;", "tokens": ["Nim\u00b7mer", "lechz\u00b7te", "mein", "Schwert", "Ha\u00b7der", "noch", "Bru\u00b7der\u00b7blut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN", "ADV", "NN", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Nimmer h\u00f6hnte mein Lied Tugend und Sittlichkeit;", "tokens": ["Nim\u00b7mer", "h\u00f6hn\u00b7te", "mein", "Lied", "Tu\u00b7gend", "und", "Sitt\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Nie umrankte mich, Wollust,", "tokens": ["Nie", "um\u00b7rank\u00b7te", "mich", ",", "Wol\u00b7lust", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Dein entmannender Circenarm.", "tokens": ["Dein", "ent\u00b7man\u00b7nen\u00b7der", "Cir\u00b7ce\u00b7narm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Wonne, Wonne, noch f\u00e4rbt heilige J\u00fcnglingsscham", "tokens": ["Won\u00b7ne", ",", "Won\u00b7ne", ",", "noch", "f\u00e4rbt", "hei\u00b7li\u00b7ge", "J\u00fcng\u00b7lings\u00b7scham"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "ADV", "VVFIN", "ADJA", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Meine Wange! Noch gl\u00e4nzt Unschuld und Reinigkeit", "tokens": ["Mei\u00b7ne", "Wan\u00b7ge", "!", "Noch", "gl\u00e4nzt", "Un\u00b7schuld", "und", "Rei\u00b7nig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "ADV", "VVFIN", "NN", "KON", "NN"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Mir im Auge; noch schau' ich", "tokens": ["Mir", "im", "Au\u00b7ge", ";", "noch", "schau'", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "$.", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fest dem Edler'n ins Angesicht.", "tokens": ["Fest", "dem", "Ed\u00b7ler'n", "ins", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "Wonne, Wonne, noch straft mich der verjubelten", "tokens": ["Won\u00b7ne", ",", "Won\u00b7ne", ",", "noch", "straft", "mich", "der", "ver\u00b7ju\u00b7bel\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "ADJA"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Stunden keine! Mich reut keine der stilleren,", "tokens": ["Stun\u00b7den", "kei\u00b7ne", "!", "Mich", "reut", "kei\u00b7ne", "der", "stil\u00b7le\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "$.", "PPER", "VVFIN", "PIAT", "ART", "ADJA", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wo dein Schleier mich h\u00fcllte,", "tokens": ["Wo", "dein", "Schlei\u00b7er", "mich", "h\u00fcll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dichterfreundin, Melancholie!", "tokens": ["Dich\u00b7ter\u00b7freun\u00b7din", ",", "Me\u00b7lan\u00b7cho\u00b7lie", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "D\u00e4mmrung, werde nicht Nacht! Werde nicht starrer", "tokens": ["D\u00e4mm\u00b7rung", ",", "wer\u00b7de", "nicht", "Nacht", "!", "Wer\u00b7de", "nicht", "star\u00b7rer"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VAFIN", "PTKNEG", "NN", "$.", "VAFIN", "PTKNEG", "ADJA"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Gram,", "tokens": ["Gram", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "S\u00fcsse Wehmuth, wenn nun richtend die Stunde", "tokens": ["S\u00fcs\u00b7se", "Weh\u00b7muth", ",", "wenn", "nun", "rich\u00b7tend", "die", "Stun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "KOUS", "ADV", "ADJD", "ART", "NN"], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "schl\u00e4gt,", "tokens": ["schl\u00e4gt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Die den Bruder vom Bruder,", "tokens": ["Die", "den", "Bru\u00b7der", "vom", "Bru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Die den Liebling vom Liebling trennt.", "tokens": ["Die", "den", "Lieb\u00b7ling", "vom", "Lieb\u00b7ling", "trennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.12": {"line.1": {"text": "Euch, ihr Trauten, vielleicht nimmer zu sehn,", "tokens": ["Euch", ",", "ihr", "Trau\u00b7ten", ",", "viel\u00b7leicht", "nim\u00b7mer", "zu", "sehn", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "vielleicht", "tokens": ["viel\u00b7leicht"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Euch vergessen zu seyn, wenn mein Gesang nicht", "tokens": ["Euch", "ver\u00b7ges\u00b7sen", "zu", "seyn", ",", "wenn", "mein", "Ge\u00b7sang", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVPP", "PTKZU", "VAINF", "$,", "KOUS", "PPOSAT", "NN", "PTKNEG"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "mehr", "tokens": ["mehr"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Um euch lispelt \u2014 diess f\u00fcrchtet", "tokens": ["Um", "euch", "lis\u00b7pelt", "diess", "f\u00fcrch\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUI", "PPER", "VVFIN", "$(", "PDS", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Meine Seele, diess \u00e4ngstet mich.", "tokens": ["Mei\u00b7ne", "See\u00b7le", ",", "diess", "\u00e4ngs\u00b7tet", "mich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VVFIN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.13": {"line.1": {"text": "Euch vergessen zu seyn \u2014 Foltergedanke, dir", "tokens": ["Euch", "ver\u00b7ges\u00b7sen", "zu", "seyn", "Fol\u00b7ter\u00b7ge\u00b7dan\u00b7ke", ",", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVPP", "PTKZU", "VAINF", "$(", "NN", "$,", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bebt der trotzige Geist. \u00c4rger, denn Gr\u00e4bernacht,", "tokens": ["Bebt", "der", "trot\u00b7zi\u00b7ge", "Geist", ".", "\u00c4r\u00b7ger", ",", "denn", "Gr\u00e4\u00b7ber\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "NN", "$,", "KON", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Und das Grauen des Nichtseyns", "tokens": ["Und", "das", "Grau\u00b7en", "des", "Nich\u00b7tseyns"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+--++", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Hasst mein Geist die Vergessenheit.", "tokens": ["Hasst", "mein", "Geist", "die", "Ver\u00b7ges\u00b7sen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.14": {"line.1": {"text": "Nein, ihr Trauten, ihr m\u00fcsst mein nicht vergessen,", "tokens": ["Nein", ",", "ihr", "Trau\u00b7ten", ",", "ihr", "m\u00fcsst", "mein", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "$,", "PPER", "VMFIN", "PPOSAT", "PTKNEG", "VVPP", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "m\u00fcsst,", "tokens": ["m\u00fcsst", ","], "token_info": ["word", "punct"], "pos": ["VMFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Wenn ihr Fluren durchwallt, wenn ihr in Zirkeln", "tokens": ["Wenn", "ihr", "Flu\u00b7ren", "durch\u00b7wallt", ",", "wenn", "ihr", "in", "Zir\u00b7keln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "jauchzt,", "tokens": ["jauchzt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Wo ich wandelt' und jauchzte,", "tokens": ["Wo", "ich", "wan\u00b7delt'", "und", "jauchz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Denken eures Verlassenen.", "tokens": ["Den\u00b7ken", "eu\u00b7res", "Ver\u00b7las\u00b7se\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.15": {"line.1": {"text": "Wenn der k\u00e4mpfende Geist einstens die Fesseln", "tokens": ["Wenn", "der", "k\u00e4mp\u00b7fen\u00b7de", "Geist", "eins\u00b7tens", "die", "Fes\u00b7seln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "sprengt,", "tokens": ["sprengt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Ins Unendliche fleugt, Leben und Freiheit jauchzt \u2014", "tokens": ["Ins", "Un\u00b7end\u00b7li\u00b7che", "fleugt", ",", "Le\u00b7ben", "und", "Frei\u00b7heit", "jauchzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "NN", "KON", "NN", "VVFIN", "$("], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "M\u00fcsst ihr \u201eBruder\u201c mich klagen,", "tokens": ["M\u00fcsst", "ihr", "\u201e", "Bru\u00b7der", "\u201c", "mich", "kla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "$(", "NN", "$(", "PPER", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "\u201efahr wohl, Bruder, du warst uns werth.\u201c", "tokens": ["\u201e", "fahr", "wohl", ",", "Bru\u00b7der", ",", "du", "warst", "uns", "werth", ".", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ADV", "$,", "NN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$.", "$("], "meter": "+-+--+-+", "measure": "gly