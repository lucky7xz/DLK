{"dta.poem.9612": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine \u00fcbersendete nelcke.  \n C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du sendest mir das blut von deinem mund und wangen/", "tokens": ["Du", "sen\u00b7dest", "mir", "das", "blut", "von", "dei\u00b7nem", "mund", "und", "wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und eine nelcke mu\u00df dein theurer bote seyn:", "tokens": ["Und", "ei\u00b7ne", "nel\u00b7cke", "mu\u00df", "dein", "theu\u00b7rer", "bo\u00b7te", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PPOSAT", "ADJD", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich schaue zwar das blut auf wei\u00dfen feldern prangen;", "tokens": ["Ich", "schau\u00b7e", "zwar", "das", "blut", "auf", "wei\u00b7\u00dfen", "fel\u00b7dern", "pran\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch stellt die w\u00e4rmde sich hier nicht als nachbar ein.", "tokens": ["Doch", "stellt", "die", "w\u00e4rm\u00b7de", "sich", "hier", "nicht", "als", "nach\u00b7bar", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "VAFIN", "PRF", "ADV", "PTKNEG", "KOUS", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die negel ehr ich zwar mit mehr als kausend k\u00fc\u00dfen/", "tokens": ["Die", "ne\u00b7gel", "ehr", "ich", "zwar", "mit", "mehr", "als", "kau\u00b7send", "k\u00fc\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "ADV", "APPR", "PIAT", "KOKOM", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich bin dazu verpflicht/ sie kommt aus deiner hand;", "tokens": ["Ich", "bin", "da\u00b7zu", "ver\u00b7pflicht", "/", "sie", "kommt", "aus", "dei\u00b7ner", "hand", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "VVFIN", "$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch wil nichts feuchtes mir auf mund und lippen fl\u00fc\u00dfen:", "tokens": ["Doch", "wil", "nichts", "feuch\u00b7tes", "mir", "auf", "mund", "und", "lip\u00b7pen", "fl\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "ADJA", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was geist und w\u00e4rmde heist/ ist ihr gantz unbekandt.", "tokens": ["Was", "geist", "und", "w\u00e4rm\u00b7de", "heist", "/", "ist", "ihr", "gantz", "un\u00b7be\u00b7kandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "VAFIN", "ADJD", "$(", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie wei\u00df mit honigthau mir nicht den mund zu netzen/", "tokens": ["Sie", "wei\u00df", "mit", "ho\u00b7nigt\u00b7hau", "mir", "nicht", "den", "mund", "zu", "net\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPER", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie kennt das schm\u00e4tzeln nicht und di\u00df was z\u00fcngeln heist/", "tokens": ["Sie", "kennt", "das", "schm\u00e4t\u00b7zeln", "nicht", "und", "di\u00df", "was", "z\u00fcn\u00b7geln", "heist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "VVFIN", "PTKNEG", "KON", "PDS", "PIS", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sie wei\u00df den purpur nicht auf meinen mund zu setzen/", "tokens": ["Sie", "wei\u00df", "den", "pur\u00b7pur", "nicht", "auf", "mei\u00b7nen", "mund", "zu", "set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "PTKNEG", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich f\u00fchle nicht/ was mich auf meine lippen beist.", "tokens": ["Ich", "f\u00fch\u00b7le", "nicht", "/", "was", "mich", "auf", "mei\u00b7ne", "lip\u00b7pen", "beist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "PWS", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sie wei\u00df mir meinen mund nicht schl\u00fcpfrig aufzuschlie\u00dfen/", "tokens": ["Sie", "wei\u00df", "mir", "mei\u00b7nen", "mund", "nicht", "schl\u00fcpf\u00b7rig", "auf\u00b7zu\u00b7schlie\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die feuchte k\u00fctzelung kennt diese nelcke nicht.", "tokens": ["Die", "feuch\u00b7te", "k\u00fct\u00b7ze\u00b7lung", "kennt", "die\u00b7se", "nel\u00b7cke", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PDS", "VVFIN", "PTKNEG", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Durch warmes b\u00f6ben kan sie keinen ku\u00df vers\u00fc\u00dfen/", "tokens": ["Durch", "war\u00b7mes", "b\u00f6\u00b7ben", "kan", "sie", "kei\u00b7nen", "ku\u00df", "ver\u00b7s\u00fc\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVINF", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Weil n\u00e4\u00dfe/ geist und blut der nelcke stets gebricht.", "tokens": ["Weil", "n\u00e4\u00b7\u00dfe", "/", "geist", "und", "blut", "der", "nel\u00b7cke", "stets", "ge\u00b7bricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "VVPP", "KON", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Doch k\u00f6mmt die nelcke mir nicht leichtlich aus dem munde/", "tokens": ["Doch", "k\u00f6mmt", "die", "nel\u00b7cke", "mir", "nicht", "leicht\u00b7lich", "aus", "dem", "mun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "PTKNEG", "ADJD", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ich aber netze fie durch einen hei\u00dfen ku\u00df.", "tokens": ["Ich", "a\u00b7ber", "net\u00b7ze", "fie", "durch", "ei\u00b7nen", "hei\u00b7\u00dfen", "ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "FM", "FM", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ach freundin! w\u00fcnsche mir doch zeitlich diese stunde/", "tokens": ["Ach", "freun\u00b7din", "!", "w\u00fcn\u00b7sche", "mir", "doch", "zeit\u00b7lich", "die\u00b7se", "stun\u00b7de", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "VVFIN", "PPER", "ADV", "ADJD", "PDAT", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da mich entz\u00fccken kan dein reicher \u00fcberflu\u00df.", "tokens": ["Da", "mich", "ent\u00b7z\u00fc\u00b7cken", "kan", "dein", "rei\u00b7cher", "\u00fc\u00b7berf\u00b7lu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "PPOSAT", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es reist mich aus mir selbst ein s\u00fcsses angedencken/", "tokens": ["Es", "reist", "mich", "aus", "mir", "selbst", "ein", "s\u00fcs\u00b7ses", "an\u00b7ge\u00b7den\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Was mir vor h\u00f6flichkeit dein ku\u00df hat angethan!", "tokens": ["Was", "mir", "vor", "h\u00f6f\u00b7lich\u00b7keit", "dein", "ku\u00df", "hat", "an\u00b7ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Du wirst mir einen ku\u00df bey dieser nelcke schencken/", "tokens": ["Du", "wirst", "mir", "ei\u00b7nen", "ku\u00df", "bey", "die\u00b7ser", "nel\u00b7cke", "schen\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und zeigen/ da\u00df dein mund mehr als die blume kan.", "tokens": ["Und", "zei\u00b7gen", "/", "da\u00df", "dein", "mund", "mehr", "als", "die", "blu\u00b7me", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$(", "KOUS", "PPOSAT", "NN", "PIAT", "KOKOM", "ART", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}