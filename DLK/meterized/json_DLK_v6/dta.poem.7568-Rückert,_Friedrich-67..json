{"dta.poem.7568": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "67.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe nichts erdacht, nur manches ausgedeutet,", "tokens": ["Ich", "ha\u00b7be", "nichts", "er\u00b7dacht", ",", "nur", "man\u00b7ches", "aus\u00b7ge\u00b7deu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVPP", "$,", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gegraben keinen Schacht, nur manchen ausgebeutet.", "tokens": ["Ge\u00b7gra\u00b7ben", "kei\u00b7nen", "Schacht", ",", "nur", "man\u00b7chen", "aus\u00b7ge\u00b7beu\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Kann ich, wo ich gelernt, auch nicht den Lehrer nennen", "tokens": ["Kann", "ich", ",", "wo", "ich", "ge\u00b7lernt", ",", "auch", "nicht", "den", "Leh\u00b7rer", "nen\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "PWAV", "PPER", "VVPP", "$,", "ADV", "PTKNEG", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich lernte doch, und mu\u00df als Sch\u00fcler mich bekennen.", "tokens": ["Ich", "lern\u00b7te", "doch", ",", "und", "mu\u00df", "als", "Sch\u00fc\u00b7ler", "mich", "be\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KON", "VMFIN", "KOUS", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und der es mich gelehrt, der wird gelernt es haben", "tokens": ["Und", "der", "es", "mich", "ge\u00b7lehrt", ",", "der", "wird", "ge\u00b7lernt", "es", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPER", "PRF", "VVPP", "$,", "PRELS", "VAFIN", "VVPP", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von seinem Lehrer, dem es andre Lehrer gaben.", "tokens": ["Von", "sei\u00b7nem", "Leh\u00b7rer", ",", "dem", "es", "and\u00b7re", "Leh\u00b7rer", "ga\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Ueberliefrung ist, wenn auch die Namen schwanden", "tokens": ["Die", "Ue\u00b7berl\u00b7ief\u00b7rung", "ist", ",", "wenn", "auch", "die", "Na\u00b7men", "schwan\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "KOUS", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Ueberliefernden, vom Anfang her vorhanden.", "tokens": ["Der", "Ue\u00b7berl\u00b7ie\u00b7fern\u00b7den", ",", "vom", "An\u00b7fang", "her", "vor\u00b7han\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "APZR", "ADJD", "$."], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Wer sagt mir nun, woher der erste selbst es nahm,", "tokens": ["Wer", "sagt", "mir", "nun", ",", "wo\u00b7her", "der", "ers\u00b7te", "selbst", "es", "nahm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ART", "ADJA", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von dem aus Hand zu Hand zu mir herab es kam?", "tokens": ["Von", "dem", "aus", "Hand", "zu", "Hand", "zu", "mir", "her\u00b7ab", "es", "kam", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "NN", "APPR", "NN", "APPR", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "So kommt der durst'ge Geist auf Wegen der Erfahrung", "tokens": ["So", "kommt", "der", "dur\u00b7st'\u00b7ge", "Geist", "auf", "We\u00b7gen", "der", "Er\u00b7fah\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "ART", "NN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Durch Ueberliefrungswald um Quell der Offenbarung.", "tokens": ["Durch", "Ue\u00b7berl\u00b7ief\u00b7rungs\u00b7wald", "um", "Quell", "der", "Of\u00b7fen\u00b7ba\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}