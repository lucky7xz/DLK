{"textgrid.poem.36944": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sagen Sie, mein grundgelehrter", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sagen Sie, mein grundgelehrter", "tokens": ["Sa\u00b7gen", "Sie", ",", "mein", "grund\u00b7ge\u00b7lehr\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herr Professor, wissen Sie:", "tokens": ["Herr", "Pro\u00b7fes\u00b7sor", ",", "wis\u00b7sen", "Sie", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welche, auf der ganzen Erde", "tokens": ["Wel\u00b7che", ",", "auf", "der", "gan\u00b7zen", "Er\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PWAT", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist die frei'ste Monarchie?", "tokens": ["Ist", "die", "frei'\u00b7ste", "Mon\u00b7ar\u00b7chie", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "China ist's! Da\u00df Sie's nicht wu\u00dften!", "tokens": ["Chi\u00b7na", "ist's", "!", "Da\u00df", "Sie's", "nicht", "wu\u00df\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "KOUS", "NE", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es liegt wahrhaftig nah'!", "tokens": ["Und", "es", "liegt", "wahr\u00b7haf\u00b7tig", "nah'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Beschr\u00e4nkteste von allen", "tokens": ["Der", "Be\u00b7schr\u00e4nk\u00b7tes\u00b7te", "von", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Menschen ist der Kaiser da.", "tokens": ["Men\u00b7schen", "ist", "der", "Kai\u00b7ser", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}