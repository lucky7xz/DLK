{"textgrid.poem.49194": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich kan mich zwar zu dir begeben", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kan mich zwar zu dir begeben", "tokens": ["Ich", "kan", "mich", "zwar", "zu", "dir", "be\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jetzt wann ich wil, mein Vatterland,", "tokens": ["Jetzt", "wann", "ich", "wil", ",", "mein", "Vat\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPER", "VMFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun ich befreyt bin von dem Leben,", "tokens": ["Nun", "ich", "be\u00b7freyt", "bin", "von", "dem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bey dem kein Gl\u00fcck ist und Bestand;", "tokens": ["Bey", "dem", "kein", "Gl\u00fcck", "ist", "und", "Be\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "VAFIN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Doch helt mich noch der Liebe Band,", "tokens": ["Doch", "helt", "mich", "noch", "der", "Lie\u00b7be", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der ich zu schwach zu widerstreben.", "tokens": ["Der", "ich", "zu", "schwach", "zu", "wi\u00b7der\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKA", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vor hab' ich mich zu dir gewendet,", "tokens": ["Vor", "hab'", "ich", "mich", "zu", "dir", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "PPER", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Ort gefiel mir besser nicht;", "tokens": ["Kein", "Ort", "ge\u00b7fiel", "mir", "bes\u00b7ser", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun hat sich alle Lust geendet,", "tokens": ["Nun", "hat", "sich", "al\u00b7le", "Lust", "ge\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach dem ich meines Hertzens Liecht,", "tokens": ["Nach", "dem", "ich", "mei\u00b7nes", "Hert\u00b7zens", "Liecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was jederman darwider spricht,", "tokens": ["Was", "je\u00b7der\u00b7man", "dar\u00b7wi\u00b7der", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit seiner Anmuth gantz verblendet.", "tokens": ["Mit", "sei\u00b7ner", "An\u00b7muth", "gantz", "ver\u00b7blen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Vor wann die Morgenr\u00f6hte lachte", "tokens": ["Vor", "wann", "die", "Mor\u00b7gen\u00b7r\u00f6h\u00b7te", "lach\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und lie\u00df sich sehen auff ihrer Bahn,", "tokens": ["Und", "lie\u00df", "sich", "se\u00b7hen", "auff", "ih\u00b7rer", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "VVINF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So gr\u00fc\u00dft' ich sie, wann ich erwachte;", "tokens": ["So", "gr\u00fc\u00dft'", "ich", "sie", ",", "wann", "ich", "er\u00b7wach\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt ist es alles nun gethan,", "tokens": ["Jetzt", "ist", "es", "al\u00b7les", "nun", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil ich mein Lieb nur preisen kan", "tokens": ["Weil", "ich", "mein", "Lieb", "nur", "prei\u00b7sen", "kan"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und allzeit ihre Ziehr betrachte.", "tokens": ["Und", "all\u00b7zeit", "ih\u00b7re", "Ziehr", "be\u00b7trach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Vor hab' ich zu den k\u00fchlen Fl\u00fcssen", "tokens": ["Vor", "hab'", "ich", "zu", "den", "k\u00fch\u00b7len", "Fl\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und klaren Brunnen mich gesellt;", "tokens": ["Und", "kla\u00b7ren", "Brun\u00b7nen", "mich", "ge\u00b7sellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Rosen, Lilien und Narcissen", "tokens": ["Die", "Ro\u00b7sen", ",", "Li\u00b7li\u00b7en", "und", "Nar\u00b7cis\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Liebt' ich f\u00fcr alles auff der Welt;", "tokens": ["Liebt'", "ich", "f\u00fcr", "al\u00b7les", "auff", "der", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jetzt wei\u00df ich viel ein sch\u00f6ner Feldt,", "tokens": ["Jetzt", "wei\u00df", "ich", "viel", "ein", "sch\u00f6\u00b7ner", "Feldt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und hoffe sein noch zu geniessen.", "tokens": ["Und", "hof\u00b7fe", "sein", "noch", "zu", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihr Bircken und ihr hohen Linden,", "tokens": ["Ihr", "Bir\u00b7cken", "und", "ihr", "ho\u00b7hen", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr W\u00fcsten und du stiller Waldt,", "tokens": ["Ihr", "W\u00fcs\u00b7ten", "und", "du", "stil\u00b7ler", "Waldt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lebt wol mit euren tieffen Gr\u00fcnden", "tokens": ["Lebt", "wol", "mit", "eu\u00b7ren", "tief\u00b7fen", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und gr\u00fcnen Wiesen mannigfalt;", "tokens": ["Und", "gr\u00fc\u00b7nen", "Wie\u00b7sen", "man\u00b7nig\u00b7falt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Trost und bester Auffenthalt", "tokens": ["Mein", "Trost", "und", "bes\u00b7ter", "Auf\u00b7fent\u00b7halt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist sonstwo als bey euch zu finden.", "tokens": ["Ist", "sonst\u00b7wo", "als", "bey", "euch", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KOKOM", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}