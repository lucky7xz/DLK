{"dta.poem.6436": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Recht eben als wie bey dem Schmecken,", "tokens": ["Recht", "e\u00b7ben", "als", "wie", "bey", "dem", "Schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KOUS", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kan uns auch die Vernunft entdecken,", "tokens": ["Kan", "uns", "auch", "die", "Ver\u00b7nunft", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In C\u00f6rpern von so mancherley Figur,", "tokens": ["In", "C\u00f6r\u00b7pern", "von", "so", "man\u00b7cher\u00b7ley", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was eigentlich auch des Geruchs Natur.", "tokens": ["Was", "ei\u00b7gent\u00b7lich", "auch", "des", "Ge\u00b7ruchs", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Nachdem in d\u00fcnner Lufft die Theilchen sind beweget,", "tokens": ["Nach\u00b7dem", "in", "d\u00fcn\u00b7ner", "Lufft", "die", "Theil\u00b7chen", "sind", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "ART", "NN", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nachdem sie spitzig oder rund,", "tokens": ["Nach\u00b7dem", "sie", "spit\u00b7zig", "o\u00b7der", "rund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wird ein\u2019 Empfindlichkeit bey uns erreget,", "tokens": ["Wird", "ein'", "Emp\u00b7find\u00b7lich\u00b7keit", "bey", "uns", "er\u00b7re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wird ihre S\u00fc\u00dfigkeit und ihre Strenge kund.", "tokens": ["Wird", "ih\u00b7re", "S\u00fc\u00b7\u00dfig\u00b7keit", "und", "ih\u00b7re", "Stren\u00b7ge", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}