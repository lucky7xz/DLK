{"textgrid.poem.66478": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Bl\u00e4tter fallen, die Haid' ist kahl,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Bl\u00e4tter fallen, die Haid' ist kahl,", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "fal\u00b7len", ",", "die", "Haid'", "ist", "kahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir ziehn mit dem Sommer aus nebligem Thal", "tokens": ["Wir", "ziehn", "mit", "dem", "Som\u00b7mer", "aus", "neb\u00b7li\u00b7gem", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Durch des sonnigen Aethers Geleise.", "tokens": ["Durch", "des", "son\u00b7ni\u00b7gen", "A\u00b7e\u00b7thers", "Ge\u00b7lei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Die Augen der Liebe sie folgen dem Flug,", "tokens": ["Die", "Au\u00b7gen", "der", "Lie\u00b7be", "sie", "fol\u00b7gen", "dem", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Sie geben uns Scheidenden Gr\u00fc\u00dfe genug", "tokens": ["Sie", "ge\u00b7ben", "uns", "Schei\u00b7den\u00b7den", "Gr\u00fc\u00b7\u00dfe", "ge\u00b7nug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Auf die weite, die luftige Reise.", "tokens": ["Auf", "die", "wei\u00b7te", ",", "die", "luf\u00b7ti\u00b7ge", "Rei\u00b7se", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ob Wolken ihn bergen, wir kennen den Pfad", "tokens": ["Ob", "Wol\u00b7ken", "ihn", "ber\u00b7gen", ",", "wir", "ken\u00b7nen", "den", "Pfad"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "VVINF", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Zum S\u00fcden, zum S\u00fcden, zum blauen Gestad,", "tokens": ["Zum", "S\u00fc\u00b7den", ",", "zum", "S\u00fc\u00b7den", ",", "zum", "blau\u00b7en", "Ge\u00b7stad", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wo die Welle sich sonnt in den Buchten.", "tokens": ["Wo", "die", "Wel\u00b7le", "sich", "sonnt", "in", "den", "Buch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "ADJD", "APPR", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Da singt der Matrose sein abendlich Lied \u2013", "tokens": ["Da", "singt", "der", "Mat\u00b7ro\u00b7se", "sein", "a\u00b7bend\u00b7lich", "Lied", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "ADJD", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Ein Gru\u00df von der Liebsten her\u00fcber dir zieht", "tokens": ["Ein", "Gru\u00df", "von", "der", "Liebs\u00b7ten", "her\u00b7\u00fc\u00b7ber", "dir", "zieht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADV", "PPER", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Aus des Nordens heimischen Schluchten!", "tokens": ["Aus", "des", "Nor\u00b7dens", "hei\u00b7mi\u00b7schen", "Schluch\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Willkommen! Leb' wohl! Sei gegr\u00fc\u00dft, und ade!", "tokens": ["Will\u00b7kom\u00b7men", "!", "Leb'", "wohl", "!", "Sei", "ge\u00b7gr\u00fc\u00dft", ",", "und", "a\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "VVIMP", "ADV", "$.", "VAFIN", "VVPP", "$,", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So geht's durch die L\u00e4nder und \u00fcber die See", "tokens": ["So", "geht's", "durch", "die", "L\u00e4n\u00b7der", "und", "\u00fc\u00b7ber", "die", "See"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Mit immer beweglichem Fl\u00fcgel.", "tokens": ["Mit", "im\u00b7mer", "be\u00b7weg\u00b7li\u00b7chem", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wir sind unter Palmen und Linden zu Gast,", "tokens": ["Wir", "sind", "un\u00b7ter", "Pal\u00b7men", "und", "Lin\u00b7den", "zu", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NE", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Doch schenkt kein S\u00fcden uns ewige Rast,", "tokens": ["Doch", "schenkt", "kein", "S\u00fc\u00b7den", "uns", "e\u00b7wi\u00b7ge", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Kein Norden kn\u00fcpfet uns Z\u00fcgel.", "tokens": ["Kein", "Nor\u00b7den", "kn\u00fcp\u00b7fet", "uns", "Z\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Wo die Heimath ist, wir wissen es kaum.", "tokens": ["Wo", "die", "Hei\u00b7math", "ist", ",", "wir", "wis\u00b7sen", "es", "kaum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ach, da droben wohl, im unendlichen Raum,", "tokens": ["Ach", ",", "da", "dro\u00b7ben", "wohl", ",", "im", "un\u00b7end\u00b7li\u00b7chen", "Raum", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "ADV", "ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dem froh wir die Schwingen vertrauen!", "tokens": ["Dem", "froh", "wir", "die", "Schwin\u00b7gen", "ver\u00b7trau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Denn Sehnsucht zieht uns und Hoffnungsgl\u00fcck", "tokens": ["Denn", "Sehn\u00b7sucht", "zieht", "uns", "und", "Hoff\u00b7nungs\u00b7gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Zum Aether, zum hohen Aether zur\u00fcck,", "tokens": ["Zum", "A\u00b7e\u00b7ther", ",", "zum", "ho\u00b7hen", "A\u00b7e\u00b7ther", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das bessere Land zu erschauen.", "tokens": ["Das", "bes\u00b7se\u00b7re", "Land", "zu", "er\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}