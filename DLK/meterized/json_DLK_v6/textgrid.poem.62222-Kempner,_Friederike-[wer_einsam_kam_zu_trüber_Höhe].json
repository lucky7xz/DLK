{"textgrid.poem.62222": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[wer einsam kam zu tr\u00fcber H\u00f6he]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer einsam kam zu tr\u00fcber H\u00f6he,", "tokens": ["Wer", "ein\u00b7sam", "kam", "zu", "tr\u00fc\u00b7ber", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Oft unverstanden angegafft,", "tokens": ["Oft", "un\u00b7ver\u00b7stan\u00b7den", "an\u00b7ge\u00b7gafft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem rauschet jedes L\u00fcftchen Wehe", "tokens": ["Dem", "rau\u00b7schet", "je\u00b7des", "L\u00fcft\u00b7chen", "We\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und jedes Bl\u00e4ttchen: halte Kraft.", "tokens": ["Und", "je\u00b7des", "Bl\u00e4tt\u00b7chen", ":", "hal\u00b7te", "Kraft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja Kraft soll dem die Gottheit geben,", "tokens": ["Ja", "Kraft", "soll", "dem", "die", "Got\u00b7theit", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "VMFIN", "ART", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer selbstlos nur das Gute will,", "tokens": ["Wer", "selbst\u00b7los", "nur", "das", "Gu\u00b7te", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADV", "ART", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit seinem Herzblut, seinem Leben,", "tokens": ["Mit", "sei\u00b7nem", "Herz\u00b7blut", ",", "sei\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sich verblutet einsam still.", "tokens": ["Und", "sich", "ver\u00b7blu\u00b7tet", "ein\u00b7sam", "still", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}