{"textgrid.poem.53414": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Morgenlied", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch die Nacht ist verflossen", "tokens": ["Auch", "die", "Nacht", "ist", "ver\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "VVPP"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und weicht dem Tage-Schein,", "tokens": ["Und", "weicht", "dem", "Ta\u00b7ge\u00b7Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Hertz ist unverdrossen", "tokens": ["Mein", "Hertz", "ist", "un\u00b7ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und danket dir allein,", "tokens": ["Und", "dan\u00b7ket", "dir", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Herr Jesu, Heil der Frommen,", "tokens": ["Herr", "Je\u00b7su", ",", "Heil", "der", "From\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df du auch diese Nacht", "tokens": ["Da\u00df", "du", "auch", "die\u00b7se", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich hast in Schutz genommen", "tokens": ["Mich", "hast", "in", "Schutz", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und v\u00e4terlich berwacht.", "tokens": ["Und", "v\u00e4\u00b7ter\u00b7lich", "ber\u00b7wacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du bist die wahre Sonne,", "tokens": ["Du", "bist", "die", "wah\u00b7re", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der S\u00fcnden-N\u00e4chte Zwang,", "tokens": ["Der", "S\u00fcn\u00b7den\u00b7N\u00e4ch\u00b7te", "Zwang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum bleib auch meine Wonne", "tokens": ["Drum", "bleib", "auch", "mei\u00b7ne", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und leuchte meinem Gang.", "tokens": ["Und", "leuch\u00b7te", "mei\u00b7nem", "Gang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Rei\u00df aus der S\u00fcnden N\u00f6te", "tokens": ["Rei\u00df", "aus", "der", "S\u00fcn\u00b7den", "N\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich, dein erworbnes Guth,", "tokens": ["Mich", ",", "dein", "er\u00b7worb\u00b7nes", "Guth", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und meiner armen Seele", "tokens": ["Und", "mei\u00b7ner", "ar\u00b7men", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hilf durch dein theures Blut.", "tokens": ["Hilf", "durch", "dein", "theu\u00b7res", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.5": {"line.1": {"text": "Leit mich auf deinen Steigen,", "tokens": ["Leit", "mich", "auf", "dei\u00b7nen", "Stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zeig mir des Lebens Pfad,", "tokens": ["Zeig", "mir", "des", "Le\u00b7bens", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich mich nicht mag neigen", "tokens": ["Da\u00df", "ich", "mich", "nicht", "mag", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VMFIN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf ein'ge Missethat.", "tokens": ["Auf", "ein'\u00b7ge", "Mis\u00b7se\u00b7that", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und m\u00f6chte mich betr\u00fcgen", "tokens": ["Und", "m\u00f6ch\u00b7te", "mich", "be\u00b7tr\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein irgend schn\u00f6der Lauf,", "tokens": ["Ein", "ir\u00b7gend", "schn\u00f6\u00b7der", "Lauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df, Jesu, mich nicht liegen,", "tokens": ["La\u00df", ",", "Je\u00b7su", ",", "mich", "nicht", "lie\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NE", "$,", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Heb mich, dein Sch\u00e4flein, auf.", "tokens": ["Heb", "mich", ",", "dein", "Sch\u00e4f\u00b7lein", ",", "auf", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Schleu\u00df um mich deine H\u00e4nde,", "tokens": ["Schleu\u00df", "um", "mich", "dei\u00b7ne", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Kommt dann mein St\u00fcndelein,", "tokens": ["Kommt", "dann", "mein", "St\u00fcn\u00b7del\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nimm durch ein seelig Ende", "tokens": ["Nimm", "durch", "ein", "see\u00b7lig", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich in den Himmel ein.", "tokens": ["Mich", "in", "den", "Him\u00b7mel", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}