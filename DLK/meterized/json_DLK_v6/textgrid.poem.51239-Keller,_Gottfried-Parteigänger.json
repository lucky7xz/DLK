{"textgrid.poem.51239": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Parteig\u00e4nger", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gefallen sind die Hiebe,", "tokens": ["Ge\u00b7fal\u00b7len", "sind", "die", "Hie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verflogen Staub und Rauch,", "tokens": ["Ver\u00b7flo\u00b7gen", "Staub", "und", "Rauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und s\u00fc\u00dfe Bruderliebe", "tokens": ["Und", "s\u00fc\u00b7\u00dfe", "Bru\u00b7der\u00b7lie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bl\u00fcht wieder an jedem Strauch!", "tokens": ["Bl\u00fcht", "wie\u00b7der", "an", "je\u00b7dem", "Strauch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hin ist so mancher Brave,", "tokens": ["Hin", "ist", "so", "man\u00b7cher", "Bra\u00b7ve", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.6": {"text": "Und bl\u00f6kend ziehn die Schafe", "tokens": ["Und", "bl\u00f6\u00b7kend", "ziehn", "die", "Scha\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zum Pferch nach altem Brauch.", "tokens": ["Zum", "Pferch", "nach", "al\u00b7tem", "Brauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nun singt in allen Pfannen", "tokens": ["Nun", "singt", "in", "al\u00b7len", "Pfan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der fette Siegesbrei;", "tokens": ["Der", "fet\u00b7te", "Sie\u00b7ges\u00b7brei", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So reit ich denn von dannen,", "tokens": ["So", "reit", "ich", "denn", "von", "dan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Stra\u00dfen sind ja frei!", "tokens": ["Die", "Stra\u00b7\u00dfen", "sind", "ja", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und winkt ein Schank an Wegen,", "tokens": ["Und", "winkt", "ein", "Schank", "an", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Will ich hinein mich legen", "tokens": ["Will", "ich", "hin\u00b7ein", "mich", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und sehn, was Ruhen sei!", "tokens": ["Und", "sehn", ",", "was", "Ru\u00b7hen", "sei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PWS", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich bin als hei\u00dfer Zecher", "tokens": ["Ich", "bin", "als", "hei\u00b7\u00dfer", "Ze\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf einen Trunk erpicht;", "tokens": ["Auf", "ei\u00b7nen", "Trunk", "er\u00b7picht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch f\u00fcllen meinen Becher", "tokens": ["Doch", "f\u00fcl\u00b7len", "mei\u00b7nen", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl Tr\u00e4nen Christi nicht \u2013", "tokens": ["Wohl", "Tr\u00e4\u00b7nen", "Chris\u00b7ti", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NE", "PTKNEG", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich trink nur herbe Reben", "tokens": ["Ich", "trink", "nur", "her\u00b7be", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und la\u00df im Herben leben", "tokens": ["Und", "la\u00df", "im", "Her\u00b7ben", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mein Sch\u00e4tzel derb und schlicht!", "tokens": ["Mein", "Sch\u00e4t\u00b7zel", "derb", "und", "schlicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich bin ein wilder Reiter,", "tokens": ["Ich", "bin", "ein", "wil\u00b7der", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auch bei\u00dft und schl\u00e4gt mein Gaul;", "tokens": ["Auch", "bei\u00dft", "und", "schl\u00e4gt", "mein", "Gaul", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin ein grober Streiter", "tokens": ["Ich", "bin", "ein", "gro\u00b7ber", "Strei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und f\u00fchr ein grobes Maul;", "tokens": ["Und", "f\u00fchr", "ein", "gro\u00b7bes", "Maul", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und sind auch allerwegen", "tokens": ["Und", "sind", "auch", "al\u00b7ler\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir rostig Schild und Degen \u2013", "tokens": ["Mir", "ros\u00b7tig", "Schild", "und", "De\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "KON", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Drein schlag ich drum nicht faul!", "tokens": ["Drein", "schlag", "ich", "drum", "nicht", "faul", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und ist der Streit geendet", "tokens": ["Und", "ist", "der", "Streit", "ge\u00b7en\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ist die Tat getan,", "tokens": ["Und", "ist", "die", "Tat", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mag ich, wie's auch sich wendet,", "tokens": ["Mag", "ich", ",", "wie's", "auch", "sich", "wen\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "VVFIN", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch keinen Lohn empfahn!", "tokens": ["Doch", "kei\u00b7nen", "Lohn", "em\u00b7pfahn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Will nicht im Rate tagen,", "tokens": ["Will", "nicht", "im", "Ra\u00b7te", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Will Ketten nicht und Kragen,", "tokens": ["Will", "Ket\u00b7ten", "nicht", "und", "Kra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PTKNEG", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die stehen mir nicht an.", "tokens": ["Die", "ste\u00b7hen", "mir", "nicht", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So sitz ich in der Schenke", "tokens": ["So", "sitz", "ich", "in", "der", "Schen\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zur braunen Distel wert,", "tokens": ["Zur", "brau\u00b7nen", "Dis\u00b7tel", "wert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil drau\u00dfen an der Tr\u00e4nke", "tokens": ["Weil", "drau\u00b7\u00dfen", "an", "der", "Tr\u00e4n\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gesattelt steht das Pferd;", "tokens": ["Ge\u00b7sat\u00b7telt", "steht", "das", "Pferd", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich lach der neuen Herren,", "tokens": ["Ich", "lach", "der", "neu\u00b7en", "Her\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die an der Beute zerren,", "tokens": ["Die", "an", "der", "Beu\u00b7te", "zer\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und lockre still mein Schwert.", "tokens": ["Und", "lock\u00b7re", "still", "mein", "Schwert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}