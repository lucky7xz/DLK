{"textgrid.poem.59074": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "293. Heilige Freude", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Freud' in Gott und seinem Willen", "tokens": ["Die", "Freud'", "in", "Gott", "und", "sei\u00b7nem", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Brunn ist, draus viel G\u00fcter quillen,", "tokens": ["Ein", "Brunn", "ist", ",", "draus", "viel", "G\u00fc\u00b7ter", "quil\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PAV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Weit mehr als aus der d\u00fcstern Pein,", "tokens": ["Weit", "mehr", "als", "aus", "der", "d\u00fcs\u00b7tern", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "KOKOM", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Die mancher hegt zum Heiligsein.", "tokens": ["Die", "man\u00b7cher", "hegt", "zum", "Hei\u00b7lig\u00b7sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}