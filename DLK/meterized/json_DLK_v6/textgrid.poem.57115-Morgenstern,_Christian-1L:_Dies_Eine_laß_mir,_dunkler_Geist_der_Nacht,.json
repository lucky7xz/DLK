{"textgrid.poem.57115": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dies Eine la\u00df mir, dunkler Geist der Nacht,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies Eine la\u00df mir, dunkler Geist der Nacht,", "tokens": ["Dies", "Ei\u00b7ne", "la\u00df", "mir", ",", "dunk\u00b7ler", "Geist", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "VVIMP", "PPER", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "dies Eine la\u00df mir: Schlummer, bis zum Ende, \u2013", "tokens": ["dies", "Ei\u00b7ne", "la\u00df", "mir", ":", "Schlum\u00b7mer", ",", "bis", "zum", "En\u00b7de", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "ART", "VVIMP", "PPER", "$.", "NN", "$,", "KOUS", "APPRART", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "wann m\u00fcd ich mich von Tag und Menschen wende,", "tokens": ["wann", "m\u00fcd", "ich", "mich", "von", "Tag", "und", "Men\u00b7schen", "wen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "traumlosen Schlummer, der vergessen macht.", "tokens": ["traum\u00b7lo\u00b7sen", "Schlum\u00b7mer", ",", "der", "ver\u00b7ges\u00b7sen", "macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "VVPP", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Des Lebens Tag ist spielend \u00fcberwunden;", "tokens": ["Des", "Le\u00b7bens", "Tag", "ist", "spie\u00b7lend", "\u00fc\u00b7berw\u00b7un\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "doch wenn das Grauen aus dem Schweigen tritt", "tokens": ["doch", "wenn", "das", "Grau\u00b7en", "aus", "dem", "Schwei\u00b7gen", "tritt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der f\u00fcrchterlichen zweiten, dritten Stunden,", "tokens": ["der", "f\u00fcrch\u00b7ter\u00b7li\u00b7chen", "zwei\u00b7ten", ",", "drit\u00b7ten", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "dann f\u00fchl' ich, da\u00df ich stets vergeblich stritt.", "tokens": ["dann", "f\u00fchl'", "ich", ",", "da\u00df", "ich", "stets", "ver\u00b7geb\u00b7lich", "stritt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es st\u00fcrzt der Ungewi\u00dfheit \u00dcbermacht", "tokens": ["Es", "st\u00fcrzt", "der", "Un\u00b7ge\u00b7wi\u00df\u00b7heit", "\u00dc\u00b7ber\u00b7macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "mein Herz in Angst und Zweifel ohne Ende ...", "tokens": ["mein", "Herz", "in", "Angst", "und", "Zwei\u00b7fel", "oh\u00b7ne", "En\u00b7de", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O wenn ich mich von Tag und Menschen wende,", "tokens": ["O", "wenn", "ich", "mich", "von", "Tag", "und", "Men\u00b7schen", "wen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "PRF", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "so la\u00df mich schlafen, dunkler Geist der Nacht!", "tokens": ["so", "la\u00df", "mich", "schla\u00b7fen", ",", "dunk\u00b7ler", "Geist", "der", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVINF", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}