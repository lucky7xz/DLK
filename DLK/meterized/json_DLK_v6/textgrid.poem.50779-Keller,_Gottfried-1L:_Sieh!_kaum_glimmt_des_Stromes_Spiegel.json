{"textgrid.poem.50779": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sieh! kaum glimmt des Stromes Spiegel", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh! kaum glimmt des Stromes Spiegel", "tokens": ["Sieh", "!", "kaum", "glimmt", "des", "Stro\u00b7mes", "Spie\u00b7gel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Silbermatt im D\u00e4mmerlicht,", "tokens": ["Sil\u00b7ber\u00b7matt", "im", "D\u00e4m\u00b7mer\u00b7licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und schon schl\u00e4gt die Sammetfl\u00fcgel", "tokens": ["Und", "schon", "schl\u00e4gt", "die", "Sam\u00b7met\u00b7fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir ein Falter ins Gesicht!", "tokens": ["Mir", "ein", "Fal\u00b7ter", "ins", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh den Abendstern dort blinken", "tokens": ["Sieh", "den", "A\u00b7bends\u00b7tern", "dort", "blin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "ADV", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Ungew\u00f6hnlich sch\u00f6n und hell!", "tokens": ["Un\u00b7ge\u00b7w\u00f6hn\u00b7lich", "sch\u00f6n", "und", "hell", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieblich ist und klar zu trinken", "tokens": ["Lieb\u00b7lich", "ist", "und", "klar", "zu", "trin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "KON", "ADJD", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser Nachtluft k\u00fchler Quell.", "tokens": ["Die\u00b7ser", "Nacht\u00b7luft", "k\u00fch\u00b7ler", "Quell", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Komm heraus, du junges Leben!", "tokens": ["Komm", "he\u00b7raus", ",", "du", "jun\u00b7ges", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Komm, so leis dein Fu\u00df dich tr\u00e4gt!", "tokens": ["Komm", ",", "so", "leis", "dein", "Fu\u00df", "dich", "tr\u00e4gt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "ADJD", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Recht in Lieb und Traum zu schweben", "tokens": ["Recht", "in", "Lieb", "und", "Traum", "zu", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4r ich jetzo aufgelegt.", "tokens": ["W\u00e4r", "ich", "jet\u00b7zo", "auf\u00b7ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und ich habe dir zu Ehren", "tokens": ["Und", "ich", "ha\u00b7be", "dir", "zu", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen guten Freund gebracht:", "tokens": ["Ei\u00b7nen", "gu\u00b7ten", "Freund", "ge\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er will uns die Minne lehren", "tokens": ["Er", "will", "uns", "die", "Min\u00b7ne", "leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Durch die kurze Sommernacht.", "tokens": ["Durch", "die", "kur\u00b7ze", "Som\u00b7mer\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Liebeslieder sollen schallen,", "tokens": ["Lie\u00b7bes\u00b7lie\u00b7der", "sol\u00b7len", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die vor siebzig Jahren schon", "tokens": ["Die", "vor", "sieb\u00b7zig", "Jah\u00b7ren", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "CARD", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsern M\u00fctterlein gefallen;", "tokens": ["Un\u00b7sern", "M\u00fct\u00b7ter\u00b7lein", "ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rein klingt ihrer Weise Ton.", "tokens": ["Rein", "klingt", "ih\u00b7rer", "Wei\u00b7se", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "La\u00df uns einmal r\u00fcckw\u00e4rts fliegen", "tokens": ["La\u00df", "uns", "ein\u00b7mal", "r\u00fcck\u00b7w\u00e4rts", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die Zeit, die still und fern!", "tokens": ["In", "die", "Zeit", ",", "die", "still", "und", "fern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieser Laune dich zu schmiegen,", "tokens": ["Die\u00b7ser", "Lau\u00b7ne", "dich", "zu", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00df ich, tust du zwiefach gern! \u2013", "tokens": ["Wei\u00df", "ich", ",", "tust", "du", "zwie\u00b7fach", "gern", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u2013 \u00bbSie kommt nicht?\u00ab fragt mein Begleiter,", "tokens": ["\u2013", "\u00bb", "Sie", "kommt", "nicht", "?", "\u00ab", "fragt", "mein", "Be\u00b7glei\u00b7ter", ","], "token_info": ["punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "$(", "PPER", "VVFIN", "PTKNEG", "$.", "$(", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u00bbund schon wird es morgenrot!\u00ab \u2013", "tokens": ["\u00bb", "und", "schon", "wird", "es", "mor\u00b7gen\u00b7rot", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "KON", "ADV", "VAFIN", "PPER", "ADJD", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, 's ist wahr! so sag ich weiter,", "tokens": ["Ach", ",", "'s", "ist", "wahr", "!", "so", "sag", "ich", "wei\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADJD", "$.", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn sie ist, wie du, schon tot!", "tokens": ["Denn", "sie", "ist", ",", "wie", "du", ",", "schon", "tot", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "PWAV", "PPER", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Armer H\u00f6lty! Du kannst gehen!", "tokens": ["Ar\u00b7mer", "H\u00f6l\u00b7ty", "!", "Du", "kannst", "ge\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Traurig such dein k\u00fchles Haus!", "tokens": ["Trau\u00b7rig", "such", "dein", "k\u00fch\u00b7les", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh, das frische Morgenwehen", "tokens": ["Sieh", ",", "das", "fri\u00b7sche", "Mor\u00b7gen\u00b7we\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lacht uns alte Kinder aus!", "tokens": ["Lacht", "uns", "al\u00b7te", "Kin\u00b7der", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}