{"textgrid.poem.42895": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dort unten ist die Erde mein", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort unten ist die Erde mein", "tokens": ["Dort", "un\u00b7ten", "ist", "die", "Er\u00b7de", "mein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Bauten und Feldern des Flei\u00dfes.", "tokens": ["Mit", "Bau\u00b7ten", "und", "Fel\u00b7dern", "des", "Flei\u00b7\u00dfes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wenn ich einmal nicht mehr werde sein,", "tokens": ["Wenn", "ich", "ein\u00b7mal", "nicht", "mehr", "wer\u00b7de", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ADV", "VAFIN", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Dann graben sie mich dort unten hinein,", "tokens": ["Dann", "gra\u00b7ben", "sie", "mich", "dort", "un\u00b7ten", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ich wei\u00df es.", "tokens": ["Ich", "wei\u00df", "es", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Dort unten ist viel M\u00fche und Not", "tokens": ["Dort", "un\u00b7ten", "ist", "viel", "M\u00fc\u00b7he", "und", "Not"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und wenig wahre Liebe. \u2013", "tokens": ["Und", "we\u00b7nig", "wah\u00b7re", "Lie\u00b7be", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun stelle ich mir sekundenlang", "tokens": ["Nun", "stel\u00b7le", "ich", "mir", "se\u00b7kun\u00b7den\u00b7lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vor, da\u00df ich oben hier bliebe,", "tokens": ["Vor", ",", "da\u00df", "ich", "o\u00b7ben", "hier", "blie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Ewig, und lebte und w\u00e4re doch tot \u2013 \u2013", "tokens": ["E\u00b7wig", ",", "und", "leb\u00b7te", "und", "w\u00e4\u00b7re", "doch", "tot", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "$,", "KON", "VVFIN", "KON", "VAFIN", "ADV", "ADJD", "$(", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "O, macht mich der Gedanke bang.", "tokens": ["O", ",", "macht", "mich", "der", "Ge\u00b7dan\u00b7ke", "bang", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein Herz und mein Gewissen schl\u00e4gt", "tokens": ["Mein", "Herz", "und", "mein", "Ge\u00b7wis\u00b7sen", "schl\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lauter als der Propeller.", "tokens": ["Lau\u00b7ter", "als", "der", "Pro\u00b7pel\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Du Flugzeug, das so schnell mich tr\u00e4gt,", "tokens": ["Du", "Flug\u00b7zeug", ",", "das", "so", "schnell", "mich", "tr\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Flieg schneller!", "tokens": ["Flieg", "schnel\u00b7ler", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}