{"textgrid.poem.58957": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "176. Jesu Blut", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie kr\u00e4ftig ist nicht Jesu Blut!", "tokens": ["Wie", "kr\u00e4f\u00b7tig", "ist", "nicht", "Je\u00b7su", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PTKNEG", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es tilget alle Schuld der S\u00fcnden,", "tokens": ["Es", "til\u00b7get", "al\u00b7le", "Schuld", "der", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es machet b\u00f6se Herzen gut;", "tokens": ["Es", "ma\u00b7chet", "b\u00f6\u00b7se", "Her\u00b7zen", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer glaubt, der kann's inwendig finden.", "tokens": ["Wer", "glaubt", ",", "der", "kann's", "in\u00b7wen\u00b7dig", "fin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}