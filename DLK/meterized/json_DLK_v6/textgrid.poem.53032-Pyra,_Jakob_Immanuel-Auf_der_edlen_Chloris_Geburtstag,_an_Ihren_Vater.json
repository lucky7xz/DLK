{"textgrid.poem.53032": {"metadata": {"author": {"name": "Pyra, Jakob Immanuel", "birth": "N.A.", "death": "N.A."}, "title": "Auf der edlen Chloris Geburtstag, an Ihren Vater", "genre": "verse", "period": "N.A.", "pub_year": 1729, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gebiethe, theurer Mann, gebiete doch auch nun", "tokens": ["Ge\u00b7bie\u00b7the", ",", "theu\u00b7rer", "Mann", ",", "ge\u00b7bie\u00b7te", "doch", "auch", "nun"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "NN", "$,", "VVFIN", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch deines Armes Winck den aufgebrachten Sayten", "tokens": ["Durch", "dei\u00b7nes", "Ar\u00b7mes", "Win\u00b7ck", "den", "auf\u00b7ge\u00b7brach\u00b7ten", "Say\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nur einen Augenblick zu ruhn;", "tokens": ["Nur", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", "zu", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bestill ihr freudenvolles Streiten.", "tokens": ["Be\u00b7still", "ihr", "freu\u00b7den\u00b7vol\u00b7les", "Strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df zu, da\u00df jetzt bey deiner Lust", "tokens": ["La\u00df", "zu", ",", "da\u00df", "jetzt", "bey", "dei\u00b7ner", "Lust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$,", "KOUS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch meine Mus aus treuer Brust", "tokens": ["Auch", "mei\u00b7ne", "Mus", "aus", "treu\u00b7er", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Allein vor deinen Augen singe:", "tokens": ["Al\u00b7lein", "vor", "dei\u00b7nen", "Au\u00b7gen", "sin\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zieh deinen aufgekl\u00e4rten Blick", "tokens": ["Zieh", "dei\u00b7nen", "auf\u00b7ge\u00b7kl\u00e4r\u00b7ten", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nebst deinem Fr\u00e4ulein nicht zur\u00fcck,", "tokens": ["Nebst", "dei\u00b7nem", "Fr\u00e4u\u00b7lein", "nicht", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Damit dis Lied nicht mi\u00dfgelinge.", "tokens": ["Da\u00b7mit", "dis", "Lied", "nicht", "mi\u00df\u00b7ge\u00b7lin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Weil deine Fr\u00e4ulein uns dein Bildni\u00df v\u00f6llig zeigt,", "tokens": ["Weil", "dei\u00b7ne", "Fr\u00e4u\u00b7lein", "uns", "dein", "Bild\u00b7ni\u00df", "v\u00f6l\u00b7lig", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Flicht meine Clio ihr die Lorbern um die Haare,", "tokens": ["Flicht", "mei\u00b7ne", "Clio", "ihr", "die", "Lor\u00b7bern", "um", "die", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "So Ph\u00f6bus um die Schl\u00e4ffe beugt,", "tokens": ["So", "Ph\u00f6\u00b7bus", "um", "die", "Schl\u00e4f\u00b7fe", "beugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df er der Tugend Lob bewahre.", "tokens": ["Da\u00df", "er", "der", "Tu\u00b7gend", "Lob", "be\u00b7wah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die edle Chloris selbst erscheint", "tokens": ["Die", "ed\u00b7le", "Chlo\u00b7ris", "selbst", "er\u00b7scheint"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit ihren Freundinnen vereint,", "tokens": ["Mit", "ih\u00b7ren", "Freun\u00b7din\u00b7nen", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Der hohe Bau der sch\u00f6nen Glieder", "tokens": ["Der", "ho\u00b7he", "Bau", "der", "sch\u00f6\u00b7nen", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist recht ein Kunstst\u00fcck der Natur;", "tokens": ["Ist", "recht", "ein", "Kunst\u00b7st\u00fcck", "der", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und ihres hohen Geistes Spur", "tokens": ["Und", "ih\u00b7res", "ho\u00b7hen", "Geis\u00b7tes", "Spur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Erscheint in ihrem Antlitz wieder.", "tokens": ["Er\u00b7scheint", "in", "ih\u00b7rem", "Ant\u00b7litz", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie gleicht der Cyntie, wenn sie nun einem Ast", "tokens": ["Sie", "gleicht", "der", "Cyn\u00b7tie", ",", "wenn", "sie", "nun", "ei\u00b7nem", "Ast"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "ADV", "ART", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den K\u00f6cher anvertraut, der auf den Schultern klinget,", "tokens": ["Den", "K\u00f6\u00b7cher", "an\u00b7ver\u00b7traut", ",", "der", "auf", "den", "Schul\u00b7tern", "klin\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sich das Chor der Nymphen angefa\u00dft,", "tokens": ["Und", "sich", "das", "Chor", "der", "Nym\u00b7phen", "an\u00b7ge\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Worinn sie selbst sich tantzend schwinget:", "tokens": ["Wo\u00b7rinn", "sie", "selbst", "sich", "tant\u00b7zend", "schwin\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie tr\u00e4gt ihr freyes Haupt erh\u00f6ht,", "tokens": ["Sie", "tr\u00e4gt", "ihr", "frey\u00b7es", "Haupt", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das \u00fcber alle andre geht,", "tokens": ["Das", "\u00fc\u00b7ber", "al\u00b7le", "and\u00b7re", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PIAT", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sich gekr\u00f6nt mit Strahlen zeiget.", "tokens": ["Und", "sich", "ge\u00b7kr\u00f6nt", "mit", "Strah\u00b7len", "zei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVPP", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Latona sieht mit stiller Brust", "tokens": ["La\u00b7to\u00b7na", "sieht", "mit", "stil\u00b7ler", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.9": {"text": "Der Tochter Sch\u00f6nheit voller Lust,", "tokens": ["Der", "Toch\u00b7ter", "Sch\u00f6n\u00b7heit", "vol\u00b7ler", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und freut sich, weil sie immer steiget.", "tokens": ["Und", "freut", "sich", ",", "weil", "sie", "im\u00b7mer", "stei\u00b7get", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch, ihre Sch\u00f6nheit ist nicht ihre gr\u00f6ste Zier,", "tokens": ["Doch", ",", "ih\u00b7re", "Sch\u00f6n\u00b7heit", "ist", "nicht", "ih\u00b7re", "gr\u00f6s\u00b7te", "Zier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPOSAT", "NN", "VAFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr tugendhafter Geist kan sie weit mehr erheben:", "tokens": ["Ihr", "tu\u00b7gend\u00b7haf\u00b7ter", "Geist", "kan", "sie", "weit", "mehr", "er\u00b7he\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PPER", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und wer sie sieht, bewundert denn an ihr", "tokens": ["Und", "wer", "sie", "sieht", ",", "be\u00b7wun\u00b7dert", "denn", "an", "ihr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VVPP", "KON", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und mu\u00df ihr dieses Zeugni\u00df geben:", "tokens": ["Und", "mu\u00df", "ihr", "die\u00b7ses", "Zeug\u00b7ni\u00df", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Sinn ist hoch, stets einerley,", "tokens": ["Ihr", "Sinn", "ist", "hoch", ",", "stets", "ei\u00b7ner\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch gantz vom blinden Hochmuth frey", "tokens": ["Doch", "gantz", "vom", "blin\u00b7den", "Hoch\u00b7muth", "frey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vollkommen edel sind die Sitten,", "tokens": ["Voll\u00b7kom\u00b7men", "e\u00b7del", "sind", "die", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es thronet selbst die Fr\u00f6mmigkeit", "tokens": ["Es", "thro\u00b7net", "selbst", "die", "Fr\u00f6m\u00b7mig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "In unverstelter Heiligkeit", "tokens": ["In", "un\u00b7ver\u00b7stel\u00b7ter", "Hei\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In ihres reinen Hertzens Mitten.", "tokens": ["In", "ih\u00b7res", "rei\u00b7nen", "Hert\u00b7zens", "Mit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Den angebohrnen Witz und herrlichen Verstand", "tokens": ["Den", "an\u00b7ge\u00b7bohr\u00b7nen", "Witz", "und", "herr\u00b7li\u00b7chen", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Pflegt sie mit klugem Flei\u00df durch lesen zu erbauen:", "tokens": ["Pflegt", "sie", "mit", "klu\u00b7gem", "Flei\u00df", "durch", "le\u00b7sen", "zu", "er\u00b7bau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "APPR", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie oftmals l\u00e4\u00dft die wohlbem\u00fchte Hand", "tokens": ["Wie", "oft\u00b7mals", "l\u00e4\u00dft", "die", "wohl\u00b7be\u00b7m\u00fch\u00b7te", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Buch gleich ihrem Vater schauen,", "tokens": ["Ein", "Buch", "gleich", "ih\u00b7rem", "Va\u00b7ter", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und wer giebt ihrer Nadel Flei\u00df", "tokens": ["Und", "wer", "giebt", "ih\u00b7rer", "Na\u00b7del", "Flei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht der Minerva w\u00fcrdgen Prei\u00df;", "tokens": ["Nicht", "der", "Mi\u00b7ner\u00b7va", "w\u00fcrd\u00b7gen", "Prei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NE", "VVFIN", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Ja wenn sie die geschwinden Finger", "tokens": ["Ja", "wenn", "sie", "die", "ge\u00b7schwin\u00b7den", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Durch die geschlagnen Claves f\u00fchrt,", "tokens": ["Durch", "die", "ge\u00b7schlag\u00b7nen", "Cla\u00b7ves", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wird jedes H\u00f6rers Hertz ger\u00fchrt,", "tokens": ["Wird", "je\u00b7des", "H\u00f6\u00b7rers", "Hertz", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und auch ihr Ruhm denn nicht geringer.", "tokens": ["Und", "auch", "ihr", "Ruhm", "denn", "nicht", "ge\u00b7rin\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dis, theurer Krosigk, ist der sch\u00f6nen Tochter Bild,", "tokens": ["Dis", ",", "theu\u00b7rer", "Kro\u00b7sigk", ",", "ist", "der", "sch\u00f6\u00b7nen", "Toch\u00b7ter", "Bild", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADJD", "NN", "$,", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und gleicht es ihr nicht recht, so schau selbst auf ihr Wesen,", "tokens": ["Und", "gleicht", "es", "ihr",