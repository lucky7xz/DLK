{"textgrid.poem.67572": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Sanct-Johannes-Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nste Sommernacht!", "tokens": ["Sch\u00f6ns\u00b7te", "Som\u00b7mer\u00b7nacht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Ich schwimm' in Rosen und bl\u00fchenden Bohnen", "tokens": ["Ich", "schwimm'", "in", "Ro\u00b7sen", "und", "bl\u00fc\u00b7hen\u00b7den", "Boh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und duftenden Hecken und Nachtviolen,", "tokens": ["Und", "duf\u00b7ten\u00b7den", "He\u00b7cken", "und", "Nacht\u00b7vi\u00b7o\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "In tausend D\u00fcften \u2013 o Natur,", "tokens": ["In", "tau\u00b7send", "D\u00fcf\u00b7ten", "\u2013", "o", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$(", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo kenn' ich Deine Kinder alle,", "tokens": ["Wo", "kenn'", "ich", "Dei\u00b7ne", "Kin\u00b7der", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Br\u00e4ute alle,", "tokens": ["Die", "Br\u00e4u\u00b7te", "al\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Die jetzt sich schm\u00fccken und lieben und paaren", "tokens": ["Die", "jetzt", "sich", "schm\u00fc\u00b7cken", "und", "lie\u00b7ben", "und", "paa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PRF", "VVINF", "KON", "VVINF", "KON", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und feiern Brautnacht! \u2013 Sch\u00f6ne Nacht!", "tokens": ["Und", "fei\u00b7ern", "Braut\u00b7nacht", "!", "\u2013", "Sch\u00f6\u00b7ne", "Nacht", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$.", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wie die Sch\u00f6pfung flammet und wallt!", "tokens": ["Wie", "die", "Sch\u00f6p\u00b7fung", "flam\u00b7met", "und", "wallt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Als ob der allanflammende Sonnenvater", "tokens": ["Als", "ob", "der", "al\u00b7lan\u00b7flam\u00b7men\u00b7de", "Son\u00b7nen\u00b7va\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Mit welcher Jugendinbrunst jetzt", "tokens": ["Mit", "wel\u00b7cher", "Ju\u00b7gen\u00b7din\u00b7brunst", "jetzt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die Erd' umarmt'! \u2013 Und der Himmel brennt:", "tokens": ["Die", "Erd'", "um\u00b7armt'", "!", "\u2013", "Und", "der", "Him\u00b7mel", "brennt", ":"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Dort Abendroth, hier Morgenroth \u2013", "tokens": ["Dort", "A\u00b7ben\u00b7droth", ",", "hier", "Mor\u00b7gen\u00b7roth", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wie k\u00fchler, d\u00e4mmernder Thautag! \u2013 Und \u2013", "tokens": ["Wie", "k\u00fch\u00b7ler", ",", "d\u00e4m\u00b7mern\u00b7der", "Thau\u00b7tag", "!", "\u2013", "Und", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "ADJA", "NN", "$.", "$(", "KON", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Und hundert Wesen schwirren empor", "tokens": ["Und", "hun\u00b7dert", "We\u00b7sen", "schwir\u00b7ren", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "In Luft und Wasser und See und Sand,", "tokens": ["In", "Luft", "und", "Was\u00b7ser", "und", "See", "und", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Summen empor! Lieben! \u2013 Unendlich, ach,", "tokens": ["Sum\u00b7men", "em\u00b7por", "!", "Lie\u00b7ben", "!", "\u2013", "Un\u00b7end\u00b7lich", ",", "ach", ","], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "ADJA", "$.", "$(", "ADJD", "$,", "ITJ", "$,"], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "Unersch\u00f6pflich bist Du sch\u00f6n,", "tokens": ["Un\u00b7er\u00b7sch\u00f6pf\u00b7lich", "bist", "Du", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Mutter Natur!", "tokens": ["Mut\u00b7ter", "Na\u00b7tur", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.20": {"text": "Und hundertartige Deiner Kinder", "tokens": ["Und", "hun\u00b7der\u00b7tar\u00b7ti\u00b7ge", "Dei\u00b7ner", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "In Leben und Lieben und Sein und Freuden!", "tokens": ["In", "Le\u00b7ben", "und", "Lie\u00b7ben", "und", "Sein", "und", "Freu\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "KON", "NN", "KON", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.22": {"text": "Wer kann sie z\u00e4hlen! wer kann sie f\u00fchlen! \u2013", "tokens": ["Wer", "kann", "sie", "z\u00e4h\u00b7len", "!", "wer", "kann", "sie", "f\u00fch\u00b7len", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$.", "PWS", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Und Du,", "tokens": ["Und", "Du", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "PPER", "$,"], "meter": "++", "measure": "spondeus"}, "line.24": {"text": "In hundert Arten und Sein und Wesen", "tokens": ["In", "hun\u00b7dert", "Ar\u00b7ten", "und", "Sein", "und", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "KON", "NN", "KON", "NN"], "meter": "++-+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.25": {"text": "Und Lieb' und Freuden Dich", "tokens": ["Und", "Lieb'", "und", "Freu\u00b7den", "Dich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Wie nenn' ich Dich?", "tokens": ["Wie", "nenn'", "ich", "Dich", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wer bin ich unter den Millionen,", "tokens": ["Wer", "bin", "ich", "un\u00b7ter", "den", "Mil\u00b7lion\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die jetzt genie\u00dfen \u2013 und wer", "tokens": ["Die", "jetzt", "ge\u00b7nie\u00b7\u00dfen", "\u2013", "und", "wer"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "VVINF", "$(", "KON", "PWS"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Unter den unendlichen Millionen,", "tokens": ["Un\u00b7ter", "den", "un\u00b7end\u00b7li\u00b7chen", "Mil\u00b7lion\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die ich genie\u00dfen nicht seh',", "tokens": ["Die", "ich", "ge\u00b7nie\u00b7\u00dfen", "nicht", "seh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "In Blum', in Bl\u00fcth', im wehenden Duft", "tokens": ["In", "Blum'", ",", "in", "Bl\u00fcth'", ",", "im", "we\u00b7hen\u00b7den", "Duft"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Der Nachtviole!", "tokens": ["Der", "Nacht\u00b7vi\u00b7o\u00b7le", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Wie Tausende sind vielleicht,", "tokens": ["Wie", "Tau\u00b7sen\u00b7de", "sind", "viel\u00b7leicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Die die Bl\u00fcthe knospen! die Ros' err\u00f6thend", "tokens": ["Die", "die", "Bl\u00fc\u00b7the", "knos\u00b7pen", "!", "die", "Ros'", "er\u00b7r\u00f6\u00b7thend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVINF", "$.", "ART", "NN", "VVPP"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Spinnen und f\u00e4rben und dufther schwimmen,", "tokens": ["Spin\u00b7nen", "und", "f\u00e4r\u00b7ben", "und", "duf\u00b7ther", "schwim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "KON", "ADJD", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "Schwimmen um mich \u2013 k\u00fchlen mich,", "tokens": ["Schwim\u00b7men", "um", "mich", "\u2013", "k\u00fch\u00b7len", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$(", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und ich seh' sie nicht!", "tokens": ["Und", "ich", "seh'", "sie", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Da fliegt der leuchtende Funke Gottes,", "tokens": ["Da", "fliegt", "der", "leuch\u00b7ten\u00b7de", "Fun\u00b7ke", "Got\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Der Sommerwurm!", "tokens": ["Der", "Som\u00b7mer\u00b7wurm", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Kleiner Wurm, leuchtender Funke, komm,", "tokens": ["Klei\u00b7ner", "Wurm", ",", "leuch\u00b7ten\u00b7der", "Fun\u00b7ke", ",", "komm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-++--+-+", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Gl\u00e4nze mir!", "tokens": ["Gl\u00e4n\u00b7ze", "mir", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.16": {"text": "Wer warst Du, da\u00df die schaffende Hand", "tokens": ["Wer", "warst", "Du", ",", "da\u00df", "die", "schaf\u00b7fen\u00b7de", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.17": {"text": "Dich also angegl\u00fcht?", "tokens": ["Dich", "al\u00b7so", "an\u00b7ge\u00b7gl\u00fcht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Mit Sonnenglanz, mit Sonnengluth!", "tokens": ["Mit", "Son\u00b7nen\u00b7glanz", ",", "mit", "Son\u00b7nen\u00b7gluth", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Wer bist Du?", "tokens": ["Wer", "bist", "Du", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.20": {"text": "Etwa der Seligen einer? Ein", "tokens": ["Et\u00b7wa", "der", "Se\u00b7li\u00b7gen", "ei\u00b7ner", "?", "Ein"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "NN", "ART", "$.", "ART"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.21": {"text": "Verbanneter Unsterblicher,", "tokens": ["Ver\u00b7ban\u00b7ne\u00b7ter", "U\u00b7nsterb\u00b7li\u00b7cher", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+---", "measure": "iambic.di.relaxed"}, "line.22": {"text": "Aus Raupenstand und Grabegespinnst", "tokens": ["Aus", "Rau\u00b7pen\u00b7stand", "und", "Gra\u00b7be\u00b7ge\u00b7spinnst"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.23": {"text": "Den Wurm zu erl\u00f6sen.", "tokens": ["Den", "Wurm", "zu", "er\u00b7l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "Und tr\u00e4gst noch Siegel der Unsterblichkeit", "tokens": ["Und", "tr\u00e4gst", "noch", "Sie\u00b7gel", "der", "U\u00b7nsterb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "NN", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Und gl\u00fchst noch lang' im Tode noch fort \u2013", "tokens": ["Und", "gl\u00fchst", "noch", "lang'", "im", "To\u00b7de", "noch", "fort", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPRART", "NN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.26": {"text": "Ziehst Blitzesfunken und duftest Feu'r,", "tokens": ["Ziehst", "Blit\u00b7zes\u00b7fun\u00b7ken", "und", "duf\u00b7test", "Feu'r", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Nicht Str\u00f6men erl\u00f6schbar, die Gold,", "tokens": ["Nicht", "Str\u00f6\u00b7men", "er\u00b7l\u00f6schbar", ",", "die", "Gold", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADJD", "$,", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.28": {"text": "Die Felsen zernagen \u2013 Wunderwurm,", "tokens": ["Die", "Fel\u00b7sen", "zer\u00b7na\u00b7gen", "\u2013", "Wun\u00b7der\u00b7wurm", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Und kriechst im Staub!", "tokens": ["Und", "kriechst", "im", "Staub", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.30": {"text": "Fleuch! ich kenne Dich nicht! Wunderwurm!", "tokens": ["Fleuch", "!", "ich", "ken\u00b7ne", "Dich", "nicht", "!", "Wun\u00b7der\u00b7wurm", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.31": {"text": "Lebe Dein Som