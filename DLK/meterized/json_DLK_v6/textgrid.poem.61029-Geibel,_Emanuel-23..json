{"textgrid.poem.61029": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun hab' ich alle Seligkeit", "tokens": ["Nun", "hab'", "ich", "al\u00b7le", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erlost von dieser Erden!", "tokens": ["Er\u00b7lost", "von", "die\u00b7ser", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "An keinem Ort, zu keiner Zeit", "tokens": ["An", "kei\u00b7nem", "Ort", ",", "zu", "kei\u00b7ner", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mag Be\u00dfres je mir werden.", "tokens": ["Mag", "Be\u00df\u00b7res", "je", "mir", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "PPER", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was nur das Herz zum Himmel hebt,", "tokens": ["Was", "nur", "das", "Herz", "zum", "Him\u00b7mel", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bescherte mir die Stunde,", "tokens": ["Be\u00b7scher\u00b7te", "mir", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Liebe voller Becher schwebt", "tokens": ["Der", "Lie\u00b7be", "vol\u00b7ler", "Be\u00b7cher", "schwebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An meinem durst'gen Munde,", "tokens": ["An", "mei\u00b7nem", "dur\u00b7st'\u00b7gen", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "O k\u00f6nnt' ich leeren den Pokal,", "tokens": ["O", "k\u00f6nnt'", "ich", "lee\u00b7ren", "den", "Po\u00b7kal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eh' dort verl\u00f6scht die Sonne,", "tokens": ["Eh'", "dort", "ver\u00b7l\u00f6scht", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und dann mit ihrem letzten Strahl", "tokens": ["Und", "dann", "mit", "ih\u00b7rem", "letz\u00b7ten", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergehn vor Liebeswonne!", "tokens": ["Ver\u00b7gehn", "vor", "Lie\u00b7bes\u00b7won\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}