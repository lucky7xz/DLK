{"textgrid.poem.44446": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[du mit dem starren Auge der Meduse]", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du mit dem starren Auge der Meduse,", "tokens": ["Du", "mit", "dem", "star\u00b7ren", "Au\u00b7ge", "der", "Me\u00b7du\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hartn\u00e4ckigkeit! du finster schaunde Magd!", "tokens": ["Hart\u00b7n\u00e4\u00b7ckig\u00b7keit", "!", "du", "fins\u00b7ter", "schaun\u00b7de", "Magd", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Begeistre du mich denn, sei meine Muse,", "tokens": ["Be\u00b7geist\u00b7re", "du", "mich", "denn", ",", "sei", "mei\u00b7ne", "Mu\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da alles andre mir den Dienst versagt.", "tokens": ["Da", "al\u00b7les", "and\u00b7re", "mir", "den", "Dienst", "ver\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "...", "tokens": ["..."], "token_info": ["punct"], "pos": ["$("]}}}}}