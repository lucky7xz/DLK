{"textgrid.poem.25568": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sage, was klapperst du denn auf deinem Neste, und rufest,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sage, was klapperst du denn auf deinem Neste, und rufest,", "tokens": ["Sa\u00b7ge", ",", "was", "klap\u00b7perst", "du", "denn", "auf", "dei\u00b7nem", "Nes\u00b7te", ",", "und", "ru\u00b7fest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "(was du sonst nicht thatst,) Gattin und Kinder herbei?", "tokens": ["(", "was", "du", "sonst", "nicht", "thatst", ",", ")", "Gat\u00b7tin", "und", "Kin\u00b7der", "her\u00b7bei", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,", "$(", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Sind dir etwa so fr\u00fch zu kalt die N\u00e4chte geworden,", "tokens": ["Sind", "dir", "et\u00b7wa", "so", "fr\u00fch", "zu", "kalt", "die", "N\u00e4ch\u00b7te", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "PTKA", "ADJD", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und du sehnest dich schon hin in das w\u00e4rmere Land,", "tokens": ["Und", "du", "seh\u00b7nest", "dich", "schon", "hin", "in", "das", "w\u00e4r\u00b7me\u00b7re", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wo der Bach das Eis, den Reif die Weyde nicht kennet,", "tokens": ["Wo", "der", "Bach", "das", "Eis", ",", "den", "Reif", "die", "Wey\u00b7de", "nicht", "ken\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wo noch Niemand sah fallen die Flocken des Schnees?", "tokens": ["Wo", "noch", "Nie\u00b7mand", "sah", "fal\u00b7len", "die", "Flo\u00b7cken", "des", "Schnees", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "VVFIN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Wenn wir gern dich auch als Hausgenossen behielten,", "tokens": ["Wenn", "wir", "gern", "dich", "auch", "als", "Haus\u00b7ge\u00b7nos\u00b7sen", "be\u00b7hiel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "ADV", "KOUS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Wenn die K\u00f6chin gleich, weil du vor Feuer das Haus", "tokens": ["Wenn", "die", "K\u00f6\u00b7chin", "gleich", ",", "weil", "du", "vor", "Feu\u00b7er", "das", "Haus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "$,", "KOUS", "PPER", "APPR", "NN", "ART", "NN"], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Sch\u00fctzest, mit manchem Hecht' verstohlen dich w\u00fcrde bewirthen,", "tokens": ["Sch\u00fct\u00b7zest", ",", "mit", "man\u00b7chem", "Hecht'", "ver\u00b7stoh\u00b7len", "dich", "w\u00fcr\u00b7de", "be\u00b7wirt\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "PIAT", "NN", "VVFIN", "PPER", "VAFIN", "VVINF", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Dennoch verdenk' ich dir nicht, da\u00df du verl\u00e4ssest die Flur,", "tokens": ["Den\u00b7noch", "ver\u00b7denk'", "ich", "dir", "nicht", ",", "da\u00df", "du", "ver\u00b7l\u00e4s\u00b7sest", "die", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Die seit deiner Geburt dich jeden Sommer zur\u00fcckzog,", "tokens": ["Die", "seit", "dei\u00b7ner", "Ge\u00b7burt", "dich", "je\u00b7den", "Som\u00b7mer", "zu\u00b7r\u00fcck\u00b7zog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+++", "measure": "trochaic.octa.plus.relaxed"}, "line.12": {"text": "Nun der Gallier hier leider! die Fr\u00f6sche dir stiehlt.", "tokens": ["Nun", "der", "Gal\u00b7lier", "hier", "lei\u00b7der", "!", "die", "Fr\u00f6\u00b7sche", "dir", "stiehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "ADV", "$.", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.13": {"text": "Tr\u00f6ste dich, Storch! mit mir; du siehest, er raubet mir alles;", "tokens": ["Tr\u00f6s\u00b7te", "dich", ",", "Storch", "!", "mit", "mir", ";", "du", "sie\u00b7hest", ",", "er", "rau\u00b7bet", "mir", "al\u00b7les", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "APPR", "PPER", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PIS", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Gold und Silber und Wein, Ruhe und Lebensgenu\u00df.", "tokens": ["Gold", "und", "Sil\u00b7ber", "und", "Wein", ",", "Ru\u00b7he", "und", "Le\u00b7bens\u00b7ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "\u00bbwas du hast, gib her, und was du nicht hast, das schaffe!\u00ab", "tokens": ["\u00bb", "was", "du", "hast", ",", "gib", "her", ",", "und", "was", "du", "nicht", "hast", ",", "das", "schaf\u00b7fe", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "PPER", "VAFIN", "$,", "VVIMP", "PTKVZ", "$,", "KON", "PWS", "PPER", "PTKNEG", "VAFIN", "$,", "PDS", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die\u00df ist, wenn er erwacht, Morgens sein h\u00f6flichster Gru\u00df.", "tokens": ["Die\u00df", "ist", ",", "wenn", "er", "er\u00b7wacht", ",", "Mor\u00b7gens", "sein", "h\u00f6f\u00b7lichs\u00b7ter", "Gru\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "KOUS", "PPER", "VVPP", "$,", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-++--+--+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "H\u00e4tt' ich Fl\u00fcgel wie du, so fl\u00f6g' ich nach Albions Insel,", "tokens": ["H\u00e4tt'", "ich", "Fl\u00fc\u00b7gel", "wie", "du", ",", "so", "fl\u00f6g'", "ich", "nach", "Al\u00b7bions", "In\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KOKOM", "PPER", "$,", "ADV", "VVFIN", "PPER", "APPR", "NE", "NN", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Die der Gallier nie, was er auch prahlet, erreicht", "tokens": ["Die", "der", "Gal\u00b7lier", "nie", ",", "was", "er", "auch", "prah\u00b7let", ",", "er\u00b7reicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ART", "NN", "ADV", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,", "VVPP"], "meter": "--+-+---+--+", "measure": "iambic.tetra.chol"}, "line.19": {"text": "H\u00e4tt' ich Kr\u00e4fte wie sonst, der vorderste w\u00e4re mein S\u00e4bel,", "tokens": ["H\u00e4tt'", "ich", "Kr\u00e4f\u00b7te", "wie", "sonst", ",", "der", "vor\u00b7ders\u00b7te", "w\u00e4\u00b7re", "mein", "S\u00e4\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KOKOM", "ADV", "$,", "ART", "ADJA", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Hoch in die L\u00fcfte gez\u00fcckt, niederzuhauen die Brut.", "tokens": ["Hoch", "in", "die", "L\u00fcf\u00b7te", "ge\u00b7z\u00fcckt", ",", "nie\u00b7der\u00b7zu\u00b7hau\u00b7en", "die", "Brut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.21": {"text": "Ach! du ziehest davon, doch ich, ich Armer, mu\u00df bleiben. \u2013", "tokens": ["Ach", "!", "du", "zie\u00b7hest", "da\u00b7von", ",", "doch", "ich", ",", "ich", "Ar\u00b7mer", ",", "mu\u00df", "blei\u00b7ben", ".", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "PAV", "$,", "KON", "PPER", "$,", "PPER", "NN", "$,", "VMFIN", "VVINF", "$.", "$("], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.22": {"text": "An dem Ufer des Nils hauset kein Gallier mehr,", "tokens": ["An", "dem", "U\u00b7fer", "des", "Nils", "hau\u00b7set", "kein", "Gal\u00b7lier", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "PIAT", "NN", "ADV", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.23": {"text": "Seines Gewehres Knall wird dich am Delta nicht schrecken,", "tokens": ["Sei\u00b7nes", "Ge\u00b7weh\u00b7res", "Knall", "wird", "dich", "am", "Del\u00b7ta", "nicht", "schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "APPRART", "NE", "PTKNEG", "VVINF", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Seine Knochen allein findest du modernd im Sumpf'.", "tokens": ["Sei\u00b7ne", "Kno\u00b7chen", "al\u00b7lein", "fin\u00b7dest", "du", "mo\u00b7dernd", "im", "Sumpf'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Lebe denn wohl, o Storch! Ich w\u00fcnsche dir gl\u00fcckliche Reise!", "tokens": ["Le\u00b7be", "denn", "wohl", ",", "o", "Storch", "!", "Ich", "w\u00fcn\u00b7sche", "dir", "gl\u00fcck\u00b7li\u00b7che", "Rei\u00b7se", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "$,", "FM", "NN", "$.", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Aber warte! Noch Eins! Sieh! meine Enkelin winkt,", "tokens": ["A\u00b7ber", "war\u00b7te", "!", "Noch", "Eins", "!", "Sieh", "!", "mei\u00b7ne", "En\u00b7ke\u00b7lin", "winkt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "NN", "$.", "NE", "$.", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Bindet ihr Halsband los, und will am Fu\u00dfe dich zeichnen,", "tokens": ["Bin\u00b7det", "ihr", "Hals\u00b7band", "los", ",", "und", "will", "am", "Fu\u00b7\u00dfe", "dich", "zeich\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKVZ", "$,", "KON", "VMFIN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Da\u00df sie dich, kehrst du zur\u00fcck, wieder erkenne daran.", "tokens": ["Da\u00df", "sie", "dich", ",", "kehrst", "du", "zu\u00b7r\u00fcck", ",", "wie\u00b7der", "er\u00b7ken\u00b7ne", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PAV", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Sie, noch ohne Gespielin, allein, sie bittet dich freundlich,", "tokens": ["Sie", ",", "noch", "oh\u00b7ne", "Ge\u00b7spie\u00b7lin", ",", "al\u00b7lein", ",", "sie", "bit\u00b7tet", "dich", "freund\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "APPR", "NN", "$,", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Bring' ihr im Schnabel doch ja dann ein Schwesterchen mit.", "tokens": ["Bring'", "ihr", "im", "Schna\u00b7bel", "doch", "ja", "dann", "ein", "Schwes\u00b7ter\u00b7chen", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "APPRART", "NN", "ADV", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Sollst dagegen, wie sonst, Herr seyn der Fr\u00f6sche. Entweder", "tokens": ["Sollst", "da\u00b7ge\u00b7gen", ",", "wie", "sonst", ",", "Herr", "seyn", "der", "Fr\u00f6\u00b7sche", ".", "Ent\u00b7we\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VMFIN", "PAV", "$,", "PWAV", "ADV", "$,", "NN", "PPOSAT", "ART", "NN", "$.", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Findest du mich nicht mehr, oder den Gallier nicht.", "tokens": ["Fin\u00b7dest", "du", "mich", "nicht", "mehr", ",", "o\u00b7der", "den", "Gal\u00b7lier", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "$,", "KON", "ART", "NN", "PTKNEG", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Aber ich hoff', o Storch! wir sehen uns wieder im Fr\u00fchling',", "tokens": ["A\u00b7ber", "ich", "hoff'", ",", "o", "Storch", "!", "wir", "se\u00b7hen", "uns", "wie\u00b7der", "im", "Fr\u00fch\u00b7ling'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "FM", "NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Wer kann zweifeln am Sieg', kennet er ", "tokens": ["Wer", "kann", "zwei\u00b7feln", "am", "Sieg'", ",", "ken\u00b7net", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VMFIN", "VVINF", "APPRART", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}