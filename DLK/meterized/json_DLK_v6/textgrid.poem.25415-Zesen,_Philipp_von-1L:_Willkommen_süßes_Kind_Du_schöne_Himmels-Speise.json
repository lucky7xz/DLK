{"textgrid.poem.25415": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Willkommen/ s\u00fc\u00dfes Kind/ Du sch\u00f6ne Himmels-Speise/", "genre": "verse", "period": "N.A.", "pub_year": 1654, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willkommen/ s\u00fc\u00dfes Kind/ Du sch\u00f6ne Himmels-Speise/", "tokens": ["Will\u00b7kom\u00b7men", "/", "s\u00fc\u00b7\u00dfes", "Kind", "/", "Du", "sch\u00f6\u00b7ne", "Him\u00b7mels\u00b7Spei\u00b7se", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADJA", "NN", "$(", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du Brunn von Bethlehem/ da Nectar \u00fcberquillt", "tokens": ["Du", "Brunn", "von", "Beth\u00b7le\u00b7hem", "/", "da", "Nec\u00b7tar", "\u00fc\u00b7berq\u00b7uillt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "APPR", "NE", "$(", "KOUS", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und unsre Seele tr\u00e4nckt. Verg\u00f6nne/ da\u00df ich preise/", "tokens": ["und", "uns\u00b7re", "See\u00b7le", "tr\u00e4nckt", ".", "Ver\u00b7g\u00f6n\u00b7ne", "/", "da\u00df", "ich", "prei\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$.", "NN", "$(", "KOUS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du Wort des Vaters/ Dich; Gib Gnade/ so du wilt/", "tokens": ["Du", "Wort", "des", "Va\u00b7ters", "/", "Dich", ";", "Gib", "Gna\u00b7de", "/", "so", "du", "wilt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$(", "PPER", "$.", "VVIMP", "NN", "$(", "ADV", "PPER", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df ich dein Nahmens-Lob erheben mag auf Erden/", "tokens": ["Da\u00df", "ich", "dein", "Nah\u00b7mens\u00b7Lob", "er\u00b7he\u00b7ben", "mag", "auf", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF", "VMFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wodurch ein fromm Gem\u00fcth die Seeligkeit gewinnt.", "tokens": ["Wo\u00b7durch", "ein", "fromm", "Ge\u00b7m\u00fcth", "die", "See\u00b7lig\u00b7keit", "ge\u00b7winnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach! m\u00f6chte nun mein Hertz al\u00dfbald zur wiegen werden!", "tokens": ["Ach", "!", "m\u00f6ch\u00b7te", "nun", "mein", "Hertz", "al\u00df\u00b7bald", "zur", "wie\u00b7gen", "wer\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "ADV", "PPOSAT", "NN", "ADV", "APPRART", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wolt ich nehmen ein/ dich/ wunder-sch\u00f6nes Kind/", "tokens": ["So", "wolt", "ich", "neh\u00b7men", "ein", "/", "dich", "/", "wun\u00b7der\u00b7sch\u00f6\u00b7nes", "Kind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ART", "$(", "PPER", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du liegst da blo\u00df und arm und zihst vor frost die Lippen/", "tokens": ["Du", "liegst", "da", "blo\u00df", "und", "arm", "und", "zihst", "vor", "frost", "die", "Lip\u00b7pen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "KON", "ADJD", "KON", "VVFIN", "APPR", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du Leit-stern Israels/ Du V\u00e4ter Vater Du", "tokens": ["Du", "Leit\u00b7stern", "Is\u00b7raels", "/", "Du", "V\u00e4\u00b7ter", "Va\u00b7ter", "Du"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "NE", "$(", "PPER", "NN", "NN", "PPER"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "und auch des Vaters Sohn; Du hast in einer Krippen/", "tokens": ["und", "auch", "des", "Va\u00b7ters", "Sohn", ";", "Du", "hast", "in", "ei\u00b7ner", "Krip\u00b7pen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NN", "$.", "PPER", "VAFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Du Sch\u00f6pfer aller Ding'/ erwehlet deine Ruh:", "tokens": ["Du", "Sch\u00f6p\u00b7fer", "al\u00b7ler", "Ding'", "/", "er\u00b7weh\u00b7let", "dei\u00b7ne", "Ruh", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PIAT", "NN", "$(", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das soll die Wiege seyn! was hastu nun f\u00fcr Betten?", "tokens": ["Das", "soll", "die", "Wie\u00b7ge", "seyn", "!", "was", "has\u00b7tu", "nun", "f\u00fcr", "Bet\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "VAINF", "$.", "PWS", "VAFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "da find' ich Stroh und Heu/ das Ochs' und Esel frist:", "tokens": ["da", "find'", "ich", "Stroh", "und", "Heu", "/", "das", "Ochs'", "und", "E\u00b7sel", "frist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$(", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ach wolte/ wolte Gott! da\u00df wir gelebet hetten/", "tokens": ["Ach", "wol\u00b7te", "/", "wol\u00b7te", "Gott", "!", "da\u00df", "wir", "ge\u00b7le\u00b7bet", "het\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$(", "VMFIN", "NN", "$.", "KOUS", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da Joseph dich/ \u00f4 Kind/ zum erstenmahl gek\u00fcst", "tokens": ["Da", "Jo\u00b7se\u00b7ph", "dich", "/", "\u00f4", "Kind", "/", "zum", "ers\u00b7ten\u00b7mahl", "ge\u00b7k\u00fcst"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NE", "PPER", "$(", "FM", "NN", "$(", "APPRART", "ADV", "VVPP"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "und fleischlich angeschaut! wie wolt ich dich empfangen!", "tokens": ["und", "fleischlich", "an\u00b7ge\u00b7schaut", "!", "wie", "wolt", "ich", "dich", "emp\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$.", "PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.18": {"text": "Die Wiege solte seyn besteckt mit Ro\u00dfmarien/", "tokens": ["Die", "Wie\u00b7ge", "sol\u00b7te", "seyn", "be\u00b7steckt", "mit", "Ro\u00df\u00b7ma\u00b7ri\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VAINF", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "und was bey diesem frost und k\u00e4lte nicht vergangen:", "tokens": ["und", "was", "bey", "die\u00b7sem", "frost", "und", "k\u00e4l\u00b7te", "nicht", "ver\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PDAT", "NN", "KON", "VVFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ich wolte k\u00fcssen dich/ Du Himmlischer Rubien!", "tokens": ["Ich", "wol\u00b7te", "k\u00fcs\u00b7sen", "dich", "/", "Du", "Himm\u00b7li\u00b7scher", "Ru\u00b7bi\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "PPER", "$(", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Du edles Kleinod Du/ Du Trost der armen Schwachen", "tokens": ["Du", "ed\u00b7les", "Klei\u00b7nod", "Du", "/", "Du", "Trost", "der", "ar\u00b7men", "Schwa\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "PPER", "$(", "PPER", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Du soltest meine Lust und beste Freude seyn:", "tokens": ["Du", "sol\u00b7test", "mei\u00b7ne", "Lust", "und", "bes\u00b7te", "Freu\u00b7de", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "KON", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Nun kann ich nicht allhier Dich leiblich sehen lachen/", "tokens": ["Nun", "kann", "ich", "nicht", "all\u00b7hier", "Dich", "leib\u00b7lich", "se\u00b7hen", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "ADV", "PPER", "ADJD", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So wird es geistlich dort geschehn/ mein Jesulein!", "tokens": ["So", "wird", "es", "geist\u00b7lich", "dort", "ge\u00b7schehn", "/", "mein", "Je\u00b7su\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "VVPP", "$(", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}