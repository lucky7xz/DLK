{"dta.poem.6169": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den absonderlichen Bewe-  \n gungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und wenn das Gantze nun solch einen Druck empfindet,", "tokens": ["Und", "wenn", "das", "Gant\u00b7ze", "nun", "solch", "ei\u00b7nen", "Druck", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "PIAT", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wodurch das Band desselben sich entbindet,", "tokens": ["Wo\u00b7durch", "das", "Band", "des\u00b7sel\u00b7ben", "sich", "ent\u00b7bin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PDS", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und die Vereinigung der kleinen C\u00f6rperlein", "tokens": ["Und", "die", "Ver\u00b7ei\u00b7ni\u00b7gung", "der", "klei\u00b7nen", "C\u00f6r\u00b7per\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zerreisst; wird ihr Verderb bald gegenw\u00e4rtig seyn.", "tokens": ["Zer\u00b7reisst", ";", "wird", "ihr", "Ver\u00b7derb", "bald", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn die Bewegungen so starck, da\u00df sie", "tokens": ["Wenn", "die", "Be\u00b7we\u00b7gun\u00b7gen", "so", "starck", ",", "da\u00df", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "$,", "KOUS", "PPER"], "meter": "+--++--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Die gantze Harmonie", "tokens": ["Die", "gant\u00b7ze", "Har\u00b7mo\u00b7nie"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Des C\u00f6rpers theilen und zerst\u00f6hren;", "tokens": ["Des", "C\u00f6r\u00b7pers", "thei\u00b7len", "und", "zer\u00b7st\u00f6h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vergeht, verdufftet er und stirbt. Wenn das Bewegen", "tokens": ["Ver\u00b7geht", ",", "ver\u00b7duff\u00b7tet", "er", "und", "stirbt", ".", "Wenn", "das", "Be\u00b7we\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "KON", "VVFIN", "$.", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nun nicht so hefftig ist hingegen,", "tokens": ["Nun", "nicht", "so", "heff\u00b7tig", "ist", "hin\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "ADJD", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und man ihn noch erkennen kan;", "tokens": ["Und", "man", "ihn", "noch", "er\u00b7ken\u00b7nen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So sieht man ihn mit Recht nur f\u00fcr ver\u00e4ndert an.", "tokens": ["So", "sieht", "man", "ihn", "mit", "Recht", "nur", "f\u00fcr", "ver\u00b7\u00e4n\u00b7dert", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "APPR", "NN", "ADV", "APPR", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}