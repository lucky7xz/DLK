{"textgrid.poem.56242": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Nacht und Winter", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von des Nordes kaltem Wehen", "tokens": ["Von", "des", "Nor\u00b7des", "kal\u00b7tem", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird der Schnee daher getrieben,", "tokens": ["Wird", "der", "Schnee", "da\u00b7her", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der die dunkle Erde decket;", "tokens": ["Der", "die", "dunk\u00b7le", "Er\u00b7de", "de\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dunkle Wolken ziehn am Himmel,", "tokens": ["Dunk\u00b7le", "Wol\u00b7ken", "ziehn", "am", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es flimmern keine Sterne,", "tokens": ["Und", "es", "flim\u00b7mern", "kei\u00b7ne", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur der Schnee im Dunkel schimmert.", "tokens": ["Nur", "der", "Schnee", "im", "Dun\u00b7kel", "schim\u00b7mert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Herb und kalt der Wind sich reget,", "tokens": ["Herb", "und", "kalt", "der", "Wind", "sich", "re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaurig st\u00f6hnt er in die Stille;", "tokens": ["Schau\u00b7rig", "st\u00f6hnt", "er", "in", "die", "Stil\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tief hat sich die Nacht gesenket.", "tokens": ["Tief", "hat", "sich", "die", "Nacht", "ge\u00b7sen\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PRF", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie sie ruhn auf dem Gefilde,", "tokens": ["Wie", "sie", "ruhn", "auf", "dem", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruhn mir in der tiefsten Seele", "tokens": ["Ruhn", "mir", "in", "der", "tiefs\u00b7ten", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dunkle Nacht und herber Winter.", "tokens": ["Dunk\u00b7le", "Nacht", "und", "her\u00b7ber", "Win\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Herb und kalt der Wind sich reget,", "tokens": ["Herb", "und", "kalt", "der", "Wind", "sich", "re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dunkle Wolken ziehn am Himmel,", "tokens": ["Dunk\u00b7le", "Wol\u00b7ken", "ziehn", "am", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tief hat sich die Nacht gesenket.", "tokens": ["Tief", "hat", "sich", "die", "Nacht", "ge\u00b7sen\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PRF", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nicht der Freude Kr\u00e4nze zieren", "tokens": ["Nicht", "der", "Freu\u00b7de", "Kr\u00e4n\u00b7ze", "zie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir das Haupt im jungen Lenze,", "tokens": ["Mir", "das", "Haupt", "im", "jun\u00b7gen", "Len\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und erheitern meine Stirne:", "tokens": ["Und", "er\u00b7hei\u00b7tern", "mei\u00b7ne", "Stir\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Denn am Morgen meines Lebens,", "tokens": ["Denn", "am", "Mor\u00b7gen", "mei\u00b7nes", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebend und begehrend Liebe,", "tokens": ["Lie\u00b7bend", "und", "be\u00b7geh\u00b7rend", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVPP", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wandl ich einsam in der Fremde,", "tokens": ["Wandl", "ich", "ein\u00b7sam", "in", "der", "Frem\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wo das Sehnen meiner Liebe,", "tokens": ["Wo", "das", "Seh\u00b7nen", "mei\u00b7ner", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo das hei\u00dfe mu\u00df, verschm\u00e4het,", "tokens": ["Wo", "das", "hei\u00b7\u00dfe", "mu\u00df", ",", "ver\u00b7schm\u00e4\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PDS", "VVFIN", "VMFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tief im Herzen sich verschlie\u00dfen.", "tokens": ["Tief", "im", "Her\u00b7zen", "sich", "ver\u00b7schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Herb und kalt der Wind sich reget,", "tokens": ["Herb", "und", "kalt", "der", "Wind", "sich", "re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dunkle Wolken ziehn am Himmel,", "tokens": ["Dunk\u00b7le", "Wol\u00b7ken", "ziehn", "am", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es flimmern keine Sterne.", "tokens": ["Und", "es", "flim\u00b7mern", "kei\u00b7ne", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wie sie ruhn auf dem Gefilde,", "tokens": ["Wie", "sie", "ruhn", "auf", "dem", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruhn mir in der tiefsten Seele", "tokens": ["Ruhn", "mir", "in", "der", "tiefs\u00b7ten", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dunkle Nacht und herber Winter.", "tokens": ["Dunk\u00b7le", "Nacht", "und", "her\u00b7ber", "Win\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Leise hallen aus der Ferne", "tokens": ["Lei\u00b7se", "hal\u00b7len", "aus", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00f6ne, die den Tag verk\u00fcnden. \u2013", "tokens": ["T\u00f6\u00b7ne", ",", "die", "den", "Tag", "ver\u00b7k\u00fcn\u00b7den", ".", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird der Tag denn sich erhellen?", "tokens": ["Wird", "der", "Tag", "denn", "sich", "er\u00b7hel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Freudebringend dem Gefilde", "tokens": ["Freu\u00b7de\u00b7brin\u00b7gend", "dem", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird er strahlen, Nacht entschweben,", "tokens": ["Wird", "er", "strah\u00b7len", ",", "Nacht", "ent\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "$,", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Herber Winter auch entfliehen,", "tokens": ["Her\u00b7ber", "Win\u00b7ter", "auch", "ent\u00b7flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Und des Jahres Kreis sich wenden,", "tokens": ["Und", "des", "Jah\u00b7res", "Kreis", "sich", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der junge Lenz in Liebe", "tokens": ["Und", "der", "jun\u00b7ge", "Lenz", "in", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Nahen der verj\u00fcngten Erde.", "tokens": ["Na\u00b7hen", "der", "ver\u00b7j\u00fcng\u00b7ten", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Mir nur, mir nur ew'ger Winter,", "tokens": ["Mir", "nur", ",", "mir", "nur", "ew'\u00b7ger", "Win\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Ew'ge Nacht, und Schmerz, und Tr\u00e4nen,", "tokens": ["Ew'\u00b7ge", "Nacht", ",", "und", "Schmerz", ",", "und", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "NN", "$,", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kein Tag, keines Sternes Flimmer!", "tokens": ["Kein", "Tag", ",", "kei\u00b7nes", "Ster\u00b7nes", "Flim\u00b7mer", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}