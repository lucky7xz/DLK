{"textgrid.poem.38615": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Blume des Andenkens", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bl\u00fcht im Fr\u00fchlingskranze dir noch die Rose,", "tokens": ["Bl\u00fcht", "im", "Fr\u00fch\u00b7lings\u00b7kran\u00b7ze", "dir", "noch", "die", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Wenn du, beim gefl\u00fcgelten Abendreigen,", "tokens": ["Wenn", "du", ",", "beim", "ge\u00b7fl\u00fc\u00b7gel\u00b7ten", "A\u00b7ben\u00b7drei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Leichter wie Sylphiden auf Blumen hinschwebst,", "tokens": ["Leich\u00b7ter", "wie", "Syl\u00b7phi\u00b7den", "auf", "Blu\u00b7men", "hin\u00b7schwebst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Liebliches M\u00e4dchen?", "tokens": ["Lieb\u00b7li\u00b7ches", "M\u00e4d\u00b7chen", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Oder kr\u00f6nt sie trauernd, als Todtenopfer,", "tokens": ["O\u00b7der", "kr\u00f6nt", "sie", "trau\u00b7ernd", ",", "als", "Tod\u00b7ten\u00b7op\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVPP", "$,", "KOUS", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Das der Sehnsucht Genius fromm dir weihte,", "tokens": ["Das", "der", "Sehn\u00b7sucht", "Ge\u00b7nius", "fromm", "dir", "weih\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "NE", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Schon dein Grabmal? Wandelt dein freier Geist schon", "tokens": ["Schon", "dein", "Grab\u00b7mal", "?", "Wan\u00b7delt", "dein", "frei\u00b7er", "Geist", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "$.", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Ueber den Sternen?", "tokens": ["Ue\u00b7ber", "den", "Ster\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Jahre schwanden: aber dein Bild erscheint mir,", "tokens": ["Jah\u00b7re", "schwan\u00b7den", ":", "a\u00b7ber", "dein", "Bild", "er\u00b7scheint", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "ADV", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Wo durch Alpenschl\u00fcnde der Waldstrom donnert,", "tokens": ["Wo", "durch", "Al\u00b7pen\u00b7schl\u00fcn\u00b7de", "der", "Wald\u00b7strom", "don\u00b7nert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wo Nachtigallen am Quell auf Mirten", "tokens": ["Und", "wo", "Nach\u00b7ti\u00b7gal\u00b7len", "am", "Quell", "auf", "Mir\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "NN", "APPRART", "NN", "APPR", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Fl\u00f6tend sich wiegen!", "tokens": ["Fl\u00f6\u00b7tend", "sich", "wie\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}