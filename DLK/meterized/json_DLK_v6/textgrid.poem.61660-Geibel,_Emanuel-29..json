{"textgrid.poem.61660": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So ist es, war's und wird es sein:", "tokens": ["So", "ist", "es", ",", "wa\u00b7r's", "und", "wird", "es", "sein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "VAFIN", "KON", "VAFIN", "PPER", "VAINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gebt Freiheit! rufen die Partein,", "tokens": ["Gebt", "Frei\u00b7heit", "!", "ru\u00b7fen", "die", "Par\u00b7tein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mit was f\u00fcr Farben sie sich schm\u00fccken;", "tokens": ["Mit", "was", "f\u00fcr", "Far\u00b7ben", "sie", "sich", "schm\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "NN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das hei\u00dft: Gebt uns das Reich allein,", "tokens": ["Das", "hei\u00dft", ":", "Gebt", "uns", "das", "Reich", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "VVIMP", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df wir die andern unterdr\u00fccken!", "tokens": ["Da\u00df", "wir", "die", "an\u00b7dern", "un\u00b7ter\u00b7dr\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So ist es, war's und wird es sein.", "tokens": ["So", "ist", "es", ",", "wa\u00b7r's", "und", "wird", "es", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "VAFIN", "KON", "VAFIN", "PPER", "VAINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "y", "tokens": ["y"], "token_info": ["word"], "pos": ["NE"], "meter": "-", "measure": "single.down"}}}}}