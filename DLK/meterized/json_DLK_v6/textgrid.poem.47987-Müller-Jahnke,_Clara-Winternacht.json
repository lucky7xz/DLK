{"textgrid.poem.47987": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Winternacht", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die lange, lange, dunkle Nacht", "tokens": ["Die", "lan\u00b7ge", ",", "lan\u00b7ge", ",", "dunk\u00b7le", "Nacht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "$,", "ADV", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "hab ich durchwacht,", "tokens": ["hab", "ich", "durch\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "mit Seufzen und in Tr\u00e4nen", "tokens": ["mit", "Seuf\u00b7zen", "und", "in", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "t\u00e4t sich mein Herz aus \u00f6der Qual", "tokens": ["t\u00e4t", "sich", "mein", "Herz", "aus", "\u00f6\u00b7der", "Qual"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "dem Sonnenstrahl,", "tokens": ["dem", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "dem Licht entgegensehnen.", "tokens": ["dem", "Licht", "ent\u00b7ge\u00b7gen\u00b7seh\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und nun es kommt \u2013 wie bleich und kalt:", "tokens": ["Und", "nun", "es", "kommt", "\u2013", "wie", "bleich", "und", "kalt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVFIN", "$(", "PWAV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "es wogt und wallt", "tokens": ["es", "wogt", "und", "wallt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "des Nebels Wahngebilde, \u2013", "tokens": ["des", "Ne\u00b7bels", "Wahn\u00b7ge\u00b7bil\u00b7de", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "zu Eis erstarrt die Tr\u00e4ne \u2013 ach!", "tokens": ["zu", "Eis", "er\u00b7starrt", "die", "Tr\u00e4\u00b7ne", "\u2013", "ach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$(", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "ein Wintertag", "tokens": ["ein", "Win\u00b7ter\u00b7tag"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "liegt \u00fcber dem Gefilde!", "tokens": ["liegt", "\u00fc\u00b7ber", "dem", "Ge\u00b7fil\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}