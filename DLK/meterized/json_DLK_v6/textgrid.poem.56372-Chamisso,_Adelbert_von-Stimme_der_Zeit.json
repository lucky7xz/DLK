{"textgrid.poem.56372": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Stimme der Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer den gestirnten Himmel fl\u00fcchtig s\u00e4he,", "tokens": ["Wer", "den", "ge\u00b7stirn\u00b7ten", "Him\u00b7mel", "fl\u00fcch\u00b7tig", "s\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der lie\u00dfe sich den Wahn vielleicht nicht rauben,", "tokens": ["Der", "lie\u00b7\u00dfe", "sich", "den", "Wahn", "viel\u00b7leicht", "nicht", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df unbeweglich starr dort alles stehe;", "tokens": ["Da\u00df", "un\u00b7be\u00b7weg\u00b7lich", "starr", "dort", "al\u00b7les", "ste\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADJD", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wer die Zeitgeschichte, m\u00f6chte glauben,", "tokens": ["Und", "wer", "die", "Zeit\u00b7ge\u00b7schich\u00b7te", ",", "m\u00f6ch\u00b7te", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "$,", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Man habe sie zum Stocken schon gebracht,", "tokens": ["Man", "ha\u00b7be", "sie", "zum", "Sto\u00b7cken", "schon", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und leichtlich lie\u00dfe sie zur\u00fcck sich schrauben.", "tokens": ["Und", "leicht\u00b7lich", "lie\u00b7\u00dfe", "sie", "zu\u00b7r\u00fcck", "sich", "schrau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wer aber w\u00e4hrend einer halben Nacht", "tokens": ["Wer", "a\u00b7ber", "w\u00e4h\u00b7rend", "ei\u00b7ner", "hal\u00b7ben", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Sterne sich erheben sah und neigen,", "tokens": ["Die", "Ster\u00b7ne", "sich", "er\u00b7he\u00b7ben", "sah", "und", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und solchem Schauspiel sinnend nachgedacht,", "tokens": ["Und", "sol\u00b7chem", "Schau\u00b7spiel", "sin\u00b7nend", "nach\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der wird die Wahrheit nimmer sich verschweigen,", "tokens": ["Der", "wird", "die", "Wahr\u00b7heit", "nim\u00b7mer", "sich", "ver\u00b7schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und sprechen, wann der Tag im Osten graut:", "tokens": ["Und", "spre\u00b7chen", ",", "wann", "der", "Tag", "im", "Os\u00b7ten", "graut", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Dort mu\u00df der Schild der Sonne bald sich zeigen;", "tokens": ["Dort", "mu\u00df", "der", "Schild", "der", "Son\u00b7ne", "bald", "sich", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ART", "NN", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und wer ein halb Jahrhundert nur geschaut,", "tokens": ["Und", "wer", "ein", "halb", "Jahr\u00b7hun\u00b7dert", "nur", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJD", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Ist mit der Weltgeschichte st\u00e4tem Gange", "tokens": ["Ist", "mit", "der", "Welt\u00b7ge\u00b7schich\u00b7te", "st\u00e4\u00b7tem", "Gan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Und allgewalt'gem Fortschritt schon vertraut.", "tokens": ["Und", "all\u00b7ge\u00b7walt'\u00b7gem", "Fort\u00b7schritt", "schon", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Ein Stern der Vorzeit stand im Niedergange,", "tokens": ["Ein", "Stern", "der", "Vor\u00b7zeit", "stand", "im", "Nie\u00b7der\u00b7gan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Als Luther aufstieg, der, ein Held, befreit", "tokens": ["Als", "Lu\u00b7ther", "auf\u00b7stieg", ",", "der", ",", "ein", "Held", ",", "be\u00b7freit"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUS", "NE", "VVFIN", "$,", "PRELS", "$,", "ART", "NN", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Die halbe Welt vom schn\u00f6den Geisteszwange.", "tokens": ["Die", "hal\u00b7be", "Welt", "vom", "schn\u00f6\u00b7den", "Geis\u00b7tes\u00b7zwan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Was Gro\u00dfes er vollbracht, war an der Zeit;", "tokens": ["Was", "Gro\u00b7\u00dfes", "er", "voll\u00b7bracht", ",", "war", "an", "der", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPER", "VVPP", "$,", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Nur mu\u00dfte, wo das Licht nicht eingedrungen,", "tokens": ["Nur", "mu\u00df\u00b7te", ",", "wo", "das", "Licht", "nicht", "ein\u00b7ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "PWAV", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Sich grimmiger erneun der alte Streit;", "tokens": ["Sich", "grim\u00b7mi\u00b7ger", "er\u00b7neun", "der", "al\u00b7te", "Streit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Denn wirrer hatte sich der Kn\u00e4ul geschlungen,", "tokens": ["Denn", "wir\u00b7rer", "hat\u00b7te", "sich", "der", "Kn\u00e4ul", "ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Derweil im Schwung das Rad der Zeit gerollt", "tokens": ["Der\u00b7weil", "im", "Schwung", "das", "Rad", "der", "Zeit", "ge\u00b7rollt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Und unvernommen, was sie schrie, verklungen:", "tokens": ["Und", "un\u00b7ver\u00b7nom\u00b7men", ",", "was", "sie", "schrie", ",", "ver\u00b7klun\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Das Licht, das mild erhellen nur gesollt,", "tokens": ["Das", "Licht", ",", "das", "mild", "er\u00b7hel\u00b7len", "nur", "ge\u00b7sollt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Es ward zum Blitzstrahl, und in Ungewittern", "tokens": ["Es", "ward", "zum", "Blitz\u00b7strahl", ",", "und", "in", "Un\u00b7ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$,", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Ward grausig Schuld und aber Schuld gezollt.", "tokens": ["Ward", "grau\u00b7sig", "Schuld", "und", "a\u00b7ber", "Schuld", "ge\u00b7zollt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "NN", "KON", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Wir sahen rings um uns den Boden zittern,", "tokens": ["Wir", "sa\u00b7hen", "rings", "um", "uns", "den", "Bo\u00b7den", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Und sahn in Blut und Aufruhr und Emp\u00f6rung", "tokens": ["Und", "sahn", "in", "Blut", "und", "Auf\u00b7ruhr", "und", "Em\u00b7p\u00f6\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Der Throne morsch gewordnes Holz zersplittern.", "tokens": ["Der", "Thro\u00b7ne", "morsch", "ge\u00b7word\u00b7nes", "Holz", "zer\u00b7split\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Im Finstern haust Verrat nur und Verschw\u00f6rung;", "tokens": ["Im", "Fins\u00b7tern", "haust", "Ver\u00b7rat", "nur", "und", "Ver\u00b7schw\u00f6\u00b7rung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Vom sonnenhellen festen Ufer sahen", "tokens": ["Vom", "son\u00b7nen\u00b7hel\u00b7len", "fes\u00b7ten", "U\u00b7fer", "sa\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Wir unbef\u00e4hrdet zu der Weltzerst\u00f6rung;", "tokens": ["Wir", "un\u00b7be\u00b7f\u00e4hr\u00b7det", "zu", "der", "Welt\u00b7zer\u00b7st\u00f6\u00b7rung", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Wir, die von Vaters H\u00e4nden schon empfahen", "tokens": ["Wir", ",", "die", "von", "Va\u00b7ters", "H\u00e4n\u00b7den", "schon", "em\u00b7pfa\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "NN", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Die G\u00fcter, denen nach sie jagen, ohne,", "tokens": ["Die", "G\u00fc\u00b7ter", ",", "de\u00b7nen", "nach", "sie", "ja\u00b7gen", ",", "oh\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVINF", "$,", "KOUI", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Vom Schein verlockt, den glei\u00dfenden zu nahen.", "tokens": ["Vom", "Schein", "ver\u00b7lockt", ",", "den", "glei\u00b7\u00dfen\u00b7den", "zu", "na\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "ART", "ADJA", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Heil ihm, der weis und stark auf festem Throne", "tokens": ["Heil", "ihm", ",", "der", "weis", "und", "stark", "auf", "fes\u00b7tem", "Thro\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PRELS", "PTKVZ", "KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.38": {"text": "Mit unsrer Liebe schirmend sich umgibt,", "tokens": ["Mit", "uns\u00b7rer", "Lie\u00b7be", "schir\u00b7mend", "sich", "um\u00b7gibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Aus Gold der Treue schmiedend seine Krone;", "tokens": ["Aus", "Gold", "der", "Treu\u00b7e", "schmie\u00b7dend", "sei\u00b7ne", "Kro\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Den wie ein Sohn ein jeder Preu\u00dfe liebt,", "tokens": ["Den", "wie", "ein", "Sohn", "ein", "je\u00b7der", "Preu\u00b7\u00dfe", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Vor dessen Fu\u00df ausbrandend ohne Schaden", "tokens": ["Vor", "des\u00b7sen", "Fu\u00df", "aus\u00b7bran\u00b7dend", "oh\u00b7ne", "Scha\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Der Zeit emp\u00f6rter Wellenschlag zerstiebt.", "tokens": ["Der", "Zeit", "em\u00b7p\u00f6r\u00b7ter", "Wel\u00b7len\u00b7schlag", "zer\u00b7stiebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Heil dir, der, ihm zun\u00e4chst im Glanz der Gnaden,", "tokens": ["Heil", "dir", ",", "der", ",", "ihm", "zu\u00b7n\u00e4chst", "im", "Glanz", "der", "Gna\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PRELS", "$,", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Das edle, treue, waffenfreud'ge Ro\u00df", "tokens": ["Das", "ed\u00b7le", ",", "treu\u00b7e", ",", "waf\u00b7fen\u00b7freu\u00b7d'\u00b7ge", "Ro\u00df"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.45": {"text": "Hilft lenken an der Liebe Seidenfaden,", "tokens": ["Hilft", "len\u00b7ken", "an", "der", "Lie\u00b7be", "Sei\u00b7den\u00b7fa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Das Ro\u00df, vor dessen Hufschlag der Kolo\u00df,", "tokens": ["Das", "Ro\u00df", ",", "vor", "des\u00b7sen", "Huf\u00b7schlag", "der", "Ko\u00b7lo\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Der lastend auf Europa einst gelegen,", "tokens": ["Der", "las\u00b7tend", "auf", "Eu\u00b7ro\u00b7pa", "einst", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "Gleich einem eitlen Nebelbild zerflo\u00df.", "tokens": ["Gleich", "