{"textgrid.poem.51386": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Klingel an Lesenden", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hundert B\u00fccher, hundert Leben,", "tokens": ["Hun\u00b7dert", "B\u00fc\u00b7cher", ",", "hun\u00b7dert", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trag ich, lieber Lesern, dir", "tokens": ["Trag", "ich", ",", "lie\u00b7ber", "Le\u00b7sern", ",", "dir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "$,", "ADV", "NN", "$,", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den hundert Reimen f\u00fcr,", "tokens": ["In", "den", "hun\u00b7dert", "Rei\u00b7men", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "NN", "APPR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wilt du darauf Achtung geben.", "tokens": ["Wilt", "du", "da\u00b7rauf", "Ach\u00b7tung", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PAV", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "B\u00fccher: nach dem Ende streben,", "tokens": ["B\u00fc\u00b7cher", ":", "nach", "dem", "En\u00b7de", "stre\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist den Anfang suchen hier.", "tokens": ["Ist", "den", "An\u00b7fang", "su\u00b7chen", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leben: eitel Wonn und Zier", "tokens": ["Le\u00b7ben", ":", "ei\u00b7tel", "Wonn", "und", "Zier"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADJD", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird um dein Verst\u00e4ndn\u00fcs schweben.", "tokens": ["Wird", "um", "dein", "Ver\u00b7st\u00e4nd\u00b7n\u00fcs", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber diese B\u00fccher seyn", "tokens": ["A\u00b7ber", "die\u00b7se", "B\u00fc\u00b7cher", "seyn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruh und Frieden im Gewissen:", "tokens": ["Ruh", "und", "Frie\u00b7den", "im", "Ge\u00b7wis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses Leben, Glantz und Schein", "tokens": ["Die\u00b7ses", "Le\u00b7ben", ",", "Glantz", "und", "Schein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und darinnen Gott gen\u00fcssen:", "tokens": ["Und", "da\u00b7rin\u00b7nen", "Gott", "ge\u00b7n\u00fcs\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich wil diese Th\u00fcr, geht ein,", "tokens": ["Ich", "wil", "die\u00b7se", "Th\u00fcr", ",", "geht", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDAT", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Dir sechs hundertmal aufschliessen.", "tokens": ["Dir", "sechs", "hun\u00b7dert\u00b7mal", "auf\u00b7schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}